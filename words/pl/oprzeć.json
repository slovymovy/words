{
    "word_family": [
        "opierać",
        "oparcie",
        "podeprzeć",
        "wesprzeć",
        "poprzeć",
        "zaprzeć",
        "wyprzeć"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "be5018a1-b72d-3c64-b97b-bba6af7f6945",
                    "sense_definition": "Ustawić lub położyć przedmiot w taki sposób, aby stykał się z czymś innym, co służy mu za podporę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem <oprzeć> ciężki rower o płot, żeby otworzyć bramę kluczem.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось <прислонить> тяжелый велосипед к забору, чтобы открыть ворота ключом."
                            }
                        },
                        {
                            "text": "Zmęczony wędrowiec postanowił <oprzeć> swój plecak o pień starego dębu.",
                            "target_lang_translations": {
                                "ru": "Уставший путник решил <прислонить> свой рюкзак к стволу старого дуба."
                            }
                        }
                    ],
                    "synonyms": [
                        "wesprzeć"
                    ],
                    "common_phrases": [
                        "oprzeć drabinę o ścianę",
                        "oprzeć głowę na rękach",
                        "oprzeć łokcie na stole"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поставить или положить предмет так, чтобы он касался чего-то другого, служащего ему опорой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прислонить",
                                "target_lang_sense_clarification": "приставить к опоре"
                            },
                            {
                                "target_lang_word": "опереть",
                                "target_lang_sense_clarification": "использовать как подпорку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "869f1bb3-cc8c-3eec-bd0b-d10b01259954",
                    "sense_definition": "Przenieść ciężar własnego ciała na jakiś przedmiot lub powierzchnię, stykając się z nim (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyna chciała <oprzeć> się o parapet i wyjrzeć przez otwarte okno.",
                            "target_lang_translations": {
                                "ru": "Девушка хотела <опереться> о подоконник и выглянуть в открытое окно."
                            }
                        },
                        {
                            "text": "Nie powinieneś <oprzeć> się o te drzwi, ponieważ zostały świeżo pomalowane.",
                            "target_lang_translations": {
                                "ru": "Тебе не стоит <прислоняться> к этой двери, так как она была только что покрашена."
                            }
                        }
                    ],
                    "synonyms": [
                        "wesprzeć się"
                    ],
                    "common_phrases": [
                        "oprzeć się o ścianę",
                        "oprzeć się wygodnie",
                        "oprzeć się plecami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перенести вес собственного тела на какой-либо предмет или поверхность, касаясь его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опереться",
                                "target_lang_sense_clarification": "навалиться весом"
                            },
                            {
                                "target_lang_word": "прислониться",
                                "target_lang_sense_clarification": "прижаться спиной или плечом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35e019db-338b-3905-a864-6ffd6382ce45",
                    "sense_definition": "Zbudować coś, na przykład teorię, dowód lub opinię, na konkretnych podstawach, faktach lub przesłankach.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_basis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sędzia musi <oprzeć> swój wyrok wyłącznie na zgromadzonym i sprawdzonym materiale dowodowym.",
                            "target_lang_translations": {
                                "ru": "Судья должен <основать> свой приговор исключительно на собранных и проверенных доказательствах."
                            }
                        },
                        {
                            "text": "Autor postanowił <oprzeć> fabułę swojej najnowszej powieści na prawdziwych wydarzeniach historycznych.",
                            "target_lang_translations": {
                                "ru": "Автор решил <построить> сюжет своего новейшего романа на реальных исторических событиях."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbudować",
                        "ufundować"
                    ],
                    "common_phrases": [
                        "oprzeć na faktach",
                        "oprzeć na dowodach",
                        "oprzeć na założeniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Построить что-либо (например, теорию, доказательство или мнение) на конкретных основаниях, фактах или предпосылках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основать",
                                "target_lang_sense_clarification": "базировать"
                            },
                            {
                                "target_lang_word": "построить",
                                "target_lang_sense_clarification": "создать на основе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4d7d10f-6230-331b-a598-b0fda33e9529",
                    "sense_definition": "Skutecznie przeciwstawić się komuś lub czemuś; nie ulec jakiejś sile, wpływowi lub pokusie (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "resistance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Było mu bardzo trudno <oprzeć> się zapachowi świeżego ciasta dochodzącemu z kuchni.",
                            "target_lang_translations": {
                                "ru": "Ему было очень трудно <устоять> перед запахом свежего пирога, доносившимся из кухни."
                            }
                        },
                        {
                            "text": "Obrońcy zdołali <oprzeć> się atakowi wroga mimo jego znacznej przewagi liczebnej.",
                            "target_lang_translations": {
                                "ru": "Защитники смогли <противостоять> атаке врага, несмотря на его значительное численное превосходство."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeciwstawić się",
                        "stawić opór"
                    ],
                    "antonyms": [
                        "ulec",
                        "poddać się"
                    ],
                    "common_phrases": [
                        "oprzeć się pokusie",
                        "oprzeć się wrażeniu",
                        "oprzeć się atakowi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Успешно противостоять кому-либо или чему-либо; не поддаться какой-либо силе, влиянию или искушению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устоять",
                                "target_lang_sense_clarification": "не поддаться искушению"
                            },
                            {
                                "target_lang_word": "противостоять",
                                "target_lang_sense_clarification": "оказывать сопротивление"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
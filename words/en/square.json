{
    "word_family": [
        "squarely",
        "squareness",
        "squarish"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ce730fce-c946-3819-a3de-593afd0dcd75",
                    "sense_definition": "A geometric shape with four straight sides of equal length and four right angles.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geometry_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Draw a <w>square</w> on the piece of paper.",
                            "target_lang_translations": {
                                "ru": "Нарисуй <w>квадрат</w> на листе бумаги."
                            }
                        },
                        {
                            "text": "The tiles on the floor form a perfect <w>square</w> pattern.",
                            "target_lang_translations": {
                                "ru": "Плитка на полу образует идеальный узор в виде <w>квадрата</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Геометрическая фигура с четырьмя прямыми сторонами равной длины и четырьмя прямыми углами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квадрат",
                                "target_lang_sense_clarification": "геометрическая фигура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d997504-9fb5-3a64-ae2c-09b88eda0561",
                    "sense_definition": "An open public space or park, usually in the center of a town or city.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "town_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We agreed to meet near the fountain in the town <w>square</w>.",
                            "target_lang_translations": {
                                "ru": "Мы договорились встретиться у фонтана на городской <w>площади</w>."
                            }
                        },
                        {
                            "text": "The market is held in the main <w>square</w> every Saturday.",
                            "target_lang_translations": {
                                "ru": "Рынок проводится на главной <w>площади</w> каждую субботу."
                            }
                        }
                    ],
                    "synonyms": [
                        "plaza",
                        "piazza",
                        "place"
                    ],
                    "common_phrases": [
                        "town-square",
                        "market-square"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытое общественное пространство или парк, обычно в центре города."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "площадь",
                                "target_lang_sense_clarification": "открытое пространство в городе"
                            },
                            {
                                "target_lang_word": "сквер",
                                "target_lang_sense_clarification": "небольшой общественный сад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c87b21c-aee1-3427-871e-9218f5c692d9",
                    "sense_definition": "A single rectangular cell within a grid, board game, or pattern.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "geometry_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Move your pawn one <w>square</w> forward.",
                            "target_lang_translations": {
                                "ru": "Передвиньте пешку на одну <w>клетку</w> вперёд."
                            }
                        },
                        {
                            "text": "Each <w>square</w> on the chessboard represents a possible position.",
                            "target_lang_translations": {
                                "ru": "Каждое <w>поле</w> на шахматной доске представляет возможную позицию."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная прямоугольная ячейка в сетке, настольной игре или узоре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клетка",
                                "target_lang_sense_clarification": "ячейка сетки"
                            },
                            {
                                "target_lang_word": "поле",
                                "target_lang_sense_clarification": "в играх, например, шахматах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24a4231d-5ed0-302f-9985-321bf5f6db11",
                    "sense_definition": "The result obtained when a number is multiplied by itself.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "math_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>square</w> of five is twenty-five.",
                            "target_lang_translations": {
                                "ru": "<w>Квадрат</w> пяти равен двадцати пяти."
                            }
                        },
                        {
                            "text": "Students learned how to calculate the <w>square</w> of various integers.",
                            "target_lang_translations": {
                                "ru": "Студенты учились вычислять <w>квадрат</w> различных целых чисел."
                            }
                        }
                    ],
                    "antonyms": [
                        "square root"
                    ],
                    "target_lang_definitions": {
                        "ru": "Результат, полученный при умножении числа на само себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квадрат",
                                "target_lang_sense_clarification": "математическая степень"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a9b1e1b-1a71-3b6a-9b45-b60b79825e79",
                    "sense_definition": "A sweet dessert baked in a pan and cut into rectangular pieces.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She brought a plate of lemon <w>squares</w> to the party.",
                            "target_lang_translations": {
                                "ru": "Она принесла на вечеринку тарелку лимонных <w>пирожных</w>."
                            }
                        },
                        {
                            "text": "Would you like a chocolate <w>square</w> with your coffee?",
                            "target_lang_translations": {
                                "ru": "Хотите шоколадный <w>квадратик</w> к кофе?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "lemon-square",
                        "chocolate-square"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкий десерт, испеченный на противне и нарезанный на прямоугольные кусочки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пирожное",
                                "target_lang_sense_clarification": "кондитерское изделие"
                            },
                            {
                                "target_lang_word": "квадратик",
                                "target_lang_sense_clarification": "кусочек десерта квадратной формы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb101059-9375-3304-bf0f-33c13bc4511d",
                    "sense_definition": "An L-shaped or T-shaped tool used by builders to draw or test right angles.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter used a <w>square</w> to mark the cut line.",
                            "target_lang_translations": {
                                "ru": "Плотник использовал <w>угольник</w>, чтобы отметить линию отреза."
                            }
                        },
                        {
                            "text": "Check the corner with a <w>square</w> to ensure it is ninety degrees.",
                            "target_lang_translations": {
                                "ru": "Проверьте угол <w>угольником</w>, чтобы убедиться, что он равен девяноста градусам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "carpenter's-square",
                        "set-square"
                    ],
                    "target_lang_definitions": {
                        "ru": "Г-образный или Т-образный инструмент, используемый строителями для черчения или проверки прямых углов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угольник",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "348e506f-6ae4-3837-a2f1-25b39ec338e1",
                    "sense_definition": "A person who is considered conventional, old-fashioned, or boring.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "character_boring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is such a <w>square</w> that he never wants to go out late.",
                            "target_lang_translations": {
                                "ru": "Он такой <w>зануда</w>, что никогда не хочет гулять допоздна."
                            }
                        },
                        {
                            "text": "Don't be a <w>square</w> and come join the fun.",
                            "target_lang_translations": {
                                "ru": "Не будь <w>занудой</w>, приходи повеселиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "bourgeois"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in a slightly derogatory way"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, которого считают консервативным, старомодным или скучным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зануда",
                                "target_lang_sense_clarification": "скучный человек"
                            },
                            {
                                "target_lang_word": "обыватель",
                                "target_lang_sense_clarification": "человек с узким кругозором"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ca99d6c1-43d5-3612-9a3a-996d9d0b906a",
                    "sense_definition": "Having the shape of a square; with four equal sides and four right angles.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geometry_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gift came in a small <w>square</w> box.",
                            "target_lang_translations": {
                                "ru": "Подарок был в маленькой <w>квадратной</w> коробке."
                            }
                        },
                        {
                            "text": "We need a <w>square</w> table to fit in this corner.",
                            "target_lang_translations": {
                                "ru": "Нам нужен <w>квадратный</w> стол, чтобы он поместился в этом углу."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий форму квадрата; с четырьмя равными сторонами и четырьмя прямыми углами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квадратный",
                                "target_lang_sense_clarification": "по форме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "104ed1e8-75be-3c56-b6a2-91866eb41b3c",
                    "sense_definition": "Denoting a unit of area measurement equal to a square with the specified side length.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "math_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The apartment is fifty <w>square</w> meters in size.",
                            "target_lang_translations": {
                                "ru": "Квартира имеет размер пятьдесят <w>квадратных</w> метров."
                            }
                        },
                        {
                            "text": "A <w>square</w> mile covers a very large area of land.",
                            "target_lang_translations": {
                                "ru": "<w>Квадратная</w> миля покрывает очень большой участок земли."
                            }
                        }
                    ],
                    "common_phrases": [
                        "square-meter",
                        "square-foot",
                        "square-mile",
                        "square-kilometer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначающий единицу измерения площади, равную квадрату с указанной длиной стороны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квадратный",
                                "target_lang_sense_clarification": "в единицах измерения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d589534-e7da-383a-93e5-d533452c96f9",
                    "sense_definition": "Forming a right angle; perpendicular.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "geometry_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure the shelf is <w>square</w> to the wall.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что полка <w>перпендикулярна</w> стене."
                            }
                        },
                        {
                            "text": "The corners of the frame must be perfectly <w>square</w>.",
                            "target_lang_translations": {
                                "ru": "Углы рамы должны быть идеально <w>прямыми</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "perpendicular",
                        "orthogonal"
                    ],
                    "antonyms": [
                        "crooked",
                        "oblique"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образующий прямой угол; перпендикулярный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перпендикулярный",
                                "target_lang_sense_clarification": "под прямым углом"
                            },
                            {
                                "target_lang_word": "прямой",
                                "target_lang_sense_clarification": "об угле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d785ab1-b0fa-3c45-9408-5b44445cf012",
                    "sense_definition": "Honest, fair, and straightforward.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "character_honest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I just want a <w>square</w> deal with no hidden costs.",
                            "target_lang_translations": {
                                "ru": "Я просто хочу <w>честную</w> сделку без скрытых расходов."
                            }
                        },
                        {
                            "text": "He is known for being <w>square</w> and honest in his business.",
                            "target_lang_translations": {
                                "ru": "Он известен тем, что он <w>честный</w> и порядочный в своём бизнесе."
                            }
                        }
                    ],
                    "synonyms": [
                        "straight",
                        "above board",
                        "on the level"
                    ],
                    "antonyms": [
                        "crooked"
                    ],
                    "common_phrases": [
                        "fair-and-square",
                        "square-deal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Честный, справедливый и прямой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "честный",
                                "target_lang_sense_clarification": "порядочный"
                            },
                            {
                                "target_lang_word": "справедливый",
                                "target_lang_sense_clarification": "беспристрастный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86612e21-d56d-3f6c-9599-c27eb020bcaa",
                    "sense_definition": "Tied in score or having settled all debts.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "status_even",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The match was <w>square</w> at two goals each.",
                            "target_lang_translations": {
                                "ru": "Счёт в матче был <w>равным</w>: по два гола у каждой команды."
                            }
                        },
                        {
                            "text": "Here is the money I owe you, so now we are <w>square</w>.",
                            "target_lang_translations": {
                                "ru": "Вот деньги, которые я тебе должен, так что теперь мы <w>в расчёте</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "all-square"
                    ],
                    "target_lang_definitions": {
                        "ru": "С равным счетом или уладивший все долги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в расчёте",
                                "target_lang_sense_clarification": "квиты (о долгах)"
                            },
                            {
                                "target_lang_word": "равный",
                                "target_lang_sense_clarification": "о счёте в игре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b8643b2-54a5-3c5e-9cb7-9910216cebef",
                    "sense_definition": "Having a broad, sturdy shape with angular outlines.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geometry_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boxer had a <w>square</w> jaw and broad shoulders.",
                            "target_lang_translations": {
                                "ru": "У боксёра была <w>квадратная</w> челюсть и широкие плечи."
                            }
                        },
                        {
                            "text": "He was a man of <w>square</w> frame and great strength.",
                            "target_lang_translations": {
                                "ru": "Он был мужчиной <w>крепкого</w> телосложения и огромной силы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "square-jaw",
                        "square-shoulders"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий широкую, крепкую форму с угловатыми очертаниями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квадратный",
                                "target_lang_sense_clarification": "угловатый (о подбородке)"
                            },
                            {
                                "target_lang_word": "широкий",
                                "target_lang_sense_clarification": "о плечах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5308138d-2a30-36df-a52f-dea114969ad3",
                    "sense_definition": "Conventional, old-fashioned, or boring.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "character_boring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My parents are too <w>square</w> to understand my music.",
                            "target_lang_translations": {
                                "ru": "Мои родители слишком <w>консервативны</w>, чтобы понять мою музыку."
                            }
                        },
                        {
                            "text": "He leads a very <w>square</w> life with no surprises.",
                            "target_lang_translations": {
                                "ru": "Он ведёт очень <w>скучную</w> жизнь без сюрпризов."
                            }
                        }
                    ],
                    "synonyms": [
                        "bourgeois",
                        "straight"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Консервативный, старомодный или скучный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "консервативный",
                                "target_lang_sense_clarification": "приверженный традициям"
                            },
                            {
                                "target_lang_word": "скучный",
                                "target_lang_sense_clarification": "неинтересный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c48399cd-8d9f-3ebc-81c3-fea852fa90b4",
                    "sense_definition": "Substantial, satisfying, and balanced (usually referring to a meal).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Everyone needs three <w>square</w> meals a day to stay healthy.",
                            "target_lang_translations": {
                                "ru": "Каждому нужно три <w>полноценных</w> приёма пищи в день, чтобы оставаться здоровым."

                        "en": [
                            {
                                "target_lang_word": "cease",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "abandon",
                                "target_lang_sense_clarification": "give up an attempt or pursuit"
                            },
                            {
                                "target_lang_word": "discontinue",
                                "target_lang_sense_clarification": "stop doing something"

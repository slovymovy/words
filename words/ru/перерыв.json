    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "11e1fc1f-9a1e-33e9-bfbe-543bcba8dd91",
                    "sense_definition": "Промежуток времени, на который временно прекращается какая-либо деятельность, работа или занятие.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_interval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Магазин закрывается на <перерыв> с двух до трёх часов дня.",
                            "target_lang_translations": {
                                "en": "The shop closes for a <break> from two to three in the afternoon."
                            }
                        },
                        {
                            "text": "Мы сделали небольшой <перерыв>, чтобы выпить кофе и отдохнуть.",
                            "target_lang_translations": {
                                "en": "We took a short <break> to drink coffee and rest."
                            }
                        }
                    ],
                    "synonyms": [
                        "пауза"
                    ],
                    "common_phrases": [
                        "обеденный перерыв",
                        "перерыв на обед",
                        "без перерыва",
                        "короткий перерыв"
                    ],
                    "target_lang_definitions": {
                        "en": "A period of time when an activity, work, or meeting is temporarily stopped."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "a rest or pause from work or activity"
                            },
                            {
                                "target_lang_word": "intermission",
                                "target_lang_sense_clarification": "a break between parts of a performance"
                            },
                            {
                                "target_lang_word": "pause",
                                "target_lang_sense_clarification": "a temporary stop"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cfb62ca-09c1-3152-a5f8-cbf5f2fa2355",
                    "sense_definition": "Временная приостановка или прекращение какого-либо действия или процесса.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cessation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за сильной грозы произошёл <перерыв> в подаче электричества.",
                            "target_lang_translations": {
                                "en": "Due to a severe thunderstorm, there was an <interruption> in the electricity supply."
                            }
                        },
                        {
                            "text": "Он работал несколько часов подряд без <перерыва> и отдыха.",
                            "target_lang_translations": {
                                "en": "He worked for several hours in a row without a <break> and rest."
                            }
                        }
                    ],
                    "synonyms": [
                        "пауза"
                    ],
                    "antonyms": [
                        "возобновление",
                        "непрерывность"
                    ],
                    "common_phrases": [
                        "перерыв в работе",
                        "технический перерыв"
                    ],
                    "target_lang_definitions": {
                        "en": "A temporary suspension or cessation of an action or process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "interruption",
                                "target_lang_sense_clarification": "an act of delaying or stopping something for a time"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "a gap in continuity"
                            },
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "a cessation of activity"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fe6f9a5b-be28-330e-aaff-97627087a3f9",
                    "sense_definition": "Деепричастие прошедшего времени от глагола <w>перерыть</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Перерыв> все ящики стола, он наконец нашёл нужные документы.",
                            "target_lang_translations": {
                                "en": "<Having searched through> all the desk drawers, he finally found the necessary documents."
                            }
                        },
                        {
                            "text": "Собака убежала в будку, <перерыв> всю цветочную клумбу.",
                            "target_lang_translations": {
                                "en": "The dog ran off to the kennel, <having dug up> the whole flowerbed."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "gerund (adverbial participle)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Past adverbial participle of the verb 'to dig up' or 'to rummage through'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "having dug up",
                                "target_lang_sense_clarification": "action of digging soil or ground"
                            },
                            {
                                "target_lang_word": "having searched through",
                                "target_lang_sense_clarification": "action of rummaging to find something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
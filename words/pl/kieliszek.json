{
    "word_family": [
        "kielich",
        "kieliszeczek",
        "kieliszkowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a053b96c-477c-34f5-a488-e7e9abee56f1",
                    "sense_definition": "Szklane naczynie, zazwyczaj na nóżce i z podstawką, służące do picia napojów alkoholowych, takich jak wino, szampan czy koktajle.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "naczynie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner postawił na stole czysty <w>kieliszek</w> do wina.",
                            "target_lang_translations": {
                                "ru": "Официант поставил на стол чистый <w>бокал</w> для вина."
                            }
                        },
                        {
                            "text": "Dostałem od nich w prezencie piękny zestaw <w>kieliszków</w> do szampana.",
                            "target_lang_translations": {
                                "ru": "Я получил от них в подарок красивый набор <w>фужеров</w> для шампанского."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kieliszek do wina",
                        "kieliszek do szampana",
                        "stłuc kieliszek",
                        "kryształowy kieliszek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стеклянный сосуд, обычно на ножке, предназначенный для питья алкогольных напитков, таких как вино, шампанское или коктейли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бокал",
                                "target_lang_sense_clarification": "для вина"
                            },
                            {
                                "target_lang_word": "фужер",
                                "target_lang_sense_clarification": "для шампанского"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba77ee64-362b-3bea-b5a6-6c6c201938bf",
                    "sense_definition": "Małe szklane naczynie służące do picia mocnych alkoholi, głównie wódki.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "naczynie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na stole stała zmrożona butelka i mały <w>kieliszek</w>.",
                            "target_lang_translations": {
                                "ru": "На столе стояла ледяная бутылка и маленькая <w>рюмка</w>."
                            }
                        },
                        {
                            "text": "Goście podnieśli każdy swój <w>kieliszek</w>, aby wznieść toast.",
                            "target_lang_translations": {
                                "ru": "Гости подняли свои <w>рюмки</w>, чтобы произнести тост."
                            }
                        }
                    ],
                    "synonyms": [
                        "lufka"
                    ],
                    "common_phrases": [
                        "kieliszek do wódki",
                        "przechylić kieliszek",
                        "pusty kieliszek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький стеклянный сосуд для питья крепких спиртных напитков, в основном водки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рюмка",
                                "target_lang_sense_clarification": "на ножке"
                            },
                            {
                                "target_lang_word": "стопка",
                                "target_lang_sense_clarification": "без ножки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f8ec0e6-4abe-3160-848a-33aeb35e4218",
                    "sense_definition": "Ilość napoju, która mieści się w kieliszku.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "miara",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do obiadu chętnie wypiję <w>kieliszek</w> czerwonego wina.",
                            "target_lang_translations": {
                                "ru": "За обедом я с удовольствием выпью <w>бокал</w> красного вина."
                            }
                        },
                        {
                            "text": "Jeden <w>kieliszek</w> wódki na pewno ci nie zaszkodzi.",
                            "target_lang_translations": {
                                "ru": "Одна <w>рюмка</w> водки тебе точно не повредит."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kieliszek wina",
                        "kieliszek wódki",
                        "wypić kieliszek",
                        "kieliszek na odwagę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество напитка, которое вмещается в такой сосуд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бокал",
                                "target_lang_sense_clarification": "вина"
                            },
                            {
                                "target_lang_word": "рюмка",
                                "target_lang_sense_clarification": "водки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
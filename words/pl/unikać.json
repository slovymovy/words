    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bb877a42-ba69-3eb8-8b15-5e55bbbb3ded",
                    "sense_definition": "Starać się nie mieć z kimś lub z czymś bezpośredniego kontaktu; trzymać się z daleka od określonych osób lub miejsc.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "avoidance_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój kot boi się gości i zawsze <unika> obcych ludzi.",
                            "target_lang_translations": {
                                "ru": "Мой кот боится гостей и всегда <избегает> незнакомых людей."
                            }
                        },
                        {
                            "text": "Dlaczego ostatnio mnie <unikasz> i nie odbierasz telefonu?",
                            "target_lang_translations": {
                                "ru": "Почему ты в последнее время <избегаешь> меня и не отвечаешь на звонки?"
                            }
                        }
                    ],
                    "synonyms": [
                        "omijać",
                        "stronić"
                    ],
                    "antonyms": [
                        "szukać",
                        "spotykać",
                        "lgnać"
                    ],
                    "common_phrases": [
                        "unikać ludzi",
                        "unikać kontaktu",
                        "unikać wzroku",
                        "unikać jak ognia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стараться не иметь с кем-то или чем-то прямого контакта; держаться подальше от определенных людей или мест."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избегать",
                                "target_lang_sense_clarification": "стараться не встречаться"
                            },
                            {
                                "target_lang_word": "сторониться",
                                "target_lang_sense_clarification": "держаться в стороне, чуждаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b42cb2bb-8402-37fb-aadb-199f1b9594e4",
                    "sense_definition": "Podejmować działania, aby nie doświadczyć czegoś nieprzyjemnego, niebezpiecznego lub uciążliwego (np. kary, obowiązku, kłopotów).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "avoidance_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor <unika> odpowiedzialności za błędy swoich pracowników.",
                            "target_lang_translations": {
                                "ru": "Директор <избегает> ответственности за ошибки своих сотрудников."
                            }
                        },
                        {
                            "text": "Powinieneś <unikać> stresu, jeśli chcesz szybko wyzdrowieć.",
                            "target_lang_translations": {
                                "ru": "Тебе следует <избегать> стресса, если ты хочешь быстро выздороветь."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystrzegać się",
                        "uchylać się",
                        "wymigiwać się"
                    ],
                    "antonyms": [
                        "narażać się",
                        "stawiać czoła",
                        "konfrontować się"
                    ],
                    "common_phrases": [
                        "unikać odpowiedzialności",
                        "unikać kłopotów",
                        "unikać ryzyka",
                        "unikać tematu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предпринимать действия, чтобы не столкнуться с чем-то неприятным, опасным или обременительным (напр. наказанием, обязанностями, проблемами)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избегать",
                                "target_lang_sense_clarification": "уходить от опасности или неприятностей"
                            },
                            {
                                "target_lang_word": "уклоняться",
                                "target_lang_sense_clarification": "стараться не выполнять что-либо (обязанности, уплату)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
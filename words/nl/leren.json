                            {
                                "target_lang_word": "учить",
                                "target_lang_sense_clarification": "передавать знания кому-то"
                            },
                            {
                                "target_lang_word": "научить",
                                "target_lang_sense_clarification": "обучить конкретному навыку"
                            },
                            {
                                "target_lang_word": "обучать",
                                "target_lang_sense_clarification": "проводить обучение"
                            }
                        ]
                    }
                }
            ]
        },

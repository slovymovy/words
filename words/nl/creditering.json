    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3776ac70-94d8-35b1-aef1-321934a143f7",
                    "sense_definition": "Het toekennen of storten van een geldbedrag op een rekening, vaak als terugbetaling.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "financial_transaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>creditering</w> van het teveel betaalde bedrag vindt volgende week plaats.",
                            "target_lang_translations": {
                                "ru": "<w>Зачисление</w> переплаченной суммы произойдет на следующей неделе.",
                                "en": "The <w>crediting</w> of the overpaid amount will take place next week."
                            }
                        },
                        {
                            "text": "U ontvangt een bevestiging per e-mail zodra de <w>creditering</w> is verwerkt.",
                            "target_lang_translations": {
                                "ru": "Вы получите подтверждение по электронной почте, как только <w>зачисление</w> будет обработано.",
                                "en": "You will receive an email confirmation as soon as the <w>refund</w> has been processed."
                            }
                        }
                    ],
                    "synonyms": [
                        "bijschrijving",
                        "terugstorting"
                    ],
                    "antonyms": [
                        "debitering",
                        "afschrijving"
                    ],
                    "common_phrases": [
                        "creditering van de rekening",
                        "creditering van het bedrag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зачисление денежной суммы на счет, часто в качестве возврата.",
                        "en": "The act of depositing a sum of money into an account, often as a refund."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачисление",
                                "target_lang_sense_clarification": "на счет"
                            },
                            {
                                "target_lang_word": "возврат средств",
                                "target_lang_sense_clarification": "денег"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "crediting",
                                "target_lang_sense_clarification": "act of adding money"
                            },
                            {
                                "target_lang_word": "refund",
                                "target_lang_sense_clarification": "return of money"
                            },
                            {
                                "target_lang_word": "credit",
                                "target_lang_sense_clarification": "financial entry"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbd078b2-e1b7-3941-9fb3-91c126cc15ea",
                    "sense_definition": "Het boeken van een post aan de creditzijde van een grootboekrekening.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "financial_transaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De accountant voerde een correctie uit door middel van een <w>creditering</w>.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер выполнил корректировку посредством <w>кредитования</w>.",
                                "en": "The accountant performed a correction by means of a <w>credit entry</w>."
                            }
                        },
                        {
                            "text": "Tegenover elke debitering staat in dit systeem een gelijke <w>creditering</w>.",
                            "target_lang_translations": {
                                "ru": "В этой системе каждому дебетованию соответствует равное <w>кредитование</w>.",
                                "en": "In this system, every debit is matched by an equal <w>credit</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "creditboeking"
                    ],
                    "antonyms": [
                        "debitering"
                    ],
                    "common_phrases": [
                        "creditering en debitering"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бухгалтерская проводка по кредиту счета.",
                        "en": "Recording an entry on the credit side of a ledger account."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кредитование",
                                "target_lang_sense_clarification": "бухгалтерский учет"
                            },
                            {
                                "target_lang_word": "запись по кредиту",
                                "target_lang_sense_clarification": "проводка"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "credit entry",
                                "target_lang_sense_clarification": "accounting record"
                            },
                            {
                                "target_lang_word": "crediting",
                                "target_lang_sense_clarification": "booking process"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06626e32-ef45-3cfb-ab03-1fa52ad21f7c",
                    "sense_definition": "De vermelding van de naam van een maker of bijdrager als erkenning voor geleverd werk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fotograaf eiste een duidelijke <w>creditering</w> bij de publicatie van zijn foto.",
                            "target_lang_translations": {
                                "ru": "Фотограф потребовал четкого <w>указания авторства</w> при публикации своей фотографии.",
                                "en": "The photographer demanded clear <w>credit</w> upon publication of his photo."
                            }
                        },
                        {
                            "text": "Zonder de juiste <w>creditering</w> mag het artikel niet worden overgenomen.",
                            "target_lang_translations": {
                                "ru": "Без надлежащего <w>упоминания автора</w> статья не может быть перепечатана.",
                                "en": "Without proper <w>attribution</w>, the article may not be reproduced."
                            }
                        }
                    ],
                    "synonyms": [
                        "naamsvermelding",
                        "bronvermelding",
                        "erkenning"
                    ],
                    "common_phrases": [
                        "juiste creditering",
                        "ontbrekende creditering"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указание имени создателя или участника как признание вклада.",
                        "en": "Mentioning the name of a creator or contributor as acknowledgment for their work."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "указание авторства",
                                "target_lang_sense_clarification": "признание авторства"
                            },
                            {
                                "target_lang_word": "упоминание",
                                "target_lang_sense_clarification": "имени автора"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "credit",
                                "target_lang_sense_clarification": "acknowledgment"
                            },
                            {
                                "target_lang_word": "attribution",
                                "target_lang_sense_clarification": "ascribing authorship"
                            }
                        ]
                    }
                }

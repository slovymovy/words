{
    "word_family": [
        "calmness",
        "calmly",
        "calming"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f437f1a4-ca3a-3763-90ff-7db951f3e88a",
                    "sense_definition": "Peaceful, quiet, and free from anger, anxiety, or stress.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She remained <w>calm</w> even when everyone else was panicking.",
                            "target_lang_translations": {
                                "ru": "Она оставалась <w>спокойной</w>, даже когда все остальные паниковали."
                            }
                        },
                        {
                            "text": "He spoke in a <w>calm</w> voice that made us feel safe.",
                            "target_lang_translations": {
                                "ru": "Он говорил <w>спокойным</w> голосом, который внушал нам чувство безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "peaceful",
                        "relaxed",
                        "composed",
                        "serene",
                        "quiet",
                        "unruffled"
                    ],
                    "antonyms": [
                        "stressed",
                        "nervous",
                        "anxious",
                        "angry",
                        "disturbed",
                        "peevish"
                    ],
                    "common_phrases": [
                        "stay calm",
                        "keep calm",
                        "calm voice",
                        "calm manner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мирный, тихий, не испытывающий гнева, тревоги или стресса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спокойный",
                                "target_lang_sense_clarification": "невозмутимый, уравновешенный"
                            },
                            {
                                "target_lang_word": "тихий",
                                "target_lang_sense_clarification": "о голосе или нраве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f598ebc-80f1-3b5c-b07f-a70bf5c1d823",
                    "sense_definition": "Without wind, storm, or rough waves; free from physical disturbance.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "weather_environment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sea was <w>calm</w> and the sun was shining brightly.",
                            "target_lang_translations": {
                                "ru": "Море было <w>спокойным</w>, и ярко светило солнце."
                            }
                        },
                        {
                            "text": "It is a <w>calm</w> day, perfect for a boat trip.",
                            "target_lang_translations": {
                                "ru": "Это <w>тихий</w> день, идеально подходящий для прогулки на лодке."
                            }
                        }
                    ],
                    "synonyms": [
                        "still",
                        "tranquil",
                        "placid",
                        "windless"
                    ],
                    "antonyms": [
                        "windy",
                        "stormy"
                    ],
                    "common_phrases": [
                        "calm sea",
                        "calm water",
                        "calm weather",
                        "dead calm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Безветренный, тихий, без волн; свободный от физических возмущений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спокойный",
                                "target_lang_sense_clarification": "о море или воде"
                            },
                            {
                                "target_lang_word": "тихий",
                                "target_lang_sense_clarification": "о погоде"
                            },
                            {
                                "target_lang_word": "безветренный",
                                "target_lang_sense_clarification": "об отсутствии ветра"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "acaf06bd-daa5-3e2d-ae89-1de6ec4d3776",
                    "sense_definition": "To stop someone from being angry, excited, or upset; to make something quiet.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mother tried to <w>calm</w> her crying baby.",
                            "target_lang_translations": {
                                "ru": "Мать пыталась <w>успокоить</w> своего плачущего ребенка."
                            }
                        },
                        {
                            "text": "You need to <w>calm</w> your nerves before the interview.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>успокоить</w> нервы перед собеседованием."
                            }
                        }
                    ],
                    "synonyms": [
                        "soothe",
                        "pacify",
                        "quieten",
                        "appease",
                        "allay"
                    ],
                    "antonyms": [
                        "agitate",
                        "excite"
                    ],
                    "common_phrases": [
                        "calm down",
                        "calm the situation",
                        "calm someone's nerves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Успокаивать кого-то, подавлять гнев или волнение; делать тихим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "успокаивать",
                                "target_lang_sense_clarification": "снимать напряжение, утешать (несов. вид)"
                            },
                            {
                                "target_lang_word": "успокоить",
                                "target_lang_sense_clarification": "привести в состояние покоя (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9874e2b7-c13a-3cc9-a373-99b50257ecc6",
                    "sense_definition": "To become quiet and relaxed; usually used with 'down'.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>calm</w> down and tell me what happened.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>успокойся</w> и расскажи мне, что случилось."
                            }
                        },
                        {
                            "text": "The wind finally <w>calmed</w> down after the storm.",
                            "target_lang_translations": {
                                "ru": "Ветер наконец <w>стих</w> после бури."
                            }
                        }
                    ],
                    "synonyms": [
                        "cool off",
                        "settle down"
                    ],
                    "common_phrases": [
                        "calm down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Успокаиваться, становиться тихим и расслабленным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "успокаиваться",
                                "target_lang_sense_clarification": "приходить в себя (несов. вид)"
                            },
                            {
                                "target_lang_word": "успокоиться",
                                "target_lang_sense_clarification": "перестать волноваться (сов. вид)"
                            },
                            {
                                "target_lang_word": "стихать",
                                "target_lang_sense_clarification": "о ветре или буре"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7ff239a5-de95-3b4c-af91-3423edb6eed7",
                    "sense_definition": "A state of peace and freedom from worry, anger, or noise.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was a strange <w>calm</w> in the office before the meeting started.",
                            "target_lang_translations": {
                                "ru": "В офисе царило странное <w>спокойствие</w> перед началом собрания."
                            }
                        },
                        {
                            "text": "She enjoyed the <w>calm</w> of the early morning.",
                            "target_lang_translations": {
                                "ru": "Она наслаждалась <w>тишиной</w> раннего утра."
                            }
                        }
                    ],
                    "synonyms": [
                        "peace",
                        "serenity",
                        "tranquility",
                        "quiet"
                    ],
                    "common_phrases": [
                        "calm before the storm",
                        "sense of calm",
                        "dead calm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние покоя и свободы от беспокойства, гнева или шума."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спокойствие",
                                "target_lang_sense_clarification": "душевное равновесие"
                            },
                            {
                                "target_lang_word": "покой",
                                "target_lang_sense_clarification": "мирное состояние"
                            },
                            {
                                "target_lang_word": "тишина",
                                "target_lang_sense_clarification": "отсутствие звуков"
                            },
                            {
                                "target_lang_word": "штиль",
                                "target_lang_sense_clarification": "полное безветрие на море"
                            },
                            {
                                "target_lang_word": "затишье",
                                "target_lang_sense_clarification": "временное спокойствие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
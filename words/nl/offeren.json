    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c6a85290-30af-3cdb-8324-271b9c39d33c",
                    "sense_definition": "Iets kostbaars (zoals een dier, bezit of tijd) afstaan, doden of wijden aan een godheid of een hoger doel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sacrifice_dedicate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude culturen <offerden> priesters vaak dieren om de goden gunstig te stemmen.",
                            "target_lang_translations": {
                                "en": "In ancient cultures, priests often <w>sacrificed</w> animals to appease the gods.",
                                "ru": "В древних культурах жрецы часто <приносили в жертву> животных, чтобы умилостивить богов."
                            }
                        },
                        {
                            "text": "De soldaten hebben hun leven <geofferd> voor de vrijheid van hun land.",
                            "target_lang_translations": {
                                "en": "The soldiers <w>sacrificed</w> their lives for the freedom of their country.",
                                "ru": "Солдаты <пожертвовали> своими жизнями ради свободы своей страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "opofferen",
                        "schenken",
                        "toewijden"
                    ],
                    "antonyms": [
                        "behouden",
                        "houden"
                    ],
                    "common_phrases": [
                        "aan de goden offeren",
                        "zijn leven offeren",
                        "tijd offeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To give up, kill, or dedicate something valuable (such as an animal, possession, or time) to a deity or a higher purpose.",
                        "ru": "Отдавать, убивать или посвящать что-то ценное (например, животное, имущество или время) божеству или высшей цели."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sacrifice",
                                "target_lang_sense_clarification": "to give up something valuable or offer to a god"
                            },
                            {
                                "target_lang_word": "offer up",
                                "target_lang_sense_clarification": "specifically in a religious context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приносить в жертву",
                                "target_lang_sense_clarification": "ритуальное действие"
                            },
                            {
                                "target_lang_word": "жертвовать",
                                "target_lang_sense_clarification": "отдавать ради чего-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24239e7f-34e8-393e-bb17-00b93e696836",
                    "sense_definition": "Geld betalen, vaak met de nuance dat het onwillig gebeurt of veel kost.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "pay_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het dure etentje moest hij flink <offeren> om de rekening te voldoen.",
                            "target_lang_translations": {
                                "en": "After the expensive dinner, he had to <w>fork out</w> a lot to settle the bill.",
                                "ru": "После дорогого ужина ему пришлось изрядно <раскошелиться>, чтобы оплатить счет."
                            }
                        },
                        {
                            "text": "Als je die boete krijgt, zul je helaas moeten <offeren>.",
                            "target_lang_translations": {
                                "en": "If you get that fine, you will unfortunately have to <w>cough up</w>.",
                                "ru": "Если тебе выпишут этот штраф, тебе, к сожалению, придется <раскошелиться>."
                            }
                        }
                    ],
                    "synonyms": [
                        "betalen",
                        "dokken",
                        "lappen"
                    ],
                    "antonyms": [
                        "ontvangen",
                        "krijgen",
                        "innen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "volkstaal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To pay money, often with the nuance that it is done unwillingly or involves a high cost.",
                        "ru": "Платить деньги, часто неохотно или когда это дорого стоит."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cough up",
                                "target_lang_sense_clarification": "informal: to pay money unwillingly"
                            },
                            {
                                "target_lang_word": "fork out",
                                "target_lang_sense_clarification": "informal: to pay a large sum of money"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раскошеливаться",
                                "target_lang_sense_clarification": "разговорное, платить много"
                            },
                            {
                                "target_lang_word": "платить",
                                "target_lang_sense_clarification": "отдавать деньги"
                            }
                        ]
                    }
                }
            ]
        }

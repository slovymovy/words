{
    "word_family": [
        "we",
        "us",
        "our",
        "ours"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "32f9e37c-38cd-309c-856c-59dea56cc602",
                    "sense_definition": "The reflexive form of <w>we</w>, used when the speaker and others are the object of a verb or preposition.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We promised <w>ourselves</w> a nice holiday this year.",
                            "target_lang_translations": {
                                "ru": "Мы пообещали <w>себе</w> хороший отпуск в этом году."
                            }
                        },
                        {
                            "text": "We should keep this secret to <w>ourselves</w>.",
                            "target_lang_translations": {
                                "ru": "Нам следует оставить этот секрет при <w>себе</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "by ourselves",
                        "to ourselves",
                        "for ourselves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвратная форма местоимения \"мы\", используется, когда говорящий и другие лица являются объектом действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            },
                            {
                                "target_lang_word": "себе",
                                "target_lang_sense_clarification": "дательный падеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cf46140-f527-3eec-b80d-4627a0a570c0",
                    "sense_definition": "Used to emphasize that <w>we</w> performed the action personally, without help from anyone else.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We built this garden shed <w>ourselves</w>.",
                            "target_lang_translations": {
                                "ru": "Мы построили этот сарай <w>сами</w>."
                            }
                        },
                        {
                            "text": "We need to see the results for <w>ourselves</w>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно увидеть результаты <w>самим</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do it ourselves",
                        "see for ourselves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления того факта, что \"мы\" выполнили действие лично, без посторонней помощи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сами",
                                "target_lang_sense_clarification": "усилительное местоимение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0a64fc3-667a-3c90-a304-60693a771cf8",
                    "sense_definition": "Refers to the whole group being addressed, often used in suggestions or commands involving the speaker.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "inclusive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let's get <w>ourselves</w> ready for the big event.",
                            "target_lang_translations": {
                                "ru": "Давайте подготовим <w>себя</w> к большому событию."
                            }
                        },
                        {
                            "text": "We must pick <w>ourselves</w> up and try again.",
                            "target_lang_translations": {
                                "ru": "Мы должны взять <w>себя</w> в руки и попробовать снова."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относится ко всей группе, к которой обращаются; часто используется в предложениях или командах, включающих говорящего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "в составе фраз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0df1147-9368-3942-9837-e79bb060ac0d",
                    "sense_definition": "Refers to a single person being addressed, typically used by a caregiver or in a patronizing way.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nurses_we",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Did we manage to wash <w>ourselves</w> this morning?",
                            "target_lang_translations": {
                                "ru": "Мы смогли помыть <w>себя</w> сегодня утром?"
                            }
                        },
                        {
                            "text": "Have we hurt <w>ourselves</w> while playing outside?",
                            "target_lang_translations": {
                                "ru": "Мы не поранили <w>себя</w>, играя на улице?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used by caregivers or to children"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относится к одному человеку, к которому обращаются (обычно используется воспитателями или в покровительственном тоне)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "в обращении к пациенту или ребенку"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
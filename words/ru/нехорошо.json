    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0901ebfa-b3d8-3f74-a81a-adf5a454b2d3",
                    "sense_definition": "Плохо, предосудительно или не так, как следует.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "manner_evaluation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты поступил очень <w>нехорошо</w>, когда не предупредил нас о своём отъезде.",
                            "target_lang_translations": {
                                "en": "You acted very <w>badly</w> when you didn't warn us about your departure."
                            }
                        },
                        {
                            "text": "Обманывать друзей — это <w>нехорошо</w> и может испортить отношения.",
                            "target_lang_translations": {
                                "en": "Deceiving friends is <w>wrong</w> and can ruin relationships."
                            }
                        }
                    ],
                    "synonyms": [
                        "плохо",
                        "дурно"
                    ],
                    "antonyms": [
                        "хорошо"
                    ],
                    "common_phrases": [
                        "поступить нехорошо",
                        "вести себя нехорошо",
                        "получилось нехорошо"
                    ],
                    "target_lang_definitions": {
                        "en": "Badly, reprehensibly, or not as one should."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "badly",
                                "target_lang_sense_clarification": "referring to behavior or actions"
                            },
                            {
                                "target_lang_word": "wrong",
                                "target_lang_sense_clarification": "morally incorrect or inappropriate"
                            },
                            {
                                "target_lang_word": "not good",
                                "target_lang_sense_clarification": "general negative evaluation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8a4e580-e844-3dc5-a974-e7ec3c4e2955",
                    "sense_definition": "О плохом физическом самочувствии или болезненном состоянии.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгой поездки в автобусе мне стало <w>нехорошо</w>.",
                            "target_lang_translations": {
                                "en": "After the long bus ride, I started feeling <w>sick</w>."
                            }
                        },
                        {
                            "text": "Если тебе <w>нехорошо</w>, лучше останься сегодня дома и отдохни.",
                            "target_lang_translations": {
                                "en": "If you feel <w>unwell</w>, it's better to stay home today and rest."
                            }
                        }
                    ],
                    "synonyms": [
                        "плохо",
                        "дурно",
                        "худо"
                    ],
                    "antonyms": [
                        "хорошо"
                    ],
                    "common_phrases": [
                        "мне нехорошо",
                        "стало нехорошо",
                        "сделалось нехорошо"
                    ],
                    "target_lang_definitions": {
                        "en": "About bad physical feeling or a sickly state."
                    },
                    "translations": {
                        "en": [
                            {

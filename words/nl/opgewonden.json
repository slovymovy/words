    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d8094287-effd-3d23-aa39-2baa96736013",
                    "sense_definition": "In een toestand van verhoogde emotie, spanning of enthousiasme.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen waren erg <w>opgewonden</w> over het naderende schoolreisje.",
                            "target_lang_translations": {
                                "en": "The children were very <w>excited</w> about the upcoming school trip.",
                                "ru": "Дети были очень <w>возбуждены</w> предстоящей школьной поездкой."
                            }
                        },
                        {
                            "text": "Hij vertelde <w>opgewonden</w> over zijn nieuwe baan en de kansen die hij kreeg.",
                            "target_lang_translations": {
                                "en": "He spoke <w>excitedly</w> about his new job and the opportunities he was getting.",
                                "ru": "Он <w>взволнованно</w> рассказывал о своей новой работе и возможностях, которые он получил."
                            }
                        }
                    ],
                    "synonyms": [
                        "enthousiast",
                        "gespannen",
                        "uitgelaten"
                    ],
                    "antonyms": [
                        "kalm",
                        "rustig",
                        "onbewogen"
                    ],
                    "common_phrases": [
                        "opgewonden raken",
                        "opgewonden praten"
                    ],
                    "target_lang_definitions": {
                        "en": "In a state of heightened emotion, tension, or enthusiasm.",
                        "ru": "В состоянии повышенной эмоции, напряжения или энтузиазма."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "excited",
                                "target_lang_sense_clarification": "feeling very happy and enthusiastic"
                            },
                            {
                                "target_lang_word": "thrilled",
                                "target_lang_sense_clarification": "extremely pleased or excited"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "возбуждённый",
                                "target_lang_sense_clarification": "находящийся в состоянии возбуждения, подъема"
                            },
                            {
                                "target_lang_word": "взволнованный",
                                "target_lang_sense_clarification": "охваченный волнением, переживанием"
                            },
                            {
                                "target_lang_word": "воодушевлённый",
                                "target_lang_sense_clarification": "полный энтузиазма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10e7ea61-3aa9-35d4-8ef0-6526538e4de7",
                    "sense_definition": "Onrustig, zenuwachtig of geïrriteerd door een bepaalde gebeurtenis.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De menigte reageerde <w>opgewonden</w> toen de wedstrijd werd afgelast.",
                            "target_lang_translations": {
                                "en": "The crowd reacted <w>agitatedly</w> when the match was cancelled.",
                                "ru": "Толпа отреагировала <w>возбуждённо</w>, когда матч отменили."
                            }
                        },
                        {
                            "text": "Doe niet zo <w>opgewonden</w> en probeer rustig te luisteren naar wat ik zeg.",
                            "target_lang_translations": {
                                "en": "Don't get so <w>worked up</w> and try to listen calmly to what I'm saying.",
                                "ru": "Не будь таким <w>взвинченным</w> и попытайся спокойно выслушать то, что я говорю."
                            }
                        }
                    ],
                    "synonyms": [
                        "onrustig",
                        "zenuwachtig",
                        "geprikkeld",
                        "driftig"
                    ],
                    "antonyms": [
                        "bedaard",
                        "ontspannen",
                        "beheerst"
                    ],
                    "common_phrases": [
                        "opgewonden standje",
                        "opgewonden reageren"
                    ],
                    "target_lang_definitions": {
                        "en": "Restless, nervous, or irritated by a specific event.",
                        "ru": "Беспокойный, нервный или раздраженный из-за определенного события."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "agitated",
                                "target_lang_sense_clarification": "feeling or appearing troubled or nervous"
                            },
                            {
                                "target_lang_word": "worked up",
                                "target_lang_sense_clarification": "upset or very excited, usually negatively"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "взвинченный",
                                "target_lang_sense_clarification": "в состоянии нервного возбуждения, раздражения"
                            },
                            {
                                "target_lang_word": "раздражённый",
                                "target_lang_sense_clarification": "сердитый, недовольный"
                            }
                        ]
                    }
                }
            ]
        },

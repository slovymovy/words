{
    "word_family": [
        "flotowy",
        "flotylla"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a6ed2751-44bc-3308-a604-cffd1e96dfe8",
                    "sense_definition": "Ogół statków wodnych (handlowych, rybackich lub wojennych) należących do jednego państwa, armatora lub przedsiębiorstwa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transport_marine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polska <flota> handlowa przewozi towary do portów na całym świecie.",
                            "target_lang_translations": {
                                "ru": "Польский торговый <флот> перевозит товары в порты по всему миру."
                            }
                        },
                        {
                            "text": "W porcie wojennym stacjonuje potężna <flota> gotowa do obrony wybrzeża.",
                            "target_lang_translations": {
                                "ru": "В военном порту базируется мощный <флот>, готовый к обороне побережья."
                            }
                        }
                    ],
                    "synonyms": [
                        "marynarka"
                    ],
                    "common_phrases": [
                        "flota handlowa",
                        "flota wojenna",
                        "flota rybacka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность судов (торговых, рыболовных или военных), принадлежащих одному государству, судовладельцу или предприятию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "флот",
                                "target_lang_sense_clarification": "морской, торговый или военный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c231c3f-2f9d-3d5d-a982-8c63093c690c",
                    "sense_definition": "Zbiór pojazdów (samochodów, autobusów, samolotów) będących własnością jednej firmy lub organizacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_logistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza firma wymieniła całą <flotę> samochodową na modele elektryczne.",
                            "target_lang_translations": {
                                "ru": "Наша фирма заменила весь автомобильный <парк> на электрические модели."
                            }
                        },
                        {
                            "text": "Zarządzanie dużą <flotą> pojazdów wymaga skomplikowanego systemu logistycznego.",
                            "target_lang_translations": {
                                "ru": "Управление большим <парком> транспортных средств требует сложной логистической системы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "flota samochodowa",
                        "flota pojazdów",
                        "zarządzanie flotą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа транспортных средств (автомобилей, автобусов, самолетов), находящихся в собственности одной компании или организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парк",
                                "target_lang_sense_clarification": "транспортных средств, автопарк"
                            },
                            {
                                "target_lang_word": "флот",
                                "target_lang_sense_clarification": "воздушный (самолетов)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6590aff4-7adb-3a76-b6c6-2b0faff5f91c",
                    "sense_definition": "Potoczne określenie na pieniądze lub gotówkę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "money_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie pójdę z wami do kina, bo nie mam dzisiaj żadnej <floty>.",
                            "target_lang_translations": {
                                "ru": "Я не пойду с вами в кино, потому что у меня сегодня нет никаких <денег>."
                            }
                        },
                        {
                            "text": "Muszę szybko zarobić trochę <floty>, żeby naprawić zepsuty samochód.",
                            "target_lang_translations": {
                                "ru": "Мне нужно быстро заработать немного <денег>, чтобы починить сломанную машину."
                            }
                        }
                    ],
                    "synonyms": [
                        "kasa",
                        "forsa",
                        "hajs",
                        "gotówka",
                        "szmal"
                    ],
                    "common_phrases": [
                        "mieć flotę",
                        "brak floty"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used informally among young people or in specific groups"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сленговое выражение, обозначающее деньги или наличные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "деньги",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "бабки",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "бабло",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]

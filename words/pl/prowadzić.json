    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "702e256f-88e8-3852-b20f-d958e9893fe1",
                    "sense_definition": "Kierować pojazdem mechanicznym, obsługiwać mechanizmy sterowania w trakcie jazdy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój starszy brat uczy się <w>prowadzić</w> samochód osobowy.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат учится <w>водить</w> легковую машину."
                            }
                        },
                        {
                            "text": "Nigdy nie <w>prowadzę</w> auta po wypiciu alkoholu.",
                            "target_lang_translations": {
                                "ru": "Я никогда не <w>вожу</w> машину после употребления алкоголя."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierować"
                    ],
                    "common_phrases": [
                        "prowadzić samochód",
                        "prowadzić pojazd",
                        "uczyć się prowadzić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Управлять транспортным средством, использовать механизмы управления во время движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водить",
                                "target_lang_sense_clarification": "иметь навык или делать это регулярно"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "управлять в данный момент"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "официальный или технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11d16cd2-68d9-31c3-b5b6-de304d759f8a",
                    "sense_definition": "Wskazywać drogę, idąc z kimś lub przed kimś; być czyimś przewodnikiem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "guiding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przewodnik <w>prowadził</w> grupę turystów na szczyt góry bezpiecznym szlakiem.",
                            "target_lang_translations": {
                                "ru": "Гид <w>вёл</w> группу туристов на вершину горы по безопасному маршруту."
                            }
                        },
                        {
                            "text": "Nauczycielka <w>prowadziła</w> dzieci za rękę przez ruchliwą ulicę.",
                            "target_lang_translations": {
                                "ru": "Учительница <w>вела</w> детей за руку через оживлённую улицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "wieść",
                        "wyprowadzać"
                    ],
                    "common_phrases": [
                        "prowadzić za rękę",
                        "prowadzić do wyjścia",
                        "prowadzić na górę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывать путь, идя вместе с кем-то или впереди; быть чьим-то проводником."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "сопровождать, указывая дорогу"
                            },
                            {
                                "target_lang_word": "провожать",
                                "target_lang_sense_clarification": "сопровождать до определенного места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4b721d0-e721-3e89-83bb-75213db26925",
                    "sense_definition": "O drodze, ścieżce lub korytarzu: biec w określonym kierunku, umożliwiać dotarcie do jakiegoś miejsca.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "guiding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta wąska i kręta ścieżka <w>prowadzi</w> prosto do naszego domu.",
                            "target_lang_translations": {
                                "ru": "Эта узкая и извилистая тропинка <w>ведёт</w> прямо к нашему дому."
                            }
                        },
                        {
                            "text": "Główna droga <w>prowadzi</w> do centrum miasta i jest bardzo ruchliwa.",
                            "target_lang_translations": {
                                "ru": "Главная дорога <w>ведёт</w> в центр города, и она очень оживлённая."
                            }
                        }
                    ],
                    "synonyms": [
                        "biec",
                        "wieść"
                    ],
                    "common_phrases": [
                        "droga prowadzi do",
                        "ścieżka prowadzi przez",
                        "drzwi prowadzą do"
                    ],
                    "target_lang_definitions": {
                        "ru": "О дороге, тропинке или коридоре: пролегать в определённом направлении, давать доступ к какому-либо месту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "иметь направление куда-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da80f694-c124-344e-987e-412e6c761f3e",
                    "sense_definition": "Zarządzać czymś, nadzorować przebieg jakichś działań, instytucji lub grupy ludzi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "management",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek od wielu lat z sukcesem <w>prowadzi</w> własną firmę budowlaną.",
                            "target_lang_translations": {
                                "ru": "Мой дядя много лет успешно <w>ведёт</w> собственную строительную фирму."
                            }
                        },
                        {
                            "text": "Profesor uniwersytetu <w>prowadzi</w> ciekawe wykłady z historii sztuki.",
                            "target_lang_translations": {
                                "ru": "Профессор университета <w>ведёт</w> интересные лекции по истории искусств."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierować",
                        "zarządzać",
                        "przewodzić"
                    ],
                    "common_phrases": [
                        "prowadzić firmę",
                        "prowadzić zajęcia",
                        "prowadzić badania",
                        "prowadzić spotkanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Руководить чем-либо, управлять ходом действий, организацией или группой людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "заниматься делами, бизнесом, собранием"
                            },
                            {
                                "target_lang_word": "руководить",
                                "target_lang_sense_clarification": "быть во главе людей или организации"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "осуществлять контроль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91b868de-13b3-31ef-92f9-490b802f661f",
                    "sense_definition": "Być przyczyną czegoś; zmierzać do określonego skutku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Brak regularnego snu i stres często <w>prowadzą</w> do problemów ze zdrowiem.",
                            "target_lang_translations": {
                                "ru": "Недостаток сна и стресс часто <w>приводят</w> к проблемам со здоровьем."
                            }
                        },
                        {
                            "text": "Twoje agresywne zachowanie <w>prowadzi</w> do niepotrzebnych kłótni w rodzinie.",
                            "target_lang_translations": {
                                "ru": "Твоё агрессивное поведение <w>ведёт</w> к ненужным ссорам в семье."
                            }
                        }
                    ],
                    "synonyms": [
                        "powodować",
                        "skutkować"
                    ],
                    "common_phrases": [
                        "prowadzić do celu",
                        "prowadzić do wniosku",
                        "prowadzić do konfliktu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть причиной чего-либо; иметь что-либо своим следствием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приводить",
                                "target_lang_sense_clarification": "к результату или последствиям"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "иметь своим следствием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01213138-81cc-394c-81e7-157404590129",
                    "sense_definition": "Wykonywać czynności w określony sposób lub trwające przez dłuższy czas (np. rozmowę, życie, dom).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Staramy się <w>prowadzić</w> zdrowy tryb życia i jemy dużo warzyw.",
                            "target_lang_translations": {
                                "ru": "Мы стараемся <w>вести</w> здоровый образ жизни и едим много овощей."
                            }
                        },
                        {
                            "text": "Trudno jest <w>prowadzić</w> poważną rozmowę w tak głośnym miejscu.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>вести</w> серьёзный разговор в таком шумном месте."
                            }
                        }
                    ],
                    "synonyms": [
                        "wieść"
                    ],
                    "common_phrases": [
                        "prowadzić dom",
                        "prowadzić rozmowę",
                        "prowadzić tryb życia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осуществлять деятельность или процесс в течение длительного времени (например, беседу, образ жизни)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "совершать действие (беседу, образ жизни)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3458faaa-e142-3718-891f-8c8d54365d65",
                    "sense_definition": "Zajmować pierwsze miejsce w rywalizacji sportowej, rankingu lub wyścigu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "competition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza drużyna <w>prowadziła</w> w meczu, ale ostatecznie przegrała w dogrywce.",
                            "target_lang_translations": {
                                "ru": "Наша команда <w>вела</w> в матче, но в итоге проиграла в овертайме."
                            }
                        },
                        {
                            "text": "Polski skoczek narciarski <w>prowadzi</w> w klasyfikacji generalnej pucharu świata.",
                            "target_lang_translations": {
                                "ru": "Польский прыгун с трамплина <w>лидирует</w> в общем зачёте Кубка мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "liderować (zob. uwagi)"
                    ],
                    "common_phrases": [
                        "prowadzić w tabeli",
                        "prowadzić w wyścigu",
                        "prowadzić punktami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занимать первое место в спортивном состязании, рейтинге или гонке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лидировать",
                                "target_lang_sense_clarification": "занимать первое место"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "выигрывать по счёту в моменте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec1ced0f-7918-3642-9d8f-36843f1e2485",
                    "sense_definition": "Systematycznie zapisywać informacje w dokumentach, księgach lub dzienniku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "management",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz systematycznie <w>prowadzić</w> notatki, aby zdać ten trudny egzamin.",
                            "target_lang_translations": {
                                "ru": "Ты должен систематически <w>вести</w> конспекты, чтобы сдать этот трудный экзамен."
                            }
                        },
                        {
                            "text": "Sekretarka <w>prowadzi</w> kalendarz spotkań prezesa bardzo skrupulatnie.",
                            "target_lang_translations": {
                                "ru": "Секретарь очень тщательно <w>ведёт</w> календарь встреч президента."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prowadzić dziennik",
                        "prowadzić księgowość",
                        "prowadzić rejestr"
                    ],
                    "target_lang_definitions": {
                        "ru": "Систематически делать записи в документах, книгах или дневнике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "составлять, заполнять регулярно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f50b3e9-a653-329b-8b27-83ab34ded44a",
                    "sense_definition": "Kierować ruchami partnera lub partnerki w tańcu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "dance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek świetnie tańczy i doskonale <w>prowadzi</w> każdą partnerkę.",
                            "target_lang_translations": {
                                "ru": "Марек отлично танцует и превосходно <w>ведёт</w> любую партнёршу."
                            }
                        },
                        {
                            "text": "W tangu to mężczyzna zazwyczaj <w>prowadzi</w> i decyduje o krokach.",
                            "target_lang_translations": {
                                "ru": "В танго обычно мужчина <w>ведёт</w> и выбирает шаги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prowadzić w tańcu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять движения партнёра или партнёрши в танце."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "управлять в танце"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa539f46-bd28-317c-ab4b-051b16d8f8a7",
                    "sense_definition": "Zachowywać się w określony sposób (często oceniany moralnie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten młody człowiek zaczął się źle <w>prowadzić</w> i wpadł w kłopoty.",
                            "target_lang_translations": {
                                "ru": "Этот молодой человек начал плохо себя <w>вести</w> и попал в неприятности."
                            }
                        },
                        {
                            "text": "Sąsiedzi plotkowali o tym, jak <w>prowadzi</w> się nowa lokatorka.",
                            "target_lang_translations": {
                                "ru": "Соседи сплетничали о том, как <w>ведёт</w> себя новая жиличка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "źle się prowadzić",
                        "dobrze się prowadzić"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "reflexive: prowadzić się"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя определённым образом (часто с моральной оценкой)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести себя",
                                "target_lang_sense_clarification": "о поведении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
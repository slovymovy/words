            "senses": [
                {
                    "sense_id": "bc055f17-ffac-3c65-b7e4-8ddb1a3c17b8",
                    "sense_definition": "Член массовой детской коммунистической организации в СССР и некоторых других странах.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "soviet_org",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Послезавтра на школьном собрании его торжественно примут в <пионеры>.",
                            "target_lang_translations": {
                                "en": "The day after tomorrow, he will be solemnly accepted as a <Young Pioneer> at the school assembly."
                            }
                        },
                        {
                            "text": "Летом все <пионеры> нашего класса поехали отдыхать в лагерь у моря.",
                            "target_lang_translations": {
                                "en": "In the summer, all the <Young Pioneers> from our class went to rest at a camp by the sea."
                            }
                        }
                    ],
                    "synonyms": [
                        "красногалстучник"
                    ],
                    "common_phrases": [
                        "принять в пионеры",
                        "пионерский галстук",
                        "дворец пионеров",
                        "пионерский лагерь"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context (USSR)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A member of the Young Pioneers, a mass youth organization in the Soviet Union and other communist countries."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Young Pioneer",
                                "target_lang_sense_clarification": "member of Soviet youth organization"
                            },
                            {
                                "target_lang_word": "Pioneer",
                                "target_lang_sense_clarification": "short form for Young Pioneer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b904bd04-b75d-3411-983b-98636612a13d",
                    "sense_definition": "Человек, который первым прокладывает новые пути в какой-либо области деятельности, науки или искусства.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "innovator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша компания стала <пионером> в разработке экологически чистого топлива.",
                            "target_lang_translations": {
                                "en": "Our company became a <pioneer> in the development of environmentally friendly fuel."
                            }
                        },
                        {
                            "text": "Великие <пионеры> науки часто сталкивались с непониманием современников.",
                            "target_lang_translations": {
                                "en": "Great <pioneers> of science often faced misunderstanding from their contemporaries."
                            }
                        }
                    ],
                    "synonyms": [
                        "первопроходец",
                        "новатор"
                    ],
                    "antonyms": [
                        "ретроград",
                        "ретроград"
                    ],
                    "common_phrases": [
                        "пионер авиации",
                        "пионер космонавтики",
                        "стать пионером"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who is among the first to explore or develop a new area of knowledge, culture, or activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pioneer",
                                "target_lang_sense_clarification": "innovator in a field"

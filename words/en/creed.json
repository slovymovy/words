                },
                {
                    "sense_id": "33faddc9-867a-3ca6-a569-782a04924a25",
                    "sense_definition": "A male given name.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "<w>Creed</w> is coming to the party later this evening.",
                            "target_lang_translations": {
                                "ru": "<w>Крид</w> придет на вечеринку сегодня вечером."
                            }
                        },
                        {
                            "text": "They decided to name their newborn son <w>Creed</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать своего новорожденного сына <w>Крид</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aebce281-47e8-38f6-b6d6-0a185053cc78",
                    "sense_definition": "Физиологическое состояние отдыха организма, при котором снижается реакция на окружающий мир и прекращается сознательная деятельность.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physiological_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгой прогулки у ребёнка был очень крепкий <w>сон</w>.",
                            "target_lang_translations": {
                                "en": "After a long walk, the child had a very deep <w>sleep</w>."
                            }
                        },
                        {
                            "text": "Перед <w>сном</w> полезно проветрить комнату для свежего воздуха.",
                            "target_lang_translations": {
                                "en": "Before <w>sleep</w>, it is useful to air the room for fresh air."
                            }
                        }
                    ],
                    "synonyms": [
                        "покой",
                        "спокойствие"
                    ],
                    "antonyms": [
                        "бодрствование",
                        "бессонница"
                    ],
                    "common_phrases": [
                        "крепкий сон",
                        "перед сном",
                        "клонит в сон",
                        "дневной сон"
                    ],
                    "target_lang_definitions": {
                        "en": "A physiological state of rest where reaction to the surroundings decreases and conscious activity stops."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sleep",
                                "target_lang_sense_clarification": "state of rest"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5810277-9126-3c1a-91ed-da111e727ed1",
                    "sense_definition": "Образы, события и картины, которые человек видит в своём сознании, когда спит.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вчера ночью мне приснился удивительный и яркий <w>сон</w>.",
                            "target_lang_translations": {
                                "en": "Last night I had an amazing and vivid <w>dream</w>."
                            }
                        },
                        {
                            "text": "Я помню этот странный <w>сон</w> в мельчайших деталях.",
                            "target_lang_translations": {
                                "en": "I remember this strange <w>dream</w> in the smallest details."
                            }
                        }
                    ],
                    "synonyms": [
                        "сновидение",
                        "грёза"
                    ],
                    "antonyms": [
                        "явь"
                    ],
                    "common_phrases": [
                        "видеть сон",
                        "страшный сон",
                        "вещий сон",
                        "дурной сон"
                    ],
                    "target_lang_definitions": {
                        "en": "Images, events, and scenes seen in the mind while sleeping."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dream",
                                "target_lang_sense_clarification": "images during sleep"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c55fd79-acf1-3cd6-85f6-5d98a24536c3",
                    "sense_definition": "Нечто прекрасное, но несбыточное или недолговечное, похожее на фантазию.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor_illusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наше короткое путешествие пролетело как волшебный <w>сон</w>.",
                            "target_lang_translations": {
                                "en": "Our short trip flew by like a magical <w>dream</w>."
                            }
                        },
                        {
                            "text": "Всё происходящее казалось мне не реальностью, а странным <w>сном</w>.",
                            "target_lang_translations": {
                                "en": "Everything happening seemed to me not reality, but a strange <w>dream</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "мечта",
                        "иллюзия"
                    ],
                    "antonyms": [
                        "действительность"
                    ],
                    "common_phrases": [
                        "как во сне",
                        "это был сон"
                    ],
                    "target_lang_definitions": {
                        "en": "Something beautiful but unreal or short-lived, similar to a fantasy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dream",
                                "target_lang_sense_clarification": "fantasy or illusion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04bb550e-b083-3c53-95a0-4caaf45b10fe",
                    "sense_definition": "Состояние застоя, бездействия, отсутствия развития и перемен в обществе или жизни.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor_stagnation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Страна наконец пробудилась от векового политического <w>сна</w>.",
                            "target_lang_translations": {
                                "en": "The country finally awoke from a centuries-old political <w>slumber</w>."
                            }
                        },
                        {
                            "text": "Мы не хотим возвращаться в этот серый <w>сон</w> и бездействие.",
                            "target_lang_translations": {
                                "en": "We do not want to return to this grey <w>slumber</w> and inaction."
                            }
                        }
                    ],
                    "synonyms": [
                        "застой",
                        "однообразие",
                        "серость",
                        "бездействие"
                    ],
                    "antonyms": [
                        "движение",
                        "разнообразие"
                    ],
                    "common_phrases": [
                        "пробудиться ото сна",
                        "вечный сон"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of stagnation, inaction, or lack of development in society or life."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slumber",
                                "target_lang_sense_clarification": "metaphorical inactivity"
                            },
                            {
                                "target_lang_word": "sleep",
                                "target_lang_sense_clarification": "metaphorical state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53a9ce95-fd50-3efd-a250-553d56da559a",
                    "sense_definition": "Состояние полной тишины и глубокого покоя в природе.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимний лес погрузился в глубокий и холодный <w>сон</w>.",
                            "target_lang_translations": {
                                "en": "The winter forest plunged into a deep and cold <w>slumber</w>."
                            }
                        },
                        {
                            "text": "Ночной <w>сон</w> природы нарушал только тихий ветер.",
                            "target_lang_translations": {
                                "en": "Nature's night <w>sleep</w> was disturbed only by a quiet wind."
                            }
                        }
                    ],
                    "synonyms": [
                        "тишина",
                        "покой"
                    ],
                    "antonyms": [
                        "суматоха"
                    ],
                    "common_phrases": [
                        "погрузиться в сон",
                        "сном объята"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of complete silence and deep peace in nature."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slumber",
                                "target_lang_sense_clarification": "deep rest in nature"
                            },
                            {
                                "target_lang_word": "sleep",
                                "target_lang_sense_clarification": "state of quietness"
                            }
                        ]
                    }
                }
            ]

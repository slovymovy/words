                        "similar"
                    ],
                    "common_phrases": [
                        "polar opposite"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся крайними противоположностями; совершенно разный или противоречивый."
                    },
                    "translations": {

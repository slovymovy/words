{
    "word_family": [
        "enable",
        "enabler",
        "enablement",
        "disable",
        "disablement"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "48ca41e0-305b-3610-ab47-eb7e5f09835a",
                    "sense_definition": "Simple past and past participle of <w>enable</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new software <w>enabled</w> the team to work much faster.",
                            "target_lang_translations": {
                                "ru": "Новое программное обеспечение <w>позволило</w> команде работать намного быстрее."
                            }
                        },
                        {
                            "text": "Her scholarship <w>enabled</w> her to attend the university.",
                            "target_lang_translations": {
                                "ru": "Стипендия <w>дала</w> ей <w>возможность</w> учиться в университете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "enabled me to",
                        "enabled us to",
                        "enabled them to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола enable (делать возможным, позволять)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позволил",
                                "target_lang_sense_clarification": "сделал возможным"
                            },
                            {
                                "target_lang_word": "дал возможность",
                                "target_lang_sense_clarification": "предоставил средства или право"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1606e19e-48f4-3dd2-b328-0615af3df456",
                    "sense_definition": "Being capable for use or action; switched on or functioning.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "capable_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure that cookies are <w>enabled</w> in your web browser.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что файлы cookie <w>включены</w> в вашем веб-браузере."
                            }
                        },
                        {
                            "text": "Once the feature is <w>enabled</w>, you can access the new settings.",
                            "target_lang_translations": {
                                "ru": "Как только функция будет <w>активирована</w>, вы сможете получить доступ к новым настройкам."
                            }
                        }
                    ],
                    "synonyms": [
                        "activated",
                        "operational"
                    ],
                    "antonyms": [
                        "disabled"
                    ],
                    "common_phrases": [
                        "enabled feature",
                        "enabled setting",
                        "enabled device"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовый к использованию или действию; включённый или функционирующий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включённый",
                                "target_lang_sense_clarification": "о функции, настройке или устройстве"
                            },
                            {
                                "target_lang_word": "активированный",
                                "target_lang_sense_clarification": "приведённый в действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63b94347-6e1b-3b88-93d7-aee9456476ae",
                    "sense_definition": "Adapted for use with a specified mechanism, system, or technology.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "capable_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You will need a wifi-<w>enabled</w> laptop to connect to the internet here.",
                            "target_lang_translations": {
                                "ru": "Вам понадобится ноутбук <w>с поддержкой</w> Wi-Fi, чтобы подключиться к интернету здесь."
                            }
                        },
                        {
                            "text": "The company launched a new voice-<w>enabled</w> assistant for smart homes.",
                            "target_lang_translations": {
                                "ru": "Компания выпустила нового помощника для умного дома <w>с функцией</w> голосового управления."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wifi-enabled",
                        "web-enabled",
                        "voice-enabled",
                        "cloud-enabled"
                    ],
                    "target_lang_definitions": {
                        "ru": "Адаптированный для использования с определённым механизмом, системой или технологией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с поддержкой",
                                "target_lang_sense_clarification": "имеющий совместимость с технологией"
                            },
                            {
                                "target_lang_word": "оснащённый",
                                "target_lang_sense_clarification": "имеющий встроенное оборудование"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
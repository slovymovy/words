        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "110044c7-e76b-33ae-be26-a8709f70c75d",
                    "sense_definition": "Een betaalde werkkring of betrekking; werk dat iemand doet om geld te verdienen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "employment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zoekt een nieuwe <w>job</w> in de marketing.",
                            "target_lang_translations": {
                                "en": "He is looking for a new <w>job</w> in marketing.",
                                "ru": "Он ищет новую <w>работу</w> в маркетинге."
                            }
                        },
                        {
                            "text": "Zij heeft een leuke <w>job</w> bij de overheid.",
                            "target_lang_translations": {
                                "en": "She has a nice <w>job</w> with the government.",
                                "ru": "У нее хорошая <w>работа</w> в государственных органах."
                            }
                        }
                    ],
                    "synonyms": [
                        "baan"
                    ],
                    "common_phrases": [
                        "een vaste job",
                        "een leuke job",
                        "parttime job"
                    ],
                    "target_lang_definitions": {
                        "en": "A paid position or employment; work someone does to earn money.",
                        "ru": "Оплачиваемая работа или должность; труд, который кто-то выполняет, чтобы заработать деньги."
                    },
                    "translations": {

                            "comment": "short form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short form of the adjective 'yaryy' (furious, bright, intense)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fierce",
                                "target_lang_sense_clarification": "intense or violent nature"
                            },
                            {
                                "target_lang_word": "bright",
                                "target_lang_sense_clarification": "of a color or light"
                            },
                            {
                                "target_lang_word": "intense",
                                "target_lang_sense_clarification": "extreme force or degree"
                            }
                        ]
                    }
                }
            ]

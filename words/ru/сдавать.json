                    "sense_id": "906e7d8a-563c-3332-aff6-480575e3d3c1",
                    "sense_definition": "Передавать что-либо кому-либо (в организацию, должностному лицу) для хранения, обработки или проверки.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transfer_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пассажиры должны <сдавать> багаж за час до вылета.",
                            "target_lang_translations": {
                                "en": "Passengers must <check> their luggage an hour before departure."
                            }
                        },
                        {
                            "text": "Ученики закончили писать и начали <сдавать> тетради учителю.",
                            "target_lang_translations": {
                                "en": "The students finished writing and started <handing in> their notebooks to the teacher."
                            }
                        }
                    ],
                    "synonyms": [
                        "выдавать",
                        "выдавать"
                    ],
                    "antonyms": [
                        "арендовать",
                        "снимать"
                    ],
                    "common_phrases": [
                        "сдавать багаж",
                        "сдавать документы",
                        "сдавать кровь",
                        "сдавать отчет"
                    ],
                    "target_lang_definitions": {
                        "en": "To hand over something to an official or organization for storage, checking, or processing."
                    },
                    "translations": {
                        "en": [
                            {

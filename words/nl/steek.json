            "senses": [
                {
                    "sense_id": "e86c13cf-5226-3bea-be2a-96ac36f0f96a",
                    "sense_definition": "Een verwonding of handeling waarbij met een scherp voorwerp of door een insect in de huid wordt geprikt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wesp gaf een gemene <w>steek</w> in mijn arm toen ik hem probeerde weg te slaan.",
                            "target_lang_translations": {
                                "en": "The wasp gave a nasty <w>sting</w> in my arm when I tried to swat it away.",
                                "ru": "Оса нанесла мне болезненный <w>укус</w> в руку, когда я пытался её отогнать."
                            }
                        },
                        {
                            "text": "Hij voelde een scherpe <w>steek</w> toen de doorn zijn vinger raakte.",
                            "target_lang_translations": {
                                "en": "He felt a sharp <w>prick</w> when the thorn touched his finger.",
                                "ru": "Он почувствовал острый <w>укол</w>, когда шип коснулся его пальца."
                            }
                        }
                    ],
                    "synonyms": [
                        "prik",
                        "beet"
                    ],
                    "common_phrases": [
                        "een gemene steek",
                        "een steek onder water"
                    ],
                    "target_lang_definitions": {
                        "en": "A wound or action where the skin is pricked by a sharp object or an insect.",
                        "ru": "Рана или действие, при котором насекомое жалит или острый предмет прокалывает кожу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sting",
                                "target_lang_sense_clarification": "caused by an insect"
                            },
                            {
                                "target_lang_word": "stab",
                                "target_lang_sense_clarification": "wound from a knife or sharp weapon"
                            },
                            {
                                "target_lang_word": "prick",
                                "target_lang_sense_clarification": "small puncture from a thorn or needle"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "укус",
                                "target_lang_sense_clarification": "насекомого"
                            },
                            {
                                "target_lang_word": "укол",
                                "target_lang_sense_clarification": "острым предметом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28eb797d-37f5-3d7c-b51c-eead7614413b",
                    "sense_definition": "Een plotselinge, korte en hevige pijn in het lichaam.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het lange rennen kreeg ik last van een vervelende <w>steek</w> in mijn zij.",
                            "target_lang_translations": {
                                "en": "After the long run I suffered from a nasty <w>stitch</w> in my side.",
                                "ru": "После долгого бега у меня возникло неприятное <w>колотьё</w> в боку."
                            }
                        },
                        {
                            "text": "Ze voelde plotseling een felle <w>steek</w> in haar hoofd en moest even gaan zitten.",
                            "target_lang_translations": {
                                "en": "She suddenly felt a sharp <w>stab</w> in her head and had to sit down for a moment.",
                                "ru": "Она внезапно почувствовала резкую <w>боль</w> в голове и была вынуждена присесть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "steek in de zij"
                    ],
                    "target_lang_definitions": {
                        "en": "A sudden, short, and intense pain in the body.",
                        "ru": "Внезапная, кратковременная и острая боль в теле."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stitch",
                                "target_lang_sense_clarification": "sharp pain in the side caused by exercise"

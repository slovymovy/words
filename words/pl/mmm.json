            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "a79f1ee1-bb1b-3515-a508-e58123983cb3",
                    "sense_definition": "Dźwięk wyrażający zadowolenie, przyjemność fizyczną lub zachwyt nad smakiem potrawy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pleasure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Mmm</w>, to ciasto truskawkowe jest po prostu przepyszne.",
                            "target_lang_translations": {
                                "ru": "<w>Ммм</w>, этот клубничный торт просто восхитителен."
                            }
                        },
                        {
                            "text": "Zamknęła oczy i powiedziała cicho <w>mmm</w>, delektując się zapachem kawy.",
                            "target_lang_translations": {
                                "ru": "Она закрыла глаза и тихо произнесла <w>ммм</w>, наслаждаясь ароматом кофе."
                            }
                        }
                    ],
                    "antonyms": [
                        "fuj"
                    ],
                    "common_phrases": [
                        "mmm pycha",
                        "mmm jakie dobre"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, выражающий удовлетворение, физическое удовольствие или восхищение вкусом еды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ммм",
                                "target_lang_sense_clarification": "выражение удовольствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8964ce8e-b36c-3488-a354-fa35efaa7b79",
                    "sense_definition": "Dźwięk wyrażający zamyślenie, wahanie lub proces zastanawiania się nad czymś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "thinking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Mmm</w>, nie jestem pewien, czy to jest właściwe rozwiązanie.",
                            "target_lang_translations": {
                                "ru": "<w>Ммм</w>, я не уверен, правильное ли это решение."
                            }
                        },
                        {
                            "text": "Kiedy zapytałem go o plany na weekend, odpowiedział tylko: <w>mmm</w>, zobaczymy.",
                            "target_lang_translations": {
                                "ru": "Когда я спросил его о планах на выходные, он ответил только: <w>ммм</w>, посмотрим."
                            }
                        }
                    ],
                    "synonyms": [
                        "hm",
                        "hmm"
                    ],
                    "common_phrases": [
                        "mmm no nie wiem",
                        "mmm pomyślmy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, выражающий задумчивость, колебание или процесс размышления над чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ммм",
                                "target_lang_sense_clarification": "выражение задумчивости"
                            },
                            {
                                "target_lang_word": "гм",
                                "target_lang_sense_clarification": "сомнение"
                            },
                            {
                                "target_lang_word": "хм",
                                "target_lang_sense_clarification": "раздумье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5da3c11-47ec-3210-a7fd-9e35b7947688",
                    "sense_definition": "Dźwięk naśladujący ciche nucenie melodii bez otwierania ust.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Siedziała w fotelu i nuciła pod nosem: <w>mmm</w>, <w>mmm</w>.",
                            "target_lang_translations": {
                                "ru": "Она сидела в кресле и напевала себе под нос: <w>ммм</w>, <w>ммм</w>."
                            }
                        },
                        {
                            "text": "Zza ściany dobiegało rytmiczne <w>mmm</w>, jakby ktoś usypiał dziecko.",
                            "target_lang_translations": {
                                "ru": "Из-за стены доносилось ритмичное <w>ммм</w>, словно кто-то убаюкивал ребенка."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, имитирующий тихое напевание мелодии с закрытым ртом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ммм",
                                "target_lang_sense_clarification": "напевание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cfe4c997-5dbb-3231-ae87-e431d0029cc8",
                    "sense_definition": "Short for <w>Santa Claus</w>, the legendary figure who is said to bring gifts to children on Christmas Eve.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "santa_concept",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "The children wrote a long letter to <w>Santa</w> asking for toys.",
                            "target_lang_translations": {
                                "ru": "Дети написали длинное письмо <w>Санте</w> с просьбой об игрушках."
                            }
                        },
                        {
                            "text": "Every year, my uncle dresses up as <w>Santa</w> to surprise the kids.",
                            "target_lang_translations": {
                                "ru": "Каждый год мой дядя наряжается <w>Сантой</w>, чтобы удивить детей."
                            }
                        }
                    ],
                    "synonyms": [
                        "Father Christmas",
                        "Saint Nick",
                        "Saint Nicholas",
                        "Kris Kringle"
                    ],
                    "common_phrases": [
                        "secret Santa",
                        "Santa hat",
                        "letter to Santa",
                        "dear Santa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от Санта-Клаус, легендарный персонаж, который, как говорят, приносит подарки детям в канун Рождества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Санта",
                                "target_lang_sense_clarification": "рождественский персонаж"
                            },
                            {
                                "target_lang_word": "Санта-Клаус",
                                "target_lang_sense_clarification": "полное имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c76ad43-6313-36a0-ab29-b0724ef03baa",
                    "sense_definition": "A female given name of Italian origin.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My great-aunt <w>Santa</w> makes the best pasta sauce in the family.",
                            "target_lang_translations": {
                                "ru": "Моя двоюродная бабушка <w>Санта</w> готовит лучший соус для пасты в семье."
                            }
                        },
                        {
                            "text": "She was named <w>Santa</w> in honor of her Italian grandmother.",
                            "target_lang_translations": {
                                "ru": "Её назвали <w>Санта</w> в честь её итальянской бабушки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Italian"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя итальянского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Санта",
                                "target_lang_sense_clarification": "женское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fb63aff-0a08-33f9-a1a5-6919ff643132",
                    "sense_definition": "A municipality in the province of Ilocos Sur, Philippines.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They traveled to <w>Santa</w> to visit their relatives for the holidays.",
                            "target_lang_translations": {
                                "ru": "Они поехали в <w>Санту</w>, чтобы навестить родственников на праздники."
                            }
                        },
                        {
                            "text": "The town of <w>Santa</w> is located near the coast.",
                            "target_lang_translations": {
                                "ru": "Город <w>Санта</w> расположен недалеко от побережья."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Philippines"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Муниципалитет в провинции Южный Илокос, Филиппины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Санта",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ebe7ee06-e41e-377f-b2c7-5a40d0453884",
                    "sense_definition": "A person dressed up as Santa Claus, often to entertain children.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "santa_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mall <w>Santa</w> listened carefully to what the little girl wanted for Christmas.",
                            "target_lang_translations": {
                                "ru": "<w>Санта</w> в торговом центре внимательно слушал, что маленькая девочка хочет на Рождество."
                            }
                        },
                        {
                            "text": "He was hired to be the <w>Santa</w> at the office holiday party.",
                            "target_lang_translations": {
                                "ru": "Его наняли быть <w>Сантой</w> на офисной вечеринке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mall Santa",
                        "dressed as Santa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, переодетый в Санта-Клауса, часто для развлечения детей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Санта",
                                "target_lang_sense_clarification": "аниматор или актер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf695570-6385-3fd5-a7df-272d3751d9aa",
                    "sense_definition": "An image, figure, doll, or chocolate shaped like Santa Claus.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "santa_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I bought a delicious chocolate <w>Santa</w> for my nephew.",
                            "target_lang_translations": {
                                "ru": "Я купил вкусного шоколадного <w>Санту</w> для своего племянника."
                            }
                        },
                        {
                            "text": "A large plastic <w>Santa</w> waved from the neighbor's front lawn.",
                            "target_lang_translations": {
                                "ru": "Большой пластиковый <w>Санта</w> махал рукой с лужайки соседа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chocolate Santa",
                        "plastic Santa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображение, фигурка, кукла или шоколадка в форме Санта-Клауса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Санта",
                                "target_lang_sense_clarification": "игрушка или фигурка"
                            },
                            {
                                "target_lang_word": "фигурка Санты",
                                "target_lang_sense_clarification": "предмет"
                            }
                        ]
                    }
                }
            ]
        }
    ]

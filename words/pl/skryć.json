    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ce1a6d1e-c980-34ad-aebc-d329426f15e6",
                    "sense_definition": "Umieścić kogoś lub coś w miejscu niewidocznym dla innych; zasłonić coś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hiding_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gęste chmury szybko <skryły> słońce i zrobiło się chłodniej.",
                            "target_lang_translations": {
                                "ru": "Густые облака быстро <скрыли> солнце, и стало прохладнее."
                            }
                        },
                        {
                            "text": "Dziewczyna próbowała <skryć> twarz w dłoniach, aby nikt jej nie rozpoznał.",
                            "target_lang_translations": {
                                "ru": "Девушка пыталась <спрятать> лицо в ладонях, чтобы никто её не узнал."
                            }
                        }
                    ],
                    "synonyms": [
                        "schować",
                        "ukryć",
                        "zasłonić"
                    ],
                    "antonyms": [
                        "odsłonić",
                        "pokazać",
                        "ujawnić"
                    ],
                    "common_phrases": [
                        "skryć twarz",
                        "skryć w dłoniach",
                        "skryć pod płaszczem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить кого-то или что-то в невидимое для других место; заслонить что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрыть",
                                "target_lang_sense_clarification": "сделать невидимым, заслонить собой"
                            },
                            {
                                "target_lang_word": "спрятать",
                                "target_lang_sense_clarification": "убрать в укромное место"
                            },
                            {
                                "target_lang_word": "укрыть",
                                "target_lang_sense_clarification": "накрыть чем-то, спрятать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3219af49-fb76-3445-ad24-45653cb3d487",
                    "sense_definition": "Znaleźć się w miejscu niewidocznym lub bezpiecznym; schronić się.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hiding_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy szybko <skryć> się pod dachem przed nadchodzącą ulewą.",
                            "target_lang_translations": {
                                "ru": "Мы должны быстро <укрыться> под крышей от надвигающегося ливня."
                            }
                        },
                        {
                            "text": "Złodziej zdołał <skryć> się w gęstym lesie, gubiąc pościg.",
                            "target_lang_translations": {
                                "ru": "Вору удалось <скрыться> в густом лесу, оторвавшись от погони."
                            }
                        }
                    ],
                    "synonyms": [
                        "schować się",
                        "ukryć się",
                        "schronić się"
                    ],
                    "antonyms": [
                        "pokazać się",
                        "ujawnić się"
                    ],
                    "common_phrases": [
                        "skryć się w cieniu",
                        "skryć się przed deszczem",
                        "skryć się w sobie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказаться в невидимом или безопасном месте; найти убежище."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрыться",
                                "target_lang_sense_clarification": "найти убежище от опасности или непогоды"
                            },
                            {
                                "target_lang_word": "скрыться",
                                "target_lang_sense_clarification": "исчезнуть из виду, убежать"
                            },
                            {
                                "target_lang_word": "спрятаться",
                                "target_lang_sense_clarification": "залезть в укромное место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91ae98d5-fba7-38c1-b330-bd3b0b4547f3",
                    "sense_definition": "Nie ujawnić swoich myśli, uczuć lub wiedzy na jakiś temat; zachować coś w tajemnicy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hiding_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo wielkiego bólu, starał się <skryć> swoje cierpienie przed rodziną.",
                            "target_lang_translations": {
                                "ru": "Несмотря на сильную боль, он старался <скрыть> свои страдания от семьи."
                            }
                        },
                        {
                            "text": "Nie udało jej się <skryć> zdziwienia, gdy zobaczyła niespodziewany prezent.",
                            "target_lang_translations": {
                                "ru": "Ей не удалось <скрыть> удивления, когда она увидела неожиданный подарок."
                            }
                        }
                    ],
                    "synonyms": [
                        "zataić",
                        "ukryć",
                        "utaić"
                    ],
                    "antonyms": [
                        "wyjawić",
                        "zdradzić",
                        "okazać"
                    ],
                    "common_phrases": [
                        "skryć emocje",
                        "skryć prawdę",
                        "skryć urazę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не раскрывать свои мысли, чувства или знания; сохранить что-то в тайне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрыть",
                                "target_lang_sense_clarification": "не показать чувств, умолчать"
                            },
                            {
                                "target_lang_word": "утаить",
                                "target_lang_sense_clarification": "сохранить в секрете"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "common_phrases": [
                        "loading bay",
                        "cargo bay",
                        "parking bay",
                        "sick bay"
                    ],
                    "target_lang_definitions": {
                        "ru": "Размеченный участок земли или отделение внутри здания или транспортного средства для определенных целей."

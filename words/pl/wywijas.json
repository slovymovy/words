                            },
                            {
                                "target_lang_word": "ловкач",
                                "target_lang_sense_clarification": "изворотливый человек"
                            },
                            {
                                "target_lang_word": "прохиндей",
                                "target_lang_sense_clarification": "жулик, плут"
                            }
                        ]
                    }

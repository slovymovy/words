    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "dcac6143-0b38-37ef-a2a9-5f81d9152318",
                    "sense_definition": "Continuing to exist or occur over a prolonged period, often in a way that is annoying or difficult to deal with.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "lasting_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She went to the doctor because of a <w>persistent</w> cough that had lasted for three weeks.",
                            "target_lang_translations": {
                                "ru": "Она пошла к врачу из-за <w>упорного</w> кашля, который не проходил три недели."
                            }
                        },
                        {
                            "text": "There have been <w>persistent</w> rumors about the factory closing down next year.",
                            "target_lang_translations": {
                                "ru": "Ходят <w>упорные</w> слухи о закрытии фабрики в следующем году."
                            }
                        }
                    ],
                    "synonyms": [
                        "constant",
                        "continuous",
                        "unrelenting",
                        "chronic",
                        "incessant"
                    ],
                    "antonyms": [
                        "temporary",
                        "fleeting",
                        "occasional",
                        "transient"
                    ],
                    "common_phrases": [
                        "persistent rain",
                        "persistent cough",
                        "persistent problem",
                        "persistent pain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжающийся в течение длительного времени, часто раздражающий или трудноустранимый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постоя́нный",
                                "target_lang_sense_clarification": "непрекращающийся"
                            },
                            {
                                "target_lang_word": "неотсту́пный",
                                "target_lang_sense_clarification": "преследующий (о мыслях, боли)"
                            },
                            {
                                "target_lang_word": "упо́рный",
                                "target_lang_sense_clarification": "трудноустранимый (о слухах, кашле)"
                            },
                            {
                                "target_lang_word": "усто́йчивый",
                                "target_lang_sense_clarification": "стабильный, не поддающийся изменениям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cda0d9ec-54ed-3549-9d3c-6c524da86d33",
                    "sense_definition": "Refusing to give up or stop doing something; determined and often stubborn.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "determined_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>persistent</w> journalist finally got the interview after asking for months.",
                            "target_lang_translations": {
                                "ru": "<w>Настойчивый</w> журналист наконец получил интервью после нескольких месяцев просьб."
                            }
                        },
                        {
                            "text": "He was a <w>persistent</w> offender who had been arrested multiple times for the same crime.",
                            "target_lang_translations": {
                                "ru": "Он был <w>злостным</w> нарушителем, которого арестовывали много раз за одно и то же преступление."
                            }
                        }
                    ],
                    "synonyms": [
                        "determined",
                        "tenacious",
                        "stubborn",
                        "obstinate",
                        "resolute"
                    ],
                    "antonyms": [
                        "yielding",
                        "irresolute",
                        "quitting"
                    ],
                    "common_phrases": [
                        "persistent offender",
                        "persistent critic",
                        "persistent effort"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказывающийся сдаваться или прекращать действия; решительный, часто упрямый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "насто́йчивый",
                                "target_lang_sense_clarification": "добивающийся своего"
                            },
                            {
                                "target_lang_word": "упо́рный",
                                "target_lang_sense_clarification": "твердый в намерениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ca50aaa-78a6-31a5-998b-96ea40ef6edc",
                    "sense_definition": "Relating to data that remains available after the program that created it has finished running.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The application uses <w>persistent</w> cookies to remember your login details for the next visit.",
                            "target_lang_translations": {
                                "ru": "Приложение использует <w>постоянные</w> файлы cookie, чтобы запомнить ваши данные для входа к следующему визиту."
                            }
                        },
                        {
                            "text": "Hard drives provide <w>persistent</w> storage, ensuring files are saved even when the power is off.",
                            "target_lang_translations": {
                                "ru": "Жесткие диски обеспечивают <w>постоянное</w> хранение данных, гарантируя сохранение файлов даже при отключении питания."
                            }
                        }
                    ],
                    "synonyms": [
                        "non-volatile"
                    ],
                    "antonyms": [
                        "volatile",
                        "transient"
                    ],
                    "common_phrases": [
                        "persistent data",
                        "persistent storage",
                        "persistent connection"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к данным, которые сохраняются после завершения работы программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постоя́нный",
                                "target_lang_sense_clarification": "сохраняемый, энергонезависимый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20bf7583-f484-3a25-9a0f-305b7c8c6782",
                    "sense_definition": "Remaining attached to the plant instead of falling off, usually for longer than is normal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Some oak trees have <w>persistent</w> leaves that stay on the branches throughout the winter.",
                            "target_lang_translations": {
                                "ru": "У некоторых дубов есть <w>неопадающие</w> листья, которые остаются на ветках всю зиму."
                            }
                        },
                        {
                            "text": "Pine cones typically have <w>persistent</w> scales that do not drop off easily.",
                            "target_lang_translations": {
                                "ru": "У сосновых шишек обычно есть <w>неопадающие</w> чешуйки, которые не отваливаются легко."
                            }
                        }
                    ],
                    "antonyms": [
                        "deciduous",
                        "caducous"
                    ],
                    "common_phrases": [
                        "persistent leaves",
                        "persistent scales"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "botany"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Остающийся на растении, не опадающий (обычно дольше, чем в норме)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неопада́ющий",
                                "target_lang_sense_clarification": "ботанический термин"
                            },
                            {
                                "target_lang_word": "сохраня́ющийся",
                                "target_lang_sense_clarification": "остающийся на месте"
                            }

{
    "word_family": [
        "dzwon",
        "dzwonnica",
        "dzwonić",
        "dzwonek",
        "dzwonkowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c3300f2a-1b4a-309d-8b43-1244a6151b3e",
                    "sense_definition": "Osoba, której zadaniem jest ręczne wprawianie w ruch dzwonów, zazwyczaj w wieży kościelnej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <dzwonnik> każdego dnia w południe wchodził na wieżę, by ogłosić godzinę.",
                            "target_lang_translations": {
                                "ru": "Старый <звонарь> каждый день в полдень поднимался на башню, чтобы отбить время."
                            }
                        },
                        {
                            "text": "W małych parafiach funkcję <dzwonnika> pełni często kościelny.",
                            "target_lang_translations": {
                                "ru": "В небольших приходах обязанности <звонаря> часто выполняет церковный сторож."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Dzwonnik z Notre-Dame"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, в обязанности которого входит звонить в колокола, обычно на церковной колокольне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонарь",
                                "target_lang_sense_clarification": "церковный служитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "413e872f-4820-3582-9f5e-9315f396d0cd",
                    "sense_definition": "Ptak z rodzaju Procnias, zamieszkujący lasy tropikalne, wydający głośne dźwięki przypominające uderzenia w metal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W gęstwinie dżungli rozległ się donośny głos <dzwonnika>.",
                            "target_lang_translations": {
                                "ru": "В гуще джунглей раздался громкий голос <звонаря>."
                            }
                        },
                        {
                            "text": "Ornitolodzy obserwowali zachowanie godowe samca <dzwonnika>.",
                            "target_lang_translations": {
                                "ru": "Орнитологи наблюдали за брачным поведением самца <звонаря>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Птица из рода Procnias, обитающая в тропических лесах и издающая громкие звуки, напоминающие удары по металлу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонарь",
                                "target_lang_sense_clarification": "птица"
                            },
                            {
                                "target_lang_word": "птица-колокольчик",
                                "target_lang_sense_clarification": "орнитология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87f9d811-fff2-3b77-a1cf-80ac0f22d4fe",
                    "sense_definition": "Muzyk specjalizujący się w grze na dzwonach lub carillonie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas festiwalu wystąpił znany <dzwonnik>, wykonując utwory klasyczne na wieżowym instrumencie.",
                            "target_lang_translations": {
                                "ru": "На фестивале выступил известный <звонарь>, исполнивший классические произведения на башенном инструменте."
                            }
                        },
                        {
                            "text": "Gra <dzwonnika> wymaga nie tylko siły fizycznej, ale i wielkiej precyzji.",
                            "target_lang_translations": {
                                "ru": "Игра <звонаря> требует не только физической силы, но и огромной точности."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкант, специализирующийся на игре на колоколах или карильоне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонарь",
                                "target_lang_sense_clarification": "музыкант"
                            },
                            {
                                "target_lang_word": "карильонист",
                                "target_lang_sense_clarification": "исполнитель на карильоне"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
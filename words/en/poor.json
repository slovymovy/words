    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2827b164-d0ba-3764-b4a9-69dcb955cd7e",
                    "sense_definition": "Having very little money or few possessions; not having enough money for basic needs.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "financial_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We were so <w>poor</w> that we could not afford to buy new shoes.",
                            "target_lang_translations": {
                                "ru": "Мы были так <w>бедны</w>, что не могли позволить себе купить новую обувь."
                            }
                        },
                        {
                            "text": "Many <w>poor</w> families struggle to pay for heating during the winter.",
                            "target_lang_translations": {
                                "ru": "Многие <w>бедные</w> семьи с трудом оплачивают отопление зимой."
                            }
                        }
                    ],
                    "synonyms": [
                        "destitute",
                        "impoverished",
                        "needy",
                        "penniless",
                        "broke"
                    ],
                    "antonyms": [
                        "rich",
                        "wealthy"
                    ],
                    "common_phrases": [
                        "poor family",
                        "poor country",
                        "dirt poor",
                        "poor people"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий очень мало денег или имущества; не имеющий достаточно средств для основных нужд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бедный",
                                "target_lang_sense_clarification": "не имеющий денег"
                            },
                            {
                                "target_lang_word": "нищий",
                                "target_lang_sense_clarification": "крайне бедный, неимущий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "197bedad-4c7b-3f61-868d-14da6b2aa3c6",
                    "sense_definition": "Of low quality or standard; not good.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team gave a very <w>poor</w> performance in yesterday's game.",
                            "target_lang_translations": {
                                "ru": "Команда показала очень <w>слабую</w> игру во вчерашнем матче."
                            }
                        },
                        {
                            "text": "Due to the <w>poor</w> air quality, people were advised to stay indoors.",
                            "target_lang_translations": {
                                "ru": "Из-за <w>плохого</w> качества воздуха людям посоветовали оставаться в помещении."
                            }
                        }
                    ],
                    "synonyms": [
                        "inferior",
                        "bad",
                        "substandard"
                    ],
                    "antonyms": [
                        "good",
                        "adequate"
                    ],
                    "common_phrases": [
                        "poor quality",
                        "poor health",
                        "poor performance",
                        "poor condition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Низкого качества или уровня; нехороший."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плохой",
                                "target_lang_sense_clarification": "низкого качества"
                            },
                            {
                                "target_lang_word": "слабый",
                                "target_lang_sense_clarification": "о навыках, исполнении или здоровье"
                            },
                            {
                                "target_lang_word": "скверный",
                                "target_lang_sense_clarification": "очень плохой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "164b3347-2789-3ec3-ba6e-d764430b3e0e",
                    "sense_definition": "Deserving of pity or sympathy; unfortunate.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oh, you <w>poor</w> thing, you look absolutely frozen!",
                            "target_lang_translations": {
                                "ru": "Ох, <w>бедняжка</w>, ты выглядишь совершенно замерзшим!"
                            }
                        },
                        {
                            "text": "The <w>poor</w> man lost his wallet and his keys on the same day.",
                            "target_lang_translations": {
                                "ru": "<w>Бедный</w> мужчина потерял кошелек и ключи в один и тот же день."
                            }
                        }
                    ],
                    "synonyms": [
                        "pitiable",
                        "unfortunate",
                        "unlucky"
                    ],
                    "common_phrases": [
                        "poor thing",
                        "poor man",
                        "my poor head"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заслуживающий жалости или сочувствия; несчастный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бедный",
                                "target_lang_sense_clarification": "вызывающий сочувствие"
                            },
                            {
                                "target_lang_word": "несчастный",
                                "target_lang_sense_clarification": "невезучий, горемычный"
                            },
                            {
                                "target_lang_word": "жалкий",
                                "target_lang_sense_clarification": "вызывающий жалость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d99b11b-d0fb-3337-847d-f328fd877c22",
                    "sense_definition": "Having a small amount of a specific substance or quality.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "deficiency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This type of soil is very <w>poor</w> in nutrients and hard to farm.",
                            "target_lang_translations": {
                                "ru": "Этот тип почвы очень <w>беден</w> питательными веществами, и его трудно возделывать."
                            }
                        },
                        {
                            "text": "Cow's milk is naturally <w>poor</w> in iron compared to other sources.",
                            "target_lang_translations": {
                                "ru": "Коровье молоко от природы <w>бедно</w> железом по сравнению с другими источниками."
                            }
                        }
                    ],
                    "synonyms": [
                        "deficient",
                        "lacking"
                    ],
                    "antonyms": [
                        "rich"
                    ],
                    "common_phrases": [
                        "poor in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий малое количество определенного вещества или качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бедный",
                                "target_lang_sense_clarification": "с низким содержанием чего-либо"
                            },
                            {
                                "target_lang_word": "скудный",
                                "target_lang_sense_clarification": "недостаточный по количеству"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f39d9556-afbd-3943-95ad-1e53b7372fc1",
                    "sense_definition": "Inadequate or insufficient; not enough.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He offered a <w>poor</w> excuse for being late to the meeting.",
                            "target_lang_translations": {
                                "ru": "Он придумал <w>жалкое</w> оправдание своему опозданию на встречу."
                            }
                        },
                        {
                            "text": "The meal was a <w>poor</w> reward for all the hard work we did.",
                            "target_lang_translations": {
                                "ru": "Еда была <w>плохой</w> наградой за всю тяжелую работу, которую мы проделали."
                            }
                        }
                    ],
                    "synonyms": [
                        "insufficient",
                        "meager",
                        "scant"
                    ],
                    "antonyms": [
                        "adequate",
                        "decent"
                    ],
                    "common_phrases": [
                        "poor excuse",
                        "poor substitute"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неадекватный или недостаточный; не хватающий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалкий",
                                "target_lang_sense_clarification": "ничтожный, неубедительный"
                            },
                            {
                                "target_lang_word": "плохой",
                                "target_lang_sense_clarification": "неудовлетворительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f640494-f4de-3d98-b88e-385f5b2a959a",
                    "sense_definition": "Humble and free from pride or arrogance.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scripture says that blessed are the <w>poor</w> in spirit.",
                            "target_lang_translations": {
                                "ru": "В Писании сказано: блаженны <w>нищие</w> духом."
                            }
                        },
                        {
                            "text": "He remained <w>poor</w> in spirit despite his great success and fame.",
                            "target_lang_translations": {
                                "ru": "Он оставался <w>нищим</w> духом, несмотря на свой огромный успех и славу."
                            }
                        }
                    ],
                    "synonyms": [
                        "humble",
                        "meek"
                    ],
                    "common_phrases": [
                        "poor in spirit"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly used in religious or literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Смиренный и лишенный гордости или высокомерия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нищий",
                                "target_lang_sense_clarification": "в библейском выражении 'нищие духом'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9e161795-03eb-34f4-b352-ebf4a6d71255",
                    "sense_definition": "People who have little money or few possessions, considered as a group.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government has introduced new policies to help the <w>poor</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство ввело новую политику помощи <w>бедным</w>."
                            }
                        },
                        {
                            "text": "There is a widening gap between the rich and the <w>poor</w> in many countries.",
                            "target_lang_translations": {
                                "ru": "Во многих странах растет разрыв между богатыми и <w>бедными</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "the needy",
                        "the destitute"
                    ],
                    "antonyms": [
                        "rich",
                        "wealthy"
                    ],
                    "common_phrases": [
                        "the poor",
                        "rich and poor",
                        "help the poor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, имеющие мало денег или имущества, рассматриваемые как группа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бедные",
                                "target_lang_sense_clarification": "слой общества"
                            },
                            {
                                "target_lang_word": "бедняки",
                                "target_lang_sense_clarification": "бедные люди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41cf665f-e96a-3348-8ab7-54a175470981",
                    "sense_definition": "A person who has little money; often used in the plural in a derogatory or humorous way.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spoke arrogantly about how the <w>poors</w> were ruining the neighborhood.",
                            "target_lang_translations": {
                                "ru": "Он высокомерно говорил о том, как <w>нищеброды</w> портят район."
                            }
                        },
                        {
                            "text": "In the story, a <w>poor</w> asked the king for a single coin.",
                            "target_lang_translations": {
                                "ru": "В рассказе <w>бедняк</w> попросил у короля одну монету."
                            }
                        }
                    ],
                    "synonyms": [
                        "pauper"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "modern usage as 'the poors' is often derogatory or ironic"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "usage as a singular count noun 'a poor' is archaic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, у которого мало денег; часто используется во множественном числе в уничижительном или шутливом смысле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нищеброд",
                                "target_lang_sense_clarification": "сленг, уничижительно"
                            },
                            {
                                "target_lang_word": "бедняк",
                                "target_lang_sense_clarification": "устаревшее или книжное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "648089f9-955c-305f-ab66-6b7dcdcc19a5",
                    "sense_definition": "A small fish of the cod family.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fisherman caught a <w>poor</w> cod in his net.",
                            "target_lang_translations": {
                                "ru": "Рыбак поймал <w>люску</w> в свою сеть."
                            }
                        },
                        {
                            "text": "We studied the habitat of the <w>poor</w> cod in marine biology class.",
                            "target_lang_translations": {
                                "ru": "Мы изучали среду обитания <w>люски</w> на уроке морской биологии."
                            }
                        }
                    ],
                    "synonyms": [
                        "poor cod"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкая рыба семейства тресковых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "люска",
                                "target_lang_sense_clarification": "вид рыбы"
                            },
                            {
                                "target_lang_word": "тресочка",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cdd4f7f8-8f8d-34f9-9327-bc6de9382fc4",
                    "sense_definition": "To make someone poor; to impoverish.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy taxes <w>poored</w> the population over several decades.",
                            "target_lang_translations": {
                                "ru": "Тяжелые налоги <w>разорили</w> население за несколько десятилетий."
                            }
                        },
                        {
                            "text": "He argued that the new policies were <w>pooring</w> down the working class.",
                            "target_lang_translations": {
                                "ru": "Он утверждал, что новая политика <w>обедняет</w> рабочий класс."
                            }
                        }
                    ],
                    "synonyms": [
                        "impoverish"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rarely used in modern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать кого-то бедным; лишать средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обеднять",
                                "target_lang_sense_clarification": "делать бедным"
                            },
                            {
                                "target_lang_word": "разорять",
                                "target_lang_sense_clarification": "лишать состояния"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ea6e0ee-70ac-3650-9410-6ddef78e55e5",
                    "sense_definition": "To become poor.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The kingdom <w>poored</w> rapidly after the war destroyed its trade.",
                            "target_lang_translations": {
                                "ru": "Королевство быстро <w>обеднело</w> после того, как война разрушила его торговлю."
                            }
                        },
                        {
                            "text": "As the resources ran out, the community <w>poored</w> further.",
                            "target_lang_translations": {
                                "ru": "По мере истощения ресурсов община <w>беднела</w> дальше."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться бедным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беднеть",
                                "target_lang_sense_clarification": "терять средства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c4974ad-e664-3257-896e-ef3af6008f64",
                    "sense_definition": "To call someone poor.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She resented being <w>poored</w> by her wealthy relatives.",
                            "target_lang_translations": {
                                "ru": "Она возмущалась тем, что богатые родственники <w>считали её бедной</w>."
                            }
                        },
                        {
                            "text": "Don't start <w>pooring</w> me just because I'm careful with money.",
                            "target_lang_translations": {
                                "ru": "Не начинай <w>называть меня нищим</w> только потому, что я бережлив."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "rare and informal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Называть кого-то бедным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называть бедным",
                                "target_lang_sense_clarification": "описание действия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e829fa58-776b-35e5-8c83-786c8e1c1951",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [

{
    "word_family": [
        "wal",
        "omwallen",
        "bewallen",
        "ontwallen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5f49129b-5477-3f49-9a67-c5a09e91bb6f",
                    "sense_definition": "Verdikkingen of zakjes onder de ogen, vaak veroorzaakt door vermoeidheid of ouderdom.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de slapeloze nacht had ze diepe <wallen> onder haar ogen.",
                            "target_lang_translations": {
                                "ru": "После бессонной ночи у нее были глубокие <w>мешки</w> под глазами."
                            }
                        },
                        {
                            "text": "Hij probeerde zijn <wallen> te verbergen met een koude lepel.",
                            "target_lang_translations": {
                                "ru": "Он пытался скрыть свои <w>мешки под глазами</w> с помощью холодной ложки."
                            }
                        }
                    ],
                    "synonyms": [
                        "oogwallen"
                    ],
                    "common_phrases": [
                        "wallen onder de ogen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Припухлости или мешки под глазами, часто вызванные усталостью или возрастом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мешки под глазами",
                                "target_lang_sense_clarification": "отечность лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38a4b84f-4eed-3bb4-a34d-80e3eec12736",
                    "sense_definition": "Meervoud van <w>wal</w>: verhoogde aarden kades of verdedigingswerken rondom een stad of water.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_fortification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De toeristen wandelden over de groene <wallen> van de oude vestingstad.",
                            "target_lang_translations": {
                                "ru": "Туристы гуляли по зеленым <w>валам</w> старого города-крепости."
                            }
                        },
                        {
                            "text": "De schepen lagen veilig aangemeerd tussen de hoge <wallen>.",
                            "target_lang_translations": {
                                "ru": "Корабли были надежно пришвартованы между высокими <w>набережными</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kades",
                        "vestingmuren",
                        "dijken"
                    ],
                    "common_phrases": [
                        "op de wallen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова \"wal\": возвышенные земляные насыпи или оборонительные сооружения вокруг города или у воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "валы",
                                "target_lang_sense_clarification": "оборонительные сооружения"
                            },
                            {
                                "target_lang_word": "насыпи",
                                "target_lang_sense_clarification": "земляные укрепления"
                            },
                            {
                                "target_lang_word": "набережные",
                                "target_lang_sense_clarification": "укрепленные берега"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c6679aeb-34ec-3d92-bbc1-798567279661",
                    "sense_definition": "Een aarden wal opwerpen of een sloot uitdiepen door de grond op de oever te gooien.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_fortification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten moesten de hele nacht <wallen> om het kamp te beschermen.",
                            "target_lang_translations": {
                                "ru": "Солдаты должны были всю ночь <w>возводить валы</w>, чтобы защитить лагерь."
                            }
                        },
                        {
                            "text": "De boer <walde> de sloten om de afwatering te verbeteren.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>обваловывал</w> канавы, чтобы улучшить дренаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "opwerpen",
                        "uitdiepen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly used in historical or technical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возводить земляной вал или углублять канаву, выбрасывая землю на берег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обваловывать",
                                "target_lang_sense_clarification": "окружать валом"
                            },
                            {
                                "target_lang_word": "насыпать вал",
                                "target_lang_sense_clarification": "возводить укрепление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa7fa6e2-ed78-35c5-a53c-783c1a9036ce",
                    "sense_definition": "Omhoog borrelen of koken van een vloeistof.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water in de grote ketel begon te <wallen>.",
                            "target_lang_translations": {
                                "ru": "Вода в большом котле начала <w>бурлить</w>."
                            }
                        },
                        {
                            "text": "De hete bron <walt> onophoudelijk en stoot stoom uit.",
                            "target_lang_translations": {
                                "ru": "Горячий источник непрерывно <w>бьет ключом</w> и выпускает пар."
                            }
                        }
                    ],
                    "synonyms": [
                        "borrelen",
                        "koken",
                        "wellen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "related to 'wellen'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бурлить или кипеть (о жидкости)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бурлить",
                                "target_lang_sense_clarification": "о воде"
                            },
                            {
                                "target_lang_word": "бить ключом",
                                "target_lang_sense_clarification": "об источнике"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
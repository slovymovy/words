{
    "word_family": [
        "market",
        "marketing",
        "marketer",
        "marketable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7109d8a0-db8a-3fad-9e7b-16b715382d73",
                    "sense_definition": "An open area in a town where a public market is held.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bought fresh vegetables at the local <w>marketplace</w> this morning.",
                            "target_lang_translations": {
                                "ru": "Мы купили свежие овощи на местном <w>рынке</w> сегодня утром."
                            }
                        },
                        {
                            "text": "Tourists love to visit the old <w>marketplace</w> in the center of the city.",
                            "target_lang_translations": {
                                "ru": "Туристы любят посещать старую <w>рыночную площадь</w> в центре города."
                            }
                        }
                    ],
                    "synonyms": [
                        "market",
                        "bazaar",
                        "square",
                        "mart"
                    ],
                    "common_phrases": [
                        "busy marketplace",
                        "town marketplace",
                        "local marketplace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытая городская территория, где проходит рыночная торговля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рынок",
                                "target_lang_sense_clarification": "место торговли"
                            },
                            {
                                "target_lang_word": "базар",
                                "target_lang_sense_clarification": "торговые ряды"
                            },
                            {
                                "target_lang_word": "рыночная площадь",
                                "target_lang_sense_clarification": "место расположения рынка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e976bb17-c4bc-3a0f-8aad-25863b379ba4",
                    "sense_definition": "A system or arena, often digital, where goods and services are bought and sold.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "commerce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The internet has created a global <w>marketplace</w> for handmade crafts.",
                            "target_lang_translations": {
                                "ru": "Интернет создал глобальную <w>торговую площадку</w> для изделий ручной работы."
                            }
                        },
                        {
                            "text": "Many companies are moving their business to an online <w>marketplace</w> to reach more customers.",
                            "target_lang_translations": {
                                "ru": "Многие компании переводят свой бизнес на онлайн-<w>маркетплейс</w>, чтобы охватить больше клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "platform",
                        "market"
                    ],
                    "common_phrases": [
                        "online marketplace",
                        "digital marketplace",
                        "global marketplace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система или платформа, часто цифровая, для купли-продажи товаров и услуг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговая площадка",
                                "target_lang_sense_clarification": "место встречи продавцов и покупателей"
                            },
                            {
                                "target_lang_word": "маркетплейс",
                                "target_lang_sense_clarification": "онлайн-платформа"
                            },
                            {
                                "target_lang_word": "рынок",
                                "target_lang_sense_clarification": "сфера сбыта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c154b3cb-daca-35ad-8cfa-cccf88f0c39a",
                    "sense_definition": "The general world of commerce, trade, and economic competition.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "commerce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "New products must be innovative to survive in the competitive <w>marketplace</w>.",
                            "target_lang_translations": {
                                "ru": "Новые продукты должны быть инновационными, чтобы выжить на конкурентном <w>рынке</w>."
                            }
                        },
                        {
                            "text": "The company withdrew the item because it failed to succeed in the <w>marketplace</w>.",
                            "target_lang_translations": {
                                "ru": "Компания отозвала товар, потому что он не имел успеха на <w>рынке</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "economy",
                        "market"
                    ],
                    "common_phrases": [
                        "competitive marketplace",
                        "free marketplace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общая сфера коммерции, торговли и экономической конкуренции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рынок",
                                "target_lang_sense_clarification": "экономическая среда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f44da564-1b2d-35d7-bf46-0d404c731bdd",
                    "sense_definition": "A sphere or environment where non-material things, such as ideas or opinions, are exchanged.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Universities serve as a vital <w>marketplace</w> of ideas for the next generation.",
                            "target_lang_translations": {
                                "ru": "Университеты служат жизненно важным <w>рынком</w> идей для следующего поколения."
                            }
                        },
                        {
                            "text": "In a democracy, different political views compete in the public <w>marketplace</w>.",
                            "target_lang_translations": {
                                "ru": "В демократии различные политические взгляды конкурируют на общественной <w>арене</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "forum",
                        "arena"
                    ],
                    "common_phrases": [
                        "marketplace of ideas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сфера или среда обмена нематериальными ценностями, например идеями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рынок",
                                "target_lang_sense_clarification": "сфера обмена мнениями"
                            },
                            {
                                "target_lang_word": "арена",
                                "target_lang_sense_clarification": "место дискуссий"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
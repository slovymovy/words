{
    "word_family": [
        "attribution",
        "attributable",
        "attributive"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb8a5fb3-d0f9-3a4d-b596-6bb8ff85e356",
                    "sense_definition": "To say or believe that a situation, event, or outcome is the result of a particular thing.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "verb_cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>attributes</w> her success to hard work and dedication."
                        },
                        {
                            "text": "The doctors <w>attribute</w> his quick recovery to the new medicine."
                        }
                    ],
                    "synonyms": [
                        "impute",
                        "qualifier",
                        "lay",
                        "refer"
                    ],
                    "common_phrases": [
                        "attribute to",
                        "attribute success to",
                        "attribute failure to"
                    ]
                },
                {
                    "sense_id": "2da32320-a3a7-3b50-a3f6-1d04113f94ef",
                    "sense_definition": "To say that a work of art, text, or object was created or owned by a particular person.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_authorship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This famous painting is <w>attributed</w> to Leonardo da Vinci."
                        },
                        {
                            "text": "The quote is often <w>attributed</w> to Shakespeare, although he may not have said it."
                        }
                    ],
                    "synonyms": [
                        "refer"
                    ],
                    "common_phrases": [
                        "attributed to",
                        "widely attributed"
                    ]
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "238ef181-cee8-3729-aecd-bb8967ed23b2",
                    "sense_definition": "A quality, characteristic, or feature that someone or something has.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His finest <w>attribute</w> is his kindness towards others."
                        },
                        {
                            "text": "Patience is an essential <w>attribute</w> for a good teacher."
                        }
                    ],
                    "synonyms": [
                        "qualifier"
                    ],
                    "common_phrases": [
                        "positive attribute",
                        "physical attribute",
                        "key attribute",
                        "personal attribute"
                    ]
                },
                {
                    "sense_id": "8cc7676b-0690-34c5-aa28-485182fb9689",
                    "sense_definition": "A specific option, setting, or piece of data belonging to a computer file or object.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The file has a read-only <w>attribute</w> that prevents anyone from editing it."
                        },
                        {
                            "text": "You can change the font <w>attribute</w> in the document settings."
                        }
                    ],
                    "common_phrases": [
                        "file attribute",
                        "data attribute"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ]
                },
                {
                    "sense_id": "21baf3fb-8c15-3dba-845c-2fa80f9d5794",
                    "sense_definition": "A word, especially an adjective, that describes or qualifies a noun.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the phrase 'red apple', the adjective 'red' functions as an <w>attribute</w>."
                        },
                        {
                            "text": "The linguist analyzed the <w>attribute</w> placed before the noun."
                        }
                    ],
                    "synonyms": [
                        "qualifier"

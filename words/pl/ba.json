            "senses": [
                {
                    "sense_id": "7a9ff1ef-7601-3011-8f6d-171ac56904e5",
                    "sense_definition": "Słowo używane potocznie, aby potwierdzić coś w sposób zdecydowany i oczywisty; wyraża całkowitą zgodę lub pewność.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "– Idziesz z nami dzisiaj do kina? – No <w>ba</w>!"
                        },
                        {
                            "text": "– Czy ten film był ciekawy? – <w>Ba</w>, najlepszy jaki w życiu widziałem!"
                        }
                    ],
                    "synonyms": [
                        "no",
                        "jasne",
                        "oczywiście"
                    ],
                    "common_phrases": [

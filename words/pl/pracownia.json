                {
                    "sense_id": "d7299282-59d7-3f8b-ba59-1a2eec613d7e",
                    "sense_definition": "Sala lekcyjna w szkole, wyposażona w sprzęt i pomoce naukowe potrzebne do nauki konkretnego przedmiotu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "school_science_room",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczniowie weszli do <w>pracowni</w> chemicznej, aby przeprowadzić ciekawe doświadczenie.",
                            "target_lang_translations": {
                                "ru": "Ученики вошли в химическую <w>лабораторию</w>, чтобы провести интересный опыт."
                            }
                        },
                        {
                            "text": "Nasza szkoła ma nowoczesną <w>pracownię</w> komputerową z dostępem do internetu.",
                            "target_lang_translations": {
                                "ru": "В нашей школе есть современный компьютерный <w>класс</w> с доступом в интернет."
                            }
                        }
                    ],
                    "synonyms": [
                        "studio",
                        "warsztat"
                    ],
                    "common_phrases": [
                        "pracownia komputerowa",
                        "pracownia językowa",
                        "pracownia biologiczna",
                        "szkolna pracownia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учебный класс в школе, оснащенный оборудованием и пособиями для изучения конкретного предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кабинет",

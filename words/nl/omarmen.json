            "senses": [
                {
                    "sense_id": "9abe1a83-f481-33a0-bde3-4b0363f84400",
                    "sense_definition": "De armen om iemand heen slaan als teken van vriendschap of liefde.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>omarmde</w> zijn geliefde hartstochtelijk toen ze elkaar weer zagen.",
                            "target_lang_translations": {
                                "en": "He passionately <w>embraced</w> his beloved when they saw each other again.",
                                "ru": "Он страстно <w>обнял</w> свою возлюбленную, когда они снова увиделись."
                            }
                        },
                        {
                            "text": "De twee oude vrienden <w>omarmden</w> elkaar stevig op het perron.",
                            "target_lang_translations": {
                                "en": "The two old friends <w>embraced</w> each other tightly on the platform.",
                                "ru": "Два старых друга крепко <w>обняли</w> друг друга на перроне."
                            }
                        }
                    ],
                    "synonyms": [
                        "omhelzen",
                        "knuffelen"
                    ],
                    "antonyms": [
                        "loslaten",
                        "afweren"
                    ],
                    "common_phrases": [
                        "iemand stevig omarmen",
                        "elkaar omarmen"
                    ],
                    "target_lang_definitions": {
                        "en": "To put one's arms around someone as a sign of friendship or love.",
                        "ru": "Обхватить кого-либо руками в знак дружбы или любви."
                    },
                    "translations": {
                        "en": [

{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "6c62b6e2-4099-3b19-bb03-8f63b70219d1",
                    "sense_definition": "Okrzyk wyrażający nagły ból fizyczny.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pain_cry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ała</w>! To jest gorące!",
                            "target_lang_translations": {
                                "ru": "<w>Ай</w>! Это горячее!"
                            }
                        },
                        {
                            "text": "<w>Ała</w>, nie ciągnij mnie za włosy!",
                            "target_lang_translations": {
                                "ru": "<w>Ай</w>, не тяни меня за волосы!"
                            }
                        }
                    ],
                    "synonyms": [
                        "aua!",
                        "au!",
                        "auć!"
                    ],
                    "common_phrases": [
                        "ała, to boli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, выражающее внезапную физическую боль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ай",
                                "target_lang_sense_clarification": "междометие боли"
                            },
                            {
                                "target_lang_word": "ой",
                                "target_lang_sense_clarification": "междометие боли или испуга"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e01ee550-4f0a-3a90-ab53-a2d35698764c",
                    "sense_definition": "Bolące miejsce na ciele, ranka lub skaleczenie (używane w gwarze śląskiej oraz w języku dziecięcym).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sore_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "On mo <w>ała</w> na prawyj szłapie.",
                            "target_lang_translations": {
                                "ru": "У него <w>вавка</w> на правой ноге."
                            }
                        },
                        {
                            "text": "Małe dziecko płakało i pokazywało mamie <w>ała</w> na kolanie.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок плакал и показывал маме <w>бобо</w> на коленке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć ała"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Górny Śląsk"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "język dziecięcy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Больное место на теле, ранка или порез (используется в детской речи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бобо",
                                "target_lang_sense_clarification": "детское слово для обозначения боли или ранки"
                            },
                            {
                                "target_lang_word": "вавка",
                                "target_lang_sense_clarification": "детское название болячки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1a8c5ae-b404-3649-b46d-27db4a00c836",
                    "sense_definition": "Stan bycia niespełna rozumu, głupota lub szaleństwo.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "madness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ty mosz <w>ała</w>!",
                            "target_lang_translations": {
                                "ru": "У тебя <w>сдвиг</w>!"
                            }
                        },
                        {
                            "text": "Chyba masz <w>ała</w>, jeśli myślisz, że się na to zgodzę.",
                            "target_lang_translations": {
                                "ru": "У тебя, наверное, <w>сдвиг</w>, если ты думаешь, что я на это соглашусь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć ała"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Górny Śląsk"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние безумия, глупость или сумасшествие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдвиг",
                                "target_lang_sense_clarification": "разг. сумасшествие, странность"
                            },
                            {
                                "target_lang_word": "придурь",
                                "target_lang_sense_clarification": "разг. глупость, странное поведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ed35ff1-4be7-386c-a2de-be11461db44c",
                    "sense_definition": "Żeński demon z mitologii ludowej południowych Słowian, sprowadzający grad i burze niszczące plony.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mythology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierzono, że <w>ała</w> prowadzi chmury gradowe nad pola uprawne.",
                            "target_lang_translations": {
                                "ru": "Верили, что <w>ала</w> ведет градовые тучи на возделанные поля."
                            }
                        },
                        {
                            "text": "Rolnicy bali się, że mściwa <w>ała</w> zniszczy ich winnice.",
                            "target_lang_translations": {
                                "ru": "Фермеры боялись, что мстительная <w>ала</w> уничтожит их виноградники."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женский демон из народной мифологии южных славян, насылающий град и бури."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ала",
                                "target_lang_sense_clarification": "мифологическое существо"
                            },
                            {
                                "target_lang_word": "хала",
                                "target_lang_sense_clarification": "вариант названия демона"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
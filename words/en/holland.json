            "senses": [
                {
                    "sense_id": "4ee50443-ea2e-325d-95ba-b8519f5e3c67",
                    "sense_definition": "The Netherlands, a country in Western Europe.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geo_location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "My friend is from <w>Holland</w> and speaks Dutch fluently.",
                            "target_lang_translations": {
                                "ru": "Мой друг из <w>Голландии</w> и свободно говорит по-голландски."
                            }
                        },
                        {
                            "text": "We are planning a trip to <w>Holland</w> to see the tulip fields.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку в <w>Голландию</w>, чтобы увидеть тюльпановые поля."
                            }
                        }
                    ],
                    "synonyms": [
                        "the Netherlands"
                    ],
                    "common_phrases": [
                        "from Holland",
                        "living in Holland",
                        "King of Holland"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to refer to the whole country of the Netherlands, though strictly referring only to a specific region"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нидерланды, страна в Западной Европе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Голландия",
                                "target_lang_sense_clarification": "страна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cb2c13c-fe9a-373c-9727-07ca203caeb1",
                    "sense_definition": "A traditional region in the western part of the Netherlands, comprising the modern provinces of North Holland and South Holland.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geo_location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Amsterdam is a major city located in the province of North <w>Holland</w>.",
                            "target_lang_translations": {
                                "ru": "Амстердам — крупный город, расположенный в провинции Северная <w>Голландия</w>."
                            }
                        },
                        {
                            "text": "The historic region of <w>Holland</w> was once the most powerful part of the Dutch Republic.",
                            "target_lang_translations": {
                                "ru": "Историческая область <w>Голландия</w> когда-то была самой могущественной частью Голландской республики."
                            }
                        }
                    ],
                    "common_phrases": [
                        "North Holland",
                        "South Holland",
                        "coast of Holland"
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционная область на западе Нидерландов, включающая современные провинции Северная и Южная Голландия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Голландия",
                                "target_lang_sense_clarification": "регион"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f327b7ed-9552-3aa3-a7a9-2ca6f2c8d54d",

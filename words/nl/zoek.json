                {
                    "sense_id": "447fb2d9-3697-368b-9f48-7909d6e7f45f",
                    "sense_definition": "De handeling of toestand van het speuren naar iets of iemand (vooral in de uitdrukking 'op zoek')",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij zijn dringend op <w>zoek</w> naar nieuwe medewerkers.",
                            "target_lang_translations": {
                                "en": "We are urgently <w>looking for</w> new employees.",
                                "ru": "Мы срочно <w>ищем</w> новых сотрудников."
                            }
                        },
                        {
                            "text": "Hij ging in het bos op <w>zoek</w> naar paddenstoelen.",
                            "target_lang_translations": {

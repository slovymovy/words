                },
                {
                    "sense_id": "f432cd65-ea0f-3539-a4a0-7f2e73eb342b",
                    "sense_definition": "Jednostka miary kąta płaskiego lub współrzędnych geograficznych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obróć się o dziewięćdziesiąt <w>stopni</w> w prawo.",

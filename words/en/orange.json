                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Оранж",
                                "target_lang_sense_clarification": "город во Франции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c8647cc-5b3b-39a4-a317-0739d5f630e6",
                    "sense_definition": "A city or town in various locations, such as California or New South Wales.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place_group",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They moved to <w>Orange</w> to enjoy the sunny California weather.",
                            "target_lang_translations": {
                                "ru": "Они переехали в <w>Ориндж</w>, чтобы насладиться солнечной погодой Калифорнии."
                            }
                        },
                        {
                            "text": "The city of <w>Orange</w> is located in central New South Wales.",
                            "target_lang_translations": {
                                "ru": "Город <w>Ориндж</w> расположен в центральной части Нового Южного Уэльса."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город или населенный пункт в различных местах, например, в Калифорнии или Новом Южном Уэльсе."

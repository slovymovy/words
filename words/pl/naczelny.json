    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "984a7ac6-58d1-32c3-9d81-9f35ccca9b1f",
                    "sense_definition": "Sprawujący najwyższą władzę lub kierownictwo w danej instytucji, organizacji lub grupie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Redaktor <naczelny> podjął ostateczną decyzję o publikacji tego artykułu.",
                            "target_lang_translations": {
                                "ru": "<Главный> редактор принял окончательное решение о публикации этой статьи."
                            }
                        },
                        {
                            "text": "Wódz <naczelny> jest odpowiedzialny za strategię obrony całego kraju.",
                            "target_lang_translations": {
                                "ru": "<Верховный> главнокомандующий отвечает за стратегию обороны всей страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "główny",
                        "zwierzchni",
                        "generalny",
                        "głównodowodzący"
                    ],
                    "common_phrases": [
                        "redaktor naczelny",
                        "wódz naczelny",
                        "dyrektor naczelny",
                        "inżynier naczelny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занимающий высший руководящий пост или осуществляющий верховную власть в организации или группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гла́вный",
                                "target_lang_sense_clarification": "о должности (напр. редактор, инженер)"
                            },
                            {
                                "target_lang_word": "верхо́вный",
                                "target_lang_sense_clarification": "о высшей власти (напр. вождь, суд)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98478c65-83c8-3426-ad0c-c2f0c59393bd",
                    "sense_definition": "Będący najważniejszym elementem czegoś, stanowiący podstawę lub priorytet.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "priority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naszym <naczelnym> celem jest zapewnienie bezpieczeństwa wszystkim pracownikom.",
                            "target_lang_translations": {
                                "ru": "Наша <главная> цель — обеспечение безопасности всех сотрудников."
                            }
                        },
                        {
                            "text": "Wolność słowa jest <naczelną> wartością w każdym demokratycznym państwie.",
                            "target_lang_translations": {
                                "ru": "Свобода слова является <основной> ценностью в любом демократическом государстве."
                            }
                        }
                    ],
                    "synonyms": [
                        "najważniejszy",
                        "pierwszy",
                        "główny"
                    ],
                    "common_phrases": [
                        "cel naczelny",
                        "zasada naczelna",
                        "zadanie naczelne",
                        "wartość naczelna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Являющийся самым важным, приоритетным или основополагающим элементом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гла́вный",
                                "target_lang_sense_clarification": "важнейший"
                            },
                            {
                                "target_lang_word": "основно́й",
                                "target_lang_sense_clarification": "фундаментальный"
                            },
                            {
                                "target_lang_word": "первостепе́нный",
                                "target_lang_sense_clarification": "приоритетный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a8c0f1a5-5073-36df-acba-ad2c2c347635",
                    "sense_definition": "Ssak należący do rzędu naczelnych (Primates), charakteryzujący się wysoko rozwiniętym mózgiem (np. małpa, człowiek).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "zoology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Większość <naczelnych> prowadzi stadny tryb życia w lasach tropikalnych.",
                            "target_lang_translations": {
                                "ru": "Большинство <приматов> ведёт стадный образ жизни в тропических лесах."
                            }
                        },
                        {
                            "text": "Badania genetyczne potwierdzają bliskie pokrewieństwo człowieka z innymi <naczelnymi>.",
                            "target_lang_translations": {
                                "ru": "Генетические исследования подтверждают близкое родство человека с другими <приматами>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rząd naczelnych",
                        "ewolucja naczelnych"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "zoologia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Млекопитающее из отряда приматов, характеризующееся высокоразвитым мозгом (например, обезьяна, человек)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прима́т",

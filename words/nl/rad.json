                {
                    "sense_id": "e5637f10-186d-30ef-9209-78fda6777263",
                    "sense_definition": "Een rond voorwerp dat om een as draait en dient voor voortbeweging of aandrijving van een machine.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "wheel_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het grote <w>rad</w> van de watermolen wordt aangedreven door de rivier.",
                            "target_lang_translations": {
                                "en": "The large <w>wheel</w> of the water mill is driven by the river.",
                                "ru": "Большое <w>колесо</w> водяной мельницы приводится в движение рекой."
                            }
                        },
                        {
                            "text": "Hij voelde zich als het vijfde <w>rad</w> aan de wagen tijdens het feest.",
                            "target_lang_translations": {
                                "en": "He felt like the fifth <w>wheel</w> at the party.",
                                "ru": "На вечеринке он чувствовал себя <w>пятым колесом</w> в телеге."
                            }

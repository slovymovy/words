    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c6a4df0-abd7-39f6-a37d-b44c19ea2097",
                    "sense_definition": "Единомышленник, товарищ по борьбе или общей деятельности, разделяющий те же взгляды и цели.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Президент и его ближайшие <w>соратники</w> обсудили план экономических реформ.",
                            "target_lang_translations": {
                                "en": "The President and his closest <w>associates</w> discussed the economic reform plan."
                            }
                        },
                        {
                            "text": "В этом сложном проекте мне нужны надёжные и преданные делу <w>соратники</w>.",
                            "target_lang_translations": {
                                "en": "In this complex project, I need reliable and dedicated <w>allies</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "единомышленник",
                        "сподвижник"
                    ],
                    "antonyms": [
                        "противник"
                    ],
                    "common_phrases": [
                        "ближайший соратник",
                        "верный соратник",
                        "политический соратник"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who shares the same views and goals, acting as a companion in a struggle, politics, or common activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "associate",
                                "target_lang_sense_clarification": "A partner or colleague in business or politics"
                            },
                            {
                                "target_lang_word": "ally",
                                "target_lang_sense_clarification": "A person who cooperates with another for a common cause"
                            },
                            {
                                "target_lang_word": "comrade",
                                "target_lang_sense_clarification": "A fellow member of an organization or struggle, often political"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ba7323a-4c7b-3876-bce0-aadbd9232ff2",
                    "sense_definition": "Товарищ по оружию, участник совместных битв или военных походов.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старый генерал часто вспоминал своих погибших <w>соратников</w> и их подвиги.",
                            "target_lang_translations": {
                                "en": "The old general often recalled his fallen <w>comrades-in-arms</w> and their heroic deeds."
                            }
                        },
                        {
                            "text": "Он был моим боевым <w>соратником</w>, с которым мы прошли всю войну.",
                            "target_lang_translations": {
                                "en": "He was my <w>comrade-in-arms</w>, with whom I went through the whole war."
                            }
                        }
                    ],
                    "synonyms": [
                        "однополчанин"
                    ],
                    "antonyms": [
                        "неприятель",
                        "противник"
                    ],
                    "common_phrases": [
                        "боевой соратник",
                        "соратник по оружию"
                    ],
                    "target_lang_definitions": {
                        "en": "A companion in battle or military campaigns; a fellow soldier."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "comrade-in-arms",
                                "target_lang_sense_clarification": "A fellow soldier or service member"
                            },
                            {
                                "target_lang_word": "brother-in-arms",
                                "target_lang_sense_clarification": "A close male associate, typically in a military context"
                            },
                            {
                                "target_lang_word": "fellow soldier",
                                "target_lang_sense_clarification": "A soldier fighting in the same group or army"
                            }
                        ]
                    }
                }
            ]
        }
    ]

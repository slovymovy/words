            "pos": "noun",
            "senses": [
                {
                    "sense_id": "765f3b9b-a7a6-3d3a-8c75-e3cb6954e901",
                    "sense_definition": "Miejsce wewnątrz pomieszczenia, w którym stykają się dwie ściany.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niegrzeczny chłopiec musiał za karę stać w <w>kącie</w> przez pięć minut.",
                            "target_lang_translations": {
                                "ru": "Непослушный мальчик должен был в наказание стоять в <w>углу</w> пять минут."
                            }
                        },
                        {
                            "text": "Wstawiłem nową lampę w ciemny <w>kąt</w> salonu, żeby go rozjaśnić.",
                            "target_lang_translations": {
                                "ru": "Я поставил новую лампу в темный <w>угол</w> гостиной, чтобы его осветить."
                            }
                        }
                    ],
                    "synonyms": [
                        "róg",
                        "narożnik"
                    ],
                    "common_phrases": [
                        "stać w kącie",
                        "postawić do kąta",
                        "siedzieć w kącie",
                        "ciemny kąt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место внутри помещения, где сходятся две стены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угол",
                                "target_lang_sense_clarification": "в помещении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "559dc364-680e-3cb8-90a4-b788812cf8e3",
                    "sense_definition": "Figura geometryczna utworzona przez dwie półproste wychodzące z tego samego punktu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trójkąt równoboczny ma każdy <w>kąt</w> równy sześćdziesiąt stopni.",
                            "target_lang_translations": {
                                "ru": "У равностороннего треугольника каждый <w>угол</w> равен шестидесяти градусам."
                            }
                        },
                        {
                            "text": "Dwie główne ulice miasta przecinają się pod <w>kątem</w> prostym.",
                            "target_lang_translations": {
                                "ru": "Две главные улицы города пересекаются под прямым <w>углом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kąt prosty",
                        "kąt ostry",
                        "kąt rozwarty",
                        "pod kątem prostym"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Геометрическая фигура, образованная двумя лучами, выходящими из одной точки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угол",
                                "target_lang_sense_clarification": "геометрическая фигура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "967d2c9d-68f2-321f-8784-48b67b519285",
                    "sense_definition": "Własne mieszkanie lub miejsce zamieszkania, dające poczucie bezpieczeństwa i prywatności.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu latach wynajmowania wreszcie kupili swój własny <w>kąt</w>.",
                            "target_lang_translations": {
                                "ru": "После многих лет аренды они наконец купили свой собственный <w>угол</w>."
                            }
                        },
                        {
                            "text": "Wszędzie jest dobrze, ale we własnych czterech <w>kątach</w> zawsze najlepiej.",
                            "target_lang_translations": {
                                "ru": "Везде хорошо, но в родных четырех <w>стенах</w> всегда лучше всего."
                            }
                        }
                    ],
                    "common_phrases": [
                        "własny kąt",
                        "cztery kąty",
                        "szukać kąta",
                        "nie mieć kąta"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used emotionally"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Собственное жилье или место проживания, дающее чувство безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угол",
                                "target_lang_sense_clarification": "жильё, пристанище"
                            },
                            {
                                "target_lang_word": "жильё",
                                "target_lang_sense_clarification": "дом, квартира"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5dcd09d3-0afb-3e21-9022-c58dc94e1e42",
                    "sense_definition": "Punkt widzenia lub aspekt, pod którym rozpatruje się i ocenia daną sprawę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy dokładnie przeanalizować ten projekt pod <w>kątem</w> opłacalności finansowej.",
                            "target_lang_translations": {
                                "ru": "Мы должны тщательно проанализировать этот проект с <w>точки зрения</w> финансовой рентабельности."
                            }
                        },
                        {
                            "text": "Dziennikarz przedstawił ten problem pod zupełnie innym <w>kątem</w> niż inni.",
                            "target_lang_translations": {
                                "ru": "Журналист представил эту проблему под совершенно иным <w>углом</w>, чем остальные."
                            }
                        }
                    ],
                    "synonyms": [
                        "punkt widzenia",
                        "strona"
                    ],
                    "common_phrases": [
                        "pod kątem",
                        "pod innym kątem",
                        "z tego kąta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точка зрения или аспект, под которым рассматривается и оценивается дело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точка зрения",
                                "target_lang_sense_clarification": "мнение, взгляд"
                            },
                            {
                                "target_lang_word": "ракурс",
                                "target_lang_sense_clarification": "аспект рассмотрения"
                            },
                            {
                                "target_lang_word": "угол",
                                "target_lang_sense_clarification": "в переносном смысле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbedeb87-901d-3823-aff2-08b9318a380e",
                    "sense_definition": "Ustronne, rzadko odwiedzane miejsce lub dowolny zakamarek jakiejś przestrzeni.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas generalnych porządków zajrzeliśmy w każdy <w>kąt</w> naszego strychu.",
                            "target_lang_translations": {
                                "ru": "Во время генеральной уборки мы заглянули в каждый <w>уголок</w> нашего чердака."
                            }
                        },
                        {
                            "text": "Turyści rzadko zaglądają w te dzikie i zapomniane <w>kąty</w> lasu.",
                            "target_lang_translations": {
                                "ru": "Туристы редко заглядывают в эти дикие и забытые <w>уголки</w> леса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "w każdym kącie",
                        "z różnych kątów",
                        "zaglądać w kąty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уединенное, редко посещаемое место или любой закоулок пространства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уголок",
                                "target_lang_sense_clarification": "укромное место"
                            },
                            {
                                "target_lang_word": "закоулок",
                                "target_lang_sense_clarification": "скрытое место"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bfe961a4-d5af-3dd0-8c0a-ae503cc9339e",
                    "sense_definition": "Nie zabrać kogoś lub czegoś ze sobą, odchodząc lub odjeżdżając z jakiegoś miejsca",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_leaving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety <w>zostawiłem</w> telefon w domu i nie mogłem do ciebie zadzwonić.",
                            "target_lang_translations": {
                                "ru": "К сожалению, я <w>оставил</w> телефон дома и не мог тебе позвонить."
                            }
                        },
                        {
                            "text": "Możesz <w>zostawić</w> swoje rzeczy w szatni przed wejściem na salę.",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>оставить</w> свои вещи в гардеробе перед входом в зал."
                            }
                        }
                    ],
                    "synonyms": [
                        "pozostawić"
                    ],
                    "antonyms": [
                        "zabrać",
                        "wziąć"
                    ],
                    "common_phrases": [
                        "zostawić klucze",
                        "zostawić wiadomość",
                        "zostawić napiwek",
                        "zostawić bagaż"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не забрать кого-то или что-то с собой, уходя или уезжая из какого-то места"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "не взять с собой, забыть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09315304-2b06-367e-9bf3-2fbefb814880",
                    "sense_definition": "Sprawić, że ktoś lub coś pozostanie w określonym stanie lub miejscu",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_maintenance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę, <w>zostaw</w> drzwi otwarte, żeby przewietrzyć ten duszny pokój.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>оставь</w> дверь открытой, чтобы проветрить эту душную комнату."
                            }
                        },
                        {
                            "text": "Wychodząc z biura, przypadkiem <w>zostawił</w> włączone światło w korytarzu.",
                            "target_lang_translations": {
                                "ru": "Уходя из офиса, он случайно <w>оставил</w> включенным свет в коридоре."
                            }
                        }
                    ],
                    "synonyms": [
                        "pozostawić"
                    ],
                    "common_phrases": [
                        "zostawić otwarte drzwi",
                        "zostawić porządek",
                        "zostawić włączone światło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы кто-то или что-то осталось в определенном состоянии или месте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "сохранить в состоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f943fad-062d-3e6a-bb3c-4ab31faac873",
                    "sense_definition": "Dać komuś spokój, przestać kogoś niepokoić, zaczepiać lub przeszkadzać",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_maintenance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zostaw</w> mnie w spokoju, muszę się teraz skupić na nauce.",
                            "target_lang_translations": {
                                "ru": "<w>Оставь</w> меня в покое, мне нужно сейчас сосредоточиться на учебе."
                            }
                        },
                        {
                            "text": "Dzieci bawiły się świetnie, więc rodzice postanowili <w>zostawić</w> je samym sobie.",
                            "target_lang_translations": {
                                "ru": "Дети отлично играли, поэтому родители решили <w>оставить</w> их самим себе."
                            }
                        }
                    ],
                    "synonyms": [
                        "odłożyć",
                        "odłożyć"
                    ],
                    "common_phrases": [
                        "zostawić w spokoju",
                        "zostawić samemu sobie",
                        "zostawić temat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать беспокоить, трогать или мешать кому-то"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "в покое"
                            },
                            {
                                "target_lang_word": "отстать",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e10eb692-1b4a-3ba3-933a-648c758a2068",
                    "sense_definition": "Odłożyć coś na później lub zachować część czegoś (np. jedzenia) do wykorzystania w przyszłości",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "postponing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zostaw</w> trochę ciasta dla brata, który wróci późno ze szkoły.",
                            "target_lang_translations": {
                                "ru": "<w>Оставь</w> немного пирога для брата, который поздно вернется из школы."
                            }
                        },
                        {
                            "text": "Musimy <w>zostawić</w> tę trudną rozmowę na bardziej dogodny moment.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>оставить</w> этот трудный разговор на более подходящий момент."
                            }
                        }
                    ],
                    "synonyms": [
                        "odłożyć",
                        "opuścić",
                        "pozostawić"
                    ],
                    "common_phrases": [
                        "zostawić na później",
                        "zostawić na jutro",
                        "zostawić resztę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отложить что-то на потом или сохранить часть чего-то для использования в будущем"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "сохранить, отложить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8cd0917-cc5d-3e4c-8f92-983e5f573599",
                    "sense_definition": "Porzucić kogoś, odejść od kogoś na stałe, zrywając więzi lub relację",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abandonment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej mąż niespodziewanie <w>zostawił</w> rodzinę i wyjechał za granicę.",
                            "target_lang_translations": {
                                "ru": "Её муж неожиданно <w>оставил</w> семью и уехал за границу."
                            }
                        },
                        {
                            "text": "Nie możesz tak po prostu <w>zostawić</w> przyjaciela w potrzebie bez pomocy.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь так просто <w>оставить</w> друга в беде без помощи."
                            }
                        }
                    ],
                    "synonyms": [
                        "opuścić",
                        "pozostawić"

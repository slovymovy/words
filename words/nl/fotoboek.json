    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e960506b-459f-3889-bcea-0653abde5297",
                    "sense_definition": "Een boek met lege bladzijden waarin men foto's kan plakken of bewaren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik plak de vakantiefoto's in een nieuw <w>fotoboek</w>.",
                            "target_lang_translations": {
                                "ru": "Я вклеиваю фотографии из отпуска в новый <w>фотоальбом</w>."
                            }
                        },
                        {
                            "text": "Oma pakt vaak het <w>fotoboek</w> om herinneringen op te halen.",
                            "target_lang_translations": {
                                "ru": "Бабушка часто берет <w>фотоальбом</w>, чтобы предаться воспоминаниям."
                            }
                        }
                    ],
                    "synonyms": [
                        "fotoalbum",
                        "plakboek"
                    ],
                    "common_phrases": [
                        "een oud fotoboek",
                        "fotoboek maken",
                        "in het fotoboek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Книга с пустыми страницами, куда можно вклеивать или где можно хранить фотографии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фотоальбом",
                                "target_lang_sense_clarification": "книга для хранения фотографий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f67fb85c-2dd2-364a-9b17-8c6419b52554",
                    "sense_definition": "Een gepubliceerd boek dat voornamelijk uit artistieke of informatieve foto's bestaat.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bekende fotograaf presenteerde zijn nieuwe <w>fotoboek</w> in de boekhandel.",
                            "target_lang_translations": {
                                "ru": "Известный фотограф представил свой новый <w>фотоальбом</w> в книжном магазине."
                            }
                        },
                        {
                            "text": "Dit <w>fotoboek</w> laat de mooiste plekken van de stad zien.",
                            "target_lang_translations": {
                                "ru": "Этот <w>фотоальбом</w> показывает самые красивые места города."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een prachtig fotoboek",
                        "het nieuwe fotoboek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опубликованная книга, состоящая в основном из художественных или документальных фотографий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фотоальбом",
                                "target_lang_sense_clarification": "печатное издание с фотографиями"
                            },
                            {
                                "target_lang_word": "фотокнига",
                                "target_lang_sense_clarification": "книга, состоящая из фотографий"
                            }
                        ]
                    }
                }
            ]
        }
    ]

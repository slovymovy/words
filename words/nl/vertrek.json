                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>vertrek</w> morgenochtend heel vroeg naar mijn werk.",
                            "target_lang_translations": {

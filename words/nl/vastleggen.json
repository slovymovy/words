                {
                    "sense_id": "a740e4d0-22bf-35aa-995b-833df79e015e",
                    "sense_definition": "Beeld, geluid of informatie registreren om het te bewaren",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "recording",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fotograaf wilde het bijzondere moment voor altijd <w>vastleggen</w>.",
                            "target_lang_translations": {
                                "ru": "Фотограф хотел навсегда <w>запечатлеть</w> этот особенный момент."
                            }
                        },
                        {
                            "text": "De politie heeft de verklaring van de getuige schriftelijk <w>vastgelegd</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция письменно <w>зафиксировала</w> показания свидетеля."
                            }
                        }

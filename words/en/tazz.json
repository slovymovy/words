                    "sense_id": "9084dc96-903c-3d31-8238-32f7d161158f",
                    "sense_definition": "To move very quickly, dash, or rush somewhere.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>tazzed</w> down the street on his new motorbike to show it off.",
                            "target_lang_translations": {
                                "ru": "Он <w>промчался</w> по улице на своем новом мотоцикле, чтобы похвастаться им."
                            }
                        },
                        {
                            "text": "The children spent the afternoon <w>tazzing</w> around the park.",
                            "target_lang_translations": {
                                "ru": "Дети провели день, <w>гоняя</w> по парку."
                            }
                        }
                    ],
                    "synonyms": [
                        "zoom",
                        "dash",
                        "rush",
                        "zip",
                        "tear"
                    ],
                    "antonyms": [
                        "crawl",
                        "creep",
                        "dawdle"
                    ],
                    "common_phrases": [
                        "tazz around",
                        "tazz off"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "23177112-dd89-3221-b38e-be26e5128e26",
                    "sense_definition": "A male given name of Hebrew origin.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "They decided to call their new baby <w>Abraham</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать своего новорожденного <w>Авраамом</w>."
                            }
                        },
                        {
                            "text": "My uncle <w>Abraham</w> has lived in this city for fifty years.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Абрам</w> живет в этом городе уже пятьдесят лет."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское личное имя еврейского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Авраам",
                                "target_lang_sense_clarification": "традиционная/библейская форма"
                            },
                            {
                                "target_lang_word": "Абрам",
                                "target_lang_sense_clarification": "распространенный вариант имени"
                            },
                            {
                                "target_lang_word": "Эйбрахам",
                                "target_lang_sense_clarification": "английское имя (транслитерация)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd0a102f-820b-3855-a38e-2ad7841198cd",
                    "sense_definition": "A surname originating as a patronymic.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Please send the report to Mr. <w>Abraham</w> in the accounting department.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отправьте отчет мистеру <w>Эйбрахаму</w> в бухгалтерию."
                            }
                        },
                        {
                            "text": "The <w>Abraham</w> family donated a large sum to the local library.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Эйбрахам</w> пожертвовала крупную сумму местной библиотеке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "originating as a patronymic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, происходящая от отчества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Эйбрахам",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc486d7e-3060-3889-947a-9c89b32ae358",
                    "sense_definition": "A prophet and patriarch in the Old Testament and Qur'an, regarded as the father of the Jewish people through Isaac and the Arab people through Ishmael.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "The story of <w>Abraham</w> is foundational to three major world religions.",
                            "target_lang_translations": {
                                "ru": "История <w>Авраама</w> является основополагающей для трех основных мировых религий."
                            }
                        },
                        {
                            "text": "In the scriptures, God made a covenant with <w>Abraham</w> to make him the father of many nations.",
                            "target_lang_translations": {
                                "ru": "В писаниях Бог заключил завет с <w>Авраамом</w>, чтобы сделать его отцом многих народов."
                            }
                        }
                    ],
                    "synonyms": [
                        "Abram",
                        "Ibrahim"
                    ],
                    "common_phrases": [
                        "God of Abraham",
                        "bosom of Abraham"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пророк и патриарх в Ветхом Завете и Коране, считающийся прародителем еврейского народа через Исаака и арабского народа через Исмаила."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Авраам",
                                "target_lang_sense_clarification": "в библейском контексте"
                            },
                            {
                                "target_lang_word": "Ибрагим",
                                "target_lang_sense_clarification": "в мусульманском контексте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a54ed6c-c5bd-3ff6-bc69-858c5e18ceda",
                    "sense_definition": "The 14th sura or chapter of the Qur'an.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "document_or_program_name",
                    "examples": [
                        {
                            "text": "The imam recited verses from <w>Abraham</w> during the evening prayer.",
                            "target_lang_translations": {
                                "ru": "Имам читал стихи из суры <w>Ибрагим</w> во время вечерней молитвы."
                            }
                        },
                        {
                            "text": "Scholars often analyze the themes of faith found in the chapter of <w>Abraham</w>.",
                            "target_lang_translations": {
                                "ru": "Ученые часто анализируют темы веры, встречающиеся в суре <w>Ибрагим</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "14-я сура (глава) Корана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ибрагим",
                                "target_lang_sense_clarification": "название суры"
                            }
                        ]
                    }
                }
            ]
        },

                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>lip</w> voorzichtig van de hete thee.",
                            "target_lang_translations": {
                                "ru": "Я осторожно <w>прихлёбываю</w> горячий чай.",
                                "en": "I carefully <w>slurp</w> the hot tea."
                            }

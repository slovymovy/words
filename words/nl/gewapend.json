                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bevolking had zich <w>gewapend</w> tegen de naderende storm.",
                            "target_lang_translations": {
                                "ru": "Население <w>подготовилось</w> к приближающейся буре.",

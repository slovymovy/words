        "unreserved"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "49d6db3a-3457-33b7-b47d-c89537f0f5fa",
                    "sense_definition": "Set aside for a particular person or purpose; spoken for.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "allocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am sorry, sir, but these are <w>reserved</w> seats.",
                            "target_lang_translations": {
                                "ru": "Простите, сэр, но это <w>зарезервированные</w> места."
                            }
                        },
                        {
                            "text": "We parked our car in the space <w>reserved</w> for staff members.",
                            "target_lang_translations": {
                                "ru": "Мы припарковали машину на месте, <w>отведённом</w> для сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "allocated",
                        "assigned",
                        "dedicated"
                    ],
                    "antonyms": [
                        "available",
                        "free",
                        "unreserved",
                        "public"
                    ],
                    "common_phrases": [
                        "reserved seat",
                        "reserved parking",
                        "all rights reserved",
                        "reserved table"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отложенный или предназначенный для конкретного человека или цели; занятый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарезервированный",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "забронированный",
                                "target_lang_sense_clarification": "о билетах, столиках, номерах"
                            },
                            {
                                "target_lang_word": "отведённый",
                                "target_lang_sense_clarification": "предназначенный для определенной цели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "917b3c18-56e1-3477-8ac3-9478b28ff036",
                    "sense_definition": "Slow to reveal emotion or opinions.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was a quiet, <w>reserved</w> person who rarely spoke about himself.",
                            "target_lang_translations": {
                                "ru": "Он был тихим, <w>сдержанным</w> человеком, который редко говорил о себе."
                            }
                        },
                        {
                            "text": "She has a <w>reserved</w> nature and takes time to warm up to new people.",
                            "target_lang_translations": {
                                "ru": "У нее <w>замкнутый</w> характер, и ей нужно время, чтобы привыкнуть к новым людям."
                            }
                        }
                    ],
                    "synonyms": [
                        "quiet",
                        "reticent",
                        "aloof",
                        "distant",
                        "undemonstrative",
                        "taciturn",
                        "standoffish"
                    ],
                    "antonyms": [
                        "outgoing",
                        "sociable",
                        "open",
                        "demonstrative"
                    ],
                    "common_phrases": [
                        "reserved nature",
                        "reserved manner",
                        "remain reserved"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сдержанный в проявлении эмоций или мнений; необщительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдержанный",
                                "target_lang_sense_clarification": "умеющий владеть собой"
                            },
                            {
                                "target_lang_word": "замкнутый",
                                "target_lang_sense_clarification": "необщительный, погруженный в себя"
                            },
                            {
                                "target_lang_word": "скрытный",
                                "target_lang_sense_clarification": "не склонный делиться мыслями"
                            }
                        ]
                    }
                }
            ]
        },
        {

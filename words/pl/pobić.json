    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9522ad89-88b2-35bf-b743-3edcf8496e97",
                    "sense_definition": "Zadać komuś wiele uderzeń, powodując ból lub obrażenia fizyczne.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napastnicy dotkliwie <w>pobili</w> turystę, zabierając mu portfel i telefon.",
                            "target_lang_translations": {
                                "ru": "Нападавшие жестоко <w>избили</w> туриста, забрав у него кошелек и телефон."
                            }
                        },
                        {
                            "text": "Chłopiec bał się wrócić do domu, myśląc, że ojciec go <w>pobije</w>.",
                            "target_lang_translations": {
                                "ru": "Мальчик боялся возвращаться домой, думая, что отец его <w>побьет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbić",
                        "stłuc",
                        "zlać",
                        "sprać"
                    ],
                    "common_phrases": [
                        "pobić kogoś",
                        "pobić dotkliwie",
                        "pobić do nieprzytomności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанести кому-либо множество ударов, причиняя боль или физические травмы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избить",
                                "target_lang_sense_clarification": "нанести телесные повреждения"
                            },
                            {
                                "target_lang_word": "побить",
                                "target_lang_sense_clarification": "нанести удары, поколотить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4c6ab4f-f345-3ea0-b6fc-c7cbee93cb44",
                    "sense_definition": "Zwyciężyć kogoś w rywalizacji sportowej lub innej walce; osiągnąć wynik lepszy od dotychczasowego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "competition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polski skoczek zdołał <w>pobić</w> rekord skoczni w drugiej serii zawodów.",
                            "target_lang_translations": {
                                "ru": "Польский прыгун сумел <w>побить</w> рекорд трамплина во второй серии соревнований."
                            }
                        },
                        {
                            "text": "Nasza drużyna musi <w>pobić</w> przeciwnika, aby awansować do finału.",
                            "target_lang_translations": {
                                "ru": "Наша команда должна <w>победить</w> противника, чтобы выйти в финал."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokonać",
                        "zwyciężyć"
                    ],
                    "antonyms": [
                        "przegrać",
                        "ulec"
                    ],
                    "common_phrases": [
                        "pobić rekord",
                        "pobić rywala",
                        "pobić mistrza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одержать верх над кем-либо в соревновании или борьбе; достичь результата лучше прежнего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побить",
                                "target_lang_sense_clarification": "рекорд"
                            },
                            {
                                "target_lang_word": "победить",
                                "target_lang_sense_clarification": "соперника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a8143b6-c011-32a9-be7b-a8a96b2aab87",
                    "sense_definition": "Wdać się w bójkę z kimś; bić się wzajemnie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dwaj uczniowie <w>pobili</w> się na przerwie o dziewczynę.",
                            "target_lang_translations": {
                                "ru": "Два ученика <w>подрались</w> на перемене из-за девушки."
                            }
                        },
                        {
                            "text": "Nie warto się <w>pobić</w> o taką błahostkę jak miejsce w kolejce.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>драться</w> из-за такого пустяка, как место в очереди."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pobić się z kimś",
                        "pobić się o coś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вступить в драку с кем-либо; наносить друг другу удары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подраться",
                                "target_lang_sense_clarification": "вступить в драку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af880667-fef8-3c36-8f86-cc3cea683eaa",
                    "sense_definition": "Uderzając, wcisnąć coś głębiej lub mocniej osadzić (np. obręcz na beczce).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "technical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bednarz musiał mocno <w>pobić</w> stalowe obręcze, aby beczka była szczelna.",
                            "target_lang_translations": {
                                "ru": "Бондарь должен был сильно <w>набить</w> стальные обручи, чтобы бочка была герметичной."
                            }
                        },
                        {
                            "text": "Stolarz użył młotka, żeby <w>pobić</w> wystający element konstrukcji.",
                            "target_lang_translations": {
                                "ru": "Столяр использовал молоток, чтобы <w>подбить</w> выступающий элемент конструкции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pobić obręcz",
                        "pobić klin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ударяя, вогнать что-либо глубже или плотнее насадить (например, обруч на бочку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набить",
                                "target_lang_sense_clarification": "насадить ударами"
                            },
                            {
                                "target_lang_word": "подбить",
                                "target_lang_sense_clarification": "поправить или закрепить ударом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecbe2ee5-f298-33fa-a8f1-e098b9bd1c45",
                    "sense_definition": "Pozbawić kogoś życia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "killing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych podaniach rycerz wyruszył, by <w>pobić</w> smoka nękającego wioskę.",
                            "target_lang_translations": {
                                "ru": "В старинных преданиях рыцарь отправился, чтобы <w>сразить</w> дракона, терзающего деревню."
                            }
                        },
                        {
                            "text": "Nadybał w lesie braci i <w>pobił</w> ich bez litości.",
                            "target_lang_translations": {
                                "ru": "Он наткнулся в лесу на братьев и <w>убил</w> их без жалости."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabić"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишить кого-либо жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убить",
                                "target_lang_sense_clarification": "лишить жизни"
                            },
                            {
                                "target_lang_word": "сразить",
                                "target_lang_sense_clarification": "в бою (устар./книжн.)"
                            }
                        ]
                    }
                }
            ]
        },
        {

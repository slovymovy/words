    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aaf6e234-61dc-3359-8d47-4805ab19ca0e",
                    "sense_definition": "Słodka, krystaliczna substancja spożywcza otrzymywana z buraków cukrowych lub trzciny cukrowej, używana do słodzenia napojów i potraw.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "culinary_sugar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do kawy zawsze dodaję jedną łyżeczkę <w>cukru</w>.",
                            "target_lang_translations": {
                                "ru": "В кофе я всегда добавляю одну ложечку <w>сахара</w>."
                            }
                        },
                        {
                            "text": "Muszę kupić <w>cukier</w> puder, żeby posypać nim upieczone ciasto.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить <w>сахарную</w> пудру, чтобы посыпать ею испечённый пирог."
                            }
                        }
                    ],
                    "synonyms": [
                        "cukier spożywczy",
                        "sacharoza"
                    ],
                    "common_phrases": [
                        "kostka cukru",
                        "cukier puder",
                        "łyżeczka cukru",
                        "cukier waniliowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкое кристаллическое пищевое вещество, получаемое из сахарной свеклы или сахарного тростника, используемое для подслащивания напитков и блюд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сахар",
                                "target_lang_sense_clarification": "продукт питания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c68982b1-7668-3bc9-84ff-234ee3c83b00",
                    "sense_definition": "Związek chemiczny z grupy węglowodanów, stanowiący podstawowe źródło energii dla organizmów żywych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "chemical_sugar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Owoce są bardzo zdrowe, ale zawierają też sporo naturalnych <w>cukrów</w>.",
                            "target_lang_translations": {
                                "ru": "Фрукты очень полезны, но они также содержат много природных <w>сахаров</w>."
                            }
                        },
                        {
                            "text": "Regularne badania pozwalają kontrolować poziom <w>cukru</w> we krwi.",
                            "target_lang_translations": {
                                "ru": "Регулярные анализы позволяют контролировать уровень <w>сахара</w> в крови."
                            }
                        }
                    ],
                    "synonyms": [
                        "węglowodan",
                        "sacharyd",
                        "cukrowiec"
                    ],
                    "common_phrases": [
                        "poziom cukru",
                        "cukry proste",
                        "cukry złożone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химическое соединение из группы углеводов, являющееся основным источником энергии для живых организмов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сахар",
                                "target_lang_sense_clarification": "химическое соединение"
                            },
                            {
                                "target_lang_word": "углевод",
                                "target_lang_sense_clarification": "научный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

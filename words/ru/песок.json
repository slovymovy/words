    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc595124-8d37-328b-8b54-47d651c99a28",
                    "sense_definition": "Рыхлая осадочная горная порода, состоящая из мелких твёрдых зёрен минералов (обычно кварца).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом мы любим лежать на тёплом <w>песке</w>.",
                            "target_lang_translations": {
                                "en": "In summer, we love lying on the warm <w>sand</w>."
                            }
                        },
                        {
                            "text": "В мои ботинки попал мелкий <w>песок</w>.",
                            "target_lang_translations": {
                                "en": "Fine <w>sand</w> got into my boots."
                            }
                        }
                    ],
                    "common_phrases": [
                        "морской песок",
                        "речной песок",
                        "строительный песок",
                        "белый песок"
                    ],
                    "target_lang_definitions": {
                        "en": "Loose sedimentary rock consisting of small hard grains of minerals (usually quartz)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sand",
                                "target_lang_sense_clarification": "granular material found on beaches and deserts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88e78f2a-d8ae-3b80-8fb7-8cdc2b1db17e",
                    "sense_definition": "Сахар в виде мелких рассыпчатых кристаллов (в отличие от кускового сахара).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама попросила купить хлеб и килограмм <w>песка</w>.",
                            "target_lang_translations": {
                                "en": "Mom asked to buy bread and a kilogram of <w>granulated sugar</w>."
                            }
                        },
                        {
                            "text": "Я обычно пью чёрный чай без <w>песка</w>.",
                            "target_lang_translations": {
                                "en": "I usually drink black tea without <w>sugar</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "сахарный песок"
                    ],
                    "common_phrases": [
                        "сахарный песок",
                        "чай с песком",
                        "килограмм песку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in everyday speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Sugar in the form of small loose crystals (as opposed to lump sugar)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "granulated sugar",
                                "target_lang_sense_clarification": "specific form of sugar"
                            },
                            {
                                "target_lang_word": "sugar",
                                "target_lang_sense_clarification": "common colloquial usage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fe6ce76-49f4-327d-816c-62817fd68cac",
                    "sense_definition": "Обширное пространство или местность, покрытая массой мелких твёрдых зёрен; пустыня.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Караван верблюдов медленно шёл через горячие <w>пески</w>.",
                            "target_lang_translations": {
                                "en": "The caravan of camels walked slowly through the hot <w>sands</w>."
                            }
                        },
                        {
                            "text": "Путешественники заблудились в бескрайних <w>песках</w>.",
                            "target_lang_translations": {
                                "en": "The travelers got lost in the endless <w>sands</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "сахарный песок"
                    ],
                    "common_phrases": [
                        "зыбучие пески",
                        "горячие пески",
                        "среди песков"
                    ],
                    "target_lang_definitions": {
                        "en": "A vast area or region covered with a mass of small hard grains; a desert."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sands",
                                "target_lang_sense_clarification": "desert or vast sandy area"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "The <w>UPS</w> driver left the package on the front porch this morning.",
                            "target_lang_translations": {
                                "ru": "Водитель <w>UPS</w> оставил посылку на крыльце сегодня утром."
                            }
                        },
                        {
                            "text": "You can track your shipment on the <w>UPS</w> website using the tracking number.",
                            "target_lang_translations": {

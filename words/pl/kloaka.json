{
    "word_family": [
        "kloaczny",
        "kloakowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9c4d90de-3283-3900-b21c-2e59d0926237",
                    "sense_definition": "Końcowy odcinek jelita u ptaków, gadów, płazów i niektórych ryb, będący wspólnym ujściem układu wydalniczego i rozrodczego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anatomia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "U ptaków <kloaka> pełni funkcję wspólnego ujścia dla układu pokarmowego i rozrodczego.",
                            "target_lang_translations": {
                                "ru": "У птиц <w>клоака</w> выполняет функцию общего выхода для пищеварительной и половой систем."
                            }
                        },
                        {
                            "text": "Biolog badał budowę wewnętrzną gada, zwracając szczególną uwagę na jego <kloakę>.",
                            "target_lang_translations": {
                                "ru": "Биолог изучал внутреннее строение рептилии, обращая особое внимание на её <w>клоаку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stek"
                    ],
                    "common_phrases": [
                        "kloaka ptaków",
                        "otwór kloaki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конечный отдел кишечника у птиц, пресмыкающихся, земноводных и некоторых рыб, являющийся общим выходом для выделительной и половой систем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клоака",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "390d6919-6b95-3df3-956a-48e290e80354",
                    "sense_definition": "Dół lub zbiornik przeznaczony do gromadzenia odchodów i nieczystości, a także kanał ściekowy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "infrastruktura",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciało męczennicy Felikuli wrzucono do rzymskiej <kloaki>.",
                            "target_lang_translations": {
                                "ru": "Тело мученицы Феликулы было брошено в римскую <w>клоаку</w>."
                            }
                        },
                        {
                            "text": "Nieprzyjemny zapach wydobywał się z otwartej <kloaki> znajdującej się za domem.",
                            "target_lang_translations": {
                                "ru": "Неприятный запах исходил из открытой <w>выгребной ямы</w>, находящейся за домом."
                            }
                        }
                    ],
                    "synonyms": [
                        "ustęp"
                    ],
                    "common_phrases": [
                        "miejska kloaka",
                        "zawartość kloaki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Яма или резервуар для сбора нечистот, а также сточная канава или канализация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выгребная яма",
                                "target_lang_sense_clarification": "резервуар для нечистот"
                            },
                            {
                                "target_lang_word": "клоака",
                                "target_lang_sense_clarification": "канализационный канал"
                            },
                            {
                                "target_lang_word": "помойная яма",
                                "target_lang_sense_clarification": "место для отбросов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af7b3564-2f8e-3d3b-9504-8df28a9d8cfa",
                    "sense_definition": "Miejsce, środowisko lub sytuacja kojarząca się z brudem moralnym, zepsuciem i wulgarnością.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "metafora",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten portal internetowy to prawdziwa <kloaka> pełna nienawiści i kłamstw.",
                            "target_lang_translations": {
                                "ru": "Этот интернет-портал — настоящая <w>клоака</w>, полная ненависти и лжи."
                            }
                        },
                        {
                            "text": "Autor opisał to miasto jako moralną <kloakę>, z której uczciwy człowiek nie ma ucieczki.",
                            "target_lang_translations": {
                                "ru": "Автор описал этот город как моральную <w>клоаку</w>, из которой честному человеку нет выхода."
                            }
                        }
                    ],
                    "common_phrases": [
                        "moralna kloaka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, среда или ситуация, ассоциирующиеся с моральной грязью, развратом и вульгарностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клоака",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "помойка",
                                "target_lang_sense_clarification": "грязное место"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
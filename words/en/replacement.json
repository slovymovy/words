{
    "word_family": [
        "replace",
        "replaceable",
        "irreplaceable",
        "replacer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1f4fedf7-0ad4-36c8-9856-284d82beb2e5",
                    "sense_definition": "A person or thing that takes the place of another; a substitute.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "substitute_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to find a <w>replacement</w> for the broken printer immediately.",
                            "target_lang_translations": {
                                "ru": "Нам нужно немедленно найти <w>замену</w> сломанному принтеру."
                            }
                        },
                        {
                            "text": "The coach sent in a <w>replacement</w> for the injured player during the second half.",
                            "target_lang_translations": {
                                "ru": "Во втором тайме тренер выпустил <w>замену</w> вместо травмированного игрока."
                            }
                        }
                    ],
                    "synonyms": [
                        "substitute",
                        "stand-in",
                        "successor",
                        "proxy"
                    ],
                    "common_phrases": [
                        "replacement part",
                        "find a replacement",
                        "temporary replacement",
                        "cheap replacement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто или то, что заменяет кого-либо или что-либо; заместитель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замена",
                                "target_lang_sense_clarification": "человек или предмет"
                            },
                            {
                                "target_lang_word": "заместитель",
                                "target_lang_sense_clarification": "человек, исполняющий чьи-то обязанности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d7744dd-16ba-38c6-984b-b47eedff747b",
                    "sense_definition": "The act of replacing something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "act_of_replacing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>replacement</w> of the old water pipes took three days to complete.",
                            "target_lang_translations": {
                                "ru": "<w>Замена</w> старых водопроводных труб заняла три дня."
                            }
                        },
                        {
                            "text": "Regular <w>replacement</w> of the air filter is necessary for the machine to work well.",
                            "target_lang_translations": {
                                "ru": "Для хорошей работы машины необходима регулярная <w>замена</w> воздушного фильтра."
                            }
                        }
                    ],
                    "synonyms": [
                        "substitution",
                        "exchange",
                        "swapping"
                    ],
                    "antonyms": [
                        "retention"
                    ],
                    "common_phrases": [
                        "replacement cost",
                        "knee replacement",
                        "hip replacement",
                        "hormone replacement therapy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по замене чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замена",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "смена",
                                "target_lang_sense_clarification": "обновление"
                            },
                            {
                                "target_lang_word": "замещение",
                                "target_lang_sense_clarification": "формальное или техническое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de5911c7-e2c2-35fb-9a91-f4866ffa7517",
                    "sense_definition": "The removal of an edge of crystal, by one plane or more.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The diagram illustrates the <w>replacement</w> of the crystal's sharp edge by a flat plane.",
                            "target_lang_translations": {
                                "ru": "На диаграмме показано <w>замещение</w> острого края кристалла плоской гранью."
                            }
                        },
                        {
                            "text": "Mineralogists studied the <w>replacement</w> patterns on the surface of the specimen.",
                            "target_lang_translations": {
                                "ru": "Минералоги изучали узоры <w>замещения</w> на поверхности образца."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс изменения формы кристалла или замещения одного минерала другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замещение",
                                "target_lang_sense_clarification": "геологический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
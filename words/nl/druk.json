                {
                    "sense_id": "956623a7-f5dc-3483-a37d-ea465b5ec5f7",
                    "sense_definition": "Met te veel kleuren, vormen of patronen waardoor het onrustig oogt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "visual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik vind dat behang veel te <w>druk</w> voor een slaapkamer.",
                            "target_lang_translations": {
                                "ru": "Я считаю эти обои слишком <w>пёстрыми</w> для спальни."
                            }
                        },
                        {
                            "text": "Ze droeg een jurk met een <w>druk</w> patroon van bloemen en strepen.",
                            "target_lang_translations": {
                                "ru": "На ней было платье с <w>наляпистым</w> узором из цветов и полосок."
                            }
                        }
                    ],
                    "synonyms": [

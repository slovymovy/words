                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ровный",
                                "target_lang_sense_clarification": "не кривой"
                            },
                            {
                                "target_lang_word": "прямо",
                                "target_lang_sense_clarification": "наречие: сидеть или стоять прямо"
                            }

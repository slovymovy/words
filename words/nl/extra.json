                        }
                    ],
                    "synonyms": [
                        "bijzonder",
                        "buitengewoon",
                        "zeer"
                    ],
                    "common_phrases": [
                        "extra groot",
                        "extra goed",
                        "extra voorzichtig",
                        "extra lekker"
                    ],
                    "target_lang_definitions": {
                        "en": "To a higher degree than normal; especially."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "extra",
                                "target_lang_sense_clarification": "exceptionally"
                            },
                            {
                                "target_lang_word": "especially",
                                "target_lang_sense_clarification": "particularly"
                            }
                        ]
                    }
                }
            ]
        },
        {

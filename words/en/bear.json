                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "direction_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When you reach the fork in the road, <w>bear</w> left.",
                            "target_lang_translations": {
                                "ru": "Когда дойдете до развилки, <w>держитесь</w> левее."
                            }

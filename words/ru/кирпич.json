    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8a097e69-760b-3319-83a5-2bb4a04a6c75",
                    "sense_definition": "Искусственный камень правильной формы, используемый как строительный материал.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот старый дом построен из красного <w>кирпича</w>.",
                            "target_lang_translations": {
                                "en": "This old house is built of red <w>brick</w>."
                            }
                        },
                        {
                            "text": "Рабочие аккуратно укладывали каждый <w>кирпич</w> в стену.",
                            "target_lang_translations": {
                                "en": "The workers carefully laid every <w>brick</w> in the wall."
                            }
                        }
                    ],
                    "common_phrases": [
                        "красный кирпич",
                        "силикатный кирпич",
                        "битый кирпич",
                        "класть кирпич"
                    ],
                    "target_lang_definitions": {
                        "en": "Artificial stone of regular shape used as a building material."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brick",
                                "target_lang_sense_clarification": "building material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "869ed651-0e1c-3e69-9d9d-98ceef1b89c2",
                    "sense_definition": "Хлеб, испечённый в форме прямоугольного бруска.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По дороге домой я купил <w>кирпич</w> чёрного хлеба.",
                            "target_lang_translations": {
                                "en": "On the way home I bought a <w>loaf</w> of black bread."
                            }
                        },
                        {
                            "text": "На столе лежал свежий <w>кирпич</w> и острый нож.",
                            "target_lang_translations": {
                                "en": "A fresh <w>loaf</w> and a sharp knife lay on the table."
                            }
                        }
                    ],
                    "synonyms": [
                        "буханка"
                    ],
                    "common_phrases": [
                        "кирпич хлеба",
                        "чёрный кирпич"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Bread baked in the shape of a rectangular block."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "loaf",
                                "target_lang_sense_clarification": "rectangular bread"
                            },
                            {
                                "target_lang_word": "rectangular loaf",
                                "target_lang_sense_clarification": "specific shape"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e61eab98-864a-36f6-9bb1-ddc5dc5cfe48",
                    "sense_definition": "Дорожный знак «Въезд запрещён» в виде красного круга с белым прямоугольником.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель не заметил <w>кирпич</w> и повернул на улицу с односторонним движением.",
                            "target_lang_translations": {
                                "en": "The driver didn't notice the <w>No Entry sign</w> and turned onto the one-way street."
                            }
                        },
                        {
                            "text": "Здесь нельзя ехать прямо, там висит <w>кирпич</w>.",
                            "target_lang_translations": {
                                "en": "You can't drive straight here, there is a <w>No Entry sign</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "проехать под кирпич",
                        "висит кирпич"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The 'No Entry' road sign in the form of a red circle with a white rectangle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "No Entry sign",
                                "target_lang_sense_clarification": "traffic sign"
                            },
                            {
                                "target_lang_word": "Do Not Enter sign",
                                "target_lang_sense_clarification": "traffic sign"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9576468-5408-38a8-8137-36668a460c9b",
                    "sense_definition": "Очень толстая, тяжёлая книга или объёмный документ.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент с трудом дочитал этот скучный <w>кирпич</w> к экзамену.",
                            "target_lang_translations": {
                                "en": "The student barely finished reading this boring <w>tome</w> for the exam."
                            }
                        },
                        {
                            "text": "В сумке у него лежал настоящий <w>кирпич</w>, который он называл учебником.",
                            "target_lang_translations": {
                                "en": "In his bag lay a real <w>brick</w>, which he called a textbook."
                            }
                        }
                    ],
                    "synonyms": [
                        "фолиант"
                    ],
                    "common_phrases": [
                        "читать кирпич",
                        "написать кирпич"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A very thick, heavy book or voluminous document."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tome",
                                "target_lang_sense_clarification": "large, heavy book"
                            },
                            {
                                "target_lang_word": "doorstop",
                                "target_lang_sense_clarification": "humorous, very thick book"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63513aab-d0d7-3ed8-ad12-1436c052c61f",
                    "sense_definition": "Электронное устройство, которое перестало работать из-за программного сбоя.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После неудачной прошивки мой телефон превратился в <w>кирпич</w>.",
                            "target_lang_translations": {
                                "en": "After a failed firmware update, my phone turned into a <w>brick</w>."
                            }
                        },
                        {
                            "text": "Теперь этот дорогой планшет просто бесполезный <w>кирпич</w>.",
                            "target_lang_translations": {
                                "en": "Now this expensive tablet is just a useless <w>brick</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "превратиться в кирпич",
                        "телефон-кирпич"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "жарг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An electronic device that has stopped working due to a software failure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brick",
                                "target_lang_sense_clarification": "non-functional device"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1dd36f54-d294-35b2-bf2f-817ec969785f",
                    "sense_definition": "Спрессованный в виде твёрдого прямоугольного бруска продукт (обычно чай).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Путешественники взяли с собой плотный <w>кирпич</w> чая.",
                            "target_lang_translations": {
                                "en": "The travelers took a dense <w>brick</w> of tea with them."
                            }
                        },
                        {
                            "text": "В магазине продавался зелёный чай, спрессованный в тяжёлый <w>кирпич</w>.",
                            "target_lang_translations": {
                                "en": "Green tea pressed into a heavy <w>brick</w> was sold at the store."
                            }
                        }
                    ],
                    "common_phrases": [

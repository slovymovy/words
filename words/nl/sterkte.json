        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "4e51d9c6-2ea9-36b2-8c71-00a41156aa48",
                    "sense_definition": "Een uitroep om iemand kracht en moed toe te wensen tijdens een moeilijke periode, zoals ziekte, overlijden of een zware taak.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gecondoleerd met het verlies van je opa, heel veel <w>sterkte</w>!",
                            "target_lang_translations": {
                                "en": "My condolences on the loss of your grandfather, wishing you much <w>strength</w>!",
                                "ru": "Прими соболезнования в связи с утратой дедушки, желаю много <w>сил</w>!"
                            }
                        },
                        {
                            "text": "Ik hoorde dat je morgen naar het ziekenhuis moet, <w>sterkte</w> gewenst.",
                            "target_lang_translations": {
                                "en": "I heard you have to go to the hospital tomorrow, <w>good luck</w>.",
                                "ru": "Я слышал, что тебе завтра в больницу, желаю <w>сил</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "heel veel sterkte",
                        "sterkte gewenst",
                        "ik wens je sterkte"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation to wish someone strength and courage during a difficult period, such as illness, death, or a heavy task.",
                        "ru": "Восклицание для пожелания кому-либо сил и мужества в трудный период, например, во время болезни, траура или выполнения тяжелой задачи."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "good luck",
                                "target_lang_sense_clarification": "for tasks or challenges"
                            },
                            {
                                "target_lang_word": "strength",
                                "target_lang_sense_clarification": "wishing support in difficult times"
                            },
                            {
                                "target_lang_word": "all the best",
                                "target_lang_sense_clarification": "general expression of support"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сил",
                                "target_lang_sense_clarification": "пожелание стойкости"
                            },
                            {
                                "target_lang_word": "мужества",
                                "target_lang_sense_clarification": "пожелание храбрости"
                            },
                            {
                                "target_lang_word": "держись",
                                "target_lang_sense_clarification": "слова поддержки"
                            }
                        ]
                    }
                }

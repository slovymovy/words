{
    "word_family": [
        "negate",
        "negation",
        "negativity",
        "negativism",
        "negatively"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b335e87b-6648-3736-a433-e6695593c9db",
                    "sense_definition": "Harmful, bad, or not wanted; not positive.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "bad_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bad weather had a <w>negative</w> effect on our travel plans.",
                            "target_lang_translations": {
                                "ru": "Плохая погода оказала <w>негативное</w> влияние на наши планы путешествия."
                            }
                        },
                        {
                            "text": "We received some <w>negative</w> feedback from the customers.",
                            "target_lang_translations": {
                                "ru": "Мы получили <w>негативные</w> отзывы от клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "bad",
                        "harmful",
                        "adverse",
                        "unfavorable",
                        "undesirable"
                    ],
                    "antonyms": [
                        "positive",
                        "good",
                        "bad"
                    ],
                    "common_phrases": [
                        "negative effect",
                        "negative impact",
                        "negative feedback",
                        "negative attitude"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вредный, плохой или нежелательный; не положительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрицательный",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "негативный",
                                "target_lang_sense_clarification": "об эмоциях, отзывах или влиянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2d0863b-763c-303a-a7e8-29367ce72d59",
                    "sense_definition": "Of a number or temperature: less than zero.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "math_science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The temperature dropped to <w>negative</w> five degrees last night.",
                            "target_lang_translations": {
                                "ru": "Прошлой ночью температура опустилась до <w>минус</w> пяти градусов."
                            }
                        },
                        {
                            "text": "If you subtract a larger number from a smaller one, you get a <w>negative</w> number.",
                            "target_lang_translations": {
                                "ru": "Если вычесть большее число из меньшего, получится <w>отрицательное</w> число."
                            }
                        }
                    ],
                    "antonyms": [
                        "positive"
                    ],
                    "common_phrases": [
                        "negative number",
                        "negative value",
                        "negative integer"
                    ],
                    "target_lang_definitions": {
                        "ru": "О числе или температуре: меньше нуля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрицательный",
                                "target_lang_sense_clarification": "математический термин"
                            },
                            {
                                "target_lang_word": "минусовой",
                                "target_lang_sense_clarification": "разговорное, о температуре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6df61ee-49c4-3050-b4b7-1b8a38742182",
                    "sense_definition": "Thinking only about the bad parts of a situation or person; pessimistic.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "bad_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stop being so <w>negative</w> and try to see the good side of things.",
                            "target_lang_translations": {
                                "ru": "Перестань быть таким <w>негативным</w> и попытайся увидеть хорошую сторону вещей."
                            }
                        },
                        {
                            "text": "He has a very <w>negative</w> outlook on life.",
                            "target_lang_translations": {
                                "ru": "У него очень <w>негативный</w> взгляд на жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "pessimistic",
                        "gloomy"
                    ],
                    "antonyms": [
                        "positive",
                        "good",
                        "affirmative"
                    ],
                    "common_phrases": [
                        "negative person",
                        "negative thoughts",
                        "negative outlook"
                    ],
                    "target_lang_definitions": {
                        "ru": "Думающий только о плохих сторонах ситуации или человека; пессимистичный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "негативный",
                                "target_lang_sense_clarification": "об отношении или настрое"
                            },
                            {
                                "target_lang_word": "пессимистичный",
                                "target_lang_sense_clarification": "склонный видеть худшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "836229a5-0a67-3957-a6f7-70ef3d581a3f",
                    "sense_definition": "Showing that a disease, condition, or substance is not present.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The athlete was relieved when his drug test came back <w>negative</w>.",
                            "target_lang_translations": {
                                "ru": "Спортсмен испытал облегчение, когда его тест на допинг оказался <w>отрицательным</w>."
                            }
                        },
                        {
                            "text": "Fortunately, the results for the virus were <w>negative</w>.",
                            "target_lang_translations": {
                                "ru": "К счастью, результаты анализа на вирус были <w>отрицательными</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "positive"
                    ],
                    "common_phrases": [
                        "test negative",
                        "negative result"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывающий, что болезнь, состояние или вещество отсутствуют."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрицательный",
                                "target_lang_sense_clarification": "о результатах анализов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cb531d1-9e90-3583-861d-9fd2ed8149cc",
                    "sense_definition": "Relating to the type of electrical charge carried by electrons.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "math_science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Electrons carry a <w>negative</w> charge, while protons are positive.",
                            "target_lang_translations": {
                                "ru": "Электроны несут <w>отрицательный</w> заряд, в то время как протоны — положительный."
                            }
                        },
                        {
                            "text": "Connect the black cable to the <w>negative</w> terminal of the battery.",
                            "target_lang_translations": {
                                "ru": "Подключите черный кабель к <w>отрицательной</w> клемме аккумулятора."
                            }
                        }
                    ],
                    "antonyms": [
                        "positive"
                    ],
                    "common_phrases": [
                        "negative charge",
                        "negative terminal",
                        "negative pole"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к типу электрического заряда, переносимого электронами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрицательный",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6a01f6b0-9ffa-31ad-9577-2d3ff57ddee6",
                    "sense_definition": "A bad feature, disadvantage, or problem.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "bad_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to consider both the positives and the <w>negatives</w> of this plan.",
                            "target_lang_translations": {
                                "ru": "Нам нужно рассмотреть как плюсы, так и <w>минусы</w> этого плана."
                            }
                        },
                        {
                            "text": "One of the <w>negatives</w> of living in the city is the noise.",
                            "target_lang_translations": {
                                "ru": "Один из <w>минусов</w> жизни в городе — это шум."
                            }
                        }
                    ],
                    "synonyms": [
                        "disadvantage",
                        "drawback",
                        "downside"
                    ],
                    "antonyms": [
                        "positive"
                    ],
                    "common_phrases": [
                        "positives and negatives"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плохая черта, недостаток или проблема."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "минус",
                                "target_lang_sense_clarification": "недостаток (разговорное)"
                            },
                            {
                                "target_lang_word": "недостаток",
                                "target_lang_sense_clarification": "изъян или слабая сторона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03298253-53dd-355a-b341-f872de2ec2a7",
                    "sense_definition": "A word or phrase that expresses denial or refusal, such as 'no' or 'not'.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In English, using two <w>negatives</w> in the same sentence is usually incorrect.",
                            "target_lang_translations": {
                                "ru": "В английском языке использование двух <w>отрицаний</w> в одном предложении обычно считается неправильным."
                            }
                        },
                        {
                            "text": "Words like 'never' and 'nothing' function as <w>negatives</w>.",
                            "target_lang_translations": {
                                "ru": "Такие слова, как 'никогда' и 'ничего', функционируют как <w>отрицания</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "double negative"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово или фраза, выражающие отказ или несогласие, например 'нет' или 'не'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрицание",
                                "target_lang_sense_clarification": "грамматический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad19e1de-3673-3117-a55c-a49d05488288",
                    "sense_definition": "A photographic image where light and dark areas are reversed.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "photography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I found some old film <w>negatives</w> in a box in the attic.",
                            "target_lang_translations": {
                                "ru": "Я нашел коробку со старыми пленочными <w>негативами</w> на чердаке."
                            }
                        },
                        {
                            "text": "You can develop these <w>negatives</w> to make new prints.",
                            "target_lang_translations": {
                                "ru": "Вы можете проявить эти <w>негативы</w>, чтобы сделать новые отпечатки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "film negative"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фотографическое изображение, где светлые и темные участки поменяны местами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "негатив",
                                "target_lang_sense_clarification": "фотопленка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "264f02c3-6614-35ce-9c28-4288e5f1a03f",
                    "sense_definition": "A refusal or rejection of a proposal.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "refusal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee gave a <w>negative</w> to the proposed changes.",
                            "target_lang_translations": {
                                "ru": "Комитет ответил <w>отказом</w> на предложенные изменения."
                            }
                        },
                        {
                            "text": "His request was met with a firm <w>negative</w>.",
                            "target_lang_translations": {
                                "ru": "Его просьба была встречена твердым <w>отказом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "refusal",
                        "veto"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказ или отклонение предложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказ",
                                "target_lang_sense_clarification": "формальное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "c4c69480-b401-3b79-a3f8-6632715acbd4",
                    "sense_definition": "Used to say 'no', especially in radio communication.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "refusal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Negative</w>, I cannot proceed with the mission at this time.",
                            "target_lang_translations": {
                                "ru": "<w>Никак нет</w>, я не могу продолжать выполнение миссии в данный момент."
                            }
                        },
                        {
                            "text": "Is the target in sight? <w>Negative</w>.",
                            "target_lang_translations": {
                                "ru": "Цель в поле зрения? <w>Никак нет</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "affirmative"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "military or radio"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения несогласия, особенно в радиосвязи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никак нет",
                                "target_lang_sense_clarification": "военное"
                            },
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "общее отрицание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "05d52b05-90b5-3745-9909-47ac6bbea52e",
                    "sense_definition": "To refuse to accept something; to veto or disprove.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "refusal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The board <w>negatived</w> the proposal immediately.",
                            "target_lang_translations": {
                                "ru": "Совет немедленно <w>отклонил</w> это предложение."
                            }
                        },
                        {
                            "text": "New evidence has <w>negatived</w> the earlier theory.",
                            "target_lang_translations": {
                                "ru": "Новые доказательства <w>опровергли</w> предыдущую теорию."
                            }
                        }
                    ],
                    "synonyms": [
                        "veto",
                        "reject",
                        "disprove"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказываться принять что-либо; накладывать вето или опровергать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отклонять",
                                "target_lang_sense_clarification": "предложение или идею"
                            },
                            {
                                "target_lang_word": "опровергать",
                                "target_lang_sense_clarification": "теорию или доказательства"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
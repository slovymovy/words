    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "54b28453-5675-3920-8859-123aed31fed6",
                    "sense_definition": "The final point of something in space or time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "noun_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "At the <w>end</w> of the garden there was a beautiful fountain.",
                            "target_lang_translations": {
                                "ru": "В <w>конце</w> сада был красивый фонтан."
                            }
                        },
                        {
                            "text": "At the <w>end</w> of the story, the main characters fall in love.",
                            "target_lang_translations": {
                                "ru": "В <w>конце</w> истории главные герои влюбляются."
                            }
                        }
                    ],
                    "synonyms": [
                        "conclusion",
                        "limit",
                        "terminus",
                        "termination"
                    ],
                    "antonyms": [
                        "beginning",
                        "start"
                    ],
                    "common_phrases": [
                        "at the end",
                        "in the end",
                        "to the end"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конечная точка чего-либо в пространстве или времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коне́ц",
                                "target_lang_sense_clarification": "предел, граница"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a034924a-ca70-35e9-8693-1d4d884fe946",
                    "sense_definition": "The most extreme point or part of an object, especially one that is longer than it is wide.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hold the string tightly at both <w>ends</w>.",
                            "target_lang_translations": {
                                "ru": "Крепко держите веревку за оба <w>конца</w>."
                            }
                        },
                        {
                            "text": "My father always sat at the <w>end</w> of the table nearest the kitchen.",
                            "target_lang_translations": {
                                "ru": "Мой отец всегда сидел на <w>конце</w> стола, ближе всего к кухне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "end to end",
                        "on end"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крайняя точка или часть предмета, особенно если он длиннее своей ширины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коне́ц",
                                "target_lang_sense_clarification": "крайняя часть предмета"
                            },
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "кромка, предел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d9e0eee-3940-355a-ba6a-3dd8bcb39b25",
                    "sense_definition": "The stopping or cessation of an effort, activity, or state.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_cessation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is there no <w>end</w> to this madness?",
                            "target_lang_translations": {
                                "ru": "Этому безумию нет <w>конца</w>?"
                            }
                        },
                        {
                            "text": "The new system means an <w>end</w> to connection issues.",
                            "target_lang_translations": {
                                "ru": "Новая система означает <w>конец</w> проблемам со связью."
                            }
                        }
                    ],
                    "synonyms": [
                        "termination",
                        "conclusion"
                    ],
                    "antonyms": [
                        "beginning",
                        "start"
                    ],
                    "common_phrases": [
                        "put an end to",
                        "come to an end",
                        "no end"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекращение или остановка усилия, деятельности или состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коне́ц",
                                "target_lang_sense_clarification": "завершение, прекращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39644981-731c-3769-90cd-cc4b9cd63039",
                    "sense_definition": "A key on a computer keyboard that moves the cursor to the last character of the current line.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Press <w>End</w> to quickly jump to the right side of the document.",
                            "target_lang_translations": {
                                "ru": "Нажмите <w>End</w>, чтобы быстро перейти к правой стороне документа."
                            }
                        },
                        {
                            "text": "I use the <w>End</w> key often when editing code.",
                            "target_lang_translations": {
                                "ru": "Я часто использую клавишу <w>End</w> при редактировании кода."
                            }
                        }
                    ],
                    "antonyms": [
                        "Home"
                    ],
                    "target_lang_definitions": {
                        "ru": "Клавиша на клавиатуре компьютера, которая перемещает курсор к последнему символу текущей строки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "End",
                                "target_lang_sense_clarification": "клавиша на клавиатуре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d54d3ef-dfbe-3236-ab6f-3b268a4a683c",
                    "sense_definition": "A purpose, goal, or aim.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>end</w> of our club is to advance conversation and friendship.",
                            "target_lang_translations": {
                                "ru": "<w>Цель</w> нашего клуба — способствовать общению и дружбе."
                            }
                        },
                        {
                            "text": "For what <w>end</w> should I work so hard?",
                            "target_lang_translations": {
                                "ru": "Ради какой <w>цели</w> мне так усердно работать?"
                            }
                        }
                    ],
                    "synonyms": [
                        "purpose"
                    ],
                    "common_phrases": [
                        "to that end",
                        "means to an end"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намерение, цель или стремление."

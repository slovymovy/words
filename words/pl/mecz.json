                        "gra"
                    ],
                    "common_phrases": [
                        "wygrać mecz",
                        "przegrać mecz",
                        "mecz piłki nożnej",
                        "oglądać mecz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть спортивных соревнований, проводимая между двумя игроками или командами для выявления победителя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "матч",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ecc97cca-4ae9-3af0-a276-219e76705e09",
                    "sense_definition": "Meervoud van <w>luik</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De houten <w>luiken</w> klapperden hard door de zware storm.",
                            "target_lang_translations": {
                                "en": "The wooden <w>shutters</w> rattled loudly due to the heavy storm.",
                                "ru": "Деревянные <w>ставни</w> громко хлопали из-за сильного шторма."
                            }
                        },
                        {
                            "text": "Elke avond sluiten we de <w>luiken</w> om de warmte binnen te houden.",
                            "target_lang_translations": {
                                "en": "Every evening we close the <w>shutters</w> to keep the heat inside.",
                                "ru": "Каждый вечер мы закрываем <w>ставни</w>, чтобы сохранить тепло внутри."
                            }
                        }
                    ],
                    "synonyms": [
                        "blinden"
                    ],
                    "common_phrases": [
                        "de luiken sluiten"
                    ],
                    "target_lang_definitions": {
                        "en": "Plural form of 'luik'.",
                        "ru": "Множественное число слова <w>luik</w> (ставень, люк)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shutters",
                                "target_lang_sense_clarification": "window covers"
                            },
                            {
                                "target_lang_word": "hatches",
                                "target_lang_sense_clarification": "openings in a floor, roof, or ship's deck"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ставни",
                                "target_lang_sense_clarification": "на окнах"
                            },
                            {
                                "target_lang_word": "люки",
                                "target_lang_sense_clarification": "отверстия с крышкой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0642bd83-5fd7-381d-8d04-274a2c766772",
                    "sense_definition": "Dichtgaan of sluiten, met name gezegd van ogen of bloemen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_close",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bloem <w>look</w> langzaam toen de avond viel.",
                            "target_lang_translations": {
                                "en": "The flower slowly <w>closed</w> when evening fell.",
                                "ru": "Цветок медленно <w>закрылся</w>, когда наступил вечер."
                            }
                        },
                        {
                            "text": "Zij zat stil in de stoel met <w>geloken</w> ogen te luisteren.",
                            "target_lang_translations": {
                                "en": "She sat quietly in the chair listening with <w>closed</w> eyes.",
                                "ru": "Она сидела тихо в кресле и слушала с <w>сомкнутыми</w> глазами."
                            }
                        }
                    ],
                    "synonyms": [
                        "sluiten",
                        "dichtgaan"
                    ],
                    "antonyms": [
                        "ontluiken",
                        "opengaan"
                    ],
                    "common_phrases": [
                        "met geloken ogen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern Dutch uses 'sluiten' or 'dichtgaan'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To close or shut, specifically said of eyes or flowers.",
                        "ru": "Закрываться или смыкаться, в основном говорится о глазах или цветах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to close",
                                "target_lang_sense_clarification": "of eyes or flowers"
                            },
                            {
                                "target_lang_word": "to shut",
                                "target_lang_sense_clarification": "archaic/poetic use"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "смыкаться",
                                "target_lang_sense_clarification": "о глазах"
                            },
                            {
                                "target_lang_word": "закрываться",
                                "target_lang_sense_clarification": "о цветах или глазах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36009dcf-0469-3fec-8a67-626d78dea2f2",
                    "sense_definition": "Vlechten of in elkaar draaien van materiaal zoals riet, biezen of touw.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_weave",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude ambachtsman zat geconcentreerd een nieuwe mand te <w>luiken</w>.",
                            "target_lang_translations": {
                                "en": "The old craftsman sat concentratedly <w>weaving</w> a new basket.",
                                "ru": "Старый ремесленник сосредоточенно сидел и <w>плёл</w> новую корзину."
                            }
                        },
                        {
                            "text": "Voor het maken van stevig touw moet je de strengen goed <w>luiken</w>.",
                            "target_lang_translations": {
                                "en": "To make strong rope, you must <w>twist</w> the strands well.",
                                "ru": "Для изготовления прочной веревки нужно хорошо <w>скрутить</w> пряди."
                            }
                        }
                    ],
                    "synonyms": [
                        "vlechten"
                    ],
                    "target_lang_definitions": {
                        "en": "To weave or twist material like reed, rushes, or rope.",
                        "ru": "Плести или скручивать материалы, такие как тростник или веревка."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to weave",
                                "target_lang_sense_clarification": "baskets or reed"
                            },
                            {
                                "target_lang_word": "to plait",
                                "target_lang_sense_clarification": "intertwining strands"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "плести",
                                "target_lang_sense_clarification": "изготавливать плетением"
                            },
                            {
                                "target_lang_word": "скручивать",
                                "target_lang_sense_clarification": "соединять вращением"
                            }
                        ]
                    }
                }
            ]
        },

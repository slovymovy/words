{
    "word_family": [
        "whenever"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1e45592f-f4a5-3f65-9e5a-8517852d96a1",
                    "sense_definition": "Used to ask at what time something happens.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>When</w> will they arrive?",
                            "target_lang_translations": {
                                "ru": "<w>Когда</w> они приедут?"
                            }
                        },
                        {
                            "text": "Do you know <w>when</w> the store opens?",
                            "target_lang_translations": {
                                "ru": "Ты знаешь, <w>когда</w> открывается магазин?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы спросить, в какое время что-то происходит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "вопросительное наречие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11aa32f1-4dfd-3e9a-b4f1-018bfc9f20d1",
                    "sense_definition": "Used to refer to a time or occasion that has just been mentioned.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That was the day <w>when</w> the Twin Towers fell.",
                            "target_lang_translations": {
                                "ru": "Это был день, <w>когда</w> рухнули башни-близнецы."
                            }
                        },
                        {
                            "text": "I remember the time <w>when</w> we first met.",
                            "target_lang_translations": {
                                "ru": "Я помню то время, <w>когда</w> мы впервые встретились."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на время или случай, о котором только что упоминалось."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "союзное слово"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "0e1259e6-44e4-3b42-9952-5714d0a0282e",
                    "sense_definition": "At the time that; as soon as.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_conj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pavlov's dogs salivate <w>when</w> they hear a bell.",
                            "target_lang_translations": {
                                "ru": "Собаки Павлова выделяют слюну, <w>когда</w> слышат звонок."
                            }
                        },
                        {
                            "text": "Call me <w>when</w> you get home.",
                            "target_lang_translations": {
                                "ru": "Позвони мне, <w>когда</w> доберешься до дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "as soon as",
                        "once"
                    ],
                    "target_lang_definitions": {
                        "ru": "В то время как; как только."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "союз времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f432111c-6d5c-3920-a710-84c78d17da07",
                    "sense_definition": "During the time that.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_conj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was raining <w>when</w> I came yesterday.",
                            "target_lang_translations": {
                                "ru": "Шел дождь, <w>когда</w> я пришел вчера."
                            }
                        },
                        {
                            "text": "Be careful <w>when</w> crossing the street.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, <w>когда</w> переходишь улицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "while",
                        "whilst"
                    ],
                    "target_lang_definitions": {
                        "ru": "В течение того времени, когда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "союз времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8d23486-7b63-304b-bb46-5e7179ff5c43",
                    "sense_definition": "Considering the fact that; although; whereas.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are picking at your scabs <w>when</w> you should be letting them heal.",
                            "target_lang_translations": {
                                "ru": "Ты ковыряешь болячки, <w>хотя</w> тебе следует дать им зажить."
                            }
                        },
                        {
                            "text": "He keeps changing things <w>when</w> the existing system works perfectly well.",
                            "target_lang_translations": {
                                "ru": "Он продолжает все менять, <w>в то время как</w> существующая система работает отлично."
                            }
                        }
                    ],
                    "synonyms": [
                        "whereas",
                        "while"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учитывая тот факт, что; хотя; тогда как."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотя",
                                "target_lang_sense_clarification": "уступка"
                            },
                            {
                                "target_lang_word": "в то время как",
                                "target_lang_sense_clarification": "противопоставление"
                            },
                            {
                                "target_lang_word": "тогда как",
                                "target_lang_sense_clarification": "противопоставление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "c29ab002-a8bd-35e3-b64e-44d3feaee07f",
                    "sense_definition": "What time; which time.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Since <w>when</w> do I need your permission?",
                            "target_lang_translations": {
                                "ru": "С <w>каких пор</w> мне нужно твое разрешение?"
                            }
                        },
                        {
                            "text": "Until <w>when</w> are you staying in the city?",
                            "target_lang_translations": {
                                "ru": "До <w>какого времени</w> ты остаешься в городе?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "since when",
                        "until when"
                    ],
                    "target_lang_definitions": {
                        "ru": "Какое время; какой момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каких пор",
                                "target_lang_sense_clarification": "в выражении 'с каких пор'"
                            },
                            {
                                "target_lang_word": "какого времени",
                                "target_lang_sense_clarification": "в выражении 'до какого времени'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "45d511a4-6e38-39b2-961f-c6c91083ca92",
                    "sense_definition": "The specific time at which something happens.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A good article will cover the who, the what, the <w>when</w>, and the where.",
                            "target_lang_translations": {
                                "ru": "Хорошая статья охватит: кто, что, <w>когда</w> и где."
                            }
                        },
                        {
                            "text": "We have not yet decided on the <w>when</w> of the meeting.",
                            "target_lang_translations": {
                                "ru": "Мы еще не решили насчет <w>времени</w> встречи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the when and where"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное время, когда что-то происходит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "время",
                                "target_lang_sense_clarification": "существительное"
                            },
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "субстантивированное наречие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "ff2c9b63-90a9-325b-8f86-62f87100a3dd",
                    "sense_definition": "Used to tell someone to stop adding something, especially food or drink.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will pour the wine, just say <w>when</w>.",
                            "target_lang_translations": {
                                "ru": "Я буду наливать вино, просто скажи, <w>когда</w> хватит."
                            }
                        },
                        {
                            "text": "He started serving the potatoes and waited for me to say <w>when</w>.",
                            "target_lang_translations": {
                                "ru": "Он начал накладывать картошку и ждал, пока я скажу <w>хватит</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "say when"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы попросить остановиться, когда накладывают еду или наливают напиток."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хватит",
                                "target_lang_sense_clarification": "команда стоп"
                            },
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "в выражении 'скажи когда'"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
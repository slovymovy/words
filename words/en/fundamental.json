                    "common_phrases": [
                        "fundamental principle",
                        "fundamental basis",
                        "fundamental rule"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служащий базой или основой, на которой что-либо строится или создается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основополагающий",
                                "target_lang_sense_clarification": "служащий началом, базой"

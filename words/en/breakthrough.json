                    "synonyms": [
                        "advance",
                        "discovery",
                        "innovation",
                        "revolution",
                        "leap"
                    ],
                    "antonyms": [
                        "setback",
                        "regression"
                    ],
                    "common_phrases": [
                        "major breakthrough",
                        "scientific breakthrough",
                        "technological breakthrough",
                        "make a breakthrough"
                    ],
                    "target_lang_definitions": {
                        "ru": "Важное открытие или событие, которое помогает улучшить ситуацию или решить проблему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прорыв",
                                "target_lang_sense_clarification": "значительное достижение"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c602e23c-c7d7-3c9d-b738-fcf9760e1e36",
                    "sense_definition": "De grote hoeveelheid mensen die tijdens de kerstperiode aanwezig is in winkels of op de weg.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "crowds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente neemt maatregelen om de verwachte <w>kerstdrukte</w> in het centrum in goede banen te leiden.",
                            "target_lang_translations": {
                                "ru": "Муниципалитет принимает меры, чтобы упорядочить ожидаемый <w>наплыв людей перед Рождеством</w> в центре."
                            }
                        },
                        {
                            "text": "Door de enorme <w>kerstdrukte</w> in de winkelstraten was er bijna geen parkeerplek meer te vinden.",
                            "target_lang_translations": {
                                "ru": "Из-за огромного <w>рождественского ажиотажа</w> на торговых улицах было почти невозможно найти место для парковки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de verwachte kerstdrukte",
                        "kerstdrukte in de stad",
                        "enorme kerstdrukte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество людей, находящихся в магазинах или на дорогах в рождественский период."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рождественский ажиотаж",
                                "target_lang_sense_clarification": "повышенная активность и скопление людей"
                            },
                            {
                                "target_lang_word": "наплыв людей",
                                "target_lang_sense_clarification": "большое скопление народа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56ba849b-eea1-3a16-81ec-d5b839e87ea9",
                    "sense_definition": "De drukke bezigheden, haast en vele activiteiten die gedaan moeten worden ter voorbereiding op Kerstmis.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij proberen dit jaar te ontsnappen aan de <w>kerstdrukte</w> door vroeg op vakantie te gaan.",
                            "target_lang_translations": {
                                "ru": "В этом году мы пытаемся сбежать от <w>рождественской суеты</w>, отправившись в отпуск пораньше."
                            }
                        },
                        {
                            "text": "Door de jaarlijkse <w>kerstdrukte</w> op het werk heb ik nog geen tijd gehad voor de versiering.",
                            "target_lang_translations": {
                                "ru": "Из-за ежегодной <w>предрождественской суеты</w> на работе у меня еще не было времени заняться украшением."
                            }
                        }
                    ],
                    "synonyms": [
                        "kersthectiek"
                    ],
                    "antonyms": [
                        "kerstrust"
                    ],
                    "common_phrases": [
                        "ontsnappen aan de kerstdrukte",
                        "de jaarlijkse kerstdrukte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напряженная деятельность, спешка и множество дел, которые необходимо выполнить при подготовке к Рождеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рождественская суета",
                                "target_lang_sense_clarification": "суматоха и спешка"
                            },
                            {
                                "target_lang_word": "предпраздничные хлопоты",
                                "target_lang_sense_clarification": "дела по подготовке к празднику"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e0b6933d-5dac-3982-8822-7ceaa03166a6",
                    "sense_definition": "Ilość wykorzystanych zasobów, takich jak woda, prąd, gaz czy paliwo.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W zimie nasze <w>zużycie</w> prądu znacznie wzrasta z powodu ogrzewania.",
                            "target_lang_translations": {
                                "ru": "Зимой наше <w>потребление</w> электроэнергии значительно возрастает из-за отопления."
                            }
                        },
                        {
                            "text": "Ten model samochodu słynie z tego, że ma niskie <w>zużycie</w> paliwa.",
                            "target_lang_translations": {
                                "ru": "Эта модель автомобиля славится тем, что у неё низкий <w>расход</w> топлива."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zużycie energii",
                        "zużycie paliwa",
                        "zużycie wody",
                        "roczne zużycie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество использованных ресурсов, таких как вода, электричество, газ или топливо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потребление",
                                "target_lang_sense_clarification": "использование ресурсов (энергии, воды)"
                            },
                            {
                                "target_lang_word": "расход",
                                "target_lang_sense_clarification": "затрата чего-либо (топлива, материалов)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37eaee00-776f-37b0-84bd-6ccccf1d14da",
                    "sense_definition": "Stopień zniszczenia lub pogorszenia stanu rzeczy wynikający z jej normalnego używania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "wear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na drewnianej podłodze widać już wyraźne ślady <w>zużycia</w>.",
                            "target_lang_translations": {
                                "ru": "На деревянном полу уже видны явные следы <w>износа</w>."
                            }
                        },
                        {
                            "text": "Części maszyny uległy szybkiemu <w>zużyciu</w> i wymagają wymiany.",
                            "target_lang_translations": {
                                "ru": "Детали машины подверглись быстрому <w>износу</w> и требуют замены."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ślady zużycia",
                        "stopień zużycia",
                        "ulec zużyciu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень разрушения или ухудшения состояния вещи в результате её нормального использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "износ",
                                "target_lang_sense_clarification": "физическое старение материала или механизма"
                            },
                            {
                                "target_lang_word": "амортизация",
                                "target_lang_sense_clarification": "постепенное снашивание (часто в экономическом контексте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "017d83fb-28ac-3adb-a067-56563e4109b1",
                    "sense_definition": "Rzeczownik odczasownikowy od słowa <w>zużyć</w>, oznaczający czynność wyczerpania lub wykorzystania czegoś do końca.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Całkowite <w>zużycie</w> zapasów zmusiło nas do zrobienia nowych zakupów.",
                            "target_lang_translations": {
                                "ru": "Полное <w>израсходование</w> запасов вынудило нас сделать новые покупки."
                            }
                        },
                        {
                            "text": "Poprzez <w>zużycie</w> dostępnych materiałów zakończyliśmy pierwszy etap budowy.",
                            "target_lang_translations": {
                                "ru": "Путём <w>использования</w> доступных материалов мы завершили первый этап строительства."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verbal noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по полному использованию или исчерпанию чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "израсходование",
                                "target_lang_sense_clarification": "полная трата запасов или средств"
                            },
                            {
                                "target_lang_word": "исчерпание",
                                "target_lang_sense_clarification": "окончательное использование доступного ресурса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7267aac9-00b0-3e2c-878f-f15a18ebd1fc",
                    "sense_definition": "Stan skrajnego zmęczenia fizycznego lub psychicznego organizmu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "exhaustion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu latach ciężkiej pracy fizycznej czuł ogromne <w>zużycie</w> organizmu.",
                            "target_lang_translations": {
                                "ru": "После многих лет тяжелой физической работы он чувствовал огромную <w>изношенность</w> организма."
                            }
                        },
                        {
                            "text": "Jego twarz zdradzała oznaki przedwczesnego <w>zużycia</w> spowodowanego stresem.",
                            "target_lang_translations": {
                                "ru": "Его лицо выдавало признаки преждевременного <w>старения</w> (износа), вызванного стрессом."
                            }
                        }
                    ],
                    "synonyms": [
                        "wycieńczenie",
                        "wyczerpanie"
                    ],
                    "common_phrases": [
                        "fizyczne zużycie",
                        "biologiczne zużycie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние крайней физической или психической усталости организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изношенность",
                                "target_lang_sense_clarification": "состояние дряхлости или сильной усталости организма"
                            },
                            {
                                "target_lang_word": "истощение",
                                "target_lang_sense_clarification": "крайняя степень утомления"
                            }
                        ]
                    }
                }
            ]
        }
    ]

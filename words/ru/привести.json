{
    "word_family": [
        "приводить",
        "привод",
        "приведение"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f8d88227-73f1-31d4-9d83-26e79e23e97a",
                    "sense_definition": "Доставить кого-либо или что-либо в определённое место, сопровождая или указывая путь.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обещал <привести> сына в зоопарк в это воскресенье."
                        },
                        {
                            "text": "Ты можешь <привести> к нам в гости своих друзей."
                        }
                    ],
                    "antonyms": [
                        "увести"
                    ],
                    "common_phrases": [
                        "привести домой",
                        "привести друга",
                        "привести ребёнка"
                    ]
                },
                {
                    "sense_id": "232cb2ca-1678-386f-b74d-61e99a636256",
                    "sense_definition": "О дороге или тропинке: дать возможность дойти до какого-либо места.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта извилистая тропинка <привела> нас прямо к реке."
                        },
                        {
                            "text": "Широкая дорога скоро <приведёт> уставших путников к городу."
                        }
                    ],
                    "common_phrases": [
                        "дорога привела",
                        "тропинка привела"
                    ]
                },
                {
                    "sense_id": "9937a6cb-40c7-31cb-8a07-925e57fd065b",
                    "sense_definition": "Заставить оказаться в каком-либо состоянии, придать определённый вид или положение.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам нужно срочно <привести> квартиру в порядок перед праздником."
                        },
                        {
                            "text": "Его неожиданный ответ <привёл> меня в полное замешательство."
                        }
                    ],
                    "common_phrases": [
                        "привести в порядок",
                        "привести в чувство",
                        "привести в восторг",
                        "привести в исполнение"
                    ]
                },
                {
                    "sense_id": "10a85fec-d88b-3b6f-9932-175cd8e605e6",
                    "sense_definition": "Послужить причиной чего-либо, иметь своим следствием (обычно с предлогом «к»).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Неосторожность в работе может <привести> к серьёзной ошибке."
                        },
                        {
                            "text": "Упорные тренировки обязательно <приведут> спортсмена к победе."
                        }
                    ],
                    "common_phrases": [
                        "привести к успеху",
                        "привести к победе",
                        "привести к последствиям"
                    ]
                },
                {
                    "sense_id": "bb25fc5b-2cff-3f0c-b3c5-f59e95256b0d",
                    "sense_definition": "Упомянуть или сообщить что-либо для подтверждения своих слов (примеры, факты, цитаты).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Профессор попросил студента <привести> конкретный пример из истории."
                        },
                        {
                            "text": "В своём докладе он <привёл> несколько убедительных доказательств."
                        }
                    ],
                    "common_phrases": [
                        "привести пример",
                        "привести доказательство",
                        "привести аргумент",
                        "привести цитату"
                    ]
                },
                {
                    "sense_id": "ea56cf20-cddc-327b-a033-b69a51ac5d26",
                    "sense_definition": "Произвести математическое преобразование для получения нужной формы выражения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сначала нужно <привести> эти дроби к общему знаменателю."
                        },
                        {
                            "text": "Ученик смог правильно <привести> сложное уравнение к каноническому виду."
                        }
                    ],
                    "common_phrases": [
                        "привести к знаменателю",
                        "привести подобные слагаемые"
                    ]
                }
            ]
        }
    ]
}
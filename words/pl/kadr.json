            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4cfa941b-76f9-38b5-bfa7-4f138b47835e",
                    "sense_definition": "Obraz widoczny w obiektywie aparatu lub kamery; sposób ujęcia i skomponowania fotografowanej sceny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "photography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagle w <kadrze> pojawiła się nieznajoma osoba i zepsuła zdjęcie.",
                            "target_lang_translations": {
                                "ru": "Внезапно в <w>кадре</w> появился незнакомый человек и испортил фотографию."
                            }
                        },
                        {
                            "text": "Fotograf długo ustawiał <kadr>, zanim nacisnął spust migawki.",
                            "target_lang_translations": {
                                "ru": "Фотограф долго выстраивал <w>кадр</w>, прежде чем нажать на спуск затвора."
                            }
                        }
                    ],
                    "synonyms": [
                        "klatka",
                        "klatka"
                    ],
                    "common_phrases": [
                        "w kadrze",
                        "poza kadrem",
                        "zmieścić w kadrze",
                        "ucięty kadr"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображение, видимое в объективе фотоаппарата или камеры; способ компоновки снимаемой сцены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кадр",
                                "target_lang_sense_clarification": "снимок, сцена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a03d546c-91d5-3d08-b3ab-33aeacdbbc47",
                    "sense_definition": "Pojedyncze pole na taśmie filmowej lub błonie fotograficznej zawierające jeden obraz.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "photography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Montażysta wyciął jeden <kadr>, aby skrócić scenę.",
                            "target_lang_translations": {
                                "ru": "Монтажер вырезал один <w>кадр</w>, чтобы сократить сцену."
                            }
                        },
                        {
                            "text": "Oglądaliśmy ten film <kadr> po kadrze, szukając ukrytych szczegółów.",
                            "target_lang_translations": {
                                "ru": "Мы просматривали этот фильм <w>кадр</w> за кадром в поисках скрытых деталей."
                            }
                        }
                    ],
                    "synonyms": [
                        "klatka"
                    ],
                    "common_phrases": [
                        "kadr filmowy",
                        "kadr po kadrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельное поле на кинопленке или фотопленке, содержащее одно изображение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кадр",
                                "target_lang_sense_clarification": "фрагмент пленки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bb4c8a2-6b40-38d2-82d3-724dd21b71db",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>kadra</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę zanieść zwolnienie lekarskie do działu <kadr>.",
                            "target_lang_translations": {
                                "ru": "Я должен отнести больничный в отдел <w>кадров</w>."
                            }
                        },
                        {
                            "text": "Firma inwestuje w szkolenie swoich <kadr>, aby podnieść jakość usług.",
                            "target_lang_translations": {
                                "ru": "Компания инвестирует в обучение своих <w>кадров</w>, чтобы повысить качество услуг."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dział kadr",
                        "szef kadr",
                        "brak kadr"
                    ],
                    "traits": [
                        {

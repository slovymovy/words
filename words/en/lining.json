                            {
                                "target_lang_word": "подкладка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a538fabb-6e9d-3ce1-97c5-df2a57fccb68",

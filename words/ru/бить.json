    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb73e213-9344-3a15-96f0-d328e90e7382",
                    "sense_definition": "Наносить удары, стучать по чему-либо или во что-либо; совершать резкие движения до соприкосновения с объектом.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный дождь громко <w>бил</w> по крыше дома всю ночь.",
                            "target_lang_translations": {
                                "en": "Heavy rain <w>beat</w> loudly on the house's roof all night."
                            }
                        },
                        {
                            "text": "В закрытую дверь кто-то настойчиво <w>бил</w> кулаком.",
                            "target_lang_translations": {
                                "en": "Someone was persistently <w>banging</w> on the locked door with their fist."
                            }
                        }
                    ],
                    "synonyms": [
                        "ударять",
                        "стучать"
                    ],
                    "common_phrases": [
                        "бить кулаком",
                        "бить ногой",
                        "бить в дверь",
                        "бить по мячу"
                    ],
                    "target_lang_definitions": {
                        "en": "To hit or strike repeatedly; to make forceful contact with an object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "to hit repeatedly"
                            },
                            {
                                "target_lang_word": "pound",
                                "target_lang_sense_clarification": "to strike heavily"
                            },
                            {
                                "target_lang_word": "bang",
                                "target_lang_sense_clarification": "to strike noisily"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c234018-1579-3afd-9f23-7aed3492afcf",
                    "sense_definition": "Подвергать насилию, причинять физическую боль ударами (человеку или животному).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В современном обществе учителям строго запрещено <w>бить</w> учеников.",
                            "target_lang_translations": {
                                "en": "In modern society, teachers are strictly forbidden to <w>beat</w> students."
                            }
                        },
                        {
                            "text": "Злой хозяин часто <w>бил</w> свою собаку палкой.",
                            "target_lang_translations": {
                                "en": "The angry owner often <w>beat</w> his dog with a stick."
                            }
                        }
                    ],
                    "synonyms": [
                        "избивать"
                    ],
                    "common_phrases": [
                        "бить ребёнка",
                        "бить палкой",
                        "бить до крови",
                        "бить кнутом"
                    ],
                    "target_lang_definitions": {
                        "en": "To inflict physical pain or punishment by hitting someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "to punish physically"
                            },
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "to strike"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddba248b-54a5-3579-bbd5-8405b977a441",
                    "sense_definition": "Разрушать или раскалывать предмет на куски, ударяя его обо что-то или чем-то тяжёлым.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Во время ссоры он начал <w>бить</w> тарелки об пол.",
                            "target_lang_translations": {
                                "en": "During the argument, he began to <w>smash</w> plates on the floor."
                            }
                        },
                        {
                            "text": "Хулиганы любят <w>бить</w> стёкла в заброшенных домах.",
                            "target_lang_translations": {
                                "en": "Hooligans love to <w>break</w> windows in abandoned houses."
                            }
                        }
                    ],
                    "synonyms": [
                        "разбивать",
                        "раскалывать"
                    ],
                    "common_phrases": [
                        "бить посуду",
                        "бить стёкла",
                        "бить горшки"
                    ],
                    "target_lang_definitions": {
                        "en": "To break, shatter, or destroy an object by striking it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "to separate into pieces"
                            },
                            {
                                "target_lang_word": "smash",
                                "target_lang_sense_clarification": "to break violently"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74d2687f-d9d4-3036-82bc-dad7f555a202",
                    "sense_definition": "Издавать громкие звуки или сигналы посредством ударов (о часах, колоколах, барабанах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старинные часы в гостиной начали <w>бить</w> полночь.",
                            "target_lang_translations": {
                                "en": "The antique clock in the living room began to <w>strike</w> midnight."
                            }
                        },
                        {
                            "text": "Музыкант продолжал ритмично <w>бить</w> в большой барабан.",
                            "target_lang_translations": {
                                "en": "The musician continued to rhythmically <w>beat</w> the big drum."
                            }
                        }
                    ],
                    "synonyms": [
                        "грохотать",
                        "хлопать"
                    ],
                    "common_phrases": [
                        "часы бьют",
                        "бить в колокол",
                        "бить в барабан",
                        "бить в ладоши"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a rhythmic sound or signal (of clocks, drums, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strike",
                                "target_lang_sense_clarification": "of a clock"
                            },
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "of a drum"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06cb127b-5c90-3f5f-9bc7-b5c3636c9e22",
                    "sense_definition": "Одерживать победу в бою или соревновании, наносить поражение противнику.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша армия успешно <w>бьёт</w> врага на всех фронтах.",
                            "target_lang_translations": {
                                "en": "Our army successfully <w>beats</w> the enemy on all fronts."
                            }
                        },
                        {
                            "text": "Эта футбольная команда <w>бьёт</w> соперников в каждом матче.",
                            "target_lang_translations": {
                                "en": "This football team <w>beats</w> its rivals in every match."
                            }
                        }
                    ],
                    "synonyms": [
                        "громить",
                        "побивать"
                    ],
                    "common_phrases": [
                        "бить врага",
                        "бить противника",
                        "бить рекорд"
                    ],
                    "target_lang_definitions": {
                        "en": "To defeat an opponent in a fight, competition, or game."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "to win against"
                            },
                            {
                                "target_lang_word": "defeat",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99a52455-6ad1-3917-a310-35ea9496cc49",
                    "sense_definition": "Вести стрельбу из оружия, посылать снаряды или пули в цель.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Снайпер умеет точно <w>бить</w> в цель с большого расстояния.",
                            "target_lang_translations": {
                                "en": "The sniper knows how to accurately <w>hit</w> the target from a long distance."
                            }
                        },
                        {
                            "text": "Артиллерия начала <w>бить</w> по укреплениям противника на рассвете.",
                            "target_lang_translations": {
                                "en": "The artillery began to <w>fire</w> at the enemy fortifications at dawn."
                            }
                        }
                    ],
                    "synonyms": [
                        "стрелять",
                        "разить"
                    ],
                    "common_phrases": [
                        "бить в цель",
                        "бить без промаха",
                        "бить прямой наводкой"
                    ],
                    "target_lang_definitions": {
                        "en": "To shoot at or hit a target with a projectile; to fire a weapon."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "to reach a target"
                            },
                            {
                                "target_lang_word": "fire",
                                "target_lang_sense_clarification": "to discharge a weapon"
                            },
                            {
                                "target_lang_word": "pound",
                                "target_lang_sense_clarification": "to shell heavily"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7fb117d-11cd-3d4e-8ff8-37a0de6b794e",
                    "sense_definition": "С силой вгонять внутрь чего-либо ударами (обычно о гвоздях или сваях).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Строители начали <w>бить</w> сваи для фундамента нового дома.",
                            "target_lang_translations": {
                                "en": "The builders began to <w>drive</w> piles for the new house's foundation."
                            }
                        },
                        {
                            "text": "Отец учил сына правильно <w>бить</w> молотком по гвоздю.",
                            "target_lang_translations": {
                                "en": "The father taught his son how to correctly <w>hit</w> the nail with a hammer."
                            }
                        }
                    ],
                    "synonyms": [
                        "заколачивать"
                    ],
                    "common_phrases": [
                        "бить сваи",
                        "бить молотком"
                    ],
                    "target_lang_definitions": {
                        "en": "To force something in with blows (like nails or piles)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "to force in"
                            },
                            {
                                "target_lang_word": "hammer",
                                "target_lang_sense_clarification": "to hit with a tool"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0563c818-5960-398f-bb47-97d97f848084",
                    "sense_definition": "Стремительно вытекать сильной струёй, подниматься вверх под напором (о жидкости).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из повреждённой трубы прямо на улицу <w>била</w> вода.",
                            "target_lang_translations": {
                                "en": "Water <w>gushed</w> from the damaged pipe right onto the street."
                            }
                        },
                        {
                            "text": "В центре парка красиво <w>бьёт</w> высокий фонтан.",
                            "target_lang_translations": {
                                "en": "A high fountain <w>gushes</w> beautifully in the center of the park."
                            }
                        }
                    ],
                    "synonyms": [
                        "извергаться"
                    ],
                    "common_phrases": [
                        "бить ключом",
                        "бить фонтаном",
                        "бить струёй"
                    ],
                    "target_lang_definitions": {
                        "en": "To flow out forcefully in a stream or jet."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gush",
                                "target_lang_sense_clarification": "to flow forcefully"
                            },
                            {
                                "target_lang_word": "spurt",
                                "target_lang_sense_clarification": "to eject suddenly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bf54f95-bbfd-30c1-a9cb-b9f631adf9a1",
                    "sense_definition": "Вызывать сильную дрожь, трясти (обычно при болезни, холоде или страхе).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physiological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Больного весь вечер <w>била</w> сильная лихорадка.",
                            "target_lang_translations": {
                                "en": "A strong fever <w>shook</w> the patient all evening."
                            }
                        },
                        {
                            "text": "От ледяного ветра и страха его начала <w>бить</w> дрожь.",
                            "target_lang_translations": {
                                "en": "He began to <w>shake</w> with tremors from the icy wind and fear."
                            }
                        }
                    ],
                    "synonyms": [
                        "знобить"
                    ],
                    "common_phrases": [
                        "бьёт дрожь",
                        "бьёт озноб",
                        "бьёт лихорадка"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause shaking or shivering (usually due to fever or cold)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shake",
                                "target_lang_sense_clarification": "to tremble"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "752be54f-9576-37ed-b963-e0c28f749861",
                    "sense_definition": "Забирать фигуру противника в настольных играх или крыть карту более старшей мастью.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой карточной игре козырный туз <w>бьёт</w> любую карту.",
                            "target_lang_translations": {
                                "en": "In this card game, the trump ace <w>beats</w> any card."
                            }
                        },
                        {
                            "text": "Шахматист решил <w>бить</w> пешку своим конём.",
                            "target_lang_translations": {
                                "en": "The chess player decided to <w>take</w> the pawn with his knight."
                            }
                        }
                    ],
                    "synonyms": [
                        "брать",
                        "крыть"
                    ],
                    "common_phrases": [
                        "бить карту",
                        "бить козырем",
                        "бить пешку",
                        "бить фигуру"
                    ],
                    "target_lang_definitions": {
                        "en": "To capture a piece or defeat a card in games."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "cards"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "chess/pieces"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db537c59-9526-322a-8a1b-5481c2f6d983",
                    "sense_definition": "Энергично перемешивать продукты, превращая их в однородную или пышную массу.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для приготовления десерта нужно долго <w>бить</w> сливки венчиком.",
                            "target_lang_translations": {
                                "en": "To make the dessert, you need to <w>whip</w> the cream with a whisk for a long time."
                            }
                        },
                        {
                            "text": "Повар начал <w>бить</w> яйца с сахаром для бисквита.",
                            "target_lang_translations": {
                                "en": "The chef began to <w>beat</w> the eggs with sugar for the sponge cake."
                            }
                        }
                    ],
                    "synonyms": [
                        "взбивать"
                    ],
                    "common_phrases": [
                        "бить яйца",
                        "бить масло",
                        "бить сливки"
                    ],
                    "target_lang_definitions": {
                        "en": "To mix ingredients vigorously to aerate them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "general cooking"
                            },
                            {
                                "target_lang_word": "whip",
                                "target_lang_sense_clarification": "cream"
                            },
                            {
                                "target_lang_word": "whisk",
                                "target_lang_sense_clarification": "using a tool"
                            }
                        ]
                    }
                }
            ]
        }

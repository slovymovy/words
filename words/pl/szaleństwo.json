    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c04fe9c2-b279-3348-a2b1-746f87cc1c3f",
                    "sense_definition": "Zachowanie skrajnie nierozsądne, niebezpieczne lub wykraczające poza przyjęte normy, często oceniane jako głupie lub ryzykowne.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "irrational_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jazda z taką prędkością w gęstej mgle to prawdziwe <w>szaleństwo</w>.",
                            "target_lang_translations": {
                                "ru": "Езда с такой скоростью в густом тумане — это настоящее <w>безумие</w>."
                            }
                        },
                        {
                            "text": "Służby ratunkowe ostrzegły, że skakanie z balkonów do basenu to niebezpieczne <w>szaleństwo</w>, które może skończyć się tragedią.",
                            "target_lang_translations": {
                                "ru": "Спасательные службы предупредили, что прыжки с балконов в бассейн — это опасное <w>безрассудство</w>, которое может закончиться трагедией."
                            }
                        }
                    ],
                    "synonyms": [
                        "wariactwo",
                        "bezsens",
                        "brak piątej klepki"
                    ],
                    "antonyms": [
                        "rozsądek",
                        "rozwaga"
                    ],
                    "common_phrases": [
                        "czyste szaleństwo",
                        "to jakieś szaleństwo",
                        "szczyt szaleństwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крайне неразумное, опасное поведение или поступок, выходящий за рамки здравого смысла и часто расцениваемый как глупость или риск."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безумие",
                                "target_lang_sense_clarification": "крайнее безрассудство"
                            },
                            {
                                "target_lang_word": "безрассудство",
                                "target_lang_sense_clarification": "отсутствие благоразумия"
                            },
                            {
                                "target_lang_word": "сумасшествие",
                                "target_lang_sense_clarification": "нелепый, рискованный поступок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3258023f-3b40-3440-bac0-455e9e7a98ea",
                    "sense_definition": "Poważne zaburzenie psychiczne lub stan utraty panowania nad zmysłami i emocjami, charakteryzujący się brakiem kontaktu z rzeczywistością.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lęk przed zachorowaniem jest lękiem atawistycznym, podobnie jak strach przed śmiercią i <w>szaleństwem</w>.",
                            "target_lang_translations": {
                                "ru": "Страх заболеть является атавистическим, так же как страх смерти и <w>сумасшествия</w>."
                            }
                        },
                        {
                            "text": "W jego oczach widać było narastające <w>szaleństwo</w>, które przerażało wszystkich domowników.",
                            "target_lang_translations": {
                                "ru": "В его глазах было видно нарастающее <w>безумие</w>, которое пугало всех домочадцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "obłęd",
                        "obłąkanie",
                        "niepoczytalność",
                        "niezrównoważenie",
                        "wariactwo"
                    ],
                    "antonyms": [
                        "zdrowie psychiczne",
                        "poczytalność"
                    ],
                    "common_phrases": [
                        "popaść w szaleństwo",
                        "ogarnęło go szaleństwo",
                        "na granicy szaleństwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серьезное психическое расстройство или состояние потери контроля над разумом и эмоциями, характеризующееся утратой связи с реальностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сумасшествие",
                                "target_lang_sense_clarification": "психическое расстройство"
                            },
                            {
                                "target_lang_word": "безумие",
                                "target_lang_sense_clarification": "потеря рассудка"
                            },
                            {
                                "target_lang_word": "умопомешательство",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1082e65-c3ed-3785-b4ad-167cddaf6cdd",
                    "sense_definition": "Stan euforycznej, beztroskiej zabawy lub intensywne, pełne pasji oddawanie się jakiejś czynności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fun_passion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimowe <w>szaleństwo</w> na stoku narciarskim przyciągnęło w tym roku tysiące turystów.",
                            "target_lang_translations": {
                                "ru": "Зимнее <w>безумие</w> на горнолыжном склоне привлекло в этом году тысячи туристов."
                            }
                        },
                        {
                            "text": "Podczas wyprzedaży w sklepach panowało zakupowe <w>szaleństwo</w>, a ludzie tłoczyli się przy kasach.",
                            "target_lang_translations": {
                                "ru": "Во время распродажи в магазинах царило покупательское <w>безумие</w>, а люди толпились у касс."
                            }
                        }
                    ],
                    "synonyms": [
                        "wariactwo"
                    ],
                    "antonyms": [
                        "nuda"
                    ],
                    "common_phrases": [
                        "białe szaleństwo",
                        "zakupowe szaleństwo",
                        "nocne szaleństwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние эйфорического веселья, ажиотажа или страстного, безудержного увлечения какой-либо деятельностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безумие",
                                "target_lang_sense_clarification": "неистовство, страсть"
                            },
                            {
                                "target_lang_word": "ажиотаж",
                                "target_lang_sense_clarification": "сильное возбуждение, суматоха"
                            }
                        ]
                    }
                }
            ]

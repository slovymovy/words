{
    "word_family": [
        "spender",
        "overspend",
        "underspend",
        "misspend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2cbb5c5d-e47e-3164-b833-a690b9ae12f0",
                    "sense_definition": "To use money to pay for goods, services, or to benefit someone.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "money_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>spends</w> a lot of money on video games every month.",
                            "target_lang_translations": {
                                "ru": "Он каждый месяц <w>тратит</w> кучу денег на видеоигры."
                            }
                        },
                        {
                            "text": "How much did you <w>spend</w> on that new car?",
                            "target_lang_translations": {
                                "ru": "Сколько ты <w>потратил</w> на эту новую машину?"
                            }
                        }
                    ],
                    "synonyms": [
                        "pay",
                        "disburse",
                        "expend"
                    ],
                    "antonyms": [
                        "save",
                        "earn",
                        "hoard"
                    ],
                    "common_phrases": [
                        "spend money",
                        "spend a fortune",
                        "spend on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать деньги для оплаты товаров, услуг или на благо кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тратить",
                                "target_lang_sense_clarification": "расходовать деньги"
                            },
                            {
                                "target_lang_word": "потратить",
                                "target_lang_sense_clarification": "израсходовать деньги (сов. вид)"
                            },
                            {
                                "target_lang_word": "расходовать",
                                "target_lang_sense_clarification": "употреблять на что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4e79361-f663-3c6f-8c0f-d8c3e13ad041",
                    "sense_definition": "To pass or use time doing something or being somewhere.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I usually <w>spend</w> my weekends reading books in the garden.",
                            "target_lang_translations": {
                                "ru": "Я обычно <w>провожу</w> выходные за чтением книг в саду."
                            }
                        },
                        {
                            "text": "They <w>spent</w> three years living in Japan before moving back.",
                            "target_lang_translations": {
                                "ru": "Они <w>провели</w> три года в Японии, прежде чем вернуться."
                            }
                        }
                    ],
                    "synonyms": [
                        "pass",
                        "occupy"
                    ],
                    "antonyms": [
                        "save"
                    ],
                    "common_phrases": [
                        "spend time",
                        "spend the night",
                        "spend hours",
                        "spend a day"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить или использовать время, занимаясь чем-либо или находясь где-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проводить",
                                "target_lang_sense_clarification": "время"
                            },
                            {
                                "target_lang_word": "провести",
                                "target_lang_sense_clarification": "время (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83d936ed-368d-3f34-8c5f-bdee22a45b16",
                    "sense_definition": "To use up energy, force, or resources completely until they are gone; to exhaust.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "exhaustion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The storm <w>spent</w> itself before it reached the city.",
                            "target_lang_translations": {
                                "ru": "Шторм <w>выдохся</w>, не дойдя до города."
                            }
                        },
                        {
                            "text": "She <w>spent</w> all her energy trying to finish the marathon.",
                            "target_lang_translations": {
                                "ru": "Она <w>истратила</w> всю свою энергию, пытаясь закончить марафон."
                            }
                        }
                    ],
                    "synonyms": [
                        "exhaust",
                        "deplete",
                        "drain"
                    ],
                    "antonyms": [
                        "conserve",
                        "preserve",
                        "renew"
                    ],
                    "common_phrases": [
                        "spend energy",
                        "force is spent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью израсходовать энергию, силы или ресурсы, пока они не закончатся; истощить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "истратить",
                                "target_lang_sense_clarification": "израсходовать полностью"
                            },
                            {
                                "target_lang_word": "израсходовать",
                                "target_lang_sense_clarification": "лишиться чего-либо, применив по назначению"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e287b10-fb79-3b35-a887-4253574d0e04",
                    "sense_definition": "The amount of money paid out for a particular purpose or during a specific period.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "money_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The marketing <w>spend</w> for this quarter is higher than expected.",
                            "target_lang_translations": {
                                "ru": "Маркетинговые <w>расходы</w> в этом квартале выше ожидаемых."
                            }
                        },
                        {
                            "text": "We need to control our total <w>spend</w> on office supplies.",
                            "target_lang_translations": {
                                "ru": "Нам нужно контролировать общие <w>затраты</w> на канцелярские товары."
                            }
                        }
                    ],
                    "synonyms": [
                        "expenditure",
                        "outlay",
                        "cost"
                    ],
                    "antonyms": [
                        "income",
                        "revenue",
                        "saving"
                    ],
                    "common_phrases": [
                        "advertising spend",
                        "total spend",
                        "annual spend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумма денег, выплаченная для определенной цели или за определенный период."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d9c99130-7c03-3160-99dc-bdbf64101745",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd, gebiedende wijs en (bij inversie) tweede persoon enkelvoud van <w>zoeken</w>",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>zoek</w> een leuk cadeau voor mijn moeder.",
                            "target_lang_translations": {
                                "en": "I am <w>looking for</w> a nice gift for my mother."
                            }
                        },
                        {
                            "text": "<w>Zoek</w> jij even mee naar de afstandsbediening?",
                            "target_lang_translations": {
                                "en": "Will you help <w>look for</w> the remote control?"
                            }
                        },
                        {
                            "text": "<w>Zoek</w> dekking als het luchtalarm afgaat!",
                            "target_lang_translations": {
                                "en": "<w>Take</w> cover when the air raid siren goes off!"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense, imperative, and (in inversion) second person singular of 'zoeken'"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look for",
                                "target_lang_sense_clarification": "to try to find something"
                            },
                            {
                                "target_lang_word": "search",
                                "target_lang_sense_clarification": "to look carefully in order to find something"
                            },
                            {
                                "target_lang_word": "seek",
                                "target_lang_sense_clarification": "to try to find or obtain (often abstract or formal)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "447fb2d9-3697-368b-9f48-7909d6e7f45f",
                    "sense_definition": "De handeling of toestand van het speuren naar iets of iemand (vooral in de uitdrukking 'op zoek')",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij zijn dringend op <w>zoek</w> naar nieuwe medewerkers.",
                            "target_lang_translations": {
                                "en": "We are urgently <w>looking for</w> new employees."
                            }
                        },
                        {
                            "text": "Hij ging in het bos op <w>zoek</w> naar paddenstoelen.",
                            "target_lang_translations": {
                                "en": "He went into the forest in <w>search</w> of mushrooms."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op zoek naar",
                        "op zoek gaan"
                    ],
                    "target_lang_definitions": {
                        "en": "The act or state of searching for something or someone (especially in the expression 'op zoek')"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "looking for",
                                "target_lang_sense_clarification": "used in the phrase 'to be looking for' (op zoek zijn naar)"
                            },
                            {
                                "target_lang_word": "search",
                                "target_lang_sense_clarification": "used in the phrase 'in search of' (op zoek naar)"
                            }
                        ]
                    }
                }
            ]
        },
        {

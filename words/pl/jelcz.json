    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "31dadc41-bfc0-3cad-ae22-31432210c16d",
                    "sense_definition": "Samochód ciężarowy, autobus lub trolejbus marki Jelcz.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "vehicles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na parkingu stał stary <w>jelcz</w>, który wymagał natychmiastowego remontu.",
                            "target_lang_translations": {
                                "ru": "На парковке стоял старый <w>Ельч</w>, который требовал немедленного ремонта."
                            }
                        },
                        {
                            "text": "Kierowca <w>jelcza</w> zatrzymał się na przystanku, aby zabrać pasażerów.",
                            "target_lang_translations": {
                                "ru": "Водитель <w>Ельча</w> остановился на остановке, чтобы забрать пассажиров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stary jelcz",
                        "kierowca jelcza",
                        "czerwony jelcz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грузовик, автобус или троллейбус марки «Ельч»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ельч",
                                "target_lang_sense_clarification": "транспортное средство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4918b394-4fbc-392a-9fd5-b36e9b2b7c36",
                    "sense_definition": "Polska marka samochodów ciężarowych, autobusów i trolejbusów oraz zakłady je produkujące.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "brands",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "W latach osiemdziesiątych autobusy <w>Jelcza</w> dominowały w polskiej komunikacji miejskiej.",
                            "target_lang_translations": {
                                "ru": "В восьмидесятые годы автобусы <w>Jelcz</w> доминировали в польском городском транспорте."
                            }
                        },
                        {
                            "text": "Zakłady <w>Jelcza</w> były przez lata ważnym pracodawcą w regionie dolnośląskim.",
                            "target_lang_translations": {
                                "ru": "Заводы <w>Jelcz</w> долгие годы были важным работодателем в Нижнесилезском регионе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "marka Jelcz",
                        "zakłady Jelcza",
                        "autobusy Jelcza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская марка грузовых автомобилей, автобусов и троллейбусов, а также завод-производитель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Jelcz",
                                "target_lang_sense_clarification": "бренд"
                            },
                            {
                                "target_lang_word": "Ельч",
                                "target_lang_sense_clarification": "бренд (транслитерация)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bb71dce-d0e3-31ff-bbd2-315a3518a578",
                    "sense_definition": "Część miasta Jelcz-Laskowice w Polsce, w województwie dolnośląskim; dawniej samodzielna wieś.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "locations",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Moi dziadkowie wybudowali swój pierwszy dom w <w>Jelczu</w> zaraz po wojnie.",
                            "target_lang_translations": {
                                "ru": "Мои дедушка и бабушка построили свой первый дом в <w>Ельче</w> сразу после войны."
                            }
                        },
                        {
                            "text": "Często jeżdżę do <w>Jelcza</w>, aby odwiedzić moją daleką rodzinę.",
                            "target_lang_translations": {
                                "ru": "Я часто езжу в <w>Ельч</w>, чтобы навестить свою дальнюю родню."
                            }
                        }
                    ],
                    "common_phrases": [
                        "w Jelczu",
                        "do Jelcza",
                        "okolice Jelcza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть города Ельч-Ласковице в Польше; ранее самостоятельное село."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ельч",
                                "target_lang_sense_clarification": "населенный пункт"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
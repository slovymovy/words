    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "51bad41a-cad4-3f05-8730-d1753fd74010",
                    "sense_definition": "Iets in gedachten hebben om te zeggen of aan te duiden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik begrijp niet precies wat je daarmee <w>bedoelt</w>.",
                            "target_lang_translations": {
                                "en": "I don't understand exactly what you <w>mean</w> by that."
                            }
                        },
                        {
                            "text": "Ik <w>bedoel</w> maar te zeggen dat we voorzichtig moeten zijn.",
                            "target_lang_translations": {
                                "en": "I just <w>mean</w> to say that we have to be careful."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wat bedoel je",
                        "ik bedoel maar"
                    ],
                    "target_lang_definitions": {
                        "en": "To have in mind to say, indicate, or signify."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mean",
                                "target_lang_sense_clarification": "to convey meaning or refer to something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7312371-2fd9-39df-ba5a-7f36483df85b",
                    "sense_definition": "Iets als doel hebben of met een bepaalde intentie doen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn opmerking was grappig <w>bedoeld</w>, maar niemand lachte.",
                            "target_lang_translations": {
                                "en": "His remark was <w>meant</w> to be funny, but nobody laughed."
                            }
                        },
                        {
                            "text": "Deze brief is speciaal voor jou <w>bedoeld</w>.",
                            "target_lang_translations": {
                                "en": "This letter is <w>meant</w> especially for you."
                            }
                        }
                    ],
                    "synonyms": [
                        "beogen"
                    ],
                    "common_phrases": [
                        "goed bedoeld",
                        "bedoeld voor"
                    ],
                    "target_lang_definitions": {
                        "en": "To have as a goal or to do with a specific intention."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mean",
                                "target_lang_sense_clarification": "to intend or have as a purpose"
                            },
                            {
                                "target_lang_word": "intend",
                                "target_lang_sense_clarification": "to plan or have in mind as a goal"
                            }
                        ]
                    }
                }
            ]
        }

                    "sense_id": "28727400-1c2e-3f5d-b2fd-a1659de75876",
                    "sense_definition": "W sposób pełen energii, dynamiki, ruchu lub entuzjazmu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "manner_energy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci bawiły się <w>żywo</w> na placu zabaw, biegając i krzycząc.",
                            "target_lang_translations": {
                                "ru": "Дети <w>оживлённо</w> играли на детской площадке, бегая и крича."
                            }
                        },
                        {
                            "text": "Publiczność reagowała <w>żywo</w> na każdy żart opowiadany przez komika.",
                            "target_lang_translations": {
                                "ru": "Публика <w>живо</w> реагировала на каждую шутку, рассказанную комиком."
                            }
                        }
                    ],
                    "synonyms": [
                        "energicznie",
                        "żwawo",
                        "z werwą",
                        "wesoło",
                        "skocznie",
                        "spontanicznie"
                    ],
                    "antonyms": [
                        "martwo"
                    ],
                    "common_phrases": [
                        "dyskutować żywo",
                        "reagować żywo",
                        "opowiadać żywo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Энергично, с энтузиазмом, в быстром темпе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оживлённо",
                                "target_lang_sense_clarification": "полно жизни и движения"
                            },
                            {
                                "target_lang_word": "живо",
                                "target_lang_sense_clarification": "быстро, с энергией"
                            },
                            {
                                "target_lang_word": "бойко",
                                "target_lang_sense_clarification": "проворно, энергично"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cc399a4-a2e0-3569-847c-210140c6ea25",
                    "sense_definition": "Z dużym natężeniem, głęboko i wyraźnie; często w odniesieniu do uczuć, zainteresowania lub pamięci.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem <w>żywo</w> zainteresowany historią starożytnego Rzymu.",
                            "target_lang_translations": {
                                "ru": "Я <w>живо</w> интересуюсь историей Древнего Рима."
                            }
                        },
                        {
                            "text": "Wciąż mam <w>żywo</w> w pamięci nasze ostatnie spotkanie nad morzem.",
                            "target_lang_translations": {
                                "ru": "Я всё ещё <w>живо</w> помню нашу последнюю встречу у моря."
                            }
                        }
                    ],
                    "synonyms": [
                        "bardzo",
                        "intensywnie",
                        "głęboko",
                        "mocno",
                        "wyraźnie",
                        "silnie",
                        "wielce",
                        "znacznie"
                    ],
                    "common_phrases": [

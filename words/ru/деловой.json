            "senses": [
                {
                    "sense_id": "917b8323-7dd4-34ed-9329-57391d48d9ae",
                    "sense_definition": "Связанный с работой, бизнесом или служебной деятельностью.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Завтра у меня важная <w>деловая</w> поездка в другой город.",
                            "target_lang_translations": {
                                "en": "I have an important <w>business</w> trip to another city tomorrow."
                            }
                        },
                        {
                            "text": "На работе принято соблюдать строгий <w>деловой</w> стиль одежды.",
                            "target_lang_translations": {
                                "en": "At work, it is customary to follow a strict <w>business</w> dress code."
                            }
                        }
                    ],
                    "synonyms": [
                        "служебный",
                        "рабочий"
                    ],
                    "antonyms": [
                        "личный",
                        "развлекательный",
                        "домашний"
                    ],
                    "common_phrases": [
                        "деловая встреча",
                        "деловой костюм",
                        "деловое письмо",
                        "деловые отношения"
                    ],
                    "target_lang_definitions": {
                        "en": "Related to work, business, or official activities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "business",
                                "target_lang_sense_clarification": "related to commercial or professional work"
                            },
                            {
                                "target_lang_word": "business-related",
                                "target_lang_sense_clarification": "connected to professional activities"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "730ea533-db94-38a5-b860-6a0c4ecd453c",
                    "sense_definition": "Знающий дело, толковый, практичный и предприимчивый (о человеке или его качествах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам нужен <w>деловой</w> и активный сотрудник, который умеет решать проблемы.",
                            "target_lang_translations": {
                                "en": "We need an <w>enterprising</w> and active employee who knows how to solve problems."
                            }
                        },
                        {
                            "text": "У него настоящий <w>деловой</w> подход к любой задаче.",
                            "target_lang_translations": {
                                "en": "He has a truly <w>businesslike</w> approach to any task."
                            }
                        }
                    ],
                    "synonyms": [
                        "дельный",
                        "предприимчивый",
                        "толковый",
                        "практичный"
                    ],
                    "antonyms": [
                        "бестолковый",
                        "пассивный",
                        "безынициативный"
                    ],
                    "common_phrases": [
                        "деловой человек",
                        "деловая хватка",
                        "деловой подход"
                    ],
                    "target_lang_definitions": {
                        "en": "Knowledgeable, sensible, practical, and enterprising (about a person or their qualities)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "businesslike",
                                "target_lang_sense_clarification": "efficient, practical, and systematic"
                            },
                            {
                                "target_lang_word": "enterprising",
                                "target_lang_sense_clarification": "showing initiative and resourcefulness"
                            },
                            {
                                "target_lang_word": "practical",
                                "target_lang_sense_clarification": "sensible and realistic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11b7cbf8-7cc8-3312-aaae-a1ec22028357",
                    "sense_definition": "Выражающий занятость, серьезность или озабоченность делом (о виде, тоне, жестах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она сделала <w>деловой</w> вид и начала быстро перебирать документы.",
                            "target_lang_translations": {
                                "en": "She put on a <w>businesslike</w> air and started quickly sorting through documents."
                            }
                        },
                        {
                            "text": "По телефону он говорил сухим и <w>деловым</w> тоном.",
                            "target_lang_translations": {
                                "en": "He spoke on the phone in a dry and <w>businesslike</w> tone."
                            }
                        }
                    ],
                    "synonyms": [
                        "серьезный",
                        "озабоченный",
                        "деловитый"
                    ],
                    "antonyms": [
                        "праздный",
                        "легкомысленный"
                    ],
                    "common_phrases": [
                        "деловой вид",
                        "деловой тон"
                    ],
                    "target_lang_definitions": {
                        "en": "Expressing busyness, seriousness, or focus on work (regarding appearance, tone, or gestures)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "businesslike",
                                "target_lang_sense_clarification": "serious, purposeful, and professional in manner"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4de71e6d-2b39-3dbb-ac95-bfe340aade27",
                    "sense_definition": "Важный, высокомерный или делающий вид, что очень занят (часто иронично).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты чего такой <w>деловой</w>, даже не поздоровался с нами?",
                            "target_lang_translations": {
                                "en": "Why are you so <w>self-important</w>, you didn't even say hello to us?"
                            }
                        },
                        {
                            "text": "Смотри, какой он стал <w>деловой</w> после повышения в должности.",
                            "target_lang_translations": {
                                "en": "Look how <w>self-important</w> he's become after the promotion."
                            }
                        }
                    ],
                    "synonyms": [
                        "важный",
                        "высокомерный"
                    ],
                    "antonyms": [
                        "простой",
                        "скромный"
                    ],
                    "common_phrases": [
                        "слишком деловой",
                        "больно деловой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "иронично, неодобрительно"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Important, arrogant, or pretending to be very busy (often ironic)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "self-important",
                                "target_lang_sense_clarification": "acting superior or arrogant"
                            },
                            {
                                "target_lang_word": "bossy",
                                "target_lang_sense_clarification": "colloquial; acting like a boss"
                            }
                        ]
                    }
                }
            ]
        }

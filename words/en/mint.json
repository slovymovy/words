                },
                {
                    "sense_id": "e933dbfd-f859-33d7-8386-b912412d49b8",
                    "sense_definition": "A pale green color.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to paint the bathroom walls in a soft <w>mint</w>.",
                            "target_lang_translations": {
                                "ru": "Она решила покрасить стены ванной в мягкий <w>мятный цвет</w>."
                            }

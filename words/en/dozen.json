    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3534d35-5756-38de-874f-887ba6339c6b",
                    "sense_definition": "A group or set of twelve things.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_12",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can I have a <w>dozen</w> eggs, please?",
                            "target_lang_translations": {
                                "ru": "Можно мне <w>дюжину</w> яиц, пожалуйста?"
                            }
                        },
                        {
                            "text": "She bought half a <w>dozen</w> apples for the pie.",
                            "target_lang_translations": {
                                "ru": "Она купила пол<w>дюжины</w> яблок для пирога."
                            }
                        },
                        {
                            "text": "I ordered two <w>dozen</w> doughnuts for the party.",
                            "target_lang_translations": {
                                "ru": "Я заказал две <w>дюжины</w> пончиков на вечеринку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "half a dozen",
                        "two dozen",
                        "baker's dozen",
                        "a dozen eggs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа или набор из двенадцати предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дюжина",
                                "target_lang_sense_clarification": "группа из 12 штук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed341adf-e8ca-36a5-8407-aaab09b0e8d6",
                    "sense_definition": "A large, unspecified number of people or things.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_many",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There were <w>dozens</w> of people waiting for the bus.",
                            "target_lang_translations": {
                                "ru": "Автобуса ждали <w>десятки</w> людей."
                            }
                        },
                        {
                            "text": "I have told you <w>dozens</w> of times to be careful.",
                            "target_lang_translations": {
                                "ru": "Я говорил тебе <w>десятки</w> раз быть осторожным."
                            }
                        },
                        {
                            "text": "She received <w>dozens</w> and <w>dozens</w> of letters after the event.",
                            "target_lang_translations": {
                                "ru": "После этого события она получила <w>десятки</w> и <w>десятки</w> писем."
                            }
                        }
                    ],
                    "synonyms": [
                        "many",
                        "a lot of",
                        "heap of",
                        "load of"
                    ],
                    "antonyms": [
                        "few"
                    ],
                    "common_phrases": [
                        "dozens of",
                        "dozens and dozens"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое, неопределённое количество людей или предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "десятки",
                                "target_lang_sense_clarification": "о большом количестве"
                            },
                            {
                                "target_lang_word": "множество",
                                "target_lang_sense_clarification": "изобилие"
                            },
                            {
                                "target_lang_word": "уйма",
                                "target_lang_sense_clarification": "разг. очень много"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "145d62a1-866d-3415-b0d7-fcee7bd8130e",
                    "sense_definition": "An old English unit of measurement for iron ore, equal to twelve hundredweight.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measure_historical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The historical records state the mine produced a <w>dozen</w> of ore daily.",
                            "target_lang_translations": {
                                "ru": "В исторических записях говорится, что шахта добывала одну <w>дюжину</w> руды ежедневно."
                            }
                        },
                        {
                            "text": "Miners used the <w>dozen</w> to measure their output in the sixteenth century.",
                            "target_lang_translations": {
                                "ru": "В шестнадцатом веке шахтёры использовали <w>дюжину</w> для измерения своей выработки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical measurement"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная английская единица измерения железной руды, равная двенадцати хандредвейтам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дюжина",
                                "target_lang_sense_clarification": "старинная мера веса"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
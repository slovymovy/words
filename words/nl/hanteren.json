    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ad6aef1a-d003-3cd5-a757-a68c6d322d7d",
                    "sense_definition": "Iets gebruiken of bedienen, met name met de handen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jongen <w>hanteerde</w> het mes als een ware kok.",
                            "target_lang_translations": {
                                "en": "The boy <w>handled</w> the knife like a true chef.",
                                "ru": "Мальчик <w>орудовал</w> ножом, как настоящий повар."
                            }
                        },
                        {
                            "text": "Je moet voorzichtig zijn als je zwaar gereedschap <w>hanteert</w>.",
                            "target_lang_translations": {
                                "en": "You must be careful when you <w>handle</w> heavy tools.",
                                "ru": "Нужно быть осторожным, когда <w>обращаешься</w> с тяжелыми инструментами."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedienen",
                        "gebruiken"
                    ],
                    "common_phrases": [
                        "een wapen hanteren",
                        "de pen hanteren",
                        "gereedschap hanteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To use or operate something, especially with the hands.",
                        "ru": "Использовать или управлять чем-либо, особенно руками"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "handle",
                                "target_lang_sense_clarification": "general physical manipulation"
                            },
                            {
                                "target_lang_word": "wield",
                                "target_lang_sense_clarification": "tools or weapons, often implying skill"
                            },
                            {
                                "target_lang_word": "operate",
                                "target_lang_sense_clarification": "machinery or equipment"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "использовать инструмент или предмет"
                            },
                            {
                                "target_lang_word": "орудовать",
                                "target_lang_sense_clarification": "активно или умело действовать инструментом"
                            },
                            {
                                "target_lang_word": "владеть",
                                "target_lang_sense_clarification": "уметь пользоваться (оружием, пером)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a9f6316-2580-3092-854d-9955890344c0",
                    "sense_definition": "Op een bepaalde manier omgaan met abstracte zaken zoals regels, methodes of kritiek; toepassen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De school <w>hanteert</w> strenge regels voor te laat komen.",
                            "target_lang_translations": {
                                "en": "The school <w>applies</w> strict rules for arriving late.",
                                "ru": "Школа <w>применяет</w> строгие правила в отношении опозданий."
                            }
                        },
                        {
                            "text": "De chef kon hun kritiek niet goed <w>hanteren</w>.",
                            "target_lang_translations": {
                                "en": "The chef could not <w>handle</w> their criticism well.",
                                "ru": "Шеф не умел правильно <w>справляться</w> с их критикой."
                            }
                        }
                    ],
                    "synonyms": [
                        "toepassen"
                    ],
                    "common_phrases": [
                        "regels hanteren",
                        "een methode hanteren",
                        "criteria hanteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To deal with abstract matters such as rules, methods, or criticism; to apply.",
                        "ru": "Иметь дело с абстрактными вещами, такими как правила, методы или критика; применять"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "apply",
                                "target_lang_sense_clarification": "rules, criteria, or standards"
                            },
                            {
                                "target_lang_word": "handle",
                                "target_lang_sense_clarification": "situations, criticism, or emotions"
                            },
                            {
                                "target_lang_word": "employ",
                                "target_lang_sense_clarification": "methods or strategies"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "применять",
                                "target_lang_sense_clarification": "использовать правила, методы или критерии"
                            },
                            {
                                "target_lang_word": "справляться",
                                "target_lang_sense_clarification": "совладать с ситуацией или критикой"
                            },
                            {
                                "target_lang_word": "придерживаться",
                                "target_lang_sense_clarification": "следовать установленным правилам"
                            }
                        ]
                    }
                }
            ]
        }
    ]

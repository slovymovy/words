                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "divide",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cook <w>portioned</w> out the stew into four equal bowls.",
                            "target_lang_translations": {
                                "ru": "Повар <w>разделил</w> рагу по четырем равным мискам."
                            }
                        },
                        {
                            "text": "They <w>portioned</w> the land among the three heirs.",
                            "target_lang_translations": {
                                "ru": "Они <w>разделили</w> землю между тремя наследниками."
                            }
                        }
                    ],
                    "synonyms": [
                        "divide",
                        "apportion",
                        "distribute",
                        "allot"
                    ],
                    "common_phrases": [
                        "portion out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делить что-либо на части или доли для распределения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делить",
                                "target_lang_sense_clarification": "на части"
                            },
                            {
                                "target_lang_word": "распределять",
                                "target_lang_sense_clarification": "раздавать"

            "senses": [
                {
                    "sense_id": "879bcc96-abb9-376d-b8ed-749fcc488bee",
                    "sense_definition": "Budowla hydrotechniczna przegradzająca dolinę rzeki w celu spiętrzenia wody lub utworzenia zbiornika.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bobry budują <w>tamy</w>, aby podnieść poziom wody i ukryć wejścia do swoich nor.",
                            "target_lang_translations": {
                                "ru": "Бобры строят <w>плотины</w>, чтобы поднять уровень воды и скрыть входы в свои норы."
                            }
                        },
                        {
                            "text": "Wielka <w>tama</w> na rzece chroni miasto przed wiosennymi powodziami.",
                            "target_lang_translations": {
                                "ru": "Большая <w>плотина</w> на реке защищает город от весенних наводнений."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapora"
                    ],
                    "common_phrases": [
                        "tama na rzece",
                        "budowa tamy",
                        "przerwanie tamy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гидротехническое сооружение, перегораживающее реку для поднятия уровня воды или создания водохранилища."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плотина",
                                "target_lang_sense_clarification": "сооружение на реке"
                            },
                            {
                                "target_lang_word": "дамба",
                                "target_lang_sense_clarification": "насыпь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df6a1a4c-9df3-3582-991a-237934cd3bab",
                    "sense_definition": "Działanie lub przeszkoda mająca na celu powstrzymanie rozwoju jakiegoś zjawiska.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd postanowił położyć <w>tamę</w> nielegalnemu handlowi w internecie.",
                            "target_lang_translations": {
                                "ru": "Правительство решило положить <w>конец</w> нелегальной торговле в интернете."
                            }
                        },
                        {
                            "text": "Nowe przepisy stanowią skuteczną <w>tamę</w> dla oszustów podatkowych.",
                            "target_lang_translations": {
                                "ru": "Новые правила представляют собой эффективную <w>преграду</w> для налоговых мошенников."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapora"
                    ],
                    "common_phrases": [
                        "położyć tamę",
                        "stawiać tamę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или препятствие, направленное на сдерживание или прекращение какого-либо явления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преграда",
                                "target_lang_sense_clarification": "препятствие"
                            },
                            {
                                "target_lang_word": "заслон",
                                "target_lang_sense_clarification": "барьер, защита"
                            },
                            {
                                "target_lang_word": "конец",
                                "target_lang_sense_clarification": "в выражении 'положить конец'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3fa8967-f064-30f6-8e9d-6f798f8e2767",
                    "sense_definition": "Konstrukcja w kopalni oddzielająca od siebie wyrobiska, chroniąca przed wodą, ogniem lub gazami.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Górnicy zbudowali <w>tamę</w> wentylacyjną, aby skierować świeże powietrze w głąb szybu.",
                            "target_lang_translations": {
                                "ru": "Шахтеры построили вентиляционную <w>перемычку</w>, чтобы направить свежий воздух вглубь шахты."
                            }
                        },
                        {
                            "text": "Szczelna <w>tama</w> zapobiegła rozprzestrzenianiu się pożaru pod ziemią.",
                            "target_lang_translations": {
                                "ru": "Герметичная <w>перемычка</w> предотвратила распространение пожара под землей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tama wentylacyjna",
                        "tama podsadzkowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сооружение в шахте, изолирующее выработки и защищающее от воды, огня или газов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перемычка",

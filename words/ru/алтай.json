                        {
                            "text": "Мы изучали географию китайского округа <w>Алтай</w> на уроке.",
                            "target_lang_translations": {
                                "en": "We studied the geography of the Chinese <w>Altay</w> prefecture in class."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A prefecture in the Xinjiang Uygur Autonomous Region of China."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Altay",
                                "target_lang_sense_clarification": "prefecture in China"
                            }

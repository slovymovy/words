                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym budynku nie ma żadnych tajnych <dojść>.",
                            "target_lang_translations": {
                                "ru": "В этом здании нет никаких тайных <ходов>."
                            }
                        },
                        {
                            "text": "Szukaliśmy alternatywnych <dojść> do plaży.",
                            "target_lang_translations": {
                                "ru": "Мы искали альтернативные <подходы> к пляжу."
                            }
                        }
                    ],
                    "traits": [

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3aee0707-6a23-383f-8ef1-0cc4ce9eedfa",
                    "sense_definition": "Nie mieć racji w swoich sądach, przewidywaniach lub popełniać błąd w działaniu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "mistake_error",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy człowiek może się czasem <mylić>.",
                            "target_lang_translations": {
                                "ru": "Каждый человек может иногда <ошибаться>."
                            }
                        },
                        {
                            "text": "Jeśli myślisz, że to będzie łatwe zadanie, to się <mylisz>.",
                            "target_lang_translations": {
                                "ru": "Если ты думаешь, что это будет легкая задача, то ты <ошибаешься>."
                            }
                        },
                        {
                            "text": "Często się <mylisz> w swoich ocenach innych ludzi.",
                            "target_lang_translations": {
                                "ru": "Ты часто <ошибаешься> в своих оценках других людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "być w błędzie",
                        "być w błędzie"
                    ],
                    "antonyms": [
                        "mieć rację"
                    ],
                    "common_phrases": [
                        "grubo się mylić",
                        "mylić się co do kogoś",
                        "bardzo się mylić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть неправым в своих суждениях, прогнозах или совершать ошибку в действиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ошибаться",
                                "target_lang_sense_clarification": "совершать ошибку"
                            },
                            {
                                "target_lang_word": "заблуждаться",
                                "target_lang_sense_clarification": "иметь ложное мнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab560240-aa59-3a3a-899c-cd5006b48604",
                    "sense_definition": "Błędnie rozpoznawać kogoś lub coś, biorąc jedną osobę lub rzecz za inną.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "confusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze <mylę> te dwie bliźniaczki, bo są identyczne.",
                            "target_lang_translations": {
                                "ru": "Я всегда <путаю> этих двух близняшек, потому что они идентичны."
                            }
                        },
                        {
                            "text": "Jak można <mylić> izotopy z izotonami na lekcji chemii?",
                            "target_lang_translations": {
                                "ru": "Как можно <путать> изотопы с изотонами на уроке химии?"
                            }
                        },
                        {
                            "text": "Przepraszam, chyba <mylę> pana z kimś innym.",
                            "target_lang_translations": {
                                "ru": "Извините, я, наверное, <путаю> вас с кем-то другим."
                            }
                        }
                    ],
                    "synonyms": [
                        "być w błędzie",
                        "być w błędzie"
                    ],
                    "antonyms": [
                        "odróżniać",
                        "rozróżniać"
                    ],
                    "common_phrases": [
                        "mylić pojęcia",
                        "mylić imiona",
                        "mylić z kimś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ошибочно принимать кого-то или что-то за другого; не различать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путать",
                                "target_lang_sense_clarification": "принимать одно за другое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e577d86-8b03-334a-a639-de30c7ce86b5",
                    "sense_definition": "Celowo wprowadzać kogoś w błąd lub powodować utratę orientacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "deception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uciekający zając kluczył po polu, aby <mylić> psy gończe.",
                            "target_lang_translations": {
                                "ru": "Убегающий заяц петлял по полю, чтобы <сбивать с толку> гончих псов."
                            }
                        },
                        {
                            "text": "Zmienne wiatry <myliły> żeglarzy podczas sztormu.",
                            "target_lang_translations": {
                                "ru": "Переменные ветры <сбивали с толку> моряков во время шторма."
                            }
                        }
                    ],
                    "synonyms": [
                        "być w błędzie",
                        "być w błędzie"
                    ],
                    "antonyms": [
                        "naprowadzać"
                    ],
                    "common_phrases": [
                        "mylić trop",
                        "mylić krok",
                        "mylić pogoń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренно вводить в заблуждение или сбивать с толку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбивать с толку",
                                "target_lang_sense_clarification": "дезориентировать"
                            },
                            {
                                "target_lang_word": "путать",
                                "target_lang_sense_clarification": "мешать, сбивать мысли"
                            },
                            {
                                "target_lang_word": "запутывать",
                                "target_lang_sense_clarification": "вводить в заблуждение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

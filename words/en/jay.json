    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ba5b5e7d-77cb-351a-aef2-1dde8c46f863",
                    "sense_definition": "A noisy, colorful bird of the crow family, often having a crest, such as the blue jay or Eurasian jay.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The blue <w>jay</w> flew down to the feeder to grab a peanut.",
                            "target_lang_translations": {
                                "ru": "Голубая <w>сойка</w> слетела к кормушке, чтобы схватить арахис."
                            }
                        },
                        {
                            "text": "We heard the loud screech of a <w>jay</w> in the pine trees.",
                            "target_lang_translations": {
                                "ru": "Мы услышали громкий крик <w>сойки</w> в соснах."
                            }
                        }
                    ],
                    "synonyms": [
                        "bluejay"
                    ],
                    "common_phrases": [
                        "blue-jay",
                        "Eurasian jay"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шумная, пестрая птица семейства врановых, часто имеющая хохолок, например, голубая сойка или обыкновенная сойка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сойка",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fba5e98-02e7-35b7-851c-d01070819649",
                    "sense_definition": "The name of the tenth letter of the English alphabet, J.",

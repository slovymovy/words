{
    "word_family": [
        "jammeren",
        "jammerlijk",
        "jammerklacht",
        "jammerdal"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "59d092cd-60e3-3005-9765-f70017f76c68",
                    "sense_definition": "Teleurstellend of spijtig; wordt gebruikt om aan te geven dat men iets niet leuk vindt of betreurt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "unfortunate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <w>jammer</w> dat je niet op het feestje kon zijn.",
                            "target_lang_translations": {
                                "ru": "<w>Жаль</w>, что ты не смог быть на вечеринке."
                            }
                        },
                        {
                            "text": "Ik vind het erg <w>jammer</w> van de tijd die we verloren hebben.",
                            "target_lang_translations": {
                                "ru": "Мне очень <w>жаль</w> времени, которое мы потеряли."
                            }
                        }
                    ],
                    "synonyms": [
                        "spijtig",
                        "zonde",
                        "betreurenswaardig"
                    ],
                    "antonyms": [
                        "fijn",
                        "leuk",
                        "prettig"
                    ],
                    "common_phrases": [
                        "wat jammer",
                        "erg jammer",
                        "vind ik jammer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает сожаление или разочарование по поводу чего-либо; соответствует словам «жаль» или «обидно»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жаль",
                                "target_lang_sense_clarification": "выражение сожаления"
                            },
                            {
                                "target_lang_word": "обидно",
                                "target_lang_sense_clarification": "чувство досады"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "52f157e8-b18a-3ca8-bd99-c9ec8a3ba52d",
                    "sense_definition": "Een uitroep om teleurstelling uit te drukken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "exclamation_disappointment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jammer</w>! Ik had me er zo op verheugd.",
                            "target_lang_translations": {
                                "ru": "<w>Жаль</w>! Я так этого ждал."
                            }
                        },
                        {
                            "text": "Heb je de wedstrijd verloren? O, wat <w>jammer</w>!",
                            "target_lang_translations": {
                                "ru": "Ты проиграл матч? О, как <w>жаль</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "helaas",
                        "spijtig"
                    ],
                    "common_phrases": [
                        "jammer maar helaas",
                        "o wat jammer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, используемое для выражения разочарования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жаль",
                                "target_lang_sense_clarification": "восклицание"
                            },
                            {
                                "target_lang_word": "как жаль",
                                "target_lang_sense_clarification": "усиленное сожаление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62a3d009-5504-31ed-a451-b6b38813fbc0",
                    "sense_definition": "Spottend gebruikt om aan te geven dat men geen medelijden heeft met de ander.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "exclamation_sarcasm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jammer</w> dan! Had je maar beter moeten uitkijken.",
                            "target_lang_translations": {
                                "ru": "<w>Ну и ладно</w>! Надо было лучше смотреть."
                            }
                        },
                        {
                            "text": "Je bent te laat voor de taart, <w>jammer</w> dan.",
                            "target_lang_translations": {
                                "ru": "Ты опоздал на торт, <w>ну и ладно</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jammer dan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется с иронией или сарказмом, чтобы показать отсутствие сочувствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ну и ладно",
                                "target_lang_sense_clarification": "безразличие"
                            },
                            {
                                "target_lang_word": "ничего не поделаешь",
                                "target_lang_sense_clarification": "смирение или сарказм"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b8e45c7a-5666-3ba7-9892-3f8dae7f6054",
                    "sense_definition": "Gebruikt in de vaste verbinding 'jammer genoeg' om aan te geven dat iets te betreuren is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adverbial_regret",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben <w>jammer</w> genoeg geen kaarten meer kunnen krijgen.",
                            "target_lang_translations": {
                                "ru": "Мы, <w>к сожалению</w>, не смогли больше достать билетов."
                            }
                        },
                        {
                            "text": "Hij is <w>jammer</w> genoeg gezakt voor zijn examen.",
                            "target_lang_translations": {
                                "ru": "Он, <w>к сожалению</w>, провалил экзамен."
                            }
                        }
                    ],
                    "synonyms": [
                        "helaas"
                    ],
                    "antonyms": [
                        "gelukkig"
                    ],
                    "common_phrases": [
                        "jammer genoeg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в устойчивом выражении 'jammer genoeg' (к сожалению)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к сожалению",
                                "target_lang_sense_clarification": "вводное слово"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6bc18d9a-2187-3ddf-849c-5426e1c4558e",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>jammeren</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>jammer</w> niet snel als ik een beetje pijn heb.",
                            "target_lang_translations": {
                                "ru": "Я не <w>ною</w> сразу, если мне немного больно."
                            }
                        },
                        {
                            "text": "<w>Jammer</w> niet zo en doe er gewoon iets aan!",
                            "target_lang_translations": {
                                "ru": "Не <w>ной</w> так, а просто сделай что-нибудь!"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "also imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола 'jammeren' (ныть, жаловаться, причитать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ныть",
                                "target_lang_sense_clarification": "надоедливо жаловаться"
                            },
                            {
                                "target_lang_word": "причитать",
                                "target_lang_sense_clarification": "громко жаловаться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aa9df35b-cca0-35cd-8094-d5677c347120",
                    "sense_definition": "Een apparaat dat elektromagnetische signalen verstoort of blokkeert.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is verboden om in Nederland een <w>jammer</w> te gebruiken.",
                            "target_lang_translations": {
                                "ru": "В Нидерландах запрещено использовать <w>глушилку</w>."
                            }
                        },
                        {
                            "text": "De politie nam een <w>jammer</w> in beslag die het telefoonsignaal blokkeerde.",
                            "target_lang_translations": {
                                "ru": "Полиция конфисковала <w>глушилку</w>, которая блокировала телефонный сигнал."
                            }
                        }
                    ],
                    "synonyms": [
                        "stoorzender"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "English loanword"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство, которое нарушает или блокирует электромагнитные сигналы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глушилка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "подавитель сигнала",
                                "target_lang_sense_clarification": "техническое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b365d35-4c0e-3166-8b0a-530a1c7f3f61",
                    "sense_definition": "Groot verdriet, ellende of weeklacht.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "misery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>jammer</w> van de oorlog was in het hele land voelbaar.",
                            "target_lang_translations": {
                                "ru": "<w>Горе</w> войны ощущалось во всей стране."
                            }
                        },
                        {
                            "text": "Zijn luide <w>jammer</w> klonk door de lege straten.",
                            "target_lang_translations": {
                                "ru": "Его громкий <w>плач</w> звучал на пустых улицах."
                            }
                        }
                    ],
                    "synonyms": [
                        "ellende",
                        "weeklacht"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубокая скорбь, горе или громкий плач (устаревшее или литературное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горе",
                                "target_lang_sense_clarification": "беда, несчастье"
                            },
                            {
                                "target_lang_word": "плач",
                                "target_lang_sense_clarification": "громкие жалобы"
                            }

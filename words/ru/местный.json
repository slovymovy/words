                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who permanently lives in a given area."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "local",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a4c82f39-4bb3-3896-8578-dce411521345",
                    "sense_definition": "A principle in Chinese philosophy associated with light, heat, activity, and masculinity, which complements yin.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In traditional Chinese medicine, maintaining a balance between yin and <w>yang</w> is essential for health.",
                            "target_lang_translations": {
                                "ru": "В традиционной китайской медицине поддержание равновесия между инь и <w>ян</w> необходимо для здоровья."
                            }
                        },
                        {
                            "text": "The concept of <w>yang</w> represents the active, bright, and positive forces in the universe.",
                            "target_lang_translations": {
                                "ru": "Концепция <w>ян</w> олицетворяет активные, светлые и позитивные силы во Вселенной."
                            }
                        }
                    ],
                    "antonyms": [
                        "yin"
                    ],
                    "common_phrases": [
                        "yin and yang"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское начало в китайской философии, ассоциирующееся со светом, теплом и активностью; противоположность инь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ян",
                                "target_lang_sense_clarification": "философия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5af16161-408f-3318-b755-fbf88c095e13",
                    "sense_definition": "A historical monetary unit of Korea used from 1892 to 1902.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The antique dealer showed us a rare coin denominated in <w>yang</w>.",
                            "target_lang_translations": {
                                "ru": "Антиквар показал нам редкую монету номиналом в <w>ян</w>."
                            }
                        },
                        {
                            "text": "During the late 19th century, the <w>yang</w> was a standard unit of currency in Korea.",
                            "target_lang_translations": {
                                "ru": "В конце XIX века <w>ян</w> был стандартной денежной единицей в Корее."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": "historical currency"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Историческая денежная единица Кореи, использовавшаяся с 1892 по 1902 год."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ян",
                                "target_lang_sense_clarification": "валюта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "632f0972-1d11-3af0-9091-502bbd0532fa",
                    "sense_definition": "The honking cry made by a wild goose.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "goose_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We heard the loud <w>yang</w> of a wild goose flying over the lake.",
                            "target_lang_translations": {
                                "ru": "Мы услышали громкое <w>гоготание</w> дикого гуся, пролетающего над озером."
                            }
                        },
                        {
                            "text": "The hunter tried to imitate the <w>yang</w> of the birds to attract them.",
                            "target_lang_translations": {
                                "ru": "Охотник пытался имитировать <w>крик</w> птиц, чтобы привлечь их."
                            }
                        }
                    ],
                    "synonyms": [
                        "honk"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "rare usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гогочущий крик дикого гуся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гоготание",
                                "target_lang_sense_clarification": "звук"
                            },
                            {
                                "target_lang_word": "крик",
                                "target_lang_sense_clarification": "птицы"
                            }
                        ]
                    }
                }
            ]
        },
        {

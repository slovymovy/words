    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ff1e990c-bfa1-3412-8b06-3975b2a3969a",
                    "sense_definition": "Подвергаться действию физической силы притяжения (магнетизма, гравитации) или испытывать взаимное влечение.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мелкие металлические предметы мгновенно <w>притягиваются</w> к сильному магниту.",
                            "target_lang_translations": {
                                "en": "Small metal objects are instantly <w>attracted</w> to a strong magnet."
                            }
                        },
                        {
                            "text": "Говорят, что полные противоположности <w>притягиваются</w> друг к другу.",
                            "target_lang_translations": {
                                "en": "They say that complete opposites <w>attract</w> each other."
                            }
                        }
                    ],
                    "antonyms": [
                        "отталкиваться"
                    ],
                    "common_phrases": [
                        "противоположности притягиваются"
                    ],
                    "target_lang_definitions": {
                        "en": "To be acted upon by a physical force of attraction (magnetism, gravity) or to experience mutual attraction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be attracted",
                                "target_lang_sense_clarification": "by magnetism, gravity, or romantically"
                            },
                            {
                                "target_lang_word": "attract",
                                "target_lang_sense_clarification": "mutual attraction, e.g., opposites attract"
                            },
                            {
                                "target_lang_word": "gravitate",
                                "target_lang_sense_clarification": "to move towards something due to an attraction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f56b7d55-c845-30dc-933b-1d95d05d838d",
                    "sense_definition": "Страдательный залог к глаголу <w>притягивать</w>; быть объектом, который приближают, тащат или плотно прижимают к чему-либо.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Крышка плотно <w>притягивается</w> к корпусу четырьмя болтами.",
                            "target_lang_translations": {
                                "en": "The lid is tightly <w>fastened</w> to the housing with four bolts."
                            }
                        },
                        {
                            "text": "Внимание зрителей невольно <w>притягивается</w> к центру картины.",
                            "target_lang_translations": {
                                "en": "The viewers' attention is involuntarily <w>drawn</w> to the center of the painting."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Passive form of the verb 'to pull'; to be an object that is brought closer, dragged, or pressed tightly against something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be drawn",
                                "target_lang_sense_clarification": "to be pulled towards something, often abstract like attention"
                            },
                            {
                                "target_lang_word": "be tightened",
                                "target_lang_sense_clarification": "to be fastened securely, e.g., with bolts"
                            },
                            {
                                "target_lang_word": "be pulled",
                                "target_lang_sense_clarification": "general physical action"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убрать лайк",
                                "target_lang_sense_clarification": "отменить лайк"
                            },
                            {
                                "target_lang_word": "разлайкать",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }

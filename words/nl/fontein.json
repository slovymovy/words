    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e23afb66-2339-3b73-bfad-f3c2025e22e6",
                    "sense_definition": "Een kunstmatige installatie die water omhoog spuit, vaak ter decoratie op pleinen of in parken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "water_feature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen spelen in de zomer graag bij de <w>fontein</w>.",
                            "target_lang_translations": {
                                "ru": "Летом дети любят играть у <w>фонтана</w>."
                            }
                        },
                        {
                            "text": "Midden op het marktplein staat een prachtige oude <w>fontein</w>.",
                            "target_lang_translations": {
                                "ru": "Посреди рыночной площади стоит красивый старинный <w>фонтан</w>."
                            }
                        },
                        {
                            "text": "Het geluid van de spuitende <w>fontein</w> werkt erg rustgevend.",
                            "target_lang_translations": {
                                "ru": "Звук бьющего <w>фонтана</w> очень успокаивает."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een spuitende fontein",
                        "in de fontein",
                        "de fontein aanzetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственное сооружение, выбрасывающее струю воды вверх, часто служащее украшением на площадях или в парках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фонтан",
                                "target_lang_sense_clarification": "декоративное водное сооружение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc133eec-61ef-3418-b0ff-69195a86d7b9",
                    "sense_definition": "Een kleine wasbak met een kraan, meestal te vinden in een toiletruimte.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sanitary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het toiletbezoek waste hij zijn handen in de <w>fontein</w>.",
                            "target_lang_translations": {
                                "ru": "После посещения туалета он вымыл руки в <w>раковине</w>."
                            }
                        },
                        {
                            "text": "Er is in deze kleine ruimte nauwelijks plaats voor een <w>fontein</w>.",
                            "target_lang_translations": {
                                "ru": "В этом маленьком помещении едва хватает места для <w>рукомойника</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wasbekken"
                    ],
                    "common_phrases": [
                        "een fontein plaatsen",
                        "koud water"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая раковина с краном, обычно устанавливаемая в туалетной комнате для мытья рук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раковина",
                                "target_lang_sense_clarification": "для мытья рук"
                            },
                            {
                                "target_lang_word": "рукомойник",
                                "target_lang_sense_clarification": "небольшой умывальник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "343e2c1d-8abe-3a1c-84f8-fbf3d47fdf61",
                    "sense_definition": "Een soort vuurwerk dat een regen van vonken omhoog spuit zonder harde knallen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fireworks",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met oud en nieuw staken we een mooie gouden <w>fontein</w> af.",
                            "target_lang_translations": {
                                "ru": "На Новый год мы зажгли красивый золотой <w>фонтан</w>."
                            }
                        },
                        {
                            "text": "Deze <w>fontein</w> is veilig genoeg voor gebruik in een kleine straat.",
                            "target_lang_translations": {
                                "ru": "Этот <w>фонтан</w> достаточно безопасен для использования на узкой улице."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een fontein afsteken",
                        "siervuurwerk en fonteinen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид фейерверка, который выбрасывает сноп искр вверх, подобно струям воды, без громких взрывов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фонтан",
                                "target_lang_sense_clarification": "пиротехническое изделие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62da1fa4-9629-35a4-a242-0900bd9fcf47",
                    "sense_definition": "Een natuurlijke bron waaruit water opwelt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "natural_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vermoeide reizigers dronken vers water uit de heldere <w>fontein</w>.",
                            "target_lang_translations": {
                                "ru": "Уставшие путники пили свежую воду из чистого <w>источника</w>."
                            }
                        },
                        {
                            "text": "In oude gedichten wordt vaak gesproken over een <w>fontein</w> van eeuwige jeugd.",
                            "target_lang_translations": {
                                "ru": "В старых стихах часто говорится об <w>источнике</w> вечной молодости."
                            }
                        }
                    ],
                    "synonyms": [
                        "bron"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern Dutch usually uses 'bron'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественный выход подземных вод на поверхность земли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "источник",
                                "target_lang_sense_clarification": "природный"
                            },
                            {
                                "target_lang_word": "ключ",
                                "target_lang_sense_clarification": "родник"
                            }
                        ]
                    }
                }
            ]
        },

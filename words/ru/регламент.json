    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4db7ef88-0f26-3bd4-9bb7-bd514b5e51be",
                    "sense_definition": "Свод правил, устанавливающий порядок работы организации, учреждения или государственного органа.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rules",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый технический <w>регламент</w> вступит в силу со следующего месяца.",
                            "target_lang_translations": {
                                "en": "The new technical <w>regulations</w> will come into force next month."
                            }
                        },
                        {
                            "text": "Сотрудники обязаны знать служебный <w>регламент</w> компании.",
                            "target_lang_translations": {
                                "en": "Employees are required to know the company's service <w>regulations</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "устав"
                    ],
                    "common_phrases": [
                        "технический регламент",
                        "служебный регламент",
                        "утвердить регламент",
                        "нарушить регламент"
                    ],
                    "target_lang_definitions": {
                        "en": "A set of rules establishing the operating procedures of an organization, institution, or government body."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "regulations",
                                "target_lang_sense_clarification": "general rules or directives"
                            },
                            {
                                "target_lang_word": "rules of procedure",
                                "target_lang_sense_clarification": "formal operational rules"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ea67375-b07d-3229-a1dc-ffc08201d6b5",
                    "sense_definition": "Порядок ведения собрания или заседания, особенно в части ограничения времени выступлений.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "meeting_procedure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Председатель попросил всех выступающих соблюдать <w>регламент</w>.",
                            "target_lang_translations": {
                                "en": "The chairman asked all speakers to observe the <w>time limit</w>."
                            }
                        },
                        {
                            "text": "По <w>регламенту</w> у каждого докладчика есть только пять минут.",
                            "target_lang_translations": {
                                "en": "According to the <w>regulations</w>, each speaker has only five minutes."
                            }
                        }
                    ],
                    "common_phrases": [
                        "соблюдать регламент",
                        "превысить регламент",
                        "по регламенту",
                        "жёсткий регламент"
                    ],
                    "target_lang_definitions": {
                        "en": "The rules for conducting a meeting or session, specifically regarding limits on speaking time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "time limit",
                                "target_lang_sense_clarification": "restriction on duration"
                            },
                            {
                                "target_lang_word": "standing orders",
                                "target_lang_sense_clarification": "parliamentary or meeting procedure"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "903da8e3-b550-3985-ae58-a49cdfbda7e9",
                    "sense_definition": "Z pomyślnym rezultatem, bez problemów, przeszkód lub przykrych zdarzeń.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo burzy samolot wylądował <w>szczęśliwie</w> na lotnisku.",

            "senses": [
                {
                    "sense_id": "35612b45-5baf-3644-81f4-9bdadc210fef",
                    "sense_definition": "Мелкие семена этого растения, которые используются в пищу, чаще всего в выпечке и кондитерских изделиях.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "poppy_plant_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы купили к чаю свежие и ароматные булочки с <w>маком</w>.",
                            "target_lang_translations": {
                                "en": "We bought fresh and fragrant <w>poppy seed</w> buns for tea."
                            }
                        },
                        {
                            "text": "Бабушка испекла для внуков вкусный домашний рулет с <w>маком</w>.",
                            "target_lang_translations": {
                                "en": "Grandma baked a delicious homemade <w>poppy seed</w> roll for the grandchildren."
                            }
                        }
                    ],
                    "common_phrases": [
                        "булочка с маком",
                        "рулет с маком",
                        "кондитерский мак",
                        "пирожки с маком"
                    ],
                    "target_lang_definitions": {
                        "en": "Small seeds of this plant used for food, most often in baking and confectionery."
                    },
                    "translations": {

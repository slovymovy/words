                        {
                            "trait_type": "slang",
                            "comment": "gaming"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To ensure a team's victory through superior personal skill (gaming slang)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "gaming slang"
                            }
                        ]
                    }
                }
            ]

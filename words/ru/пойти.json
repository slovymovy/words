            "senses": [
                {
                    "sense_id": "3abadaae-2c69-31d5-a86c-fd7a4a849181",
                    "sense_definition": "Начать двигаться, перемещаться в каком-либо направлении; отправиться куда-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <пойти> домой, потому что очень устал.",
                            "target_lang_translations": {
                                "en": "I want to <go> home because I am very tired."
                            }
                        },
                        {
                            "text": "Завтра мы <пойдём> в кино вместе с друзьями.",
                            "target_lang_translations": {
                                "en": "Tomorrow we will <go> to the cinema together with friends."
                            }
                        },
                        {
                            "text": "Тебе нужно <пойти> прямо, а потом повернуть налево.",
                            "target_lang_translations": {
                                "en": "You need to <go> straight, and then turn left."
                            }
                        }
                    ],
                    "synonyms": [
                        "двинуться",
                        "зашагать"
                    ],
                    "antonyms": [
                        "остановиться",
                        "встать"
                    ],
                    "common_phrases": [
                        "пойти домой",
                        "пойти гулять",
                        "пойти спать",
                        "пойти пешком"
                    ],
                    "target_lang_definitions": {
                        "en": "To start moving, move in a direction; to set off somewhere."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "movement"
                            },
                            {
                                "target_lang_word": "set off",
                                "target_lang_sense_clarification": "start a journey"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a29b87a-acf0-3083-84d7-a422a6ce709d",
                    "sense_definition": "Начаться (о природных явлениях, осадках или событиях).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "start_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На улице внезапно <пошёл> сильный дождь.",
                            "target_lang_translations": {
                                "en": "Heavy rain suddenly <started> outside."
                            }
                        },
                        {
                            "text": "Если <пойдёт> снег, мы будем лепить снеговика.",
                            "target_lang_translations": {
                                "en": "If it <starts> snowing, we will build a snowman."
                            }

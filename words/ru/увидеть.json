{
    "word_family": [
        "видеть",
        "видение",
        "вид",
        "видимый",
        "предвидеть",
        "очевидец"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a5b45ca0-4777-3838-bd63-439c52dbed23",
                    "sense_definition": "Воспринять что-либо зрением, заметить глазами.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <увидеть> этот красивый город своими глазами.",
                            "target_lang_translations": {
                                "en": "I want to <w>see</w> this beautiful city with my own eyes."
                            }
                        },
                        {
                            "text": "Вчера в парке мы <увидели> маленькую белку.",
                            "target_lang_translations": {
                                "en": "Yesterday in the park we <w>saw</w> a small squirrel."
                            }
                        }
                    ],
                    "synonyms": [
                        "заметить"
                    ],
                    "antonyms": [
                        "развидеть"
                    ],
                    "common_phrases": [
                        "увидеть своими глазами",
                        "увидеть свет",
                        "рад увидеть"
                    ],
                    "target_lang_definitions": {
                        "en": "To perceive something with one's sight; to notice with one's eyes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "see",
                                "target_lang_sense_clarification": "general visual perception"
                            },
                            {
                                "target_lang_word": "catch sight of",
                                "target_lang_sense_clarification": "to notice suddenly"
                            },
                            {
                                "target_lang_word": "spot",
                                "target_lang_sense_clarification": "to locate or identify visually"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5463290a-bdc4-307c-b46f-764102cdc955",
                    "sense_definition": "Встретиться с кем-либо, провести время вместе.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я надеюсь <увидеть> тебя на празднике в субботу.",
                            "target_lang_translations": {
                                "en": "I hope to <w>see</w> you at the party on Saturday."
                            }
                        },
                        {
                            "text": "Мне нужно срочно <увидеть> врача.",
                            "target_lang_translations": {
                                "en": "I need to <w>see</w> a doctor urgently."
                            }
                        }
                    ],
                    "synonyms": [
                        "встретить"
                    ],
                    "common_phrases": [
                        "хочу увидеть",
                        "надеяться увидеть"
                    ],
                    "target_lang_definitions": {
                        "en": "To meet with someone or spend time together."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "see",
                                "target_lang_sense_clarification": "to meet socially or professionally"
                            },
                            {
                                "target_lang_word": "meet",
                                "target_lang_sense_clarification": "to encounter someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3db1895-b31a-3dc9-8a86-ef42816b564a",
                    "sense_definition": "Осознать, понять что-либо или обнаружить какое-то качество.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты скоро <увидишь>, что я был прав в этом споре.",
                            "target_lang_translations": {
                                "en": "You will soon <w>see</w> that I was right in this argument."
                            }
                        },
                        {
                            "text": "В его поступке можно <увидеть> глубокий смысл.",
                            "target_lang_translations": {
                                "en": "One can <w>see</w> deep meaning in his action."
                            }
                        }
                    ],
                    "synonyms": [
                        "понять",
                        "осознать"
                    ],
                    "common_phrases": [
                        "увидеть разницу",
                        "увидеть ошибку",
                        "увидеть смысл"
                    ],
                    "target_lang_definitions": {
                        "en": "To realize, understand, or discover a quality or fact."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "see",
                                "target_lang_sense_clarification": "to understand or realize"
                            },
                            {
                                "target_lang_word": "realize",
                                "target_lang_sense_clarification": "to become aware of a fact"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
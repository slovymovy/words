                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation of the first month of the year, January.",
                        "ru": "Сокращение от названия первого месяца года, января."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Jan.",
                                "target_lang_sense_clarification": "Abbreviation of January"
                            }

                    ],
                    "common_phrases": [
                        "niepopularny polityk",
                        "niepopularny uczeń",
                        "niepopularny autor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не пользующийся симпатией, признанием или интересом у многих людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непопулярный",
                                "target_lang_sense_clarification": "не пользующийся успехом или любовью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1d66f11-85ec-3249-acc7-ad9771b246b8",
                    "sense_definition": "Taki, który jest źle odbierany przez społeczeństwo, budzi sprzeciw lub jest niezgodny z powszechną opinią.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd podjął bardzo <w>niepopularną</w> decyzję o podwyższeniu podatków, co wywołało protesty.",
                            "target_lang_translations": {
                                "ru": "Правительство приняло очень <w>непопулярное</w> решение о повышении налогов, что вызвало протесты."
                            }
                        },
                        {
                            "text": "Jego <w>niepopularne</w> poglądy często prowadziły do gorących dyskusji w gronie znajomych.",
                            "target_lang_translations": {
                                "ru": "Его <w>непопулярные</w> взгляды часто приводили к жарким спорам в кругу знакомых."
                            }
                        }
                    ],
                    "synonyms": [
                        "nielubiany",
                        "kontrowersyjny"
                    ],
                    "antonyms": [
                        "popularny"
                    ],
                    "common_phrases": [
                        "niepopularna decyzja",
                        "niepopularne reformy",
                        "niepopularny pogląd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий неодобрение общества, возражения или противоречащий общественному мнению."

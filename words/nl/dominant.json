{
    "word_family": [
        "dominantie",
        "domineren"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "241b7ac6-b322-3dce-b614-20e8f3a00b97",
                    "sense_definition": "Overheersend; met meer invloed, macht of gezag dan anderen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf heeft een <w>dominante</w> positie op de wereldmarkt veroverd.",
                            "target_lang_translations": {
                                "ru": "Компания завоевала <w>доминирующее</w> положение на мировом рынке."
                            }
                        },
                        {
                            "text": "Zij speelt een <w>dominante</w> rol in de besluitvorming van de groep.",
                            "target_lang_translations": {
                                "ru": "Она играет <w>доминирующую</w> роль в принятии решений группы."
                            }
                        }
                    ],
                    "synonyms": [
                        "overheersend"
                    ],
                    "common_phrases": [
                        "dominante positie",
                        "dominante rol",
                        "dominant gedrag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преобладающий; имеющий больше влияния, власти или авторитета, чем другие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доминирующий",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "господствующий",
                                "target_lang_sense_clarification": "о положении или мнении"
                            },
                            {
                                "target_lang_word": "преобладающий",
                                "target_lang_sense_clarification": "численно или по силе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd2e06db-4334-3d00-aeba-a4e5cae3b233",
                    "sense_definition": "Zegt men van een erfelijke eigenschap die een andere eigenschap (de recessieve) onderdrukt en daardoor zichtbaar is.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bruine ogen zijn meestal een <w>dominant</w> kenmerk ten opzichte van blauwe ogen.",
                            "target_lang_translations": {
                                "ru": "Карие глаза обычно являются <w>доминантным</w> признаком по сравнению с голубыми глазами."
                            }
                        },
                        {
                            "text": "Het gen voor deze ziekte is <w>dominant</w> aanwezig.",
                            "target_lang_translations": {
                                "ru": "Ген этого заболевания является <w>доминантным</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "recessief"
                    ],
                    "common_phrases": [
                        "dominant gen",
                        "dominante overerving"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к наследственному признаку, который подавляет другой признак (рецессивный) и поэтому проявляется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доминантный",
                                "target_lang_sense_clarification": "генетика"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d20ea0b8-2faa-34f7-839f-21349ffb563b",
                    "sense_definition": "De vijfde toon van een diatonische toonladder.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de toonsoort C-groot is de noot G de <w>dominant</w>.",
                            "target_lang_translations": {
                                "ru": "В тональности до мажор нота соль — это <w>доминанта</w>."
                            }
                        },
                        {
                            "text": "Het muziekstuk moduleert tijdelijk naar de <w>dominant</w>.",
                            "target_lang_translations": {
                                "ru": "Музыкальное произведение временно модулирует в <w>доминанту</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tonica en dominant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пятая ступень диатонической гаммы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доминанта",
                                "target_lang_sense_clarification": "музыкальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77466031-4984-3413-96a0-9cc9bb88c30b",
                    "sense_definition": "Een erfelijke factor die sterker is dan de recessieve factor.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij deze kruising bepaalt de <w>dominant</w> de kleur van de bloem.",
                            "target_lang_translations": {
                                "ru": "При этом скрещивании <w>доминант</w> определяет цвет цветка."
                            }
                        },
                        {
                            "text": "Als beide ouders de <w>dominant</w> dragen, krijgt het kind de eigenschap ook.",
                            "target_lang_translations": {
                                "ru": "Если оба родителя несут <w>доминант</w>, ребенок также получит этот признак."
                            }
                        }
                    ],
                    "antonyms": [
                        "recessief"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наследственный фактор, который сильнее рецессивного фактора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доминант",
                                "target_lang_sense_clarification": "генетический фактор"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
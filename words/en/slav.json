    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "992db095-1e2f-3556-b1d1-390e2b774122",
                    "sense_definition": "A member of any of the peoples of Europe who speak the Slavic languages.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "Many <w>Slavs</w> settled in this region during the migration period.",
                            "target_lang_translations": {
                                "ru": "Многие <w>славяне</w> поселились в этом регионе в эпоху переселения народов."
                            }
                        },
                        {
                            "text": "Historians study the ancient <w>Slavs</w> to understand the cultural roots of Eastern Europe.",
                            "target_lang_translations": {
                                "ru": "Историки изучают древних <w>славян</w>, чтобы понять культурные корни Восточной Европы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ancient Slavs",
                        "southern Slavs",
                        "eastern Slavs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель группы народов Европы, говорящих на славянских языках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "славянин",
                                "target_lang_sense_clarification": "мужской род / общее название"
                            },
                            {
                                "target_lang_word": "славянка",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7a3e129-5e43-3d29-a453-8112b823020b",
                    "sense_definition": "The Slavonian grebe, a water bird found in northern regions.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The birdwatcher was excited to spot a rare <w>Slav</w> on the lake.",
                            "target_lang_translations": {
                                "ru": "Орнитолог был в восторге, заметив редкую <w>красношейную поганку</w> на озере."
                            }
                        },
                        {
                            "text": "We observed the nesting habits of the <w>Slav</w> near the reeds.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали за гнездованием <w>красношейной поганки</w> возле камышей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Красношейная поганка, водоплавающая птица, обитающая в северных регионах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красношейная поганка",
                                "target_lang_sense_clarification": "вид птицы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fcb8dc88-9ebc-3a53-b399-b89c16ce1b76",
                    "sense_definition": "Relating to the Slavic peoples, their languages, or their cultures.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She spoke French with a very <w>Slav</w> accent.",
                            "target_lang_translations": {
                                "ru": "Она говорила по-французски с сильным <w>славянским</w> акцентом."
                            }
                        },
                        {
                            "text": "The architecture in the old town has a distinctly <w>Slav</w> character.",
                            "target_lang_translations": {
                                "ru": "Архитектура старого города имеет отчетливо <w>славянский</w> характер."
                            }
                        }
                    ],
                    "synonyms": [
                        "Slavic"
                    ],
                    "common_phrases": [
                        "Slav languages",
                        "Slav accent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к славянским народам, их языкам или культуре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "славянский",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
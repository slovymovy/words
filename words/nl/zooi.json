{
    "word_family": [
        "rotzooi",
        "zooien"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c3be3150-1884-3360-8f22-eb9020d4ea9f",
                    "sense_definition": "Een rommelige toestand of chaos.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat een <w>zooi</w> is het hier in de woonkamer.",
                            "target_lang_translations": {
                                "ru": "Какой здесь в гостиной <w>бардак</w>."
                            }
                        },
                        {
                            "text": "Ruim je die <w>zooi</w> even op voordat je weggaat?",
                            "target_lang_translations": {
                                "ru": "Ты не уберешь этот <w>бардак</w>, прежде чем уйти?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zootje"
                    ],
                    "common_phrases": [
                        "wat een zooi",
                        "er een zooi van maken"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Беспорядок, хаос или очень грязное состояние помещения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бардак",
                                "target_lang_sense_clarification": "разговорное, полный беспорядок"
                            },
                            {
                                "target_lang_word": "беспорядок",
                                "target_lang_sense_clarification": "отсутствие порядка"
                            },
                            {
                                "target_lang_word": "свинарник",
                                "target_lang_sense_clarification": "разговорное, очень грязное место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62a1aca6-cdb6-352d-87f9-bb3ff4058153",
                    "sense_definition": "Een grote hoeveelheid spullen, vaak rommel of dingen van weinig waarde.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stuff",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb veel te veel <w>zooi</w> in mijn auto liggen.",
                            "target_lang_translations": {
                                "ru": "У меня в машине лежит слишком много <w>хлама</w>."
                            }
                        },
                        {
                            "text": "Gooi die oude <w>zooi</w> toch eens in de prullenbak.",
                            "target_lang_translations": {
                                "ru": "Выбрось уже этот старый <w>хлам</w> в мусорное ведро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oude zooi",
                        "een hoop zooi"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество вещей, часто старых, ненужных или не имеющих ценности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хлам",
                                "target_lang_sense_clarification": "старые или ненужные вещи"
                            },
                            {
                                "target_lang_word": "барахло",
                                "target_lang_sense_clarification": "разговорное, вещи низкого качества"
                            },
                            {
                                "target_lang_word": "мусор",
                                "target_lang_sense_clarification": "отходы, то, что нужно выбросить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b24adfaf-7419-32ae-bfe4-f8dffe600a5a",
                    "sense_definition": "Een hoeveelheid voedsel die gekookt is.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De visser at de warme <w>zooi</w> direct uit de pan.",
                            "target_lang_translations": {
                                "ru": "Рыбак ел это горячее <w>варево</w> прямо из кастрюли."
                            }
                        },
                        {
                            "text": "Ze schepte de dampende <w>zooi</w> op de borden.",
                            "target_lang_translations": {
                                "ru": "Она раскладывала дымящееся <w>варево</w> по тарелкам."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "or dialectal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество приготовленной (сваренной) еды, часто с оттенком простоты или небрежности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "варево",
                                "target_lang_sense_clarification": "жидкая вареная пища"
                            },
                            {
                                "target_lang_word": "похлёбка",
                                "target_lang_sense_clarification": "простой суп"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "04195298-e0fd-3057-8e8f-b42cfc5d0952",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>zooien</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>zooi</w> de aardappelen voor het avondeten.",
                            "target_lang_translations": {
                                "ru": "Я <w>варю</w> картошку на ужин."
                            }
                        },
                        {
                            "text": "Als ik vis <w>zooi</w>, ruikt het hele huis ernaar.",
                            "target_lang_translations": {
                                "ru": "Когда я <w>варю</w> рыбу, пахнет на весь дом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола 'zooien' (варить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "варю",
                                "target_lang_sense_clarification": "действие варки (я варю)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da53dd97-d0a7-3277-ab82-4d40a2c17c7c",
                    "sense_definition": "Gebiedende wijs van <w>zooien</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zooi</w> die groenten niet te lang, anders worden ze papperig.",
                            "target_lang_translations": {
                                "ru": "Не <w>вари</w> эти овощи слишком долго, иначе они разварятся."
                            }
                        },
                        {
                            "text": "<w>Zooi</w> de vis in een grote pan met water.",
                            "target_lang_translations": {
                                "ru": "<w>Вари</w> рыбу в большой кастрюле с водой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола 'zooien' (варить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вари",
                                "target_lang_sense_clarification": "приказ или просьба варить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
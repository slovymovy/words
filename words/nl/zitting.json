    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a40a5a31-d211-3d5c-b81d-de7ebac673ce",
                    "sense_definition": "Het gedeelte van een stoel of bank waar men op zit.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "chair_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>zitting</w> van deze oude stoel is helemaal versleten.",
                            "target_lang_translations": {
                                "ru": "<w>Сиденье</w> этого старого стула полностью изношено."
                            }
                        },
                        {
                            "text": "Ik zoek een bureaustoel met een zachte <w>zitting</w>.",
                            "target_lang_translations": {
                                "ru": "Я ищу офисный стул с мягким <w>сиденьем</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zachte zitting",
                        "leren zitting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть стула или дивана, на которой сидят."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сиденье",
                                "target_lang_sense_clarification": "мебель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ede4d4ec-2139-3510-9140-9667e98fdd28",
                    "sense_definition": "Een officiële bijeenkomst van een rechtbank, parlement of ander bestuurlijk orgaan.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter schorste de <w>zitting</w> voor een korte pauze.",
                            "target_lang_translations": {
                                "ru": "Судья прервал <w>заседание</w> на короткий перерыв."
                            }
                        },
                        {
                            "text": "Tijdens de openbare <w>zitting</w> waren veel journalisten aanwezig.",
                            "target_lang_translations": {
                                "ru": "Во время открытого <w>заседания</w> присутствовало много журналистов."
                            }
                        }
                    ],
                    "synonyms": [
                        "vergadering",
                        "sessie",
                        "bijeenkomst"
                    ],
                    "common_phrases": [
                        "openbare zitting",
                        "de zitting openen",
                        "de zitting schorsen",
                        "gesloten zitting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное собрание суда, парламента или другого административного органа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заседание",
                                "target_lang_sense_clarification": "суд, парламент"
                            },
                            {
                                "target_lang_word": "слушание",
                                "target_lang_sense_clarification": "судебное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08a2d0b9-4e3b-31cb-9499-6c90d8b176c1",
                    "sense_definition": "Het lidmaatschap van een bestuur, commissie of raad.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "membership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zal volgend jaar <w>zitting</w> nemen in het bestuur.",
                            "target_lang_translations": {
                                "ru": "В следующем году он войдет в <w>состав</w> правления."
                            }
                        },
                        {
                            "text": "Zij heeft al jaren <w>zitting</w> in de gemeenteraad.",
                            "target_lang_translations": {
                                "ru": "Она уже много лет является <w>членом</w> городского совета."
                            }
                        }
                    ],
                    "synonyms": [
                        "lidmaatschap"
                    ],
                    "common_phrases": [
                        "zitting nemen in",
                        "zitting hebben in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Членство в правлении, комитете или совете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "членство",
                                "target_lang_sense_clarification": "в организации"
                            },
                            {
                                "target_lang_word": "место",
                                "target_lang_sense_clarification": "в совете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70ae3631-c160-3410-bc0c-cd22dc633a39",
                    "sense_definition": "De periode waarin iemand poseert voor een kunstenaar of fotograaf.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "posing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het portret was na drie lange <w>zittingen</w> eindelijk klaar.",
                            "target_lang_translations": {
                                "ru": "Портрет был наконец готов после трех долгих <w>сеансов</w>."
                            }
                        },
                        {
                            "text": "De kunstenaar vroeg om nog een extra <w>zitting</w> om de details af te werken.",
                            "target_lang_translations": {
                                "ru": "Художник попросил еще об одном дополнительном <w>сеансе</w>, чтобы доработать детали."
                            }
                        }
                    ],
                    "synonyms": [
                        "sessie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период, в течение которого кто-то позирует художнику или фотографу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сеанс",
                                "target_lang_sense_clarification": "позирования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15e76c43-226d-3e61-bdba-7bc591fecd2f",
                    "sense_definition": "Het vlak in een kraan of klep dat zorgt voor de afdichting.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De monteur verving de versleten <w>zitting</w> van de kraan.",
                            "target_lang_translations": {
                                "ru": "Механик заменил изношенное <w>седло</w> крана."
                            }
                        },
                        {
                            "text": "Als de <w>zitting</w> beschadigd is, blijft de kraan lekken.",
                            "target_lang_translations": {
                                "ru": "Если <w>седло</w> повреждено, кран продолжает течь."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поверхность в кране или клапане, обеспечивающая герметичность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "седло",
                                "target_lang_sense_clarification": "техническое"
                            }
                        ]
                    }
                }
            ]
        }
    ]

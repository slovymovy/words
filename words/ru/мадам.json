            "pos": "noun",
            "senses": [
                {
                    "sense_id": "77f0037b-8ae1-32e9-9a9e-70730bc05964",
                    "sense_definition": "Наименование замужней женщины или вежливое обращение к женщине (обычно во Франции, в дореволюционной России и некоторых других странах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "address_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В комнату вошла элегантная <мадам> в шляпке с вуалью.",
                            "target_lang_translations": {
                                "en": "An elegant <w>madame</w> wearing a hat with a veil entered the room."
                            }
                        },
                        {
                            "text": "Прошу прощения, <мадам>, вы не подскажете, как пройти к театру?",
                            "target_lang_translations": {
                                "en": "Excuse me, <w>madam</w>, could you tell me how to get to the theater?"
                            }
                        }
                    ],
                    "synonyms": [
                        "госпожа",
                        "суда́рыня"
                    ],
                    "antonyms": [
                        "мадемуазель"
                    ],
                    "common_phrases": [
                        "бонжур мадам",
                        "мадам и месье"
                    ],
                    "target_lang_definitions": {
                        "en": "A title for a married woman or a polite form of address to a woman (especially in French contexts or pre-revolutionary Russia)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "madam",
                                "target_lang_sense_clarification": "polite form of address"
                            },
                            {
                                "target_lang_word": "Madame",
                                "target_lang_sense_clarification": "title or reference to a lady"
                            },
                            {
                                "target_lang_word": "ma'am",
                                "target_lang_sense_clarification": "polite address (short for madam)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "accf5561-039a-3339-99e2-0cd7f801cd18",
                    "sense_definition": "Гувернантка, воспитательница детей в богатой семье (обычно иностранка).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К детям была приставлена строгая <мадам>, которая говорила только по-французски.",
                            "target_lang_translations": {
                                "en": "A strict <w>governess</w> was assigned to the children, who spoke only French."
                            }
                        },
                        {
                            "text": "Наша старая <мадам> очень любила читать нам сказки перед сном.",
                            "target_lang_translations": {
                                "en": "Our old <w>governess</w> loved reading us fairy tales before bed."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "устаревшее значение"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A governess or female tutor in a wealthy family (usually of foreign origin)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "governess",
                                "target_lang_sense_clarification": "female private tutor"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e67e82c6-d0dd-3119-8617-dcf53fa80156",
                    "sense_definition": "To become a member of an organization, group, or society.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "membership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My younger brother wants to <w>join</w> the football team next season.",
                            "target_lang_translations": {
                                "ru": "Мой младший брат хочет <w>вступить</w> в футбольную команду в следующем сезоне."
                            }
                        },
                        {
                            "text": "She decided to <w>join</w> a gym to improve her health and fitness.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>записаться</w> в спортзал, чтобы улучшить здоровье и физическую форму."
                            }
                        }
                    ],
                    "synonyms": [
                        "enroll",
                        "enter",
                        "sign up"
                    ],
                    "antonyms": [
                        "detach",
                        "disconnect",
                        "disjoin",
                        "divide",
                        "unfasten"
                    ],
                    "common_phrases": [
                        "join a club",
                        "join the army",
                        "join a team",
                        "join a group"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать членом организации, группы или общества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вступать",
                                "target_lang_sense_clarification": "становиться участником организации"
                            },
                            {
                                "target_lang_word": "присоединяться",
                                "target_lang_sense_clarification": "становиться частью группы"
                            },
                            {
                                "target_lang_word": "записаться",
                                "target_lang_sense_clarification": "зарегистрироваться (в клуб, секцию)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea7b4678-1fc0-3aeb-acf2-5d3d84fc32d7",
                    "sense_definition": "To come into the company of someone; to meet or go with someone.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "accompaniment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please feel free to <w>join</w> us for dinner at the restaurant tonight.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не стесняйтесь <w>присоединиться</w> к нам за ужином в ресторане сегодня вечером."
                            }
                        },
                        {
                            "text": "I will <w>join</w> you in the meeting room as soon as I finish this call.",
                            "target_lang_translations": {
                                "ru": "Я <w>присоединюсь</w> к вам в переговорной, как только закончу этот звонок."
                            }
                        }
                    ],
                    "synonyms": [
                        "accompany",
                        "meet"
                    ],
                    "common_phrases": [
                        "join us",
                        "join me",
                        "join the party"
                    ],
                    "target_lang_definitions": {
                        "ru": "Присоединиться к чьей-либо компании; встретиться или пойти вместе с кем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присоединяться",
                                "target_lang_sense_clarification": "составить компанию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00efe320-850c-3099-9843-9d4f7f8ca315",
                    "sense_definition": "To connect or combine two or more things into one unit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter used strong glue to <w>join</w> the two pieces of wood together.",
                            "target_lang_translations": {
                                "ru": "Плотник использовал сильный клей, чтобы <w>соединить</w> два куска дерева вместе."
                            }
                        },
                        {
                            "text": "We should <w>join</w> hands to show that we are united in this cause.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>взяться</w> за руки, чтобы показать наше единство в этом деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "connect",
                        "link",
                        "attach",
                        "combine",
                        "unite",
                        "fasten",
                        "couple"
                    ],
                    "antonyms": [
                        "disconnect",
                        "detach",
                        "divide",
                        "unfasten",
                        "disjoin"
                    ],
                    "common_phrases": [
                        "join forces",
                        "join hands",
                        "join together"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединять или объединять два или более предмета в единое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединять",
                                "target_lang_sense_clarification": "связывать вместе"
                            },
                            {
                                "target_lang_word": "скреплять",
                                "target_lang_sense_clarification": "фиксировать соединение"
                            },
                            {
                                "target_lang_word": "объединять",
                                "target_lang_sense_clarification": "создавать единое целое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e47ff32c-0037-3052-8fe7-d077616a8d2d",
                    "sense_definition": "To come together to form a single entity, often used for rivers, roads, or lines.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "convergence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two small paths <w>join</w> near the top of the mountain to form a wider trail.",
                            "target_lang_translations": {
                                "ru": "Две маленькие тропинки <w>соединяются</w> у вершины горы, образуя более широкую тропу."
                            }
                        },
                        {
                            "text": "This river will eventually <w>join</w> the sea after flowing for hundreds of miles.",
                            "target_lang_translations": {
                                "ru": "Эта река в конечном итоге <w>сольется</w> с морем, протекая сотни миль."
                            }
                        }
                    ],
                    "synonyms": [
                        "merge",
                        "converge",
                        "meet"
                    ],
                    "antonyms": [
                        "divide"
                    ],
                    "common_phrases": [
                        "rivers join",
                        "roads join"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сходиться вместе, образуя единое целое (часто о реках, дорогах или линиях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединяться",
                                "target_lang_sense_clarification": "сходиться в одной точке"
                            },
                            {
                                "target_lang_word": "сливаться",
                                "target_lang_sense_clarification": "объединяться в один поток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84f75939-08f7-35c8-8230-627b99b21f74",
                    "sense_definition": "To combine data from two or more database tables based on a related column.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>join</w> the customer table with the orders table to see the purchase history.",
                            "target_lang_translations": {
                                "ru": "Вам нужно <w>объединить</w> таблицу клиентов с таблицей заказов, чтобы увидеть историю покупок."
                            }
                        },
                        {
                            "text": "The query failed because the system could not <w>join</w> the large datasets efficiently.",
                            "target_lang_translations": {
                                "ru": "Запрос не удался, потому что система не смогла эффективно <w>объединить</w> большие наборы данных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "join tables",
                        "join data"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединять данные из двух или более таблиц базы данных на основе связанного столбца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединять",
                                "target_lang_sense_clarification": "связывать данные"
                            },
                            {
                                "target_lang_word": "соединять",
                                "target_lang_sense_clarification": "техническое соединение таблиц"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "390d3957-3a95-381e-a826-d7d6d37fffab",
                    "sense_definition": "A place where two things are connected or fastened together.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "junction_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can hardly see the <w>join</w> where the two strips of wallpaper meet.",
                            "target_lang_translations": {
                                "ru": "Вы едва можете заметить <w>стык</w>, где встречаются две полосы обоев."
                            }
                        },
                        {
                            "text": "The welder inspected the <w>join</w> to ensure the metal structure was secure.",
                            "target_lang_translations": {
                                "ru": "Сварщик осмотрел <w>соединение</w>, чтобы убедиться в надежности металлической конструкции."
                            }
                        }
                    ],
                    "synonyms": [
                        "junction",
                        "connection",
                        "seam",
                        "joint"
                    ],
                    "common_phrases": [
                        "visible join",
                        "seamless join"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где два предмета соединены или скреплены вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стык",
                                "target_lang_sense_clarification": "место соприкосновения"
                            },
                            {
                                "target_lang_word": "соединение",
                                "target_lang_sense_clarification": "место скрепления"
                            },
                            {
                                "target_lang_word": "шов",
                                "target_lang_sense_clarification": "линия соединения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4495be31-5527-3b18-af0c-3b0e114330b6",
                    "sense_definition": "An operation that combines rows from two or more tables in a database.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "An inner <w>join</w> will only return rows when there is a match in both tables.",
                            "target_lang_translations": {
                                "ru": "Внутреннее <w>соединение</w> вернет строки только при наличии совпадения в обеих таблицах."
                            }
                        },
                        {
                            "text": "The database administrator optimized the <w>join</w> to speed up the report generation.",
                            "target_lang_translations": {
                                "ru": "Администратор базы данных оптимизировал <w>соединение</w>, чтобы ускорить создание отчета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "inner join",
                        "outer join",
                        "left join"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Операция, которая объединяет строки из двух или более таблиц в базе данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединение",
                                "target_lang_sense_clarification": "операция в базах данных"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
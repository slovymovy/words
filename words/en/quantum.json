                    }
                },
                {
                    "sense_id": "b85e08c4-266f-3708-a2ab-5248758f6e23",
                    "sense_definition": "A specific amount, portion, or share of something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "amount_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>quantum</w> of proof required in this case is exceptionally high.",

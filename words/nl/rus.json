        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "93af275b-0724-34ae-b1a0-bd4fde53f764",
                    "sense_definition": "Een inwoner van Rusland.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "De <w>Rus</w> vertelde enthousiast over zijn geboortestad Moskou.",
                            "target_lang_translations": {
                                "en": "The <w>Russian</w> spoke enthusiastically about his hometown Moscow.",
                                "ru": "<w>Русский</w> с энтузиазмом рассказывал о своем родном городе Москве."
                            }
                        },
                        {
                            "text": "Veel <w>Russen</w> vieren het kerstfeest in januari.",
                            "target_lang_translations": {
                                "en": "Many <w>Russians</w> celebrate Christmas in January.",
                                "ru": "Многие <w>русские</w> отмечают Рождество в январе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "geschreven met hoofdletter"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An inhabitant of Russia.",
                        "ru": "Житель России."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Russian",
                                "target_lang_sense_clarification": "person from Russia"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "русский",
                                "target_lang_sense_clarification": "национальность"
                            },
                            {
                                "target_lang_word": "россиянин",
                                "target_lang_sense_clarification": "гражданство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6081da22-f371-3fe7-b473-aa42abf5a186",
                    "sense_definition": "Een plant uit het geslacht Juncus (russenfamilie), die uiterlijk op gras lijkt en vaak op vochtige grond groeit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Langs de slootkant stond een grote pol <w>russen</w>.",
                            "target_lang_translations": {
                                "en": "A large clump of <w>rushes</w> stood along the ditch bank.",
                                "ru": "На краю канавы росла большая кочка <w>ситника</w>."
                            }
                        },
                        {
                            "text": "De stengel van de <w>rus</w> is meestal rond en onbehaard.",
                            "target_lang_translations": {
                                "en": "The stem of the <w>rush</w> is usually round and hairless.",
                                "ru": "Стебель <w>ситника</w> обычно круглый и гладкий."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A plant of the genus Juncus (rush family), resembling grass and often growing on moist ground.",
                        "ru": "Растение из рода Ситник (Juncus), внешне похожее на траву и часто растущее на влажной почве."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rush",
                                "target_lang_sense_clarification": "plant"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ситник",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84216658-5ba1-3d68-bf5b-8c434e45f1cd",
                    "sense_definition": "Een uitgestoken stuk grond met gras en wortels; een graszode.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tuinman legde een nieuwe <w>rus</w> op de kale plek in het gazon.",
                            "target_lang_translations": {
                                "en": "The gardener laid a new <w>sod</w> on the bare spot in the lawn.",
                                "ru": "Садовник положил новый кусок <w>дёрна</w> на голое место на газоне."
                            }
                        },
                        {
                            "text": "Hij stak voorzichtig een <w>rus</w> uit het weiland om de grond te inspecteren.",
                            "target_lang_translations": {
                                "en": "He carefully dug a <w>sod</w> out of the pasture to inspect the soil.",
                                "ru": "Он осторожно вырезал кусок <w>дёрна</w> с пастбища, чтобы осмотреть почву."
                            }
                        }
                    ],
                    "synonyms": [
                        "plag",
                        "zode"
                    ],
                    "target_lang_definitions": {
                        "en": "A cut piece of ground with grass and roots; a sod.",
                        "ru": "Вырезанный кусок земли с травой и корнями; дёрн."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sod",
                                "target_lang_sense_clarification": "piece of turf"
                            },
                            {
                                "target_lang_word": "turf",
                                "target_lang_sense_clarification": "layer of grass and earth"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дёрн",
                                "target_lang_sense_clarification": "верхний слой почвы"
                            },
                            {
                                "target_lang_word": "дернина",
                                "target_lang_sense_clarification": "пласт земли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46d9788a-1d24-3cc7-8702-5945b1f5b746",
                    "sense_definition": "Een rechercheur.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>rus</w> hield de verdachte al dagenlang onopvallend in de gaten.",
                            "target_lang_translations": {
                                "en": "The <w>detective</w> had been keeping an eye on the suspect unobtrusively for days.",
                                "ru": "<w>Сыщик</w> уже несколько дней незаметно следил за подозреваемым."
                            }
                        },
                        {
                            "text": "Pas op met wat je zegt, die man aan de bar is een <w>rus</w>.",
                            "target_lang_translations": {
                                "en": "Watch what you say, that man at the bar is a <w>cop</w>.",
                                "ru": "Осторожнее со словами, тот человек у бара — <w>сыщик</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Bargoens"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A detective (slang).",
                        "ru": "Детектив или полицейский (на сленге)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "detective",
                                "target_lang_sense_clarification": "police officer"
                            },
                            {
                                "target_lang_word": "cop",
                                "target_lang_sense_clarification": "slang term"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сыщик",
                                "target_lang_sense_clarification": "детектив"
                            },
                            {
                                "target_lang_word": "легавый",
                                "target_lang_sense_clarification": "полицейский (сленг)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbc71a79-7cee-3d79-b7a1-01971dbcd7de",
                    "sense_definition": "De plant Engels gras.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "plants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de duinen kun je de <w>rus</w> in de zomer zien bloeien.",
                            "target_lang_translations": {
                                "en": "In the dunes you can see the <w>thrift</w> blooming in the summer.",
                                "ru": "В дюнах летом можно увидеть, как цветет <w>армерия</w>."
                            }
                        },
                        {
                            "text": "Deze soort <w>rus</w> is goed bestand tegen de zoute zeewind.",
                            "target_lang_translations": {
                                "en": "This species of <w>thrift</w> is resistant to the salty sea wind.",
                                "ru": "Этот вид <w>армерии</w> хорошо переносит соленый морской ветер."
                            }
                        }
                    ],
                    "synonyms": [
                        "Engels gras"
                    ],
                    "target_lang_definitions": {
                        "en": "The plant thrift (Armeria maritima).",
                        "ru": "Растение армерия (армерия приморская)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thrift",
                                "target_lang_sense_clarification": "plant"
                            },
                            {
                                "target_lang_word": "sea pink",
                                "target_lang_sense_clarification": "plant"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "армерия",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                }
            ]

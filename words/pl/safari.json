                {
                    "sense_id": "1ce657ef-4cd3-36b5-bc16-052283054808",
                    "sense_definition": "Wyprawa do Afryki w celu obserwowania dzikich zwierząt w ich naturalnym środowisku lub polowania na nie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "expedition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marzę o tym, by kiedyś pojechać na <w>safari</w> do Kenii.",
                            "target_lang_translations": {
                                "ru": "Я мечтаю когда-нибудь поехать на <w>сафари</w> в Кению."
                            }
                        },
                        {
                            "text": "Podczas <w>safari</w> zrobiliśmy mnóstwo zdjęć lwom i żyrafom.",
                            "target_lang_translations": {
                                "ru": "Во время <w>сафари</w> мы сделали множество фотографий львов и жирафов."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyprawa"
                    ],
                    "common_phrases": [
                        "pojechać na safari",
                        "afrykańskie safari",
                        "zdjęcia z safari",
                        "uczestnik safari"
                    ],
                    "target_lang_definitions": {
                        "ru": "Экспедиция в Африку с целью наблюдения за дикими животными в их естественной среде обитания или охоты на них."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сафари",
                                "target_lang_sense_clarification": "путешествие"
                            }

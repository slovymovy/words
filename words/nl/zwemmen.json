    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "56d6328d-ea9e-38fa-8fc3-47ace083f8b8",
                    "sense_definition": "Zich door het water voortbewegen door bewegingen te maken met het lichaam",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer <w>zwemmen</w> wij vaak in de zee.",
                            "target_lang_translations": {
                                "en": "In the summer, we often <w>swim</w> in the sea.",
                                "ru": "Летом мы часто <w>плаваем</w> в море."
                            }
                        },
                        {
                            "text": "Hij heeft gisteren de hele middag in het meer <w>gezwommen</w>.",
                            "target_lang_translations": {
                                "en": "Yesterday, he <w>swam</w> in the lake all afternoon.",
                                "ru": "Вчера он весь день <w>плавал</w> в озере."
                            }
                        }
                    ],
                    "antonyms": [
                        "zinken"
                    ],
                    "common_phrases": [
                        "in zee zwemmen",
                        "leren zwemmen",
                        "baantjes zwemmen",
                        "gaan zwemmen"
                    ],
                    "target_lang_definitions": {
                        "en": "To move through the water by making movements with the body",
                        "ru": "Передвигаться в воде с помощью движений тела"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swim",
                                "target_lang_sense_clarification": "to move through water"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "плавать",
                                "target_lang_sense_clarification": "общее действие, умение или движение без определенного направления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ed68e6d-f440-301c-a298-84e3cf775fd1",
                    "sense_definition": "Al zwemmend een bepaalde plaats bereiken of een afstand afleggen",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jongen is helemaal naar de overkant <w>gezwommen</w>.",
                            "target_lang_translations": {
                                "en": "The boy <w>swam</w> all the way to the other side.",
                                "ru": "Мальчик <w>переплыл</w> на другую сторону."
                            }
                        },
                        {
                            "text": "Wij <w>zwommen</w> zo snel mogelijk terug naar de boot.",
                            "target_lang_translations": {
                                "en": "We <w>swam</w> back to the boat as fast as possible.",
                                "ru": "Мы <w>плыли</w> обратно к лодке как можно быстрее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naar de overkant zwemmen"
                    ],
                    "target_lang_definitions": {
                        "en": "To reach a specific place or cover a distance by swimming",
                        "ru": "Достигать определенного места или преодолевать расстояние вплавь"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swim",
                                "target_lang_sense_clarification": "to cover a distance or reach a destination"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "плыть",
                                "target_lang_sense_clarification": "движение в определенном направлении"
                            }
                        ]
                    }
                }
            ]
        }
    ]

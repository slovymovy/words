    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3086f1e2-d2ef-3692-96d5-62c96cf6d093",
                    "sense_definition": "Znak handlowy, nazwa producenta lub produktu, która pozwala odróżnić go od konkurencji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "business_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta <w>marka</w> produkuje bardzo niezawodne samochody.",
                            "target_lang_translations": {
                                "ru": "Эта <w>марка</w> производит очень надежные автомобили."
                            }
                        },
                        {
                            "text": "Kupiłem buty znanej <w>marki</w>, aby służyły mi przez lata.",
                            "target_lang_translations": {
                                "ru": "Я купил ботинки известной <w>марки</w>, чтобы они прослужили мне долгие годы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "znana marka",
                        "marka odzieżowa",
                        "własna marka",
                        "budować markę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торговый знак, название производителя или продукта, позволяющее отличить его от конкурентов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марка",
                                "target_lang_sense_clarification": "торговый знак"
                            },
                            {
                                "target_lang_word": "бренд",
                                "target_lang_sense_clarification": "современный синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6269cb35-0ce7-31ce-9ae9-992242ca7c36",
                    "sense_definition": "Jednostka monetarna używana dawniej m.in. w Niemczech, Finlandii i Polsce, a obecnie w Bośni i Hercegowinie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi dziadkowie trzymali oszczędności w niemieckich <w>markach</w>.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка хранили сбережения в немецких <w>марках</w>."
                            }
                        },
                        {
                            "text": "Przed wyjazdem do Berlina wymieniłem złotówki na <w>marki</w>.",
                            "target_lang_translations": {
                                "ru": "Перед поездкой в Берлин я обменял злотые на <w>марки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "marka niemiecka",
                        "milion marek",
                        "marka zamienna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная единица, использовавшаяся ранее в Германии, Финляндии и Польше, а ныне в Боснии и Герцеговине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марка",
                                "target_lang_sense_clarification": "валюта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9a3e4d0-e14b-360f-b555-f5568eef1897",
                    "sense_definition": "Dobra opinia, renoma i uznanie, jakimi cieszy się ktoś lub coś ze względu na wysoką jakość.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "business_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten lekarz ma wyrobioną <w>markę</w> w całym województwie.",
                            "target_lang_translations": {
                                "ru": "У этого врача отличная <w>репутация</w> во всем воеводстве."
                            }
                        },
                        {
                            "text": "Musimy dbać o jakość usług, aby trzymać <w>markę</w>.",
                            "target_lang_translations": {
                                "ru": "Мы должны заботиться о качестве услуг, чтобы держать <w>марку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "renoma",
                        "reputacja"
                    ],
                    "common_phrases": [
                        "trzymać markę",
                        "stracić markę",
                        "mieć markę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошая репутация, престиж и признание, которыми пользуется кто-то или что-то благодаря высокому качеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "репутация",
                                "target_lang_sense_clarification": "доброе имя"
                            },
                            {
                                "target_lang_word": "марка",
                                "target_lang_sense_clarification": "в выражении 'держать марку'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "817094c3-8739-3ec8-ae38-21bf8821a590",
                    "sense_definition": "Dawna jednostka wagi metali szlachetnych oraz dawna waluta.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiec zapłacił za towar jedną <w>marką</w> czystego srebra.",
                            "target_lang_translations": {
                                "ru": "Купец заплатил за товар одной <w>маркой</w> чистого серебра."
                            }
                        },
                        {
                            "text": "W średniowieczu <w>marka</w> była ważną jednostką obrachunkową.",
                            "target_lang_translations": {
                                "ru": "В Средние века <w>марка</w> была важной расчетной единицей."
                            }
                        }
                    ],
                    "synonyms": [
                        "grzywna"
                    ],
                    "common_phrases": [
                        "marka srebra"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historyczna jednostka miary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная единица веса драгоценных металлов, а также старинная валюта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марка",
                                "target_lang_sense_clarification": "мера веса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1a09909-0a57-3610-aa8f-c14199dccf83",
                    "sense_definition": "Przygraniczna jednostka administracyjna w dawnych państwach, rządzona przez margrabiego.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "territory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerze bronili granic wschodniej <w>marki</w> przed najazdami.",
                            "target_lang_translations": {
                                "ru": "Рыцари защищали границы восточной <w>марки</w> от набегов."
                            }
                        },
                        {
                            "text": "Władca nadał mu ziemie należące do nowej <w>marki</w>.",
                            "target_lang_translations": {
                                "ru": "Правитель пожаловал ему земли, принадлежащие новой <w>марке</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "margrabstwo"
                    ],
                    "common_phrases": [
                        "Marka Brandenburska"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "termin historyczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пограничная административная единица в средневековых государствах, управляемая маркграфом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марка",
                                "target_lang_sense_clarification": "административная единица"

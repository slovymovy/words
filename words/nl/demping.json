    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d5537cea-2594-3cd4-b48d-885ca9bb426b",
                    "sense_definition": "De handeling van het <w>dempen</w>, oftewel het verminderen van de intensiteit van geluid, licht of schokken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reduction_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>demping</w> in deze hardloopschoenen beschermt je knieën tegen blessures.",
                            "target_lang_translations": {
                                "en": "The <w>cushioning</w> in these running shoes protects your knees from injuries.",
                                "ru": "<w>Амортизация</w> в этих беговых кроссовках защищает ваши колени от травм."
                            }
                        },
                        {
                            "text": "Door de goede <w>demping</w> van de muren hoor je de buren bijna niet.",
                            "target_lang_translations": {
                                "en": "Due to the good <w>sound insulation</w> of the walls, you hardly hear the neighbors.",
                                "ru": "Благодаря хорошей <w>звукоизоляции</w> стен вы почти не слышите соседей."
                            }
                        }
                    ],
                    "synonyms": [
                        "afzwakking",
                        "reductie"
                    ],
                    "antonyms": [
                        "versterking"
                    ],
                    "common_phrases": [
                        "goede demping",
                        "demping van geluid"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of damping, or reducing the intensity of sound, light, or shocks.",
                        "ru": "Действие по снижению интенсивности звука, света или ударов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cushioning",
                                "target_lang_sense_clarification": "protection against impact, e.g., in shoes"
                            },
                            {
                                "target_lang_word": "shock absorption",
                                "target_lang_sense_clarification": "mechanical reduction of impact"
                            },
                            {
                                "target_lang_word": "damping",
                                "target_lang_sense_clarification": "general reduction of intensity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "амортизация",
                                "target_lang_sense_clarification": "смягчение ударов, например, в обуви"
                            },
                            {
                                "target_lang_word": "звукоизоляция",
                                "target_lang_sense_clarification": "снижение уровня шума"
                            },
                            {
                                "target_lang_word": "глушение",
                                "target_lang_sense_clarification": "общее ослабление звука или сигнала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b70553e5-5482-3382-bd4d-e556d862af80",
                    "sense_definition": "De mate waarin de uitslag of kracht van een trilling afneemt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reduction_physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ingenieurs berekenden de exacte <w>demping</w> die nodig was voor de brugconstructie.",
                            "target_lang_translations": {
                                "en": "The engineers calculated the exact <w>damping</w> needed for the bridge construction.",
                                "ru": "Инженеры рассчитали точное <w>демпфирование</w>, необходимое для конструкции моста."
                            }
                        },
                        {
                            "text": "Zonder voldoende <w>demping</w> bleef het systeem te lang natrillen.",
                            "target_lang_translations": {
                                "en": "Without sufficient <w>damping</w>, the system kept vibrating for too long.",
                                "ru": "Без достаточного <w>демпфирования</w> система продолжала вибрировать слишком долго."
                            }
                        }
                    ],
                    "synonyms": [
                        "attenuatie"
                    ],
                    "common_phrases": [
                        "kritische demping",
                        "magnetische demping"
                    ],
                    "target_lang_definitions": {
                        "en": "The degree to which the amplitude or force of a vibration decreases.",
                        "ru": "Степень, в которой уменьшается амплитуда или сила колебаний."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "damping",
                                "target_lang_sense_clarification": "physics: reduction of oscillation"
                            },
                            {
                                "target_lang_word": "attenuation",
                                "target_lang_sense_clarification": "reduction of signal or force strength"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "демпфирование",
                                "target_lang_sense_clarification": "физика: принудительное гашение колебаний"
                            },
                            {
                                "target_lang_word": "затухание",
                                "target_lang_sense_clarification": "естественное ослабление колебаний"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "272b3a2b-1efe-33bd-8188-9bf30e6c940a",
                    "sense_definition": "Een hand waarvan de vingers stevig naar binnen gebogen zijn tegen de handpalm.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij sloeg hard met zijn <w>vuist</w> op de houten tafel.",
                            "target_lang_translations": {
                                "ru": "Он сильно ударил <w>кулаком</w> по деревянному столу.",
                                "en": "He hit the wooden table hard with his <w>fist</w>."
                            }
                        },
                        {
                            "text": "Zij balde haar hand tot een <w>vuist</w> om haar woede te tonen.",
                            "target_lang_translations": {
                                "ru": "Она сжала руку в <w>кулак</w>, чтобы показать свой гнев.",
                                "en": "She clenched her hand into a <w>fist</w> to show her anger."
                            }
                        }
                    ],
                    "common_phrases": [
                        "met de vuist op tafel slaan",
                        "voor de vuist weg",
                        "in het vuistje lachen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кисть руки, пальцы которой плотно прижаты к ладони.",
                        "en": "A hand with the fingers firmly curled inwards against the palm."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кулак",
                                "target_lang_sense_clarification": "сжатая кисть руки"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fist",
                                "target_lang_sense_clarification": "clenched hand"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44b97b88-8431-3231-9890-4f038cf638de",
                    "sense_definition": "Een zware hamer met een korte steel en een vierkante kop.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stratenmaker gebruikte een <w>vuist</w> om de stenen aan te stampen.",

            "senses": [
                {
                    "sense_id": "dd57713c-362c-3797-b965-39c84e8a75a2",
                    "sense_definition": "Potoczna nazwa Zagłębia Dąbrowskiego, regionu historyczno-geograficznego w południowej Polsce.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mieszkańcy Śląska i <w>Zagłębia</w> często rywalizują ze sobą w sporcie.",
                            "target_lang_translations": {
                                "ru": "Жители Силезии и <w>Заглембе</w> часто соперничают друг с другом в спорте."
                            }
                        },
                        {
                            "text": "Sosnowiec jest nieoficjalną stolicą <w>Zagłębia</w>.",
                            "target_lang_translations": {
                                "ru": "Сосновец является неофициальной столицей <w>Заглембе</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Zagłębie Dąbrowskie"
                    ],
                    "common_phrases": [
                        "mieszkańcy Zagłębia",
                        "klub z Zagłębia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название Домбровского угольного бассейна, историко-географического региона на юге Польши."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Заглембе",
                                "target_lang_sense_clarification": "регион"
                            },
                            {
                                "target_lang_word": "Домбровский угольный бассейн",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                }
            ]
        }

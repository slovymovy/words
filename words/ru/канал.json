                    "frequency": "Low",
                    "semantic_group_id": "physical_channel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Огромный корабль медленно вошёл в узкий <канал>.",
                            "target_lang_translations": {
                                "en": "The huge ship slowly entered the narrow <channel>."
                            }

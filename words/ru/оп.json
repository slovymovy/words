    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "5d7d922e-9093-35c0-bc50-c1628ca90758",
                    "sense_definition": "Возглас, сопровождающий быстрое, резкое движение, прыжок или неожиданное изменение ситуации.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он разбежался, крикнул «<w>оп</w>» и перепрыгнул через забор.",
                            "target_lang_translations": {
                                "en": "He took a running start, shouted \"<w>hup</w>\", and jumped over the fence."
                            }
                        },
                        {
                            "text": "<w>Оп</w>, и мы уже оказались на другой стороне улицы.",
                            "target_lang_translations": {
                                "en": "<w>Boom</w>, and we were already on the other side of the street."
                            }
                        }
                    ],
                    "synonyms": [
                        "опа",
                        "хоп",
                        "оп-ля"
                    ],
                    "common_phrases": [
                        "оп и всё",
                        "оп и готово"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation accompanying a quick, sharp movement, jump, or unexpected change in a situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hup",
                                "target_lang_sense_clarification": "sound made during exertion or a jump"
                            },
                            {
                                "target_lang_word": "hop",
                                "target_lang_sense_clarification": "sound accompanying a jump or quick movement"
                            },
                            {
                                "target_lang_word": "boom",
                                "target_lang_sense_clarification": "indicating a sudden change or result"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff12bbc3-afda-3230-9dd4-3f45655142a9",
                    "sense_definition": "Команда или призыв к началу энергичного действия, прыжку или физическому усилию.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Когда я скажу «<w>оп</w>», дружно поднимаем коробку.",
                            "target_lang_translations": {
                                "en": "When I say \"<w>up</w>\", we lift the box together."
                            }
                        },
                        {
                            "text": "Давайте все вместе на счёт три: <w>оп</w>!",
                            "target_lang_translations": {
                                "en": "Let's all do it together on the count of three: <w>go</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "гоп"
                    ],
                    "common_phrases": [
                        "раз-два и оп"
                    ],
                    "target_lang_definitions": {
                        "en": "A command or call to start an energetic action, jump, or physical effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "up",
                                "target_lang_sense_clarification": "command to lift or jump"
                            },
                            {
                                "target_lang_word": "heave",
                                "target_lang_sense_clarification": "command for heavy lifting"
                            },
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "signal to start an action"
                            }
                        ]
                    }
                }
            ]
        },

                {
                    "sense_id": "daa457d3-7848-36e4-8ddd-19e41a9730fe",
                    "sense_definition": "Not doubting; unsuspecting.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "trust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She accepted the stranger's explanation with <w>indubious</w> confidence.",
                            "target_lang_translations": {

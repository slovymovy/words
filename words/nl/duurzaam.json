            "senses": [
                {
                    "sense_id": "af8029e3-308b-35bf-8b62-ba20ed062271",
                    "sense_definition": "Met zo min mogelijk negatieve gevolgen voor het milieu en het klimaat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zon, wind en warmte zijn <w>duurzame</w> energiebronnen.",
                            "target_lang_translations": {
                                "en": "Sun, wind, and heat are <w>sustainable</w> energy sources.",
                                "ru": "Солнце, ветер и тепло — это <w>возобновляемые</w> источники энергии."
                            }
                        },
                        {
                            "text": "Veel bedrijven kiezen tegenwoordig voor <w>duurzame</w> energie om het milieu te sparen.",
                            "target_lang_translations": {
                                "en": "Many companies nowadays choose <w>sustainable</w> energy to save the environment.",
                                "ru": "Многие компании сегодня выбирают <w>экологичную</w> энергию, чтобы сберечь окружающую среду."
                            }
                        }
                    ],
                    "synonyms": [
                        "milieuvriendelijk",
                        "groen",
                        "ecologisch"
                    ],
                    "antonyms": [
                        "onduurzaam"
                    ],
                    "common_phrases": [
                        "duurzame energie",
                        "duurzame ontwikkeling",
                        "duurzaam leven"
                    ],
                    "target_lang_definitions": {
                        "en": "With minimal negative impact on the environment and climate.",
                        "ru": "С минимальными негативными последствиями для окружающей среды и климата."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sustainable",
                                "target_lang_sense_clarification": "environmentally friendly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "экологичный",
                                "target_lang_sense_clarification": "безопасный для природы"
                            },
                            {
                                "target_lang_word": "устойчивый",
                                "target_lang_sense_clarification": "не истощающий ресурсы (об экономике, развитии)"
                            },
                            {
                                "target_lang_word": "возобновляемый",
                                "target_lang_sense_clarification": "об источниках энергии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c3b4414-a1aa-3e79-bc96-a4a11b06682c",
                    "sense_definition": "Gemaakt om lang mee te gaan zonder te slijten of kapot te gaan; van degelijke kwaliteit.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ons bankstel is zo <w>duurzaam</w> dat onze kleinkinderen er waarschijnlijk nog op kunnen zitten.",
                            "target_lang_translations": {
                                "en": "Our sofa is so <w>durable</w> that our grandchildren will probably still be able to sit on it.",
                                "ru": "Наш диван настолько <w>долговечный</w>, что наши внуки, вероятно, еще смогут на нем сидеть."
                            }
                        },
                        {
                            "text": "Zij hebben al jaren een <w>duurzame</w> relatie en zijn erg gelukkig samen.",
                            "target_lang_translations": {
                                "en": "They have had a <w>lasting</w> relationship for years and are very happy together.",
                                "ru": "У них уже много лет <w>прочные</w> отношения, и они очень счастливы вместе."
                            }
                        }
                    ],
                    "synonyms": [
                        "degelijk",
                        "slijtvast",
                        "bestendig"
                    ],
                    "antonyms": [
                        "onduurzaam"
                    ],
                    "common_phrases": [
                        "duurzame goederen",
                        "een duurzame relatie",
                        "duurzame oplossing"
                    ],
                    "target_lang_definitions": {
                        "en": "Made to last a long time without wearing out or breaking; of solid quality.",
                        "ru": "Рассчитанный на долгий срок службы; добротного качества."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "durable",
                                "target_lang_sense_clarification": "able to resist wear"
                            },
                            {
                                "target_lang_word": "long-lasting",
                                "target_lang_sense_clarification": "enduring for a long time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "долговечный",
                                "target_lang_sense_clarification": "служащий долго"
                            },
                            {
                                "target_lang_word": "прочный",
                                "target_lang_sense_clarification": "надежный, крепкий (о вещах или отношениях)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "a76f7b1e-1771-3dd4-a9c1-6ed479db5c3f",
                    "sense_definition": "Een jongensnaam van Hebreeuwse oorsprong.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn neef heet <w>Saul</w> en hij woont in Amsterdam.",
                            "target_lang_translations": {
                                "en": "My cousin is named <w>Saul</w> and he lives in Amsterdam.",
                                "ru": "Моего племянника зовут <w>Саул</w>, и он живет в Амстердаме."
                            }
                        },
                        {
                            "text": "We hebben besloten onze zoon <w>Saul</w> te noemen.",
                            "target_lang_translations": {
                                "en": "We have decided to name our son <w>Saul</w>.",
                                "ru": "Мы решили назвать нашего сына <w>Саулом</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {

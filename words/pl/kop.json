                {
                    "sense_id": "28b2d705-3351-3712-bdef-59a409244bef",
                    "sense_definition": "Uderzenie zadane nogą, stopą lub kolanem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napastnik wymierzył ofierze silnego <w>kopa</w> w brzuch.",

            "senses": [
                {
                    "sense_id": "fe54ed3c-233b-3670-9f5a-301f650b2ab4",
                    "sense_definition": "Een mannelijke kip of een mannetje van andere hoenderachtige vogels.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>haan</w> kraait elke ochtend luid bij zonsopgang.",
                            "target_lang_translations": {
                                "en": "The <w>rooster</w> crows loudly every morning at sunrise.",
                                "ru": "<w>Петух</w> громко кукарекает каждое утро на рассвете."
                            }
                        },
                        {
                            "text": "Op de kinderboerderij loopt een grote, kleurrijke <w>haan</w> rond.",
                            "target_lang_translations": {
                                "en": "A large, colorful <w>rooster</w> is walking around at the petting zoo.",
                                "ru": "По контактному зоопарку ходит большой разноцветный <w>петух</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "er kraait geen haan naar",
                        "haantje de voorste"
                    ],
                    "target_lang_definitions": {
                        "en": "A male chicken or a male of other gallinaceous birds.",
                        "ru": "Самец курицы или самец других курообразных птиц."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rooster",
                                "target_lang_sense_clarification": "male domestic chicken"
                            },
                            {
                                "target_lang_word": "cock",
                                "target_lang_sense_clarification": "male bird"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "петух",
                                "target_lang_sense_clarification": "домашняя птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7779443-b748-3dbb-aaff-37421beb473a",
                    "sense_definition": "Een man die zich opvallend stoer, dominant of ijdel gedraagt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij loopt erbij als een trotse <w>haan</w> om indruk te maken.",
                            "target_lang_translations": {
                                "en": "He walks around like a proud <w>rooster</w> to make an impression.",
                                "ru": "Он ходит как гордый <w>петух</w>, чтобы произвести впечатление."
                            }
                        },
                        {
                            "text": "Die jongen is echt een <w>haan</w> en wil altijd de baas spelen.",
                            "target_lang_translations": {
                                "en": "That boy is a real <w>show-off</w> and always wants to play the boss.",
                                "ru": "Этот парень — настоящий <w>задира</w> и всегда хочет командовать."
                            }
                        }
                    ],
                    "synonyms": [
                        "mannetje"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe behavior"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A man who behaves in a remarkably tough, dominant, or vain manner.",
                        "ru": "Мужчина, который ведет себя вызывающе, властно или тщеславно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rooster",
                                "target_lang_sense_clarification": "figurative: proud or pompous person"
                            },
                            {
                                "target_lang_word": "show-off",
                                "target_lang_sense_clarification": "vain person"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "задира",
                                "target_lang_sense_clarification": "человек, ищущий ссоры"
                            },
                            {
                                "target_lang_word": "петух",
                                "target_lang_sense_clarification": "перен. заносчивый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9f6f32f-6e1a-3b6d-9d82-e701a8a1452b",
                    "sense_definition": "Het onderdeel van een vuurwapen dat een slaande beweging maakt om het schot te lossen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weapon_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schutter spande de <w>haan</w> van zijn revolver voordat hij mikte.",
                            "target_lang_translations": {
                                "en": "The shooter cocked the <w>hammer</w> of his revolver before he aimed.",
                                "ru": "Стрелок взвел <w>курок</w> своего револьвера, прежде чем прицелиться."
                            }
                        },
                        {
                            "text": "Zonder een goed werkende <w>haan</w> kan het pistool niet vuren.",
                            "target_lang_translations": {
                                "en": "Without a properly working <w>hammer</w>, the pistol cannot fire.",
                                "ru": "Без исправного <w>курка</w> пистолет не может выстрелить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de haan spannen",
                        "de haan overhalen"
                    ],
                    "target_lang_definitions": {
                        "en": "The part of a firearm that makes a striking motion to discharge the shot.",
                        "ru": "Деталь огнестрельного оружия, которая совершает ударное движение для производства выстрела."
                    },
                    "translations": {
                        "en": [

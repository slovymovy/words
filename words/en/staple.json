            "senses": [
                {
                    "sense_id": "0a3d2c15-063d-329d-af18-2db853f5cc90",
                    "sense_definition": "Forming a basic, large, or important part of something; standard or principal.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "essential_adj",
                    "name_type": "no",
                    "examples": [

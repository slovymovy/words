            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d3ded81-c86d-3ff3-82ae-2188a5accebe",
                    "sense_definition": "Iets geheim houden of aan het oog onttrekken zodat het niet zichtbaar is.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "hiding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dief probeerde de gestolen goederen snel te <w>verduiken</w>.",
                            "target_lang_translations": {
                                "en": "The thief tried to quickly <w>hide</w> the stolen goods.",
                                "ru": "Вор попытался быстро <w>спрятать</w> украденные товары."
                            }
                        },
                        {
                            "text": "Zij kon haar teleurstelling nauwelijks <w>verduiken</w> tijdens het gesprek.",
                            "target_lang_translations": {
                                "en": "She could hardly <w>conceal</w> her disappointment during the conversation.",
                                "ru": "Она едва могла <w>скрыть</w> свое разочарование во время разговора."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbergen",
                        "verhullen",
                        "wegstoppen"
                    ],
                    "target_lang_definitions": {
                        "en": "To keep something secret or hide it from view.",
                        "ru": "Держать что-то в секрете или скрывать от глаз, чтобы это не было видно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hide",
                                "target_lang_sense_clarification": "to put something out of sight"
                            },
                            {
                                "target_lang_word": "conceal",
                                "target_lang_sense_clarification": "to prevent something from being seen or known"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "скрывать",
                                "target_lang_sense_clarification": "делать невидимым или тайным"
                            },
                            {
                                "target_lang_word": "прятать",
                                "target_lang_sense_clarification": "убирать с глаз долой"
                            },
                            {
                                "target_lang_word": "утаивать",
                                "target_lang_sense_clarification": "держать в секрете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cf1e1bb-3ebc-3722-bf01-ec7c6309c7d0",
                    "sense_definition": "Wegduiken of zich snel naar beneden of weg bewegen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De eend wist zich behendig in het riet te <w>verduiken</w>.",
                            "target_lang_translations": {
                                "en": "The duck managed to skillfully <w>hide</w> in the reeds.",
                                "ru": "Утка сумела ловко <w>спрятаться</w> в камышах."
                            }
                        },
                        {
                            "text": "Hij moest zich <w>verduiken</w> om niet geraakt te worden door de bal.",
                            "target_lang_translations": {
                                "en": "He had to <w>duck</w> to avoid being hit by the ball.",
                                "ru": "Ему пришлось <w>пригнуться</w>, чтобы в него не попал мяч."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To duck away or quickly move out of sight.",
                        "ru": "Быстро пригнуться или скрыться из виду."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "duck",
                                "target_lang_sense_clarification": "to lower the head or body quickly to avoid something"
                            },
                            {
                                "target_lang_word": "hide",
                                "target_lang_sense_clarification": "to put oneself out of sight"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спрятаться",
                                "target_lang_sense_clarification": "скрыть себя от взгляда"
                            },
                            {
                                "target_lang_word": "пригнуться",
                                "target_lang_sense_clarification": "наклониться вниз, чтобы избежать чего-либо"
                            },
                            {
                                "target_lang_word": "укрыться",
                                "target_lang_sense_clarification": "найти убежище"
                            }
                        ]
                    }
                }
            ]
        }
    ]

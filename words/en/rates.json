                },
                {
                    "sense_id": "aa05c2e9-2d10-3e54-886d-2aff560384c4",
                    "sense_definition": "Taxes, usually on property, levied by local government.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Homeowners have to pay their council <w>rates</w> annually.",
                            "target_lang_translations": {
                                "ru": "Домовладельцы должны платить муниципальные <w>налоги</w> ежегодно."
                            }

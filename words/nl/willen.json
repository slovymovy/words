{
    "word_family": [
        "wil",
        "gewild",
        "welwillend",
        "willekeur",
        "vrijwillig",
        "moedwil",
        "tegenwil"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6ee8b9a9-e9c0-3920-b9bb-dfbd620b762e",
                    "sense_definition": "Iets graag willen hebben of wensen te bezitten.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "desire_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>wil</w> graag een ijsje met slagroom.",
                            "target_lang_translations": {
                                "en": "I <w>would like</w> an ice cream with whipped cream.",
                                "ru": "Я очень <w>хочу</w> мороженое со взбитыми сливками."
                            }
                        },
                        {
                            "text": "Zij <w>wil</w> liever thee dan koffie bij het ontbijt.",
                            "target_lang_translations": {
                                "en": "She <w>wants</w> tea rather than coffee for breakfast.",
                                "ru": "Она больше <w>хочет</w> чай, чем кофе на завтрак."
                            }
                        }
                    ],
                    "synonyms": [
                        "wensen"
                    ],
                    "common_phrases": [
                        "graag willen",
                        "iets willen"
                    ],
                    "target_lang_definitions": {
                        "en": "To really want to have something or wish to possess it.",
                        "ru": "Сильно желать что-то иметь или владеть чем-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "want",
                                "target_lang_sense_clarification": "general desire for an object"
                            },
                            {
                                "target_lang_word": "would like",
                                "target_lang_sense_clarification": "polite expression of desire"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "испытывать желание обладать чем-либо"
                            },
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "более формальный синоним хотеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a4ee51f-6732-3052-8300-fa3097cf16ad",
                    "sense_definition": "Het voornemen of de wens hebben om een handeling te verrichten.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "desire_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>wil</w> vroeg gaan slapen vanavond.",
                            "target_lang_translations": {
                                "en": "He <w>wants</w> to go to sleep early tonight.",
                                "ru": "Он <w>хочет</w> лечь спать пораньше сегодня вечером."
                            }
                        },
                        {
                            "text": "Wij <w>willen</w> volgend jaar naar Spanje verhuizen.",
                            "target_lang_translations": {
                                "en": "We <w>want</w> to move to Spain next year.",
                                "ru": "Мы <w>хотим</w> переехать в Испанию в следующем году."
                            }
                        }
                    ],
                    "synonyms": [
                        "wensen"
                    ],
                    "common_phrases": [
                        "willen doen",
                        "niet willen"
                    ],
                    "target_lang_definitions": {
                        "en": "To have the intention or wish to perform an action.",
                        "ru": "Иметь намерение или желание совершить действие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "want",
                                "target_lang_sense_clarification": "desire to do something"
                            },
                            {
                                "target_lang_word": "intend",
                                "target_lang_sense_clarification": "plan or mean to do something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "планировать или желать сделать что-то"
                            },
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "иметь намерение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a3447d2-f2c6-3ef6-8dbd-53cc2d60491f",
                    "sense_definition": "Gebruikt om een vraag of verzoek beleefd of dringend te formuleren.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wil</w> je even naar de keuken gaan?",
                            "target_lang_translations": {
                                "en": "<w>Will</w> you go to the kitchen for a moment?",
                                "ru": "<w>Хочешь</w> сходить на кухню?"
                            }
                        },
                        {
                            "text": "<w>Wilt</w> u mij helpen met deze zware tas?",
                            "target_lang_translations": {
                                "en": "<w>Would</w> you help me with this heavy bag?",
                                "ru": "Не <w>хотите</w> ли вы помочь мне с этой тяжелой сумкой?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "wil je even",
                        "wilt u"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to formulate a question or request politely or urgently.",
                        "ru": "Используется для вежливого или настойчивого выражения просьбы или вопроса."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "will",
                                "target_lang_sense_clarification": "used in requests"
                            },
                            {
                                "target_lang_word": "would",
                                "target_lang_sense_clarification": "polite request form"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "в вопросительных конструкциях (не хочешь ли ты...)"
                            },
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "вежливая форма (желаете ли вы...)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5f97539-d964-3093-a32b-4aeeecea49d1",
                    "sense_definition": "Wensen dat een situatie anders was (vaak uitgedrukt met 'wou').",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "wish_subjunctive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>wou</w> dat ik die taal vloeiend kon spreken.",
                            "target_lang_translations": {
                                "en": "I <w>wish</w> I could speak that language fluently.",
                                "ru": "Я <w>хотел бы</w>, чтобы я мог свободно говорить на этом языке."
                            }
                        },
                        {
                            "text": "Hij <w>wou</w> dat hij gisteren eerder was vertrokken.",
                            "target_lang_translations": {
                                "en": "He <w>wished</w> he had left earlier yesterday.",
                                "ru": "Он <w>хотел бы</w>, чтобы он вчера уехал раньше."
                            }
                        }
                    ],
                    "synonyms": [
                        "wensen"
                    ],
                    "common_phrases": [
                        "ik wou dat"
                    ],
                    "target_lang_definitions": {
                        "en": "To wish that a situation were different (often expressed with 'wou').",
                        "ru": "Желать, чтобы ситуация была иной (часто выражается формой прошедшего времени)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wish",
                                "target_lang_sense_clarification": "desire for a different reality"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "в сослагательном наклонении (хотел бы)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50c7fba9-c326-351e-aae8-ef715b24a63b",
                    "sense_definition": "De wens hebben om ergens naartoe te gaan (zonder expliciet bewegingswerkwoord).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen <w>wilden</w> het huis in toen het begon te regenen.",
                            "target_lang_translations": {
                                "en": "The children <w>wanted to go</w> into the house when it started raining.",
                                "ru": "Дети <w>захотели</w> домой, когда начался дождь."
                            }
                        },
                        {
                            "text": "De kat staat bij de deur en <w>wil</w> naar buiten.",
                            "target_lang_translations": {
                                "en": "The cat is standing by the door and <w>wants to go</w> outside.",
                                "ru": "Кот стоит у двери и <w>хочет</w> на улицу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naar huis willen",
                        "naar binnen willen"
                    ],
                    "target_lang_definitions": {
                        "en": "To have the wish to go somewhere (without an explicit verb of motion).",
                        "ru": "Желать пойти куда-либо (без явного глагола движения)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "want to go",
                                "target_lang_sense_clarification": "implied motion"
                            },
                            {
                                "target_lang_word": "want to get",
                                "target_lang_sense_clarification": "desire to enter/exit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "хотеть направиться куда-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d69f8b8d-75e4-32cd-a0f6-61367ab1fa8f",
                    "sense_definition": "Een voorstel doen om samen iets te ondernemen.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "suggestion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Willen</w> we gaan wandelen in het bos?",
                            "target_lang_translations": {
                                "en": "<w>Shall</w> we go for a walk in the forest?",
                                "ru": "<w>Хотим</w> ли мы пойти гулять в лес?"
                            }
                        },
                        {
                            "text": "<w>Willen</w> we vanavond samen eten?",
                            "target_lang_translations": {
                                "en": "<w>Shall</w> we have dinner together tonight?",
                                "ru": "<w>Хотим</w> ли мы сегодня поужинать вместе?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "willen we"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a proposal to undertake something together.",
                        "ru": "Делать предложение заняться чем-то вместе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shall",
                                "target_lang_sense_clarification": "suggestion for joint action"
                            },
                            {
                                "target_lang_word": "want",
                                "target_lang_sense_clarification": "asking about shared desire"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "в вопросе о совместном желании (хотим ли мы...)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9998265f-0094-3f0b-8a0c-ce5bd52e460e",
                    "sense_definition": "Vlotten of op de gewenste manier verlopen (vaak in ontkennende of vragende zin).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "succeed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>wil</w> vandaag nog niet zo lukken met mijn huiswerk.",
                            "target_lang_translations": {
                                "en": "My homework isn't really <w>working out</w> today.",
                                "ru": "Сегодня с домашним заданием как-то не <w>ладится</w>."
                            }
                        },
                        {
                            "text": "<w>Wil</w> het een beetje met de voorbereidingen voor het feest?",
                            "target_lang_translations": {
                                "en": "Is it <w>going</w> okay with the preparations for the party?",
                                "ru": "Ну как, <w>продвигается</w> подготовка к вечеринке?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "het wil niet",
                        "wil het"
                    ],
                    "target_lang_definitions": {
                        "en": "To progress or proceed in the desired way (often in negative or interrogative sentences).",
                        "ru": "Ладиться или протекать желаемым образом (часто в отрицании)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "work out",
                                "target_lang_sense_clarification": "succeed or happen as planned"
                            },
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "progress smoothly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ладиться",
                                "target_lang_sense_clarification": "успешно продвигаться (о деле)"
                            },
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "протекать (о процессе)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b88996b1-f134-3992-97c7-a2e5b8fbe117",
                    "sense_definition": "De neiging hebben om te gebeuren of mogelijk zijn.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "tendency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat <w>wil</w> in de winter nog weleens gebeuren als het vriest.",
                            "target_lang_translations": {
                                "en": "That <w>tends to</w> happen in the winter when it freezes.",
                                "ru": "Такое зимой иногда <w>случается</w>, когда подмораживает."
                            }
                        },
                        {
                            "text": "Het <w>wil</w> nog wel eens voorkomen dat de trein vertraging heeft.",
                            "target_lang_translations": {
                                "en": "It <w>tends to</w> happen that the train is delayed.",
                                "ru": "Иногда <w>бывает</w>, что поезд опаздывает."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wil nog weleens"
                    ],
                    "target_lang_definitions": {
                        "en": "To have the tendency to happen or be possible.",
                        "ru": "Иметь склонность происходить или быть возможным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tend to",
                                "target_lang_sense_clarification": "be prone to happening"
                            },
                            {
                                "target_lang_word": "will",
                                "target_lang_sense_clarification": "habitual occurrence"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "случаться",
                                "target_lang_sense_clarification": "происходить время от времени"
                            },
                            {
                                "target_lang_word": "бывать",
                                "target_lang_sense_clarification": "иметь обыкновение происходить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2060d0b5-a731-3acc-895a-fde18304e809",
                    "sense_definition": "Volgens een verhaal of gerucht luiden.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rumor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het gerucht <w>wil</w> dat de directeur binnenkort aftreedt.",
                            "target_lang_translations": {
                                "en": "Rumor <w>has it</w> that the director will resign soon.",
                                "ru": "Слух <w>гласит</w>, что директор скоро уйдет в отставку."
                            }
                        },
                        {
                            "text": "De legende <w>wil</w> dat hier een schat begraven ligt.",
                            "target_lang_translations": {
                                "en": "Legend <w>has it</w> that a treasure is buried here.",
                                "ru": "Легенда <w>гласит</w>, что здесь зарыт клад."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het gerucht wil",
                        "het verhaal wil"
                    ],
                    "target_lang_definitions": {
                        "en": "To state according to a story or rumor.",
                        "ru": "Гласить (согласно истории или слухам)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have it",
                                "target_lang_sense_clarification": "as in 'rumor has it'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гласить",
                                "target_lang_sense_clarification": "сообщать (о легенде, слухе)"
                            },
                            {
                                "target_lang_word": "утверждать",
                                "target_lang_sense_clarification": "говорить с уверенностью"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b8a91a15-ea65-3bda-89ce-dabad7c0618d",
                    "sense_definition": "Meervoud van de <w>wil</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verschillende <w>willen</w> van de commissieleden botsten met elkaar.",
                            "target_lang_translations": {
                                "en": "The different <w>wills</w> of the committee members clashed.",
                                "ru": "Различные <w>воли</w> членов комиссии столкнулись друг с другом."
                            }
                        },
                        {
                            "text": "Het is moeilijk om twee sterke <w>willen</w> te verenigen.",
                            "target_lang_translations": {
                                "en": "It is difficult to unite two strong <w>wills</w>.",
                                "ru": "Трудно объединить две сильные <w>воли</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'the will'.",
                        "ru": "Множественное число слова 'воля'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wills",
                                "target_lang_sense_clarification": "plural noun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "воли",
                                "target_lang_sense_clarification": "множественное число от воля"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
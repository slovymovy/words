    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f779bfc-43ac-3faf-beed-71910c155b73",
                    "sense_definition": "Форма прошедшего времени мужского рода единственного числа глагола <w>надеть</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он быстро <w>надел</w> тёплое пальто и вышел на улицу.",
                            "target_lang_translations": {
                                "en": "He quickly <w>put on</w> a warm coat and went outside."
                            }
                        },
                        {
                            "text": "Мальчик <w>надел</w> шапку, потому что на улице было холодно.",
                            "target_lang_translations": {
                                "en": "The boy <w>put on</w> a hat because it was cold outside."
                            }
                        }
                    ],
                    "common_phrases": [
                        "надел пальто",
                        "надел очки",
                        "надел маску",
                        "надел шапку"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense of надеть"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Masculine singular past tense form of the verb \"надеть\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put on",
                                "target_lang_sense_clarification": "clothing or accessories"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4d5fad18-149f-39d9-a719-3a80fc72f1c3",
                    "sense_definition": "Земельный участок, выделявшийся в пользование крестьянской семье (историческое).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждая семья в деревне получила свой земельный <w>надел</w>.",
                            "target_lang_translations": {
                                "en": "Each family in the village received their land <w>allotment</w>."
                            }
                        },
                        {
                            "text": "Размер крестьянского <w>надела</w> часто зависел от качества почвы.",
                            "target_lang_translations": {
                                "en": "The size of the peasant <w>allotment</w> often depended on the quality of the soil."
                            }
                        }
                    ],
                    "synonyms": [
                        "участок"
                    ],
                    "common_phrases": [
                        "земельный надел",
                        "крестьянский надел"
                    ],
                    "target_lang_definitions": {
                        "en": "A plot of land allocated for use by a peasant family (historical)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "allotment",
                                "target_lang_sense_clarification": "share of land"
                            },
                            {
                                "target_lang_word": "plot",
                                "target_lang_sense_clarification": "piece of land"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "523dfa48-5506-3a21-9b54-62dd4c602f05",
                    "sense_definition": "Действие по значению глагола <w>наделить</w>; предоставление чего-либо.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Щедрый <w>надел</w> землёй позволил фермерам расширить хозяйство.",
                            "target_lang_translations": {
                                "en": "The generous <w>endowment</w> of land allowed the farmers to expand their business."
                            }
                        },
                        {
                            "text": "Этот <w>надел</w> полномочиями оказался преждевременным решением.",
                            "target_lang_translations": {
                                "en": "This <w>conferment</w> of powers turned out to be a premature decision."
                            }
                        }
                    ],
                    "synonyms": [
                        "наделение"
                    ],
                    "target_lang_definitions": {
                        "en": "The action corresponding to the verb \"наделить\"; the provision of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "endowment",
                                "target_lang_sense_clarification": "act of providing/giving"
                            },
                            {
                                "target_lang_word": "conferment",
                                "target_lang_sense_clarification": "granting of powers or rights"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "94a94dcd-eedd-364a-8cba-44756fbc6209",
                    "sense_definition": "Niet langer; duidt aan dat een handeling of toestand is gestopt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adverb_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik woon hier niet <w>meer</w> sinds vorig jaar.",
                            "target_lang_translations": {
                                "en": "I don't live here <w>anymore</w> since last year.",
                                "ru": "Я <w>больше</w> не живу здесь с прошлого года."
                            }
                        },
                        {
                            "text": "Hij wil niet <w>meer</w> met de auto gaan.",
                            "target_lang_translations": {
                                "en": "He doesn't want to go by car <w>anymore</w>.",
                                "ru": "Он <w>больше</w> не хочет ехать на машине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niet meer",
                        "nooit meer"
                    ],
                    "target_lang_definitions": {
                        "en": "No longer; indicates that an action or state has stopped.",
                        "ru": "Больше не; указывает на то, что действие или состояние прекратилось."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "anymore",
                                "target_lang_sense_clarification": "used in negative constructions"
                            },
                            {
                                "target_lang_word": "no longer",
                                "target_lang_sense_clarification": "formal or literary"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "больше",
                                "target_lang_sense_clarification": "в значении 'больше не'"
                            },
                            {
                                "target_lang_word": "уже не",
                                "target_lang_sense_clarification": "прекращение действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3f6ef4c-55e7-3228-801d-ec2e157a8828",
                    "sense_definition": "In hogere mate of intensiteit.",

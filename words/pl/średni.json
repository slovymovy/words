    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "68dc9a35-4f88-39e2-9698-95d80138f1ae",
                    "sense_definition": "Będący pośrodku między czymś małym a dużym lub o umiarkowanym nasileniu; nie najmniejszy i nie największy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "size_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest <w>średniego</w> wzrostu, tak jak nasz tata.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>среднего</w> роста, как и наш папа."
                            }
                        },
                        {
                            "text": "Ten płaszcz jest dostępny tylko w <w>średnim</w> rozmiarze.",
                            "target_lang_translations": {
                                "ru": "Это пальто доступно только в <w>среднем</w> размере."
                            }
                        }
                    ],
                    "synonyms": [
                        "umiarkowany"
                    ],
                    "common_phrases": [
                        "średni wzrost",
                        "średnia wielkość",
                        "w średnim wieku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся посередине между чем-то маленьким и большим или умеренной интенсивности; не самый маленький и не самый большой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средний",
                                "target_lang_sense_clarification": "промежуточный по размеру или силе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f369b54-a87b-3370-a76f-d3761d8b9af8",
                    "sense_definition": "Dotyczący etapu edukacji następującego po szkole podstawowej, a przed studiami wyższymi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ukończeniu szkoły <w>średniej</w> planuję iść na studia medyczne.",
                            "target_lang_translations": {
                                "ru": "После окончания <w>средней</w> школы я планирую поступать в медицинский институт."
                            }
                        },
                        {
                            "text": "Do tej pracy wymagane jest wykształcenie <w>średnie</w> techniczne.",
                            "target_lang_translations": {
                                "ru": "Для этой работы требуется <w>среднее</w> техническое образование."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szkoła średnia",
                        "wykształcenie średnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к этапу образования, следующему за начальной школой и предшествующему высшему образованию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средний",
                                "target_lang_sense_clarification": "об уровне образования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "462eb63a-8332-390c-90fd-eb566fcbc5de",
                    "sense_definition": "Niewyróżniający się niczym szczególnym, ani bardzo dobry, ani bardzo zły; taki sobie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obiad w tej restauracji był <w>średni</w>, jadłem tam lepsze rzeczy.",
                            "target_lang_translations": {
                                "ru": "Обед в этом ресторане был <w>средним</w>, я ел там вещи и получше."
                            }
                        },
                        {
                            "text": "Film okazał się dość <w>średni</w>, mimo udziału znanych aktorów.",
                            "target_lang_translations": {
                                "ru": "Фильм оказался довольно <w>средним</w>, несмотря на участие известных актёров."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeciętny",
                        "zwykły"
                    ],
                    "common_phrases": [
                        "średni film",
                        "średnia jakość",
                        "średni pomysł"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to express disappointment or lack of enthusiasm"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ничем не выделяющийся, ни очень хороший, ни очень плохой; посредственный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средний",
                                "target_lang_sense_clarification": "заурядный, так себе"
                            },
                            {
                                "target_lang_word": "посредственный",
                                "target_lang_sense_clarification": "невысокого качества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4126a729-d8b4-3ab2-b8b1-aeed1a6c91fa",
                    "sense_definition": "Stanowiący wynik średniej arytmetycznej lub statystycznej; dotyczący uśrednionej wartości.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "statistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Średni</w> wiek pracowników w naszej firmie to trzydzieści lat.",
                            "target_lang_translations": {
                                "ru": "<w>Средний</w> возраст сотрудников в нашей компании — тридцать лет."
                            }
                        },
                        {
                            "text": "Samochód rozwijał <w>średnią</w> prędkość stu kilometrów na godzinę.",
                            "target_lang_translations": {
                                "ru": "Автомобиль развивал <w>среднюю</w> скорость сто километров в час."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeciętny"
                    ],
                    "common_phrases": [
                        "średnia krajowa",
                        "średnia prędkość",
                        "średni zarobek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Являющийся результатом среднего арифметического или статистического; относящийся к усреднённому значению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средний",
                                "target_lang_sense_clarification": "статистический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ebee4e9-b1c0-35ed-9aa6-8730f3a7d173",
                    "sense_definition": "Znajdujący się wewnątrz lub pomiędzy dwoma okresami albo punktami (używane głównie w stałych wyrażeniach z zakresu anatomii lub historii).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "location_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zapalenie ucha <w>średniego</w> jest bardzo bolesną dolegliwością u dzieci.",
                            "target_lang_translations": {
                                "ru": "Воспаление <w>среднего</w> уха — очень болезненный недуг у детей."
                            }
                        },
                        {
                            "text": "Historia Europy dzieli się na starożytność, wieki <w>średnie</w> i nowożytność.",
                            "target_lang_translations": {
                                "ru": "История Европы делится на древность, <w>средние</w> века и новое время."
                            }
                        }
                    ],
                    "synonyms": [
                        "środkowy"
                    ],
                    "common_phrases": [
                        "ucho średnie",
                        "wieki średnie",
                        "palec średni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся внутри или между двумя периодами или точками (используется в основном в устойчивых выражениях в анатомии или истории)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средний",
                                "target_lang_sense_clarification": "серединный (анатомия, история)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

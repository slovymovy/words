    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d2485742-46b0-32ad-87fc-130dc5c4057b",
                    "sense_definition": "Niet gemakkelijk te vervormen of te buigen; ook gebruikt voor spieren die pijnlijk en moeilijk te bewegen zijn.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een lange wandeling voelen mijn benen <w>stijf</w> aan.",
                            "target_lang_translations": {
                                "en": "After a long walk, my legs feel <w>stiff</w>.",
                                "ru": "После долгой прогулки мои ноги кажутся <w>деревянными</w>."
                            }
                        },
                        {
                            "text": "Dit materiaal is veel te <w>stijf</w> om netjes te kunnen vouwen.",
                            "target_lang_translations": {
                                "en": "This material is much too <w>stiff</w> to fold neatly.",
                                "ru": "Этот материал слишком <w>жёсткий</w>, чтобы его можно было аккуратно сложить."
                            }
                        }
                    ],
                    "synonyms": [
                        "onbuigzaam",
                        "rigide",
                        "star"
                    ],
                    "antonyms": [
                        "buigzaam",
                        "plooibaar"
                    ],
                    "common_phrases": [
                        "stijve spieren",
                        "een stijve nek",
                        "stijf van de kou"
                    ],
                    "target_lang_definitions": {
                        "en": "Not easily bent or changed in shape; also used for muscles that feel tight or painful.",
                        "ru": "Твердый, не поддающийся деформации; также используется для описания болезненных или малоподвижных мышц."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stiff",
                                "target_lang_sense_clarification": "difficult to bend or move; painful (muscles)"
                            },
                            {
                                "target_lang_word": "rigid",
                                "target_lang_sense_clarification": "unable to bend or be forced out of shape"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "жёсткий",
                                "target_lang_sense_clarification": "о материале или поверхности"
                            },
                            {
                                "target_lang_word": "скованный",
                                "target_lang_sense_clarification": "о движениях или мышцах"
                            },
                            {
                                "target_lang_word": "негнущийся",
                                "target_lang_sense_clarification": "о предметах, потерявших гибкость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f032625c-eb84-3245-bb5d-2a6fe47092b3",
                    "sense_definition": "Ongemakkelijk, formeel of niet spontaan in de omgang.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gedroeg zich erg <w>stijf</w> tijdens het officiële diner.",
                            "target_lang_translations": {
                                "en": "He behaved very <w>stiffly</w> during the official dinner.",
                                "ru": "Он вёл себя очень <w>скованно</w> во время официального ужина."
                            }
                        },
                        {
                            "text": "Mijn nieuwe collega is zo <w>stijf</w> als een hark en maakt nooit grapjes.",
                            "target_lang_translations": {
                                "en": "My new colleague is as <w>stiff</w> as a poker and never makes jokes.",
                                "ru": "Мой новый коллега очень <w>чопорный</w> и никогда не шутит."
                            }
                        }
                    ],
                    "synonyms": [
                        "star"
                    ],
                    "common_phrases": [
                        "stijf als een hark",
                        "stijf doen"
                    ],
                    "target_lang_definitions": {
                        "en": "Lacking ease or grace; formal and not spontaneous in behavior.",
                        "ru": "Неловкий, формальный или лишенный спонтанности в общении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stiff",
                                "target_lang_sense_clarification": "formal, lacking ease or spontaneity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чопорный",
                                "target_lang_sense_clarification": "чрезмерно строгий и формальный в поведении"
                            },
                            {
                                "target_lang_word": "скованный",
                                "target_lang_sense_clarification": "зажатый, чувствующий себя неловко"
                            },
                            {
                                "target_lang_word": "натянутый",
                                "target_lang_sense_clarification": "неестественный (об отношениях или улыбке)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2549af7-7224-3f63-a5cd-1d1370c18b5e",
                    "sense_definition": "Helemaal of volkomen; vaak gebruikt om een toestand te versterken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het concert was binnen een uur <w>stijf</w> uitverkocht.",
                            "target_lang_translations": {
                                "en": "The concert was <w>completely</w> sold out within an hour.",
                                "ru": "Билеты на концерт были <w>полностью</w> распроданы за час."
                            }
                        },
                        {
                            "text": "Hij stond <w>stijf</w> van de zenuwen voor zijn presentatie.",
                            "target_lang_translations": {
                                "en": "He was <w>stiff</w> with nerves before his presentation.",
                                "ru": "Он <w>оцепенел</w> от нервов перед своей презентацией."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stijf uitverkocht",
                        "stijf staan van"
                    ],
                    "target_lang_definitions": {
                        "en": "Completely or thoroughly; often used to emphasize a state or condition.",
                        "ru": "Полностью или совершенно; часто используется для усиления описания состояния."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "completely",
                                "target_lang_sense_clarification": "used as an intensifier (e.g., completely sold out)"
                            },
                            {
                                "target_lang_word": "stiff",
                                "target_lang_sense_clarification": "rigid with emotion or tension"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "совершенно",
                                "target_lang_sense_clarification": "полностью (усилительная частица)"
                            },
                            {
                                "target_lang_word": "насквозь",
                                "target_lang_sense_clarification": "полностью пронизанный чем-либо"
                            },
                            {
                                "target_lang_word": "битком",
                                "target_lang_sense_clarification": "о переполненности (разг.)"
                            }
                        ]
                    }
                }
            ]
        },
        {

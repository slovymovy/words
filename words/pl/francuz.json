        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c142a426-8d35-35dc-8613-9e56355f2985",
                    "sense_definition": "Mężczyzna narodowości francuskiej lub obywatel Francji.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "person_nationality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy sąsiad to <w>Francuz</w>, który przeprowadził się do Polski w zeszłym roku.",
                            "target_lang_translations": {
                                "ru": "Мой новый сосед — <w>француз</w>, который переехал в Польшу в прошлом году."
                            }
                        },
                        {
                            "text": "Wielu <w>Francuzów</w> jest bardzo dumnych ze swojej kuchni i kultury.",
                            "target_lang_translations": {
                                "ru": "Многие <w>французы</w> очень гордятся своей кухней и культурой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rodowity Francuz",
                        "przeciętny Francuz",
                        "typowy Francuz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина французской национальности или гражданин Франции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "француз",
                                "target_lang_sense_clarification": "национальность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6df96845-a2f4-3b73-83c4-96154047fe91",
                    "sense_definition": "Potoczne określenie języka francuskiego, najczęściej jako przedmiotu szkolnego lub lekcji.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "school_subject",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro mam trudny sprawdzian z <w>francuza</w>, więc muszę się dzisiaj uczyć.",
                            "target_lang_translations": {
                                "ru": "Завтра у меня сложная контрольная по <w>французскому</w>, поэтому я должен сегодня учиться."
                            }
                        },
                        {
                            "text": "W naszej szkole można wybrać między niemieckim a <w>francuzem</w>.",
                            "target_lang_translations": {
                                "ru": "В нашей школе можно выбрать между немецким и <w>французским</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "język francuski"
                    ],
                    "common_phrases": [
                        "uczyć się francuza",
                        "zdać francuza",
                        "lekcja francuza"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "school slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название французского языка, чаще всего как школьного предмета или урока."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "09762e29-a8af-3f13-afdd-2836f4961abc",
                    "sense_definition": "The position, rank, or authority of a chieftain.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "rank_authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He inherited the <w>chieftainship</w> immediately after his father passed away.",
                            "target_lang_translations": {
                                "ru": "Он унаследовал <w>титул вождя</w> сразу после смерти отца."
                            }
                        },
                        {
                            "text": "The council met to decide who would assume the <w>chieftainship</w> of the tribe.",
                            "target_lang_translations": {
                                "ru": "Совет собрался, чтобы решить, кто примет <w>звание вождя</w> племени."
                            }
                        }
                    ],
                    "synonyms": [
                        "headship",
                        "leadership",
                        "command",
                        "chiefdom"
                    ],
                    "common_phrases": [
                        "hereditary chieftainship",
                        "assume chieftainship",
                        "claim chieftainship"
                    ],
                    "target_lang_definitions": {
                        "ru": "Должность, звание или полномочия вождя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звание вождя",
                                "target_lang_sense_clarification": "ранг или статус"
                            },
                            {
                                "target_lang_word": "титул вождя",
                                "target_lang_sense_clarification": "формальное наименование"
                            },
                            {
                                "target_lang_word": "предводительство",
                                "target_lang_sense_clarification": "руководство группой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca7ad865-248e-3d0b-8926-db130d28ae82",
                    "sense_definition": "The region or territory governed by a chieftain.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "territory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The river marks the northern boundary of his <w>chieftainship</w>.",
                            "target_lang_translations": {
                                "ru": "Река обозначает северную границу его <w>владений</w>."
                            }
                        },
                        {
                            "text": "Several small villages were located within the <w>chieftainship</w>.",
                            "target_lang_translations": {
                                "ru": "Несколько небольших деревень располагались на территории <w>вождества</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "chiefdom",
                        "domain",
                        "territory"
                    ],
                    "common_phrases": [
                        "local chieftainship",
                        "within the chieftainship"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регион или территория, управляемая вождем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вождество",
                                "target_lang_sense_clarification": "территориально-политическая единица"
                            },
                            {
                                "target_lang_word": "владения вождя",
                                "target_lang_sense_clarification": "подконтрольная территория"
                            }
                        ]
                    }
                }
            ]
        }
    ]

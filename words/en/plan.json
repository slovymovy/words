                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "планировать",
                                "target_lang_sense_clarification": "намереваться"
                            },
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "иметь намерение сделать"
                            },
                            {
                                "target_lang_word": "намереваться",
                                "target_lang_sense_clarification": "желать совершить"
                            }

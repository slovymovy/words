{
    "word_family": [
        "match",
        "unmatched",
        "mismatch",
        "matching"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "12e57e45-1bc3-3fe8-b10d-71dace8c1788",
                    "sense_definition": "Having no equal; superior to all others.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist captured the landscape with <w>matchless</w> skill.",
                            "target_lang_translations": {
                                "ru": "Художник запечатлел пейзаж с <w>несравненным</w> мастерством."
                            }
                        },
                        {
                            "text": "Her <w>matchless</w> beauty was admired by everyone in the kingdom.",
                            "target_lang_translations": {
                                "ru": "Её <w>бесподобной</w> красотой восхищались все в королевстве."
                            }
                        }
                    ],
                    "synonyms": [
                        "incomparable",
                        "peerless",
                        "unequaled",
                        "unparalleled",
                        "unsurpassed"
                    ],
                    "antonyms": [
                        "ordinary",
                        "mediocre",
                        "common"
                    ],
                    "common_phrases": [
                        "matchless beauty",
                        "matchless skill",
                        "matchless elegance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий себе равных; превосходящий всех остальных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несравненный",
                                "target_lang_sense_clarification": "не имеющий равных"
                            },
                            {
                                "target_lang_word": "бесподобный",
                                "target_lang_sense_clarification": "исключительный, восхитительный"
                            },
                            {
                                "target_lang_word": "непревзойдённый",
                                "target_lang_sense_clarification": "лучший из всех, кого невозможно превзойти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "159011ce-8252-33c8-b0f6-95b78e3b17f4",
                    "sense_definition": "Having no mate or partner.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The matchmaker tried to find a husband for her <w>matchless</w> daughter.",
                            "target_lang_translations": {
                                "ru": "Сваха пыталась найти мужа для своей <w>не имеющей пары</w> дочери."
                            }
                        },
                        {
                            "text": "I found a <w>matchless</w> sock in the laundry basket.",
                            "target_lang_translations": {
                                "ru": "Я нашел <w>непарный</w> носок в корзине для белья."
                            }
                        }
                    ],
                    "synonyms": [
                        "single",
                        "unattached"
                    ],
                    "antonyms": [
                        "paired",
                        "matched"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий пары или партнёра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непарный",
                                "target_lang_sense_clarification": "о предметах, обычно используемых в паре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11bb2f77-7c69-39ec-a629-ea9b4bf3f244",
                    "sense_definition": "Designed to ignite or operate without using a match.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This <w>matchless</w> stove is perfect for camping in windy conditions.",
                            "target_lang_translations": {
                                "ru": "Эта <w>бесспичечная</w> горелка идеально подходит для кемпинга в ветреную погоду."
                            }
                        },
                        {
                            "text": "The heater features a safe <w>matchless</w> ignition system.",
                            "target_lang_translations": {
                                "ru": "Обогреватель оснащен безопасной системой <w>бесспичечного</w> розжига."
                            }
                        }
                    ],
                    "common_phrases": [
                        "matchless stove",
                        "matchless ignition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для зажигания или работы без использования спичек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесспичечный",
                                "target_lang_sense_clarification": "о механизме зажигания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        "ru": [
                            {
                                "target_lang_word": "стойкость",
                                "target_lang_sense_clarification": "способность противостоять трудностям"
                            },
                            {
                                "target_lang_word": "жизнестойкость",
                                "target_lang_sense_clarification": "жизненная сила, способность к восстановлению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa7bbdb3-dd55-37ee-996a-4342e57d1e2a",
                    "sense_definition": "Het vermogen van een materiaal om na vervorming weer de oorspronkelijke vorm aan te nemen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>veerkracht</w> van het oude elastiek is volledig verdwenen.",
                            "target_lang_translations": {
                                "en": "The <w>elasticity</w> of the old rubber band is completely gone.",
                                "ru": "<w>Упругость</w> старой резинки полностью исчезла."
                            }
                        },
                        {
                            "text": "Dankzij de <w>veerkracht</w> van de matras slaap je erg comfortabel.",
                            "target_lang_translations": {
                                "en": "Thanks to the <w>springiness</w> of the mattress, you sleep very comfortably.",
                                "ru": "Благодаря <w>упругости</w> матраса спать очень комфортно."
                            }

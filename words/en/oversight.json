{
    "word_family": [
        "oversee",
        "overseer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e31c699-4aaa-35dc-8b8a-a4681bf4bd4a",
                    "sense_definition": "An omission; something that is left out, missed, or forgotten.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A small <w>oversight</w> at this stage can lead to big problems later.",
                            "target_lang_translations": {
                                "ru": "Небольшая <w>оплошность</w> на этом этапе может привести к большим проблемам в дальнейшем."
                            }
                        },
                        {
                            "text": "The error was due to an unfortunate <w>oversight</w> by the accounting department.",
                            "target_lang_translations": {
                                "ru": "Ошибка произошла из-за досадного <w>недосмотра</w> бухгалтерии."
                            }
                        }
                    ],
                    "synonyms": [
                        "omission",
                        "mistake",
                        "error",
                        "lapse",
                        "slip"
                    ],
                    "common_phrases": [
                        "by an oversight",
                        "due to an oversight",
                        "unfortunate oversight",
                        "administrative oversight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упущение; что-то, что было пропущено, не замечено или забыто."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недосмотр",
                                "target_lang_sense_clarification": "ошибка по невнимательности"
                            },
                            {
                                "target_lang_word": "оплошность",
                                "target_lang_sense_clarification": "небольшая ошибка"
                            },
                            {
                                "target_lang_word": "упущение",
                                "target_lang_sense_clarification": "то, что не было сделано или учтено"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd30bdb2-a8c2-345d-8fd9-d75134a56a85",
                    "sense_definition": "Supervision or management.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bureaucracy was subject to government <w>oversight</w>.",
                            "target_lang_translations": {
                                "ru": "Бюрократический аппарат находился под правительственным <w>надзором</w>."
                            }
                        },
                        {
                            "text": "The committee provides strict regulatory <w>oversight</w> of the banking industry.",
                            "target_lang_translations": {
                                "ru": "Комитет осуществляет строгий нормативный <w>надзор</w> за банковской сферой."
                            }
                        }
                    ],
                    "synonyms": [
                        "supervision",
                        "management",
                        "surveillance",
                        "control"
                    ],
                    "antonyms": [
                        "neglect"
                    ],
                    "common_phrases": [
                        "regulatory oversight",
                        "government oversight",
                        "judicial oversight",
                        "provide oversight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надзор или управление; наблюдение за деятельностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надзор",
                                "target_lang_sense_clarification": "наблюдение с целью проверки"
                            },
                            {
                                "target_lang_word": "контроль",
                                "target_lang_sense_clarification": "проверка деятельности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f039097-6fb0-3896-9727-e2645fa2da31",
                    "sense_definition": "To oversee; to supervise.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new manager <w>oversights</w> the entire production process.",
                            "target_lang_translations": {
                                "ru": "Новый менеджер <w>контролирует</w> весь производственный процесс."
                            }
                        },
                        {
                            "text": "They were appointed to <w>oversight</w> the implementation of the safety protocols.",
                            "target_lang_translations": {
                                "ru": "Их назначили <w>надзирать</w> за внедрением протоколов безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "oversee",
                        "supervise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осуществлять надзор; контролировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контролировать",
                                "target_lang_sense_clarification": "управлять процессом"
                            },
                            {
                                "target_lang_word": "надзирать",
                                "target_lang_sense_clarification": "следить за порядком"
                            },
                            {
                                "target_lang_word": "курировать",
                                "target_lang_sense_clarification": "руководить направлением"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "a9692e52-825f-3c9b-9b79-c73343ea1452",
                    "sense_definition": "Знак, надпись, щит или стрелка, которые указывают направление движения или местонахождение чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "signs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На перекрёстке мы увидели дорожный <указатель> и повернули направо.",
                            "target_lang_translations": {
                                "en": "At the intersection, we saw a road <sign> and turned right."
                            }
                        },
                        {
                            "text": "Туристы долго искали <указатель> метро, чтобы добраться до гостиницы.",
                            "target_lang_translations": {
                                "en": "The tourists spent a long time looking for the metro <sign> to get to the hotel."
                            }
                        }
                    ],
                    "synonyms": [
                        "знак",
                        "табличка"
                    ],
                    "common_phrases": [
                        "дорожный указатель",
                        "указатель поворота",
                        "указатель направления"
                    ],
                    "target_lang_definitions": {
                        "en": "A sign, inscription, board, or arrow that indicates a direction of movement or the location of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sign",
                                "target_lang_sense_clarification": "board or notice giving information"
                            },
                            {
                                "target_lang_word": "signpost",
                                "target_lang_sense_clarification": "post giving guidance"
                            },
                            {
                                "target_lang_word": "indicator",
                                "target_lang_sense_clarification": "device showing direction (e.g., on a vehicle)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be01a27f-6692-38e9-87d7-97be4a651bcf",
                    "sense_definition": "Справочный список имён, терминов или названий, обычно расположенный в конце книги для облегчения поиска.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент открыл алфавитный <указатель>, чтобы быстро найти определение сложного термина.",
                            "target_lang_translations": {
                                "en": "The student opened the alphabetical <index> to quickly find the definition of a complex term."
                            }
                        },
                        {
                            "text": "В этой энциклопедии есть подробный предметный <указатель> с номерами страниц.",
                            "target_lang_translations": {
                                "en": "This encyclopedia has a detailed subject <index> with page numbers."
                            }
                        }
                    ],
                    "synonyms": [
                        "список",
                        "перечень",
                        "индекс"
                    ],
                    "common_phrases": [
                        "алфавитный указатель",
                        "предметный указатель",
                        "именной указатель"
                    ],
                    "target_lang_definitions": {
                        "en": "A reference list of names, terms, or titles, usually located at the end of a book to facilitate searching."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "index",
                                "target_lang_sense_clarification": "alphabetical list in a book"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ccc236a-0953-3d91-a39b-11e29880f0a6",
                    "sense_definition": "Прибор или часть прибора (обычно стрелка), показывающая уровень, величину или состояние чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "instruments",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель заметил, что <указатель> уровня топлива приближается к нулю.",
                            "target_lang_translations": {
                                "en": "The driver noticed that the fuel level <indicator> was approaching zero."
                            }
                        },
                        {
                            "text": "На приборной панели мигал <указатель> температуры двигателя.",
                            "target_lang_translations": {
                                "en": "The engine temperature <indicator> was blinking on the dashboard."
                            }

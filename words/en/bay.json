    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cadfdfa9-ee92-3a5a-ae9a-7b096252aaf1",
                    "sense_definition": "A body of water partially enclosed by land but with a wide mouth, affording access to the sea.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ship anchored in the quiet <w>bay</w> to wait out the storm.",
                            "target_lang_translations": {
                                "ru": "Корабль бросил якорь в тихой <w>бухте</w>, чтобы переждать шторм."
                            }
                        },
                        {
                            "text": "Our hotel room had a wonderful view across the <w>bay</w>.",
                            "target_lang_translations": {
                                "ru": "Из нашего номера открывался чудесный вид на <w>залив</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gulf"
                    ],
                    "common_phrases": [
                        "San Francisco Bay",
                        "Hudson Bay",
                        "across the bay",
                        "bay area"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть водоема, частично окруженная сушей, но имеющая широкий выход в море."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "залив",
                                "target_lang_sense_clarification": "обширный участок водного пространства"
                            },
                            {
                                "target_lang_word": "бухта",
                                "target_lang_sense_clarification": "небольшой, защищенный залив"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1adcf59-bcd7-351b-aa2e-008e2b551a7f",
                    "sense_definition": "The aromatic dried leaf of the laurel tree, used as a herb in cooking.",

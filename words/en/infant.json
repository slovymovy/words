    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2c92c7e3-6975-36d2-a828-6e74b2da7f2a",
                    "sense_definition": "A very young human being, from birth to somewhere between six months and two years of age.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "young_human",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nurse gently handed the newborn <w>infant</w> to its mother.",
                            "target_lang_translations": {
                                "ru": "Медсестра осторожно передала новорожденного <w>младенца</w> матери."
                            }
                        },
                        {
                            "text": "Proper nutrition is essential for healthy <w>infant</w> development.",
                            "target_lang_translations": {
                                "ru": "Правильное питание необходимо для здорового развития <w>младенца</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "baby"
                    ],
                    "common_phrases": [
                        "infant mortality",
                        "newborn infant",
                        "infant formula"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень маленький ребенок, от рождения до возраста примерно от шести месяцев до двух лет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "младенец",
                                "target_lang_sense_clarification": "ребенок первого года жизни"
                            },
                            {
                                "target_lang_word": "грудной ребёнок",
                                "target_lang_sense_clarification": "ребенок, питающийся материнским молоком"
                            },
                            {
                                "target_lang_word": "малыш",
                                "target_lang_sense_clarification": "разговорное; маленький ребенок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac453447-7c3d-315a-9007-7c7459049289",
                    "sense_definition": "A student in the early years of primary education, typically aged between four and seven.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "school_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My youngest daughter is in the <w>infants</w> this year.",
                            "target_lang_translations": {
                                "ru": "Моя младшая дочь в этом году ходит в <w>младшие классы</w>."
                            }
                        },
                        {
                            "text": "The <w>infant</w> school is located in a separate building from the juniors.",
                            "target_lang_translations": {
                                "ru": "Школа для <w>малышей</w> находится в отдельном здании от школы для детей постарше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "infant school",
                        "infant class"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British and Irish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ученик начальных классов (в системе образования Великобритании), обычно в возрасте от четырех до семи лет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ученик младших классов",
                                "target_lang_sense_clarification": "ребенок 4-7 лет в школе"
                            },
                            {
                                "target_lang_word": "малыш",
                                "target_lang_sense_clarification": "в контексте школы для самых маленьких"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "debdfbc3-69ff-36fb-9ba8-d0e8756ecd4b",
                    "sense_definition": "A person who has not yet reached the legal age of adulthood; a minor.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legal_minor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The court appointed a guardian because the heir was still an <w>infant</w>.",
                            "target_lang_translations": {
                                "ru": "Суд назначил опекуна, так как наследник был еще <w>несовершеннолетним</w>."
                            }
                        },
                        {
                            "text": "Legally, an <w>infant</w> cannot enter into binding contracts without approval.",
                            "target_lang_translations": {
                                "ru": "По закону <w>несовершеннолетний</w> не может заключать обязательные договоры без одобрения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "infant plaintiff"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, не достигшее возраста совершеннолетия; несовершеннолетний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несовершеннолетний",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8e87338-ecf0-3c03-a429-680bef71fe7b",
                    "sense_definition": "A noble or aristocratic youth.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noble_youth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The royal <w>infant</w> was presented to the court with great ceremony.",
                            "target_lang_translations": {
                                "ru": "Королевский <w>инфант</w> был представлен двору с большой церемонией."
                            }
                        },
                        {
                            "text": "Old ballads tell stories of a brave <w>infant</w> seeking his fortune.",
                            "target_lang_translations": {
                                "ru": "Старинные баллады рассказывают истории о храбром <w>отроке</w>, ищущем счастья."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Юноша знатного или аристократического происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инфант",
                                "target_lang_sense_clarification": "титул принцев в Испании и Португалии"
                            },
                            {
                                "target_lang_word": "отрок",
                                "target_lang_sense_clarification": "устаревшее; подросток, юноша"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "47451366-3d0c-3171-9f73-6e2c5c724143",
                    "sense_definition": "Being in a very early stage of development or growth; nascent.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "early_stage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government imposed tariffs to protect the <w>infant</w> industry from foreign competition.",
                            "target_lang_translations": {
                                "ru": "Правительство ввело тарифы, чтобы защитить <w>зарождающуюся</w> промышленность от иностранной конкуренции."
                            }
                        },
                        {
                            "text": "We walked along the banks of the <w>infant</w> river near its source.",
                            "target_lang_translations": {
                                "ru": "Мы шли по берегам <w>молодой</w> реки недалеко от ее истока."
                            }
                        }
                    ],
                    "common_phrases": [
                        "infant industry",
                        "infant stage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся на очень ранней стадии развития или роста; зарождающийся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарождающийся",
                                "target_lang_sense_clarification": "только начинающий существовать"
                            },
                            {
                                "target_lang_word": "молодой",
                                "target_lang_sense_clarification": "недавно возникший"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5180cc70-3ec7-3c29-add6-29509123bbf3",
                    "sense_definition": "Relating to the education of young children, usually between four and seven years old.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "school_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is training to become an <w>infant</w> teacher at the local college.",
                            "target_lang_translations": {
                                "ru": "Она учится на учителя <w>младших классов</w> в местном колледже."
                            }
                        },
                        {
                            "text": "The <w>infant</w> curriculum focuses heavily on play-based learning.",
                            "target_lang_translations": {
                                "ru": "Учебная программа <w>для малышей</w> делает большой упор на игровое обучение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "infant education",
                        "infant teacher"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British and Irish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к обучению маленьких детей, обычно в возрасте от четырех до семи лет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "младший",
                                "target_lang_sense_clarification": "относящийся к начальным классам"
                            },
                            {
                                "target_lang_word": "дошкольный",
                                "target_lang_sense_clarification": "относящийся к воспитанию детей раннего возраста"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b020ea02-dec7-317b-a130-38d7e2f9daae",
                    "sense_definition": "To give birth to a child or bring something into existence.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_bear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The queen <w>infanted</w> a healthy son during the harsh winter.",
                            "target_lang_translations": {
                                "ru": "Королева <w>родила</w> здорового сына суровой зимой."
                            }
                        },
                        {
                            "text": "New political movements were <w>infanted</w> out of the chaos of the revolution.",
                            "target_lang_translations": {
                                "ru": "Новые политические движения <w>зародились</w> в хаосе революции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rare"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Родить ребенка или произвести что-либо на свет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родить",
                                "target_lang_sense_clarification": "произвести на свет ребенка"
                            },
                            {
                                "target_lang_word": "породить",
                                "target_lang_sense_clarification": "создать, стать причиной возникновения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

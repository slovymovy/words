            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa4c6d94-283b-3a3b-932d-8781342cff64",
                    "sense_definition": "Obraz, ilustracja lub element wizualny tworzony i przetwarzany za pomocą technik komputerowych; także ogół oprawy wizualnej w grach lub programach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computer_graphics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta nowa gra komputerowa ma niesamowicie realistyczną <grafikę>.",
                            "target_lang_translations": {
                                "ru": "У этой новой компьютерной игры невероятно реалистичная <графика>."
                            }
                        },
                        {
                            "text": "Do naszej prezentacji potrzebna jest czytelna <grafika> wykresu.",
                            "target_lang_translations": {
                                "ru": "Для нашей презентации нужна четкая <графика> (визуализация) графика."
                            }
                        }
                    ],
                    "synonyms": [
                        "obraz",
                        "ilustracja",
                        "wizualizacja"
                    ],
                    "antonyms": [
                        "tekst"
                    ],
                    "common_phrases": [
                        "grafika komputerowa",
                        "grafika trójwymiarowa",
                        "grafika wektorowa",
                        "silnik grafiki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображение, иллюстрация или визуальный элемент, созданный с помощью компьютерных технологий; также визуальное оформление игр или программ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "графика",
                                "target_lang_sense_clarification": "компьютерная или визуальная"
                            },
                            {
                                "target_lang_word": "изображение",
                                "target_lang_sense_clarification": "картинка, визуальный элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94204561-089c-3026-8c8b-c9362c29c1e2",
                    "sense_definition": "Dział sztuk plastycznych obejmujący techniki pozwalające na powielanie rysunku na papierze lub tkaninie z uprzednio przygotowanej formy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "art_discipline",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum narodowym otwarto dużą wystawę malarstwa i <grafiki>.",
                            "target_lang_translations": {
                                "ru": "В Национальном музее открылась большая выставка живописи и <графики>."
                            }
                        },
                        {
                            "text": "Jej ulubioną dziedziną sztuki jest <grafika> użytkowa, zwłaszcza plakat.",
                            "target_lang_translations": {
                                "ru": "Её любимым видом искусства является прикладная <графика>, особенно плакат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grafika użytkowa",
                        "grafika warsztatowa",
                        "wydział grafiki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид изобразительного искусства, использующий техники тиражирования рисунка на бумаге или ткани с подготовленной формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "графика",
                                "target_lang_sense_clarification": "вид искусства"
                            }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f0ecf667-dfe6-35ec-b5a2-8b51a56a5766",
                    "sense_definition": "Zich te voet verplaatsen in een rustig tempo.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_basic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>loop</w> elke ochtend naar mijn werk.",
                            "target_lang_translations": {
                                "en": "I <w>walk</w> to work every morning."
                            }
                        },
                        {
                            "text": "We kunnen naar de supermarkt <w>lopen</w> om boodschappen te doen.",
                            "target_lang_translations": {
                                "en": "We can <w>walk</w> to the supermarket to get groceries."
                            }
                        }
                    ],
                    "synonyms": [
                        "wandelen",
                        "stappen",
                        "gaan"
                    ],
                    "antonyms": [
                        "stilstaan",
                        "rijden"
                    ],
                    "common_phrases": [
                        "naar huis lopen",
                        "op straat lopen",
                        "een stukje lopen"
                    ],
                    "target_lang_definitions": {
                        "en": "To move on foot at a moderate pace."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "walk",
                                "target_lang_sense_clarification": "move on foot"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d21b122-eee8-34db-bc02-6ab6cb853486",
                    "sense_definition": "Functioneren, werken of zich ontwikkelen (van zaken, machines of processen).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het nieuwe project <w>loopt</w> precies volgens plan.",
                            "target_lang_translations": {
                                "en": "The new project is <w>running</w> exactly according to plan."
                            }
                        },
                        {
                            "text": "De motor van de auto <w>loopt</w> heel rustig en soepel.",
                            "target_lang_translations": {
                                "en": "The car's engine <w>runs</w> very quietly and smoothly."
                            }
                        }
                    ],
                    "synonyms": [
                        "gaan",
                        "druppen"
                    ],
                    "antonyms": [
                        "stilstaan"
                    ],
                    "common_phrases": [
                        "gesmeerd lopen",
                        "op rolletjes lopen",
                        "lekker lopen"
                    ],
                    "target_lang_definitions": {
                        "en": "To function, operate, or progress (of things, machines, or processes)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "operate or function"
                            },
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "function properly"
                            },
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "proceed or progress"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b861e20e-db16-39fc-8ffc-115f1477474e",
                    "sense_definition": "Zich uitstrekken over een bepaalde afstand of in een bepaalde richting.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rivier <w>loopt</w> dwars door het oude centrum van de stad.",
                            "target_lang_translations": {
                                "en": "The river <w>runs</w> straight through the old city center."
                            }
                        },
                        {
                            "text": "Deze lange weg <w>loopt</w> helemaal tot aan de kust.",
                            "target_lang_translations": {
                                "en": "This long road <w>runs</w> all the way to the coast."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dood lopen",
                        "door lopen"
                    ],
                    "target_lang_definitions": {
                        "en": "To extend over a certain distance or in a certain direction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "extend spatially"
                            },
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "lead or extend"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fffdec5-69fe-35e2-a165-ead37027f029",
                    "sense_definition": "Zich snel voortbewegen; rennen (vaak als sportbeoefening).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_fast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft gisteren de marathon in een recordtijd <w>gelopen</w>.",
                            "target_lang_translations": {
                                "en": "He <w>ran</w> the marathon in record time yesterday."
                            }
                        },
                        {
                            "text": "Je moet hard <w>lopen</w> als je de trein nog wilt halen.",
                            "target_lang_translations": {
                                "en": "You have to <w>run</w> fast if you still want to catch the train."
                            }
                        }
                    ],
                    "synonyms": [
                        "rennen",
                        "hardlopen",
                        "hollen"
                    ],
                    "antonyms": [
                        "wandelen"
                    ],
                    "common_phrases": [
                        "hard lopen",
                        "een record lopen"
                    ],
                    "target_lang_definitions": {
                        "en": "To move quickly; to run (often as a sport)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "move quickly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f707cea-7136-3e41-a319-1f6a770a137b",
                    "sense_definition": "Vloeien, stromen of vloeistof verliezen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "liquid_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is een lek in de kraan en het water <w>loopt</w> langzaam weg.",
                            "target_lang_translations": {
                                "en": "There is a leak in the tap and the water is slowly <w>running</w> away."
                            }
                        },
                        {
                            "text": "Toen het vat viel, begon het bier eruit te <w>lopen</w>.",
                            "target_lang_translations": {
                                "en": "When the barrel fell, the beer started to <w>run</w> out of it."
                            }
                        }
                    ],
                    "synonyms": [
                        "lekken",
                        "druppen",
                        "lekken"
                    ],
                    "common_phrases": [
                        "leeg lopen",
                        "over lopen",
                        "de kraan loopt"
                    ],
                    "target_lang_definitions": {
                        "en": "To flow, stream, or lose liquid."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "flow of liquid"
                            },
                            {
                                "target_lang_word": "leak",
                                "target_lang_sense_clarification": "lose liquid"
                            },
                            {
                                "target_lang_word": "flow",
                                "target_lang_sense_clarification": "move as a fluid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f81a0fb8-7d82-3d56-b86a-50ed242942d7",
                    "sense_definition": "Geldig zijn of aan de gang zijn (van een periode, contract of proces).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_validity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn contract bij dit bedrijf <w>loopt</w> binnenkort af.",
                            "target_lang_translations": {
                                "en": "My contract with this company <w>runs</w> out soon."
                            }
                        },
                        {
                            "text": "De rechtszaak tegen de verdachte <w>loopt</w> al een hele tijd.",
                            "target_lang_translations": {
                                "en": "The lawsuit against the suspect has been <w>running</w> for a long time."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het contract loopt",
                        "de rente loopt",
                        "ten einde lopen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be valid or in progress (of a period, contract, or process)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "be active or ongoing"
                            },
                            {
                                "target_lang_word": "last",
                                "target_lang_sense_clarification": "continue for a duration"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7456cbd7-a67a-3654-87db-dc418934d8f4",
                    "sense_definition": "Hulpwerkwoord dat een voortdurende handeling uitdrukt, vaak met een ondertoon van irritatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "grammar_auxiliary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Loop</w> niet zo te zaniken over die kleine details.",
                            "target_lang_translations": {
                                "en": "Don't <w>go around</w> whining about those small details."
                            }
                        },
                        {
                            "text": "Wat <w>loop</w> jij daar nou de hele tijd te doen?",
                            "target_lang_translations": {
                                "en": "What are you <w>going around</w> doing there all the time?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "lopen te zeuren",
                        "lopen te klieren",
                        "lopen te dromen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to express annoyance"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Auxiliary verb expressing a continuous action, often with an undertone of irritation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go around",
                                "target_lang_sense_clarification": "continuous action with annoyance"
                            },
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "repetitive action"
                            }
                        ]
                    }
                }
            ]
        },

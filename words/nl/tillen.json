                    ],
                    "synonyms": [
                        "oplichten",
                        "heffen",
                        "beuren"
                    ],
                    "common_phrases": [
                        "zwaar aan iets tillen",
                        "uit bed tillen",
                        "iets zwaars tillen"
                    ],
                    "target_lang_definitions": {

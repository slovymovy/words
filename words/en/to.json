    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "98e1f66e-95d6-3c43-b126-4ca194101bda",
                    "sense_definition": "Indicating destination, direction, or the final position of movement.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_dest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to walk <w>to</w> the nearest shop.",
                            "target_lang_translations": {
                                "ru": "Мы решили пойти <w>в</w> ближайший магазин."
                            }
                        },
                        {
                            "text": "The children ran <w>to</w> the park after school.",
                            "target_lang_translations": {
                                "ru": "После школы дети побежали <w>в</w> парк."
                            }
                        },
                        {
                            "text": "She looked <w>to</w> the heavens for an answer.",
                            "target_lang_translations": {
                                "ru": "Она посмотрела <w>на</w> небеса в поисках ответа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go to school",
                        "walk to work",
                        "fly to London",
                        "come to me"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на направление, место назначения или конечное положение движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "направление внутрь чего-либо"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "направление на поверхность или мероприятие"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "направление к кому-либо или чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3edfa0ce-31a5-346b-af9a-2a582acdad67",
                    "sense_definition": "Used to indicate the target or recipient of an action.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "recipient",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please give the book <w>to</w> your brother.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отдай книгу твоему <w>брату</w>."
                            }
                        },
                        {
                            "text": "I need to speak <w>to</w> the manager immediately.",
                            "target_lang_translations": {
                                "ru": "Мне нужно немедленно поговорить <w>с</w> менеджером."
                            }
                        }
                    ],
                    "common_phrases": [
                        "give it to me",
                        "talk to him",
                        "listen to music",
                        "explain to her"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на лицо или предмет, на который направлено действие (адресат)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "(дательный падеж)",
                                "target_lang_sense_clarification": "используется без предлога (кому?)"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "предлог направления к лицу (обращаться к)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c54613f8-c7be-35ed-bb75-a8d3bb7a597d",
                    "sense_definition": "Denotes the end point of a range or period.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "range",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The store is open from nine <w>to</w> five every day.",
                            "target_lang_translations": {
                                "ru": "Магазин открыт с девяти <w>до</w> пяти каждый день."
                            }
                        },
                        {
                            "text": "It will take two <w>to</w> three weeks to fix the car.",
                            "target_lang_translations": {
                                "ru": "Починка машины займет от двух <w>до</w> трех недель."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nine to five",
                        "Monday to Friday",
                        "start to finish",
                        "top to bottom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает конечную точку диапазона или периода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "указывает на предел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a56023bc-27d6-3031-bd06-5219dfa26d59",
                    "sense_definition": "Used when telling the time to indicate the minutes preceding the next hour.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is currently a quarter <w>to</w> four in the afternoon.",
                            "target_lang_translations": {
                                "ru": "Сейчас <w>без</w> четверти четыре дня."
                            }
                        },
                        {
                            "text": "We have ten minutes <w>to</w> nine before the show starts.",
                            "target_lang_translations": {
                                "ru": "У нас есть десять минут <w>до</w> девяти до начала шоу."
                            }
                        }
                    ],
                    "antonyms": [
                        "past"
                    ],
                    "common_phrases": [
                        "quarter to",
                        "ten to",
                        "five to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется при указании времени для обозначения минут, оставшихся до следующего часа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "без",
                                "target_lang_sense_clarification": "при указании времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "baa7c683-542e-39ff-af5b-45b0d189c875",
                    "sense_definition": "Used after certain adjectives to indicate their application or connection.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "adj_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This problem is very similar <w>to</w> the one we solved yesterday.",
                            "target_lang_translations": {
                                "ru": "Эта задача очень похожа <w>на</w> ту, которую мы решали вчера."
                            }
                        },
                        {
                            "text": "He was always very kind <w>to</w> animals.",
                            "target_lang_translations": {
                                "ru": "Он всегда был очень добр <w>к</w> животным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "similar to",
                        "relevant to",
                        "kind to",
                        "used to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется после определенных прилагательных для указания связи или отношения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "напр. добр к"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "напр. похож на"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76c9737c-6829-3614-a6fb-4ab08c1db757",
                    "sense_definition": "So as to contact, press against, or be attached to something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I fixed the important notice <w>to</w> the wall.",
                            "target_lang_translations": {
                                "ru": "Я прикрепил важное объявление <w>на</w> стену."
                            }
                        },
                        {
                            "text": "She held the cold cloth <w>to</w> her forehead.",
                            "target_lang_translations": {
                                "ru": "Она прижала холодную ткань <w>к</w> своему лбу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shoulder to the wheel",
                        "back to back",
                        "face to face"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на контакт, прижатие или прикрепление к чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "прикрепление к поверхности"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "размещение на поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5252df71-6896-3c0d-b470-765edcdb5b4b",
                    "sense_definition": "Used to indicate a ratio or comparison between two numbers or quantities.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ratio",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The odds of winning are ten <w>to</w> one.",
                            "target_lang_translations": {
                                "ru": "Шансы на победу — десять <w>к</w> одному."
                            }
                        },
                        {
                            "text": "Our team won the game by three goals <w>to</w> zero.",
                            "target_lang_translations": {
                                "ru": "Наша команда выиграла игру со счетом три <w>к</w> нулю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ten to one",
                        "one to one"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания соотношения или сравнения двух чисел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "математическое отношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c2af039-1d36-3273-b26d-9b534f0e9e1a",
                    "sense_definition": "Indicating a change of state or a result of an action.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mother gently sang her baby <w>to</w> sleep.",
                            "target_lang_translations": {
                                "ru": "Мать нежно пела ребенку, убаюкивая его <w>ко</w> сну."
                            }
                        },
                        {
                            "text": "You should whisk the mixture <w>to</w> a smooth consistency.",
                            "target_lang_translations": {
                                "ru": "Вам следует взбить смесь <w>до</w> однородной консистенции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "to sleep",
                        "to death",
                        "to pieces"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на изменение состояния или результат действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "доведение до состояния"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "разделение на части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef0f68bc-2670-3373-a41b-1c41a7eb745e",
                    "sense_definition": "Used in mathematics to indicate exponentiation.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Three <w>to</w> the power of two equals nine.",
                            "target_lang_translations": {
                                "ru": "Три <w>в</w> степени два равно девяти."
                            }
                        },
                        {
                            "text": "Ten <w>to</w> the fourth power is ten thousand.",
                            "target_lang_translations": {
                                "ru": "Десять <w>в</w> четвертой степени — это десять тысяч."
                            }
                        }
                    ],
                    "common_phrases": [
                        "to the power of"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "mathematical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в математике для обозначения возведения в степень."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "в степени"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "ddbd8eb3-b8f0-3b3e-b676-f449b122a61c",
                    "sense_definition": "A particle used before a verb to mark the infinitive form.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "infinitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I really want <w>to</w> learn a new language.",
                            "target_lang_translations": {
                                "ru": "Я очень хочу выучи<w>ть</w> новый язык."
                            }
                        },
                        {
                            "text": "It is important <w>to</w> sleep for eight hours.",
                            "target_lang_translations": {
                                "ru": "Важно спа<w>ть</w> по восемь часов."
                            }
                        },
                        {
                            "text": "To be or not <w>to</w> be is the question.",
                            "target_lang_translations": {
                                "ru": "Бы<w>ть</w> или не бы<w>ть</w> — вот в чем вопрос."
                            }
                        }
                    ],
                    "common_phrases": [
                        "to be",
                        "to do",
                        "to go",
                        "to have"
                    ],
                    "target_lang_definitions": {
                        "ru": "Частица, используемая перед глаголом для обозначения неопределенной формы (инфинитива)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "-ть",
                                "target_lang_sense_clarification": "суффикс инфинитива"
                            },
                            {
                                "target_lang_word": "-ти",
                                "target_lang_sense_clarification": "суффикс инфинитива"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "098ba363-c0f9-36e1-9fed-4b0ae2477d69",
                    "sense_definition": "Used to indicate the purpose or intention of an action (short for 'in order to').",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She went to the market <w>to</w> buy some fresh fruit.",
                            "target_lang_translations": {
                                "ru": "Она пошла на рынок, <w>чтобы</w> купить свежих фруктов."
                            }
                        },
                        {
                            "text": "He studied hard <w>to</w> pass the difficult exam.",
                            "target_lang_translations": {
                                "ru": "Он усердно учился, <w>чтобы</w> сдать сложный экзамен."
                            }
                        }
                    ],
                    "common_phrases": [
                        "to get",
                        "to see",
                        "to find"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на цель или намерение действия (сокращение от 'in order to')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чтобы",
                                "target_lang_sense_clarification": "союз цели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc99a23c-253f-3104-8ba9-f4f123c36e13",
                    "sense_definition": "Used at the end of a sentence to imply a verb mentioned previously.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "implied",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I did not want to go, but I had <w>to</w>.",
                            "target_lang_translations": {
                                "ru": "Я не хотел идти, но мне пришло<w>сь</w>."
                            }
                        },
                        {
                            "text": "You can come with us if you want <w>to</w>.",
                            "target_lang_translations": {
                                "ru": "Ты можешь пойти с нами, если хоче<w>шь</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "want to",
                        "have to",
                        "ought to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в конце предложения, подразумевая ранее упомянутый глагол."
                    },
                    "translations": {
                        "ru": []
                    }
                },
                {
                    "sense_id": "98356366-5b2e-3c74-8e64-1325b12ba91c",
                    "sense_definition": "Used to indicate an obligation or a directive given to the subject.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are <w>to</w> go to the store and buy a bottle of milk.",
                            "target_lang_translations": {
                                "ru": "Ты <w>должен</w> пойти в магазин и купить бутылку молока."
                            }
                        },
                        {
                            "text": "The students are <w>to</w> remain seated until the bell rings.",
                            "target_lang_translations": {
                                "ru": "Студенты <w>должны</w> оставаться на местах до звонка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "are to",
                        "is to"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "formal or strict context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на обязательство или указание, данное субъекту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "должен",
                                "target_lang_sense_clarification": "выражение обязанности"
                            }
                        ]
                    }
                }
            ]
        },
        {

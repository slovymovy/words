{
    "word_family": [
        "verversen",
        "versheid",
        "versbakken"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b81ddeb7-da9d-3618-b02d-6a12f8e1401d",
                    "sense_definition": "Nieuw, onlangs gemaakt of gebeurd; nog niet oud of bedorven.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "fresh_new",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bakker verkoopt elke ochtend heerlijk <w>vers</w> brood.",
                            "target_lang_translations": {
                                "en": "The baker sells delicious <w>fresh</w> bread every morning.",
                                "ru": "Пекарь каждое утро продаёт вкусный <w>свежий</w> хлеб."
                            }
                        },
                        {
                            "text": "Er ligt een laag <w>verse</w> sneeuw in de tuin.",
                            "target_lang_translations": {
                                "en": "There is a layer of <w>fresh</w> snow in the garden.",
                                "ru": "В саду лежит слой <w>свежего</w> снега."
                            }
                        },
                        {
                            "text": "Deze krant is <w>vers</w> van de pers en staat vol nieuws.",
                            "target_lang_translations": {
                                "en": "This newspaper is <w>fresh</w> off the press and full of news.",
                                "ru": "Эта газета <w>свежая</w>, прямо из печати, и полна новостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieuw",
                        "recent"
                    ],
                    "antonyms": [
                        "oud",
                        "bedorven"
                    ],
                    "common_phrases": [
                        "vers brood",
                        "vers van de pers",
                        "verse lucht"
                    ],
                    "target_lang_definitions": {
                        "en": "New, recently made or happened; not yet old or spoiled.",
                        "ru": "Новый, недавно сделанный или произошедший; ещё не старый и не испорченный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fresh",
                                "target_lang_sense_clarification": "newly made or obtained"
                            },
                            {
                                "target_lang_word": "recent",
                                "target_lang_sense_clarification": "happened a short while ago"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "свежий",
                                "target_lang_sense_clarification": "новый, недавний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e726ffd5-e138-3c7d-a90a-a75fe344a161",
                    "sense_definition": "Niet geconserveerd, ingeblikt of diepgevroren; in natuurlijke toestand.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fresh_food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kook liever met <w>verse</w> groenten dan met groenten uit blik.",
                            "target_lang_translations": {
                                "en": "I prefer cooking with <w>fresh</w> vegetables rather than canned vegetables.",
                                "ru": "Я предпочитаю готовить со <w>свежими</w> овощами, а не с консервированными."
                            }
                        },
                        {
                            "text": "Op de markt kun je goedkoop <w>verse</w> vis kopen.",
                            "target_lang_translations": {
                                "en": "You can buy cheap <w>fresh</w> fish at the market.",
                                "ru": "На рынке можно дёшево купить <w>свежую</w> рыбу."
                            }
                        }
                    ],
                    "synonyms": [
                        "onbewerkt"
                    ],
                    "antonyms": [
                        "geconserveerd",
                        "ingeblikt",
                        "diepgevroren"
                    ],
                    "common_phrases": [
                        "verse groenten",
                        "verse vis",
                        "verse melk"
                    ],
                    "target_lang_definitions": {
                        "en": "Not preserved, canned, or frozen; in a natural state.",
                        "ru": "Не консервированный, не в банке и не замороженный; в натуральном виде."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fresh",
                                "target_lang_sense_clarification": "not preserved or frozen (food)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "свежий",
                                "target_lang_sense_clarification": "натуральный, не обработанный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b5d1caa6-368b-3e83-a18d-1a9f6500a4b8",
                    "sense_definition": "Een groep regels in een gedicht of lied die bij elkaar horen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "stanza",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de kerk zongen we het eerste en het laatste <w>vers</w> van het lied.",
                            "target_lang_translations": {
                                "en": "In church we sang the first and the last <w>verse</w> of the song.",
                                "ru": "В церкви мы пели первый и последний <w>куплет</w> песни."
                            }
                        },
                        {
                            "text": "Ken jij de tekst van het tweede <w>vers</w> uit je hoofd?",
                            "target_lang_translations": {
                                "en": "Do you know the lyrics of the second <w>verse</w> by heart?",
                                "ru": "Ты знаешь текст второго <w>куплета</w> наизусть?"
                            }
                        }
                    ],
                    "synonyms": [
                        "strofe",
                        "couplet"
                    ],
                    "common_phrases": [
                        "het eerste vers",
                        "een vers zingen"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of lines in a poem or song that belong together.",
                        "ru": "Группа строк в стихотворении или песне, которые связаны друг с другом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "verse",
                                "target_lang_sense_clarification": "section of a song or poem"
                            },
                            {
                                "target_lang_word": "stanza",
                                "target_lang_sense_clarification": "group of lines in poetry"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "куплет",
                                "target_lang_sense_clarification": "часть песни"
                            },
                            {
                                "target_lang_word": "строфа",
                                "target_lang_sense_clarification": "часть стихотворения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cbd6ef3-95c4-36b7-99a4-4bab45b0fb8f",
                    "sense_definition": "Een gedicht, meestal kort van omvang en op rijm.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "poem",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het kind leerde een leuk <w>versje</w> voor Sinterklaas.",
                            "target_lang_translations": {
                                "en": "The child learned a nice <w>poem</w> for Sinterklaas.",
                                "ru": "Ребёнок выучил забавный <w>стишок</w> для Синтерклааса."
                            }
                        },
                        {
                            "text": "Hij schreef een mooi <w>vers</w> in haar poëziealbum.",
                            "target_lang_translations": {
                                "en": "He wrote a beautiful <w>poem</w> in her poetry album.",
                                "ru": "Он написал красивое <w>стихотворение</w> в её альбом."
                            }
                        }
                    ],
                    "synonyms": [
                        "gedicht",
                        "rijmpje"
                    ],
                    "common_phrases": [
                        "een versje opzeggen",
                        "een vers schrijven"
                    ],
                    "target_lang_definitions": {
                        "en": "A poem, usually short in size and rhyming.",
                        "ru": "Стихотворение, обычно небольшое по объёму и рифмованное."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "poem",
                                "target_lang_sense_clarification": "literary composition"
                            },
                            {
                                "target_lang_word": "rhyme",
                                "target_lang_sense_clarification": "short poem with rhyming lines"
                            },
                            {
                                "target_lang_word": "verse",
                                "target_lang_sense_clarification": "poetry in general"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "стихотворение",
                                "target_lang_sense_clarification": "поэтическое произведение"
                            },
                            {
                                "target_lang_word": "стишок",
                                "target_lang_sense_clarification": "короткое или детское стихотворение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee9a3f54-3b61-39cc-89c6-567a9c2f3831",
                    "sense_definition": "Een enkele regel uit een gedicht.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "line",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit <w>vers</w> heeft een bijzonder ritme vergeleken met de rest.",
                            "target_lang_translations": {
                                "en": "This <w>line</w> has a special rhythm compared to the rest.",
                                "ru": "У этого <w>стиха</w> особенный ритм по сравнению с остальными."
                            }
                        },
                        {
                            "text": "De dichter heeft lang nagedacht over het laatste <w>vers</w>.",
                            "target_lang_translations": {
                                "en": "The poet thought for a long time about the last <w>line</w>.",
                                "ru": "Поэт долго размышлял над последним <w>стихом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dichtregel"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "In modern Dutch 'dichtregel' is more common for a single line."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A single line from a poem.",
                        "ru": "Отдельная строка из стихотворения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "line",
                                "target_lang_sense_clarification": "single row of words in a poem"
                            },
                            {
                                "target_lang_word": "verse",
                                "target_lang_sense_clarification": "single line (archaic or literary)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "стих",
                                "target_lang_sense_clarification": "строка поэтического текста"
                            },
                            {
                                "target_lang_word": "строка",
                                "target_lang_sense_clarification": "единичная линия текста"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
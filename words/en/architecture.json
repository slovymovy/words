    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e4d5a2b1-8b11-3609-8d9f-fef650377d01",
                    "sense_definition": "The art and science of designing and constructing buildings and other physical structures.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "building_design",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to study <w>architecture</w> at the university.",
                            "target_lang_translations": {
                                "ru": "Она решила изучать <w>архитектуру</w> в университете."
                            }
                        },
                        {
                            "text": "The city is famous for its beautiful and historic <w>architecture</w>.",
                            "target_lang_translations": {
                                "ru": "Город славится своей красивой и исторической <w>архитектурой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "landscape architecture",
                        "study architecture",
                        "degree in architecture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусство и наука проектирования и строительства зданий и других сооружений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "архитекту́ра",
                                "target_lang_sense_clarification": "наука и искусство"
                            },
                            {
                                "target_lang_word": "зо́дчество",
                                "target_lang_sense_clarification": "высокий стиль, искусство строительства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09071202-2d23-36df-ba5a-29b83bc6bef2",
                    "sense_definition": "A particular style or method of building design.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "building_design",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cathedral is a stunning example of Gothic <w>architecture</w>.",
                            "target_lang_translations": {
                                "ru": "Этот собор — потрясающий образец готической <w>архитектуры</w>."
                            }
                        },
                        {
                            "text": "We admired the modern <w>architecture</w> of the new library.",
                            "target_lang_translations": {
                                "ru": "Мы восхищались современной <w>архитектурой</w> новой библиотеки."
                            }
                        }
                    ],
                    "synonyms": [
                        "style",
                        "design"
                    ],
                    "common_phrases": [
                        "modern architecture",
                        "classical architecture",
                        "Gothic architecture",
                        "colonial architecture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный стиль или метод проектирования зданий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "архитекту́ра",
                                "target_lang_sense_clarification": "стиль постройки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28a0e85a-ea3e-3d9f-8ef5-29c4cb2e9795",
                    "sense_definition": "The conceptual structure and logical organization of a computer system, software, or network.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "system_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineers are discussing the <w>architecture</w> of the new database system.",
                            "target_lang_translations": {
                                "ru": "Инженеры обсуждают <w>архитектуру</w> новой системы баз данных."
                            }
                        },
                        {
                            "text": "Good software <w>architecture</w> makes the application easier to maintain and update.",
                            "target_lang_translations": {
                                "ru": "Хорошая <w>архитектура</w> программного обеспечения упрощает обслуживание и обновление приложения."
                            }
                        }
                    ],
                    "synonyms": [
                        "structure",
                        "design"
                    ],
                    "common_phrases": [
                        "software architecture",
                        "system architecture",
                        "network architecture",
                        "computer architecture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Концептуальная структура и логическая организация компьютерной системы, программного обеспечения или сети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "архитекту́ра",
                                "target_lang_sense_clarification": "компьютерная/системная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cdd72a2-7bde-309c-a96a-216376419ec0",
                    "sense_definition": "The complex structure or arrangement of the parts of something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "system_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>architecture</w> of the novel involves multiple timelines and perspectives.",
                            "target_lang_translations": {
                                "ru": "<w>Архитектура</w> романа включает в себя множество временных линий и точек зрения."
                            }
                        },
                        {
                            "text": "Scientists are studying the complex <w>architecture</w> of the human brain.",
                            "target_lang_translations": {
                                "ru": "Ученые изучают сложную <w>архитектуру</w> человеческого мозга."
                            }
                        }
                    ],
                    "synonyms": [
                        "structure",
                        "composition",
                        "makeup",
                        "construction"
                    ],
                    "common_phrases": [
                        "internal architecture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сложная структура или расположение составных частей чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "архитекту́ра",
                                "target_lang_sense_clarification": "перен., строение сложного объекта"
                            },
                            {
                                "target_lang_word": "строе́ние",
                                "target_lang_sense_clarification": "структура"
                            }
                        ]
                    }
                }
            ]
        }
    ]

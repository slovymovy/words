    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5c077b6d-fcc7-3e9d-ae79-23810b920bd0",
                    "sense_definition": "Something that is made to be sold, usually in large quantities.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "commercial_goods",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company launched a new <w>product</w> onto the market last week.",
                            "target_lang_translations": {
                                "ru": "На прошлой неделе компания выпустила на рынок новый <w>продукт</w>."
                            }
                        },
                        {
                            "text": "We guarantee the quality of every <w>product</w> we sell in our store.",
                            "target_lang_translations": {
                                "ru": "Мы гарантируем качество каждого <w>товара</w>, который продаем в нашем магазине."
                            }
                        }
                    ],
                    "synonyms": [
                        "goods",
                        "merchandise",
                        "wares"
                    ],
                    "common_phrases": [
                        "consumer product",
                        "dairy product",
                        "new product",
                        "product launch"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что произведено для продажи, обычно в больших количествах; изделие или товар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продукт",
                                "target_lang_sense_clarification": "общее значение, результат производства"
                            },
                            {
                                "target_lang_word": "товар",
                                "target_lang_sense_clarification": "предмет торговли"
                            },
                            {
                                "target_lang_word": "продукция",
                                "target_lang_sense_clarification": "совокупность произведенных товаров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18c93465-8628-37a8-aca8-706ce43c740a",
                    "sense_definition": "A quantity obtained by multiplication of two or more numbers.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>product</w> of 5 and 4 is 20.",
                            "target_lang_translations": {
                                "ru": "<w>Произведение</w> 5 и 4 равно 20."
                            }
                        },
                        {
                            "text": "You need to calculate the <w>product</w> of these two figures to solve the equation.",
                            "target_lang_translations": {
                                "ru": "Вам нужно вычислить <w>произведение</w> этих двух чисел, чтобы решить уравнение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "product of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Величина, получаемая в результате умножения двух или более чисел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "произведение",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "738b25d7-7e4d-3dee-8fbc-2496ada7424a",
                    "sense_definition": "Something that happens or exists as a result of a particular process or set of circumstances.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His incredible skill is the <w>product</w> of many hours of practice.",
                            "target_lang_translations": {
                                "ru": "Его невероятное мастерство — это <w>результат</w> многих часов практики."
                            }
                        },
                        {
                            "text": "We are all a <w>product</w> of our environment and upbringing.",
                            "target_lang_translations": {
                                "ru": "Мы все — <w>продукт</w> нашей окружающей среды и воспитания."
                            }
                        }
                    ],
                    "synonyms": [
                        "merchandise",
                        "wares",
                        "goods"
                    ],
                    "common_phrases": [
                        "product of"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что происходит или существует как результат определенного процесса или стечения обстоятельств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продукт",
                                "target_lang_sense_clarification": "порождение среды или обстоятельств"
                            },
                            {
                                "target_lang_word": "результат",
                                "target_lang_sense_clarification": "итог усилий или действий"
                            },
                            {
                                "target_lang_word": "плод",
                                "target_lang_sense_clarification": "переносное значение (плод воображения/труда)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1268dd2-09ea-3351-9569-f7186a1cef44",
                    "sense_definition": "A chemical substance formed as a result of a chemical reaction.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Water is a <w>product</w> of the reaction between hydrogen and oxygen.",
                            "target_lang_translations": {
                                "ru": "Вода — это <w>продукт</w> реакции между водородом и кислородом."
                            }
                        },
                        {
                            "text": "The chemist analyzed the final <w>product</w> in the test tube.",
                            "target_lang_translations": {
                                "ru": "Химик проанализировал конечный <w>продукт</w> в пробирке."
                            }
                        }
                    ],
                    "synonyms": [
                        "yield"
                    ],
                    "common_phrases": [
                        "chemical product"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химическое вещество, образовавшееся в результате химической реакции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продукт",
                                "target_lang_sense_clarification": "вещество, полученное в реакции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b220777-4e30-3477-bbdd-7f3b0fd29dc8",
                    "sense_definition": "Any preparation to be applied to the hair, skin, or nails to improve their appearance.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cosmetics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She uses a lot of <w>product</w> to keep her hair in place all day.",
                            "target_lang_translations": {
                                "ru": "Она использует много <w>средства</w> для укладки, чтобы волосы держались весь день."
                            }
                        },
                        {
                            "text": "You should wash the styling <w>product</w> out of your hair before going to sleep.",
                            "target_lang_translations": {
                                "ru": "Тебе следует смыть <w>средство</w> для укладки с волос перед сном."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hair product",
                        "beauty product"
                    ],
                    "target_lang_definitions": {
                        "ru": "Косметическое средство, наносимое на волосы, кожу или ногти для улучшения их внешнего вида."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средство",
                                "target_lang_sense_clarification": "косметический препарат"
                            },
                            {
                                "target_lang_word": "продукт",
                                "target_lang_sense_clarification": "в индустрии красоты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52eb64b7-1473-3553-bdad-0965d0c72bbc",
                    "sense_definition": "Illegal drugs, especially cocaine, when viewed as a commodity.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_drugs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police seized a large amount of <w>product</w> during the raid.",
                            "target_lang_translations": {
                                "ru": "Полиция изъяла крупную партию <w>товара</w> во время рейда."
                            }
                        },
                        {
                            "text": "He was arrested for trying to move the <w>product</w> across the border.",
                            "target_lang_translations": {
                                "ru": "Его арестовали за попытку перевезти <w>товар</w> через границу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used in criminal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нелегальные наркотики, рассматриваемые как товар (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "товар",
                                "target_lang_sense_clarification": "сленг, наркотики"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "47235b7e-e009-3059-8497-927c03e13e29",
                    "sense_definition": "To produce or bring forth.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fertile land <w>producted</w> a great harvest in those ancient times.",
                            "target_lang_translations": {
                                "ru": "Плодородная земля <w>производила</w> богатый урожай в те древние времена."
                            }
                        },
                        {
                            "text": "The document was <w>producted</w> before the judge for inspection.",
                            "target_lang_translations": {
                                "ru": "Документ был <w>предъявлен</w> судье для осмотра."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": "rarely used in modern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить, создавать или предъявлять (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "производить",
                                "target_lang_sense_clarification": "создавать"
                            },
                            {
                                "target_lang_word": "предъявлять",
                                "target_lang_sense_clarification": "показывать (документ)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
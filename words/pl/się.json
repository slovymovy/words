{
    "word_family": [],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "a07cf492-ec0a-39ff-b63e-ece6f7992187",
                    "sense_definition": "Zaimek zwrotny wskazujący, że wykonawca czynności jest jednocześnie jej obiektem (czynność wraca do podmiotu).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano myję <się> zimną wodą.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я умываю<сь> холодной водой."
                            }
                        },
                        {
                            "text": "Marek bardzo źle <się> dzisiaj czuje.",
                            "target_lang_translations": {
                                "ru": "Марек сегодня очень плохо <себя> чувствует."
                            }
                        }
                    ],
                    "synonyms": [
                        "siebie"
                    ],
                    "common_phrases": [
                        "nazywać się",
                        "czuć się",
                        "uczyć się",
                        "bawić się",
                        "bać się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвратное местоимение, указывающее, что действие направлено на самого деятеля (действие возвращается к субъекту)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "-ся",
                                "target_lang_sense_clarification": "суффикс возвратных глаголов"
                            },
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "214fc9e9-88fc-365a-9186-dc1051e1ceb2",
                    "sense_definition": "Zaimek oznaczający, że czynność jest wykonywana wzajemnie przez dwie lub więcej osób.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reciprocal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawno <się> nie widzieliśmy.",
                            "target_lang_translations": {
                                "ru": "Мы давно не видели<сь>."
                            }
                        },
                        {
                            "text": "Sąsiedzi często kłócą <się> o hałas.",
                            "target_lang_translations": {
                                "ru": "Соседи часто ругают<ся> из-за шума."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spotykać się",
                        "znać się",
                        "kochać się",
                        "lubić się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Местоимение, указывающее, что действие выполняется взаимно двумя или более лицами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "-ся",
                                "target_lang_sense_clarification": "суффикс взаимных глаголов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "474a3395-7c41-3ba7-ae11-09c294a779b5",
                    "sense_definition": "Słowo pomocnicze tworzące formy bezosobowe czasowników, używane gdy wykonawca czynności jest nieokreślony lub ogólny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "impersonal_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą bardzo szybko robi <się> ciemno.",
                            "target_lang_translations": {
                                "ru": "Зимой очень быстро становит<ся> темно."
                            }
                        },
                        {
                            "text": "W Polsce pije <się> dużo herbaty.",
                            "target_lang_translations": {
                                "ru": "В Польше пьет<ся> много чая."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mówi się",
                        "robi się",
                        "wydaje się",
                        "okazuje się",
                        "zdaje się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служебное слово для образования безличных форм глаголов (когда деятель не определен или обобщен)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "-ся",
                                "target_lang_sense_clarification": "суффикс безличных или пассивных конструкций"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69a6fd47-432d-3f13-9e48-a1577c937bb2",
                    "sense_definition": "Określenie własnego domu, miejsca zamieszkania lub stanu równowagi psychicznej (używane w formach przypadkowych).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "idiomatic_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wypadku długo dochodził do <siebie>.",
                            "target_lang_translations": {
                                "ru": "После аварии он долго приходил в <себя>."
                            }
                        },
                        {
                            "text": "Proszę, czuj się jak u <siebie> w domu.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, чувствуй себя как у <себя> дома."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dojść do siebie",
                        "u siebie",
                        "do siebie",
                        "być sobą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначение собственного дома, места жительства или душевного равновесия (используется в падежных формах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "полная форма возвратного местоимения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
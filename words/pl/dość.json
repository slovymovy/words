                    "sense_definition": "Wystarczyć; być wystarczającym (używane w konstrukcjach bezosobowych).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sufficiency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Dość> powiedzieć, że nikt nie był zadowolony z wyniku.",
                            "target_lang_translations": {
                                "ru": "<Достаточно> сказать, что никто не был доволен результатом."
                            }
                        },
                        {
                            "text": "O tym temacie powiedziano już <dość>.",
                            "target_lang_translations": {
                                "ru": "На эту тему уже сказано <достаточно>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystarczyć",
                        "dosyć"
                    ],
                    "common_phrases": [
                        "dość powiedzieć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть достаточным; хватать (используется в безличных конструкциях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достаточно",
                                "target_lang_sense_clarification": "в роли сказуемого"

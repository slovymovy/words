    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f3d9e34f-f046-32b8-ad96-0518c6fac3f8",
                    "sense_definition": "Simple past and past participle of <w>bar</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_block",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The security guard <barred> the entrance to the private club.",
                            "target_lang_translations": {
                                "ru": "Охранник <преградил> вход в частный клуб."
                            }
                        },
                        {
                            "text": "A fallen tree <barred> the road, forcing us to turn back.",
                            "target_lang_translations": {
                                "ru": "Упавшее дерево <перегородило> дорогу, заставив нас повернуть назад."
                            }
                        }
                    ],
                    "common_phrases": [
                        "barred the way",
                        "barred the door",
                        "barred access"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола <w>bar</w> (преграждать, запирать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преградил",
                                "target_lang_sense_clarification": "заблокировал путь или вход"
                            },
                            {
                                "target_lang_word": "перегородил",
                                "target_lang_sense_clarification": "создал препятствие поперек чего-либо"
                            },
                            {
                                "target_lang_word": "запер",
                                "target_lang_sense_clarification": "закрыл на засов или заблокировал дверь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14d2e9a3-6bb4-3b43-9c39-8fd275c8df94",
                    "sense_definition": "Simple past and past participle of <w>barr</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the ancient manuscript, it was written that they <barred> the gates at dusk.",
                            "target_lang_translations": {
                                "ru": "В древней рукописи было написано, что они <запирали> ворота на закате."
                            }
                        },
                        {
                            "text": "The merchant <barred> the goods from being sold in the market.",
                            "target_lang_translations": {
                                "ru": "Торговец <не допустил> товары к продаже на рынке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "obsolete spelling"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола <w>barr</w> (устаревшее написание)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запирал",
                                "target_lang_sense_clarification": "устаревшая или книжная форма действия"
                            },
                            {
                                "target_lang_word": "преграждал",
                                "target_lang_sense_clarification": "блокировал доступ (в историческом контексте)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fc7238c1-23aa-30f3-90da-dbf5d7237fe4",
                    "sense_definition": "Not allowed to do something or go somewhere; officially prohibited.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action_block",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <barred> from practicing law due to his past misconduct.",
                            "target_lang_translations": {
                                "ru": "Он <отстранён> от юридической практики из-за проступков в прошлом."
                            }
                        },
                        {
                            "text": "Players found guilty of doping are <barred> from the competition for two years.",
                            "target_lang_translations": {
                                "ru": "Игроки, уличенные в допинге, <отстраняются> от соревнований на два года."
                            }
                        }
                    ],
                    "synonyms": [
                        "banned",
                        "prohibited",
                        "forbidden",
                        "excluded"
                    ],
                    "common_phrases": [
                        "barred from entering",
                        "barred from holding office",
                        "permanently barred"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально не допущенный к чему-либо или запрещённый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отстранённый",
                                "target_lang_sense_clarification": "временно или постоянно лишенный права участвовать"
                            },
                            {
                                "target_lang_word": "лишённый права",
                                "target_lang_sense_clarification": "юридически не имеющий разрешения"
                            },
                            {
                                "target_lang_word": "запрещённый",
                                "target_lang_sense_clarification": "не разрешенный к использованию или посещению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a3478fb-6797-3d5c-8f22-bcdb780fa7c6",
                    "sense_definition": "Marked with stripes or bands; secured or covered with metal bars.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_bars",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prisoner stared out through the <barred> window of his small cell.",
                            "target_lang_translations": {
                                "ru": "Заключенный смотрел через <зарешёченное> окно своей маленькой камеры."
                            }
                        },
                        {
                            "text": "We were lucky to spot a <barred> owl resting on a high branch.",
                            "target_lang_translations": {
                                "ru": "Нам повезло заметить <пёструю> неясыть, отдыхающую на высокой ветке."
                            }
                        }
                    ],
                    "synonyms": [
                        "striped",
                        "banded",
                        "grated",
                        "reinforced"
                    ],
                    "common_phrases": [
                        "barred window",
                        "barred owl",
                        "barred spiral galaxy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий полосы или защищённый металлической решёткой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарешёченный",
                                "target_lang_sense_clarification": "закрытый решёткой (об окнах, дверях)"
                            },
                            {
                                "target_lang_word": "полосатый",
                                "target_lang_sense_clarification": "имеющий окрас в виде полос"
                            },
                            {
                                "target_lang_word": "пёстрый",
                                "target_lang_sense_clarification": "в названиях животных (например, пёстрая неясыть)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d149910e-477a-37bc-851d-a37b01a5b16c",
                    "sense_definition": "Relating to a crossword grid where words are separated by thick lines rather than black squares.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <barred> grid offers a different challenge compared to a standard blocked one.",
                            "target_lang_translations": {
                                "ru": "Сетка <с перегородками> предлагает иную задачу по сравнению со стандартной блочной."
                            }
                        },
                        {
                            "text": "She enjoys solving <barred> crosswords because they often have more complex clues.",
                            "target_lang_translations": {
                                "ru": "Ей нравится разгадывать кроссворды <с перегородками>, потому что в них часто более сложные подсказки."
                            }
                        }
                    ],
                    "antonyms": [
                        "blocked"
                    ],
                    "common_phrases": [
                        "barred grid",
                        "barred crossword"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к типу кроссвордов, где слова разделяются линиями, а не чёрными клетками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с перегородками",
                                "target_lang_sense_clarification": "о сетке кроссворда с разделительными линиями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f928edf8-446f-3354-b361-4cdb69372db8",
                    "sense_definition": "Intoxicated or high on the drug Xanax.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was so <barred> that he could barely stand up or speak clearly.",
                            "target_lang_translations": {
                                "ru": "Он был настолько <обдолбанным>, что едва мог стоять или говорить внятно."
                            }
                        },
                        {
                            "text": "They spent the entire night getting <barred> and listening to loud music.",
                            "target_lang_translations": {
                                "ru": "Они провели всю ночь, <накачиваясь> и слушая громкую музыку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в состоянии сильного наркотического опьянения (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обдолбанный",
                                "target_lang_sense_clarification": "грубый сленг: под воздействием веществ"
                            },
                            {
                                "target_lang_word": "под кайфом",
                                "target_lang_sense_clarification": "в состоянии опьянения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
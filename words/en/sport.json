                {
                    "sense_id": "8eb73a24-e463-32ec-affc-0860197095eb",
                    "sense_definition": "An activity involving physical exertion and skill in which an individual or team competes against another or others for entertainment.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Football is the most popular <w>sport</w> in the world.",
                            "target_lang_translations": {
                                "ru": "Футбол — самый популярный <w>вид спорта</w> в мире."
                            }
                        },
                        {
                            "text": "She enjoys playing a team <w>sport</w> like volleyball on weekends.",
                            "target_lang_translations": {
                                "ru": "Ей нравится заниматься командным <w>спортом</w>, таким как волейбол, по выходным."
                            }
                        }
                    ],
                    "synonyms": [
                        "physical activity"
                    ],
                    "common_phrases": [

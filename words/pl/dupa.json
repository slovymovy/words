    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3f8407fd-c068-39d8-9bfe-5290a3caecbf",
                    "sense_definition": "Tylna, miękka część ciała człowieka, na której się siedzi; pośladki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po całym dniu jazdy na rowerze strasznie boli mnie <dupa>.",
                            "target_lang_translations": {
                                "ru": "После целого дня езды на велосипеде у меня жутко болит <w>жопа</w>."
                            }
                        },
                        {
                            "text": "Nie chce mi się ruszyć <dupy> z kanapy, żeby zrobić zakupy.",
                            "target_lang_translations": {
                                "ru": "Мне лень поднять <w>задницу</w> с дивана, чтобы сходить за покупками."
                            }
                        }
                    ],
                    "synonyms": [
                        "tyłek",
                        "zadek",
                        "pupa",
                        "cztery litery",
                        "siedzenie"
                    ],
                    "common_phrases": [
                        "ruszyć dupę",
                        "siedzieć na dupie",
                        "kopnąć w dupę",
                        "trząść dupą",
                        "dobrać się do dupy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo pospolite, często uważane za wulgarne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Задняя, мягкая часть тела человека, на которой сидят; ягодицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жопа",
                                "target_lang_sense_clarification": "вульгарное"
                            },
                            {
                                "target_lang_word": "задница",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "пятая точка",
                                "target_lang_sense_clarification": "идиома, шутливое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29bfd07d-74f1-33a5-8f71-c17cfa10e268",
                    "sense_definition": "Osoba niezaradna, mało sprytna, która nie potrafi nic załatwić; oferma.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jaka ze mnie <dupa>, zapomniałem zabrać kluczyków do samochodu.",
                            "target_lang_translations": {
                                "ru": "Какой же я <w>растяпа</w>, забыл взять ключи от машины."
                            }
                        },
                        {
                            "text": "Nie bądź taką <dupą> wołową, spróbuj to naprawić samodzielnie.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>тюфяком</w>, попробуй починить это сам."
                            }
                        }
                    ],
                    "synonyms": [
                        "oferma",
                        "niedojda",
                        "gapa",
                        "ciamajda",
                        "fujara",
                        "sierota",
                        "niezdara"
                    ],
                    "common_phrases": [
                        "dupa wołowa",
                        "ciemna dupa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejoratywne, wulgarne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Беспомощный, неловкий человек, который не может ничего уладить; растяпа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растяпа",
                                "target_lang_sense_clarification": "о неуклюжем человеке"
                            },
                            {
                                "target_lang_word": "тюфяк",
                                "target_lang_sense_clarification": "о безвольном, медлительном человеке"
                            },
                            {
                                "target_lang_word": "недотёпа",
                                "target_lang_sense_clarification": "о неловком человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "174f3ad4-7a3d-319d-95c8-2840f91c848b",
                    "sense_definition": "Pospolite, uprzedmiotawiające określenie atrakcyjnej kobiety lub partnerki seksualnej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_female",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Koledzy z pracy komentowali, że nowa asystentka to niezła <dupa>.",
                            "target_lang_translations": {
                                "ru": "Коллеги по работе говорили, что новая ассистентка — неплохая <w>тёлка</w>."
                            }
                        },
                        {
                            "text": "Na imprezie pojawiła się niejedna fajna <dupa>, ale on nikogo nie zaczepił.",
                            "target_lang_translations": {
                                "ru": "На вечеринке была не одна классная <w>цыпочка</w>, но он ни к кому не подкатил."
                            }
                        }
                    ],
                    "synonyms": [
                        "laska",
                        "cizia",
                        "towar"
                    ],
                    "common_phrases": [
                        "niezła dupa",
                        "fajna dupa",
                        "stara dupa"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "obraźliwe, wulgarne, seksistowskie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное, опредмечивающее обозначение привлекательной женщины или сексуальной партнерши."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тёлка",
                                "target_lang_sense_clarification": "сленг, пренебрежительное"
                            },
                            {
                                "target_lang_word": "соска",
                                "target_lang_sense_clarification": "вульгарный сленг"
                            },
                            {
                                "target_lang_word": "цыпочка",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "b090b09f-beb7-307d-a440-20ae87823b3d",
                    "sense_definition": "Wyrażenie oznaczające nagłe niepowodzenie, rozczarowanie, rezygnację lub brak nadziei na sukces.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miało być piękne słońce na weekend, a pada deszcz i <dupa>.",
                            "target_lang_translations": {
                                "ru": "На выходные обещали солнце, а идет дождь — и <w>жопа</w>."
                            }
                        },
                        {
                            "text": "Uczyłem się do tego egzaminu przez tydzień, a i tak <dupa> zbita.",
                            "target_lang_translations": {
                                "ru": "Я готовился к этому экзамену неделю, а всё равно полная <w>жопа</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "no i dupa",
                        "dupa zbita",
                        "o do dupy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "wulgarne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение, означающее внезапную неудачу, разочарование или отсутствие надежды на успех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жопа",
                                "target_lang_sense_clarification": "плохая ситуация"
                            },
                            {
                                "target_lang_word": "облом",
                                "target_lang_sense_clarification": "разочарование, неудача"
                            },
                            {
                                "target_lang_word": "провал",
                                "target_lang_sense_clarification": "неудача"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "voorziening"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "89baf7c3-2452-3ba9-99c0-f5e31acb9c68",
                    "sense_definition": "Iemand of iets uitrusten met wat nodig of gewenst is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "provide",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kamer is van alle gemakken <voorzien>.",
                            "target_lang_translations": {
                                "ru": "Комната <оборудована> всеми удобствами.",
                                "en": "The room is <w>equipped</w> with all conveniences."
                            }
                        },
                        {
                            "text": "De school <voorziet> de leerlingen van nieuwe boeken.",
                            "target_lang_translations": {
                                "ru": "Школа <снабжает> учеников новыми учебниками.",
                                "en": "The school <w>provides</w> the students with new books."
                            }
                        }
                    ],
                    "synonyms": [
                        "vervullen",
                        "voorbereiden"
                    ],
                    "common_phrases": [
                        "voorzien van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжать или оснащать кого-либо или что-либо необходимым.",
                        "en": "To equip or supply someone or something with necessities."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снабжать",
                                "target_lang_sense_clarification": "предоставлять необходимые ресурсы или предметы"
                            },
                            {
                                "target_lang_word": "обеспечивать",
                                "target_lang_sense_clarification": "гарантировать наличие чего-либо"
                            },
                            {
                                "target_lang_word": "оборудовать",
                                "target_lang_sense_clarification": "оснащать техникой или удобствами"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "provide",
                                "target_lang_sense_clarification": "to give what is needed or desired"
                            },
                            {
                                "target_lang_word": "supply",
                                "target_lang_sense_clarification": "to furnish with something needed"
                            },
                            {
                                "target_lang_word": "equip",
                                "target_lang_sense_clarification": "to fit out with necessary items"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7428c35-7cf7-3691-8d35-72d315e5c1be",
                    "sense_definition": "Van tevoren zien aankomen of verwachten dat iets gaat gebeuren.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "foresee",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <voorzag> dat dit tot ongelukken zou leiden.",
                            "target_lang_translations": {
                                "ru": "Он <предвидел>, что это приведет к несчастным случаям.",
                                "en": "He <w>foresaw</w> that this would lead to accidents."
                            }
                        },
                        {
                            "text": "Niemand had deze problemen kunnen <voorzien>.",
                            "target_lang_translations": {
                                "ru": "Никто не мог <предвидеть> эти проблемы.",
                                "en": "No one could have <w>foreseen</w> these problems."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorspellen"
                    ],
                    "common_phrases": [
                        "te voorzien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предвидеть или ожидать наступление события заранее.",
                        "en": "To see or expect an event before it happens."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предвидеть",
                                "target_lang_sense_clarification": "заранее знать или догадываться о будущем"
                            },
                            {
                                "target_lang_word": "предусматривать",
                                "target_lang_sense_clarification": "учитывать возможность чего-либо заранее"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "foresee",
                                "target_lang_sense_clarification": "to perceive a situation in advance"
                            },
                            {
                                "target_lang_word": "predict",
                                "target_lang_sense_clarification": "to state that a specific event will occur"
                            },
                            {
                                "target_lang_word": "anticipate",
                                "target_lang_sense_clarification": "to expect or prepare for a future event"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ef372d6-2329-3f87-acee-03f38adfc759",
                    "sense_definition": "Zorgen voor wat nodig is of voldoen aan een behoefte.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "provide",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het eiland wil zelf <voorzien> in water en energie.",
                            "target_lang_translations": {
                                "ru": "Остров хочет самостоятельно <обеспечивать> себя водой и энергией.",
                                "en": "The island wants to <w>supply</w> its own water and energy."
                            }
                        },
                        {
                            "text": "Deze maatregel <voorziet> in een duidelijke behoefte.",
                            "target_lang_translations": {
                                "ru": "Эта мера <удовлетворяет> явную потребность.",
                                "en": "This measure <w>meets</w> a clear need."
                            }
                        }
                    ],
                    "synonyms": [
                        "vervullen"
                    ],
                    "common_phrases": [
                        "voorzien in",
                        "in een behoefte voorzien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удовлетворять потребность или обеспечивать наличие необходимого.",
                        "en": "To meet a need or ensure necessary resources are available."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удовлетворять",
                                "target_lang_sense_clarification": "отвечать на запрос или потребность"
                            },
                            {
                                "target_lang_word": "обеспечивать",
                                "target_lang_sense_clarification": "покрывать нужды"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "meet",
                                "target_lang_sense_clarification": "to satisfy a requirement or need"
                            },
                            {
                                "target_lang_word": "provide for",
                                "target_lang_sense_clarification": "to take measures to ensure needs are met"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
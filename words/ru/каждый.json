    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "970b7992-c703-3cfb-877d-6f3bd5cae753",
                    "sense_definition": "Один из всех в данной группе или ряду; любой из однородных предметов или лиц.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Каждый> день приносит нам новые знания.",
                            "target_lang_translations": {
                                "en": "<Each> day brings us new knowledge."
                            }
                        },
                        {
                            "text": "Мы проверяем <каждую> деталь перед отправкой.",
                            "target_lang_translations": {
                                "en": "We check <every> detail before shipping."
                            }
                        },
                        {
                            "text": "<Каждую> весну мы ездим к морю.",
                            "target_lang_translations": {
                                "en": "<Every> spring we go to the sea."
                            }
                        }
                    ],
                    "synonyms": [
                        "всякий",
                        "любой"
                    ],
                    "common_phrases": [
                        "каждый день",
                        "каждый раз",
                        "на каждом шагу",
                        "каждый год"
                    ],
                    "target_lang_definitions": {
                        "en": "One of a group considered individually; every single one in a series."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "every",
                                "target_lang_sense_clarification": "referring to all members of a group together"
                            },
                            {
                                "target_lang_word": "each",
                                "target_lang_sense_clarification": "referring to individual members of a group"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "41aa5792-8185-34ca-bd94-06ee4d60884f",
                    "sense_definition": "Любой человек; все люди в группе по отдельности.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "98420f2e-9b12-3b80-83b1-26ec2b893f49",
                    "sense_definition": "Naar boven klimmen of klauteren om een hoger punt te bereiken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wandelaars moesten een steile heuvel <w>opklimmen</w> om bij het uitzichtpunt te komen.",
                            "target_lang_translations": {
                                "en": "The hikers had to <w>climb up</w> a steep hill to reach the viewpoint.",
                                "ru": "Туристам пришлось <w>взобраться</w> на крутой холм, чтобы добраться до смотровой площадки."
                            }
                        },
                        {
                            "text": "Hij was voorzichtig de ladder <w>opgeklommen</w> om de dakgoot schoon te maken.",
                            "target_lang_translations": {
                                "en": "He had carefully <w>climbed up</w> the ladder to clean the gutter.",
                                "ru": "Он осторожно <w>поднялся</w> по лестнице, чтобы почистить водосточный желоб."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestijgen",
                        "beklimmen",
                        "klauteren"
                    ],
                    "antonyms": [
                        "afdalen"
                    ],
                    "common_phrases": [
                        "de ladder opklimmen",
                        "een berg opklimmen",
                        "uit het dal opklimmen"
                    ],
                    "target_lang_definitions": {
                        "en": "To climb or clamber upwards to reach a higher point.",
                        "ru": "Подниматься или карабкаться наверх, чтобы достичь более высокой точки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "climb up",
                                "target_lang_sense_clarification": "physical movement"
                            },
                            {
                                "target_lang_word": "ascend",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "взбираться",
                                "target_lang_sense_clarification": "с усилием, наверх"
                            },
                            {
                                "target_lang_word": "подниматься",
                                "target_lang_sense_clarification": "движение вверх"
                            },
                            {
                                "target_lang_word": "карабкаться",
                                "target_lang_sense_clarification": "лезть с трудом, используя руки и ноги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d39947a-fecf-3da3-94d2-317282f6af4a",
                    "sense_definition": "Een hogere maatschappelijke positie, rang of functie verwerven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij wist binnen het bedrijf snel <w>op te klimmen</w> tot manager.",
                            "target_lang_translations": {
                                "en": "She managed to quickly <w>work her way up</w> to manager within the company.",
                                "ru": "В компании ей удалось быстро <w>подняться</w> до должности менеджера."
                            }
                        },
                        {
                            "text": "Na jaren hard werken is hij <w>opgeklommen</w> in rang.",
                            "target_lang_translations": {
                                "en": "After years of hard work, he has <w>risen</w> in rank.",
                                "ru": "После многих лет упорной работы он <w>продвинулся</w> в звании."
                            }
                        }
                    ],
                    "synonyms": [
                        "promoveren",
                        "hogerop komen",
                        "carrière maken"
                    ],
                    "antonyms": [
                        "degraderen"
                    ],
                    "common_phrases": [
                        "opklimmen in rang",
                        "opklimmen tot directeur",
                        "maatschappelijk opklimmen"
                    ],
                    "target_lang_definitions": {
                        "en": "To acquire a higher social position, rank, or function.",
                        "ru": "Достигать более высокого социального положения, ранга или должности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rise",

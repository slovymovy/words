                            },
                            {
                                "target_lang_word": "спокойный",
                                "target_lang_sense_clarification": "не испытывающий волнения или тревоги"
                            },
                            {
                                "target_lang_word": "довольный",
                                "target_lang_sense_clarification": "удовлетворенный ситуацией (обычно в краткой форме: доволен)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "beef1f67-239c-3a17-a2d2-fc83b2ac87af",
                    "sense_definition": "Large enough to be satisfactory or to avoid worry; ample.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sufficiency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team won the game by a <w>comfortable</w> margin.",

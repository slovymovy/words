        "toe-eigenen"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "278a5868-a85a-3cbf-b0f4-12a58efb297a",
                    "sense_definition": "In werkelijkheid; zoals de situatie echt is, vaak in tegenstelling tot hoe het lijkt of wat men verwacht.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het lijkt op een auto, maar <w>eigenlijk</w> is het een fiets.",
                            "target_lang_translations": {
                                "en": "It looks like a car, but <w>actually</w> it is a bicycle.",
                                "ru": "Это похоже на машину, но <w>на самом деле</w> это велосипед."
                            }
                        },
                        {
                            "text": "Ik had <w>eigenlijk</w> nee moeten zeggen, maar ik durfde niet.",
                            "target_lang_translations": {
                                "en": "I should have <w>actually</w> said no, but I didn't dare.",
                                "ru": "Мне <w>на самом деле</w> следовало сказать нет, но я не решился."
                            }
                        },
                        {
                            "text": "Hij doet stoer, maar <w>eigenlijk</w> is hij heel onzeker.",
                            "target_lang_translations": {
                                "en": "He acts tough, but <w>in reality</w> he is very insecure.",
                                "ru": "Он строит из себя крутого, но <w>в действительности</w> он очень неуверен в себе."
                            }
                        }
                    ],
                    "synonyms": [
                        "feitelijk",
                        "werkelijk",
                        "in feite",
                        "heus"
                    ],
                    "antonyms": [
                        "schijnbaar",
                        "ogenschijnlijk",
                        "zogenaamd"
                    ],
                    "common_phrases": [
                        "eigenlijk niet",
                        "eigenlijk wel",
                        "maar eigenlijk"
                    ],
                    "target_lang_definitions": {
                        "en": "In reality; as the situation truly is, often contrary to how it appears or what is expected.",
                        "ru": "На самом деле; какова ситуация в реальности, часто в противовес тому, как она выглядит или чего ожидают."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "actually",
                                "target_lang_sense_clarification": "indicating the truth contrary to expectation"
                            },
                            {
                                "target_lang_word": "really",
                                "target_lang_sense_clarification": "emphasizing the true state of affairs"
                            },
                            {
                                "target_lang_word": "in fact",
                                "target_lang_sense_clarification": "formal expression of reality"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "на самом деле",
                                "target_lang_sense_clarification": "указывает на истинное положение вещей"
                            },
                            {
                                "target_lang_word": "в действительности",
                                "target_lang_sense_clarification": "более формальное выражение реальности"
                            },
                            {
                                "target_lang_word": "по сути",
                                "target_lang_sense_clarification": "касательно сущности ситуации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1dfc968e-4258-30ff-bb18-ea321c0454cb",
                    "sense_definition": "Bij nader inzien of om eerlijk te zijn; gebruikt om een mening te geven, een vraag te versterken of een onderwerp aan te snijden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "modal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is toch <w>eigenlijk</w> heel vreemd verlopen.",
                            "target_lang_translations": {
                                "en": "That went very strangely, <w>when you think about it</w>.",
                                "ru": "<w>Если подумать</w>, всё прошло очень странно."
                            }
                        },
                        {
                            "text": "Weet jij <w>eigenlijk</w> hoe laat de film begint?",
                            "target_lang_translations": {
                                "en": "Do you <w>actually</w> know what time the movie starts?",
                                "ru": "Ты, <w>кстати</w>, знаешь, во сколько начинается фильм?"
                            }
                        },
                        {
                            "text": "Wat vind je <w>eigenlijk</w> van mijn nieuwe kapsel?",
                            "target_lang_translations": {
                                "en": "What do you <w>actually</w> think of my new haircut?",
                                "ru": "Что ты <w>вообще</w> думаешь о моей новой прическе?"
                            }
                        }
                    ],
                    "synonyms": [
                        "welbeschouwd",
                        "trouwens",
                        "per slot van rekening"
                    ],
                    "common_phrases": [
                        "wat eigenlijk",
                        "wie eigenlijk",
                        "waarom eigenlijk"
                    ],
                    "target_lang_definitions": {
                        "en": "On second thought or to be honest; used to express an opinion, strengthen a question, or bring up a subject.",
                        "ru": "Если подумать или честно говоря; используется для выражения мнения, усиления вопроса или поднятия темы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "actually",
                                "target_lang_sense_clarification": "used to soften a statement or ask a question"
                            },
                            {
                                "target_lang_word": "by the way",
                                "target_lang_sense_clarification": "used when introducing a new topic or question"
                            },
                            {
                                "target_lang_word": "come to think of it",
                                "target_lang_sense_clarification": "upon reflection"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вообще-то",
                                "target_lang_sense_clarification": "используется для смягчения утверждения или возражения"
                            },
                            {
                                "target_lang_word": "кстати",
                                "target_lang_sense_clarification": "используется при введении новой темы или вопроса"
                            },
                            {
                                "target_lang_word": "собственно",
                                "target_lang_sense_clarification": "вводное слово, часто используется в вопросах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5c5255ab-c791-3188-b62d-bd271756d2fb",
                    "sense_definition": "Echt; behorend tot de werkelijkheid of de kern van de zaak, niet slechts in naam of schijn.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reality_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit historisch feit wordt gezien als het <w>eigenlijke</w> begin van de opstand.",
                            "target_lang_translations": {
                                "en": "This historical fact is seen as the <w>actual</w> beginning of the revolt.",
                                "ru": "Этот исторический факт рассматривается как <w>фактическое</w> начало восстания."
                            }
                        },
                        {
                            "text": "Wat is nu je <w>eigenlijke</w> bedoeling met dit plan?",
                            "target_lang_translations": {
                                "en": "What is your <w>real</w> intention with this plan?",
                                "ru": "Какова твоя <w>истинная</w> цель этого плана?"
                            }
                        },
                        {
                            "text": "De <w>eigenlijke</w> oorzaak van het probleem is nog steeds onbekend.",
                            "target_lang_translations": {
                                "en": "The <w>actual</w> cause of the problem is still unknown.",
                                "ru": "<w>Истинная</w> причина проблемы до сих пор неизвестна."
                            }
                        }
                    ],
                    "synonyms": [
                        "werkelijk",
                        "echt",
                        "feitelijk",
                        "oorspronkelijk"
                    ],
                    "antonyms": [
                        "oneigenlijk",
                        "vermeend"
                    ],
                    "common_phrases": [
                        "het eigenlijke doel",
                        "de eigenlijke reden"
                    ],
                    "target_lang_definitions": {
                        "en": "Real; belonging to reality or the essence of the matter, not merely in name or appearance.",
                        "ru": "Настоящий; относящийся к реальности или сути дела, а не только по названию или видимости."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "actual",
                                "target_lang_sense_clarification": "existing in fact rather than potential"
                            },
                            {
                                "target_lang_word": "real",
                                "target_lang_sense_clarification": "genuine or essential"
                            },
                            {
                                "target_lang_word": "true",
                                "target_lang_sense_clarification": "relating to the core meaning"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "истинный",
                                "target_lang_sense_clarification": "подлинный, соответствующий сути"
                            },
                            {
                                "target_lang_word": "фактический",
                                "target_lang_sense_clarification": "существующий по факту"
                            },
                            {
                                "target_lang_word": "настоящий",
                                "target_lang_sense_clarification": "реальный, не кажущийся"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "277d4be7-f23e-31ef-977f-1d2e387fd318",
                    "sense_definition": "Процесс превращения сырья, материалов или отходов в новую продукцию.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем городе построили современный завод по <w>переработке</w> мусора.",
                            "target_lang_translations": {
                                "en": "A modern waste <w>recycling</w> plant was built in our city."
                            }
                        },
                        {
                            "text": "Глубокая <w>переработка</w> нефти позволяет получать высококачественное топливо.",
                            "target_lang_translations": {
                                "en": "Deep oil <w>processing</w> allows for the production of high-quality fuel."
                            }
                        }
                    ],
                    "synonyms": [
                        "перерабатывание"
                    ],
                    "common_phrases": [
                        "вторичная переработка",
                        "переработка отходов",
                        "переработка сырья",
                        "завод по переработке"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of converting raw materials, substances, or waste into new products."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recycling",
                                "target_lang_sense_clarification": "converting waste into reusable material"
                            },
                            {
                                "target_lang_word": "processing",
                                "target_lang_sense_clarification": "industrial treatment of raw materials"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a406c33a-4632-359e-848f-0f139b61a2ca",
                    "sense_definition": "Работа, выполняемая сотрудником сверх установленного рабочего времени, или время, затраченное на такую работу.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания обязана оплачивать любую <w>переработку</w> в двойном размере.",
                            "target_lang_translations": {
                                "en": "The company is required to pay for any <w>overtime</w> at double the rate."
                            }
                        },
                        {
                            "text": "Из-за срочного проекта у всей команды была большая <w>переработка</w> в этом месяце.",
                            "target_lang_translations": {
                                "en": "Due to an urgent project, the whole team had a lot of <w>overtime</w> this month."
                            }
                        }
                    ],
                    "synonyms": [
                        "кранч"
                    ],
                    "common_phrases": [
                        "оплата переработки",
                        "часы переработки",
                        "постоянные переработки"
                    ],
                    "target_lang_definitions": {
                        "en": "Work performed by an employee beyond the established working hours, or the time spent on such work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overtime",
                                "target_lang_sense_clarification": "extra hours worked"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "910a51b5-474a-3453-9252-9d464d023a30",
                    "sense_definition": "Изменение, исправление или переделка текста, произведения или проекта для создания нового варианта.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта пьеса представляет собой творческую <w>переработку</w> известного романа.",
                            "target_lang_translations": {
                                "en": "This play is a creative <w>reworking</w> of a famous novel."
                            }
                        },
                        {
                            "text": "После серьезной <w>переработки</w> статья была принята к публикации в журнале.",
                            "target_lang_translations": {
                                "en": "After serious <w>revision</w>, the article was accepted for publication in the journal."
                            }
                        }
                    ],
                    "synonyms": [
                        "перерабатывание"
                    ],
                    "common_phrases": [
                        "литературная переработка",
                        "творческая переработка",
                        "переработка сценария"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of altering, correcting, or remaking a text or creative work to create a new version."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "revision",
                                "target_lang_sense_clarification": "editing or correcting text"
                            },
                            {
                                "target_lang_word": "reworking",
                                "target_lang_sense_clarification": "substantial adaptation or remaking"
                            }
                        ]
                    }
                }
            ]
        }
    ]

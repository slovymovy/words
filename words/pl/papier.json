    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b89c0d14-9351-3606-bca6-4b649b35440f",
                    "sense_definition": "Cienki materiał wytwarzany z włókien roślinnych, służący do pisania, drukowania lub pakowania.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko narysowało piękny dom na czystym <papierze>.",
                            "target_lang_translations": {
                                "ru": "Ребенок нарисовал красивый дом на чистой <бумаге>."
                            }
                        },
                        {
                            "text": "Muszę kupić nowy <papier> do drukarki, bo stary się skończył.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую <бумагу> для принтера, так как старая закончилась."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Śląsk Cieszyński i Górny Śląsk) papiór"
                    ],
                    "common_phrases": [
                        "kartka papieru",
                        "papier toaletowy",
                        "papier ścierny",
                        "papier pakowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий материал из растительных волокон, используемый для письма, печати или упаковки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бумага",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa0eb080-bc09-3e1d-a968-4e143a92600f",
                    "sense_definition": "Symbol otwartej dłoni pokazywany w grze towarzyskiej, który wygrywa z kamieniem, ale przegrywa z nożycami.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zagrajmy w kamień, <papier>, nożyce, żeby zdecydować, kto zaczyna.",
                            "target_lang_translations": {
                                "ru": "Давай сыграем в камень, ножницы, <бумагу>, чтобы решить, кто начинает."
                            }
                        },
                        {
                            "text": "Zawsze wybieram <papier>, a on ciągle pokazuje nożyce.",
                            "target_lang_translations": {
                                "ru": "Я всегда выбираю <бумагу>, а он постоянно показывает ножницы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kamień papier nożyce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Символ открытой ладони в игре, который побеждает камень, но проигрывает ножницам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бумага",
                                "target_lang_sense_clarification": "жест в игре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2581f9c1-9375-3f39-a637-631531da58e3",
                    "sense_definition": "Oficjalny dokument, zaświadczenie lub dowód tożsamości; często używane w liczbie mnogiej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz złożyć wszystkie <papiery> w urzędzie przed końcem miesiąca.",
                            "target_lang_translations": {
                                "ru": "Ты должен подать все <бумаги> в учреждение до конца месяца."
                            }
                        },
                        {
                            "text": "On ma <papier> na to, że jest wykwalifikowanym elektrykiem.",
                            "target_lang_translations": {
                                "ru": "У него есть <бумага> на то, что он квалифицированный электрик."
                            }
                        }
                    ],
                    "synonyms": [
                        "dokument"
                    ],
                    "common_phrases": [
                        "składać papiery",
                        "papiery rozwodowe",
                        "papiery wartościowe",
                        "mieć papiery"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used instead of 'dokument'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный документ, справка или удостоверение личности; часто используется во множественном числе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бумаги",
                                "target_lang_sense_clarification": "документы (разг.)"
                            },
                            {
                                "target_lang_word": "документ",
                                "target_lang_sense_clarification": "официальная бумага"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce9b9798-8feb-3f59-a758-e02dc98562b1",
                    "sense_definition": "Potoczne określenie na pieniądze papierowe, banknoty lub dolary amerykańskie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej transakcji liczy się tylko żywy <papier>, a nie przelewy.",
                            "target_lang_translations": {
                                "ru": "В этой сделке важны только живые <деньги>, а не переводы."
                            }
                        },
                        {
                            "text": "Złodzieje uciekli z walizką pełną <papieru>.",
                            "target_lang_translations": {
                                "ru": "Воры сбежали с чемоданом, полным <денег>."
                            }
                        }
                    ],
                    "synonyms": [
                        "baks",
                        "dolar",
                        "dolec",
                        "zielony",
                        "bankot"
                    ],
                    "common_phrases": [
                        "kupa papieru",
                        "fałszywy papier"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название бумажных денег, банкнот или долларов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "банкноты",
                                "target_lang_sense_clarification": "деньги"
                            },
                            {
                                "target_lang_word": "наличные",
                                "target_lang_sense_clarification": "деньги (разг.)"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "toczyć",
        "przetoczyć",
        "wytoczyć",
        "zatoczyć",
        "stoczyć",
        "roztoczyć",
        "potoczyć",
        "otoczyć",
        "przetoczenie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "27e1a88d-8294-319a-9343-d46d9a1505a5",
                    "sense_definition": "Przesuwać ciężki przedmiot o obłym kształcie lub obiekt na kołach, wprawiając go w ruch obrotowy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy musieli ręcznie <przetaczać> wielkie beczki do magazynu.",
                            "target_lang_translations": {
                                "ru": "Рабочим пришлось вручную <перекатывать> огромные бочки на склад."
                            }
                        },
                        {
                            "text": "Pomogłem sąsiadowi <przetaczać> ciężkie kłody drzewa na przyczepę.",
                            "target_lang_translations": {
                                "ru": "Я помог соседу <перекатывать> тяжелые бревна на прицеп."
                            }
                        }
                    ],
                    "synonyms": [
                        "turlać"
                    ],
                    "common_phrases": [
                        "przetaczać beczki",
                        "przetaczać kłody",
                        "przetaczać wagony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать тяжелый круглый предмет или объект на колесах, заставляя его вращаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перекатывать",
                                "target_lang_sense_clarification": "тяжелые предметы, бочки"
                            },
                            {
                                "target_lang_word": "катить",
                                "target_lang_sense_clarification": "перемещать вращением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a30efd8-b454-33f8-b275-53fe54949a36",
                    "sense_definition": "Wprowadzać krew, osocze lub inne płyny infuzyjne bezpośrednio do układu krwionośnego pacjenta.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_procedure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarze zdecydowali się natychmiast <przetaczać> krew poszkodowanemu w wypadku.",
                            "target_lang_translations": {
                                "ru": "Врачи решили немедленно <переливать> кровь пострадавшему в аварии."
                            }
                        },
                        {
                            "text": "Pielęgniarka zaczęła powoli <przetaczać> pacjentowi niezbędne leki.",
                            "target_lang_translations": {
                                "ru": "Медсестра начала медленно <вводить> пациенту необходимые лекарства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przetaczać krew",
                        "przetaczać osocze",
                        "przetaczać płyny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вводить кровь, плазму или другие жидкости непосредственно в кровеносную систему пациента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переливать",
                                "target_lang_sense_clarification": "кровь, плазму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56cd1adb-130a-3729-9258-00b803a2ff8e",
                    "sense_definition": "Przemieszczać się gwałtownie lub masowo przez dany obszar (używane w kontekście zjawisk atmosferycznych, wojen lub wydarzeń historycznych).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad miastem lada chwila będzie <przetaczać> się potężna nawałnica.",
                            "target_lang_translations": {
                                "ru": "Над городом с минуты на минуту будет <проноситься> мощная буря."
                            }
                        },
                        {
                            "text": "Przez kraj zaczęła <przetaczać> się fala gwałtownych protestów.",
                            "target_lang_translations": {
                                "ru": "По стране начала <прокатываться> волна ожесточенных протестов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przetaczać się przez kraj",
                        "burza się przetacza",
                        "front się przetacza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стремительно или массово проходить через определенную территорию (о штормах, войнах или исторических событиях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокатываться",
                                "target_lang_sense_clarification": "о буре, волне протестов"
                            },
                            {
                                "target_lang_word": "проноситься",
                                "target_lang_sense_clarification": "о шторме, урагане"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30869809-ddab-3931-8b3b-d3fc63b4c9e7",
                    "sense_definition": "Przelewać ciecz z jednego naczynia do drugiego, często przy użyciu węża, rurki lub pompy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "liquid_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Winiarz pokazał mi, jak ostrożnie <przetaczać> wino, aby go nie zmącić.",
                            "target_lang_translations": {
                                "ru": "Винодел показал мне, как осторожно <переливать> вино, чтобы не взболтать его."
                            }
                        },
                        {
                            "text": "Musimy <przetaczać> paliwo z kanistra do baku samochodu.",
                            "target_lang_translations": {
                                "ru": "Мы должны <переливать> топливо из канистры в бак автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "przelewać",
                        "przepompowywać"
                    ],
                    "common_phrases": [
                        "przetaczać wino",
                        "przetaczać paliwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать жидкость из одного сосуда в другой, часто с помощью шланга, трубки или насоса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переливать",
                                "target_lang_sense_clarification": "жидкость, топливо"
                            },
                            {
                                "target_lang_word": "сцеживать",
                                "target_lang_sense_clarification": "вино, осадок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "987651c9-3234-385a-a0c0-f3bd0fd17814",
                    "sense_definition": "Poruszać się w określonym kierunku, wykonując obroty wokół własnej osi (o przedmiotach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Piłka zaczęła powoli <przetaczać> się po podłodze w stronę wyjścia.",
                            "target_lang_translations": {
                                "ru": "Мяч начал медленно <катиться> по полу в сторону выхода."
                            }
                        },
                        {
                            "text": "Widzieliśmy, jak wielki głaz zaczął <przetaczać> się w dół zbocza.",
                            "target_lang_translations": {
                                "ru": "Мы видели, как огромный валун начал <катиться> вниз по склону."
                            }
                        }
                    ],
                    "synonyms": [
                        "turlać się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться в определенном направлении, вращаясь вокруг собственной оси (о предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "катиться",
                                "target_lang_sense_clarification": "двигаться вращаясь"
                            },
                            {
                                "target_lang_word": "перекатываться",
                                "target_lang_sense_clarification": "с места на место"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
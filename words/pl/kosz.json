            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb9ff96e-743a-3487-bb18-0a8e53a44a54",
                    "sense_definition": "Pojemnik pleciony z wikliny, słomy, drutu lub tworzywa, służący do przenoszenia lub przechowywania rzeczy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na stole w kuchni stał duży wiklinowy <w>kosz</w> z jabłkami.",
                            "target_lang_translations": {
                                "ru": "На кухонном столе стояла большая плетёная <w>корзина</w> с яблоками."
                            }
                        },
                        {
                            "text": "Babcia wzięła swój ulubiony <w>kosz</w> i poszła do lasu na grzyby.",
                            "target_lang_translations": {
                                "ru": "Бабушка взяла свою любимую <w>корзину</w> и пошла в лес за грибами."
                            }
                        }
                    ],
                    "synonyms": [
                        "plecionka"
                    ],
                    "common_phrases": [
                        "kosz wiklinowy",
                        "kosz z owocami",
                        "pleść kosze",
                        "dostać kosza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плетёная ёмкость из лозы, соломы или проволоки для переноски или хранения вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корзина",
                                "target_lang_sense_clarification": "плетёное изделие"
                            },
                            {
                                "target_lang_word": "лукошко",
                                "target_lang_sense_clarification": "небольшая ручная корзина, обычно для ягод или грибов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a20cc42-dc9b-3c14-b3b5-ae595c1ce29d",
                    "sense_definition": "Pojemnik przeznaczony do gromadzenia odpadów i śmieci.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę wyrzucić te brudne papiery prosto do <w>kosza</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, выбросьте эти грязные бумаги прямо в <w>мусорную корзину</w>."
                            }
                        },
                        {
                            "text": "Nasz domowy <w>kosz</w> na śmieci jest już pełny i trzeba go opróżnić.",
                            "target_lang_translations": {
                                "ru": "Наше домашнее <w>мусорное ведро</w> уже полное, и его нужно вынести."
                            }
                        }
                    ],
                    "synonyms": [
                        "śmietnik",
                        "kosz na śmieci / kosz na śmiecie",
                        "kubeł (na śmieci[e])"
                    ],
                    "common_phrases": [
                        "kosz na śmieci",
                        "wyrzucić do kosza",
                        "opróżnić kosz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ёмкость, предназначенная для сбора отходов и мусора."

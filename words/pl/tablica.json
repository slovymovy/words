{
    "word_family": [
        "tabliczka",
        "tablicowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5f7dfb56-2152-3874-83fd-4a150193c8b0",
                    "sense_definition": "Płaska powierzchnia, zwykle ciemna lub biała, służąca do pisania kredą lub specjalnymi pisakami, używana głównie w szkołach i salach wykładowych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "writing_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel napisał temat dzisiejszej lekcji na <w>tablicy</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель написал тему сегодняшнего урока на <w>доске</w>."
                            }
                        },
                        {
                            "text": "Uczeń podszedł do <w>tablicy</w>, aby rozwiązać zadanie matematyczne.",
                            "target_lang_translations": {
                                "ru": "Ученик подошел к <w>доске</w>, чтобы решить математическую задачу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pisać na tablicy",
                        "zetrzeć tablicę",
                        "wywołać do tablicy",
                        "tablica interaktywna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская поверхность, обычно темная или белая, предназначенная для письма мелом или маркерами, используемая в учебных заведениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доска",
                                "target_lang_sense_clarification": "школьная/для письма"
                            },
                            {
                                "target_lang_word": "классная доска",
                                "target_lang_sense_clarification": "в классе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b90fb59c-e704-3789-8a2f-3dd81ec7bff3",
                    "sense_definition": "Miejsce, zwykle w formie płyty zawieszonej na ścianie, przeznaczone do wywieszania ogłoszeń, informacji lub plakatów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "notice_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprawdź harmonogram zajęć na <w>tablicy</w> ogłoszeń w korytarzu.",
                            "target_lang_translations": {
                                "ru": "Проверь расписание занятий на <w>доске</w> объявлений в коридоре."
                            }
                        },
                        {
                            "text": "Dyrektor wywiesił na <w>tablicy</w> ważną informację dla pracowników.",
                            "target_lang_translations": {
                                "ru": "Директор вывесил на <w>доске</w> важную информацию для сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "tablica ogłoszeń",
                        "gazetka"
                    ],
                    "common_phrases": [
                        "tablica ogłoszeń",
                        "wywiesić na tablicy",
                        "tablica informacyjna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специальное место или панель на стене для размещения объявлений, информации или плакатов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доска объявлений",
                                "target_lang_sense_clarification": "информационная"
                            },
                            {
                                "target_lang_word": "стенд",
                                "target_lang_sense_clarification": "для информации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d93ddf62-f895-39ad-879c-09a26db2e54b",
                    "sense_definition": "Zestawienie danych, liczb, wzorów lub informacji uporządkowanych w rzędach i kolumnach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "data_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na egzaminie z chemii można korzystać z <w>tablicy</w> Mendelejewa.",
                            "target_lang_translations": {
                                "ru": "На экзамене по химии можно пользоваться <w>таблицей</w> Менделеева."
                            }
                        },
                        {
                            "text": "Ta <w>tablica</w> zawiera wyniki pomiarów temperatury z całego miesiąca.",
                            "target_lang_translations": {
                                "ru": "Эта <w>таблица</w> содержит результаты измерений температуры за весь месяц."
                            }
                        }
                    ],
                    "synonyms": [
                        "tabela"
                    ],
                    "common_phrases": [
                        "tablica Mendelejewa",
                        "tablice matematyczne",
                        "tablica wyników"
                    ],
                    "target_lang_definitions": {
                        "ru": "Систематизированный свод данных, чисел или формул, расположенных по строкам и столбцам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таблица",
                                "target_lang_sense_clarification": "данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71b2c468-59e1-368d-adff-977c03007c50",
                    "sense_definition": "Płyta z trwałego materiału z napisem, często pamiątkowa lub informacyjna, umieszczona na budynku lub pomniku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "plaque",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na ścianie ratusza wisi pamiątkowa <w>tablica</w> poświęcona bohaterom wojennym.",
                            "target_lang_translations": {
                                "ru": "На стене ратуши висит мемориальная <w>доска</w>, посвященная героям войны."
                            }
                        },
                        {
                            "text": "Prezydent miasta uroczyście odsłonił <w>tablicę</w> podczas rocznicy powstania.",
                            "target_lang_translations": {
                                "ru": "Мэр города торжественно открыл <w>доску</w> во время годовщины восстания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tablica pamiątkowa",
                        "odsłonić tablicę",
                        "wmurować tablicę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плита из прочного материала с надписью, установленная на здании или памятнике в память о ком-либо или для информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мемориальная доска",
                                "target_lang_sense_clarification": "памятная"
                            },
                            {
                                "target_lang_word": "табличка",
                                "target_lang_sense_clarification": "на здании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89b001e7-36ba-3a7d-940c-308d095d79c6",
                    "sense_definition": "Struktura danych w informatyce przechowująca uporządkowany zbiór elementów tego samego typu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "data_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Programista zadeklarował <w>tablicę</w>, która przechowuje imiona użytkowników.",
                            "target_lang_translations": {
                                "ru": "Программист объявил <w>массив</w>, который хранит имена пользователей."
                            }
                        },
                        {
                            "text": "Dostęp do elementów <w>tablicy</w> odbywa się za pomocą indeksu liczbowego.",
                            "target_lang_translations": {
                                "ru": "Доступ к элементам <w>массива</w> осуществляется с помощью числового индекса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tablica jednowymiarowa",
                        "indeks tablicy",
                        "element tablicy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "informatyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Структура данных в программировании, содержащая упорядоченный набор элементов одного типа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "массив",
                                "target_lang_sense_clarification": "информатика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a2c1ba5-d9c9-3e66-ae98-c35817d5bf71",
                    "sense_definition": "Panel służący do łączenia obwodów elektrycznych i umieszczania elementów sterowniczych lub zabezpieczających.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_panel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Elektryk otworzył <w>tablicę</w> rozdzielczą, aby wymienić przepalony bezpiecznik.",
                            "target_lang_translations": {
                                "ru": "Электрик открыл распределительный <w>щит</w>, чтобы заменить перегоревший предохранитель."
                            }
                        },
                        {
                            "text": "Główna <w>tablica</w> licznikowa znajduje się w piwnicy bloku.",
                            "target_lang_translations": {
                                "ru": "Главный <w>щит</w> со счетчиками находится в подвале дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozdzielnica",
                        "panel"
                    ],
                    "common_phrases": [
                        "tablica rozdzielcza",
                        "tablica licznikowa",
                        "tablica sterownicza"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "elektryka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Панель для монтажа электрооборудования, соединения цепей и размещения приборов управления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распределительный щит",
                                "target_lang_sense_clarification": "электрика"
                            },
                            {
                                "target_lang_word": "щиток",
                                "target_lang_sense_clarification": "электрический"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
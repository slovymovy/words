    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a4b82707-f5f6-37e9-ac9d-1fc80e1e33d0",
                    "sense_definition": "De toestand waarin er geen oorlog of gewapend conflict is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "absence_of_war",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De twee landen leven al jaren in <w>vrede</w> met elkaar.",
                            "target_lang_translations": {
                                "en": "The two countries have lived in <w>peace</w> with each other for years.",
                                "ru": "Две страны уже много лет живут в <w>мире</w> друг с другом."
                            }
                        },
                        {
                            "text": "Het is voor de regering moeilijk om de <w>vrede</w> te bewaren.",
                            "target_lang_translations": {
                                "en": "It is difficult for the government to keep the <w>peace</w>.",
                                "ru": "Правительству трудно сохранять <w>мир</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "oorlog",
                        "conflict"
                    ],
                    "common_phrases": [
                        "vrede sluiten",
                        "in vrede leven",
                        "vrede op aarde"
                    ],
                    "target_lang_definitions": {
                        "en": "The state in which there is no war or armed conflict.",
                        "ru": "Состояние, при котором нет войны или вооруженного конфликта."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "peace",
                                "target_lang_sense_clarification": "absence of war"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мир",
                                "target_lang_sense_clarification": "отсутствие войны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb66d3b4-dbe9-35a2-a9cd-c2d3a92abd39",
                    "sense_definition": "Een toestand van innerlijke rust, kalmte of harmonie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tranquility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de stilte van het bos vond hij eindelijk <w>vrede</w>.",
                            "target_lang_translations": {
                                "en": "In the silence of the forest, he finally found <w>peace</w>.",
                                "ru": "В тишине леса он наконец обрел <w>покой</w>."
                            }
                        },
                        {
                            "text": "Ik wil gewoon dat je me met <w>vrede</w> laat.",
                            "target_lang_translations": {
                                "en": "I just want you to leave me in <w>peace</w>.",
                                "ru": "Я просто хочу, чтобы ты оставил меня в <w>покое</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "onvrede"
                    ],
                    "common_phrases": [
                        "rust en vrede",
                        "met vrede laten"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of inner rest, calmness, or harmony.",
                        "ru": "Состояние внутреннего спокойствия, умиротворения или гармонии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "peace",
                                "target_lang_sense_clarification": "inner calm"
                            },
                            {
                                "target_lang_word": "tranquility",
                                "target_lang_sense_clarification": "state of calm"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "покой",
                                "target_lang_sense_clarification": "спокойствие, отсутствие тревоги"
                            },
                            {
                                "target_lang_word": "мир",
                                "target_lang_sense_clarification": "душевное равновесие"
                            },
                            {
                                "target_lang_word": "умиротворение",
                                "target_lang_sense_clarification": "глубокое спокойствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61982b4a-84fb-30f7-964d-a2444f93f90e",
                    "sense_definition": "Een officieel verdrag dat een einde maakt aan een oorlog.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "treaty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>vrede</w> werd getekend in een neutraal land.",
                            "target_lang_translations": {
                                "en": "The <w>peace</w> was signed in a neutral country.",
                                "ru": "<w>Мир</w> был подписан в нейтральной стране."
                            }
                        },
                        {
                            "text": "Deze <w>vrede</w> zorgde voor nieuwe grenzen in Europa.",
                            "target_lang_translations": {
                                "en": "This <w>peace</w> created new borders in Europe.",
                                "ru": "Этот <w>мир</w> установил новые границы в Европе."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An official treaty that ends a war.",
                        "ru": "Официальный договор, который кладет конец войне."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "peace treaty",

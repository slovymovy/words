{
    "word_family": [
        "onderscheiden",
        "onderscheid",
        "onderscheidend"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "722ebd49-a390-3415-a4d5-96714c16e245",
                    "sense_definition": "Een officieel ereteken, zoals een medaille of lintje, dat iemand ontvangt als beloning voor een bijzondere prestatie of verdienste.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kreeg een <w>onderscheiding</w> voor zijn jarenlange trouwe dienst.",
                            "target_lang_translations": {
                                "en": "He received an <w>award</w> for his years of loyal service.",
                                "ru": "Он получил <w>награду</w> за долгие годы верной службы."
                            }
                        },
                        {
                            "text": "De burgemeester reikte een koninklijke <w>onderscheiding</w> uit aan de vrijwilliger.",
                            "target_lang_translations": {
                                "en": "The mayor presented a royal <w>decoration</w> to the volunteer.",
                                "ru": "Мэр вручил волонтеру королевскую <w>награду</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "decoratie"
                    ],
                    "common_phrases": [
                        "koninklijke onderscheiding",
                        "militaire onderscheiding",
                        "hoge onderscheiding",
                        "onderscheiding ontvangen"
                    ],
                    "target_lang_definitions": {
                        "en": "An official mark of honor, such as a medal or ribbon, received as a reward for a special achievement or merit.",
                        "ru": "Официальный знак почета, такой как медаль или лента, получаемый в награду за особые достижения или заслуги."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decoration",
                                "target_lang_sense_clarification": "medal or badge of honor"
                            },
                            {
                                "target_lang_word": "award",
                                "target_lang_sense_clarification": "prize or mark of recognition"
                            },
                            {
                                "target_lang_word": "distinction",
                                "target_lang_sense_clarification": "honor or recognition"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "награда",
                                "target_lang_sense_clarification": "приз или знак признания"
                            },
                            {
                                "target_lang_word": "знак отличия",
                                "target_lang_sense_clarification": "медаль или орден"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a70d665-b8f9-3b10-8681-c6341492e206",
                    "sense_definition": "De handeling van het maken van onderscheid of het vaststellen van een verschil tussen zaken of personen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ter <w>onderscheiding</w> van de andere modellen heeft deze versie een rood label.",
                            "target_lang_translations": {
                                "en": "To <w>distinguish</w> it from the other models, this version has a red label.",
                                "ru": "Для <w>отличия</w> от других моделей у этой версии есть красная этикетка."
                            }
                        },
                        {
                            "text": "De <w>onderscheiding</w> tussen deze twee begrippen is niet altijd even duidelijk.",
                            "target_lang_translations": {
                                "en": "The <w>distinction</w> between these two concepts is not always clear.",
                                "ru": "<w>Различие</w> между этими двумя понятиями не всегда очевидно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ter onderscheiding van",
                        "scherpe onderscheiding"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of making a distinction or establishing a difference between things or persons.",
                        "ru": "Действие по установлению различия или разграничению между предметами или людьми."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "distinction",
                                "target_lang_sense_clarification": "difference or contrast"
                            },
                            {
                                "target_lang_word": "differentiation",
                                "target_lang_sense_clarification": "act of distinguishing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "различие",
                                "target_lang_sense_clarification": "разница или контраст"
                            },
                            {
                                "target_lang_word": "отличие",
                                "target_lang_sense_clarification": "признак, создающий разницу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55ca8e5c-8b46-3bf8-a63a-3eba5aa22ba2",
                    "sense_definition": "Het vermogen om fijnzinnige verschillen waar te nemen en te beoordelen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij behandelde de gevoelige kwestie met veel tact en <w>onderscheiding</w>.",
                            "target_lang_translations": {
                                "en": "She handled the sensitive matter with much tact and <w>distinction</w>.",
                                "ru": "Она подошла к этому деликатному вопросу с большим тактом и <w>утонченностью</w>."
                            }
                        },
                        {
                            "text": "Voor deze taak is een grote mate van geestelijke <w>onderscheiding</w> vereist.",
                            "target_lang_translations": {
                                "en": "For this task, a high degree of spiritual <w>discernment</w> is required.",
                                "ru": "Для этой задачи требуется высокая степень духовной <w>проницательности</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "met onderscheiding",
                        "geest van onderscheiding"
                    ],
                    "target_lang_definitions": {
                        "en": "The ability to perceive and judge subtle differences; refinement or discernment.",
                        "ru": "Способность воспринимать и оценивать тонкие различия; утонченность или проницательность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "discernment",
                                "target_lang_sense_clarification": "ability to judge well"
                            },
                            {
                                "target_lang_word": "distinction",
                                "target_lang_sense_clarification": "excellence or refinement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "проницательность",
                                "target_lang_sense_clarification": "способность здраво судить"
                            },
                            {
                                "target_lang_word": "утонченность",
                                "target_lang_sense_clarification": "изысканность или благородство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жить",
                                "target_lang_sense_clarification": "быть живым, существовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d891eda-1b27-3b55-b749-339763999de4",
                    "sense_definition": "Mieszkać lub przebywać na stałe w określonym miejscu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niedźwiedzie polarne to zwierzęta, które <żyją> w zimnym klimacie.",
                            "target_lang_translations": {
                                "ru": "Белые медведи — это животные, которые <живут> в холодном климате."
                            }
                        },
                        {
                            "text": "Moi rodzice od wielu lat <żyją> w małym domku na wsi.",
                            "target_lang_translations": {
                                "ru": "Мои родители уже много лет <живут> в маленьком домике в деревне."
                            }

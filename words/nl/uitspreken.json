    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "38d8330d-2876-31e5-9297-c123ee8827e3",
                    "sense_definition": "Klanken vormen om een woord of zin hoorbaar te maken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pronunciation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je dit moeilijke woord voor mij <w>uitspreken</w>?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>произнести</w> это сложное слово для меня?",
                                "en": "Can you <w>pronounce</w> this difficult word for me?"
                            }
                        },
                        {
                            "text": "In het Nederlands wordt de letter g vaak hard <w>uitgesproken</w>.",
                            "target_lang_translations": {
                                "ru": "В нидерландском языке буква g часто <w>произносится</w> твердо.",
                                "en": "In Dutch, the letter g is often <w>pronounced</w> hard."
                            }
                        }
                    ],
                    "synonyms": [
                        "declareren",
                        "verklaren",
                        "verkondigen",
                        "declareren",
                        "verklaren"
                    ],
                    "common_phrases": [
                        "duidelijk uitspreken",
                        "verkeerd uitspreken",
                        "hardop uitspreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить звуки речи, чтобы сделать слово или фразу слышимыми.",
                        "en": "To produce speech sounds to make a word or phrase audible."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "произносить",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "выговаривать",
                                "target_lang_sense_clarification": "четко артикулировать"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "pronounce",
                                "target_lang_sense_clarification": "general meaning"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "526172ce-9083-3f95-8c16-fe101b21a5e2",
                    "sense_definition": "Gedachten, gevoelens of een mening onder woorden brengen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur <w>sprak</w> zijn dank <w>uit</w> voor de inzet van het personeel.",
                            "target_lang_translations": {
                                "ru": "Директор <w>выразил</w> свою благодарность за усилия персонала.",
                                "en": "The director <w>expressed</w> his gratitude for the staff's efforts."
                            }
                        },
                        {
                            "text": "Zij heeft haar bezorgdheid over de situatie duidelijk <w>uitgesproken</w>.",
                            "target_lang_translations": {
                                "ru": "Она ясно <w>высказала</w> свою обеспокоенность ситуацией.",
                                "en": "She clearly <w>voiced</w> her concern about the situation."
                            }
                        }
                    ],
                    "synonyms": [
                        "verklaren",
                        "verkondigen",
                        "verklaren"
                    ],
                    "antonyms": [
                        "verzwijgen"
                    ],
                    "common_phrases": [
                        "zijn dank uitspreken",
                        "steun uitspreken",
                        "een wens uitspreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Облекать мысли, чувства или мнение в слова.",
                        "en": "To put thoughts, feelings, or opinions into words."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выражать",
                                "target_lang_sense_clarification": "чувства, благодарность"
                            },
                            {
                                "target_lang_word": "высказывать",
                                "target_lang_sense_clarification": "мнение, мысли"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "express",
                                "target_lang_sense_clarification": "feelings, gratitude"
                            },
                            {
                                "target_lang_word": "voice",
                                "target_lang_sense_clarification": "concerns, opinions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "614f7ed9-8b88-359e-b1c3-033bf47cfa5c",
                    "sense_definition": "Duidelijk maken wat men van iets vindt; een standpunt innemen (wederkerend).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij durfde zich niet <w>uit</w> te <w>spreken</w> over dat gevoelige onderwerp.",
                            "target_lang_translations": {
                                "ru": "Он не осмелился <w>высказаться</w> на эту деликатную тему.",
                                "en": "He did not dare to <w>speak out</w> about that sensitive subject."
                            }
                        },
                        {
                            "text": "De bevolking heeft zich in de enquête <w>uitgesproken</w> voor verandering.",
                            "target_lang_translations": {
                                "ru": "Население в ходе опроса <w>высказалось</w> за перемены.",
                                "en": "The population <w>spoke out</w> in favor of change in the survey."
                            }
                        }
                    ],
                    "synonyms": [
                        "verklaren",
                        "verkondigen",
                        "verklaren"
                    ],
                    "common_phrases": [
                        "zich uitspreken over",
                        "zich uitspreken tegen",
                        "zich uitspreken voor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ясно давать понять, что кто-то думает о чем-то; занимать позицию (возвратный глагол).",
                        "en": "To make clear what one thinks about something; to take a position (reflexive)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высказываться",
                                "target_lang_sense_clarification": "о мнении или позиции"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "speak out",
                                "target_lang_sense_clarification": "take a stand"
                            },
                            {
                                "target_lang_word": "express oneself",
                                "target_lang_sense_clarification": "state one's opinion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b8cf543-9445-3099-98dd-15f0a4d9ef8e",
                    "sense_definition": "Op officiële wijze een oordeel of vonnis bekendmaken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "declaration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter zal volgende week het vonnis <w>uitspreken</w>.",
                            "target_lang_translations": {
                                "ru": "Судья <w>вынесет</w> приговор на следующей неделе.",
                                "en": "The judge will <w>pronounce</w> the sentence next week."
                            }
                        },
                        {
                            "text": "Na lang beraad werd het oordeel over de verdachte <w>uitgesproken</w>.",
                            "target_lang_translations": {
                                "ru": "После долгого совещания решение по подозреваемому было <w>вынесено</w>.",
                                "en": "After long deliberation, the judgment on the suspect was <w>pronounced</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verkondigen",
                        "verklaren",
                        "verklaren"
                    ],
                    "common_phrases": [
                        "een vonnis uitspreken",
                        "een oordeel uitspreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально объявлять решение или приговор.",
                        "en": "To officially announce a judgment or sentence."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выносить",
                                "target_lang_sense_clarification": "приговор, решение"
                            },
                            {
                                "target_lang_word": "оглашать",
                                "target_lang_sense_clarification": "публично зачитывать"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "pronounce",
                                "target_lang_sense_clarification": "a verdict or sentence"
                            },
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "a sentence"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
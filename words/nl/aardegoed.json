                {
                    "sense_id": "17727c53-3be9-391b-b762-ddba505a7f92",
                    "sense_definition": "Voorwerpen, zoals potten, schalen en bekers, die gemaakt zijn van gebakken klei.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De archeologen vonden scherven van oud <w>aardegoed</w> in de bodem.",
                            "target_lang_translations": {
                                "en": "The archaeologists found shards of old <w>earthenware</w> in the soil.",
                                "ru": "Археологи нашли в земле черепки старой <w>глиняной посуды</w>."
                            }
                        },
                        {
                            "text": "Zij serveerde de soep in een kom van grof <w>aardegoed</w>.",
                            "target_lang_translations": {
                                "en": "She served the soup in a bowl of coarse <w>earthenware</w>.",
                                "ru": "Она подала суп в миске из грубой <w>керамики</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "aardewerk"
                    ],
                    "common_phrases": [
                        "gebakken aardegoed"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "wordt in modern Nederlands meestal 'aardewerk' genoemd"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Objects, such as pots, bowls, and cups, made of baked clay.",
                        "ru": "Предметы, такие как горшки, миски и чашки, изготовленные из обожженной глины."
                    },

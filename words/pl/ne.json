                {
                    "sense_id": "8bdc9697-18d6-33c8-a0a0-891204066fec",
                    "sense_definition": "Okrzyk używany na wsi do przywoływania krów i cieląt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "call_animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz wyszedł na łąkę i wołał: <w>ne</w>, <w>ne</w>, chodźcie tutaj!",
                            "target_lang_translations": {
                                "ru": "Хозяин вышел на луг и звал: <w>тпрусь</w>, <w>тпрусь</w>, идите сюда!"
                            }
                        },
                        {
                            "text": "<w>Ne</w>, krasula, wracaj szybko do zagrody.",
                            "target_lang_translations": {
                                "ru": "<w>Тпрусь</w>, красуля, возвращайся скорее в загон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ne krasula",
                        "ne malutka"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "używane w gwarach wiejskich"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, используемый в деревне для подзывания коров и телят."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тпрусь",
                                "target_lang_sense_clarification": "возглас для подзывания коров"
                            },
                            {
                                "target_lang_word": "бысь",
                                "target_lang_sense_clarification": "возглас для подзывания телят"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a2671a1-2969-35d8-9d58-2f172ab66d7e",
                    "sense_definition": "Gwarowa forma wykrzyknika <w>na</w>, oznaczająca zachętę do wzięcia czegoś (masz, weź).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "take_it",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ne</w>, weź sobie to jabłko, jeśli jesteś głodny.",
                            "target_lang_translations": {
                                "ru": "<w>На</w>, возьми себе это яблоко, если ты голоден."
                            }
                        },
                        {
                            "text": "Chcesz trochę pieniędzy? <w>Ne</w>, masz tutaj drobne.",
                            "target_lang_translations": {
                                "ru": "Хочешь немного денег? <w>На</w>, вот тебе мелочь."
                            }
                        }
                    ],
                    "synonyms": [
                        "na",
                        "masz"
                    ],
                    "common_phrases": [
                        "ne masz",
                        "ne weź"
                    ],
                    "traits": [
                        {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b124a5f7-59ae-35d5-9b95-ddd707e737a6",
                    "sense_definition": "Moving through the air; in flight.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "flight_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy plane struggled to get <w>airborne</w> on the short runway.",
                            "target_lang_translations": {
                                "ru": "Тяжелому самолету было трудно <w>подняться в воздух</w> на короткой взлетно-посадочной полосе."
                            }
                        },
                        {
                            "text": "Once the bird is <w>airborne</w>, it glides effortlessly.",
                            "target_lang_translations": {
                                "ru": "Как только птица оказывается <w>в воздухе</w>, она парит без усилий."
                            }
                        }
                    ],
                    "synonyms": [
                        "aloft",
                        "flying"
                    ],
                    "antonyms": [
                        "grounded"
                    ],
                    "common_phrases": [
                        "get airborne",
                        "become airborne",
                        "stay airborne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в полете или движущийся по воздуху."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находящийся в воздухе",
                                "target_lang_sense_clarification": "общее значение состояния полета"
                            },
                            {
                                "target_lang_word": "оторвавшийся от земли",
                                "target_lang_sense_clarification": "о моменте взлета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9fc71ee-6368-34e8-9ce7-e8899eddf0fb",
                    "sense_definition": "Carried by or spread through the air, often referring to small particles, viruses, or pollution.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "carried_by_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Masks are effective against many <w>airborne</w> viruses.",
                            "target_lang_translations": {
                                "ru": "Маски эффективны против многих вирусов, <w>передающихся воздушно-капельным путем</w>."
                            }
                        },
                        {
                            "text": "Strong winds made the desert sand <w>airborne</w>.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер поднял песок пустыни <w>в воздух</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "airborne virus",
                        "airborne disease",
                        "airborne particles",
                        "airborne dust"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переносимый по воздуху (о частицах, вирусах или загрязнениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передающийся воздушно-капельным путем",
                                "target_lang_sense_clarification": "о болезнях и вирусах"
                            },
                            {
                                "target_lang_word": "переносимый по воздуху",
                                "target_lang_sense_clarification": "о пыли, пыльце или частицах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b5459f0-eff5-335c-a7a1-bd7143652479",
                    "sense_definition": "Transported by aircraft, especially referring to military soldiers or units.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "military_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>airborne</w> troops were deployed behind enemy lines.",
                            "target_lang_translations": {
                                "ru": "<w>Воздушно-десантные</w> войска были переброшены в тыл врага."
                            }
                        },
                        {
                            "text": "They planned a large <w>airborne</w> operation for the morning.",
                            "target_lang_translations": {
                                "ru": "Они запланировали крупную <w>воздушно-десантную</w> операцию на утро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "airborne troops",
                        "airborne division",
                        "airborne assault",
                        "airborne unit"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in military contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перевозимый авиацией (о военных подразделениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воздушно-десантный",
                                "target_lang_sense_clarification": "военный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "142a3427-ab49-37fa-9993-2a990525def8",
                    "sense_definition": "Fitted to, carried on, or operating from an aircraft.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "aircraft_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The jet uses <w>airborne</w> radar to track weather patterns.",
                            "target_lang_translations": {
                                "ru": "Самолет использует <w>бортовой</w> радар для отслеживания погодных условий."
                            }
                        },
                        {
                            "text": "Engineers tested the new <w>airborne</w> surveillance system.",
                            "target_lang_translations": {
                                "ru": "Инженеры протестировали новую <w>авиационную</w> систему наблюдения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "airborne radar",
                        "airborne sensors"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Установленный на летательном аппарате или работающий с него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бортовой",
                                "target_lang_sense_clarification": "об оборудовании на борту"
                            },
                            {
                                "target_lang_word": "авиационный",
                                "target_lang_sense_clarification": "относящийся к авиационной технике"
                            }
                        ]
                    }
                }
            ]
        },
        {

            "senses": [
                {
                    "sense_id": "a45b8df2-48f6-3282-bd5d-b96e6d4c2308",
                    "sense_definition": "Policja wojskowa, której zadaniem jest dbanie o porządek i dyscyplinę w siłach zbrojnych.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głównym zadaniem, jakie ma <w>żandarmeria</w>, jest pilnowanie przestrzegania prawa przez żołnierzy."
                        },
                        {
                            "text": "Na miejscu wypadku z udziałem wojskowej ciężarówki szybko pojawiła się <w>żandarmeria</w>."
                        }
                    ],
                    "synonyms": [
                        "żandarmeria wojskowa",
                        "ŻW",
                        "szkarłatne berety"
                    ],
                    "common_phrases": [
                        "żandarmeria wojskowa",
                        "patrol żandarmerii",
                        "komendant żandarmerii"
                    ]
                },
                {
                    "sense_id": "b6568ade-773e-315c-88d9-95b27f60f74e",
                    "sense_definition": "Ogół funkcjonariuszy służących w policji wojskowej lub podobnej formacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ponieważ wszedł na teren jednostki bez dokumentów, aresztowała go <w>żandarmeria</w>."
                        },
                        {
                            "text": "Cała miejscowa <w>żandarmeria</w> została postawiona w stan gotowości przed przyjazdem generała."
                        }
                    ],
                    "common_phrases": [
                        "interwencja żandarmerii",
                        "oddział żandarmerii"
                    ]
                },
                {
                    "sense_id": "141c78ec-4b00-36c4-997d-2024945687c3",
                    "sense_definition": "Formacja policyjna o charakterze wojskowym w niektórych państwach (np. we Francji czy Turcji), dbająca o bezpieczeństwo publiczne.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wakacji we Francji widzieliśmy, jak <w>żandarmeria</w> kieruje ruchem na skrzyżowaniu."
                        },
                        {
                            "text": "Włoska <w>żandarmeria</w> prowadzi śledztwo w sprawie kradzieży dzieł sztuki."
                        }
                    ],
                    "common_phrases": [
                        "żandarmeria francuska",
                        "posterunek żandarmerii"
                    ]
                },
                {
                    "sense_id": "f11f91c5-e552-31a5-a4d1-e5dac4cb7945",
                    "sense_definition": "W XIX i na początku XX wieku: policja państwowa pełniąca funkcje porządkowe i polityczne, często kojarzona z represjami.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Carska <w>żandarmeria</w> bezlitośnie ścigała uczestników powstania styczniowego."
                        },
                        {
                            "text": "W tamtych czasach <w>żandarmeria</w> budziła powszechny strach wśród ludności cywilnej."
                        }
                    ],
                    "common_phrases": [
                        "carska żandarmeria"
                    ],
                    "traits": [

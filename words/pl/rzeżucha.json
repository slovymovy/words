{
    "word_family": [
        "rzeżuszka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "83a910bc-71ac-34dd-b496-5b412ed75193",
                    "sense_definition": "Jadalna roślina o ostrym smaku, często uprawiana w domu na wacie, tradycyjnie spożywana w Polsce podczas Wielkanocy; botanicznie to pieprzyca siewna.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "edible_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad stołem unosił się zapach świeżo ściętej <w>rzeżuchy</w>.",
                            "target_lang_translations": {
                                "ru": "Над столом витал запах свежесрезанного <w>кресс-салата</w>."
                            }
                        },
                        {
                            "text": "Dzieci w przedszkolu uczą się, jak siać <w>rzeżuchę</w> na wilgotnej wacie.",
                            "target_lang_translations": {
                                "ru": "Дети в детском саду учатся сеять <w>кресс-салат</w> на влажной вате."
                            }
                        }
                    ],
                    "synonyms": [
                        "pieprzyca siewna",
                        "rzeżucha ogrodowa",
                        "rzeżucha siewna"
                    ],
                    "common_phrases": [
                        "siać rzeżuchę",
                        "rzeżucha na wacie",
                        "kanapka z rzeżuchą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобное растение с острым вкусом, часто выращиваемое дома на вате, традиционно употребляемое в Польше на Пасху; ботанически — кресс-салат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кресс-салат",
                                "target_lang_sense_clarification": "jadalna roślina / съедобное растение"
                            },
                            {
                                "target_lang_word": "кресс",
                                "target_lang_sense_clarification": "nazwa potoczna / разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e843b79-5efd-3484-8d14-a3392e5eff2b",
                    "sense_definition": "Niejadalna lub gorzka roślina zielna z rodzaju Cardamine, występująca dziko na łąkach i w lasach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botanical_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie <w>rzeżuchy</w> z tego gatunku są gorzkie i nie nadają się do jedzenia.",
                            "target_lang_translations": {
                                "ru": "Все <w>сердечники</w> этого вида горькие и не годятся в пищу."
                            }
                        },
                        {
                            "text": "Podczas spaceru biolog pokazał nam dziko rosnącą <w>rzeżuchę</w> na skraju lasu.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки биолог показал нам дикорастущий <w>сердечник</w> на опушке леса."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Несъедобное или горькое травянистое растение рода Cardamine (Сердечник), дикорастущее на лугах и в лесах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сердечник",
                                "target_lang_sense_clarification": "rodzaj botaniczny / ботанический род"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "3faea7bc-37c7-36d8-9665-44b4f09186f0",
                    "sense_definition": "Het voortbewegen van vloeistoffen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water <stroomt> rustig door de rivier naar de zee.",
                            "target_lang_translations": {
                                "en": "The water <w>flows</w> quietly through the river to the sea.",
                                "ru": "Вода спокойно <течёт> по реке к морю."
                            }
                        },
                        {
                            "text": "Na de hevige regenval is er veel water de straten in <gestroomd>.",
                            "target_lang_translations": {
                                "en": "After the heavy rainfall, a lot of water <w>flowed</w> into the streets.",
                                "ru": "После сильного ливня вода <потекла> на улицы."
                            }
                        }
                    ],
                    "synonyms": [
                        "vloeien"
                    ],
                    "common_phrases": [
                        "stromend water",
                        "tranen stromen"
                    ],
                    "target_lang_definitions": {
                        "en": "To move continuously and smoothly in a certain direction, like a liquid.",
                        "ru": "Движение жидкости в определённом направлении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flow",
                                "target_lang_sense_clarification": "to move steadily and continuously"
                            },
                            {
                                "target_lang_word": "stream",
                                "target_lang_sense_clarification": "to flow freely or rapidly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "о реке, воде"
                            },
                            {
                                "target_lang_word": "литься",
                                "target_lang_sense_clarification": "активно вытекать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97e7eee2-aa68-3d60-a367-9727a8917556",
                    "sense_definition": "Zich in grote aantallen of hoeveelheden verplaatsen (van mensen of dingen).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bestellingen <stroomden> binnen na de succesvolle reclamecampagne.",
                            "target_lang_translations": {
                                "en": "The orders <w>streamed</w> in after the successful advertising campaign.",
                                "ru": "После успешной рекламной кампании заказы <потекли> рекой."
                            }
                        },
                        {
                            "text": "Toen de deuren openden, <stroomden> de bezoekers de zaal binnen.",
                            "target_lang_translations": {
                                "en": "When the doors opened, the visitors <w>streamed</w> into the hall.",
                                "ru": "Когда двери открылись, посетители <хлынули> в зал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "binnen stromen",
                        "vol stromen"
                    ],
                    "target_lang_definitions": {
                        "en": "To move in large numbers or quantities (of people or things).",
                        "ru": "Перемещение большого количества людей или предметов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stream",
                                "target_lang_sense_clarification": "to move in a continuous flow"
                            },
                            {
                                "target_lang_word": "pour",
                                "target_lang_sense_clarification": "to come or go in large numbers"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "перен. двигаться потоком"
                            },
                            {
                                "target_lang_word": "стекаться",
                                "target_lang_sense_clarification": "собираться в одном месте"
                            },
                            {
                                "target_lang_word": "поступать",
                                "target_lang_sense_clarification": "приходить в большом количестве"
                            }
                        ]
                    }
                }
            ]
        },
        {

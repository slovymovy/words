{
    "word_family": [
        "placuszek",
        "plackowaty"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bab42c36-fa2d-3013-a47f-bd13f73ce254",
                    "sense_definition": "Płaski, zazwyczaj słodki wypiek, często z owocami, kruszonką lub innymi dodatkami.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_baked",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia piecze najlepszy <w>placek</w> drożdżowy ze śliwkami.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка печет лучший дрожжевой <w>пирог</w> со сливами."
                            }
                        },
                        {
                            "text": "Na deser podano nam ciepły <w>placek</w> z owocami i herbatę.",
                            "target_lang_translations": {
                                "ru": "На десерт нам подали теплый <w>пирог</w> с фруктами и чай."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciasto",
                        "wypiek"
                    ],
                    "common_phrases": [
                        "placek z owocami",
                        "placek drożdżowy",
                        "placek ze śliwkami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская, обычно сладкая выпечка, часто с фруктами, посыпкой или другими добавками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пирог",
                                "target_lang_sense_clarification": "выпечка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34c1b60c-0121-3f08-bda6-5e0b6413de27",
                    "sense_definition": "Niewielki, smażony na tłuszczu krążek z masy mącznej, ziemniaczanej lub warzywnej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_fried",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci uwielbiają chrupiące <w>placki</w> ziemniaczane ze śmietaną.",
                            "target_lang_translations": {
                                "ru": "Дети обожают хрустящие картофельные <w>драники</w> со сметаной."
                            }
                        },
                        {
                            "text": "Na kolację usmażę słodkie <w>placki</w> z jabłkami.",
                            "target_lang_translations": {
                                "ru": "На ужин я пожарю сладкие <w>оладьи</w> с яблоками."
                            }
                        }
                    ],
                    "synonyms": [
                        "racuch"
                    ],
                    "common_phrases": [
                        "placki ziemniaczane",
                        "placki po węgiersku",
                        "placki z jabłkami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая лепешка из мучной, картофельной или овощной массы, жаренная в масле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оладья",
                                "target_lang_sense_clarification": "из теста или овощей"
                            },
                            {
                                "target_lang_word": "драник",
                                "target_lang_sense_clarification": "картофельный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0b27a79-ad78-3396-95e8-b7bf3a0a4c24",
                    "sense_definition": "Plama lub obszar różniący się kolorem albo wyglądem od otoczenia.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "visual_spot",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze stresu na jego szyi pojawiły się czerwone <w>placki</w>.",
                            "target_lang_translations": {
                                "ru": "От стресса на его шее появились красные <w>пятна</w>."
                            }
                        },
                        {
                            "text": "Trawa na boisku była wytarta i gdzieniegdzie widać było <w>placki</w> gołej ziemi.",
                            "target_lang_translations": {
                                "ru": "Трава на поле была вытоптана, и кое-где виднелись <w>пятна</w> голой земли."
                            }
                        }
                    ],
                    "synonyms": [
                        "plama"
                    ],
                    "common_phrases": [
                        "czerwone placki",
                        "łysy placek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пятно или участок, отличающийся цветом или видом от окружения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пятно",
                                "target_lang_sense_clarification": "на коже или поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f633bd9-3775-32e3-a60e-3b7fe1b7eb7e",
                    "sense_definition": "Płaski kawałek uformowany z miękkiej substancji lub masy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape_mass",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj, żeby nie wdepnąć w krowi <w>placek</w> na łące.",
                            "target_lang_translations": {
                                "ru": "Смотри, не наступи в коровью <w>лепёшку</w> на лугу."
                            }
                        },
                        {
                            "text": "Z rozwałkowanego ciasta wycinamy duży <w>placek</w> na spód pizzy.",
                            "target_lang_translations": {
                                "ru": "Из раскатанного теста вырезаем большую <w>лепёшку</w> для основы пиццы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "krowi placek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский кусок, сформированный из мягкой субстанции или массы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лепёшка",
                                "target_lang_sense_clarification": "форма"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
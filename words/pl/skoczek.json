    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a54ee022-1454-395e-8e36-4e27b1375194",
                    "sense_definition": "Figura w szachach, zwykle w kształcie głowy konia, która porusza się o dwa pola w jednym kierunku i jedno w bok (w kształcie litery L).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "szachy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wykonał sprytny ruch <w>skoczkiem</w> i zagroził królowej przeciwnika.",
                            "target_lang_translations": {
                                "ru": "Он сделал хитрый ход <w>конём</w> и создал угрозу ферзю противника."
                            }
                        },
                        {
                            "text": "Na szachownicy zostały mi tylko król i jeden <w>skoczek</w>.",
                            "target_lang_translations": {
                                "ru": "На шахматной доске у меня остались только король и один <w>конь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "koń",
                        "konik"
                    ],
                    "common_phrases": [
                        "ruch skoczkiem",
                        "bicie skoczkiem",
                        "biały skoczek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шахматная фигура, обычно в форме головы лошади, которая ходит буквой «Г» (на два поля в одном направлении и на одно в сторону)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конь",
                                "target_lang_sense_clarification": "шахматная фигура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7a8c787-0403-39ac-bfe0-930374b68c8f",
                    "sense_definition": "Sportowiec, który uprawia dyscyplinę polegającą na wykonywaniu skoków (np. narciarskich, wzwyż lub o tyczce).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polski <w>skoczek</w> zdobył złoty medal na zimowej olimpiadzie.",
                            "target_lang_translations": {
                                "ru": "Польский <w>прыгун</w> завоевал золотую медаль на зимней олимпиаде."
                            }
                        },
                        {
                            "text": "Każdy <w>skoczek</w> narciarski musi bardzo dbać o odpowiednią wagę ciała.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>прыгун</w> с трамплина должен очень следить за весом тела."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skoczek narciarski",
                        "skoczek wzwyż",
                        "skoczek o tyczce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортсмен, специализирующийся на прыжках (например, на лыжах, в высоту или с шестом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прыгун",
                                "target_lang_sense_clarification": "спортсмен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adf00ba8-6556-3d76-b4b0-4e3884ffc10f",
                    "sense_definition": "Osoba wykonująca skoki ze spadochronem.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony <w>skoczek</w> otworzył spadochron w ostatniej bezpiecznej chwili.",
                            "target_lang_translations": {
                                "ru": "Опытный <w>парашютист</w> раскрыл парашют в последний безопасный момент."
                            }
                        },
                        {
                            "text": "Wojskowi <w>skoczkowie</w> desantowali się na terenie wroga w środku nocy.",
                            "target_lang_translations": {
                                "ru": "Военные <w>парашютисты</w> десантировались на территории врага посреди ночи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skoczek spadochronowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, совершающий прыжки с парашютом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парашютист",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6233ace-de98-3e6a-af64-db33523b048a",
                    "sense_definition": "Pracownik, który bardzo często zmienia miejsce zatrudnienia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "praca",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracodawcy niechętnie zatrudniają <w>skoczków</w>, bojąc się, że szybko odejdą do innej firmy.",
                            "target_lang_translations": {
                                "ru": "Работодатели неохотно нанимают <w>летунов</w>, боясь, что они быстро уйдут в другую фирму."
                            }
                        },
                        {
                            "text": "On jest typowym <w>skoczkiem</w>, w żadnej pracy nie zagrzał miejsca dłużej niż rok.",
                            "target_lang_translations": {
                                "ru": "Он типичный <w>летун</w>, ни на одной работе не задержался дольше года."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Работник, который очень часто меняет место работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "летун",
                                "target_lang_sense_clarification": "о работнике (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f96a51d-8f1b-3532-a06f-1ae2b9897814",
                    "sense_definition": "Mały, krótki skok; zdrobnienie od słowa <w>skok</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ruch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko robiło wesołe <w>skoczki</w> przez kałuże na chodniku.",
                            "target_lang_translations": {
                                "ru": "Ребёнок делал весёлые <w>прыжки</w> через лужи на тротуаре."
                            }
                        },
                        {
                            "text": "To był tylko taki mały <w>skoczek</w> w bok, nic poważnego.",
                            "target_lang_translations": {
                                "ru": "Это был всего лишь маленький <w>скачок</w> в сторону, ничего серьёзного."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький, короткий прыжок; уменьшительная форма от слова «прыжок»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прыжок",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "скачок",
                                "target_lang_sense_clarification": "резкое движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bf034d7-349a-3d76-9047-93703ecbe0d8",
                    "sense_definition": "Niewielki gryzoń pustynny z długimi tylnymi nogami, poruszający się skokami.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "zwierzęta",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W terrarium w zoo widzieliśmy małego <w>skoczka</w> pustynnego.",
                            "target_lang_translations": {
                                "ru": "В террариуме в зоопарке мы видели маленького пустынного <w>тушканчика</w>."
                            }
                        },
                        {
                            "text": "<w>Skoczek</w> potrafi wykonywać bardzo długie susy dzięki silnym tylnym nogom.",
                            "target_lang_translations": {
                                "ru": "<w>Тушканчик</w> может совершать очень длинные прыжки благодаря сильным задним ногам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skoczek egipski",
                        "skoczek pustynny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой пустынный грызун с длинными задними ногами, передвигающийся прыжками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тушканчик",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
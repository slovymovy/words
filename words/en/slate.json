    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c6a48b90-18ec-3dad-ae23-8a70dd41aae1",
                    "sense_definition": "A fine-grained grey rock formed from clay or ash that splits easily into thin layers, often used for roofing tiles.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old cottage has a roof made of heavy grey <w>slate</w>.",
                            "target_lang_translations": {
                                "ru": "У старого коттеджа крыша сделана из тяжелого серого <w>сланца</w>."
                            }
                        },
                        {
                            "text": "Geologists found a large deposit of <w>slate</w> near the quarry.",
                            "target_lang_translations": {
                                "ru": "Геологи обнаружили крупное месторождение <w>сланца</w> возле карьера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "slate roof",
                        "slate quarry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкозернистая серая горная порода, образованная из глины или пепла, которая легко расслаивается на тонкие пластины; часто используется для кровельной черепицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сланец",
                                "target_lang_sense_clarification": "горная порода"
                            },
                            {
                                "target_lang_word": "шифер",
                                "target_lang_sense_clarification": "кровельный материал"
                            },
                            {
                                "target_lang_word": "аспидный сланец",
                                "target_lang_sense_clarification": "геологический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0967b976-e01a-35be-b4d5-3fea6586acef",
                    "sense_definition": "A list of candidates proposed by a political party for an election.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "list_schedule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The party announced its full <w>slate</w> of candidates for the upcoming election.",
                            "target_lang_translations": {
                                "ru": "Партия объявила свой полный <w>список</w> кандидатов на предстоящие выборы."
                            }
                        },
                        {
                            "text": "Voters can choose to support the entire <w>slate</w> or pick individual nominees.",
                            "target_lang_translations": {
                                "ru": "Избиратели могут поддержать весь <w>список</w> или выбрать отдельных кандидатов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "party slate",
                        "candidate slate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Список кандидатов, выдвинутых политической партией на выборы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "список кандидатов",
                                "target_lang_sense_clarification": "политический"
                            },
                            {
                                "target_lang_word": "список",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ef0b0ee-5f85-3334-9e1d-d4d5a5e0cc4b",
                    "sense_definition": "A schedule or list of events, projects, or works.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "list_schedule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The film studio has a busy <w>slate</w> of releases planned for this year.",
                            "target_lang_translations": {
                                "ru": "У киностудии на этот год запланирован плотный <w>график</w> релизов."
                            }
                        },
                        {
                            "text": "We have a full <w>slate</w> of meetings scheduled for tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "У нас на завтрашнее утро запланировано полное <w>расписание</w> встреч."
                            }
                        }
                    ],
                    "common_phrases": [
                        "full slate",
                        "blank slate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расписание или перечень событий, проектов или работ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "план",
                                "target_lang_sense_clarification": "намеченные действия"
                            },
                            {
                                "target_lang_word": "график",
                                "target_lang_sense_clarification": "расписание"
                            },
                            {
                                "target_lang_word": "список",
                                "target_lang_sense_clarification": "перечень"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "009d3348-8e94-3e5e-8de6-9cdb78020b25",
                    "sense_definition": "A dark grey board or sheet of stone used for writing on with chalk, formerly common in schools.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "writing_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The student wiped the chalk numbers off his <w>slate</w> with a cloth.",
                            "target_lang_translations": {
                                "ru": "Ученик стер меловые цифры со своей <w>грифельной доски</w> тряпкой."
                            }
                        },
                        {
                            "text": "In the nineteenth century, children learned to write using a <w>slate</w> and pencil.",
                            "target_lang_translations": {
                                "ru": "В девятнадцатом веке дети учились писать, используя <w>грифельную доску</w> и грифель."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clean slate",
                        "wipe the slate clean"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to historical school equipment"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Темно-серая доска или пластина из камня, используемая для письма мелом, ранее распространенная в школах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грифельная доска",
                                "target_lang_sense_clarification": "для письма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c56f41c4-e9cc-30b1-931b-cac98a99c08d",
                    "sense_definition": "A dark bluish-grey colour.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "colour",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist painted the stormy sky in shades of <w>slate</w> and charcoal.",
                            "target_lang_translations": {
                                "ru": "Художник нарисовал грозовое небо в оттенках <w>грифельного</w> и угольного."
                            }
                        },
                        {
                            "text": "She wore a dress in a deep <w>slate</w> colour that matched her eyes.",
                            "target_lang_translations": {
                                "ru": "На ней было платье глубокого <w>грифельного</w> цвета, подходившее к её глазам."
                            }
                        }
                    ],
                    "synonyms": [
                        "slate blue",
                        "slate grey",
                        "slate gray"
                    ],
                    "target_lang_definitions": {
                        "ru": "Темный голубовато-серый цвет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грифельный цвет",
                                "target_lang_sense_clarification": "оттенок серого"
                            },
                            {
                                "target_lang_word": "аспидный цвет",
                                "target_lang_sense_clarification": "черно-серый"
                            }
                        ]

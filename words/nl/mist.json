    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4cbd4572-e71c-3fe2-b156-da2a2f4f9927",
                    "sense_definition": "Laaghangende bewolking die het zicht belemmert.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de dichte <w>mist</w> kon ik de overkant van de straat niet zien.",
                            "target_lang_translations": {
                                "ru": "Из-за густого <w>тумана</w> я не мог видеть другую сторону улицы.",
                                "en": "Due to the dense <w>fog</w>, I couldn't see the other side of the street."
                            }
                        },
                        {
                            "text": "De <w>mist</w> trok langzaam op toen de zon begon te schijnen.",
                            "target_lang_translations": {
                                "ru": "<w>Туман</w> медленно рассеивался, когда начало светить солнце.",
                                "en": "The <w>fog</w> slowly lifted when the sun started shining."
                            }
                        }
                    ],
                    "synonyms": [
                        "nevel"
                    ],
                    "common_phrases": [
                        "dichte mist",
                        "in de mist",
                        "de mist ingaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Низкая облачность, ограничивающая видимость.",
                        "en": "Low-hanging clouds that obstruct visibility."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туман",
                                "target_lang_sense_clarification": "атмосферное явление"
                            },
                            {
                                "target_lang_word": "мгла",
                                "target_lang_sense_clarification": "дымка, непрозрачный воздух"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fog",
                                "target_lang_sense_clarification": "thick cloud near the ground"
                            },
                            {
                                "target_lang_word": "mist",
                                "target_lang_sense_clarification": "lighter atmospheric phenomenon"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c7f1b9b6-96fc-36b3-af4f-0f64114d8bf9",
                    "sense_definition": "Vervoeging van <w>missen</w>: tweede en derde persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>mist</w> de bus en komt daardoor te laat op school.",
                            "target_lang_translations": {
                                "ru": "Он <w>пропускает</w> автобус и из-за этого опаздывает в школу.",
                                "en": "He <w>misses</w> the bus and therefore arrives late for school."
                            }
                        },
                        {
                            "text": "Zij <w>mist</w> haar familie erg sinds ze verhuisd is.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>скучает</w> по своей семье с тех пор, как переехала.",
                                "en": "She <w>misses</w> her family very much since she moved."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола <w>missen</w>: 2-е и 3-е лицо единственного числа настоящего времени.",
                        "en": "Conjugation of <w>missen</w>: 2nd and 3rd person singular present tense."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропускает",
                                "target_lang_sense_clarification": "транспорт, событие"
                            },
                            {
                                "target_lang_word": "скучает",
                                "target_lang_sense_clarification": "испытывает тоску по кому-либо"
                            },
                            {
                                "target_lang_word": "опаздывает",
                                "target_lang_sense_clarification": "на транспорт (контекстуально)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "misses",
                                "target_lang_sense_clarification": "fails to catch or feels the absence of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc50a43c-c653-3b90-9f42-42068be9c85c",
                    "sense_definition": "Vervoeging van <w>misten</w>: onpersoonlijke tegenwoordige tijd.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>mist</w> behoorlijk, dus je moet voorzichtig rijden.",
                            "target_lang_translations": {
                                "ru": "На улице сильный <w>туман</w>, поэтому нужно ехать осторожно.",
                                "en": "It <w>is</w> quite <w>foggy</w>, so you have to drive carefully."
                            }
                        },
                        {
                            "text": "Als het <w>mist</w>, is het zicht op de weg erg slecht.",
                            "target_lang_translations": {
                                "ru": "Когда <w>туманно</w>, видимость на дороге очень плохая.",
                                "en": "When it <w>is foggy</w>, visibility on the road is very poor."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола <w>misten</w>: безличная форма настоящего времени.",
                        "en": "Conjugation of <w>misten</w>: impersonal present tense."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туманно",
                                "target_lang_sense_clarification": "наречие, состояние погоды"
                            },
                            {
                                "target_lang_word": "стоит туман",
                                "target_lang_sense_clarification": "фраза, описывающая погоду"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "is foggy",
                                "target_lang_sense_clarification": "weather condition"
                            },
                            {
                                "target_lang_word": "there is fog",
                                "target_lang_sense_clarification": "presence of fog"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "68314ac5-3a81-3cb6-acad-99e8b430bc62",
                    "sense_definition": "A designated place where pedestrians, vehicles, or animals can safely cross a road, river, or railway.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "place_to_cross",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should always use the pedestrian <w>crossing</w> to stay safe.",
                            "target_lang_translations": {
                                "ru": "Вам следует всегда пользоваться пешеходным <w>переходом</w> для безопасности."
                            }
                        },
                        {
                            "text": "The border <w>crossing</w> was crowded with tourists and trucks.",
                            "target_lang_translations": {
                                "ru": "Пограничный <w>переход</w> был переполнен туристами и грузовиками."
                            }
                        }
                    ],
                    "synonyms": [
                        "crosswalk",
                        "zebra crossing"
                    ],
                    "common_phrases": [
                        "pedestrian crossing",
                        "border crossing",
                        "level crossing",
                        "ferry crossing",
                        "zebra crossing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специально отведенное место, где пешеходы, транспортные средства или животные могут безопасно пересечь дорогу, реку или железнодорожные пути"
                    },
                    "translations": {

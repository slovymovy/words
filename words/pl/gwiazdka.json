    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "afc982d6-a015-3b20-ab06-ac0d7b030155",
                    "sense_definition": "Zdrobnienie od słowa <w>gwiazda</w>; małe ciało niebieskie lub przedmiot, wzór albo ozdoba w tym kształcie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "diminutive_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na szczycie choinki błyszczy piękna, złota <gwiazdka>.",
                            "target_lang_translations": {
                                "ru": "На верхушке ёлки блестит красивая золотая <звёздочка>."
                            }
                        },
                        {
                            "text": "Dziecko narysowało niebo pełne małych <gwiazdek>.",
                            "target_lang_translations": {
                                "ru": "Ребёнок нарисовал небо, полное маленьких <звёздочек>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gwiazdeczka"
                    ],
                    "common_phrases": [
                        "patrzeć w gwiazdki",
                        "złota gwiazdka",
                        "wycinać gwiazdki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова <w>gwiazda</w>; маленькое небесное тело, предмет, узор или украшение такой формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звёздочка",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное / предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73562ae5-bead-3642-ae7a-e74cbc788d44",
                    "sense_definition": "Znak graficzny w kształcie małej gwiazdy (*), używany w tekście np. do odesłania do przypisu, zastąpienia litery lub jako symbol mnożenia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hasło musi zawierać litery, cyfry i jedną <gwiazdkę>.",
                            "target_lang_translations": {
                                "ru": "Пароль должен содержать буквы, цифры и одну <звёздочку>."
                            }
                        },
                        {
                            "text": "Trudne wyrazy w tekście zostały oznaczone <gwiazdką>.",
                            "target_lang_translations": {
                                "ru": "Сложные слова в тексте были помечены <звёздочкой>."
                            }
                        }
                    ],
                    "synonyms": [
                        "asterysk"
                    ],
                    "common_phrases": [
                        "oznaczyć gwiazdką",
                        "hasło z gwiazdką",
                        "tekst z gwiazdką"
                    ],
                    "target_lang_definitions": {
                        "ru": "Графический знак в форме маленькой звезды (*), используемый в тексте, например, для отсылки к сноске, замены буквы или как символ умножения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звёздочка",
                                "target_lang_sense_clarification": "типографский знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8fc0f8b-b4c3-3cfa-9711-f12b9d1fe15c",
                    "sense_definition": "Element odznaki noszonej na mundurze, oznaczający stopień wojskowy lub policyjny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oficerska <gwiazdka> musi zachować swoją cenę i budzić poważanie wśród żołnierzy.",
                            "target_lang_translations": {
                                "ru": "Офицерская <звёздочка> должна сохранять свою ценность и вызывать уважение у солдат."
                            }
                        },
                        {
                            "text": "Po latach służby w końcu dostał upragnioną <gwiazdkę> na pagonie.",
                            "target_lang_translations": {
                                "ru": "После многих лет службы он наконец получил долгожданную <звёздочку> на погон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dostać gwiazdkę",
                        "belka i gwiazdka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент знака различия на форме, обозначающий воинское или полицейское звание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звёздочка",
                                "target_lang_sense_clarification": "знак различия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "65b83348-b2c9-3f4c-b12c-26cd48182169",
                    "sense_definition": "Potoczne określenie świąt Bożego Narodzenia, Wigilii oraz zwyczaju dawania prezentów w tym czasie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "holiday",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "Na <Gwiazdkę> dostałam od rodziców najnowszego iPhone'a.",
                            "target_lang_translations": {
                                "ru": "На <Рождество> я получила от родителей новейший айфон."
                            }
                        },
                        {
                            "text": "Cała rodzina zjeżdża się do nas na <Gwiazdkę>.",
                            "target_lang_translations": {
                                "ru": "Вся семья съезжается к нам на <Рождество>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Boże Narodzenie",
                        "Wigilia"
                    ],
                    "common_phrases": [
                        "prezent na Gwiazdkę",
                        "wesołej Gwiazdki",
                        "dostać na Gwiazdkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common informal name for Christmas"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название Рождества, Сочельника, а также обычая дарить подарки в это время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Рождество",
                                "target_lang_sense_clarification": "праздник"
                            }
                        ]
                    }
                }
            ]
        }
    ]

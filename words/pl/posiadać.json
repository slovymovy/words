{
    "word_family": [
        "posiadacz",
        "posiadłość",
        "współposiadać",
        "posiadanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1532b9bf-8a02-3e57-b5d0-3d4e7cb378a3",
                    "sense_definition": "Być właścicielem czegoś, mieć coś na własność (zazwyczaj w odniesieniu do rzeczy materialnych o pewnej wartości).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ownership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi dziadkowie <w>posiadają</w> duży dom z ogrodem na wsi.",
                            "target_lang_translations": {
                                "ru": "Мои дедушка и бабушка <w>владеют</w> большим домом с садом в деревне."
                            }
                        },
                        {
                            "text": "Każdy obywatel tego kraju <w>posiada</w> prawo do wolności słowa.",
                            "target_lang_translations": {
                                "ru": "Каждый гражданин этой страны <w>имеет</w> право на свободу слова."
                            }
                        }
                    ],
                    "synonyms": [
                        "mieć",
                        "dysponować",
                        "usiąść"
                    ],
                    "common_phrases": [
                        "posiadać majątek",
                        "posiadać ziemię",
                        "posiadać dom",
                        "posiadać prawo"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often used in formal contexts as a synonym for 'mieć'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть собственником чего-либо, иметь что-либо в собственности (обычно в отношении материальных ценностей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "владеть",
                                "target_lang_sense_clarification": "иметь в собственности"
                            },
                            {
                                "target_lang_word": "иметь",
                                "target_lang_sense_clarification": "общее значение обладания"
                            },
                            {
                                "target_lang_word": "обладать",
                                "target_lang_sense_clarification": "располагать чем-либо ценным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44d0a265-9efb-33e0-b3e0-1f89590eaa0b",
                    "sense_definition": "Mieć określoną cechę, właściwość, strukturę lub element składowy (często o przedmiotach lub pojęciach abstrakcyjnych).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "characteristics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowoczesny telefon <w>posiada</w> wiele przydatnych funkcji i aplikacji.",
                            "target_lang_translations": {
                                "ru": "Этот современный телефон <w>имеет</w> много полезных функций и приложений."
                            }
                        },
                        {
                            "text": "Substancja ta <w>posiada</w> silne właściwości żrące i należy z nią uważać.",
                            "target_lang_translations": {
                                "ru": "Это вещество <w>обладает</w> сильными едкими свойствами, и с ним нужно быть осторожным."
                            }
                        }
                    ],
                    "synonyms": [
                        "mieć",
                        "mieć",
                        "dysponować"
                    ],
                    "common_phrases": [
                        "posiadać cechy",
                        "posiadać zdolność",
                        "posiadać funkcję",
                        "posiadać znaczenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь определенную черту, свойство, структуру или составной элемент (часто о предметах или абстрактных понятиях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обладать",
                                "target_lang_sense_clarification": "свойствами или характеристиками"
                            },
                            {
                                "target_lang_word": "иметь",
                                "target_lang_sense_clarification": "функции или составные части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c073c432-6819-3f31-b4c8-ef6c3fbcbeac",
                    "sense_definition": "Opanować jakąś wiedzę, umiejętność lub legitymować się określonymi kwalifikacjami.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kandydat na to stanowisko musi <w>posiadać</w> biegłą znajomość języka angielskiego.",
                            "target_lang_translations": {
                                "ru": "Кандидат на эту должность должен <w>владеть</w> английским языком в совершенстве."
                            }
                        },
                        {
                            "text": "Nasz zespół inżynierów <w>posiada</w> wieloletnie doświadczenie w budowie mostów.",
                            "target_lang_translations": {
                                "ru": "Наша команда инженеров <w>имеет</w> многолетний опыт в строительстве мостов."
                            }
                        }
                    ],
                    "synonyms": [
                        "mieć",
                        "dysponować",
                        "mieć"
                    ],
                    "common_phrases": [
                        "posiadać wiedzę",
                        "posiadać doświadczenie",
                        "posiadać wykształcenie",
                        "posiadać umiejętności"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in job advertisements and formal descriptions"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Овладеть какими-либо знаниями, навыками или иметь определенную квалификацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "владеть",
                                "target_lang_sense_clarification": "языком или навыком"
                            },
                            {
                                "target_lang_word": "иметь",
                                "target_lang_sense_clarification": "опыт или образование"
                            },
                            {
                                "target_lang_word": "обладать",
                                "target_lang_sense_clarification": "знаниями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc185274-06fe-3452-9f4d-eddb482ee489",
                    "sense_definition": "W zwrocie \"nie posiadać się\": odczuwać bardzo silne emocje, tracić panowanie nad sobą pod wpływem uczuć (zazwyczaj radości lub gniewu).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy dowiedziała się o wygranej na loterii, nie <w>posiadała</w> się ze szczęścia.",
                            "target_lang_translations": {
                                "ru": "Когда она узнала о выигрыше в лотерею, она <w>была вне себя</w> от счастья."
                            }
                        },
                        {
                            "text": "Ojciec nie <w>posiadał</w> się z dumy, patrząc na dyplom ukończenia studiów przez syna.",
                            "target_lang_translations": {
                                "ru": "Отец <w>был вне себя</w> от гордости, глядя на диплом сына об окончании университета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nie posiadać się z radości",
                        "nie posiadać się z oburzenia",
                        "nie posiadać się ze szczęścia"
                    ],
                    "target_lang_definitions": {
                        "ru": "В выражении \"nie posiadać się\": испытывать очень сильные эмоции, терять самообладание под влиянием чувств (обычно радости или гнева)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть вне себя",
                                "target_lang_sense_clarification": "идиома: от радости, от гнева"
                            },
                            {
                                "target_lang_word": "не помнить себя",
                                "target_lang_sense_clarification": "от сильных эмоций"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd167b44-6135-3f3f-a4f0-1127c2b425e3",
                    "sense_definition": "Usiąść jeden po drugim lub zająć miejsca siedzące w wielu miejscach (o grupie osób lub zwierząt).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczeni turyści <w>posiadali</w> na trawie, aby odpocząć po długiej wędrówce.",
                            "target_lang_translations": {
                                "ru": "Уставшие туристы <w>расселись</w> на траве, чтобы отдохнуть после долгих блужданий."
                            }
                        },
                        {
                            "text": "Stado wróbli przyleciało nagle i ptaki <w>posiadały</w> na liniach wysokiego napięcia.",
                            "target_lang_translations": {
                                "ru": "Стая воробьев внезапно прилетела, и птицы <w>уселись</w> на линиях высокого напряжения."
                            }
                        }
                    ],
                    "synonyms": [
                        "pousiadać",
                        "usiąść"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "distributive aspect of 'siadać'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сесть один за другим или занять сидячие места во многих местах (о группе людей или животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассесться",
                                "target_lang_sense_clarification": "занять места (о многих)"
                            },
                            {
                                "target_lang_word": "усесться",
                                "target_lang_sense_clarification": "разместиться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
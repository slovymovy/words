    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cfc24dba-3593-3c14-9e94-a031ae30d7fa",
                    "sense_definition": "Meervoud van <w>heilige</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de oude kerk staan prachtige beelden van verschillende <w>heiligen</w>.",
                            "target_lang_translations": {
                                "ru": "В старой церкви стоят прекрасные статуи различных <w>святых</w>.",
                                "en": "In the old church stand beautiful statues of various <w>saints</w>."
                            }
                        },
                        {
                            "text": "Veel gelovigen bidden tot <w>heiligen</w> om steun en bescherming te vragen.",
                            "target_lang_translations": {
                                "ru": "Многие верующие молятся <w>святым</w>, чтобы попросить о поддержке и защите.",
                                "en": "Many believers pray to <w>saints</w> to ask for support and protection."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова \"святой\".",
                        "en": "Plural form of \"saint\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "святые",
                                "target_lang_sense_clarification": "люди, признанные церковью святыми"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "saints",
                                "target_lang_sense_clarification": "people recognized as holy by a religion"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7115247a-f702-3352-9c93-2735766d0978",
                    "sense_definition": "Heilig maken, wijden of met grote eerbied behandelen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel mensen vragen zich af of het doel de middelen wel <w>heiligt</w>.",
                            "target_lang_translations": {
                                "ru": "Многие люди задаются вопросом, действительно ли цель <w>оправдывает</w> средства.",
                                "en": "Many people wonder if the end really <w>justifies</w> the means."
                            }
                        },
                        {
                            "text": "De priester <w>heiligde</w> het water voordat de ceremonie begon.",
                            "target_lang_translations": {
                                "ru": "Священник <w>освятил</w> воду перед началом церемонии.",
                                "en": "The priest <w>sanctified</w> the water before the ceremony began."
                            }
                        }
                    ],
                    "synonyms": [
                        "wijden",
                        "zegenen"
                    ],
                    "antonyms": [
                        "ontheiligen"
                    ],
                    "common_phrases": [
                        "het doel heiligt de middelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать святым, освящать или относиться с большим благоговением.",
                        "en": "To make holy, consecrate, or treat with great reverence."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освящать",
                                "target_lang_sense_clarification": "делать святым, совершать обряд"
                            },
                            {
                                "target_lang_word": "оправдывать",
                                "target_lang_sense_clarification": "в выражении \"цель оправдывает средства\""
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "sanctify",
                                "target_lang_sense_clarification": "to make holy or set apart as sacred"
                            },
                            {
                                "target_lang_word": "justify",
                                "target_lang_sense_clarification": "used in the idiom \"the end justifies the means\""
                            },
                            {
                                "target_lang_word": "hallow",
                                "target_lang_sense_clarification": "to honor as holy"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cbbb4777-5b8d-3359-8323-f80cc3f94224",
                    "sense_definition": "A bag, usually used by women, for carrying personal items such as a wallet, keys, and a phone.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "bag_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She searched through her <w>purse</w> to find her car keys.",
                            "target_lang_translations": {
                                "ru": "Она обыскала свою <w>сумочку</w>, чтобы найти ключи от машины."
                            }
                        },
                        {
                            "text": "My mother bought a beautiful leather <w>purse</w> at the store yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера мама купила в магазине красивую кожаную <w>сумку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "handbag"
                    ],
                    "common_phrases": [
                        "leather purse",
                        "designer purse",
                        "carry a purse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская сумка, обычно используемая для ношения личных вещей, таких как кошелек, ключи и телефон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сумочка",
                                "target_lang_sense_clarification": "женская"
                            },
                            {
                                "target_lang_word": "сумка",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfec57b0-e0a8-3fe3-b920-faf2f43a3253",
                    "sense_definition": "A small pouch or container used specifically for carrying money, particularly coins.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "bag_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He took a few coins out of his <w>purse</w> to pay the driver.",
                            "target_lang_translations": {
                                "ru": "Он достал несколько монет из своего <w>кошелька</w>, чтобы расплатиться с водителем."
                            }
                        },
                        {
                            "text": "The old lady kept her savings in a small velvet <w>purse</w>.",
                            "target_lang_translations": {
                                "ru": "Старушка хранила свои сбережения в маленьком бархатном <w>кошельке</w>."
                            }

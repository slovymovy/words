                {
                    "sense_id": "7f1da817-0809-3801-a8ed-869f6ef5eda8",
                    "sense_definition": "In a manner that must be kept sacred, secure, and not broken or violated.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient traditions were held <w>inviolably</w> by the community for many generations.",
                            "target_lang_translations": {
                                "ru": "Древние традиции <w>свято</w> хранились общиной на протяжении многих поколений."
                            }
                        },
                        {
                            "text": "She insisted that the confidentiality of their discussion be maintained <w>inviolably</w>.",
                            "target_lang_translations": {
                                "ru": "Она настаивала на том, чтобы конфиденциальность их беседы соблюдалась <w>нерушимо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "unbreakably",
                        "unassailably",
                        "sacredly"
                    ],
                    "common_phrases": [
                        "held inviolably",
                        "kept inviolably",
                        "maintained inviolably"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, который должен оставаться священным, надежным и не подлежит нарушению или посягательству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нерушимо",
                                "target_lang_sense_clarification": "о том, что нельзя нарушить или разрушить (клятвы, союзы)"
                            },
                            {
                                "target_lang_word": "свято",
                                "target_lang_sense_clarification": "о том, что чтится как святыня (традиции, долг)"
                            },
                            {
                                "target_lang_word": "незыблемо",
                                "target_lang_sense_clarification": "о том, что остается твердым и неизменным (права, законы)"
                            }
                        ]
                    }
                }
            ]
        }

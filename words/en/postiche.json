                    "frequency": "Low",
                    "semantic_group_id": "false_hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actor applied a <w>postiche</w> to his chin to transform into the elderly character.",
                            "target_lang_translations": {
                                "ru": "Актёр прикрепил <w>постиж</w> к подбородку, чтобы перевоплотиться в пожилого персонажа."
                            }
                        },
                        {
                            "text": "She decided to wear a <w>postiche</w> to add volume and length to her hairstyle for the event.",
                            "target_lang_translations": {
                                "ru": "Она решила использовать <w>накладные волосы</w>, чтобы добавить объём и длину своей причёске для мероприятия."
                            }
                        }
                    ],
                    "synonyms": [
                        "hairpiece",
                        "wig",
                        "toupee",
                        "switch"
                    ],
                    "common_phrases": [

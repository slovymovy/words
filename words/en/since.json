{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "d06ddb8c-53c4-363a-ad1b-c43c4d488413",
                    "sense_definition": "From a specific point in time in the past until the present moment.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have been working here <w>since</w> six o'clock.",
                            "target_lang_translations": {
                                "ru": "Я работаю здесь <w>с</w> шести часов."
                            }
                        },
                        {
                            "text": "She has lived in this house <w>since</w> 2010.",
                            "target_lang_translations": {
                                "ru": "Она живет в этом доме <w>с</w> 2010 года."
                            }
                        }
                    ],
                    "antonyms": [
                        "until"
                    ],
                    "common_phrases": [
                        "since then",
                        "ever since",
                        "since when"
                    ],
                    "target_lang_definitions": {
                        "ru": "С определенного момента в прошлом до настоящего времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "предлог времени"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "a45cbb9a-76a6-3806-8534-ffa7ff60d72c",
                    "sense_definition": "From the past time when an event occurred until the present.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have loved you <w>since</w> I first met you.",
                            "target_lang_translations": {
                                "ru": "Я люблю тебя <w>с тех пор как</w> впервые встретил тебя."
                            }
                        },
                        {
                            "text": "It has been a long time <w>since</w> we last spoke.",
                            "target_lang_translations": {
                                "ru": "Прошло много времени <w>с тех пор как</w> мы в последний раз разговаривали."
                            }
                        }
                    ],
                    "antonyms": [
                        "until"
                    ],
                    "common_phrases": [
                        "ever since"
                    ],
                    "target_lang_definitions": {
                        "ru": "С того момента в прошлом, когда произошло событие, и до настоящего времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с тех пор как",
                                "target_lang_sense_clarification": "союз времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "330dc66f-3dcf-32e3-aba6-41d6e917a6bd",
                    "sense_definition": "For the reason that; because.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Since</w> you are here, you can help me with this task.",
                            "target_lang_translations": {
                                "ru": "<w>Поскольку</w> ты здесь, ты можешь помочь мне с этой задачей."
                            }
                        },
                        {
                            "text": "We decided to go home <w>since</w> it was getting very late.",
                            "target_lang_translations": {
                                "ru": "Мы решили пойти домой, <w>так как</w> становилось очень поздно."
                            }
                        }
                    ],
                    "synonyms": [
                        "because"
                    ],
                    "common_phrases": [
                        "since you asked"
                    ],
                    "target_lang_definitions": {
                        "ru": "По той причине, что; так как."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поскольку",
                                "target_lang_sense_clarification": "причина"
                            },
                            {
                                "target_lang_word": "так как",
                                "target_lang_sense_clarification": "причина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05822368-b24a-3035-85cb-9b66f29fe526",
                    "sense_definition": "When; used to refer to a past event.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you remember <w>since</w> we lay all night in the windmill?",
                            "target_lang_translations": {
                                "ru": "Ты помнишь, <w>как</w> мы всю ночь пролежали на мельнице?"
                            }
                        },
                        {
                            "text": "It is a long time <w>since</w> we were happy together.",
                            "target_lang_translations": {
                                "ru": "Прошло много времени с тех пор, <w>как</w> мы были счастливы вместе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "found in older literature or dialects"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Когда; используется для ссылки на событие в прошлом (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "устаревшее"
                            },
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7d03f70b-f5bd-34cb-9047-96802e568c61",
                    "sense_definition": "From a specified time in the past until now; subsequently.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He left town last year and hasn't been seen <w>since</w>.",
                            "target_lang_translations": {
                                "ru": "Он уехал из города в прошлом году, и <w>с тех пор</w> его не видели."
                            }
                        },
                        {
                            "text": "I met him once, but I have not heard from him <w>since</w>.",
                            "target_lang_translations": {
                                "ru": "Я встретил его однажды, но <w>с тех пор</w> ничего о нем не слышал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ever since",
                        "long since"
                    ],
                    "target_lang_definitions": {
                        "ru": "С определенного времени в прошлом до настоящего момента; впоследствии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с тех пор",
                                "target_lang_sense_clarification": "наречие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
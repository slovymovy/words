{
    "word_family": [
        "przytulić",
        "przytulny",
        "przytulnie",
        "przytulność",
        "przytulanka",
        "przytulenie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6155bab4-d2c5-3415-aaad-a41553d66f84",
                    "sense_definition": "Obejmować kogoś lub coś ramionami, aby wyrazić miłość, sympatię, czułość lub pocieszenie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_affection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama codziennie <przytula> swoje dzieci przed snem.",
                            "target_lang_translations": {
                                "ru": "Мама каждый день <обнимает> своих детей перед сном."
                            }
                        },
                        {
                            "text": "Chciałbym cię teraz mocno <przytulić> i pocieszyć.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы сейчас крепко тебя <обнять> и утешить."
                            }
                        }
                    ],
                    "synonyms": [
                        "obejmować",
                        "ściskać"
                    ],
                    "antonyms": [
                        "odpychać"
                    ],
                    "common_phrases": [
                        "przytulać do serca",
                        "przytulać na powitanie",
                        "przytulać mocno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнимать кого-то или что-то руками, выражая любовь, симпатию, нежность или утешение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнимать",
                                "target_lang_sense_clarification": "заключать в объятия"
                            },
                            {
                                "target_lang_word": "прижимать к себе",
                                "target_lang_sense_clarification": "крепко обнимать, удерживать рядом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c73a4c4-ddcc-31e0-9836-afc281f623aa",
                    "sense_definition": "Przylegać do kogoś lub czegoś ciałem, szukając bliskości, ciepła lub pocieszenia (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_affection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestraszony pies <przytulał się> do nogi swojego właściciela.",
                            "target_lang_translations": {
                                "ru": "Испуганная собака <прижималась> к ноге своего хозяина."
                            }
                        },
                        {
                            "text": "Wieczorami lubimy <przytulać się> do siebie na kanapie.",
                            "target_lang_translations": {
                                "ru": "По вечерам мы любим <обниматься> на диване."
                            }
                        }
                    ],
                    "synonyms": [
                        "tulić się",
                        "lgnąć"
                    ],
                    "antonyms": [
                        "odsuwać się"
                    ],
                    "common_phrases": [
                        "przytulać się do mamy",
                        "przytulać się do poduszki"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny (reflexive verb)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прижиматься к кому-то или чему-то телом в поисках близости, тепла или утешения (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прижиматься",
                                "target_lang_sense_clarification": "тесно прилегать, льнуть"
                            },
                            {
                                "target_lang_word": "обниматься",
                                "target_lang_sense_clarification": "обнимать друг друга"
                            },
                            {
                                "target_lang_word": "ластиться",
                                "target_lang_sense_clarification": "проявлять нежность (обычно о детях или животных)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e52d9dba-a33a-3904-b450-87e48d275875",
                    "sense_definition": "Dawać komuś schronienie, przyjmować pod swój dach lub otaczać opieką.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To schronisko <przytula> bezdomne zwierzęta i szuka im nowych domów.",
                            "target_lang_translations": {
                                "ru": "Этот приют <пригревает> бездомных животных и ищет им новые дома."
                            }
                        },
                        {
                            "text": "W czasie wojny rodzina ta <przytulała> uciekinierów w swojej piwnicy.",
                            "target_lang_translations": {
                                "ru": "Во время войны эта семья <укрывала> беженцев в своем подвале."
                            }
                        }
                    ],
                    "synonyms": [
                        "przygarniać",
                        "gościć"
                    ],
                    "antonyms": [
                        "wyrzucać",
                        "wyganiać"
                    ],
                    "common_phrases": [
                        "przytulać bezdomnych",
                        "przytulać sieroty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать кому-то убежище, принимать под свой кров или окружать заботой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давать приют",
                                "target_lang_sense_clarification": "предоставлять убежище"
                            },
                            {
                                "target_lang_word": "пригревать",
                                "target_lang_sense_clarification": "давать кров и заботу"
                            },
                            {
                                "target_lang_word": "укрывать",
                                "target_lang_sense_clarification": "прятать, давать убежище"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
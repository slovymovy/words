    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "78195a1a-6673-37a3-8847-bed26c58c665",
                    "sense_definition": "Iemand die aan het woord is storen, waardoor diegene moet stoppen met praten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "interrupt_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik je heel even <w>onderbreken</w> om een vraag te stellen?",
                            "target_lang_translations": {
                                "ru": "Могу я тебя на секунду <w>перебить</w>, чтобы задать вопрос?"
                            }
                        },
                        {
                            "text": "Het is niet beleefd om iemand te <w>onderbreken</w> tijdens een gesprek.",
                            "target_lang_translations": {
                                "ru": "Невежливо <w>перебивать</w> кого-то во время разговора."
                            }
                        }
                    ],
                    "synonyms": [
                        "schorsen"
                    ],
                    "antonyms": [
                        "laten uitpraten"
                    ],
                    "common_phrases": [
                        "iemand onderbreken",
                        "mag ik even onderbreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мешать говорящему, заставляя его прекратить речь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перебивать",
                                "target_lang_sense_clarification": "кого-либо в разговоре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53c10447-3206-3d22-a68e-11c3d0631100",
                    "sense_definition": "Een handeling, proces of toestand tijdelijk stillegen of stoppen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "interrupt_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De voorzitter besloot de vergadering te <w>onderbreken</w> voor een korte pauze.",
                            "target_lang_translations": {
                                "ru": "Председатель решил <w>прервать</w> собрание на короткий перерыв."
                            }
                        },
                        {
                            "text": "Een luid geluid <w>onderbrak</w> plotseling de stilte in de bibliotheek.",
                            "target_lang_translations": {
                                "ru": "Громкий звук внезапно <w>прервал</w> тишину в библиотеке."
                            }
                        }
                    ],
                    "synonyms": [
                        "schorsen",
                        "schorsen"
                    ],
                    "antonyms": [
                        "voortzetten",
                        "vervolgen"
                    ],
                    "common_phrases": [
                        "de stilte onderbreken",
                        "het werk onderbreken",
                        "een reis onderbreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временно останавливать действие, процесс или состояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прерывать",
                                "target_lang_sense_clarification": "действие, процесс"
                            },
                            {
                                "target_lang_word": "приостанавливать",
                                "target_lang_sense_clarification": "делать паузу"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "vermommen",
        "vermomming",
        "gemomd"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "52f080da-4a3f-39e7-bb87-c39996c46b08",
                    "sense_definition": "Een reden die wordt opgegeven om de werkelijke bedoeling te verbergen; een bedrieglijke schijn.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pretext_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vertrok vroeg van het feest onder het <w>mom</w> van een dringende afspraak.",
                            "target_lang_translations": {
                                "ru": "Он ушел с вечеринки рано под <w>предлогом</w> срочной встречи."
                            }
                        },
                        {
                            "text": "De regering voerde de nieuwe wet in onder het <w>mom</w> van veiligheid.",
                            "target_lang_translations": {
                                "ru": "Правительство ввело новый закон под <w>видом</w> обеспечения безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorwendsel",
                        "dekmantel",
                        "smoes",
                        "schijn"
                    ],
                    "common_phrases": [
                        "onder het mom van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причина, указываемая для сокрытия истинных намерений; обманчивая видимость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предлог",
                                "target_lang_sense_clarification": "ложная причина"
                            },
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "внешняя видимость, прикрытие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57eac9e3-767d-32e5-80c5-24b9a2ed7e25",
                    "sense_definition": "Een bedekking voor het gezicht om onherkenbaar te blijven.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mask_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De mysterieuze gast droeg een fluwelen <w>mom</w> tijdens het bal.",
                            "target_lang_translations": {
                                "ru": "Таинственный гость носил бархатную <w>маску</w> во время бала."
                            }
                        },
                        {
                            "text": "Achter zijn vrolijke <w>mom</w> verborg de clown veel verdriet.",
                            "target_lang_translations": {
                                "ru": "За своей веселой <w>маской</w> клоун скрывал много горя."
                            }
                        }
                    ],
                    "synonyms": [
                        "masker",
                        "gelaatsmasker"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "wordt tegenwoordig meestal 'masker' genoemd"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытие для лица, чтобы оставаться неузнаваемым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маска",
                                "target_lang_sense_clarification": "накладка на лицо"
                            },
                            {
                                "target_lang_word": "личина",
                                "target_lang_sense_clarification": "устаревшее или книжное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fe97c20-331a-3ddb-aa46-b6501dbcfa92",
                    "sense_definition": "Een zwaar, donker bier dat oorspronkelijk uit de Duitse stad Brunswijk komt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "beer_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zeventiende eeuw werd er veel <w>mom</w> geëxporteerd naar Nederland.",
                            "target_lang_translations": {
                                "ru": "В семнадцатом веке в Нидерланды экспортировалось много <w>мума</w>."
                            }
                        },
                        {
                            "text": "De liefhebber bestelde een glas <w>mom</w> om de historische smaak te proeven.",
                            "target_lang_translations": {
                                "ru": "Любитель заказал бокал <w>мума</w>, чтобы попробовать исторический вкус."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжелое темное пиво, родом из немецкого города Брауншвейг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мум",
                                "target_lang_sense_clarification": "исторический сорт пива"
                            }
                        ]
                    }
                }
            ]
        },
        {

                {
                    "sense_id": "2aae1813-aaa6-38a9-bbee-915994911d9e",
                    "sense_definition": "Not equal in size, amount, or quality.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "unequal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The partners contributed to the project in <w>unlike</w> amounts.",

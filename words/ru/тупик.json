    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e00a55d8-852f-31dd-aa74-0d0c3214dc1c",
                    "sense_definition": "Улица, переулок или дорога, не имеющие сквозного прохода или проезда.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "dead_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы случайно заехали в <w>тупик</w> и долго не могли развернуться на узкой дороге.",
                            "target_lang_translations": {
                                "en": "We accidentally drove into a <w>dead end</w> and couldn't turn around on the narrow road for a long time."
                            }
                        },
                        {
                            "text": "В конце улицы находится глухой <w>тупик</w>, за которым начинается парк.",
                            "target_lang_translations": {
                                "en": "At the end of the street there is a complete <w>dead end</w>, beyond which a park begins."
                            }
                        }
                    ],
                    "antonyms": [
                        "проезд"
                    ],
                    "common_phrases": [
                        "глухой тупик",
                        "железнодорожный тупик",
                        "заехать в тупик"
                    ],
                    "target_lang_definitions": {
                        "en": "A street, alley, or road that has no through passage or exit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dead end",
                                "target_lang_sense_clarification": "general term for a road with no exit"
                            },
                            {
                                "target_lang_word": "cul-de-sac",
                                "target_lang_sense_clarification": "a street closed at one end, often in a residential area"
                            },
                            {
                                "target_lang_word": "blind alley",
                                "target_lang_sense_clarification": "a path or road that leads nowhere"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42543c50-39d4-3f3a-ac1b-8b367a057e6c",
                    "sense_definition": "Безвыходное положение или ситуация, не имеющая благоприятной перспективы развития.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "dead_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Длительные переговоры зашли в <w>тупик</w>, так как стороны не хотели уступать друг другу.",
                            "target_lang_translations": {
                                "en": "The lengthy negotiations reached an <w>impasse</w> because the parties did not want to yield to each other."
                            }
                        },
                        {
                            "text": "Неожиданный вопрос экзаменатора поставил студента в <w>тупик</w>.",
                            "target_lang_translations": {
                                "en": "The examiner's unexpected question <w>stumped</w> the student."
                            }
                        }
                    ],
                    "synonyms": [
                        "безвыходность",
                        "безвыходное положение"
                    ],
                    "antonyms": [
                        "перспектива"
                    ],
                    "common_phrases": [
                        "зайти в тупик",
                        "поставить в тупик",
                        "вывести из тупика"
                    ],
                    "target_lang_definitions": {
                        "en": "A hopeless position or situation with no favorable prospect for development."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dead end",
                                "target_lang_sense_clarification": "a situation where no progress is possible"
                            },
                            {
                                "target_lang_word": "impasse",
                                "target_lang_sense_clarification": "a deadlock, especially in negotiations"
                            },
                            {
                                "target_lang_word": "stalemate",
                                "target_lang_sense_clarification": "a situation in which further action is blocked"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "058567f5-f6ed-3d8b-b94f-cb9533e48f43",
                    "sense_definition": "Морская птица семейства чистиковых с массивным, ярко окрашенным клювом.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На скалистом берегу сидел <w>тупик</w> и держал в клюве несколько маленьких рыбок.",
                            "target_lang_translations": {
                                "en": "A <w>puffin</w> sat on the rocky shore holding several small fish in its beak."
                            }
                        },
                        {
                            "text": "Атлантический <w>тупик</w> гнездится большими колониями на северных побережьях.",
                            "target_lang_translations": {
                                "en": "The Atlantic <w>puffin</w> nests in large colonies on northern coasts."
                            }
                        }
                    ],
                    "synonyms": [
                        "топорик"
                    ],
                    "common_phrases": [
                        "атлантический тупик"
                    ],
                    "target_lang_definitions": {
                        "en": "A seabird of the auk family with a massive, brightly colored beak."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "puffin",
                                "target_lang_sense_clarification": "seabird"
                            }
                        ]
                    }
                }
            ]
        }
    ]

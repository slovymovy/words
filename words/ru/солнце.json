            "senses": [
                {
                    "sense_id": "98d986ea-75e7-36d1-9e54-10d6bbf085f6",
                    "sense_definition": "Небесное светило шарообразной формы, которое мы видим на небе днем и которое освещает Землю.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "celestial_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Утром <w>солнце</w> встает на востоке.",
                            "target_lang_translations": {
                                "en": "In the morning, the <w>sun</w> rises in the east."
                            }
                        },
                        {
                            "text": "Сегодня на небе светит яркое <w>солнце</w>.",
                            "target_lang_translations": {
                                "en": "A bright <w>sun</w> is shining in the sky today."
                            }
                        },
                        {
                            "text": "Мы наблюдали красивый закат <w>солнца</w> у моря.",
                            "target_lang_translations": {
                                "en": "We watched the beautiful setting of the <w>sun</w> by the sea."
                            }
                        }
                    ],
                    "synonyms": [
                        "солнышко",
                        "дневное светило"
                    ],
                    "antonyms": [
                        "луна"
                    ],
                    "common_phrases": [
                        "яркое солнце",
                        "восход солнца",
                        "закат солнца",
                        "луч солнца"
                    ],
                    "target_lang_definitions": {
                        "en": "The celestial body of spherical shape that we see in the sky during the day and that illuminates the Earth."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sun",
                                "target_lang_sense_clarification": "celestial body"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b8dee0d-a605-3293-ab56-ea71567a45ab",
                    "sense_definition": "Свет и тепло, которые излучает это небесное тело.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "light_heat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой комнате всегда много <w>солнца</w>.",
                            "target_lang_translations": {
                                "en": "There is always a lot of <w>sun</w> in this room."
                            }
                        },
                        {
                            "text": "Врачи не рекомендуют долго лежать на <w>солнце</w>.",
                            "target_lang_translations": {
                                "en": "Doctors do not recommend lying in the <w>sun</w> for a long time."
                            }
                        },
                        {
                            "text": "Уйти с <w>солнца</w> в тень.",
                            "target_lang_translations": {
                                "en": "To go out of the <w>sun</w> into the shade."
                            }
                        }
                    ],
                    "synonyms": [
                        "солнечный свет"
                    ],
                    "antonyms": [
                        "тень",
                        "тьма"
                    ],
                    "common_phrases": [
                        "греться на солнце",
                        "место под солнцем",
                        "загорать на солнце"
                    ],
                    "target_lang_definitions": {
                        "en": "The light and heat emitted by this celestial body."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sun",
                                "target_lang_sense_clarification": "light and heat"
                            },
                            {
                                "target_lang_word": "sunshine",
                                "target_lang_sense_clarification": "direct sunlight"
                            },
                            {
                                "target_lang_word": "sunlight",
                                "target_lang_sense_clarification": "light from the sun"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c37cb8f1-c912-38e1-b88e-ac9efee6f94b",
                    "sense_definition": "Источник радости, счастья или жизни (обычно о человеке или явлении).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты мое <w>солнце</w>, без тебя мне очень грустно.",
                            "target_lang_translations": {
                                "en": "You are my <w>sunshine</w>, I am very sad without you."
                            }
                        },
                        {
                            "text": "Пушкин — это <w>солнце</w> русской поэзии.",
                            "target_lang_translations": {
                                "en": "Pushkin is the <w>sun</w> of Russian poetry."
                            }

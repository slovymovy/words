                {
                    "sense_id": "9f03cb3c-537f-3f6f-a7f6-45aa32d2a722",
                    "sense_definition": "Een korte reis, rondrit of uitstapje, vaak gemaakt voor het plezier.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We maken vandaag een mooie <w>toer</w> door de bergen met de auto.",
                            "target_lang_translations": {
                                "en": "We are taking a nice <w>tour</w> through the mountains by car today.",
                                "ru": "Сегодня мы совершим красивую <w>поездку</w> по горам на машине."
                            }
                        },
                        {
                            "text": "Tijdens onze vakantie hebben we elke dag een kleine <w>toer</w> gemaakt.",
                            "target_lang_translations": {
                                "en": "During our vacation, we went on a small <w>trip</w> every day.",
                                "ru": "Во время отпуска мы каждый день совершали небольшую <w>поездку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rit",
                        "tocht",
                        "rondrit",
                        "uitstapje"
                    ],
                    "common_phrases": [

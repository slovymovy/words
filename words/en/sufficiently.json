                    "sense_id": "b142f75b-bc89-39ec-a15b-61bd719ef20e",
                    "sense_definition": "To a degree that is enough for a particular purpose.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We could not find <w>sufficiently</w> long sticks to build the shelter.",
                            "target_lang_translations": {
                                "ru": "Мы не смогли найти <w>достаточно</w> длинные палки, чтобы построить укрытие."
                            }
                        },
                        {
                            "text": "Please ensure that the instructions are <w>sufficiently</w> clear for the students to understand.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, убедитесь, что инструкции <w>достаточно</w> понятны для студентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "adequately",
                        "enough"
                    ],
                    "antonyms": [
                        "insufficiently"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "135095a8-a3a4-3d8b-bc01-b022da54c2a5",
                    "sense_definition": "The power or ability to generate an outcome or perform a specific task.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new software has the <w>capability</w> to process large amounts of data very quickly.",
                            "target_lang_translations": {
                                "ru": "Новое программное обеспечение обладает <w>способностью</w> очень быстро обрабатывать большие объемы данных."
                            }
                        },
                        {
                            "text": "Our military <w>capability</w> has improved significantly over the last decade due to better training.",
                            "target_lang_translations": {
                                "ru": "Наш военный <w>потенциал</w> значительно вырос за последнее десятилетие благодаря лучшей подготовке."
                            }
                        }
                    ],
                    "synonyms": [
                        "ability",
                        "capacity",
                        "competence",
                        "potential",
                        "power"
                    ],
                    "antonyms": [
                        "inability",
                        "incapacity",
                        "incompetence"
                    ],
                    "common_phrases": [
                        "have the capability",
                        "military capability",
                        "nuclear capability",
                        "beyond capability",
                        "manufacturing capability"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сила или умение произвести результат или выполнить определенную задачу; наличие ресурсов для действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "способность",
                                "target_lang_sense_clarification": "свойство или умение выполнять действие"
                            },
                            {
                                "target_lang_word": "потенциал",
                                "target_lang_sense_clarification": "совокупность средств и возможностей (часто в военном или промышленном контексте)"
                            },
                            {
                                "target_lang_word": "возможность",
                                "target_lang_sense_clarification": "техническая характеристика или функция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f285dacc-b51d-3c4d-8468-b598c1b4d31f",
                    "sense_definition": "A digital token allowing a user or process to interact in a specified way with an object that is subject to access control.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The security kernel checks the process <w>capability</w> before granting access to the requested resource.",
                            "target_lang_translations": {
                                "ru": "Ядро безопасности проверяет <w>мандат</w> процесса перед предоставлением доступа к запрашиваемому ресурсу."
                            }
                        },
                        {
                            "text": "A file descriptor acts as a <w>capability</w> that permits specific operations on a file.",
                            "target_lang_translations": {
                                "ru": "Файловый дескриптор действует как <w>мандат</w>, который разрешает определенные операции с файлом."
                            }
                        }
                    ],
                    "synonyms": [
                        "token",
                        "permission",
                        "ticket"
                    ],
                    "common_phrases": [
                        "capability-based security",
                        "access capability"
                    ],
                    "target_lang_definitions": {
                        "ru": "Цифровой маркер или ключ, предоставляющий пользователю или процессу права на взаимодействие с защищенным объектом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мандат",
                                "target_lang_sense_clarification": "в контексте информационной безопасности"
                            },
                            {
                                "target_lang_word": "право доступа",
                                "target_lang_sense_clarification": "разрешение на выполнение операций"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "bbb3e556-aa6e-3960-b139-0f963b7df9ed",
                    "sense_definition": "Kierować działalnością instytucji, przedsiębiorstwa lub dysponować zasobami i ludźmi, podejmując decyzje dotyczące ich funkcjonowania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "management_authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy menedżer bardzo sprawnie <w>zarządza</w> naszym zespołem projektowym.",
                            "target_lang_translations": {
                                "ru": "Новый менеджер очень эффективно <w>управляет</w> нашей проектной командой."
                            }
                        },
                        {
                            "text": "W naszym małżeństwie to żona <w>zarządza</w> domowym budżetem i wydatkami.",
                            "target_lang_translations": {
                                "ru": "В нашем браке жена <w>распоряжается</w> семейным бюджетом и расходами."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierować",
                        "zawiadywać",
                        "gospodarować",
                        "dysponować"
                    ],
                    "common_phrases": [
                        "zarządzać firmą",
                        "zarządzać czasem",
                        "zarządzać zasobami",
                        "zarządzać projektem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Руководить деятельностью учреждения, предприятия или распоряжаться ресурсами и людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "руководить, контролировать"
                            },
                            {
                                "target_lang_word": "руководить",
                                "target_lang_sense_clarification": "направлять деятельность людей"
                            },
                            {
                                "target_lang_word": "распоряжаться",
                                "target_lang_sense_clarification": "ресурсами или бюджетом"

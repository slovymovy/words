            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "172683ee-2955-3c87-a9cb-b901f8c3446e",
                    "sense_definition": "В первую очередь, с преимуществом перед другими; на основе приоритета.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Заявки от постоянных клиентов обрабатываются <w>приоритетно</w>.",
                            "target_lang_translations": {
                                "en": "Applications from regular clients are processed <w>as a priority</w>."
                            }
                        },
                        {
                            "text": "Мы должны <w>приоритетно</w> решать вопросы безопасности сотрудников.",
                            "target_lang_translations": {
                                "en": "We must address employee safety issues <w>as a priority</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "первоочередно",
                        "преимущественно"
                    ],
                    "antonyms": [
                        "второстепенно"
                    ],
                    "common_phrases": [
                        "рассматривать приоритетно",
                        "финансировать приоритетно",
                        "обслуживать приоритетно"
                    ],
                    "target_lang_definitions": {
                        "en": "First and foremost, with precedence over others; on a priority basis."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "as a priority",
                                "target_lang_sense_clarification": "adverbial phrase indicating precedence"
                            },
                            {
                                "target_lang_word": "preferentially",
                                "target_lang_sense_clarification": "giving preference or advantage"
                            },
                            {
                                "target_lang_word": "with priority",
                                "target_lang_sense_clarification": "manner of processing or treating"
                            }
                        ]
                    }
                }
            ]
        },
        {

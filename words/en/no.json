{
    "word_family": [
        "nobody",
        "nothing",
        "nowhere",
        "none"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "efb8963b-3a05-300d-a437-2ae2524d6da1",
                    "sense_definition": "Used to indicate that there is not any of a particular thing or person.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "det_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is <w>no</w> milk left in the fridge, so we need to buy some.",
                            "target_lang_translations": {
                                "ru": "В холодильнике <w>нет</w> молока, поэтому нам нужно купить его."
                            }
                        },
                        {
                            "text": "I have <w>no</w> idea where I put my keys yesterday.",
                            "target_lang_translations": {
                                "ru": "Я понятия <w>не</w> имею, куда я положил свои ключи вчера."
                            }
                        },
                        {
                            "text": "<w>No</w> students came to the extra class this morning.",
                            "target_lang_translations": {
                                "ru": "<w>Ни один</w> студент не пришел на дополнительное занятие сегодня утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "zero#Determiner",
                        "not one#Determiner"
                    ],
                    "antonyms": [
                        "any#Determiner",
                        "some#Determiner",
                        "many#Determiner",
                        "numerous#Determiner"
                    ],
                    "common_phrases": [
                        "no problem",
                        "no doubt",
                        "no way",
                        "no one",
                        "no time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на отсутствие какого-либо предмета или лица."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "отсутствие чего-либо"
                            },
                            {
                                "target_lang_word": "никакой",
                                "target_lang_sense_clarification": "усиленное отрицание"
                            },
                            {
                                "target_lang_word": "ни один",
                                "target_lang_sense_clarification": "ни единого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "946dece4-d7ec-332e-bf59-d7d742a9f2da",
                    "sense_definition": "Used in notices or instructions to indicate that something is forbidden or not allowed.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "det_prohibition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a strict <w>no</w> smoking policy in this building.",
                            "target_lang_translations": {
                                "ru": "В этом здании действует строгий запрет: <w>не</w> курить."
                            }
                        },
                        {
                            "text": "The sign on the gate clearly said <w>no</w> entry.",
                            "target_lang_translations": {
                                "ru": "Табличка на воротах ясно гласила: «Входа <w>нет</w>»."
                            }
                        }
                    ],
                    "common_phrases": [
                        "no smoking",
                        "no parking",
                        "no entry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в объявлениях или инструкциях для указания на то, что действие запрещено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспрещается",
                                "target_lang_sense_clarification": "официальный запрет"
                            },
                            {
                                "target_lang_word": "не",
                                "target_lang_sense_clarification": "запрет действия"
                            },
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "отсутствие возможности (напр. входа)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e12df8c2-dd17-32c5-a380-fa10be273832",
                    "sense_definition": "Used to emphasize that someone or something is not the type of person or thing mentioned, often implying the opposite.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "det_emphatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>no</w> fool; he understands exactly what is happening.",
                            "target_lang_translations": {
                                "ru": "Он <w>вовсе не</w> дурак; он точно понимает, что происходит."
                            }
                        },
                        {
                            "text": "It was <w>no</w> accident that they met at the cafe that day.",
                            "target_lang_translations": {
                                "ru": "Это была <w>не</w> случайность, что они встретились в кафе в тот день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "no fool",
                        "no accident",
                        "no stranger to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы подчеркнуть, что кто-то или что-то не является упомянутым типом лица или вещи, часто подразумевая обратное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вовсе не",
                                "target_lang_sense_clarification": "усиление отрицания"
                            },
                            {
                                "target_lang_word": "совсем не",
                                "target_lang_sense_clarification": "полное отрицание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "db8eee99-bb38-3c19-aad9-bf44c998e2fd",
                    "sense_definition": "Used to give a negative response to a question, request, or offer, or to express disagreement.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intj_refusal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>No</w>, I do not want to go to the cinema tonight.",
                            "target_lang_translations": {
                                "ru": "<w>Нет</w>, я не хочу идти в кино сегодня вечером."
                            }
                        },
                        {
                            "text": "\"Did you finish your homework?\" \"<w>No</w>, not yet.\"",
                            "target_lang_translations": {
                                "ru": "«Ты закончил домашнюю работу?» — «<w>Нет</w>, еще нет»."
                            }
                        }
                    ],
                    "synonyms": [
                        "nope",
                        "nah",
                        "nay"
                    ],
                    "antonyms": [
                        "yes",
                        "yea",
                        "aye",
                        "maybe"
                    ],
                    "common_phrases": [
                        "oh no",
                        "no thanks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для отрицательного ответа на вопрос, просьбу или предложение, или для выражения несогласия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "отрицательный ответ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bc2c2c7-908f-3344-808e-be2eab8e99d1",
                    "sense_definition": "Used to show agreement with a negative statement or question.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "intj_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "\"You don't like spicy food, do you?\" \"<w>No</w>, I hate it.\"",
                            "target_lang_translations": {
                                "ru": "«Ты ведь не любишь острую еду?» — «<w>Нет</w>, я её ненавижу»."
                            }
                        },
                        {
                            "text": "\"He isn't coming, is he?\" \"<w>No</w>, he stayed home.\"",
                            "target_lang_translations": {
                                "ru": "«Он ведь не придет?» — «<w>Нет</w>, он остался дома»."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения согласия с отрицательным утверждением или вопросом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "подтверждение отрицания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4a9ae750-582d-3cce-b1fd-98d31fffb66d",
                    "sense_definition": "Used before comparative adjectives and adverbs to mean 'not at all' or 'to no extent'.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adv_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new model is <w>no</w> better than the old one.",
                            "target_lang_translations": {
                                "ru": "Новая модель <w>ничуть не</w> лучше старой."
                            }
                        },
                        {
                            "text": "We can wait <w>no</w> longer for the delivery to arrive.",
                            "target_lang_translations": {
                                "ru": "Мы <w>больше не</w> можем ждать прибытия доставки."
                            }
                        },
                        {
                            "text": "This task is <w>no</w> less difficult than the previous one.",
                            "target_lang_translations": {
                                "ru": "Эта задача <w>не</w> менее сложная, чем предыдущая."
                            }
                        }
                    ],
                    "common_phrases": [
                        "no more",
                        "no less",
                        "no longer",
                        "no further",
                        "no sooner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется перед сравнительными прилагательными и наречиями в значении «совсем не» или «нисколько не»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничуть не",
                                "target_lang_sense_clarification": "нисколько не"
                            },
                            {
                                "target_lang_word": "не",
                                "target_lang_sense_clarification": "отрицание степени"
                            },
                            {
                                "target_lang_word": "больше не",
                                "target_lang_sense_clarification": "прекращение действия во времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f032789-8154-353e-8b84-b604479bcd65",
                    "sense_definition": "Used before certain adjectives to indicate the negative, often implying something is useless or invalid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adv_idiom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This broken toaster is <w>no</w> good to anyone.",
                            "target_lang_translations": {
                                "ru": "Этот сломанный тостер никому <w>не</w> нужен."
                            }
                        },
                        {
                            "text": "It is <w>no</w> different from what we have done before.",
                            "target_lang_translations": {
                                "ru": "Это <w>ничем не</w> отличается от того, что мы делали раньше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "no good",
                        "no different"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется перед определенными прилагательными для обозначения отрицания, часто подразумевая бесполезность или недействительность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не",
                                "target_lang_sense_clarification": "отрицание качества"
                            },
                            {
                                "target_lang_word": "ни",
                                "target_lang_sense_clarification": "усиление отрицания в идиомах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4b05b5bf-6089-3e8c-b202-b0729b323a9d",
                    "sense_definition": "A negative answer or decision; a refusal.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_refusal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was hoping for a yes, but she gave me a definite <w>no</w>.",
                            "target_lang_translations": {
                                "ru": "Я надеялся на согласие, но она ответила мне категорическим <w>нет</w>."
                            }
                        },
                        {
                            "text": "We won't take <w>no</w> for an answer.",
                            "target_lang_translations": {
                                "ru": "Мы не примем <w>отказа</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nah",
                        "nope"
                    ],
                    "antonyms": [
                        "yes",
                        "aye",
                        "yea"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрицательный ответ или решение; отказ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "отрицательный ответ"
                            },
                            {
                                "target_lang_word": "отказ",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "844ceac6-6076-35bd-9cff-20feeeaa553d",
                    "sense_definition": "A vote against a proposal or motion.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_vote",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>noes</w> have it, and the motion is rejected.",
                            "target_lang_translations": {
                                "ru": "<w>Голоса против</w> победили, и предложение отклонено."
                            }
                        },
                        {
                            "text": "There were thirty yeses and only two <w>nos</w>.",
                            "target_lang_translations": {
                                "ru": "Было тридцать голосов «за» и только два <w>голоса против</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Голос против предложения или выдвижения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голос против",
                                "target_lang_sense_clarification": "на голосовании"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7c680fda-523f-3b00-b779-ee480e9e93d1",
                    "sense_definition": "A lake in South Sudan.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Lake <w>No</w> is located just north of the Sudd swamp.",
                            "target_lang_translations": {
                                "ru": "Озеро <w>Но</w> расположено к северу от болота Судд."
                            }
                        },
                        {
                            "text": "The White Nile flows into Lake <w>No</w>.",
                            "target_lang_translations": {
                                "ru": "Белый Нил впадает в озеро <w>Но</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Озеро в Южном Судане."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Но",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "684e9e25-8c49-3685-a9fb-d1de97ae1c9e",
                    "sense_definition": "Kategoria gramatyczna przymiotników i przysłówków służąca do wyrażania natężenia cechy (równy, wyższy, najwyższy).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przymiotnik „lepszy” to <w>stopień</w> wyższy od słowa „dobry”.",
                            "target_lang_translations": {
                                "ru": "Прилагательное «лучший» — это сравнительная <w>степень</w> от слова «хороший»."
                            }
                        },
                        {
                            "text": "W tym zdaniu musisz użyć przymiotnika w <w>stopniu</w> najwyższym.",
                            "target_lang_translations": {
                                "ru": "В этом предложении нужно использовать прилагательное в превосходной <w>степени</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stopień równy",
                        "stopień wyższy",
                        "stopień najwyższy",
                        "stopniowanie przymiotników"

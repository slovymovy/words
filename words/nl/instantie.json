                {
                    "sense_id": "41c43887-9381-3ed5-bd2d-2a13f9fe987e",
                    "sense_definition": "Een officiële organisatie of overheidsorgaan met een specifieke taak of bevoegdheid.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is een <w>instantie</w> die zich met de ruimtelijke ordening bezighoudt.",
                            "target_lang_translations": {
                                "ru": "Это <w>инстанция</w>, которая занимается территориальным планированием."
                            }
                        },
                        {
                            "text": "Je moet bij de bevoegde <w>instantie</w> een vergunning aanvragen voor de verbouwing.",
                            "target_lang_translations": {
                                "ru": "Вам нужно запросить разрешение на перестройку в компетентной <w>инстанции</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "instelling",
                        "institutie"
                    ],
                    "common_phrases": [
                        "bevoegde instantie",
                        "officiële instantie",
                        "overheidsinstantie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная организация или государственный орган с определенными задачами или полномочиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инстанция",

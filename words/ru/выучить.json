    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8f405798-5cb0-3af6-913f-666cb94aecbb",
                    "sense_definition": "Запомнить, усвоить что-либо в процессе обучения (текст, правило, иностранный язык).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент должен <w>выучить</w> все новые слова к завтрашнему дню.",
                            "target_lang_translations": {
                                "en": "The student must <w>learn</w> all the new words by tomorrow."
                            }
                        },
                        {
                            "text": "Актёру пришлось <w>выучить</w> свою роль наизусть за одну неделю.",
                            "target_lang_translations": {
                                "en": "The actor had to <w>learn</w> his role by heart in one week."
                            }
                        }
                    ],
                    "synonyms": [
                        "заучить"
                    ],
                    "antonyms": [
                        "забыть"
                    ],
                    "common_phrases": [
                        "выучить наизусть",
                        "выучить урок",
                        "выучить язык",
                        "выучить стихотворение"
                    ],
                    "target_lang_definitions": {
                        "en": "To memorize or master something through study (text, rule, foreign language)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "learn",
                                "target_lang_sense_clarification": "to acquire knowledge of or skill in"
                            },
                            {
                                "target_lang_word": "memorize",
                                "target_lang_sense_clarification": "to commit to memory"
                            },
                            {
                                "target_lang_word": "master",
                                "target_lang_sense_clarification": "to learn completely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70c3f37e-9a36-3e49-8363-d3958d47a884",
                    "sense_definition": "Передать кому-либо знания, научить кого-либо чему-либо.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка обещала <w>выучить</w> внука читать и писать до школы.",
                            "target_lang_translations": {
                                "en": "Grandma promised to <w>teach</w> her grandson to read and write before school."
                            }
                        },
                        {
                            "text": "Родители много работали, чтобы <w>выучить</w> детей и дать им хорошее образование.",
                            "target_lang_translations": {
                                "en": "The parents worked hard to <w>educate</w> their children and give them a good education."
                            }
                        }
                    ],
                    "synonyms": [
                        "обучить",
                        "научить"
                    ],
                    "common_phrases": [
                        "выучить детей",
                        "выучить читать"
                    ],
                    "target_lang_definitions": {
                        "en": "To impart knowledge to someone, to teach someone something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "teach",
                                "target_lang_sense_clarification": "to give instruction to someone"
                            },
                            {
                                "target_lang_word": "educate",
                                "target_lang_sense_clarification": "to provide schooling for"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
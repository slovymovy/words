    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f49fe2c2-8d32-320b-a68c-5c3dc4ca9f1a",
                    "sense_definition": "Waarde of belang toekennen aan iets.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel mensen <w>hechten</w> grote waarde aan eerlijkheid in een relatie.",
                            "target_lang_translations": {
                                "en": "Many people <w>attach</w> great value to honesty in a relationship."
                            }
                        },
                        {
                            "text": "Je moet niet te veel geloof <w>hechten</w> aan alles wat er in de krant staat.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>attach</w> too much belief to everything that is in the newspaper."
                            }
                        }
                    ],
                    "synonyms": [
                        "toekennen"
                    ],
                    "common_phrases": [
                        "waarde hechten aan",
                        "belang hechten aan",
                        "geloof hechten aan"
                    ],
                    "target_lang_definitions": {
                        "en": "To assign value or importance to something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attach",
                                "target_lang_sense_clarification": "to attribute importance or value to something"
                            },
                            {
                                "target_lang_word": "place",
                                "target_lang_sense_clarification": "used in phrases like 'place value on'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09e1d3fa-f3ca-36bc-83a8-076eab7f3d86",
                    "sense_definition": "Een wond of snede dichtmaken met behulp van naald en draad.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts moest de diepe snee in zijn been met vijf steekjes <w>hechten</w>.",
                            "target_lang_translations": {
                                "en": "The doctor had to <w>stitch</w> the deep cut in his leg with five stitches."
                            }
                        },
                        {
                            "text": "Na de operatie werd de wond zorgvuldig <w>gehecht</w> om littekens te voorkomen.",
                            "target_lang_translations": {
                                "en": "After the surgery, the wound was carefully <w>sutured</w> to prevent scarring."
                            }
                        }
                    ],
                    "synonyms": [
                        "naaien",
                        "dichtnaaien"
                    ],
                    "common_phrases": [
                        "een wond hechten",
                        "laten hechten"
                    ],
                    "target_lang_definitions": {
                        "en": "To close a wound or cut using a needle and thread."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stitch",
                                "target_lang_sense_clarification": "common term for closing a wound"
                            },
                            {
                                "target_lang_word": "suture",
                                "target_lang_sense_clarification": "medical term for closing a wound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9495c859-6a03-3a01-aef9-dd364ff89797",
                    "sense_definition": "Een emotionele band vormen met iemand of een omgeving.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional_bond",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is normaal dat jonge kinderen zich sterk aan hun ouders <w>hechten</w>.",
                            "target_lang_translations": {
                                "en": "It is normal for young children to strongly <w>bond</w> with their parents."
                            }
                        },
                        {
                            "text": "Ik probeer me niet te veel aan deze plek te <w>hechten</w> omdat ik binnenkort verhuis.",
                            "target_lang_translations": {
                                "en": "I try not to <w>get attached</w> to this place too much because I am moving soon."
                            }
                        }
                    ],
                    "synonyms": [
                        "binden"
                    ],
                    "common_phrases": [
                        "zich hechten aan"
                    ],
                    "target_lang_definitions": {
                        "en": "To form an emotional bond with someone or an environment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bond",
                                "target_lang_sense_clarification": "to form a close relationship"
                            },
                            {
                                "target_lang_word": "attach",
                                "target_lang_sense_clarification": "usually reflexive: to become emotionally connected"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d54774e4-0e02-3ef4-8542-f1a6ee0af622",
                    "sense_definition": "Vast blijven zitten aan een oppervlak of materiaal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_bond",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zorg dat de ondergrond schoon is, anders zal de verf niet goed <w>hechten</w>.",
                            "target_lang_translations": {
                                "en": "Ensure the surface is clean, otherwise the paint will not <w>adhere</w> well."
                            }
                        },
                        {
                            "text": "Deze speciale lijm kan materialen aan elkaar <w>hechten</w> die normaal niet plakken.",
                            "target_lang_translations": {
                                "en": "This special glue can <w>bond</w> materials together that normally don't stick."
                            }
                        }
                    ],
                    "synonyms": [
                        "plakken",
                        "kleven",
                        "vastmaken"
                    ],
                    "antonyms": [
                        "loslaten"
                    ],
                    "common_phrases": [
                        "goed hechten",
                        "niet hechten"
                    ],
                    "target_lang_definitions": {
                        "en": "To remain fixed to a surface or material."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "adhere",
                                "target_lang_sense_clarification": "to stick firmly to a surface"
                            },
                            {
                                "target_lang_word": "bond",
                                "target_lang_sense_clarification": "to join materials together physically"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3232e1f8-c3fb-3495-a743-83d72a01a905",
                    "sense_definition": "To increase the speed of a motor, or to operate at a higher speed.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "engine_speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>revved</w> the engine to warm it up before driving.",
                            "target_lang_translations": {
                                "ru": "Он <w>погазовал</w>, чтобы прогреть двигатель перед поездкой."
                            }
                        },
                        {
                            "text": "You could hear the cars <w>revving</w> loudly at the starting line.",
                            "target_lang_translations": {
                                "ru": "Было слышно, как машины громко <w>газуют</w> на стартовой линии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rev up",
                        "rev the engine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличивать скорость работы двигателя или работать на повышенных оборотах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газовать",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "увеличивать обороты",
                                "target_lang_sense_clarification": "двигателя"
                            }
                        ]
                    }
                }
            ]
        },

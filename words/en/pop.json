                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выделяться",
                                "target_lang_sense_clarification": "быть заметным"
                            },
                            {
                                "target_lang_word": "бросаться в глаза",
                                "target_lang_sense_clarification": "привлекать внимание"
                            }
                        ]
                    }
                }
            ]
        },
        {

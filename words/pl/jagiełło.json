                    "sense_id": "a665b9ed-0537-39b0-80c6-bf05361324b2",
                    "sense_definition": "Władysław II Jagiełło, wielki książę litewski i król Polski, założyciel dynastii Jagiellonów",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "historical_figure",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "Pomnik króla Władysława <w>Jagiełły</w> znajduje się w centralnym punkcie parku.",
                            "target_lang_translations": {
                                "ru": "Памятник королю Владиславу <w>Ягайло</w> находится в центре парка."
                            }
                        },
                        {
                            "text": "Zwycięstwo pod Grunwaldem było największym sukcesem militarnym <w>Jagiełły</w>.",
                            "target_lang_translations": {
                                "ru": "Победа при Грюнвальде была крупнейшим военным успехом <w>Ягайло</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "król Władysław Jagiełło",
                        "pomnik Jagiełły",
                        "czasy Jagiełły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Владислав II Ягайло, великий князь литовский и король польский, основатель династии Ягеллонов"
                    },
                    "translations": {

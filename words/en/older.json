                {
                    "sense_id": "fc912b42-f3f9-3334-b45a-0f4a0b64723b",
                    "sense_definition": "Comparative form of <w>old</w>: having lived or existed for a longer time than someone or something else.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "age_comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>older</w> brother helps me with my homework every evening.",
                            "target_lang_translations": {
                                "ru": "Мой <w>старший</w> брат помогает мне с домашним заданием каждый вечер."
                            }
                        },
                        {
                            "text": "She looks much <w>older</w> than her actual age.",
                            "target_lang_translations": {
                                "ru": "Она выглядит намного <w>старше</w> своего настоящего возраста."
                            }
                        }
                    ],
                    "synonyms": [
                        "elder",
                        "senior"
                    ],
                    "antonyms": [
                        "younger"
                    ],
                    "common_phrases": [
                        "older than",
                        "get older",
                        "grow older",
                        "older brother",
                        "older sister"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "comparative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сравнительная степень прилагательного \"old\": проживший или существующий дольше, чем кто-то или что-то другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "старше",

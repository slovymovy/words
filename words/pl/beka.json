                    }
                },
                {
                    "sense_id": "0ad7675f-d1a9-3c46-b7d5-bb3e931302f1",
                    "sense_definition": "Osoba bardzo otyła.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_desc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W szkole przezywali go <beką>, co było bardzo przykre.",
                            "target_lang_translations": {
                                "ru": "В школе его обзывали <w>бочкой</w>, что было очень обидно."
                            }
                        },
                        {
                            "text": "Ta <beka> ledwo zmieściła się w drzwiach autobusu.",
                            "target_lang_translations": {
                                "ru": "Эта <w>туша</w> едва пролезла в двери автобуса."
                            }
                        }
                    ],
                    "synonyms": [
                        "grubas",
                        "baryła",
                        "tłuścioch"

                        },
                        {
                            "trait_type": "regional",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who mindlessly and annoyingly repeats others' words or actions without having their own opinion."
                    },
                    "translations": {

                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "art_paint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Artysta wycisnął <olej> na paletę i zaczął mieszać kolory.",
                            "target_lang_translations": {
                                "ru": "Художник выдавил <масло> на палитру и начал смешивать цвета."
                            }
                        },
                        {
                            "text": "Malowanie <olejem> wymaga cierpliwości, ponieważ farba długo schnie.",
                            "target_lang_translations": {
                                "ru": "Рисование <маслом> требует терпения, так как краска долго сохнет."
                            }
                        }
                    ],
                    "synonyms": [
                        "farba olejna"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for farba olejna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Художественная краска, в которой пигмент взвешен в масле."

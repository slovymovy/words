            "senses": [
                {
                    "sense_id": "b1776f21-dce2-309a-bd95-ce7c04fe5e0b",
                    "sense_definition": "Tytuł dawnych władców Rosji, Bułgarii lub Serbii, odpowiadający randze cesarza lub króla; władca noszący ten tytuł.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ruler_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W pokoju gubernatora wisiał wielki portret <w>cara</w>.",
                            "target_lang_translations": {
                                "ru": "В комнате губернатора висел большой портрет <w>царя</w>."
                            }
                        },
                        {
                            "text": "Mikołaj II był ostatnim <w>carem</w> Rosji i abdykował w 1917 roku.",
                            "target_lang_translations": {
                                "ru": "Николай II был последним <w>царём</w> России и отрёкся от престола в 1917 году."
                            }
                        }
                    ],
                    "synonyms": [
                        "cesarz",
                        "batiuszka"
                    ],
                    "common_phrases": [
                        "car Rosji",
                        "za cara",
                        "ostatni car"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул бывших правителей России, Болгарии или Сербии, соответствующий рангу императора или короля; правитель, носящий этот титул."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "царь",
                                "target_lang_sense_clarification": "титул монарха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d5bbc1f-de96-3443-93ac-b63ff85822b7",
                    "sense_definition": "Regionalna nazwa lobelii wodnej, rzadkiej rośliny wodnej występującej w czystych jeziorach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miejscowi rybacy nazywają tę chronioną roślinę <w>carem</w>.",
                            "target_lang_translations": {
                                "ru": "Местные рыбаки называют это охраняемое растение <w>лобелией</w>."
                            }
                        },
                        {
                            "text": "W przejrzystej wodzie jeziora można dostrzec rzadkiego <w>cara</w>.",
                            "target_lang_translations": {
                                "ru": "В прозрачной воде озера можно заметить редкую <w>лобелию</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "nazwa ludowa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное название лобелии водной — редкого водного растения, встречающегося в чистых озерах."
                    },
                    "translations": {

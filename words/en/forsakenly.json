{
    "word_family": [
        "forsake",
        "forsaken",
        "forsakenness"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "540addd7-a74f-35db-89ca-a309234c02e0",
                    "sense_definition": "In a manner that appears abandoned, deserted, or lonely.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "abandonment_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old, ruined farmhouse stood <w>forsakenly</w> in the middle of the overgrown field.",
                            "target_lang_translations": {
                                "ru": "Старый разрушенный фермерский дом <w>сиротливо</w> стоял посреди заросшего поля."
                            }
                        },
                        {
                            "text": "After the guests departed, the empty chairs sat <w>forsakenly</w> around the table.",
                            "target_lang_translations": {
                                "ru": "После ухода гостей пустые стулья <w>одиноко</w> стояли вокруг стола."
                            }
                        }
                    ],
                    "synonyms": [
                        "desolately",
                        "forlornly",
                        "solitarily"
                    ],
                    "common_phrases": [
                        "stood-forsakenly",
                        "sat-forsakenly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Таким образом, который кажется заброшенным, пустынным или одиноким."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сиротливо",
                                "target_lang_sense_clarification": "вызывая чувство одиночества и покинутости"
                            },
                            {
                                "target_lang_word": "заброшенно",
                                "target_lang_sense_clarification": "имея необитаемый, покинутый вид"
                            },
                            {
                                "target_lang_word": "одиноко",
                                "target_lang_sense_clarification": "находясь в изоляции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a99d3ca2-ca3b-35d7-bb65-1452a37e4a66",
                    "sense_definition": "In a helpless or wretched manner, often reflecting a feeling of being abandoned.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "abandonment_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lost puppy whined <w>forsakenly</w> until a passerby stopped to help.",
                            "target_lang_translations": {
                                "ru": "Потерявшийся щенок <w>жалобно</w> скулил, пока прохожий не остановился помочь."
                            }
                        },
                        {
                            "text": "He looked around <w>forsakenly</w>, realizing that no one was coming to his aid.",
                            "target_lang_translations": {
                                "ru": "Он <w>потерянно</w> огляделся, понимая, что никто не придет ему на помощь."
                            }
                        }
                    ],
                    "synonyms": [
                        "helplessly",
                        "wretchedly",
                        "miserably"
                    ],
                    "common_phrases": [
                        "cried-forsakenly",
                        "wept-forsakenly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Беспомощно или несчастно, часто отражая чувство покинутости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потерянно",
                                "target_lang_sense_clarification": "выражая растерянность и одиночество"
                            },
                            {
                                "target_lang_word": "безнадежно",
                                "target_lang_sense_clarification": "без надежды на помощь"
                            },
                            {
                                "target_lang_word": "жалобно",
                                "target_lang_sense_clarification": "вызывая жалость (о звуках или виде)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
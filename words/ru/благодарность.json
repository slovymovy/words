            "senses": [
                {
                    "sense_id": "9a4dc01d-a3f8-3fb3-a994-05ebf64edb10",
                    "sense_definition": "Чувство признательности за сделанное добро, оказанное внимание или услугу.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу выразить вам огромную <w>благодарность</w> за вашу помощь.",
                            "target_lang_translations": {
                                "en": "I want to express my huge <w>gratitude</w> to you for your help."
                            }
                        },
                        {
                            "text": "В знак <w>благодарности</w> мы подарили ей красивый букет цветов.",
                            "target_lang_translations": {
                                "en": "As a sign of <w>gratitude</w>, we gave her a beautiful bouquet of flowers."
                            }
                        }
                    ],
                    "synonyms": [
                        "признательность"
                    ],
                    "antonyms": [
                        "неблагодарность"
                    ],
                    "common_phrases": [
                        "выразить благодарность",
                        "в знак благодарности",
                        "слова благодарности",
                        "чувство благодарности"
                    ],
                    "target_lang_definitions": {
                        "en": "A feeling of appreciation for kindness shown, attention given, or a service rendered."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gratitude",
                                "target_lang_sense_clarification": "feeling of thankfulness"
                            },
                            {
                                "target_lang_word": "appreciation",
                                "target_lang_sense_clarification": "recognition of value or quality"
                            },
                            {
                                "target_lang_word": "thanks",
                                "target_lang_sense_clarification": "expression of gratitude"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b3acad8-3c9a-3941-a458-533c1304be41",
                    "sense_definition": "Официальная мера поощрения за хорошую работу или особые заслуги.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "award",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор завода объявил сотруднику <w>благодарность</w> за отличную работу.",
                            "target_lang_translations": {
                                "en": "The factory director issued a <w>commendation</w> to the employee for excellent work."
                            }
                        },
                        {
                            "text": "В его трудовой книжке записана <w>благодарность</w> от руководства компании.",
                            "target_lang_translations": {
                                "en": "A <w>commendation</w> from the company management is recorded in his employment record book."
                            }
                        }
                    ],
                    "synonyms": [
                        "поощрение"
                    ],
                    "common_phrases": [
                        "объявить благодарность",
                        "получить благодарность"
                    ],
                    "target_lang_definitions": {
                        "en": "An official measure of encouragement for good work or special merits."
                    },
                    "translations": {

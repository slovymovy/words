                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>begin</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het boek worden verschillende <w>beginnen</w> beschreven.",
                            "target_lang_translations": {
                                "ru": "В книге описываются различные <w>начала</w>."
                            }
                        },
                        {
                            "text": "Niet alle <w>beginnen</w> zijn even makkelijk.",
                            "target_lang_translations": {
                                "ru": "Не все <w>начала</w> одинаково легки."
                            }
                        }
                    ],
                    "traits": [

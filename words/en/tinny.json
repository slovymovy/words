    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a7bf6542-129b-3a06-af65-e045a75ce3e9",
                    "sense_definition": "Pertaining to a thin, unpleasant, metallic sound that lacks depth or resonance.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "sound_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The music sounded terrible coming from the <w>tinny</w> speakers.",
                            "target_lang_translations": {
                                "ru": "Музыка звучала ужасно из этих <w>дребезжащих</w> колонок."
                            }
                        },
                        {
                            "text": "Her voice sounded <w>tinny</w> and distant over the old telephone connection.",
                            "target_lang_translations": {
                                "ru": "Её голос звучал <w>жестяным</w> и далеким из-за старой телефонной связи."
                            }
                        }
                    ],
                    "synonyms": [
                        "metallic",
                        "thin",
                        "hollow",
                        "harsh"
                    ],
                    "antonyms": [
                        "resonant",
                        "deep",
                        "rich"
                    ],
                    "common_phrases": [
                        "tinny sound",
                        "tinny voice",
                        "tinny speakers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к тонкому, неприятному, металлическому звуку, лишенному глубины или резонанса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жестяной",
                                "target_lang_sense_clarification": "о звуке или голосе"
                            },
                            {
                                "target_lang_word": "дребезжащий",
                                "target_lang_sense_clarification": "о некачественном звуке динамиков"
                            },
                            {
                                "target_lang_word": "металлический",
                                "target_lang_sense_clarification": "о тембре голоса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fb51cd8-3204-377e-befc-378edd1e9f11",
                    "sense_definition": "Appearing cheap, flimsy, or of poor quality, often resembling thin metal.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The souvenir shop was full of <w>tinny</w> trinkets that looked like they would break instantly.",
                            "target_lang_translations": {
                                "ru": "Сувенирная лавка была полна <w>хлипких</w> безделушек, которые выглядели так, будто сломаются мгновенно."
                            }
                        },
                        {
                            "text": "He refused to buy the car because the door made a cheap, <w>tinny</w> noise when it closed.",
                            "target_lang_translations": {
                                "ru": "Он отказался покупать машину, потому что дверь издавала дешевый <w>жестяной</w> звук при закрытии."
                            }
                        }
                    ],
                    "synonyms": [
                        "cheap",
                        "flimsy",
                        "shoddy",
                        "insubstantial"
                    ],
                    "antonyms": [
                        "solid",
                        "substantial",
                        "robust",
                        "high-quality"
                    ],
                    "common_phrases": [
                        "tinny construction",
                        "tinny jewelry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выглядящий дешевым, хлипким или некачественным, часто напоминающий тонкий металл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хлипкий",
                                "target_lang_sense_clarification": "ненадежный, ломкий"
                            },
                            {
                                "target_lang_word": "дешёвый",
                                "target_lang_sense_clarification": "низкого качества"
                            },
                            {
                                "target_lang_word": "жестяной",
                                "target_lang_sense_clarification": "сделанный из тонкого металла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0527d79f-478e-3551-b4c9-205c23f7c481",
                    "sense_definition": "Resembling the metal tin in appearance, taste, or smell.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "material_resemblance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The canned soup had a slightly <w>tinny</w> taste that I found unpleasant.",
                            "target_lang_translations": {
                                "ru": "У консервированного супа был легкий <w>металлический</w> привкус, который показался мне неприятным."
                            }
                        },
                        {
                            "text": "After eating with the old spoon, I was left with a <w>tinny</w> flavor in my mouth.",
                            "target_lang_translations": {
                                "ru": "После еды этой старой ложкой у меня во рту остался <w>металлический</w> привкус."
                            }
                        }
                    ],
                    "synonyms": [
                        "metallic"
                    ],
                    "common_phrases": [
                        "tinny taste",
                        "tinny smell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий олово или жесть по виду, вкусу или запаху."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "металлический",
                                "target_lang_sense_clarification": "о привкусе"
                            },
                            {
                                "target_lang_word": "отдающий жестью",
                                "target_lang_sense_clarification": "о вкусе консервов"
                            }
                        ]
                    }
                }
            ]
        },

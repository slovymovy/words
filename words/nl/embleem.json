{
    "word_family": [
        "emblematisch",
        "emblematiek"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b216bda1-2154-3ef2-bb7b-9ace63124bdc",
                    "sense_definition": "Een figuur of voorwerp dat symbool staat voor een abstract begrip, zoals een deugd, een gevoel of een idee.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "symbolic_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een rood hart is wereldwijd bekend als het <w>embleem</w> van de liefde.",
                            "target_lang_translations": {
                                "en": "A red heart is known worldwide as the <w>emblem</w> of love."
                            }
                        },
                        {
                            "text": "De adelaar wordt in veel landen gebruikt als <w>embleem</w> voor macht en onafhankelijkheid.",
                            "target_lang_translations": {
                                "en": "The eagle is used in many countries as an <w>emblem</w> of power and independence."
                            }
                        }
                    ],
                    "synonyms": [
                        "symbool",
                        "zinnebeeld"
                    ],
                    "common_phrases": [
                        "het embleem van",
                        "als embleem dienen",
                        "nationaal embleem"
                    ],
                    "target_lang_definitions": {
                        "en": "A figure or object that stands as a symbol for an abstract concept, such as a virtue, a feeling, or an idea."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "emblem",
                                "target_lang_sense_clarification": "symbolic representation of a quality or concept"
                            },
                            {
                                "target_lang_word": "symbol",
                                "target_lang_sense_clarification": "general representation of an idea"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66510db9-8ee6-32dc-8a1e-f1167c2324e6",
                    "sense_definition": "Een herkenningsteken of logo op kleding of voorwerpen dat verwijst naar een specifieke groep, rang of organisatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "badge_or_logo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de mouw van zijn uniform zat een opvallend <w>embleem</w> genaaid.",
                            "target_lang_translations": {
                                "en": "A striking <w>emblem</w> was sewn onto the sleeve of his uniform."
                            }
                        },
                        {
                            "text": "De leden van de sportclub dragen allemaal hetzelfde <w>embleem</w> op hun jas.",
                            "target_lang_translations": {
                                "en": "The members of the sports club all wear the same <w>emblem</w> on their jackets."
                            }
                        }
                    ],
                    "synonyms": [
                        "symbool",
                        "chiffre"
                    ],
                    "common_phrases": [
                        "geborduurd embleem",
                        "militair embleem",
                        "embleem op de kraag"
                    ],
                    "target_lang_definitions": {
                        "en": "A distinguishing mark or logo on clothing or objects that refers to a specific group, rank, or organization."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "emblem",
                                "target_lang_sense_clarification": "distinctive design or logo"
                            },
                            {
                                "target_lang_word": "badge",
                                "target_lang_sense_clarification": "insignia worn on clothing"
                            },
                            {
                                "target_lang_word": "patch",
                                "target_lang_sense_clarification": "cloth badge sewn onto a uniform"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f8bed5e-bf3f-3429-9060-1ae7e44c70b6",
                    "sense_definition": "Een allegorische afbeelding voorzien van een motto en een gedicht, vooral populair in de zeventiende-eeuwse literatuur.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "literary_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De professor bestudeert oude boeken met zeventiende-eeuwse <w>emblemen</w> en hun betekenis.",
                            "target_lang_translations": {
                                "en": "The professor studies old books with seventeenth-century <w>emblems</w> and their meanings."
                            }
                        },
                        {
                            "text": "Dit boek bevat een verzameling van religieuze <w>emblemen</w> die morele lessen uitbeelden.",
                            "target_lang_translations": {
                                "en": "This book contains a collection of religious <w>emblems</w> depicting moral lessons."
                            }
                        }
                    ],
                    "synonyms": [
                        "zinnebeeld"
                    ],
                    "common_phrases": [
                        "zeventiende-eeuwse emblemen",
                        "bundel met emblemen"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "vooral in historische context gebruikt"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An allegorical image accompanied by a motto and a poem, especially popular in seventeenth-century literature."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "emblem",
                                "target_lang_sense_clarification": "allegorical picture with text"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
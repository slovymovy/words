    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5e20c372-2814-3eda-9005-66c313443c1b",
                    "sense_definition": "A document with blank spaces to be filled in with information.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please fill out the application <w>form</w> with your personal details.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, заполните <w>анкету</w> своими личными данными."
                            }
                        },
                        {
                            "text": "You need to sign the consent <w>form</w> before the surgery.",
                            "target_lang_translations": {
                                "ru": "Вам нужно подписать <w>бланк</w> согласия перед операцией."
                            }
                        }
                    ],
                    "common_phrases": [
                        "application form",
                        "fill in a form",
                        "entry form",
                        "consent form"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ с пустыми полями для внесения информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анке́та",
                                "target_lang_sense_clarification": "опросный лист для внесения личных данных"
                            },
                            {
                                "target_lang_word": "бланк",
                                "target_lang_sense_clarification": "лист с напечатанным текстом и местами для заполнения"
                            },
                            {
                                "target_lang_word": "фо́рма",
                                "target_lang_sense_clarification": "общий термин для документа установленного образца"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16b582c7-f407-3666-87cb-8711a6468611",
                    "sense_definition": "The visible shape, configuration, or structure of a person or object.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The clouds took the <w>form</w> of a giant dragon.",
                            "target_lang_translations": {
                                "ru": "Облака приняли <w>форму</w> гигантского дракона."
                            }
                        },
                        {
                            "text": "He could see a dark <w>form</w> standing in the doorway.",
                            "target_lang_translations": {
                                "ru": "Он видел тёмную <w>фигуру</w>, стоящую в дверном проёме."
                            }
                        }
                    ],
                    "synonyms": [
                        "shape",
                        "figure",
                        "configuration"
                    ],
                    "common_phrases": [
                        "in the form of",
                        "human form",
                        "take form"
                    ],
                    "target_lang_definitions": {
                        "ru": "Видимая форма, очертание или строение человека или предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фо́рма",
                                "target_lang_sense_clarification": "внешний вид или очертание предмета"
                            },
                            {
                                "target_lang_word": "фигу́ра",
                                "target_lang_sense_clarification": "очертания тела человека или животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8067eb4-7582-3f87-ab70-6ae1a3d57ca1",
                    "sense_definition": "A particular type, variety, or mode of existence of something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Swimming is an excellent <w>form</w> of exercise for the whole body.",
                            "target_lang_translations": {
                                "ru": "Плавание — это отличный <w>вид</w> физической нагрузки для всего тела."
                            }
                        },
                        {
                            "text": "They discussed the best <w>form</w> of government for the new country.",
                            "target_lang_translations": {
                                "ru": "Они обсуждали лучшую <w>форму</w> правления для новой страны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "form of government",
                        "form of entertainment",
                        "form of life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный тип, разновидность или способ существования чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "разновидность, тип"
                            },
                            {
                                "target_lang_word": "фо́рма",
                                "target_lang_sense_clarification": "способ организации или существования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6760e28-8b73-3e53-bf7f-3aaefec04a9c",
                    "sense_definition": "A specific spelling or inflection of a word used in a certain grammatical context.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The past <w>form</w> of the verb \"go\" is \"went\".",
                            "target_lang_translations": {
                                "ru": "<w>Форма</w> прошедшего времени глагола \"go\" — \"went\"."
                            }
                        },
                        {
                            "text": "You must use the plural <w>form</w> of the noun in this sentence.",
                            "target_lang_translations": {
                                "ru": "В этом предложении вы должны использовать <w>форму</w> множественного числа существительного."
                            }
                        }
                    ],
                    "common_phrases": [
                        "verb form",
                        "plural form",
                        "base form"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "linguistics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфическое написание или изменение слова в определенном грамматическом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фо́рма",
                                "target_lang_sense_clarification": "грамматическая категория слова"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23e38fef-a8d9-3cc7-ab7b-dff2dd716cd0",
                    "sense_definition": "The current level of ability, fitness, or performance of an athlete or team.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team has been in poor <w>form</w> since the beginning of the season.",
                            "target_lang_translations": {
                                "ru": "Команда находится в плохой <w>форме</w> с начала сезона."
                            }
                        },
                        {
                            "text": "She is in top <w>form</w> and expects to win the championship.",
                            "target_lang_translations": {
                                "ru": "Она в лучшей <w>форме</w> и рассчитывает выиграть чемпионат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in good form",
                        "on top form",
                        "out of form",
                        "return to form"
                    ],
                    "target_lang_definitions": {
                        "ru": "Текущий уровень способностей, физической подготовки или результатов спортсмена или команды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фо́рма",
                                "target_lang_sense_clarification": "физическое или спортивное состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99d11990-d755-310c-8c22-2c84dd9ff648",
                    "sense_definition": "A class or year of students in a school.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "school",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Students in the sixth <w>form</w> are preparing for their final exams.",
                            "target_lang_translations": {
                                "ru": "Ученики в шестом <w>классе</w> готовятся к выпускным экзаменам."
                            }
                        },
                        {
                            "text": "The teacher is responsible for the discipline of the entire <w>form</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель отвечает за дисциплину всего <w>класса</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "grade"
                    ],
                    "common_phrases": [
                        "sixth form"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Класс или год обучения школьников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класс",
                                "target_lang_sense_clarification": "группа учеников одного года обучения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86d366c7-f92c-3676-832f-198de392d75d",
                    "sense_definition": "A hollow container or mold used to give a specific shape to a substance.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pour the concrete into the wooden <w>form</w> and let it set.",
                            "target_lang_translations": {
                                "ru": "Залейте бетон в деревянную <w>форму</w> и дайте ему застыть."
                            }
                        },
                        {
                            "text": "The baker used a special <w>form</w> to create the heart-shaped cake.",
                            "target_lang_translations": {
                                "ru": "Пекарь использовал специальную <w>форму</w>, чтобы испечь торт в виде сердца."
                            }
                        }
                    ],
                    "synonyms": [
                        "mold",
                        "cast",
                        "pattern"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полый контейнер или форма, используемая для придания веществу определенной формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фо́рма",
                                "target_lang_sense_clarification": "емкость для отливки или выпечки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60a3f8eb-ecf3-3c60-8ed8-9cb59ea3ce93",
                    "sense_definition": "Accepted social behavior, convention, or etiquette.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "etiquette",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is considered bad <w>form</w> to leave the party without saying goodbye to the host.",
                            "target_lang_translations": {
                                "ru": "Считается дурным <w>тоном</w> уходить с вечеринки, не попрощавшись с хозяином."
                            }
                        },
                        {
                            "text": "He ignored the usual <w>form</w> and spoke directly to the director.",
                            "target_lang_translations": {
                                "ru": "Он проигнорировал принятые <w>правила</w> и обратился напрямую к директору."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bad form",
                        "good form"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принятое в обществе поведение, условности или этикет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тон",
                                "target_lang_sense_clarification": "манера поведения (хороший/дурной тон)"
                            },
                            {
                                "target_lang_word": "мане́ры",
                                "target_lang_sense_clarification": "правила поведения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c5cc6e22-14ab-3641-abd3-a57df9f38b2c",
                    "sense_definition": "To bring something into existence by combining elements or organizing people.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "establish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to <w>form</w> a band and play music together.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>создать</w> группу и играть музыку вместе."
                            }
                        },
                        {
                            "text": "The winning party will <w>form</w> a new government next week.",
                            "target_lang_translations": {
                                "ru": "Победившая партия <w>сформирует</w> новое правительство на следующей неделе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "form a government",
                        "form a committee",
                        "form a band",
                        "form a group"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать что-либо, объединяя элементы или организуя людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формирова́ть",
                                "target_lang_sense_clarification": "организовывать, составлять (правительство, комитет)"
                            },
                            {
                                "target_lang_word": "создава́ть",
                                "target_lang_sense_clarification": "основывать (группу, коллектив)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23709bc9-a4fb-3165-82af-c959b4947cd6",
                    "sense_definition": "To make up or serve as the parts of a whole.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "constitute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "These three chapters <w>form</w> the main part of the book.",
                            "target_lang_translations": {
                                "ru": "Эти три главы <w>составляют</w> основную часть книги."
                            }
                        },
                        {
                            "text": "Women <w>form</w> the majority of the workforce in this company.",
                            "target_lang_translations": {
                                "ru": "Женщины <w>составляют</w> большинство рабочей силы в этой компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "compose",
                        "make up"
                    ],
                    "common_phrases": [
                        "form the basis",
                        "form a part of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Составлять или служить частями целого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составля́ть",
                                "target_lang_sense_clarification": "быть частью чего-либо"
                            },
                            {
                                "target_lang_word": "образо́вывать",
                                "target_lang_sense_clarification": "создавать собой целое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28db38ea-d5ed-3b4c-8aea-de02666b9a44",
                    "sense_definition": "To give a particular shape or structure to something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "shape_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children held hands to <w>form</w> a large circle.",
                            "target_lang_translations": {
                                "ru": "Дети взялись за руки, чтобы <w>образовать</w> большой круг."
                            }
                        },
                        {
                            "text": "Use your hands to <w>form</w> the dough into small balls.",
                            "target_lang_translations": {
                                "ru": "Используйте руки, чтобы <w>сформировать</w> из теста маленькие шарики."
                            }
                        }
                    ],
                    "synonyms": [
                        "shape",
                        "mold"
                    ],
                    "antonyms": [
                        "deform",
                        "distort",
                        "misshape"
                    ],
                    "common_phrases": [
                        "form a line",
                        "form a circle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать чему-либо определенную форму или структуру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "образо́вывать",
                                "target_lang_sense_clarification": "составлять фигуру (круг, линию)"
                            },
                            {
                                "target_lang_word": "формирова́ть",
                                "target_lang_sense_clarification": "придавать форму материалу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "445f3cdb-2724-3877-b59e-c8b05f9c04e9",
                    "sense_definition": "To come into existence or start to be visible.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "develop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ice began to <w>form</w> on the lake as the temperature dropped.",
                            "target_lang_translations": {
                                "ru": "На озере начал <w>образовываться</w> лёд, когда температура упала."
                            }
                        },
                        {
                            "text": "A clear plan started to <w>form</w> in her mind.",
                            "target_lang_translations": {
                                "ru": "В её голове начал <w>формироваться</w> чёткий план."
                            }
                        }
                    ],
                    "synonyms": [
                        "take shape",
                        "come into being"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возникать, появляться или становиться видимым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "образо́вываться",
                                "target_lang_sense_clarification": "появляться (о природных явлениях)"
                            },
                            {
                                "target_lang_word": "формирова́ться",
                                "target_lang_sense_clarification": "складываться, развиваться (о планах, идеях)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
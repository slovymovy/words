            "senses": [
                {
                    "sense_id": "733c585f-43a9-301f-87b3-ef3f694dcc0e",
                    "sense_definition": "Wyrażenie żalu i prośba o wybaczenie za wyrządzoną krzywdę, błąd, spóźnienie lub niezręczność.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "apology_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Och, <w>przepraszam</w> najmocniej, nie chciałem cię potrącić.",
                            "target_lang_translations": {
                                "ru": "Ох, <w>простите</w> великодушно, я не хотел тебя толкнуть."
                            }
                        },
                        {
                            "text": "Bardzo <w>przepraszam</w> za spóźnienie, uciekł mi autobus.",
                            "target_lang_translations": {
                                "ru": "Очень <w>прошу прощения</w> за опоздание, я упустил автобус."
                            }
                        }
                    ],
                    "synonyms": [
                        "sorry"
                    ],
                    "common_phrases": [

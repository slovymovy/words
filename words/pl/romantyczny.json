            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d83097d4-9404-3fa8-8cf5-a84c7fedffa1",
                    "sense_definition": "Związany z miłością, sprzyjający zakochanym i budujący nastrojową, uczuciową atmosferę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "love_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój mąż przygotował dla nas <w>romantyczną</w> kolację przy świecach z okazji rocznicy.",
                            "target_lang_translations": {
                                "ru": "Мой муж приготовил для нас <w>романтический</w> ужин при свечах по случаю годовщины."
                            }
                        },
                        {
                            "text": "To był niezwykle <w>romantyczny</w> wieczór, podczas którego spacerowaliśmy brzegiem morza.",
                            "target_lang_translations": {
                                "ru": "Это был невероятно <w>романтический</w> вечер, во время которого мы гуляли по берегу моря."
                            }
                        }
                    ],
                    "antonyms": [
                        "nieromantyczny"
                    ],
                    "common_phrases": [
                        "romantyczna kolacja",
                        "romantyczny wieczór",
                        "romantyczny spacer",
                        "romantyczny nastrój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с любовью, создающий возвышенную атмосферу и настроение для влюблённых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "романтический",
                                "target_lang_sense_clarification": "о ситуации, вечере, ужине"
                            },
                            {
                                "target_lang_word": "романтичный",
                                "target_lang_sense_clarification": "проникнутый романтикой, создающий настроение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f26121c4-3fcf-33c4-8eeb-0d12c846062d",
                    "sense_definition": "Cechujący się dużą wrażliwością, skłonnością do marzeń i idealizowaniem rzeczywistości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ona ma bardzo <w>romantyczną</w> duszę i często buja w obłokach zamiast skupić się na pracy.",
                            "target_lang_translations": {
                                "ru": "У неё очень <w>романтическая</w> душа, и она часто витает в облаках вместо того, чтобы сосредоточиться на работе."
                            }
                        },
                        {
                            "text": "Jego <w>romantyczne</w> podejście do życia sprawia, że czasem trudno mu zmierzyć się z twardymi realiami.",
                            "target_lang_translations": {
                                "ru": "Его <w>романтический</w> подход к жизни приводит к тому, что ему порой трудно столкнуться с суровой реальностью."
                            }
                        }
                    ],
                    "synonyms": [
                        "sentymentalny",
                        "idealistyczny"
                    ],
                    "common_phrases": [
                        "romantyczna dusza",
                        "romantyczne usposobienie",
                        "romantyczny gest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличающийся чувствительностью, склонностью к мечтам и идеализации реальности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "романтичный",
                                "target_lang_sense_clarification": "о характере, натуре человека"
                            },
                            {
                                "target_lang_word": "романтический",
                                "target_lang_sense_clarification": "мечтательный, возвышенный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3d5acab-fa49-3ce1-b537-a265facfe3f4",
                    "sense_definition": "Odnoszący się do romantyzmu – nurtu w kulturze, literaturze i sztuce europejskiej przełomu XVIII i XIX wieku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_era",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Adam Mickiewicz jest uważany za najważniejszego polskiego poetę <w>romantycznego</w>.",
                            "target_lang_translations": {
                                "ru": "Адам Мицкевич считается важнейшим польским <w>романтическим</w> поэтом."
                            }
                        },
                        {
                            "text": "W szkole średniej uczniowie dokładnie omawiają cechy dramatu <w>romantycznego</w>.",
                            "target_lang_translations": {
                                "ru": "В средней школе ученики подробно обсуждают черты <w>романтической</w> драмы."
                            }
                        }

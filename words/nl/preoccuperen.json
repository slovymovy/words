{
    "word_family": [
        "preoccupatie",
        "gepreoccupeerd"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3258c5d7-7a07-3cfa-8472-f7a2e3534a00",
                    "sense_definition": "De gedachten van iemand volledig in beslag nemen, waardoor er weinig aandacht is voor andere zaken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De naderende deadline <preoccupeerde> het hele team gedurende de week.",
                            "target_lang_translations": {
                                "en": "The approaching deadline <preoccupied> the entire team throughout the week.",
                                "ru": "Приближающийся крайний срок <w>занимал мысли</w> всей команды в течение недели."
                            }
                        },
                        {
                            "text": "Zijn gezondheidsproblemen <preoccuperen> hem zozeer dat hij nergens anders aan kan denken.",
                            "target_lang_translations": {
                                "en": "His health problems <preoccupy> him so much that he cannot think of anything else.",
                                "ru": "Проблемы со здоровьем настолько его <w>занимают</w>, что он не может думать ни о чем другом."
                            }
                        }
                    ],
                    "synonyms": [
                        "obsederen",
                        "beheersen",
                        "in beslag nemen"
                    ],
                    "antonyms": [
                        "loslaten"
                    ],
                    "common_phrases": [
                        "iemand preoccuperen",
                        "gepreoccupeerd zijn met"
                    ],
                    "target_lang_definitions": {
                        "en": "To completely occupy someone's thoughts, leaving little attention for other matters.",
                        "ru": "Полностью занимать чьи-либо мысли, не оставляя внимания для других вещей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "preoccupy",
                                "target_lang_sense_clarification": "to dominate the mind"
                            },
                            {
                                "target_lang_word": "engross",
                                "target_lang_sense_clarification": "to absorb all attention"
                            },
                            {
                                "target_lang_word": "obsess",
                                "target_lang_sense_clarification": "to fill the mind continually"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "занимать (мысли)",
                                "target_lang_sense_clarification": "поглощать внимание"
                            },
                            {
                                "target_lang_word": "беспокоить",
                                "target_lang_sense_clarification": "вызывать сильную озабоченность"
                            },
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "полностью увлекать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28e313cc-c71e-3f2e-b0d4-aa8ea98297c9",
                    "sense_definition": "Iemand op voorhand beïnvloeden of een bepaalde mening of vooroordeel bezorgen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Laat je niet <preoccuperen> door geruchten voordat je de feiten kent.",
                            "target_lang_translations": {
                                "en": "Do not let yourself be <prejudiced> by rumors before you know the facts.",
                                "ru": "Не позволяй слухам <w>предубеждать</w> тебя, пока ты не узнаешь фактов."
                            }
                        },
                        {
                            "text": "Zijn eerdere ervaringen <preoccupeerden> hem tegen de nieuwe voorstellen.",
                            "target_lang_translations": {
                                "en": "His previous experiences <biased> him against the new proposals.",
                                "ru": "Его прошлый опыт <w>настроил</w> его против новых предложений."
                            }
                        }
                    ],
                    "synonyms": [
                        "bevooroordelen",
                        "innemen"
                    ],
                    "target_lang_definitions": {
                        "en": "To influence someone beforehand or instill a certain opinion or prejudice.",
                        "ru": "Заранее влиять на кого-либо, внушая определенное мнение или предубеждение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prejudice",
                                "target_lang_sense_clarification": "to influence an opinion beforehand"
                            },
                            {
                                "target_lang_word": "bias",
                                "target_lang_sense_clarification": "to cause partiality"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "предубеждать",
                                "target_lang_sense_clarification": "создавать предвзятое мнение"
                            },
                            {
                                "target_lang_word": "настраивать",
                                "target_lang_sense_clarification": "формировать отношение заранее"
                            }
                        ]
                    }
                }
            ]
        }
    ]

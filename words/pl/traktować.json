{
    "word_family": [
        "potraktować",
        "traktowanie",
        "traktat"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d5fd7529-9c46-3423-a510-aa2877d73575",
                    "sense_definition": "Odnosić się do kogoś lub czegoś w określony sposób, przejawiać określony stosunek lub zachowanie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój szef zawsze <w>traktuje</w> wszystkich pracowników uczciwie i sprawiedliwie.",
                            "target_lang_translations": {
                                "ru": "Мой начальник всегда <w>относится</w> ко всем сотрудникам честно и справедливо."
                            }
                        },
                        {
                            "text": "Nie możesz <w>traktować</w> tego ważnego zadania jako żartu.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь <w>относиться</w> к этому важному заданию как к шутке."
                            }
                        }
                    ],
                    "synonyms": [
                        "odnosić się",
                        "postępować z"
                    ],
                    "common_phrases": [
                        "traktować poważnie",
                        "traktować z góry",
                        "traktować jak powietrze",
                        "traktować ulgowo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относиться к кому-либо или чему-либо определенным образом, проявлять определенное отношение или поведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "относиться",
                                "target_lang_sense_clarification": "проявлять отношение"
                            },
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "вести себя с кем-то"
                            },
                            {
                                "target_lang_word": "воспринимать",
                                "target_lang_sense_clarification": "считать чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20452599-b199-3fc3-857b-2dd9804cd5cd",
                    "sense_definition": "Zachowywać się wobec siebie nawzajem w określony sposób (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo rozwodu, moi rodzice nadal <w>traktują</w> się z dużym szacunkiem.",
                            "target_lang_translations": {
                                "ru": "Несмотря на развод, мои родители по-прежнему <w>относятся</w> друг к другу с большим уважением."
                            }
                        },
                        {
                            "text": "W naszej małej firmie wszyscy <w>traktują</w> się jak bliska rodzina.",
                            "target_lang_translations": {
                                "ru": "В нашей маленькой фирме все <w>относятся</w> друг к другу как близкие родственники."
                            }
                        }
                    ],
                    "synonyms": [
                        "odnosić się"
                    ],
                    "common_phrases": [
                        "traktować się z szacunkiem",
                        "traktować się jak",
                        "traktować się nawzajem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя по отношению друг к другу определенным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "относиться",
                                "target_lang_sense_clarification": "друг к другу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbf4138e-f6f8-3686-b64d-70873520b6f4",
                    "sense_definition": "Poddawać kogoś lub coś działaniu jakiegoś czynnika, środka chemicznego lub procesu w określonym celu (np. medycznym lub technicznym).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarze zdecydowali się <w>traktować</w> tę infekcję nowym, silnym antybiotykiem.",
                            "target_lang_translations": {
                                "ru": "Врачи решили <w>лечить</w> эту инфекцию новым сильным антибиотиком."
                            }
                        },
                        {
                            "text": "Drewno ogrodowe należy regularnie <w>traktować</w> specjalnym impregnatem.",
                            "target_lang_translations": {
                                "ru": "Садовую древесину следует регулярно <w>обрабатывать</w> специальной пропиткой."
                            }
                        }
                    ],
                    "synonyms": [
                        "poddawać",
                        "oddziaływać"
                    ],
                    "common_phrases": [
                        "traktować ranę",
                        "traktować chemikaliami",
                        "traktować lekiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергать воздействию какого-либо фактора, вещества или процесса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обрабатывать",
                                "target_lang_sense_clarification": "подвергать действию вещества"
                            },
                            {
                                "target_lang_word": "лечить",
                                "target_lang_sense_clarification": "применять лекарства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5f0e032-37ab-3354-a26e-d045c4c9ed83",
                    "sense_definition": "Omawiać jakiś temat, mieć coś za przedmiot rozważań (zwykle o książce, artykule lub filmie).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "topic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten rozdział podręcznika szczegółowo <w>traktuje</w> o historii starożytnego Rzymu.",
                            "target_lang_translations": {
                                "ru": "Эта глава учебника подробно <w>рассказывает</w> об истории Древнего Рима."
                            }
                        },
                        {
                            "text": "Najnowszy film tego reżysera <w>traktuje</w> o problemach współczesnej młodzieży.",
                            "target_lang_translations": {
                                "ru": "Новый фильм этого режиссера <w>повествует</w> о проблемах современной молодежи."
                            }
                        }
                    ],
                    "synonyms": [
                        "omawiać",
                        "dotyczyć",
                        "być na (jakiś) temat"
                    ],
                    "common_phrases": [
                        "traktować o czymś",
                        "traktować o problemie",
                        "artykuł traktuje o"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "książkowe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обсуждать тему, иметь что-либо предметом повествования (о книге, фильме)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повествовать",
                                "target_lang_sense_clarification": "рассказывать о чем-либо"
                            },
                            {
                                "target_lang_word": "рассматривать",
                                "target_lang_sense_clarification": "обсуждать тему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e277224-3c86-300c-a4b3-2203acbcf127",
                    "sense_definition": "Podejmować kogoś gościną, dawać coś do jedzenia lub picia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hospitality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz hojnie <w>traktował</w> swoich gości najlepszym winem z piwnicy.",
                            "target_lang_translations": {
                                "ru": "Хозяин щедро <w>угощал</w> своих гостей лучшим вином из погреба."
                            }
                        },
                        {
                            "text": "Na przyjęciu weselnym <w>traktowano</w> nas wyśmienitymi regionalnymi potrawami.",
                            "target_lang_translations": {
                                "ru": "На свадебном приеме нас <w>угощали</w> изысканными региональными блюдами."
                            }
                        }
                    ],
                    "synonyms": [
                        "częstować",
                        "raczyć"
                    ],
                    "common_phrases": [
                        "traktować gości",
                        "traktować obiadem",
                        "traktować drinkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать гостей, предлагать еду или напитки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угощать",
                                "target_lang_sense_clarification": "предлагать еду или питье"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "3be2dccc-81ec-305b-9614-c0caf071e912",
                    "sense_definition": "Centralna postać religii chrześcijańskiej, Jezus z Nazaretu, uważany przez chrześcijan za Syna Bożego i Mesjasza.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Chrześcijanie wierzą, że <w>Jezus</w> oddał życie za grzechy ludzi.",
                            "target_lang_translations": {
                                "ru": "Христиане верят, что <w>Иисус</w> отдал жизнь за грехи людей."
                            }
                        },
                        {
                            "text": "W kościele znajduje się piękny obraz przedstawiający <w>Jezusa</w>.",
                            "target_lang_translations": {
                                "ru": "В церкви находится прекрасная картина, изображающая <w>Иисуса</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Chrystus",
                        "Jezus Chrystus",
                        "Mesjasz",
                        "Jezus z Nazaretu"
                    ],
                    "common_phrases": [
                        "w imię Jezusa",
                        "Serce Jezusa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Центральная личность христианской религии, Иисус из Назарета, почитаемый христианами как Сын Божий и Мессия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Иисус",
                                "target_lang_sense_clarification": "центральная фигура христианства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "197e2010-2704-3a13-907d-5d706ffad966",
                    "sense_definition": "Imię męskie pochodzenia semickiego, popularne w krajach hiszpańskojęzycznych.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Nazywam się <w>Jezus</w> Miguel Brito Garcia i jestem Hiszpanem.",
                            "target_lang_translations": {
                                "ru": "Меня зовут <w>Иисус</w> Мигель Брито Гарсия, и я испанец."
                            }
                        },
                        {
                            "text": "Mój kolega z pracy ma na imię <w>Jezus</w> i pochodzi z Meksyku.",
                            "target_lang_translations": {
                                "ru": "Моего коллегу зовут <w>Иисус</w>, и он родом из Мексики."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя семитского происхождения, популярное в испаноязычных странах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Иисус",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "3f6845c8-24d6-358d-8f90-c64dab01cfbc",
                    "sense_definition": "Okrzyk wyrażający silne emocje, takie jak strach, zdziwienie, ból lub irytacja.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "O <w>Jezu</w>, zostawiłam w domu włączone żelazko!",
                            "target_lang_translations": {
                                "ru": "О <w>Господи</w>, я оставила дома включенный утюг!"
                            }
                        },
                        {
                            "text": "<w>Jezus</w>, ale mnie przestraszyłeś, wchodząc tak cicho!",
                            "target_lang_translations": {
                                "ru": "<w>Господи</w>, как ты меня напугал, войдя так тихо!"
                            }
                        }
                    ],
                    "synonyms": [
                        "Jezus Maria!",
                        "Jezuniu!",
                        "Jezu Chryste!",
                        "Chryste!",
                        "Chryste Panie!"
                    ],
                    "common_phrases": [
                        "o Jezu",
                        "Jezus Maria"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, выражающий сильные эмоции, такие как страх, удивление, боль или раздражение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Господи",
                                "target_lang_sense_clarification": "выражение страха или удивления"
                            },
                            {
                                "target_lang_word": "Боже",
                                "target_lang_sense_clarification": "эмоциональный возглас"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
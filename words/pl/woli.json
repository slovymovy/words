    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "86697009-7bf7-3f07-8fa2-560be467ea17",
                    "sense_definition": "trzecia osoba liczby pojedynczej czasu teraźniejszego czasownika <w>woleć</w>",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat zdecydowanie <w>woli</w> psy od kotów.",
                            "target_lang_translations": {
                                "ru": "Мой брат определенно <w>предпочитает</w> собак кошкам."
                            }
                        },
                        {
                            "text": "Ona <w>woli</w> czytać książki niż oglądać filmy.",
                            "target_lang_translations": {
                                "ru": "Она <w>предпочитает</w> читать книги, а не смотреть фильмы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on woli",
                        "ona woli",
                        "bardziej woli"
                    ],
                    "target_lang_definitions": {
                        "ru": "третье лицо единственного числа настоящего времени глагола \"woleć\" (предпочитать)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предпочитает",
                                "target_lang_sense_clarification": "глагол"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aacb1562-9100-33c2-9074-e797591a3f2d",
                    "sense_definition": "forma (dopełniacz, celownik lub miejscownik liczby pojedynczej) rzeczownika <w>wola</w>",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystko zależy teraz od twojej dobrej <w>woli</w>.",
                            "target_lang_translations": {
                                "ru": "Всё зависит теперь от твоей доброй <w>воли</w>."
                            }
                        },
                        {
                            "text": "Postąpił tak wbrew <w>woli</w> swoich rodziców.",
                            "target_lang_translations": {
                                "ru": "Он поступил так против <w>воли</w> своих родителей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "siła woli",
                        "z własnej woli",
                        "wolna wola"
                    ],
                    "target_lang_definitions": {
                        "ru": "форма (родительный, дательный или предложный падеж единственного числа) существительного \"wola\" (воля)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воли",
                                "target_lang_sense_clarification": "форма слова"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6a10204-16e2-3d40-930a-f69670b5bb66",
                    "sense_definition": "forma (dopełniacz liczby mnogiej) rzeczownika <w>wole</w>",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz weterynarii dokonał oględzin <w>woli</w> chorych ptaków.",
                            "target_lang_translations": {
                                "ru": "Ветеринар провел осмотр <w>зобов</w> больных птиц."
                            }
                        },
                        {
                            "text": "U tego gatunku nie stwierdzono obecności powiększonych <w>woli</w>.",
                            "target_lang_translations": {
                                "ru": "У этого вида не обнаружено увеличенных <w>зобов</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "форма (родительный падеж множественного числа) существительного \"wole\" (зоб)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зобов",
                                "target_lang_sense_clarification": "анатомия птиц"

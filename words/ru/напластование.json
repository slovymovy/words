    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1e7e2b5-e69d-3a34-afb5-4e6fb456a503",
                    "sense_definition": "Поздние элементы, черты или особенности, наложившиеся на что-то уже существующее, сложившееся ранее.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В местных традициях можно заметить сложное культурное <w>напластование</w> разных эпох.",
                            "target_lang_translations": {
                                "en": "In local traditions, one can notice complex cultural <w>layering</w> of different eras."
                            }
                        },
                        {
                            "text": "Это слово — позднее историческое <w>напластование</w>, которого не было в древнем языке.",
                            "target_lang_translations": {
                                "en": "This word is a later historical <w>accretion</w> that did not exist in the ancient language."
                            }
                        }
                    ],
                    "synonyms": [
                        "наслоение"
                    ],
                    "common_phrases": [
                        "культурные напластования",
                        "историческое напластование",
                        "вековые напластования"
                    ],
                    "target_lang_definitions": {
                        "en": "Later elements, features, or peculiarities superimposed on something already existing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "layering",
                                "target_lang_sense_clarification": "cultural or historical context"
                            },
                            {
                                "target_lang_word": "stratification",
                                "target_lang_sense_clarification": "formal/abstract"
                            },
                            {
                                "target_lang_word": "accretion",
                                "target_lang_sense_clarification": "gradual accumulation of layers"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e481a865-0bd6-3c06-918b-c73d8d613c92",
                    "sense_definition": "Расположение осадочных горных пород в земной коре в виде слоёв или пластов.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Геологи внимательно изучали горизонтальное <w>напластование</w> пород в каньоне.",
                            "target_lang_translations": {
                                "en": "Geologists carefully studied the horizontal <w>stratification</w> of rocks in the canyon."
                            }
                        },
                        {
                            "text": "На срезе скалы хорошо видно древнее <w>напластование</w> известняка.",
                            "target_lang_translations": {
                                "en": "On the cliff face, the ancient <w>bedding</w> of limestone is clearly visible."
                            }
                        }
                    ],
                    "synonyms": [
                        "наслоение"
                    ],
                    "common_phrases": [
                        "горизонтальное напластование",
                        "напластование пород"
                    ],
                    "target_lang_definitions": {
                        "en": "The arrangement of sedimentary rocks in the earth's crust in the form of layers or strata."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stratification",
                                "target_lang_sense_clarification": "geological term"
                            },
                            {
                                "target_lang_word": "bedding",
                                "target_lang_sense_clarification": "of rocks"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f6b29a7-06f8-3fd0-82c4-e3017547fef8",
                    "sense_definition": "Процесс укладывания чего-либо слоями или образование таких слоёв.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный ветер вызывает неравномерное <w>напластование</w> снега на крышах домов.",
                            "target_lang_translations": {
                                "en": "Strong wind causes uneven <w>accumulation</w> of snow on house roofs."
                            }
                        },
                        {
                            "text": "Постепенное <w>напластование</w> ила на дне реки меняет её русло.",
                            "target_lang_translations": {
                                "en": "The gradual <w>buildup</w> of silt on the riverbed changes its channel."
                            }
                        }
                    ],
                    "synonyms": [
                        "наслоение"
                    ],
                    "common_phrases": [
                        "процесс напластования",
                        "напластование льда"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of laying something in layers or the formation of such layers."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accumulation",
                                "target_lang_sense_clarification": "piling up of material"
                            },
                            {
                                "target_lang_word": "buildup",
                                "target_lang_sense_clarification": "gradual increase in layers"
                            },
                            {
                                "target_lang_word": "layering",
                                "target_lang_sense_clarification": "physical process"
                            }
                        ]
                    }
                }
            ]
        }
    ]

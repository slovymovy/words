                },
                {
                    "sense_id": "d9bd5221-5606-32e4-b858-33a804491210",
                    "sense_definition": "De echtgenoot van Maria en de pleegvader van Jezus in de Bijbel.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "biblical_figures",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "<w>Jozef</w> en Maria reisden samen naar Bethlehem.",
                            "target_lang_translations": {
                                "en": "<w>Joseph</w> and Mary traveled together to Bethlehem.",
                                "ru": "<w>Иосиф</w> и Мария вместе отправились в Вифлеем."
                            }
                        },
                        {
                            "text": "In het kerstverhaal speelt <w>Jozef</w> een belangrijke rol.",
                            "target_lang_translations": {

{
    "word_family": [
        "vluchteling",
        "vluchtig",
        "ontvluchten",
        "invluchten",
        "wegvluchten"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d454d387-7c53-3b33-8223-0e6897d1a5c6",
                    "sense_definition": "Een reis door de lucht, meestal met een vliegtuig.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "aviation_travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De KLM annuleerde deze <w>vlucht</w> naar Schiphol.",
                            "target_lang_translations": {
                                "ru": "KLM отменила этот <w>рейс</w> в Схипхол."
                            }
                        },
                        {
                            "text": "Tijdens de lange <w>vlucht</w> keek hij naar een film.",
                            "target_lang_translations": {
                                "ru": "Во время долгого <w>перелёта</w> он смотрел фильм."
                            }
                        }
                    ],
                    "synonyms": [
                        "vliegreis"
                    ],
                    "common_phrases": [
                        "een vlucht boeken",
                        "de vlucht missen",
                        "tijdens de vlucht"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествие по воздуху, обычно на самолете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рейс",
                                "target_lang_sense_clarification": "регулярный полет по расписанию"
                            },
                            {
                                "target_lang_word": "полёт",
                                "target_lang_sense_clarification": "процесс перемещения по воздуху"
                            },
                            {
                                "target_lang_word": "перелёт",
                                "target_lang_sense_clarification": "путешествие из одного места в другое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0dbfe044-c48f-33d6-8685-fdf106895e49",
                    "sense_definition": "De handeling van het wegrennen of ontsnappen uit een gevaarlijke situatie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "escape_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het leger sloeg op de <w>vlucht</w> toen de vijand naderde.",
                            "target_lang_translations": {
                                "ru": "Армия обратилась в <w>бегство</w>, когда приблизился враг."
                            }
                        },
                        {
                            "text": "Zijn <w>vlucht</w> uit het land was gevaarlijk en duurde weken.",
                            "target_lang_translations": {
                                "ru": "Его <w>побег</w> из страны был опасным и длился недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontsnapping"
                    ],
                    "common_phrases": [
                        "op de vlucht slaan",
                        "op de vlucht zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в убегании или спасении из опасной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегство",
                                "target_lang_sense_clarification": "спасение бегством от опасности"
                            },
                            {
                                "target_lang_word": "побег",
                                "target_lang_sense_clarification": "тайный уход, освобождение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba4b2c4e-e3fa-3067-ab5b-dfc1f30e3a26",
                    "sense_definition": "Een groep vogels die samen vliegt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een <w>vlucht</w> regenwulpen vloog over het veld.",
                            "target_lang_translations": {
                                "ru": "<w>Стая</w> средних кроншнепов пролетела над полем."
                            }
                        },
                        {
                            "text": "In de herfst zagen we een grote <w>vlucht</w> ganzen naar het zuiden trekken.",
                            "target_lang_translations": {
                                "ru": "Осенью мы видели, как большая <w>стая</w> гусей тянулась на юг."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwerm"
                    ],
                    "common_phrases": [
                        "een vlucht vogels",
                        "een vlucht ganzen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа птиц, летящих вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стая",
                                "target_lang_sense_clarification": "группа птиц"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e99080e-87e6-3949-af07-b3d1956bca82",
                    "sense_definition": "De afstand tussen de uiteinden van de vleugels van een vogel of vliegtuig.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De condor is een vogel met een enorme <w>vlucht</w>.",
                            "target_lang_translations": {
                                "ru": "Кондор — это птица с огромным <w>размахом крыльев</w>."
                            }
                        },
                        {
                            "text": "De <w>vlucht</w> van dit kleine vliegtuigje is slechts tien meter.",
                            "target_lang_translations": {
                                "ru": "<w>Размах крыльев</w> этого маленького самолетика составляет всего десять метров."
                            }
                        }
                    ],
                    "synonyms": [
                        "spanwijdte",
                        "vleugelwijdte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расстояние между кончиками крыльев птицы или самолета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размах крыльев",
                                "target_lang_sense_clarification": "измерение ширины"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "78e6f1f7-0e36-3d95-8398-46531b1a5b8a",
                    "sense_definition": "*vervoeging van* <w>vluchten</w>: eerste, tweede en derde persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De inbreker <w>vlucht</w> door het open raam naar buiten.",
                            "target_lang_translations": {
                                "ru": "Взломщик <w>убегает</w> наружу через открытое окно."
                            }
                        },
                        {
                            "text": "Zij <w>vlucht</w> in haar fantasie als de werkelijkheid te moeilijk wordt.",
                            "target_lang_translations": {
                                "ru": "Она <w>сбегает</w> в свои фантазии, когда реальность становится слишком тяжелой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present tense singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "*спряжение глагола* <w>vluchten</w>: 1, 2 и 3 лицо единственного числа настоящего времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убегает",
                                "target_lang_sense_clarification": "совершает побег"
                            },
                            {
                                "target_lang_word": "сбегает",
                                "target_lang_sense_clarification": "скрывается"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c5e3723-f899-3cc5-b28a-36eb551390cc",
                    "sense_definition": "*vervoeging van* <w>vluchten</w>: gebiedende wijs.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Vlucht</w> zo snel mogelijk weg als je rook ruikt!",
                            "target_lang_translations": {
                                "ru": "<w>Беги</w> прочь как можно скорее, если почувствуешь запах дыма!"
                            }
                        },
                        {
                            "text": "<w>Vlucht</w> niet voor je verantwoordelijkheden, maar los ze op.",
                            "target_lang_translations": {
                                "ru": "Не <w>убегай</w> от своих обязанностей, а решай их."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "*спряжение глагола* <w>vluchten</w>: повелительное наклонение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беги",
                                "target_lang_sense_clarification": "призыв к бегству"
                            },
                            {
                                "target_lang_word": "спасайся",
                                "target_lang_sense_clarification": "призыв спасти себя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
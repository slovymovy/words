                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застольный",
                                "target_lang_sense_clarification": "связанный с песнями, застольем"
                            },
                            {
                                "target_lang_word": "питейный",
                                "target_lang_sense_clarification": "связанный с заведениями"
                            }
                        ]
                    }
                }
            ]
        }
    ]

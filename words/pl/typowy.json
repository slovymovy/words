            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "82851a5d-d861-3b95-9f6f-f23eb1c85035",
                    "sense_definition": "Taki, który jest często spotykany, zwyczajny i zgodny z przyjętym wzorcem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "common_standard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest <w>typowy</w> przykład architektury gotyckiej, którą można spotkać w tym regionie.",
                            "target_lang_translations": {
                                "ru": "Это <w>типичный</w> пример готической архитектуры, которую можно встретить в этом регионе."
                            }
                        },
                        {
                            "text": "Mój <w>typowy</w> dzień pracy zaczyna się od sprawdzenia poczty elektronicznej.",
                            "target_lang_translations": {
                                "ru": "Мой <w>обычный</w> рабочий день начинается с проверки электронной почты."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwyczajny",
                        "zwykły",
                        "standardowy",
                        "powszechny",
                        "normalny",
                        "przeciętny",
                        "statystyczny"
                    ],
                    "antonyms": [
                        "nietypowy",
                        "niestandardowy",
                        "specjalny",
                        "szczególny"
                    ],
                    "common_phrases": [
                        "typowy dzień",
                        "w typowy sposób",
                        "typowy przykład",
                        "typowy objaw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который часто встречается, обычный и соответствующий принятому образцу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "типичный",
                                "target_lang_sense_clarification": "наиболее часто встречающийся"
                            },
                            {
                                "target_lang_word": "обычный",
                                "target_lang_sense_clarification": "повседневный, привычный"

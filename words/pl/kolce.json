            "senses": [
                {
                    "sense_id": "d959ac12-6fad-3a7f-9421-e0e4566e0c07",
                    "sense_definition": "forma liczby mnogiej (mianownik, biernik, wołacz) rzeczownika <w>kolec</w>",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Róża ma ostre <kolce>, które mogą zranić dłonie przy dotknięciu.",
                            "target_lang_translations": {
                                "ru": "У розы есть острые <шипы>, которые могут поранить ладони при прикосновении."
                            }
                        },
                        {
                            "text": "Jeż zwinął się w kulkę i nastroszył swoje <kolce>.",
                            "target_lang_translations": {
                                "ru": "Ёж свернулся в клубок и взъерошил свои <иголки>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciernie"
                    ],
                    "common_phrases": [
                        "ostre kolce",
                        "kolce róży",
                        "bez kolców"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural of kolec"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Острые выступы на растениях или теле некоторых животных; множественное число от слова \"kolec\""
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шипы",
                                "target_lang_sense_clarification": "у растений, например, у розы"
                            },
                            {
                                "target_lang_word": "иголки",
                                "target_lang_sense_clarification": "у животных, например, у ежа"
                            },
                            {
                                "target_lang_word": "колючки",
                                "target_lang_sense_clarification": "общее название острых выступов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58714a36-fa31-3a4c-8726-413b3ad182b2",
                    "sense_definition": "specjalistyczne buty sportowe z metalowymi wkrętami w podeszwie, używane głównie w lekkoatletyce do biegania",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprinter założył nowe <kolce> przed wyjściem na bieżnię.",
                            "target_lang_translations": {
                                "ru": "Спринтер надел новые <шиповки> перед выходом на беговую дорожку."
                            }
                        },
                        {
                            "text": "Na mokrej nawierzchni <kolce> zapewniają zawodnikom niezbędną przyczepność.",
                            "target_lang_translations": {
                                "ru": "На мокром покрытии <шиповки> обеспечивают спортсменам необходимое сцепление."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolcówki"
                    ],
                    "common_phrases": [
                        "biegać w kolcach",
                        "buty kolce",
                        "kolce sprinterskie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специальная спортивная обувь с металлическими шипами на подошве для бега"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шиповки",
                                "target_lang_sense_clarification": "спортивная обувь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8431ad5a-e765-36bc-87f2-24dd99bd8a56",
                    "sense_definition": "forma liczby pojedynczej (celownik, miejscownik) rzeczownika <w>kolka</w>",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz długo opowiadał pacjentowi o nerkowej <kolce> i sposobach jej leczenia.",
                            "target_lang_translations": {
                                "ru": "Врач долго рассказывал пациенту о почечной <колике> и способах её лечения."
                            }
                        },
                        {
                            "text": "Dzięki szybkiej uldze w <kolce> dziecko w końcu przestało płakać.",
                            "target_lang_translations": {
                                "ru": "Благодаря быстрому облегчению при <колике> ребёнок наконец перестал плакать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "o kolce",
                        "przy kolce"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "dative/locative singular of kolka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма единственного числа (дательный и предложный падежи) существительного \"kolka\" (внезапная острая боль)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колике",
                                "target_lang_sense_clarification": "падежная форма слова колика"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d4c1e739-ea55-3610-9acc-d5eba8175639",
                    "sense_definition": "Завершиться успехом, привести к желаемому результату; удаться.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "outcome_success",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У него не <w>получилось</w> устроиться на новую работу с первого раза.",
                            "target_lang_translations": {
                                "en": "He didn't <w>manage</w> to get the new job on the first try."
                            }
                        },
                        {
                            "text": "Не переживай, в следующий раз у тебя обязательно всё <w>получится</w>.",
                            "target_lang_translations": {
                                "en": "Don't worry, next time everything will definitely <w>work out</w> for you."
                            }
                        }
                    ],
                    "synonyms": [
                        "удаться",
                        "выйти"
                    ],
                    "antonyms": [
                        "провалиться"
                    ],
                    "common_phrases": [
                        "всё получится",
                        "ничего не получилось",
                        "хорошо получиться",
                        "не получиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To end successfully, lead to a desired result; to succeed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "work out",
                                "target_lang_sense_clarification": "to have a good result or outcome"
                            },
                            {
                                "target_lang_word": "succeed",
                                "target_lang_sense_clarification": "to achieve a desired aim or result"
                            },
                            {
                                "target_lang_word": "manage",
                                "target_lang_sense_clarification": "to be able to do something difficult"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b5f3925-e5e5-3705-8159-3a5c74197c78",
                    "sense_definition": "Образоваться, возникнуть или быть созданным в результате какого-либо действия или процесса.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "outcome_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если смешать синий и жёлтый цвета, <w>получится</w> зелёный.",
                            "target_lang_translations": {
                                "en": "If you mix blue and yellow colors, you <w>get</w> green."
                            }
                        },
                        {
                            "text": "Из этого куска ткани <w>получится</w> отличное платье для праздника.",
                            "target_lang_translations": {
                                "en": "This piece of fabric will <w>make</w> an excellent dress for the holiday."
                            }
                        }
                    ],
                    "synonyms": [
                        "выйти",
                        "выйти"
                    ],
                    "common_phrases": [
                        "что получится",
                        "получиться в итоге",
                        "получиться из"
                    ],
                    "target_lang_definitions": {
                        "en": "To form, arise, or be created as a result of an action or process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn out",
                                "target_lang_sense_clarification": "to result or end up being"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "to obtain as a result of calculation or mixing"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "to constitute or form something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10f8c523-383c-3229-8f33-6816295a5db2",
                    "sense_definition": "Сложиться определенным образом (об обстоятельствах), оказаться каким-либо по качеству или внешнему виду.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "outcome_situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Так <w>получилось</w>, что мы не смогли встретиться вовремя.",
                            "target_lang_translations": {
                                "en": "It just so <w>happened</w> that we couldn't meet on time."
                            }
                        },
                        {
                            "text": "На этой фотографии ты <w>получился</w> очень весёлым и счастливым.",
                            "target_lang_translations": {
                                "en": "In this photo you <w>came out</w> looking very cheerful and happy."
                            }
                        }
                    ],
                    "synonyms": [
                        "выйти",
                        "сложиться",
                        "произойти",
                        "случиться"
                    ],
                    "common_phrases": [
                        "так получилось",
                        "как получилось",
                        "случайно получилось",
                        "получиться на фото"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в значении 'произойти' или 'выглядеть'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To develop in a certain way (of circumstances), or to appear/look a certain way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "happen",
                                "target_lang_sense_clarification": "to occur by chance or circumstance"
                            },
                            {
                                "target_lang_word": "come out",
                                "target_lang_sense_clarification": "to appear in a photograph or recording"
                            },
                            {
                                "target_lang_word": "turn out",
                                "target_lang_sense_clarification": "to prove to be the case"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "8ca53902-b363-3ab1-be55-55bd8de5ee07",
                    "sense_definition": "The simple past tense of <w>ding</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The church bell <w>dang</w> loudly across the quiet valley.",
                            "target_lang_translations": {
                                "ru": "Церковный колокол громко <w>звенел</w> над тихой долиной."
                            }

            "senses": [
                {
                    "sense_id": "9b2b7f3a-b517-30cf-8c44-bce2e1719a83",
                    "sense_definition": "A famous archaeological site of the Maya civilization located in Chiapas, Mexico, or one of several towns in Latin America.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "locations",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Tourists travel from all over the world to see the ancient pyramids of <w>Palenque</w>.",
                            "target_lang_translations": {
                                "ru": "Туристы со всего мира приезжают, чтобы увидеть древние пирамиды <w>Паленке</w>."
                            }
                        },
                        {
                            "text": "The inscriptions found at <w>Palenque</w> have provided key insights into Maya history.",
                            "target_lang_translations": {
                                "ru": "Надписи, найденные в <w>Паленке</w>, дали ключевое представление об истории майя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ruins of Palenque",
                        "visit Palenque"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Latin America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Известный археологический памятник цивилизации майя, расположенный в штате Чьяпас, Мексика, или название нескольких городов в Латинской Америке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Паленке",
                                "target_lang_sense_clarification": "древний город майя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2dcd89d2-4be9-31de-9fa4-fc36ffceaa6c",
                    "sense_definition": "A community or fortified settlement established by runaway slaves in Latin America.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "communities",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The escaped slaves built a <w>palenque</w> in the mountains to defend their freedom.",
                            "target_lang_translations": {
                                "ru": "Беглые рабы построили <w>паленке</w> в горах, чтобы защитить свою свободу."
                            }
                        },
                        {
                            "text": "Life in a <w>palenque</w> was organized around communal defense and agriculture.",
                            "target_lang_translations": {
                                "ru": "Жизнь в <w>паленке</w> была организована вокруг совместной обороны и сельского хозяйства."
                            }
                        }
                    ],
                    "synonyms": [
                        "maroon community"
                    ],
                    "common_phrases": [
                        "establish a palenque",
                        "fortified palenque"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Latin America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Община или укрепленное поселение, основанное беглыми рабами в Латинской Америке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паленке",
                                "target_lang_sense_clarification": "поселение беглых рабов"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7b73dc72-3efe-3ddb-869f-902dcbe86758",
                    "sense_definition": "To bring people or things together to form a single unit or group.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "unite_verb_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new government will try to <w>unite</w> the various factions.",
                            "target_lang_translations": {
                                "ru": "Новое правительство попытается <w>объединить</w> различные фракции."
                            }
                        },
                        {
                            "text": "Music has the power to <w>unite</w> people from different cultures.",
                            "target_lang_translations": {
                                "ru": "Музыка обладает силой <w>объединять</w> людей разных культур."
                            }
                        }
                    ],
                    "synonyms": [
                        "join"
                    ],
                    "antonyms": [
                        "divide",
                        "separate"
                    ],
                    "common_phrases": [
                        "unite the country",
                        "unite the people",
                        "unite efforts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединять людей или вещи вместе, образуя единое целое или группу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединять",
                                "target_lang_sense_clarification": "соединять в одно целое"
                            },
                            {
                                "target_lang_word": "сплачивать",
                                "target_lang_sense_clarification": "делать дружным, единодушным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a98df84-6d94-3148-9c51-964e483d7f20",
                    "sense_definition": "To come together or join forces for a common purpose.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "unite_verb_group",
                    "name_type": "no",
                    "examples": [
                        {

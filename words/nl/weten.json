                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "gebruikt in de constructie 'te weten komen'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To discover something; to find out information one didn't have yet.",
                        "ru": "Узнавать что-либо; выяснять информацию, которая ранее была неизвестна."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find out",
                                "target_lang_sense_clarification": "to discover information"
                            },
                            {

            "senses": [
                {
                    "sense_id": "27d41831-5ec2-363b-9c68-f2a76e6459f5",
                    "sense_definition": "Przywódca plemienia lub klanu, sprawujący władzę nad społecznością.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tribal_leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <w>wódz</w> zebrał radę starszych, aby omówić warunki pokoju.",
                            "target_lang_translations": {
                                "ru": "Старый <w>вождь</w> собрал совет старейшин, чтобы обсудить условия мира."
                            }
                        },
                        {
                            "text": "Jako <w>wódz</w> plemienia, musiał dbać o bezpieczeństwo wszystkich mieszkańców wioski.",
                            "target_lang_translations": {
                                "ru": "Как <w>вождь</w> племени, он должен был заботиться о безопасности всех жителей деревни."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywódca"
                    ],
                    "common_phrases": [
                        "wielki wódz",
                        "wódz plemienia",
                        "wódz indiański"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предводитель племени или клана, обладающий властью над общиной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вождь",
                                "target_lang_sense_clarification": "племени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8b71898-d2c4-3883-beaf-9a7fe262a255",
                    "sense_definition": "Osoba sprawująca najwyższe dowództwo nad wojskiem lub państwem.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "military_leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wódz</w> naczelny wydał rozkaz do natychmiastowego ataku na pozycje wroga.",
                            "target_lang_translations": {
                                "ru": "<w>Главнокомандующий</w> отдал приказ о немедленной атаке на позиции врага."
                            }
                        },
                        {
                            "text": "Żołnierze ufali, że ich <w>wódz</w> poprowadzi ich do ostatecznego zwycięstwa.",
                            "target_lang_translations": {
                                "ru": "Солдаты верили, что их <w>полководец</w> приведет их к окончательной победе."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywódca"
                    ],
                    "common_phrases": [
                        "wódz naczelny",
                        "wódz narodu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, осуществляющее верховное командование армией или государством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полководец",
                                "target_lang_sense_clarification": "военный предводитель"
                            },

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9bd228d3-717c-37f1-b52a-ba8891c50a9b",
                    "sense_definition": "W sposób obustronny, tak że działania lub uczucia dotyczą obu stron relacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiedzi powinni sobie <w>wzajemnie</w> pomagać w trudnych sytuacjach.",
                            "target_lang_translations": {
                                "ru": "Соседи должны <w>взаимно</w> помогать друг другу в трудных ситуациях."
                            }
                        },
                        {
                            "text": "Te dwa kolory <w>wzajemnie</w> się uzupełniają, tworząc piękny obraz.",
                            "target_lang_translations": {
                                "ru": "Эти два цвета <w>взаимно</w> дополняют друг друга, создавая красивую картину."
                            }
                        }
                    ],
                    "synonyms": [
                        "nawzajem",
                        "wzajem"
                    ],
                    "antonyms": [
                        "niewzajemnie"
                    ],
                    "common_phrases": [
                        "wspierać się wzajemnie",
                        "wykluczać się wzajemnie",
                        "uzupełniać się wzajemnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двусторонним образом, так что действия или чувства касаются обеих сторон отношений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взаимно",
                                "target_lang_sense_clarification": "совершаемое обеими сторонами"
                            },
                            {
                                "target_lang_word": "обоюдно",
                                "target_lang_sense_clarification": "с обеих сторон"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "eec607c7-a055-3045-935a-de809a2d40e9",
                    "sense_definition": "Zwrot grzecznościowy używany w odpowiedzi na czyjeś życzenia, oznaczający życzenie tego samego rozmówcy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "– Życzę ci miłego dnia! – Dziękuję i <w>wzajemnie</w>.",
                            "target_lang_translations": {
                                "ru": "— Желаю тебе хорошего дня! — Спасибо, и <w>тебе того же</w>."
                            }
                        },
                        {
                            "text": "– Wesołych Świąt dla całej rodziny. – <w>Wzajemnie</w>!",
                            "target_lang_translations": {
                                "ru": "— Веселых праздников всей семье. — <w>И вам того же</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "nawzajem"
                    ],
                    "common_phrases": [
                        "dziękuję i wzajemnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливая фраза, используемая в ответ на чьи-то пожелания, означающая пожелание того же самого собеседнику."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и вам того же",
                                "target_lang_sense_clarification": "ответное пожелание (формально)"
                            },
                            {
                                "target_lang_word": "взаимно",
                                "target_lang_sense_clarification": "краткий вежливый ответ"
                            },
                            {
                                "target_lang_word": "и тебе того же",
                                "target_lang_sense_clarification": "ответное пожелание (неформально)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

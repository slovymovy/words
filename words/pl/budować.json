            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9613ebfc-e5df-3f28-a4a0-32e9bf9184a8",
                    "sense_definition": "Wznosić obiekt, taki jak dom, most czy droga, łącząc ze sobą różne materiały i elementy konstrukcyjne.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice planują <w>budować</w> nowy dom na wsi w przyszłym roku.",
                            "target_lang_translations": {
                                "ru": "Мои родители планируют <w>строить</w> новый дом в деревне в следующем году."
                            }
                        },
                        {
                            "text": "Dzieci uwielbiają <w>budować</w> wysokie wieże z kolorowych klocków.",
                            "target_lang_translations": {
                                "ru": "Дети обожают <w>строить</w> высокие башни из разноцветных кубиков."
                            }
                        }
                    ],
                    "synonyms": [
                        "wznosić",
                        "stawiać",
                        "konstruować"
                    ],
                    "antonyms": [
                        "burzyć",
                        "niszczyć",
                        "rozbierać"
                    ],
                    "common_phrases": [
                        "budować dom",
                        "budować most",
                        "budować z klocków",
                        "budować gniazdo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возводить объект, такой как дом, мост или дорога, соединяя различные материалы и элементы конструкции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строить",
                                "target_lang_sense_clarification": "сооружать здания или конструкции"
                            },
                            {
                                "target_lang_word": "возводить",
                                "target_lang_sense_clarification": "строить высокие или значительные объекты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffb05a12-aca4-3415-9359-a132677664d5",
                    "sense_definition": "Tworzyć i rozwijać coś niematerialnego, np. relacje, atmosferę, karierę lub organizację, poprzez systematyczne działanie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy menedżer stara się <w>budować</w> zaufanie w swoim zespole poprzez otwartą komunikację.",
                            "target_lang_translations": {
                                "ru": "Новый менеджер старается <w>выстраивать</w> доверие в своей команде посредством открытого общения."
                            }
                        },
                        {
                            "text": "Autor kryminałów potrafi mistrzowsko <w>budować</w> napięcie aż do ostatniej strony.",
                            "target_lang_translations": {
                                "ru": "Автор детективов умеет мастерски <w>нагнетать</w> напряжение вплоть до последней страницы."
                            }
                        }
                    ],
                    "synonyms": [
                        "konstruować",
                        "stwarzać",
                        "wznosić",
                        "stawiać"
                    ],
                    "antonyms": [
                        "niszczyć",
                        "rujnować",
                        "psuć"
                    ],
                    "common_phrases": [
                        "budować zaufanie",
                        "budować atmosferę",
                        "budować napięcie",
                        "budować przyszłość",
                        "budować relacje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать и развивать что-то нематериальное, например, отношения, атмосферу, карьеру или организацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строить",
                                "target_lang_sense_clarification": "создавать планы, отношения, карьеру"
                            },
                            {
                                "target_lang_word": "создавать",
                                "target_lang_sense_clarification": "творить, формировать что-то новое"
                            },
                            {
                                "target_lang_word": "выстраивать",
                                "target_lang_sense_clarification": "организовывать систему или отношения шаг за шагом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de258efa-bd5f-3f6b-8804-5f545e86be32",
                    "sense_definition": "Powstawać w wyniku prac budowlanych; być w trakcie wznoszenia (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naprzeciwko naszej szkoły <w>buduje</w> się nowoczesne centrum handlowe.",
                            "target_lang_translations": {
                                "ru": "Напротив нашей школы <w>строится</w> современный торговый центр."
                            }
                        },
                        {
                            "text": "To osiedle mieszkaniowe <w>buduje</w> się już od ponad dwóch lat.",
                            "target_lang_translations": {
                                "ru": "Этот жилой комплекс <w>строится</w> уже более двух лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "wznosić",
                        "konstruować",
                        "stawiać"
                    ],
                    "common_phrases": [

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "22d623cc-4263-3779-bcbd-f8c44d68f883",
                    "sense_definition": "Wielkość fizyczna przedmiotu określona przez jego długość, szerokość i wysokość.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę sprawdzić dokładne <w>wymiary</w> tej szafy, żeby wiedzieć, czy zmieści się w pokoju.",
                            "target_lang_translations": {
                                "ru": "Мне нужно проверить точные <w>размеры</w> этого шкафа, чтобы знать, поместится ли он в комнате."
                            }
                        },
                        {
                            "text": "Paczka przekracza dopuszczalne <w>wymiary</w> i nie można jej wysłać pocztą.",
                            "target_lang_translations": {
                                "ru": "Посылка превышает допустимые <w>габариты</w>, и её нельзя отправить по почте."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozmiar"
                    ],
                    "common_phrases": [
                        "podać wymiary",
                        "wymiary zewnętrzne",
                        "idealne wymiary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая величина предмета, определяемая его длиной, шириной и высотой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размер",
                                "target_lang_sense_clarification": "величина предмета"
                            },
                            {
                                "target_lang_word": "габариты",
                                "target_lang_sense_clarification": "внешние очертания предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2a2930d-e424-3d2c-8ba6-77f423afe64d",
                    "sense_definition": "Współrzędna określająca położenie w przestrzeni lub czasie; w fantastyce często oznacza świat równoległy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "space_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żyjemy w przestrzeni, która ma trzy <w>wymiary</w> przestrzenne i jeden czasowy.",
                            "target_lang_translations": {
                                "ru": "Мы живем в пространстве, которое имеет три пространственных <w>измерения</w> и одно временное."
                            }
                        },
                        {
                            "text": "W tej powieści bohaterowie przenoszą się do innego, magicznego <w>wymiaru</w>.",
                            "target_lang_translations": {
                                "ru": "В этом романе герои переносятcя в другое, магическое <w>измерение</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trzeci wymiar",
                        "w innym wymiarze",
                        "cztery wymiary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Координата, определяющая положение в пространстве или времени; в фантастике часто означает параллельный мир."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "измерение",
                                "target_lang_sense_clarification": "пространственно-временная координата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35a0a075-b659-3cdf-bd24-14c7042c1f36",
                    "sense_definition": "Zakres, znaczenie, waga lub aspekt jakiegoś zjawiska, problemu lub sytuacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_aspect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kryzys gospodarczy przybrał globalny <w>wymiar</w> i dotknął wiele krajów.",
                            "target_lang_translations": {
                                "ru": "Экономический кризис приобрел глобальный <w>масштаб</w> и затронул многие страны."
                            }
                        },
                        {
                            "text": "To wydarzenie ma dla nas nie tylko <w>wymiar</w> polityczny, ale także społeczny.",
                            "target_lang_translations": {
                                "ru": "Это событие имеет для нас не только политический <w>аспект</w>, но и социальный."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wymiar społeczny",
                        "szerszy wymiar",
                        "nabrać wymiaru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охват, значение, важность или аспект какого-либо явления, проблемы или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "масштаб",
                                "target_lang_sense_clarification": "размах, охват явления"
                            },
                            {
                                "target_lang_word": "аспект",
                                "target_lang_sense_clarification": "сторона, точка зрения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "785583d7-0cbb-350f-a733-34771618cc4d",
                    "sense_definition": "Określona ilość, wielkość lub zakres czegoś, często w odniesieniu do czasu pracy lub kary sądowej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "amount_extent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obecnie jestem zatrudniony w firmie w pełnym <w>wymiarze</w> godzin.",
                            "target_lang_translations": {
                                "ru": "В настоящее время я работаю в компании на полную <w>ставку</w>."
                            }
                        },
                        {
                            "text": "Sąd wziął pod uwagę okoliczności łagodzące i zmniejszył <w>wymiar</w> kary.",
                            "target_lang_translations": {
                                "ru": "Суд принял во внимание смягчающие обстоятельства и уменьшил <w>срок</w> наказания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wymiar czasu pracy",
                        "wymiar kary",
                        "w pełnym wymiarze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенное количество, величина или объем чего-либо, часто в отношении рабочего времени или судебного наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размер",
                                "target_lang_sense_clarification": "величина (наказания, выплаты)"
                            },
                            {
                                "target_lang_word": "объём",
                                "target_lang_sense_clarification": "количество (работы, времени)"
                            },
                            {
                                "target_lang_word": "срок",
                                "target_lang_sense_clarification": "длительность (наказания)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af31fb50-8e61-34e2-8d42-c7f6391e3c4d",
                    "sense_definition": "Cecha wielkości fizycznej określająca jej rodzaj w zależności od jednostek podstawowych (np. długość, czas, masa).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physics_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas rozwiązywania zadania musisz sprawdzić, czy zgadza się <w>wymiar</w> jednostek po obu stronach równania.",
                            "target_lang_translations": {
                                "ru": "При решении задачи ты должен проверить, совпадает ли <w>размерность</w> единиц по обе стороны уравнения."
                            }
                        },
                        {
                            "text": "Amper jest jednostką natężenia prądu i ma swój określony <w>wymiar</w> w układzie SI.",
                            "target_lang_translations": {
                                "ru": "Ампер является единицей силы тока и имеет свою определенную <w>размерность</w> в системе СИ."
                            }
                        }
                    ],
                    "common_phrases": [
                        "analiza wymiarowa",
                        "wymiar wielkości fizycznej"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in scientific/technical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство физической величины, определяющее её вид в зависимости от основных единиц измерения (например, длины, времени, массы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размерность",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7193e848-e6a9-361d-bdbd-6d7e2de51f88",
                    "sense_definition": "Liczbowe oznaczenie wielkości elementu umieszczone na rysunku technicznym.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_drawing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Architekt zapomniał nanieść jednego ważnego <w>wymiaru</w> na planie budowy.",
                            "target_lang_translations": {
                                "ru": "Архитектор забыл нанести один важный <w>размер</w> на план строительства."
                            }
                        },
                        {
                            "text": "Wszystkie <w>wymiary</w> na tym rysunku są podane w milimetrach.",
                            "target_lang_translations": {
                                "ru": "Все <w>размеры</w> на этом чертеже указаны в миллиметрах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "linia wymiarowa",
                        "nanieść wymiary"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Числовое обозначение величины элемента, указанное на техническом чертеже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размер",
                                "target_lang_sense_clarification": "числовое значение на чертеже"
                            }
                        ]
                    }
                }
            ]
        }
    ]

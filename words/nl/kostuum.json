    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8d0980c9-1836-3034-9455-a5912508f331",
                    "sense_definition": "Een set nette kleding voor mannen, bestaande uit een jasje en een broek van dezelfde stof, vaak gedragen met een overhemd en das",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing_suit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij moet voor zijn werk op kantoor altijd een <w>kostuum</w> dragen.",
                            "target_lang_translations": {
                                "ru": "На работе в офисе он всегда должен носить <w>костюм</w>."
                            }
                        },
                        {
                            "text": "Mijn broer heeft een duur driedelig <w>kostuum</w> gekocht voor zijn bruiloft.",
                            "target_lang_translations": {
                                "ru": "Мой брат купил дорогой <w>костюм</w>-тройку на свою свадьбу."
                            }
                        },
                        {
                            "text": "We moesten daar in <w>kostuum</w> heen.",
                            "target_lang_translations": {
                                "ru": "Нам нужно было пойти туда в <w>костюме</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pak"
                    ],
                    "common_phrases": [
                        "in kostuum",
                        "driedelig kostuum",
                        "zwart kostuum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплект строгой мужской одежды, состоящий из пиджака и брюк из одной ткани, который часто носят с рубашкой и галстуком"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "костюм",
                                "target_lang_sense_clarification": "мужской, деловой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "103a598f-443c-3d6c-a459-80fea1c63fa5",
                    "sense_definition": "Kleding die bedoeld is om een bepaald personage uit te beelden of die bij een specifieke rol hoort, bijvoorbeeld in een toneelstuk, film of tijdens carnaval",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing_theatrical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De acteurs droegen prachtige historische <w>kostuums</w> uit de achttiende eeuw.",
                            "target_lang_translations": {
                                "ru": "Актеры носили прекрасные исторические <w>костюмы</w> XVIII века."
                            }
                        },
                        {
                            "text": "Wat een mooi <w>kostuum</w> heb je aan!",
                            "target_lang_translations": {
                                "ru": "Какой на тебе красивый <w>костюм</w>!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "historisch kostuum",
                        "traditioneel kostuum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одежда, предназначенная для изображения определенного персонажа или роли, например, в театре, кино или на карнавале"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "костюм",
                                "target_lang_sense_clarification": "театральный, маскарадный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "przełomowy",
        "przełamać",
        "przełamywać",
        "przełamanie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3a22af7b-033b-32d2-b08b-2d7d4cb5755a",
                    "sense_definition": "Granica między dwoma okresami czasu, koniec jednego i początek drugiego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Urodził się na <w>przełomie</w> maja i czerwca.",
                            "target_lang_translations": {
                                "ru": "Он родился на <w>стыке</w> мая и июня."
                            }
                        },
                        {
                            "text": "Ten styl w architekturze dominował na <w>przełomie</w> dziewiętnastego i dwudziestego wieku.",
                            "target_lang_translations": {
                                "ru": "Этот архитектурный стиль доминировал на <w>рубеже</w> девятнадцатого и двадцатого веков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "na przełomie roku",
                        "na przełomie wieków",
                        "przełom tysiącleci",
                        "na przełomie miesięcy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Граница между двумя периодами времени, конец одного и начало другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рубеж",
                                "target_lang_sense_clarification": "граница веков, эпох или лет"
                            },
                            {
                                "target_lang_word": "стык",
                                "target_lang_sense_clarification": "место соединения временных отрезков (месяцев, сезонов)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f901e57-45c4-32ad-9fdb-33743406b7c4",
                    "sense_definition": "Nagła, zasadnicza zmiana, zwrotny moment w przebiegu jakichś spraw lub zdarzeń.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "change_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To odkrycie naukowe stanowiło prawdziwy <w>przełom</w> w medycynie.",
                            "target_lang_translations": {
                                "ru": "Это научное открытие стало настоящим <w>прорывом</w> в медицине."
                            }
                        },
                        {
                            "text": "Mamy nadzieję, że ta rozmowa przyniesie <w>przełom</w> w naszych negocjacjach.",
                            "target_lang_translations": {
                                "ru": "Мы надеемся, что этот разговор внесет <w>перелом</w> в наши переговоры."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwrot"
                    ],
                    "common_phrases": [
                        "przełom w sprawie",
                        "dokonać przełomu",
                        "życiowy przełom",
                        "nastąpił przełom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резкое, коренное изменение; поворотный момент в развитии событий или дел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прорыв",
                                "target_lang_sense_clarification": "значительное достижение, резкий шаг вперед"
                            },
                            {
                                "target_lang_word": "перелом",
                                "target_lang_sense_clarification": "резкое изменение хода событий"
                            },
                            {
                                "target_lang_word": "сдвиг",
                                "target_lang_sense_clarification": "заметное изменение в положении дел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad6f1390-7746-3717-8b3b-cb4e0b656688",
                    "sense_definition": "Odcinek wąskiej doliny o stromych zboczach, w którym rzeka przedziera się przez pasmo górskie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Turyści chętnie wybierają się na spływ przez malowniczy <w>przełom</w> rzeki.",
                            "target_lang_translations": {
                                "ru": "Туристы охотно отправляются на сплав через живописное <w>ущелье</w> реки."
                            }
                        },
                        {
                            "text": "Rzeka wyżłobiła głęboki <w>przełom</w> w skałach wapiennych.",
                            "target_lang_translations": {
                                "ru": "Река пробила глубокий <w>каньон</w> в известняковых скалах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przełom rzeki",
                        "przełom Dunajca",
                        "skalny przełom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок глубокой речной долины с крутыми склонами, где река прорезает горный хребет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ущелье",
                                "target_lang_sense_clarification": "глубокая долина"
                            },
                            {
                                "target_lang_word": "каньон",
                                "target_lang_sense_clarification": "глубокая долина с отвесными берегами"
                            },
                            {
                                "target_lang_word": "прорыв",
                                "target_lang_sense_clarification": "место, где река пробила русло через горы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8acde52d-ea19-3365-a594-48132b0e2609",
                    "sense_definition": "Moment zwrotny w przebiegu ostrej choroby, po którym następuje wyraźna poprawa lub pogorszenie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz poinformował rodzinę, że w nocy nastąpił <w>przełom</w> i pacjent wraca do zdrowia.",
                            "target_lang_translations": {
                                "ru": "Врач сообщил семье, что ночью наступил <w>перелом</w> (кризис миновал), и пациент идет на поправку."
                            }
                        },
                        {
                            "text": "Nagły spadek gorączki oznaczał <w>przełom</w> w walce z infekcją.",
                            "target_lang_translations": {
                                "ru": "Внезапное падение температуры означало <w>перелом</w> в борьбе с инфекцией."
                            }
                        }
                    ],
                    "synonyms": [
                        "kryzys",
                        "przesilenie"
                    ],
                    "common_phrases": [
                        "przełom w chorobie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решающий момент в течении острой болезни, после которого наступает улучшение или ухудшение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кризис",
                                "target_lang_sense_clarification": "медицинский термин"
                            },
                            {
                                "target_lang_word": "перелом",
                                "target_lang_sense_clarification": "момент изменения состояния"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3de71535-97d0-3c9a-8b28-26540840c377",
                    "sense_definition": "Manewr szachowy mający na celu otwarcie linii lub zdobycie przewagi pozycyjnej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "chess_tactic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mistrz zastosował niespodziewany <w>przełom</w> w centrum szachownicy.",
                            "target_lang_translations": {
                                "ru": "Мастер применил неожиданный <w>прорыв</w> в центре доски."
                            }
                        },
                        {
                            "text": "Ten odważny <w>przełom</w> pionkami pozwolił mu na szybki atak na króla.",
                            "target_lang_translations": {
                                "ru": "Этот смелый пешечный <w>прорыв</w> позволил ему быстро атаковать короля."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical (chess)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шахматный маневр, направленный на вскрытие линий или получение позиционного преимущества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прорыв",
                                "target_lang_sense_clarification": "тактический прием"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
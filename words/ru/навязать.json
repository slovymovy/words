    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b5ee5ca2-774e-35fd-bf2e-06f554caffc5",
                    "sense_definition": "Прикрепить что-либо к чему-либо, привязав концы; сделать узел.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tie_fasten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рыбак помог мне <w>навязать</w> новый крючок на леску.",
                            "target_lang_translations": {
                                "en": "The fisherman helped me <w>tie</w> a new hook onto the line."
                            }
                        },
                        {
                            "text": "Нужно крепко <w>навязать</w> верёвку на этот столб, чтобы лодка не уплыла.",
                            "target_lang_translations": {
                                "en": "We need to firmly <w>tie</w> the rope to this post so the boat doesn't float away."
                            }
                        }
                    ],
                    "synonyms": [
                        "привязать"
                    ],
                    "antonyms": [
                        "отвязать"
                    ],
                    "common_phrases": [
                        "навязать узел",
                        "навязать верёвку"
                    ],
                    "target_lang_definitions": {
                        "en": "To attach something to something else by tying the ends; to make a knot."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tie on",
                                "target_lang_sense_clarification": "attach by tying"
                            },
                            {
                                "target_lang_word": "fasten",
                                "target_lang_sense_clarification": "secure with a rope or string"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8230f9a4-9fb9-3553-a700-7d41b1fd0626",
                    "sense_definition": "Заставить кого-либо принять что-либо (мнение, волю, услуги, товар) против его желания.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "impose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Продавец в магазине пытался <w>навязать</w> нам совершенно ненужный товар.",
                            "target_lang_translations": {
                                "en": "The shop assistant tried to <w>foist</w> completely unnecessary goods on us."
                            }
                        },
                        {
                            "text": "Никто не имеет права <w>навязать</w> мне своё мнение, если я с ним не согласен.",
                            "target_lang_translations": {
                                "en": "No one has the right to <w>impose</w> their opinion on me if I disagree with it."
                            }
                        }
                    ],
                    "synonyms": [
                        "привязать"
                    ],
                    "common_phrases": [
                        "навязать своё мнение",
                        "навязать услуги",
                        "навязать борьбу",
                        "навязать свою волю"
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone to accept something (opinion, will, services, goods) against their desire."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "impose",
                                "target_lang_sense_clarification": "force an opinion or rule"
                            },
                            {
                                "target_lang_word": "foist",
                                "target_lang_sense_clarification": "force something unwanted"
                            },
                            {
                                "target_lang_word": "force upon",
                                "target_lang_sense_clarification": "compel to accept"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9e4d895-a182-3dfb-829d-3161922046cc",
                    "sense_definition": "Изготовить вязанием в большом количестве.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "knit_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За эту зиму бабушка успела <w>навязать</w> внукам много тёплых носков.",
                            "target_lang_translations": {
                                "en": "Over this winter, grandma managed to <w>knit</w> a lot of warm socks for the grandchildren."
                            }
                        },
                        {
                            "text": "Она купила пряжу, чтобы <w>навязать</w> шарфов для всей семьи.",
                            "target_lang_translations": {
                                "en": "She bought yarn to <w>knit</w> scarves for the whole family."
                            }
                        }
                    ],
                    "synonyms": [
                        "понавязать"
                    ],
                    "antonyms": [
                        "распустить"
                    ],
                    "common_phrases": [
                        "навязать носков",
                        "навязать варежек"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies creating a large quantity"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To produce a large quantity of items by knitting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "knit",
                                "target_lang_sense_clarification": "create a quantity of items"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79a6773f-4b14-337d-a506-00df47af34ed",
                    "sense_definition": "Прилипать, приставать к поверхности (о грязи, снеге и т. п.); застревать.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "stick",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тяжёлая глина <w>навязала</w> на сапоги, и идти стало очень трудно.",
                            "target_lang_translations": {
                                "en": "Heavy clay <w>stuck</w> to the boots, and walking became very difficult."
                            }
                        },
                        {
                            "text": "Мокрый снег <w>навязал</w> на колёса машины, мешая движению.",
                            "target_lang_translations": {
                                "en": "Wet snow <w>caked</w> onto the car wheels, hindering movement."
                            }
                        }
                    ],
                    "common_phrases": [
                        "грязь навязала"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "standard form is usually налипать or вязнуть"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stick or adhere to a surface (regarding mud, snow, etc.); to accumulate."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stick",
                                "target_lang_sense_clarification": "adhere to a surface"
                            },
                            {
                                "target_lang_word": "cake",
                                "target_lang_sense_clarification": "accumulate in a layer"
                            }
                        ]
                    }
                }
            ]
        }
    ]

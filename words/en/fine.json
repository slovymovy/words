{
    "word_family": [
        "finely",
        "fineness",
        "refine",
        "refinement",
        "refinery"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "83b0fbe0-7ece-3086-8af0-d55da8074fa4",
                    "sense_definition": "Being acceptable, adequate, passable, or satisfactory.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quality_satisfactory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "How are you today? – I am <w>fine</w>, thank you.",
                            "target_lang_translations": {
                                "ru": "Как вы сегодня? – Я в <w>порядке</w>, спасибо."
                            }
                        },
                        {
                            "text": "It is <w>fine</w> with me if you want to stay out late.",
                            "target_lang_translations": {
                                "ru": "Мне <w>нормально</w>, если ты хочешь погулять подольше."
                            }
                        }
                    ],
                    "synonyms": [
                        "ok",
                        "okay",
                        "all right",
                        "alright",
                        "OK",
                        "o.k."
                    ],
                    "common_phrases": [
                        "I am fine",
                        "that is fine",
                        "fine by me"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть приемлемым, адекватным или удовлетворительным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нормальный",
                                "target_lang_sense_clarification": "приемлемый, обычный"
                            },
                            {
                                "target_lang_word": "в порядке",
                                "target_lang_sense_clarification": "о состоянии дел или здоровье"
                            },
                            {
                                "target_lang_word": "хороший",
                                "target_lang_sense_clarification": "удовлетворительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0f0b2e1-510d-3d83-bc6a-ae63ebcab391",
                    "sense_definition": "Of superior quality; excellent.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quality_superior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They enjoyed a bottle of <w>fine</w> wine with their dinner.",
                            "target_lang_translations": {
                                "ru": "За ужином они насладились бутылкой <w>изысканного</w> вина."
                            }
                        },
                        {
                            "text": "The museum has a large collection of <w>fine</w> art.",
                            "target_lang_translations": {
                                "ru": "В музее большая коллекция <w>изобразительного</w> искусства."
                            }
                        }
                    ],
                    "synonyms": [
                        "good",
                        "excellent"
                    ],
                    "common_phrases": [
                        "fine dining",
                        "fine art",
                        "fine wine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокого качества; отличный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "превосходный",
                                "target_lang_sense_clarification": "очень высокого качества"
                            },
                            {
                                "target_lang_word": "изысканный",
                                "target_lang_sense_clarification": "утонченный, о вкусе или искусстве"
                            },
                            {
                                "target_lang_word": "отличный",
                                "target_lang_sense_clarification": "замечательный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c27fc5b-de05-389e-b9a9-b3ad32b04969",
                    "sense_definition": "Sunny and not raining (describing weather).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We should go for a walk while the weather is <w>fine</w>.",
                            "target_lang_translations": {
                                "ru": "Нам стоит пойти на прогулку, пока погода <w>хорошая</w>."
                            }
                        },
                        {
                            "text": "It was a <w>fine</w> sunny morning in May.",
                            "target_lang_translations": {
                                "ru": "Это было <w>ясное</w> солнечное майское утро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fine weather",
                        "fine day"
                    ],
                    "target_lang_definitions": {
                        "ru": "Солнечный, без дождя (о погоде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ясный",
                                "target_lang_sense_clarification": "безоблачный"
                            },
                            {
                                "target_lang_word": "хороший",
                                "target_lang_sense_clarification": "приятный, без осадков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3bda50b-08fc-3414-a097-fdc5ba4ffd57",
                    "sense_definition": "Consisting of very small particles or having a very thin structure; not coarse.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "texture_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to grind the coffee beans into a <w>fine</w> powder.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно смолоть кофейные зерна в <w>мелкий</w> порошок."
                            }
                        },
                        {
                            "text": "She has very <w>fine</w> hair that is difficult to style.",
                            "target_lang_translations": {
                                "ru": "У нее очень <w>тонкие</w> волосы, которые трудно укладывать."
                            }
                        }
                    ],
                    "synonyms": [
                        "powdery",
                        "fine-grained",
                        "powdered"
                    ],
                    "antonyms": [
                        "coarse"
                    ],
                    "common_phrases": [
                        "fine powder",
                        "fine sand",
                        "fine hair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий из очень мелких частиц или имеющий очень тонкую структуру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелкий",
                                "target_lang_sense_clarification": "о частицах, порошке, песке"
                            },
                            {
                                "target_lang_word": "тонкий",
                                "target_lang_sense_clarification": "о структуре, волосах, нитях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88eee4b4-4521-3dab-83b1-5f3a0866e539",
                    "sense_definition": "Good-looking or attractive.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He thinks that the new actor is so <w>fine</w>.",
                            "target_lang_translations": {
                                "ru": "Он считает, что новый актер такой <w>красивый</w>."
                            }
                        },
                        {
                            "text": "She looks <w>fine</w> in her new dress.",
                            "target_lang_translations": {
                                "ru": "Она выглядит <w>прекрасно</w> в своем новом платье."
                            }
                        }
                    ],
                    "common_phrases": [
                        "look fine"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe people"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Привлекательный или красивый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красивый",
                                "target_lang_sense_clarification": "общая внешняя привлекательность"
                            },
                            {
                                "target_lang_word": "привлекательный",
                                "target_lang_sense_clarification": "притягивающий взгляд"
                            },
                            {
                                "target_lang_word": "прекрасный",
                                "target_lang_sense_clarification": "очень красивый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7418bdae-2e4f-3f56-a434-81a92cc09f22",
                    "sense_definition": "Subtle, delicately balanced, or requiring precision.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "subtle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a <w>fine</w> line between confidence and arrogance.",
                            "target_lang_translations": {
                                "ru": "Между уверенностью и высокомерием существует <w>тонкая</w> грань."
                            }
                        },
                        {
                            "text": "Make sure you read the <w>fine</w> print before signing the contract.",
                            "target_lang_translations": {
                                "ru": "Обязательно прочитайте <w>мелкий</w> шрифт перед подписанием контракта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fine line",
                        "fine print",
                        "fine distinction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Едва уловимый, требующий точности или деликатности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тонкий",
                                "target_lang_sense_clarification": "едва различимый, деликатный"
                            },
                            {
                                "target_lang_word": "мелкий",
                                "target_lang_sense_clarification": "о шрифте или деталях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "90d72afe-4a91-39ce-9c04-a11bd2a15893",
                    "sense_definition": "In a positive, agreeable, or satisfactory way.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quality_satisfactory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't worry, everything will work out <w>fine</w>.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся, все будет <w>хорошо</w>."
                            }
                        },
                        {
                            "text": "The team did <w>fine</w> without their captain.",
                            "target_lang_translations": {
                                "ru": "Команда справилась <w>нормально</w> и без своего капитана."
                            }
                        }
                    ],
                    "synonyms": [
                        "very well",
                        "all right",
                        "alright",
                        "OK"
                    ],
                    "common_phrases": [
                        "work out fine",
                        "do fine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положительно, приемлемо или удовлетворительно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "успешно, как надо"
                            },
                            {
                                "target_lang_word": "нормально",
                                "target_lang_sense_clarification": "приемлемо"
                            },
                            {
                                "target_lang_word": "отлично",
                                "target_lang_sense_clarification": "очень хорошо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e29e5f7c-0178-30b3-8fff-49b8395cd3a2",
                    "sense_definition": "A sum of money levied as punishment for breaking the law or a rule.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legal_penalty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had to pay a <w>fine</w> for speeding.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось заплатить <w>штраф</w> за превышение скорости."
                            }
                        },
                        {
                            "text": "The library charges a small <w>fine</w> for late books.",
                            "target_lang_translations": {
                                "ru": "Библиотека взимает небольшой <w>штраф</w> за просроченные книги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "parking fine",
                        "pay a fine",
                        "heavy fine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежное взыскание в качестве наказания за нарушение закона или правила."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штраф",
                                "target_lang_sense_clarification": "денежное наказание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3d7070da-160b-3268-b85a-323d59318dda",
                    "sense_definition": "To issue a financial penalty to someone as punishment.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_penalty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company was <w>fined</w> for polluting the river.",
                            "target_lang_translations": {
                                "ru": "Компанию <w>оштрафовали</w> за загрязнение реки."
                            }
                        },
                        {
                            "text": "She was <w>fined</w> for parking in a no-stopping zone.",
                            "target_lang_translations": {
                                "ru": "Ее <w>оштрафовали</w> за парковку в неположенном месте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fined for speeding",
                        "heavily fined"
                    ],
                    "target_lang_definitions": {
                        "ru": "Налагать денежное взыскание (штраф) в качестве наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штрафовать",
                                "target_lang_sense_clarification": "налагать штраф (несов. вид)"
                            },
                            {
                                "target_lang_word": "оштрафовать",
                                "target_lang_sense_clarification": "наложить штраф (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2a1a2ec-abfc-3597-9033-ec89d0bb2ecd",
                    "sense_definition": "To make finer, purer, or cleaner; to clarify.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "purify",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They use a special process to <w>fine</w> the gold.",
                            "target_lang_translations": {
                                "ru": "Они используют специальный процесс, чтобы <w>очистить</w> золото."
                            }
                        },
                        {
                            "text": "Winemakers <w>fine</w> the wine before bottling it.",
                            "target_lang_translations": {
                                "ru": "Виноделы <w>осветляют</w> вино перед розливом в бутылки."
                            }
                        }
                    ],
                    "synonyms": [
                        "clarify",
                        "refine",
                        "purify"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать более чистым, очищать; осветлять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очищать",
                                "target_lang_sense_clarification": "удалять примеси"
                            },
                            {
                                "target_lang_word": "осветлять",
                                "target_lang_sense_clarification": "делать прозрачным (о жидкостях)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "c8a9bea6-1176-3092-b97f-cd41592ef697",
                    "sense_definition": "Used to express agreement, often reluctantly or to end an argument.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quality_satisfactory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Fine</w>, I will do it your way.",
                            "target_lang_translations": {
                                "ru": "<w>Ладно</w>, я сделаю по-твоему."
                            }
                        },
                        {
                            "text": "Oh, <w>fine</w>, let's go then.",
                            "target_lang_translations": {
                                "ru": "Ой, <w>ладно</w>, пошли тогда."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения согласия, часто неохотного, или чтобы закончить спор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ладно",
                                "target_lang_sense_clarification": "разговорное выражение согласия или уступки"
                            },
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "стандартное выражение согласия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An indefinite object, thing, or phenomenon (often used when the speaker cannot or does not want to name the object precisely)."

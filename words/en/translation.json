    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "29ee5990-c9a8-306b-8641-7e6793801089",
                    "sense_definition": "The process of changing text or speech from one language into another.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "language_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>translation</w> of the contract took longer than expected due to its complexity.",
                            "target_lang_translations": {
                                "ru": "<w>Перевод</w> контракта занял больше времени, чем ожидалось, из-за его сложности."
                            }
                        },
                        {
                            "text": "Software tools can assist with <w>translation</w>, but human review is often necessary.",
                            "target_lang_translations": {
                                "ru": "Программные средства могут помочь с <w>переводом</w>, но часто необходима проверка человеком."
                            }
                        }
                    ],
                    "synonyms": [
                        "conversion",
                        "rendering"
                    ],
                    "common_phrases": [
                        "literal translation",
                        "machine translation",
                        "simultaneous translation",
                        "lost in translation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс перевода текста или речи с одного языка на другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевод",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f9d619a-4a1b-3296-9524-82772cb41acc",
                    "sense_definition": "A text, book, or work that has been changed from one language into another.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "language_product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I prefer reading the original novel rather than the English <w>translation</w>.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю читать роман в оригинале, а не в английском <w>переводе</w>."
                            }
                        },
                        {
                            "text": "This <w>translation</w> captures the humor of the author very well.",
                            "target_lang_translations": {
                                "ru": "Этот <w>перевод</w> очень хорошо передает юмор автора."
                            }
                        }
                    ],
                    "synonyms": [
                        "version"
                    ],
                    "common_phrases": [
                        "accurate translation",
                        "English translation",
                        "authorized translation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Текст, книга или произведение, переведенные с одного языка на другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевод",
                                "target_lang_sense_clarification": "результат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "422418f0-a8f5-3dd6-931b-938fa5b5d092",
                    "sense_definition": "The conversion of something from one form, medium, or state to another.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_conversion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>translation</w> of complex theories into practical applications is a difficult task.",
                            "target_lang_translations": {
                                "ru": "<w>Перевод</w> сложных теорий в практическую плоскость — трудная задача."
                            }
                        },
                        {
                            "text": "His <w>translation</w> of raw data into a visual chart helped everyone understand the trend.",
                            "target_lang_translations": {
                                "ru": "Его <w>преобразование</w> необработанных данных в наглядный график помогло всем понять тенденцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "transformation",
                        "conversion"
                    ],
                    "common_phrases": [
                        "translation into practice",
                        "translation of ideas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преобразование чего-либо из одной формы, среды или состояния в другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преобразование",
                                "target_lang_sense_clarification": "изменение формы"
                            },
                            {
                                "target_lang_word": "перевод",
                                "target_lang_sense_clarification": "изменение состояния"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5676729-795e-3077-9f04-cd656cc770c5",
                    "sense_definition": "The biological process in which a cell makes proteins using the genetic information carried in messenger RNA.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "During <w>translation</w>, the ribosome reads the sequence of the mRNA to assemble amino acids.",
                            "target_lang_translations": {
                                "ru": "Во время <w>трансляции</w> рибосома считывает последовательность мРНК для сборки аминокислот."
                            }
                        },
                        {
                            "text": "Errors in protein <w>translation</w> can lead to cellular dysfunction.",
                            "target_lang_translations": {
                                "ru": "Ошибки в <w>трансляции</w> белка могут привести к клеточной дисфункции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "biology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Биологический процесс, в ходе которого клетка создает белки, используя генетическую информацию мРНК."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трансляция",
                                "target_lang_sense_clarification": "генетика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8005d399-b078-354e-aa4a-6add136f415b",
                    "sense_definition": "A movement of a geometric figure or object in a straight line without rotation or change in shape.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>translation</w> moves every point of the triangle the same distance in the same direction.",
                            "target_lang_translations": {
                                "ru": "<w>Параллельный перенос</w> перемещает каждую точку треугольника на одинаковое расстояние в одном и том же направлении."
                            }
                        },
                        {
                            "text": "In geometry, a <w>translation</w> is a type of rigid transformation.",
                            "target_lang_translations": {
                                "ru": "В геометрии <w>параллельный перенос</w> — это вид движения."
                            }
                        }
                    ],
                    "synonyms": [
                        "shift",
                        "displacement"
                    ],
                    "antonyms": [
                        "rotation"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещение геометрической фигуры или объекта по прямой линии без вращения или изменения формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "параллельный перенос",
                                "target_lang_sense_clarification": "геометрия"
                            },
                            {
                                "target_lang_word": "трансляция",
                                "target_lang_sense_clarification": "физика"
                            }
                        ]
                    }
                }
            ]
        }
    ]

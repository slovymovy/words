            "senses": [
                {
                    "sense_id": "c2afc861-e9b5-32c0-a33b-46e1d6dd0e05",
                    "sense_definition": "Een sterk verlangen naar iets dat men graag wil hebben of laten gebeuren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "desire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is haar grootste <w>wens</w> om ooit een wereldreis te maken.",
                            "target_lang_translations": {
                                "en": "It is her greatest <w>wish</w> to travel the world someday.",
                                "ru": "Это её самое большое <w>желание</w> — когда-нибудь отправиться в кругосветное путешествие."
                            }
                        },
                        {
                            "text": "Alles verliep tijdens het feestje helemaal naar <w>wens</w>.",
                            "target_lang_translations": {
                                "en": "Everything at the party went completely according to <w>wishes</w>.",
                                "ru": "На вечеринке всё прошло именно так, как <w>хотелось</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verlangen",
                        "droom",
                        "begeerte"
                    ],
                    "common_phrases": [
                        "naar wens",
                        "een wens doen",
                        "te wensen overlaten"
                    ],
                    "target_lang_definitions": {
                        "en": "A strong desire for something one would like to have or happen.",
                        "ru": "Сильное желание получить что-то или осуществить что-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wish",
                                "target_lang_sense_clarification": "a hope or desire for something to happen"
                            },
                            {
                                "target_lang_word": "desire",
                                "target_lang_sense_clarification": "a strong feeling of wanting to have something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "желание",
                                "target_lang_sense_clarification": "стремление"
                            },
                            {
                                "target_lang_word": "мечта",
                                "target_lang_sense_clarification": "заветное желание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90a29b39-e983-3e55-a7cf-4f204ffd302a",
                    "sense_definition": "Een uitspraak waarmee men hoopt dat iemand gelukkig of succesvol zal zijn.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij stuurde ons zijn beste <w>wensen</w> voor het nieuwe jaar.",
                            "target_lang_translations": {
                                "en": "He sent us his best <w>wishes</w> for the new year.",
                                "ru": "Он прислал нам свои наилучшие <w>пожелания</w> на Новый год."
                            }
                        },
                        {
                            "text": "Mijn enige <w>wens</w> voor jou is dat je gelukkig wordt.",
                            "target_lang_translations": {
                                "en": "My only <w>wish</w> for you is that you become happy.",
                                "ru": "Моё единственное <w>пожелание</w> тебе — чтобы ты был счастлив."
                            }
                        }
                    ],
                    "synonyms": [
                        "gelukwens"
                    ],
                    "antonyms": [
                        "verwensing"
                    ],
                    "common_phrases": [
                        "de beste wensen"
                    ],
                    "target_lang_definitions": {
                        "en": "A statement expressing hope that someone will be happy or successful.",
                        "ru": "Высказывание, выражающее надежду на чье-либо счастье или успех."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wish",

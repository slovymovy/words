                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На ярмарке продавались лошади разных <w>шерстей</w>.",
                            "target_lang_translations": {
                                "en": "Horses of different <w>coats</w> were sold at the fair."

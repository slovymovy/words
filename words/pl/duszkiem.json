    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c9786321-1c87-3669-bf68-e002f6160855",
                    "sense_definition": "Sposób picia polegający na wypiciu całej zawartości naczynia naraz, bez odrywania ust i bez przerw na oddech.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Był tak spragniony po biegu, że wypił całą butelkę wody <w>duszkiem</w>.",
                            "target_lang_translations": {
                                "ru": "Он так хотел пить после бега, что выпил целую бутылку воды <w>залпом</w>."
                            }
                        },
                        {
                            "text": "Nalałem sobie pełną szklankę zimnego mleka i wypiłem ją <w>duszkiem</w>.",
                            "target_lang_translations": {
                                "ru": "Я налил себе полный стакан холодного молока и выпил его <w>залпом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "jednym tchem",
                        "jednym haustem",
                        "na hejnał"
                    ],
                    "common_phrases": [
                        "wypić duszkiem",
                        "wychylić duszkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ питья, при котором содержимое сосуда выпивается сразу, не отрываясь и без перерывов на вдох."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "залпом",
                                "target_lang_sense_clarification": "сразу, без передышки"
                            },
                            {
                                "target_lang_word": "единым духом",
                                "target_lang_sense_clarification": "на одном дыхании"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "238a2f35-ffcd-39c5-a9f7-487a8c281b03",
                    "sense_definition": "Forma fleksyjna (narzędnik liczby pojedynczej) rzeczownika <w>duszek</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia zawsze była dobrym <w>duszkiem</w> naszej rodziny, dbającym o zgodę.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка всегда была добрым <w>духом</w> нашей семьи, заботящимся о согласии."
                            }
                        },
                        {
                            "text": "W tej grze komputerowej sterujesz małym, zwinnym <w>duszkiem</w>.",
                            "target_lang_translations": {
                                "ru": "В этой компьютерной игре ты управляешь маленьким, юрким <w>привидением</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "być dobrym duszkiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "narzędnik liczby pojedynczej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма творительного падежа единственного числа существительного «duszek» (маленький дух, привидение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "духом",
                                "target_lang_sense_clarification": "форма слова «дух»"
                            },
                            {
                                "target_lang_word": "привидением",
                                "target_lang_sense_clarification": "форма слова «привидение»"
                            }
                        ]

            "senses": [
                {
                    "sense_id": "3a8ce33d-51c0-3d45-8bcf-e0cd9a81fa08",
                    "sense_definition": "Op dezelfde plaats blijven of de tijd laten verstrijken totdat iemand komt of er iets gebeurt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "waiting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moesten meer dan een uur op de trein <w>wachten</w>.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось больше часа <w>ждать</w> поезда."
                            }
                        },
                        {
                            "text": "Kun je even <w>wachten</w> terwijl ik mijn jas pak?",
                            "target_lang_translations": {
                                "ru": "Можешь немного <w>подождать</w>, пока я возьму куртку?"
                            }
                        },
                        {
                            "text": "Natuurlijk had ik ook tot mijn pensioen kunnen <w>wachten</w>, maar ik wilde het nu.",
                            "target_lang_translations": {
                                "ru": "Конечно, я мог бы <w>ждать</w> до пенсии, но я хотел этого сейчас."
                            }
                        }
                    ],
                    "antonyms": [
                        "vertrekken",
                        "beginnen"
                    ],
                    "common_phrases": [
                        "wachten op",
                        "even wachten",
                        "laten wachten",
                        "niet kunnen wachten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставаться на месте или проводить время в ожидании, пока кто-то придет или что-то случится."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ждать",
                                "target_lang_sense_clarification": "самое общее слово"
                            },
                            {
                                "target_lang_word": "подождать",
                                "target_lang_sense_clarification": "провести некоторое время в ожидании (совершенный вид)"
                            },
                            {
                                "target_lang_word": "ожидать",
                                "target_lang_sense_clarification": "более формально; рассчитывать на появление чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d83b3aad-e489-37a0-952a-0be8e55602cd",
                    "sense_definition": "Zich hoeden voor iets of iemand; ergens voor oppassen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "beware",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wacht</w> u voor de hond als u het terrein betreedt!",
                            "target_lang_translations": {
                                "ru": "<w>Остерегайтесь</w> собаки, когда входите на территорию!"
                            }
                        },
                        {
                            "text": "Hij zal zich wel <w>wachten</w> om die fout nog een keer te maken.",
                            "target_lang_translations": {
                                "ru": "Он уж точно будет <w>остерегаться</w> снова совершить эту ошибку."
                            }
                        }
                    ],
                    "synonyms": [
                        "oppassen",
                        "uitkijken",
                        "zich hoeden"
                    ],
                    "common_phrases": [
                        "zich wachten voor"
                    ],
                    "traits": [

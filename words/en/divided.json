                {
                    "sense_id": "c0e86796-fade-3226-b960-28285e9ca33d",
                    "sense_definition": "Simple past and past participle of <w>divide</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher <w>divided</w> the students into small groups for the project.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>разделил</w> учеников на небольшие группы для работы над проектом."
                            }
                        },
                        {
                            "text": "She <w>divided</w> the apple into four equal pieces.",
                            "target_lang_translations": {
                                "ru": "Она <w>разделила</w> яблоко на четыре равные части."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола divide."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разделил",
                                "target_lang_sense_clarification": "прошедшее время"
                            },
                            {
                                "target_lang_word": "разделённый",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "da567969-5917-3ffc-b213-db78ed125367",
                    "sense_definition": "Separated or split into distinct parts or pieces.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>divided</w> lands were distributed among the heirs.",
                            "target_lang_translations": {
                                "ru": "<w>Разделённые</w> земли были распределены между наследниками."
                            }
                        },
                        {
                            "text": "We sat in a room <w>divided</w> by a glass partition.",
                            "target_lang_translations": {
                                "ru": "Мы сидели в комнате, <w>разделённой</w> стеклянной перегородкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "split",
                        "separated"
                    ],
                    "antonyms": [
                        "combined",
                        "undivided"
                    ],
                    "common_phrases": [
                        "divided into parts",
                        "divided attention"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разделённый или расщеплённый на отдельные части или куски."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разделённый",
                                "target_lang_sense_clarification": "на части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef7ce4e9-8f6f-3473-b1bf-5c8ec58ce475",
                    "sense_definition": "Characterized by disagreement, conflicting interests, or disunity.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "disagreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Public opinion is deeply <w>divided</w> on this controversial issue.",
                            "target_lang_translations": {
                                "ru": "Общественное мнение по этому спорному вопросу глубоко <w>расколото</w>."
                            }
                        },
                        {
                            "text": "The committee remained <w>divided</w> regarding the new policy.",
                            "target_lang_translations": {
                                "ru": "Комитет остался <w>разобщённым</w> в отношении новой политики."
                            }
                        }
                    ],
                    "synonyms": [
                        "disunited"
                    ],
                    "antonyms": [
                        "unified",
                        "undivided"
                    ],
                    "common_phrases": [
                        "deeply divided",
                        "divided opinion",
                        "divided loyalties"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся разногласиями, конфликтом интересов или отсутствием единства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расколотый",
                                "target_lang_sense_clarification": "о мнениях или группах"
                            },
                            {
                                "target_lang_word": "разобщённый",
                                "target_lang_sense_clarification": "лишённый единства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45147365-98b8-3e97-a673-aa7356874f2c",
                    "sense_definition": "Describes a road separated into lanes that move in opposite directions by a median.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We drove for hours along the <w>divided</w> highway without seeing a town.",
                            "target_lang_translations": {
                                "ru": "Мы часами ехали по шоссе <w>с разделительной полосой</w>, не видя ни одного города."
                            }
                        },
                        {
                            "text": "It is safer to drive on a <w>divided</w> road at night.",
                            "target_lang_translations": {
                                "ru": "Ночью безопаснее ехать по дороге <w>с разделительной полосой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "divided highway",
                        "divided road"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывает дорогу, на которой встречные потоки отделены друг от друга разделительной полосой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с разделительной полосой",
                                "target_lang_sense_clarification": "о дороге"
                            }
                        ]
                    }
                }
            ]
        }
    ]

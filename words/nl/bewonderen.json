    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bdccf8fb-9cec-3e14-97bc-590128b45b47",
                    "sense_definition": "Iemand of iets erg goed vinden en daar veel respect voor hebben.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleine jongen <w>bewonderde</w> zijn vader enorm om zijn kracht.",
                            "target_lang_translations": {
                                "ru": "Маленький мальчик очень <w>восхищался</w> своим отцом за его силу."
                            }
                        },
                        {
                            "text": "Ik <w>bewonder</w> je moed en eerlijkheid in deze lastige situatie.",
                            "target_lang_translations": {
                                "ru": "Я <w>восхищаюсь</w> твоей смелостью и честностью в этой сложной ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "respecteren",
                        "hoogachten",
                        "waarderen",
                        "opkijken tegen"
                    ],
                    "antonyms": [
                        "minachten",
                        "verachten"
                    ],
                    "common_phrases": [
                        "iemand mateloos bewonderen",
                        "iemand heimelijk bewonderen",
                        "grote moed bewonderen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высоко ценить кого-то или что-то и испытывать к этому глубокое уважение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восхищаться",
                                "target_lang_sense_clarification": "испытывать восторг и уважение к качествам или поступкам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60ac0a4d-d941-38d3-bbd1-e0eeeef84880",
                    "sense_definition": "Met veel plezier en aandacht naar iets kijken omdat het mooi is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het museum <w>bewonderen</w> veel mensen de oude kunstwerken.",
                            "target_lang_translations": {
                                "ru": "В музее многие люди <w>любуются</w> старинными произведениями искусства."
                            }
                        },
                        {
                            "text": "We bleven even staan om het prachtige uitzicht te <w>bewonderen</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ненадолго остановились, чтобы <w>полюбоваться</w> прекрасным видом."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanschouwen",
                        "bekijken"
                    ],
                    "common_phrases": [
                        "het uitzicht bewonderen",
                        "de natuur bewonderen",
                        "zichzelf bewonderen"
                    ],
                    "target_lang_definitions": {
                        "ru": "С удовольствием и вниманием смотреть на что-то красивое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любоваться",
                                "target_lang_sense_clarification": "наслаждаться видом чего-либо"
                            },
                            {
                                "target_lang_word": "восхищаться",
                                "target_lang_sense_clarification": "быть в восторге от внешнего вида"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab18c348-fd26-37f2-ad25-a5c7c6552806",
                    "sense_definition": "Osoba zachowująca się nienormalnie, nieracjonalnie, gwałtownie lub w sposób budzący zdziwienie otoczenia.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_crazy_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten kierowca to jakiś kompletny <w>oszołom</w>, jedzie pod prąd na autostradzie!",
                            "target_lang_translations": {
                                "ru": "Этот водитель — какой-то полный <w>псих</w>, он едет по встречной полосе на шоссе!"
                            }
                        },
                        {
                            "text": "Nie słuchaj tego, co mówi ten <w>oszołom</w>, on zawsze opowiada bzdury.",
                            "target_lang_translations": {
                                "ru": "Не слушай, что говорит этот <w>чокнутый</w>, он всегда несёт чушь."
                            }
                        }
                    ],
                    "synonyms": [
                        "wariat",
                        "świr",
                        "szajbus",
                        "pomyleniec",
                        "postrzeleniec"
                    ],
                    "common_phrases": [
                        "totalny oszołom",
                        "zwykły oszołom",
                        "niegroźny oszołom"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo o wydźwięku negatywnym, często obraźliwym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, ведущий себя ненормально, иррационально или вызывающе, удивляя окружающих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неадекват",
                                "target_lang_sense_clarification": "разг., человек с неадекватным поведением"
                            },
                            {
                                "target_lang_word": "псих",
                                "target_lang_sense_clarification": "разг., сумасшедший, неуравновешенный человек"
                            },
                            {
                                "target_lang_word": "чокнутый",
                                "target_lang_sense_clarification": "разг., странный или глупый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b76b7102-6b8f-321b-9cee-449784aa6153",
                    "sense_definition": "Człowiek wierzący bezgranicznie w jakąś ideologię, ślepo jej podporządkowany i często agresywnie narzucający ją innym.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_crazy_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jakiś polityczny <w>oszołom</w> krzyczał przez megafon na rynku, strasząc ludzi końcem świata.",
                            "target_lang_translations": {
                                "ru": "Какой-то политический <w>фанатик</w> кричал в мегафон на рынке, пугая людей концом света."
                            }
                        },
                        {
                            "text": "Dyskusja z nim nie ma sensu, to religijny <w>oszołom</w> zamknięty na argumenty.",
                            "target_lang_translations": {
                                "ru": "Спорить с ним бессмысленно, это религиозный <w>фанатик</w>, закрытый для аргументов."
                            }
                        }
                    ],
                    "synonyms": [
                        "fanatyk",
                        "radykał",
                        "maniak",
                        "narwaniec"
                    ],
                    "common_phrases": [
                        "polityczny oszołom",
                        "religijny oszołom"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo nacechowane negatywnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, слепо преданный какой-либо идеологии и агрессивно навязывающий её другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фанатик",
                                "target_lang_sense_clarification": "человек, исступлённо преданный какой-либо идее"
                            },
                            {
                                "target_lang_word": "одержимый",
                                "target_lang_sense_clarification": "человек, находящийся во власти навязчивой идеи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5dfe4b16-c1f9-3b94-bd80-e66b542cbf96",
                    "sense_definition": "Potoczne, żartobliwe określenie sieci hipermarketów Auchan, wynikające z podobieństwa brzmienia nazwy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sobotę rano jedziemy do <w>oszołoma</w> na duże zakupy spożywcze.",
                            "target_lang_translations": {
                                "ru": "В субботу утром мы едем в <w>Ашан</w> за большими покупками продуктов."
                            }
                        },
                        {
                            "text": "Spotkajmy się na parkingu pod <w>oszołomem</w> o godzinie siedemnastej.",
                            "target_lang_translations": {
                                "ru": "Давай встретимся на парковке у <w>Ашана</w> в семнадцать часов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zakupy w oszołomie",
                        "jechać do oszołoma"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "używane głównie w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шутливое сленговое название сети гипермаркетов «Ашан» (игра слов на основе созвучия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ашан",
                                "target_lang_sense_clarification": "название торговой сети"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3342df59-3261-354f-9862-489d1eeb289d",
                    "sense_definition": "Druga osoba liczby pojedynczej trybu rozkazującego czasownika <w>oszołomić</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wejdź na scenę i <w>oszołom</w> publiczność swoim niezwykłym talentem wokalnym.",
                            "target_lang_translations": {
                                "ru": "Выйди на сцену и <w>ошеломи</w> публику своим необычайным вокальным талантом."
                            }
                        },
                        {
                            "text": "<w>Oszołom</w> ich swoją wiedzą, a na pewno dostaniesz tę pracę.",
                            "target_lang_translations": {
                                "ru": "<w>Порази</w> их своими знаниями, и ты точно получишь эту работу."
                            }
                        }
                    ],
                    "traits": [
                        {

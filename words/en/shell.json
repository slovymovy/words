                    "frequency": "Low",
                    "semantic_group_id": "science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Elements in the same column of the periodic table often have the same number of electrons in their outer <w>shell</w>.",
                            "target_lang_translations": {
                                "ru": "Элементы в одном столбце периодической таблицы часто имеют одинаковое количество электронов на внешней <w>оболочке</w>."
                            }
                        },
                        {
                            "text": "Chemical bonding is determined by the electrons in the valence <w>shell</w>.",
                            "target_lang_translations": {
                                "ru": "Химическая связь определяется электронами на валентной <w>оболочке</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "electron shell",
                        "valence shell"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "scientific context"
                        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8cd6b86e-4fbc-34e0-ae15-a41e50b04a53",
                    "sense_definition": "An organization in the United States formed to raise money privately to influence elections or legislation, especially at the federal level (acronym for Political Action Committee).",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "political_org",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "The candidate received significant funding from a corporate <w>PAC</w> during the campaign.",
                            "target_lang_translations": {
                                "ru": "Во время кампании кандидат получил значительное финансирование от корпоративного <w>комитета политических действий</w>."
                            }
                        },
                        {
                            "text": "Strict regulations govern how much money a <w>PAC</w> can donate to a specific political party.",
                            "target_lang_translations": {
                                "ru": "Строгие правила регулируют, сколько денег <w>комитет политических действий</w> может пожертвовать конкретной политической партии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "super PAC",
                        "leadership PAC",
                        "form a PAC"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "acronym"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "chiefly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Организация в США, занимающаяся сбором средств для влияния на выборы или законодательство (аббревиатура от Political Action Committee)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комитет политических действий",
                                "target_lang_sense_clarification": "политика США"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33512191-968f-3050-a064-36c27568cae0",
                    "sense_definition": "A government committee responsible for examining public expenditure to ensure value for money (initialism for Public Accounts Committee).",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "political_org",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "The <w>PAC</w> published a critical report on the government's spending habits.",
                            "target_lang_translations": {
                                "ru": "<w>Комитет по государственным счетам</w> опубликовал критический отчет о государственных расходах."
                            }
                        },
                        {
                            "text": "She was called to testify before the <w>PAC</w> regarding the new infrastructure project.",
                            "target_lang_translations": {
                                "ru": "Ее вызвали для дачи показаний перед <w>Комитетом по государственным счетам</w> по поводу нового инфраструктурного проекта."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "UK, Commonwealth"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Парламентский комитет, ответственный за проверку государственных расходов (сокращение от Public Accounts Committee)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Комитет по государственным счетам",
                                "target_lang_sense_clarification": "политика Великобритании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "083d0d37-9469-3d6f-9c59-6e942d5a1539",

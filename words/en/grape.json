    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1aa139a8-51d7-354a-a4a2-578ee5ad58b9",
                    "sense_definition": "A small, round, smooth-skinned edible fruit, usually purple, red, or green, that grows in bunches on vines.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "fruit_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a large bunch of seedless <w>grapes</w> from the market.",
                            "target_lang_translations": {
                                "ru": "Она купила на рынке большую гроздь <w>винограда</w> без косточек."
                            }
                        },
                        {
                            "text": "Red <w>grapes</w> are commonly used to make various types of wine.",
                            "target_lang_translations": {
                                "ru": "Красный <w>виноград</w> обычно используется для изготовления различных сортов вина."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bunch of grapes",
                        "sour grapes",
                        "seedless grapes",
                        "table grapes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький круглый съедобный плод, обычно фиолетового, красного или зеленого цвета, растущий гроздьями на лозе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виноград",
                                "target_lang_sense_clarification": "плоды (собирательное)"
                            },
                            {
                                "target_lang_word": "виноградина",
                                "target_lang_sense_clarification": "одна ягода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc7144c4-da4d-3135-a21d-ed7deb98f4bf",
                    "sense_definition": "A dark purplish-red colour, resembling the colour of many grapes.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist chose a deep shade of <w>grape</w> for the background of the painting.",
                            "target_lang_translations": {
                                "ru": "Художник выбрал глубокий оттенок <w>винограда</w> для фона картины."
                            }
                        },
                        {
                            "text": "She decided to paint the accent wall in <w>grape</w> to match the curtains.",
                            "target_lang_translations": {
                                "ru": "Она решила покрасить акцентную стену в <w>цвет винограда</w>, чтобы она сочеталась со шторами."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тёмно-лиловый цвет, напоминающий цвет многих сортов винограда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виноградный цвет",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "цвет винограда",
                                "target_lang_sense_clarification": "оттенок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fb47d28-417b-32aa-8910-92d83bf513c7",
                    "sense_definition": "A woody vine that bears clusters of grapes.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fruit_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wild <w>grape</w> had grown over the entire garden fence by the end of summer.",
                            "target_lang_translations": {
                                "ru": "К концу лета дикий <w>виноград</w> разросся по всему садовому забору."
                            }
                        },
                        {
                            "text": "Farmers prune the <w>grape</w> in winter to ensure a good harvest the following year.",
                            "target_lang_translations": {
                                "ru": "Фермеры обрезают <w>виноград</w> зимой, чтобы обеспечить хороший урожай в следующем году."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Древесная лиана, на которой растут гроздья винограда."
                    },
                    "translations": {

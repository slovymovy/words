            "pos": "noun",
            "senses": [
                {
                    "sense_id": "39a77450-bf71-3da4-880f-1352261a2e5e",
                    "sense_definition": "Wszystkie elementy czegoś tworzące razem jakąś samodzielną jednostkę; kompletny zbiór lub system.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "wholeness_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przeczytałem tę książkę w <w>całości</w> w jeden wieczór.",
                            "target_lang_translations": {
                                "ru": "Я прочитал эту книгу <w>целиком</w> за один вечер."
                            }
                        },
                        {
                            "text": "Te dwa oddzielne elementy tworzą razem spójną <w>całość</w>.",
                            "target_lang_translations": {
                                "ru": "Эти два отдельных элемента вместе образуют единое <w>целое</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogół",
                        "komplet",
                        "całokształt",
                        "suma",
                        "jedność"
                    ],
                    "antonyms": [
                        "część"
                    ],
                    "common_phrases": [
                        "w całości",
                        "stanowić całość",
                        "tworzyć całość",
                        "jako całość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Все элементы чего-либо, образующие вместе самостоятельную единицу; полный набор или система."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целое",
                                "target_lang_sense_clarification": "единый объект, единство"
                            },
                            {
                                "target_lang_word": "совокупность",
                                "target_lang_sense_clarification": "сочетание, общая сумма элементов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c065988d-2f21-3361-9eb9-c55311c376e4",
                    "sense_definition": "Liczba całkowita lub część liczby mieszanej znajdująca się przed przecinkiem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "math_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił ucznia, aby wyłączył <w>całości</w> z ułamka niewłaściwego.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил ученика выделить <w>целую часть</w> из неправильной дроби."
                            }
                        },
                        {
                            "text": "Wynik tego równania to dwie <w>całości</w> i jedna druga.",
                            "target_lang_translations": {
                                "ru": "Результат этого уравнения — две <w>целых</w> и одна вторая."
                            }
                        }
                    ],
                    "synonyms": [
                        "liczba całkowita"
                    ],
                    "antonyms": [
                        "ułamek"
                    ],
                    "common_phrases": [
                        "wyciągać całości",
                        "wyłączyć całości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Целое число или часть смешанного числа, стоящая перед запятой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целое",
                                "target_lang_sense_clarification": "в чтении дробей и смешанных чисел"
                            },
                            {
                                "target_lang_word": "целое число",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b21cd224-e8de-3c95-a3d0-0d2b6f3b32fd",
                    "sense_definition": "Stan bycia nienaruszonym, kompletnym i niepodzielnym; brak uszkodzeń.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "wholeness_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo groźnego wypadku samochód ocalał w <w>całości</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на серьёзную аварию, автомобиль остался в <w>целости</w>."
                            }
                        },
                        {
                            "text": "Rząd musi dbać o terytorialną <w>całość</w> i bezpieczeństwo kraju.",
                            "target_lang_translations": {
                                "ru": "Правительство должно заботиться о территориальной <w>целостности</w> и безопасности страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "nienaruszalność",
                        "integralność",
                        "niepodzielność"
                    ],
                    "common_phrases": [
                        "zachować w całości",
                        "dotrzeć w całości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние нетронутости, полноты и неделимости; отсутствие повреждений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целостность",
                                "target_lang_sense_clarification": "неделимость, единство"
                            },
                            {
                                "target_lang_word": "сохранность",
                                "target_lang_sense_clarification": "отсутствие повреждений"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b8fdb808-8785-3552-8997-53cd87e3ccd0",
                    "sense_definition": "The reduction in the magnitude of oscillations or vibrations by the dissipation of energy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physics_damping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineer increased the <w>damping</w> in the system to prevent structural damage.",
                            "target_lang_translations": {
                                "ru": "Инженер увеличил <w>демпфирование</w> в системе, чтобы предотвратить структурные повреждения."
                            }
                        },
                        {
                            "text": "Without sufficient <w>damping</w>, the bridge began to sway dangerously in the wind.",
                            "target_lang_translations": {
                                "ru": "Без достаточного <w>гашения колебаний</w> мост начал опасно раскачиваться на ветру."
                            }
                        }
                    ],
                    "synonyms": [
                        "attenuation",
                        "suppression"
                    ],
                    "antonyms": [
                        "amplification",
                        "resonance"
                    ],
                    "common_phrases": [
                        "critical damping",
                        "viscous damping",
                        "magnetic damping"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшение амплитуды колебаний или вибраций за счет рассеивания энергии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демпфирование",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "гашение",
                                "target_lang_sense_clarification": "вибраций или колебаний"
                            },
                            {
                                "target_lang_word": "затухание",
                                "target_lang_sense_clarification": "ослабление сигнала или волн"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c4aedba-71cd-3c52-a4d5-a5c7e9e78f80",
                    "sense_definition": "The act of making something slightly wet.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>damping</w> of the paper fibers is a crucial step in the recycling process.",
                            "target_lang_translations": {

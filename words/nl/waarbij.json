            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "134c76b5-6a71-3af1-b151-81fd04c52a33",
                    "sense_definition": "Vraagwoord dat vraagt naar de zaak of het ding waar iets bij hoort of bij betrokken is.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Waarbij</w> kan ik u vandaag helpen?",
                            "target_lang_translations": {
                                "en": "What can I help you <w>with</w> today?",
                                "ru": "<w>С чем</w> я могу вам сегодня помочь?"
                            }
                        },
                        {
                            "text": "<w>Waarbij</w> hoort dit kleine onderdeel van de machine?",
                            "target_lang_translations": {
                                "en": "What does this small part of the machine belong <w>to</w>?",
                                "ru": "<w>К чему</w> относится эта маленькая деталь машины?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Question word asking about the thing or matter something belongs to or is involved with.",
                        "ru": "Вопросительное слово, используемое для вопроса о предмете или деле, к которому что-то относится или с которым что-то связано."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "with what",
                                "target_lang_sense_clarification": "interrogative"
                            },
                            {
                                "target_lang_word": "what ... with",
                                "target_lang_sense_clarification": "split interrogative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "с чем",
                                "target_lang_sense_clarification": "вопрос о помощи или содействии"
                            },
                            {
                                "target_lang_word": "к чему",
                                "target_lang_sense_clarification": "вопрос о принадлежности или отношении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b96fd8ac-7e9a-32d2-86bc-c09ea3408f3b",
                    "sense_definition": "Verwijswoord dat terugwijst naar een eerder genoemde situatie, gebeurtenis of voorwerp, met de betekenis 'bij dat' of 'tijdens dat'.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er ontstond een gevaarlijke situatie <w>waarbij</w> iedereen snel moest vluchten.",
                            "target_lang_translations": {
                                "en": "A dangerous situation arose <w>during which</w> everyone had to flee quickly.",
                                "ru": "Возникла опасная ситуация, <w>при которой</w> всем пришлось быстро бежать."
                            }
                        },
                        {
                            "text": "Dit is het document <w>waarbij</w> de nieuwe regels officieel zijn vastgelegd.",
                            "target_lang_translations": {
                                "en": "This is the document <w>whereby</w> the new rules are officially established.",
                                "ru": "Это документ, <w>в котором</w> официально зафиксированы новые правила."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het proces waarbij",
                        "een situatie waarbij"
                    ],
                    "target_lang_definitions": {
                        "en": "Reference word referring back to a previously mentioned situation, event, or object, meaning 'at which', 'during which', or 'whereby'.",
                        "ru": "Относительное местоимение, отсылающее к ранее упомянутой ситуации, событию или объекту; соответствует значениям «при котором», «в ходе которого» или «где»."
                    },
                    "translations": {

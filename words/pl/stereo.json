    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5d523a25-9855-3d6c-b319-a2e42f784a49",
                    "sense_definition": "Zestaw urządzeń elektronicznych służący do odtwarzania dźwięku przestrzennego; potoczna nazwa zestawu stereofonicznego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem nowe <w>stereo</w> do mojego pokoju, żeby słuchać muzyki w lepszej jakości.",
                            "target_lang_translations": {
                                "ru": "Я купил новое <w>стерео</w> в свою комнату, чтобы слушать музыку в лучшем качестве."
                            }
                        },
                        {
                            "text": "Podłączyliśmy telefon pod <w>stereo</w> i zaczęliśmy imprezę.",
                            "target_lang_translations": {
                                "ru": "Мы подключили телефон к <w>стерео</w> и начали вечеринку."
                            }
                        }
                    ],
                    "synonyms": [
                        "wieża",
                        "zestaw stereo"
                    ],
                    "common_phrases": [
                        "samochodowe stereo",
                        "domowe stereo",
                        "włączyć stereo"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "nieodmienny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплект электронной аппаратуры для воспроизведения объемного звука; разговорное название стереосистемы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стерео",
                                "target_lang_sense_clarification": "разговорное, устройство"
                            },
                            {
                                "target_lang_word": "стереосистема",
                                "target_lang_sense_clarification": "полное название"
                            },
                            {
                                "target_lang_word": "музыкальный центр",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "54dd2228-0f29-3a81-809c-99e1275c82ef",
                    "sense_definition": "Dotyczący dźwięku zapisanego lub odtwarzanego na dwóch oddzielnych kanałach; krótka forma od słowa <w>stereofoniczny</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten film ma ścieżkę dźwiękową nagraną w formacie <w>stereo</w>.",
                            "target_lang_translations": {
                                "ru": "У этого фильма звуковая дорожка записана в формате <w>стерео</w>."
                            }
                        },
                        {
                            "text": "Wolę używać słuchawek <w>stereo</w>, ponieważ dają lepsze wrażenia słuchowe.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю использовать <w>стерео</w> наушники, так как они дают лучшие слуховые ощущения."
                            }
                        }
                    ],
                    "synonyms": [
                        "stereofoniczny",
                        "dwukanałowy",
                        "przestrzenny"
                    ],
                    "antonyms": [
                        "monofoniczny",
                        "mono"
                    ],
                    "common_phrases": [
                        "dźwięk stereo",
                        "słuchawki stereo",
                        "kabel stereo"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "nieodmienny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к звуку, записанному или воспроизводимому по двум отдельным каналам; краткая форма слова «стереофонический»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стерео",
                                "target_lang_sense_clarification": "неизменяемое прилагательное"
                            },
                            {
                                "target_lang_word": "стереофонический",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "69988eb3-dead-314a-a49e-266133082153",
                    "sense_definition": "W sposób wykorzystujący dwa kanały dźwiękowe; krótka forma od słowa <w>stereofonicznie</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sound_mode",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta stacja radiowa zaczęła niedawno nadawać audycje <w>stereo</w>.",
                            "target_lang_translations": {
                                "ru": "Эта радиостанция недавно начала вещать в <w>стерео</w>."
                            }
                        },
                        {
                            "text": "Utwór brzmi znacznie lepiej, gdy jest odtwarzany <w>stereo</w>.",
                            "target_lang_translations": {
                                "ru": "Композиция звучит гораздо лучше, когда воспроизводится в <w>стерео</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stereofonicznie"
                    ],
                    "antonyms": [
                        "monofonicznie"
                    ],
                    "common_phrases": [
                        "nadawać stereo",
                        "grać stereo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способом, использующим два звуковых канала; краткая форма слова «стереофонически»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в стерео",
                                "target_lang_sense_clarification": "режим звучания"
                            },
                            {
                                "target_lang_word": "стереофонически",
                                "target_lang_sense_clarification": "наречие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

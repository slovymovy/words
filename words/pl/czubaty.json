{
    "word_family": [
        "czub",
        "czubek",
        "czubatka"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "922a3851-141a-3df0-9cc1-0bc1d7b8f319",
                    "sense_definition": "Napełniony tak, że zawartość wystaje ponad brzegi naczynia, tworząc wzniesienie; przeciwieństwo płaskiego napełnienia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do ciasta należy dodać jedną <czubatą> łyżkę gorzkiego kakao.",
                            "target_lang_translations": {
                                "ru": "В тесто нужно добавить одну ложку горького какао <с горкой>."
                            }
                        },
                        {
                            "text": "Czy mam wziąć <czubatą> czy płaską łyżeczkę proszku do pieczenia?",
                            "target_lang_translations": {
                                "ru": "Мне взять чайную ложку разрыхлителя <с верхом> или без?"
                            }
                        }
                    ],
                    "synonyms": [
                        "kopiasty",
                        "pełny"
                    ],
                    "antonyms": [
                        "płaski"
                    ],
                    "common_phrases": [
                        "czubata łyżka",
                        "czubata łyżeczka",
                        "czubaty talerz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наполненный с избытком, так, что содержимое возвышается над краями сосуда или ложки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с верхом",
                                "target_lang_sense_clarification": "о ложке, стакане и т.п."
                            },
                            {
                                "target_lang_word": "с горкой",
                                "target_lang_sense_clarification": "разговорное; обычно о сыпучих продуктах в ложке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e34d0370-12b3-318d-82af-cc58724f0c06",
                    "sense_definition": "O zwierzęciu, zwłaszcza ptaku: posiadający na głowie pęk piór lub sierści tworzący ozdobny czub.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sikora <czubata> jest łatwa do rozpoznania dzięki charakterystycznym piórom na głowie.",
                            "target_lang_translations": {
                                "ru": "<Хохлатая> синица легко узнаваема благодаря характерным перьям на голове."
                            }
                        },
                        {
                            "text": "W naszym kurniku mieszka jedna kura <czubata>, która wygląda bardzo zabawnie.",
                            "target_lang_translations": {
                                "ru": "В нашем курятнике живет одна <хохлатая> курица, которая выглядит очень забавно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sikora czubata",
                        "kura czubata",
                        "perkoz czubaty"
                    ],
                    "target_lang_definitions": {
                        "ru": "О животном, особенно о птице: имеющий на голове пучок перьев или шерсти (хохолок)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хохлатый",
                                "target_lang_sense_clarification": "имеющий хохолок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71df5615-a139-302f-833a-9f7cba0bea03",
                    "sense_definition": "Mający kształt zbliżony do stożka lub zakończony ostrym wierzchołkiem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krasnoludek w bajce nosił czerwoną, <czubatą> czapkę.",
                            "target_lang_translations": {
                                "ru": "Гном в сказке носил красную <остроконечную> шапку."
                            }
                        },
                        {
                            "text": "Dziecko narysowało na kartce wysokie, <czubate> góry.",
                            "target_lang_translations": {
                                "ru": "Ребенок нарисовал на листе высокие <островерхие> горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "spiczasty",
                        "stożkowaty"
                    ],
                    "antonyms": [
                        "płaski"
                    ],
                    "common_phrases": [
                        "czubata czapka",
                        "czubata góra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий форму, близкую к конусу, или заканчивающийся острым верхом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остроконечный",
                                "target_lang_sense_clarification": "имеющий острый конец"
                            },
                            {
                                "target_lang_word": "островерхий",
                                "target_lang_sense_clarification": "имеющий острую вершину (о горах, шапках)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
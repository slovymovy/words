{
    "word_family": [
        "meegaand",
        "meegaandheid"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2be3d92a-f697-39f2-85f7-ee20ba63d84f",
                    "sense_definition": "Samen met iemand anders naar dezelfde plek gaan.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil je vanavond met ons <w>meegaan</w> naar de bioscoop?",
                            "target_lang_translations": {
                                "en": "Do you want to <w>come along</w> with us to the cinema tonight?",
                                "ru": "Ты хочешь сегодня вечером <w>пойти</w> с нами в кино?"
                            }
                        },
                        {
                            "text": "Hij vroeg of ik wilde <w>meegaan</w> om boodschappen te doen.",
                            "target_lang_translations": {
                                "en": "He asked if I wanted to <w>come along</w> to get groceries.",
                                "ru": "Он спросил, хочу ли я <w>пойти</w> с ним за покупками."
                            }
                        }
                    ],
                    "synonyms": [
                        "vergezellen",
                        "meekomen"
                    ],
                    "antonyms": [
                        "achterblijven"
                    ],
                    "common_phrases": [
                        "met iemand meegaan",
                        "ga je mee"
                    ],
                    "target_lang_definitions": {
                        "en": "To go somewhere together with someone else.",
                        "ru": "Идти или ехать вместе с кем-то в одно и то же место."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come along",
                                "target_lang_sense_clarification": "to go somewhere with someone"
                            },
                            {
                                "target_lang_word": "join",
                                "target_lang_sense_clarification": "to connect with someone for an activity"
                            },
                            {
                                "target_lang_word": "accompany",
                                "target_lang_sense_clarification": "formal; to go with someone as a companion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пойти (вместе)",
                                "target_lang_sense_clarification": "отправиться куда-либо с кем-то"
                            },
                            {
                                "target_lang_word": "сопровождать",
                                "target_lang_sense_clarification": "быть спутником в пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4b16d01-e192-303c-9d72-f292b4213be7",
                    "sense_definition": "Een bepaalde tijd blijven functioneren of bruikbaar blijven.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "endurance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Goede wandelschoenen kunnen jarenlang <w>meegaan</w> als je ze goed onderhoudt.",
                            "target_lang_translations": {
                                "en": "Good hiking boots can <w>last</w> for years if you maintain them well.",
                                "ru": "Хорошие походные ботинки могут <w>прослужить</w> годами, если за ними правильно ухаживать."
                            }
                        },
                        {
                            "text": "Deze batterij zal waarschijnlijk niet zo <w>lang meegaan</w>.",
                            "target_lang_translations": {
                                "en": "This battery probably won't <w>last long</w>.",
                                "ru": "Эта батарейка, вероятно, не <w>прослужит долго</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "duren",
                        "standhouden"
                    ],
                    "antonyms": [
                        "stukgaan",
                        "kapotgaan"
                    ],
                    "common_phrases": [
                        "lang meegaan",
                        "jaren meegaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To remain functional or usable for a certain period of time.",
                        "ru": "Продолжать работать или оставаться пригодным для использования в течение определенного времени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "last",
                                "target_lang_sense_clarification": "to continue to function or exist"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "служить",
                                "target_lang_sense_clarification": "быть пригодным к использованию (об одежде, вещах)"
                            },
                            {
                                "target_lang_word": "прослужить",
                                "target_lang_sense_clarification": "проработать определенное время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "376edad0-5203-319b-9b34-6c78d4e528c4",
                    "sense_definition": "Zich aanpassen aan of instemmen met een idee, trend of ontwikkeling.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf moet <w>met de tijd meegaan</w> om relevant te blijven.",
                            "target_lang_translations": {
                                "en": "The company must <w>move with the times</w> to remain relevant.",
                                "ru": "Компания должна <w>идти в ногу со временем</w>, чтобы оставаться актуальной."
                            }
                        },
                        {
                            "text": "Ik kan helaas niet in jouw redenering <w>meegaan</w>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, I cannot <w>go along</w> with your reasoning.",
                                "ru": "К сожалению, я не могу <w>согласиться</w> с твоими доводами."
                            }
                        }
                    ],
                    "synonyms": [
                        "volgen",
                        "instemmen"
                    ],
                    "antonyms": [
                        "verzetten",
                        "weigeren"
                    ],
                    "common_phrases": [
                        "met de tijd meegaan",
                        "in iets meegaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To adapt to or agree with an idea, trend, or development.",
                        "ru": "Адаптироваться или соглашаться с идеей, тенденцией или развитием."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go along with",
                                "target_lang_sense_clarification": "to agree or comply with an idea"
                            },
                            {
                                "target_lang_word": "keep up with",
                                "target_lang_sense_clarification": "to adapt to changes or trends"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "соглашаться",
                                "target_lang_sense_clarification": "принимать чье-то мнение или идею"
                            },
                            {
                                "target_lang_word": "идти в ногу",
                                "target_lang_sense_clarification": "соответствовать (например, времени или моде)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
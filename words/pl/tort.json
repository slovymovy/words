                },
                {
                    "sense_id": "4cad14ad-81df-3827-bff4-0ba4db1398fe",
                    "sense_definition": "Chleb (w gwarze więziennej lub przestępczej).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więźniowie narzekali na twardy <w>tort</w> podawany na śniadanie.",

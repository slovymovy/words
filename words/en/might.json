{
    "word_family": [
        "mighty",
        "mightily",
        "mightiness",
        "almighty"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ac18b6b3-65c1-3971-bcc6-d99edd8166ba",
                    "sense_definition": "Used to indicate that something is possible, likely, or conditional.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "possibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>might</w> go to the party later if I finish my work.",
                            "target_lang_translations": {
                                "ru": "Я, <w>может быть</w>, пойду на вечеринку позже, если закончу работу."
                            }
                        },
                        {
                            "text": "It <w>might</w> rain this afternoon, so take an umbrella.",
                            "target_lang_translations": {
                                "ru": "Сегодня днем <w>может</w> пойти дождь, так что возьми зонт."
                            }
                        }
                    ],
                    "synonyms": [
                        "may"
                    ],
                    "common_phrases": [
                        "might be",
                        "might not",
                        "might as well"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на то, что что-то возможно, вероятно или зависит от условий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "может быть",
                                "target_lang_sense_clarification": "вводное слово, выражающее вероятность"
                            },
                            {
                                "target_lang_word": "возможно",
                                "target_lang_sense_clarification": "наречие, выражающее допустимость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8e42aaf-67af-3847-80a7-7a8763c9161c",
                    "sense_definition": "The simple past form of <w>may</w>, often used in reported speech.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "possibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He asked if he <w>might</w> join us for dinner.",
                            "target_lang_translations": {
                                "ru": "Он спросил, <w>можно</w> ли ему присоединиться к нам за ужином."
                            }
                        },
                        {
                            "text": "She said she <w>might</w> be late due to traffic.",
                            "target_lang_translations": {
                                "ru": "Она сказала, что <w>может</w> опоздать из-за пробок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "said he might",
                        "thought I might"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время от глагола may, часто используется в косвенной речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мог",
                                "target_lang_sense_clarification": "прошедшее время, мужской род"
                            },
                            {
                                "target_lang_word": "могла",
                                "target_lang_sense_clarification": "прошедшее время, женский род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb85c1eb-f39d-3f1c-8711-4d3dc5d8c5e2",
                    "sense_definition": "Used to ask for permission or make a suggestion in a polite and formal way.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Might</w> I ask where you bought that beautiful coat?",
                            "target_lang_translations": {
                                "ru": "<w>Могу</w> ли я спросить, где вы купили это красивое пальто?"
                            }
                        },
                        {
                            "text": "You <w>might</w> want to check the schedule before you leave.",
                            "target_lang_translations": {
                                "ru": "<w>Может быть</w>, вам стоит проверить расписание перед уходом."
                            }
                        }
                    ],
                    "synonyms": [
                        "may"
                    ],
                    "common_phrases": [
                        "might I ask",
                        "might I suggest"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для просьбы о разрешении или для предложения в вежливой и формальной форме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "могу ли я",
                                "target_lang_sense_clarification": "вежливая форма вопроса"
                            },
                            {
                                "target_lang_word": "можно",
                                "target_lang_sense_clarification": "разрешение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f591a377-619c-34a4-a00b-c5dd90e882d8",
                    "sense_definition": "Used to indicate that a past action was possible but did not happen, often to express annoyance or criticism.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "possibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You <w>might</w> have told me you were going to be late!",
                            "target_lang_translations": {
                                "ru": "Ты <w>мог бы</w> и сказать мне, что опаздываешь!"
                            }
                        },
                        {
                            "text": "He <w>might</w> have won the race if he had trained harder.",
                            "target_lang_translations": {
                                "ru": "Он <w>мог бы</w> выиграть гонку, если бы тренировался усерднее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "might have been",
                        "might have known"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на то, что прошлое действие было возможно, но не произошло; часто выражает раздражение или критику."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мог бы",
                                "target_lang_sense_clarification": "условное наклонение, упрек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62e2de92-29d3-3764-90ea-edeaca318852",
                    "sense_definition": "Used to admit a fact before stating something that contrasts with it.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "concession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>might</w> be young, but he is very experienced.",
                            "target_lang_translations": {
                                "ru": "Он, <w>может</w>, и молод, но очень опытен."
                            }
                        },
                        {
                            "text": "It <w>might</w> look easy, but the task is actually quite difficult.",
                            "target_lang_translations": {
                                "ru": "Это <w>может</w> выглядеть легко, но задача на самом деле довольно сложная."
                            }
                        }
                    ],
                    "common_phrases": [
                        "might be but"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для признания факта перед утверждением чего-то, что ему противоречит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "может",
                                "target_lang_sense_clarification": "уступка"
                            },
                            {
                                "target_lang_word": "пусть",
                                "target_lang_sense_clarification": "допущение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "61392bee-0f40-3b5c-985a-0a7f821ba980",
                    "sense_definition": "Great physical strength or force.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "strength",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He pushed the heavy door with all his <w>might</w>.",
                            "target_lang_translations": {
                                "ru": "Он толкал тяжелую дверь изо всех <w>сил</w>."
                            }
                        },
                        {
                            "text": "She threw the ball with all her <w>might</w> to reach the other side.",
                            "target_lang_translations": {
                                "ru": "Она бросила мяч изо всех <w>сил</w>, чтобы добросить до другой стороны."
                            }
                        }
                    ],
                    "synonyms": [
                        "strength",
                        "power",
                        "brawn",
                        "pith"
                    ],
                    "common_phrases": [
                        "with all one's might",
                        "main might"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая физическая сила или мощь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сила",
                                "target_lang_sense_clarification": "физическая способность"
                            },
                            {
                                "target_lang_word": "мощь",
                                "target_lang_sense_clarification": "огромная сила"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "407eac08-0dac-3459-9245-78bcdc87b540",
                    "sense_definition": "Power, authority, or influence possessed by a person, group, or nation.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The parade was a display of the nation's military <w>might</w>.",
                            "target_lang_translations": {
                                "ru": "Парад был демонстрацией военной <w>мощи</w> страны."
                            }
                        },
                        {
                            "text": "They feared the economic <w>might</w> of the large corporation.",
                            "target_lang_translations": {
                                "ru": "Они боялись экономической <w>мощи</w> крупной корпорации."
                            }
                        }
                    ],
                    "synonyms": [
                        "power",
                        "authority",
                        "potency"
                    ],
                    "common_phrases": [
                        "military might",
                        "economic might"
                    ],
                    "target_lang_definitions": {
                        "ru": "Власть, авторитет или влияние, которыми обладает человек, группа или нация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мощь",
                                "target_lang_sense_clarification": "военная или государственная сила"
                            },
                            {
                                "target_lang_word": "могущество",
                                "target_lang_sense_clarification": "власть и влияние"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "dc3c30b8-c5c3-3746-903b-ef693b6dc6f4",
                    "sense_definition": "Possessing great power or strength.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>might</w> warrior stood tall against his enemies in the ancient tale.",
                            "target_lang_translations": {
                                "ru": "<w>Могучий</w> воин стоял во весь рост против своих врагов в древнем сказании."
                            }
                        },
                        {
                            "text": "They spoke of <w>might</w> deeds done in the days of old.",
                            "target_lang_translations": {
                                "ru": "Они говорили о <w>великих</w> деяниях, совершенных в былые времена."
                            }
                        }
                    ],
                    "synonyms": [
                        "power",
                        "strength"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rarely used as an adjective in modern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий огромной властью или силой (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "могучий",
                                "target_lang_sense_clarification": "сильный, мощный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
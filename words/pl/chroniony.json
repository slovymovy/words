                    "sense_id": "bf1f7551-5602-3daa-b872-7d1f74a12847",
                    "sense_definition": "Taki, który jest otoczony opieką i zabezpieczony przed zniszczeniem, atakiem lub wyginięciem (często na mocy prawa).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "protection_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym parku narodowym każdy gatunek zwierzęcia jest ściśle <w>chroniony</w>.",
                            "target_lang_translations": {
                                "ru": "В этом национальном парке каждый вид животных строго <w>охраняется</w>."
                            }
                        },
                        {
                            "text": "Wchodzisz na obszar <w>chroniony</w>, więc musisz zachować szczególną ostrożność.",
                            "target_lang_translations": {
                                "ru": "Вы входите на <w>охраняемую</w> территорию, поэтому должны соблюдать особую осторожность."
                            }
                        }
                    ],
                    "synonyms": [
                        "strzeżony",
                        "zabezpieczony",
                        "osłaniany"
                    ],
                    "antonyms": [
                        "zagrożony",
                        "niechroniony"
                    ],
                    "common_phrases": [

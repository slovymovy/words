                    "sense_id": "26331779-b603-3cff-8d0f-7724c2ecb161",
                    "sense_definition": "Een groot bassin gevuld met water om in te zwemmen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze maakten 's winters het <w>zwembad</w> leeg.",
                            "target_lang_translations": {
                                "ru": "Зимой они спускали воду из <w>бассейна</w>.",
                                "en": "They emptied the <w>swimming pool</w> in the winter."
                            }
                        },
                        {
                            "text": "Het water in het <w>zwembad</w> is lekker warm.",
                            "target_lang_translations": {
                                "ru": "Вода в <w>бассейне</w> приятно теплая.",
                                "en": "The water in the <w>swimming pool</w> is nice and warm."
                            }

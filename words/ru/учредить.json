    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d7753d5-8050-3d8d-9d85-aa78b3e4f8b7",
                    "sense_definition": "Основать или создать организацию, предприятие, учебное заведение или иную структуру.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Партнёры решили официально <w>учредить</w> новую торговую компанию в следующем месяце.",
                            "target_lang_translations": {
                                "en": "The partners decided to officially <w>found</w> a new trading company next month."
                            }
                        },
                        {
                            "text": "Меценат мечтал <w>учредить</w> стипендию для талантливых студентов университета.",
                            "target_lang_translations": {
                                "en": "The patron dreamed of <w>establishing</w> a scholarship for talented university students."
                            }
                        }
                    ],
                    "synonyms": [
                        "основать"
                    ],
                    "antonyms": [
                        "ликвидировать",
                        "расформировать"
                    ],
                    "common_phrases": [
                        "учредить компанию",
                        "учредить фонд",
                        "учредить банк",
                        "учредить общество"
                    ],
                    "target_lang_definitions": {
                        "en": "To start or create an organization, company, or institution."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "found",
                                "target_lang_sense_clarification": "an organization or institution"
                            },
                            {
                                "target_lang_word": "establish",
                                "target_lang_sense_clarification": "a company or business"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd0f78ce-55d4-3544-b7ff-a35dfb7366b6",
                    "sense_definition": "Ввести в действие или установить какое-либо правило, должность, звание или награду.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство планирует <w>учредить</w> специальную награду за заслуги в области науки.",
                            "target_lang_translations": {
                                "en": "The government plans to <w>institute</w> a special award for achievements in the field of science."
                            }
                        },
                        {
                            "text": "Директор завода приказал <w>учредить</w> строгий контроль качества продукции.",
                            "target_lang_translations": {
                                "en": "The factory director ordered to <w>establish</w> strict quality control of the products."
                            }
                        }
                    ],
                    "synonyms": [
                        "ввести",
                        "установить"
                    ],
                    "antonyms": [
                        "упразднить"
                    ],
                    "common_phrases": [
                        "учредить награду",
                        "учредить премию",
                        "учредить праздник",
                        "учредить пост"
                    ],
                    "target_lang_definitions": {
                        "en": "To introduce or set up a rule, position, title, or award."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "institute",
                                "target_lang_sense_clarification": "a rule, award, or custom"
                            },
                            {
                                "target_lang_word": "establish",
                                "target_lang_sense_clarification": "a rule, position, or control"
                            }
                        ]
                    }
                }
            ]
        }
    ]

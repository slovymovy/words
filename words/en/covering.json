    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a3825120-7020-3d9e-80a5-60bfbd3e3387",
                    "sense_definition": "Something that covers, conceals, or protects a surface or object.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The floor had a durable vinyl <covering> that was easy to clean.",
                            "target_lang_translations": {
                                "ru": "На полу было прочное виниловое <w>покрытие</w>, которое легко мыть."
                            }
                        },
                        {
                            "text": "A thick <covering> of snow made the roads impassable.",
                            "target_lang_translations": {
                                "ru": "Толстый <w>покров</w> снега сделал дороги непроходимыми."
                            }
                        }
                    ],
                    "synonyms": [
                        "layer",
                        "coating",
                        "blanket",
                        "overlay"
                    ],
                    "common_phrases": [
                        "floor covering",
                        "face covering",
                        "protective covering",
                        "wall covering"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то, что покрывает, скрывает или защищает поверхность или объект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрытие",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "покров",
                                "target_lang_sense_clarification": "слой (снега, листвы)"
                            },
                            {
                                "target_lang_word": "настил",
                                "target_lang_sense_clarification": "для пола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "968253e7-be12-3ae0-a056-c95fc114559f",
                    "sense_definition": "The action of placing something over an object or surface to conceal or protect it.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <covering> of the furniture with plastic sheets protected it during the painting process.",
                            "target_lang_translations": {
                                "ru": "<w>Укрывание</w> мебели полиэтиленовой пленкой защитило её во время покраски."
                            }
                        },
                        {
                            "text": "Proper <covering> of the wound is essential to prevent infection.",
                            "target_lang_translations": {
                                "ru": "Правильное <w>закрытие</w> раны необходимо для предотвращения инфекции."
                            }
                        }
                    ],
                    "synonyms": [
                        "wrapping",
                        "concealing",
                        "masking"
                    ],
                    "antonyms": [
                        "exposing",
                        "uncovering"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по размещению чего-либо поверх объекта или поверхности для сокрытия или защиты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрывание",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "накрывание",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "065c6b95-d26d-3ca7-a757-c729bc611533",
                    "sense_definition": "A collection of subsets whose union contains the entire set.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor asked the students to find a finite <covering> for the compact set.",
                            "target_lang_translations": {
                                "ru": "Профессор попросил студентов найти конечное <w>покрытие</w> для компактного множества."
                            }
                        },
                        {
                            "text": "In topology, an open <covering> is a fundamental concept for defining compactness.",
                            "target_lang_translations": {
                                "ru": "В топологии открытое <w>покрытие</w> является фундаментальным понятием для определения компактности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "open covering"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор подмножеств, объединение которых содержит всё множество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрытие",
                                "target_lang_sense_clarification": "математика"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "table",
        "tabular",
        "tabulate"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "97259b19-3f6d-3809-9e05-ddc4bf3deafb",
                    "sense_definition": "A portable computer with a touchscreen interface, usually smaller than a laptop but larger than a smartphone.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "computer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She uses her <w>tablet</w> to watch movies while traveling on the train.",
                            "target_lang_translations": {
                                "ru": "Она использует свой <w>планшет</w>, чтобы смотреть фильмы во время поездки на поезде."
                            }
                        },
                        {
                            "text": "I downloaded a new drawing application onto my <w>tablet</w> yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера я скачал новое приложение для рисования на свой <w>планшет</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "graphics tablet",
                        "digital tablet",
                        "gaming tablet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Портативный компьютер с сенсорным интерфейсом, обычно меньше ноутбука, но больше смартфона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "планшет",
                                "target_lang_sense_clarification": "компьютер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32010daf-3da4-362e-917b-80026438e970",
                    "sense_definition": "A small, solid piece of medicine that is swallowed.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medicine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor told him to take one <w>tablet</w> with water every morning.",
                            "target_lang_translations": {
                                "ru": "Врач сказал ему принимать одну <w>таблетку</w> с водой каждое утро."
                            }
                        },
                        {
                            "text": "She swallowed a vitamin <w>tablet</w> immediately after eating her breakfast.",
                            "target_lang_translations": {
                                "ru": "Она проглотила <w>таблетку</w> витамина сразу после завтрака."
                            }
                        }
                    ],
                    "synonyms": [
                        "pill"
                    ],
                    "common_phrases": [
                        "vitamin tablet",
                        "sleeping tablet",
                        "take a tablet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой твердый кусочек лекарства, который проглатывают."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таблетка",
                                "target_lang_sense_clarification": "лекарство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61997d54-97f0-35b5-b378-4f9f896864e8",
                    "sense_definition": "A flat slab of stone, clay, or wood used for writing or carving, especially in ancient times.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "slab",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Archaeologists discovered a clay <w>tablet</w> covered in ancient writing near the ruins.",
                            "target_lang_translations": {
                                "ru": "Археологи обнаружили глиняную <w>табличку</w>, покрытую древними письменами, недалеко от руин."
                            }
                        },
                        {
                            "text": "The names of the fallen soldiers were carved onto a stone memorial <w>tablet</w>.",
                            "target_lang_translations": {
                                "ru": "Имена павших солдат были высечены на каменной мемориальной <w>плите</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "slab"
                    ],
                    "common_phrases": [
                        "stone tablet",
                        "clay tablet",
                        "memorial tablet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская плита из камня, глины или дерева, используемая для письма или резьбы, особенно в древности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "табличка",
                                "target_lang_sense_clarification": "глиняная или с надписью"
                            },
                            {
                                "target_lang_word": "дощечка",
                                "target_lang_sense_clarification": "деревянная или восковая"
                            },
                            {
                                "target_lang_word": "плита",
                                "target_lang_sense_clarification": "каменная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "416ae640-1bbc-37c3-bc73-8b3bd77c7127",
                    "sense_definition": "A pad of paper sheets bound together at one edge.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "paper",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He wrote his shopping list on a small <w>tablet</w> of paper.",
                            "target_lang_translations": {
                                "ru": "Он написал список покупок в маленьком <w>блокноте</w>."
                            }
                        },
                        {
                            "text": "Please tear a clean sheet off the writing <w>tablet</w> for me.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, оторви мне чистый лист из <w>блокнота</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pad",
                        "notepad"
                    ],
                    "common_phrases": [
                        "writing tablet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Блок бумажных листов, скрепленных с одной стороны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блокнот",
                                "target_lang_sense_clarification": "для записей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fb14775-516c-352f-aebe-22ddad0b7897",
                    "sense_definition": "A hard, sugary confection made with butter and milk, similar to fudge but with a grainier texture.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bought a bag of sweet vanilla <w>tablet</w> from the local shop.",
                            "target_lang_translations": {
                                "ru": "Мы купили пакетик сладкого ванильного <w>таблета</w> в местном магазине."
                            }
                        },
                        {
                            "text": "My grandmother makes delicious butter <w>tablet</w> for the holidays every year.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка каждый год готовит вкусный сливочный <w>таблет</w> к праздникам."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердое сладкое кондитерское изделие из масла и молока, похожее на помадку, но с более зернистой текстурой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таблет",
                                "target_lang_sense_clarification": "шотландская сладость"
                            },
                            {
                                "target_lang_word": "помадка",
                                "target_lang_sense_clarification": "конфета"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a00de966-8927-3ebf-ac38-a2be2bad685c",
                    "sense_definition": "To form a substance into a small, solid pill or flat piece.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pharmaceutical machine is used to <w>tablet</w> the powder into precise doses.",
                            "target_lang_translations": {
                                "ru": "Фармацевтическая машина используется, чтобы <w>таблетировать</w> порошок в точные дозы."
                            }
                        },
                        {
                            "text": "They <w>tablet</w> the mixture under high pressure to create the final product.",
                            "target_lang_translations": {
                                "ru": "Они <w>таблетируют</w> смесь под высоким давлением для создания конечного продукта."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Формовать вещество в небольшую твердую пилюлю или плоский кусок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таблетировать",
                                "target_lang_sense_clarification": "прессовать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            },
                            {
                                "target_lang_word": "вмиг",
                                "target_lang_sense_clarification": "мгновенно"
                            },
                            {
                                "target_lang_word": "моментально",
                                "target_lang_sense_clarification": "сразу же"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1767c4b2-139d-35e4-be69-7ad5cda062d1",
                    "sense_definition": "W sposób charakteryzujący się dużą ilością światła; przeciwieństwo ciemności.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "light_and_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem bardzo wcześnie rano robi się <w>jasno</w>.",
                            "target_lang_translations": {
                                "ru": "Летом очень рано утром становится <w>светло</w>."
                            }
                        },
                        {
                            "text": "W tym pokoju jest bardzo <w>jasno</w> dzięki dużym oknom.",
                            "target_lang_translations": {
                                "ru": "В этой комнате очень <w>светло</w> благодаря большим окнам."
                            }
                        }
                    ],
                    "antonyms": [
                        "ciemno"
                    ],
                    "common_phrases": [
                        "robi się jasno",
                        "jest jasno"
                    ],
                    "target_lang_definitions": {
                        "ru": "С большим количеством света; противоположность темноте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светло",
                                "target_lang_sense_clarification": "об освещении"
                            },
                            {
                                "target_lang_word": "ясно",
                                "target_lang_sense_clarification": "о небе или погоде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b41786c0-00f5-33be-9f83-76fe47418cd8",
                    "sense_definition": "W sposób łatwy do zrozumienia, oczywisty i niebudzący wątpliwości.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel <w>jasno</w> wytłumaczył uczniom zasady nowego zadania.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>ясно</w> объяснил ученикам правила нового задания."
                            }
                        },
                        {
                            "text": "Musimy <w>jasno</w> określić nasze cele na przyszły rok.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>чётко</w> определить наши цели на следующий год."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrozumiale",
                        "wyraźnie",
                        "przejrzyście",
                        "prosto"
                    ],
                    "antonyms": [
                        "niejasno",
                        "zawile"
                    ],
                    "common_phrases": [
                        "jasno określić",
                        "jasno powiedzieć",
                        "wyrazić się jasno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понятно, очевидно и не вызывая сомнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ясно",
                                "target_lang_sense_clarification": "понятно"
                            },
                            {
                                "target_lang_word": "понятно",
                                "target_lang_sense_clarification": "доступно для понимания"
                            },
                            {
                                "target_lang_word": "чётко",
                                "target_lang_sense_clarification": "разборчиво, точно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b48bfb87-fc31-3648-9fef-68bf2f2b170d",
                    "sense_definition": "W jasnym odcieniu lub kolorze; blado.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "light_and_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na ślub siostry ubrała się <w>jasno</w>, w pastelowe kolory.",
                            "target_lang_translations": {
                                "ru": "На свадьбу сестры она оделась <w>светло</w>, в пастельные тона."
                            }
                        },
                        {
                            "text": "Dlaczego tak <w>jasno</w> się malujesz, skoro jest wieczór?",
                            "target_lang_translations": {
                                "ru": "Почему ты так <w>светло</w> красишься, если уже вечер?"
                            }
                        }
                    ],
                    "antonyms": [
                        "ciemno"
                    ],
                    "common_phrases": [
                        "jasno ubrany",
                        "jasno pomalowany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Светлого оттенка или цвета; бледно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светло",
                                "target_lang_sense_clarification": "о цвете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63c04f4b-29ba-3182-930b-d4d276bd3d3b",
                    "sense_definition": "W sposób zdecydowany, stanowczy i bezpośredni, nie pozostawiając złudzeń.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Postawiła sprawę <w>jasno</w>: albo on przestanie pić, albo ona odejdzie.",
                            "target_lang_translations": {
                                "ru": "Она обозначила ситуацию <w>ясно</w>: или он бросит пить, или она уйдёт."
                            }
                        },
                        {
                            "text": "Dyrektor wyraził się <w>jasno</w> w kwestii spóźnień do pracy.",
                            "target_lang_translations": {
                                "ru": "Директор выразился <w>ясно</w> по вопросу опозданий на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyraźnie",
                        "prosto"
                    ],
                    "antonyms": [
                        "niejasno"
                    ],
                    "common_phrases": [
                        "postawić sprawę jasno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решительно, твердо и прямо, не оставляя иллюзий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямо",
                                "target_lang_sense_clarification": "откровенно"
                            },
                            {
                                "target_lang_word": "чётко",
                                "target_lang_sense_clarification": "недвусмысленно"
                            },
                            {
                                "target_lang_word": "ясно",
                                "target_lang_sense_clarification": "определённо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80f8a84e-38b2-3c00-8f5e-b1f7343d9bcd",
                    "sense_definition": "W sposób beztroski, pogodny i pełen optymizmu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo wielu trudności, wciąż <w>jasno</w> patrzył w przyszłość.",
                            "target_lang_translations": {
                                "ru": "Несмотря на множество трудностей, он всё ещё <w>светло</w> смотрел в будущее."
                            }
                        },
                        {
                            "text": "Jej twarz uśmiechnęła się <w>jasno</w> na widok przyjaciela.",
                            "target_lang_translations": {
                                "ru": "Её лицо <w>светло</w> улыбнулось при виде друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "pogodnie"
                    ],
                    "common_phrases": [
                        "patrzeć jasno"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in literary or metaphorical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Беззаботно, радостно и с оптимизмом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "светло",
                                "target_lang_sense_clarification": "радостно, метафорически"
                            },
                            {
                                "target_lang_word": "радостно",
                                "target_lang_sense_clarification": "весело"
                            }
                        ]
                    }
                }
            ]
        }

                {
                    "sense_id": "a016410b-f525-31fb-a38e-af503006d4af",
                    "sense_definition": "Brać udział w starciu zbrojnym lub fizycznym, używając siły lub broni przeciwko wrogowi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "combat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze musieli dzielnie <w>walczyć</w> w obronie swojego kraju.",
                            "target_lang_translations": {
                                "ru": "Солдаты должны были отважно <w>сражаться</w>, защищая свою страну."
                            }
                        },
                        {
                            "text": "Dwa psy zaczęły głośno szczekać i <w>walczyć</w> na środku ulicy.",
                            "target_lang_translations": {
                                "ru": "Две собаки начали громко лаять и <w>драться</w> посреди улицы."
                            }
                        }
                    ],
                    "synonyms": [
                        "bić się",
                        "wojować",
                        "toczyć walkę"
                    ],
                    "antonyms": [
                        "poddawać się"
                    ],
                    "common_phrases": [

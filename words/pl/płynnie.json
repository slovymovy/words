            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "57ee3913-afb4-3de8-b628-66204b1c7a57",
                    "sense_definition": "W sposób biegły i swobodny, bez trudności (zwykle o posługiwaniu się językiem obcym).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fluency_language",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat potrafi <w>płynnie</w> mówić w trzech językach obcych.",
                            "target_lang_translations": {
                                "ru": "Мой брат может <w>бегло</w> говорить на трех иностранных языках."
                            }
                        },
                        {
                            "text": "Moja nowa sąsiadka mówi <w>płynnie</w> po polsku, choć pochodzi z Gruzji.",
                            "target_lang_translations": {
                                "ru": "Моя новая соседка <w>свободно</w> говорит по-польски, хотя родом из Грузии."
                            }
                        }
                    ],
                    "synonyms": [
                        "biegle",
                        "swobodnie"
                    ],
                    "common_phrases": [
                        "mówić płynnie",
                        "czytać płynnie",
                        "płynnie po angielsku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободно, без запинок и затруднений (обычно о речи или чтении на иностранном языке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегло",
                                "target_lang_sense_clarification": "о чтении или речи"
                            },
                            {
                                "target_lang_word": "свободно",
                                "target_lang_sense_clarification": "о владении языком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e04d043c-540a-331d-811d-279b84161436",
                    "sense_definition": "W sposób ciągły, harmonijny, bez przerw, zacięć lub zakłóceń (o ruchu, działaniu urządzenia lub procesie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "smoothness_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na twoim starym laptopie ta nowa gra nie będzie <w>płynnie</w> działać.",
                            "target_lang_translations": {
                                "ru": "На твоем старом ноутбуке эта новая игра не будет <w>плавно</w> работать."
                            }
                        },
                        {
                            "text": "Samochód ruszył <w>płynnie</w> ze skrzyżowania, nie szarpiąc silnikiem.",
                            "target_lang_translations": {
                                "ru": "Автомобиль <w>плавно</w> тронулся с перекрестка, без рывков двигателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "wprawnie",
                        "swobodnie"
                    ],
                    "common_phrases": [
                        "działać płynnie",
                        "poruszać się płynnie",
                        "przechodzić płynnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без рывков, задержек или сбоев; гармонично (о движении или работе техники)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плавно",
                                "target_lang_sense_clarification": "о движении без рывков"
                            },
                            {
                                "target_lang_word": "бесперебойно",
                                "target_lang_sense_clarification": "о работе системы или процесса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "394fac7c-ca97-3623-926c-e93c5d4ef766",
                    "sense_definition": "W sposób przypominający ruch cieczy; miękko, łagodnie i bez kanciastości.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fluidity_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kolory na tym obrazie <w>płynnie</w> przechodzą jeden w drugi.",
                            "target_lang_translations": {
                                "ru": "Цвета на этой картине <w>плавно</w> переходят друг в друга."
                            }
                        },
                        {
                            "text": "Tancerka poruszała się po scenie bardzo <w>płynnie</w> i z gracją.",
                            "target_lang_translations": {
                                "ru": "Танцовщица двигалась по сцене очень <w>плавно</w> и грациозно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "płynnie zmieniać się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мягко, перетекая, подобно движению жидкости; без резких границ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плавно",
                                "target_lang_sense_clarification": "о переходах или грациозных движениях"
                            }
                        ]
                    }
                }
            ]
        }
    ]

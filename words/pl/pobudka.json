    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c5ea203e-57f9-306d-b0b7-16a4fd4fe661",
                    "sense_definition": "sygnał wzywający do wstania ze snu lub sama czynność budzenia",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro <pobudka> zaplanowana jest na godzinę szóstą rano.",
                            "target_lang_translations": {
                                "ru": "Завтра <подъём> запланирован на шесть часов утра."
                            }
                        },
                        {
                            "text": "Głośna <pobudka> nie pozwoliła nam dłużej spać.",
                            "target_lang_translations": {
                                "ru": "Громкая <побудка> не позволила нам дольше спать."
                            }
                        }
                    ],
                    "synonyms": [
                        "budzenie"
                    ],
                    "antonyms": [
                        "capstrzyk"
                    ],
                    "common_phrases": [
                        "poranna pobudka",
                        "wczesna pobudka",
                        "sygnał pobudki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сигнал к пробуждению или само действие пробуждения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подъём",
                                "target_lang_sense_clarification": "время вставать, команда"
                            },
                            {
                                "target_lang_word": "побудка",
                                "target_lang_sense_clarification": "сигнал (обычно в армии или лагере)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0153f844-cb37-37d4-a268-9c7488f95ec1",
                    "sense_definition": "wewnętrzny bodziec, motyw lub przyczyna skłaniająca do określonego działania",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego decyzją kierowały wyłącznie szlachetne <pobudki>.",
                            "target_lang_translations": {
                                "ru": "Его решением руководили исключительно благородные <побуждения>."
                            }
                        },
                        {
                            "text": "Nie sądzę, aby miał jakieś ukryte <pobudki>.",
                            "target_lang_translations": {
                                "ru": "Не думаю, что у него были какие-то скрытые <мотивы>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyczyna",
                        "budzenie"
                    ],
                    "common_phrases": [
                        "szlachetne pobudki",
                        "niskie pobudki",
                        "działać z pobudek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренний стимул, мотив или причина, побуждающая к определенному действию"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побуждение",
                                "target_lang_sense_clarification": "внутренний порыв"
                            },
                            {
                                "target_lang_word": "мотив",
                                "target_lang_sense_clarification": "причина поступка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "fb19c7c8-8303-366d-bbd4-25d9965179ba",
                    "sense_definition": "okrzyk używany, aby kogoś obudzić i nakłonić do wstania",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Pobudka>, musimy zdążyć na pociąg!",
                            "target_lang_translations": {
                                "ru": "<Подъём>, мы должны успеть на поезд!"
                            }
                        },
                        {
                            "text": "<Pobudka>! Słońce już dawno wzeszło.",
                            "target_lang_translations": {
                                "ru": "<Подъём>! Солнце уже давно взошло."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pobudka wstać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, используемый, чтобы кого-то разбудить и заставить встать"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Подъём!",
                                "target_lang_sense_clarification": "команда встать"
                            },
                            {
                                "target_lang_word": "Вставай!",
                                "target_lang_sense_clarification": "призыв проснуться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "3f95c86e-c81f-3bd8-931e-5231e210d097",
                    "sense_definition": "Быстро, резко укусить (обычно о животном или насекомом).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Осторожно, эта злая собака может больно <w>тяпнуть</w> за ногу.",
                            "target_lang_translations": {
                                "en": "Careful, that angry dog can painfully <w>nip</w> your leg."
                            }
                        },
                        {
                            "text": "Меня успел <w>тяпнуть</w> комар, пока я гулял в лесу.",
                            "target_lang_translations": {
                                "en": "A mosquito managed to <w>bite</w> me while I was walking in the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "куснуть"
                    ],
                    "common_phrases": [
                        "собака тяпнула",
                        "тяпнуть за палец",
                        "больно тяпнуть"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "прост."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To bite quickly or sharply (usually said of an animal or insect)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nip",
                                "target_lang_sense_clarification": "to bite quickly or lightly"
                            },
                            {
                                "target_lang_word": "bite",
                                "target_lang_sense_clarification": "general action of biting"
                            },
                            {
                                "target_lang_word": "snap",
                                "target_lang_sense_clarification": "to make a sudden biting motion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "308df418-7d82-3978-aff6-6650617d1995",
                    "sense_definition": "Выпить спиртного (обычно быстро, залпом или небольшое количество).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После тяжёлой работы мы решили <w>тяпнуть</w> по рюмочке водки.",
                            "target_lang_translations": {
                                "en": "After hard work, we decided to <w>knock back</w> a shot of vodka."
                            }
                        },
                        {
                            "text": "Я бы сейчас с удовольствием <w>тяпнул</w> холодного пива.",
                            "target_lang_translations": {
                                "en": "I would gladly <w>down</w> a cold beer right now."
                            }
                        }
                    ],
                    "synonyms": [
                        "хряпнуть"
                    ],
                    "common_phrases": [
                        "тяпнуть водки",
                        "тяпнуть стопку",
                        "тяпнуть с горя"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "прост."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To drink alcohol (usually quickly, in one gulp, or a small amount)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "knock back",

                    "synonyms": [
                        "agreed",
                        "you have a deal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения согласия с предложением или заключением сделки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по рукам",
                                "target_lang_sense_clarification": "согласие на сделку"
                            },
                            {
                                "target_lang_word": "договорились",
                                "target_lang_sense_clarification": "согласие"
                            }
                        ]
                    }
                }

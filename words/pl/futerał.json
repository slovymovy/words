    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c156722-1cd6-38db-bc7f-130df071e420",
                    "sense_definition": "Ochronne opakowanie lub pojemnik służący do przechowywania i przenoszenia przedmiotów, które mogą łatwo ulec uszkodzeniu, np. instrumentów muzycznych, okularów czy broni.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić nowy, twardy <w>futerał</w> na moją gitarę, aby bezpiecznie ją przewozić.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый жесткий <w>футляр</w> для моей гитары, чтобы безопасно ее перевозить."
                            }
                        },
                        {
                            "text": "Włóż okulary do <w>futerału</w>, żeby szkła się nie porysowały w torebce.",
                            "target_lang_translations": {
                                "ru": "Положи очки в <w>футляр</w>, чтобы стекла не поцарапались в сумочке."
                            }
                        }
                    ],
                    "synonyms": [
                        "etui",
                        "pokrowiec"
                    ],
                    "common_phrases": [
                        "futerał na skrzypce",
                        "futerał na okulary",
                        "skórzany futerał",
                        "twardy futerał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитная упаковка или контейнер для хранения и переноски предметов, которые могут легко повредиться, например, музыкальных инструментов, очков или оружия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "футляр",
                                "target_lang_sense_clarification": "жесткий кейс для очков, инструментов"
                            },
                            {
                                "target_lang_word": "чехол",
                                "target_lang_sense_clarification": "мягкая или жесткая оболочка для защиты"
                            },
                            {
                                "target_lang_word": "кейс",
                                "target_lang_sense_clarification": "жесткий чемоданчик для аппаратуры или инструментов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d185bd3-374f-3e3d-bc53-e5ad7b77ab73",
                    "sense_definition": "Potoczne lub żartobliwe określenie trumny.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "coffin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziadek zawsze żartował, że jeszcze nie wybiera się w sosnowy <w>futerał</w>.",
                            "target_lang_translations": {
                                "ru": "Дедушка всегда шутил, что еще не собирается в сосновый <w>ящик</w>."
                            }
                        },
                        {
                            "text": "Dla tak zasłużonego człowieka zamówiono wyjątkowo ozdobny, dębowy <w>futerał</w>.",
                            "target_lang_translations": {
                                "ru": "Для такого заслуженного человека заказали исключительно украшенный дубовый <w>гроб</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "trumna"
                    ],
                    "common_phrases": [
                        "sosnowy futerał",
                        "drewniany futerał"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used humorously or euphemistically"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное или шутливое название гроба."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гроб",
                                "target_lang_sense_clarification": "ироничное или эвфемистическое употребление"
                            },
                            {
                                "target_lang_word": "ящик",
                                "target_lang_sense_clarification": "разговорное, сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
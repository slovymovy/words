    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ad1dfc51-4639-34bb-9714-4e4e02694876",
                    "sense_definition": "Tradycyjne, słodkie, płaskie ciasto na kruchym spodzie, bogato zdobione lukrem, masą i bakaliami, wypiekane zazwyczaj na Wielkanoc.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wielkanocnym stole nie może zabraknąć słodkiego <w>mazurka</w>.",
                            "target_lang_translations": {
                                "ru": "На пасхальном столе не может не быть сладкой <w>мазурки</w>."
                            }
                        },
                        {
                            "text": "Moja babcia piecze najlepszy <w>mazurek</w> kajmakowy na świecie.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка печет лучшую <w>мазурку</w> с каймаком в мире."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mazurek wielkanocny",
                        "piec mazurka",
                        "kawałek mazurka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционный сладкий плоский пирог на песочном тесте, богато украшенный глазурью, массой и сухофруктами, выпекаемый обычно на Пасху."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мазурка",
                                "target_lang_sense_clarification": "пасхальный пирог"
                            },
                            {
                                "target_lang_word": "мазурек",
                                "target_lang_sense_clarification": "польская выпечка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d97ec0e0-27f6-3a5c-a9a8-192269ce1b0d",
                    "sense_definition": "Mały ptak z rodziny wróblowatych (Passer montanus), podobny do wróbla, z brązową czapeczką na głowie i czarną plamką na policzku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W karmniku obok wróbli pojawił się też jeden <w>mazurek</w>.",
                            "target_lang_translations": {
                                "ru": "В кормушке рядом с воробьями появился и один <w>полевой воробей</w>."
                            }
                        },
                        {
                            "text": "Zimą często dokarmiamy <w>mazurki</w> ziarnami słonecznika.",
                            "target_lang_translations": {
                                "ru": "Зимой мы часто подкармливаем <w>полевых воробьёв</w> семенами подсолнечника."
                            }
                        }
                    ],
                    "synonyms": [
                        "wróbel mazurek"
                    ],
                    "common_phrases": [
                        "gniazdo mazurka",
                        "samiec mazurka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькая птица из семейства воробьиных (Passer montanus), похожая на воробья, с коричневой шапочкой на голове и черным пятнышком на щеке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полевой воробей",
                                "target_lang_sense_clarification": "вид птицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46e9e88b-4a25-30b6-9ed2-73917ed086e9",
                    "sense_definition": "Żywy polski taniec ludowy w metrum trójdzielnym, o skocznym charakterze i z akcentem na drugą lub trzecią miarę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_dance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zespół ludowy zatańczył skocznego <w>mazurka</w> na scenie.",
                            "target_lang_translations": {
                                "ru": "Фольклорный ансамбль станцевал прыгучую <w>мазурку</w> на сцене."
                            }
                        },
                        {
                            "text": "Na zajęciach tanecznych uczyliśmy się podstawowych kroków <w>mazurka</w>.",
                            "target_lang_translations": {
                                "ru": "На занятиях танцами мы учили основные шаги <w>мазурки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mazur"
                    ],
                    "common_phrases": [
                        "tańczyć mazurka",
                        "kroki mazurka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живой польский народный танец в трехдольном размере, скачкообразного характера с акцентом на вторую или третью долю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мазурка",
                                "target_lang_sense_clarification": "танец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "346573d9-b10c-3ea5-ad72-ac4ff7155f81",
                    "sense_definition": "Utwór muzyczny stylizowany na tańcu ludowym, często o charakterze lirycznym lub wirtuozowskim, spopularyzowany przez Fryderyka Chopina.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_dance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pianista zagrał przepiękny <w>mazurek</w> Chopina na bis.",
                            "target_lang_translations": {
                                "ru": "Пианист сыграл прекрасную <w>мазурку</w> Шопена на бис."
                            }
                        },
                        {
                            "text": "W filharmonii wysłuchaliśmy koncertu, w którym dominowały <w>mazurki</w>.",
                            "target_lang_translations": {
                                "ru": "В филармонии мы слушали концерт, в котором преобладали <w>мазурки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mazurki Chopina",
                        "grać mazurka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальное произведение, стилизованное под народный танец, часто лирического или виртуозного характера, популяризированное Фредериком Шопеном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мазурка",
                                "target_lang_sense_clarification": "музыкальное произведение"
                            }
                        ]
                    }
                }
            ]
        },

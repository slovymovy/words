                },
                {
                    "sense_id": "e05c7fe2-4b55-3ede-aa8f-27b2981af0ed",
                    "sense_definition": "Pomieszczenie na poddaszu służące do przechowywania rzeczy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "rooms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zanieśliśmy stare meble na <izbę>, żeby nie przeszkadzały w domu.",
                            "target_lang_translations": {
                                "ru": "Мы отнесли старую мебель на <чердак>, чтобы она не мешала в доме."
                            }
                        },
                        {
                            "text": "Na <izbie> suszyło się pranie rozwieszone na sznurach.",
                            "target_lang_translations": {
                                "ru": "На <чердаке> сушилось белье, развешанное на веревках."
                            }
                        }
                    ],
                    "synonyms": [
                        "strych"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "regionalizm śląski lub dawne znaczenie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещение на чердаке, используемое для хранения вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чердак",
                                "target_lang_sense_clarification": "помещение под крышей"

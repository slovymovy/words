    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7296d8b7-6837-3760-ab61-bb206ec97481",
                    "sense_definition": "Posiadający dużą wiedzę, wprawę i umiejętności w jakiejś dziedzinie; wykonujący coś sprawnie i fachowo.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "skill_proficiency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wymagana jest <biegła> znajomość języka angielskiego w mowie i piśmie.",
                            "target_lang_translations": {
                                "ru": "Требуется <свободное> владение английским языком в устной и письменной форме."
                            }
                        },
                        {
                            "text": "Ten <biegły> rzemieślnik potrafi naprawić nawet najbardziej zniszczony zegar.",
                            "target_lang_translations": {
                                "ru": "Этот <искусный> ремесленник может починить даже самые сломанные часы."
                            }
                        }
                    ],
                    "synonyms": [
                        "wprawny",
                        "doświadczony",
                        "wprawiony",
                        "płynny"
                    ],
                    "common_phrases": [
                        "biegła znajomość",
                        "biegły w piśmie",
                        "biegły w sztuce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий большими знаниями, опытом и навыками в какой-либо области; выполняющий что-либо умело и профессионально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искусный",
                                "target_lang_sense_clarification": "умелый, мастерский"
                            },
                            {
                                "target_lang_word": "опытный",
                                "target_lang_sense_clarification": "обладающий опытом"
                            },
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "о владении языком"
                            },
                            {
                                "target_lang_word": "беглый",
                                "target_lang_sense_clarification": "о чтении или речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0ebda02-3968-336e-babd-0e0d500b5712",
                    "sense_definition": "Zaradny, sprytny i umiejący sobie radzić w trudnych sytuacjach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "skill_proficiency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To bardzo <biegła> osoba, na pewno szybko załatwi tę sprawę w urzędzie.",
                            "target_lang_translations": {
                                "ru": "Это очень <ловкая> особа, она наверняка быстро уладит это дело в ведомстве."
                            }
                        },
                        {
                            "text": "Jej <biegła> natura pozwoliła jej wyjść cało z wielu kłopotów.",
                            "target_lang_translations": {
                                "ru": "Её <изворотливая> натура позволила ей выйти сухой из многих передряг."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Находчивый, хитрый и умеющий справляться с трудными ситуациями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ловкий",
                                "target_lang_sense_clarification": "изворотливый, умеющий найти выход"
                            },
                            {
                                "target_lang_word": "проворный",
                                "target_lang_sense_clarification": "быстрый и ловкий в делах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8ee45d92-3cc3-3f30-98e4-088474f15691",
                    "sense_definition": "Specjalista posiadający fachową wiedzę w danej dziedzinie, powoływany przez sąd lub inny organ do wydania oficjalnej opinii.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_expert",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd postanowił powołać <biegłego> z zakresu medycyny sądowej.",
                            "target_lang_translations": {
                                "ru": "Суд постановил назначить <эксперта> в области судебной медицины."
                            }
                        },
                        {
                            "text": "Opinia wydana przez <biegłego> była kluczowa dla ostatecznego wyroku.",
                            "target_lang_translations": {
                                "ru": "Заключение, выданное <экспертом>, было ключевым для окончательного приговора."
                            }
                        }
                    ],
                    "synonyms": [
                        "ekspert",
                        "rzeczoznawca"
                    ],
                    "antonyms": [
                        "laik"
                    ],
                    "common_phrases": [
                        "biegły sądowy",
                        "opinia biegłego",
                        "powołać biegłego",
                        "zespół biegłych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист, обладающий профессиональными знаниями в данной области, назначаемый судом или иным органом для выдачи официального заключения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эксперт",
                                "target_lang_sense_clarification": "судебный или технический специалист"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "37e78ff3-95c6-3456-8042-1a393fc641da",
                    "sense_definition": "Forma czasu przeszłego (3. osoba liczby mnogiej, rodzaj niemęskoosobowy) czasownika <w>biec</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci szybko <biegły> do domu, gdy zaczęło mocno padać.",
                            "target_lang_translations": {
                                "ru": "Дети быстро <бежали> домой, когда начал лить сильный дождь."
                            }
                        },
                        {
                            "text": "Sarny przestraszyły się hałasu i <biegły> w stronę gęstego lasu.",
                            "target_lang_translations": {
                                "ru": "Косули испугались шума и <бежали> в сторону густого леса."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прошедшего времени (3-е лицо множественного числа, немужской род) глагола <w>biec</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бежали",
                                "target_lang_sense_clarification": "глагол бежать в прошедшем времени"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "laur",
        "laurowy",
        "Laura"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e87280d1-103a-37a6-b4d0-f3d169c67a07",
                    "sense_definition": "Ozdobna kartka z życzeniami lub rysunkiem, wykonywana własnoręcznie, najczęściej przez dzieci z okazji świąt rodzinnych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "card",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja córka zrobiła mi piękną <w>laurkę</w> z okazji moich urodzin.",
                            "target_lang_translations": {
                                "ru": "Моя дочь сделала мне красивую <w>открытку</w> по случаю моего дня рождения."
                            }
                        },
                        {
                            "text": "W przedszkolu wszystkie dzieci przygotowują kolorowe <w>laurki</w> dla swoich dziadków.",
                            "target_lang_translations": {
                                "ru": "В детском саду все дети готовят красочные <w>открытки</w> для своих дедушек и бабушек."
                            }
                        }
                    ],
                    "synonyms": [
                        "kartka"
                    ],
                    "common_phrases": [
                        "laurka dla mamy",
                        "zrobić laurkę",
                        "laurka na Dzień Babci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративная открытка с пожеланиями или рисунком, сделанная своими руками, чаще всего детьми по случаю семейных праздников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытка",
                                "target_lang_sense_clarification": "сделанная своими руками, обычно ребенком"
                            },
                            {
                                "target_lang_word": "самодельная открытка",
                                "target_lang_sense_clarification": "детская поделка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b276c237-9815-37da-8b8f-49d74f85d656",
                    "sense_definition": "Przesadnie pozytywna opinia, recenzja lub tekst na czyjś temat, często nieszczera i pozbawiona krytycyzmu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "praise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten artykuł w gazecie to zwykła <w>laurka</w> dla prezesa firmy, a nie rzetelne dziennikarstwo.",
                            "target_lang_translations": {
                                "ru": "Эта статья в газете — обычный <w>панегирик</w> президенту компании, а не добросовестная журналистика."
                            }
                        },
                        {
                            "text": "Dziennikarz wystawił politykowi <w>laurkę</w>, pomijając wszystkie jego wcześniejsze błędy.",
                            "target_lang_translations": {
                                "ru": "Журналист написал политику <w>хвалебную оду</w>, упустив все его предыдущие ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "panegiryk",
                        "pean",
                        "pochwała"
                    ],
                    "antonyms": [
                        "paszkwil",
                        "krytyka"
                    ],
                    "common_phrases": [
                        "wystawić laurkę",
                        "pisać laurkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезмерно позитивное мнение, рецензия или текст о ком-либо, часто неискренний и лишенный критики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "панегирик",
                                "target_lang_sense_clarification": "хвалебная речь или текст"
                            },
                            {
                                "target_lang_word": "хвалебная ода",
                                "target_lang_sense_clarification": "ироничное, переносное значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c51d38ab-b1a9-3fdb-bf19-9d510b24a391",
                    "sense_definition": "Zdrobnienie od imienia żeńskiego <w>Laura</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "name_diminutive",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mała <w>Laurka</w> bardzo lubi bawić się w piaskownicy ze swoimi koleżankami.",
                            "target_lang_translations": {
                                "ru": "Маленькая <w>Лаурочка</w> очень любит играть в песочнице со своими подружками."
                            }
                        },
                        {
                            "text": "Ciocia kupiła <w>Laurce</w> nową lalkę na urodziny.",
                            "target_lang_translations": {
                                "ru": "Тетя купила <w>Лаурочке</w> новую куклу на день рождения."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма женского имени Лаура."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лаурочка",
                                "target_lang_sense_clarification": "ласкательное имя"
                            },
                            {
                                "target_lang_word": "Лаурка",
                                "target_lang_sense_clarification": "разговорная форма"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "9d3112d7-e8b6-34c7-b054-8b5ce14ff644",
                    "sense_definition": "Een karakteristieke eigenschap of bijzonderheid waaraan men iets of iemand herkent.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn belangrijkste <w>kenmerk</w> was zijn liefde voor de natuur.",
                            "target_lang_translations": {
                                "ru": "Его главной <w>чертой</w> была любовь к природе.",
                                "en": "His most important <w>characteristic</w> was his love for nature."
                            }
                        },
                        {
                            "text": "Een opvallend <w>kenmerk</w> van deze vogel is de felblauwe staart.",
                            "target_lang_translations": {
                                "ru": "Бросающейся в глаза <w>особенностью</w> этой птицы является ярко-синий хвост.",
                                "en": "A striking <w>feature</w> of this bird is the bright blue tail."
                            }
                        }
                    ],
                    "synonyms": [
                        "eigenschap",
                        "karakteristiek",
                        "trek",
                        "bijzonderheid"
                    ],
                    "common_phrases": [
                        "opvallend kenmerk",
                        "belangrijk kenmerk",
                        "uiterlijk kenmerk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характерная черта или особенность, по которой узнают что-то или кого-то.",
                        "en": "A characteristic property or detail by which something or someone is recognized."
                    },
                    "translations": {

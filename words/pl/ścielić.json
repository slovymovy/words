{
    "word_family": [
        "pościel",
        "prześcieradło",
        "posłanie",
        "wyściółka",
        "pościelowy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e209815a-d9e5-33bc-ba12-74725e678899",
                    "sense_definition": "Układać pościel, poduszki i kołdrę na łóżku, aby je uporządkować lub przygotować do snu.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "bed_arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdego ranka muszę <ścielić> łóżko, zanim wyjdę do szkoły.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я должен <застилать> кровать перед уходом в школу."
                            }
                        },
                        {
                            "text": "Mama nauczyła mnie starannie <ścielić> łóżko, żeby pokój wyglądał schludnie.",
                            "target_lang_translations": {
                                "ru": "Мама научила меня аккуратно <застилать> кровать, чтобы комната выглядела опрятно."
                            }
                        }
                    ],
                    "synonyms": [
                        "słać"
                    ],
                    "common_phrases": [
                        "ścielić łóżko",
                        "ścielić do snu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскладывать постельное белье, подушки и одеяло на кровати, чтобы привести их в порядок или подготовить ко сну."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застилать",
                                "target_lang_sense_clarification": "приводить кровать в порядок"
                            },
                            {
                                "target_lang_word": "стелить",
                                "target_lang_sense_clarification": "готовить постель ко сну"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d1d35fb-0cc8-3582-9421-288fe36e6047",
                    "sense_definition": "Rozpościerać się płasko na dużej powierzchni lub unosić się nisko nad ziemią (często o mgle, dymie, cieniu lub krajobrazie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spreading_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gęsta mgła zaczęła <ścielić> się nad łąkami tuż po zachodzie słońca.",
                            "target_lang_translations": {
                                "ru": "Густой туман начал <стелиться> над лугами сразу после заката."
                            }
                        },
                        {
                            "text": "Długi cień starego dębu <ścielił> się na trawie w popołudniowym świetle.",
                            "target_lang_translations": {
                                "ru": "Длинная тень старого дуба <стелилась> по траве в послеобеденном свете."
                            }
                        }
                    ],
                    "synonyms": [
                        "słać się",
                        "rozpościerać się"
                    ],
                    "common_phrases": [
                        "mgła się ścieli",
                        "cień się ścieli",
                        "dym się ścieli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расстилаться плоско по большой поверхности или низко нависать над землей (часто о тумане, дыме, тени или пейзаже)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стелиться",
                                "target_lang_sense_clarification": "о тумане, дыме, тени"
                            },
                            {
                                "target_lang_word": "расстилаться",
                                "target_lang_sense_clarification": "покрывать пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "808da5a4-827f-3063-a5ea-cd9c7bef1ce1",
                    "sense_definition": "Rozkładać lub kłaść warstwę czegoś na podłożu, przykrywając powierzchnię.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "covering_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz poszedł do stajni, aby <ścielić> świeżą słomę dla koni.",
                            "target_lang_translations": {
                                "ru": "Хозяин пошел в конюшню, чтобы <стелить> свежую солому для лошадей."
                            }
                        },
                        {
                            "text": "Na powitanie ważnego gościa służba zaczęła <ścielić> czerwone dywany.",
                            "target_lang_translations": {
                                "ru": "Для приветствия важного гостя слуги начали <расстилать> красные ковры."
                            }
                        }
                    ],
                    "synonyms": [
                        "słać",
                        "rozpościerać się"
                    ],
                    "common_phrases": [
                        "ścielić słomę",
                        "ścielić dywan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскладывать или класть слой чего-либо на основание, покрывая поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стелить",
                                "target_lang_sense_clarification": "класть слой (солому, ткань)"
                            },
                            {
                                "target_lang_word": "расстилать",
                                "target_lang_sense_clarification": "разворачивать (ковер)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
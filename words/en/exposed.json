{
    "word_family": [
        "expose",
        "exposure",
        "exposition",
        "expository"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "16feeb80-348d-32a7-8bea-a615016b0d56",
                    "sense_definition": "Not covered, hidden, or protected, often making someone or something visible or vulnerable to harm.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers felt unsafe in their <w>exposed</w> position on the hill.",
                            "target_lang_translations": {
                                "ru": "Солдаты чувствовали себя небезопасно на своей <w>открытой</w> позиции на холме."
                            }
                        },
                        {
                            "text": "This plant will not survive in such an <w>exposed</w> area without shelter.",
                            "target_lang_translations": {
                                "ru": "Это растение не выживет на таком <w>открытом</w> участке без укрытия."
                            }
                        },
                        {
                            "text": "The apartment features high ceilings and <w>exposed</w> brick walls.",
                            "target_lang_translations": {
                                "ru": "В квартире высокие потолки и стены с <w>открытой</w> кирпичной кладкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "unprotected",
                        "vulnerable",
                        "uncovered",
                        "bare",
                        "open"
                    ],
                    "antonyms": [
                        "protected",
                        "sheltered",
                        "covered",
                        "hidden",
                        "concealed"
                    ],
                    "common_phrases": [
                        "exposed to",
                        "exposed brick",
                        "exposed skin",
                        "exposed wiring"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не укрытый, не спрятанный или не защищённый; часто видимый или уязвимый для повреждений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "незащищённый",
                                "target_lang_sense_clarification": "уязвимый для опасности или атаки"
                            },
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "доступный воздействию стихий или взгляду"
                            },
                            {
                                "target_lang_word": "обнажённый",
                                "target_lang_sense_clarification": "не покрытый одеждой или оболочкой (о проводах, теле)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "92964405-f66c-37af-bf41-e1dc07f0c390",
                    "sense_definition": "Simple past and past participle of <w>expose</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The investigation <w>exposed</w> the corruption within the organization.",
                            "target_lang_translations": {
                                "ru": "Расследование <w>вскрыло</w> коррупцию внутри организации."
                            }
                        },
                        {
                            "text": "The film was ruined because it was accidentally <w>exposed</w> to light.",
                            "target_lang_translations": {
                                "ru": "Плёнка была испорчена, потому что её случайно <w>подвергли</w> воздействию света."
                            }
                        }
                    ],
                    "common_phrases": [
                        "exposed to danger",
                        "exposed the truth"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола expose (раскрывать, подвергать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разоблачил",
                                "target_lang_sense_clarification": "раскрыл правду или преступление"
                            },
                            {
                                "target_lang_word": "подверг",
                                "target_lang_sense_clarification": "заставил испытать воздействие чего-либо"
                            },
                            {
                                "target_lang_word": "выставил",
                                "target_lang_sense_clarification": "поместил на видное место"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
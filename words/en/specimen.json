    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b54c31d8-26ca-3208-bfa7-1605a114b1a7",
                    "sense_definition": "An individual instance that represents a class; an example.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum displayed a rare <w>specimen</w> of the ancient beetle found in amber.",
                            "target_lang_translations": {
                                "ru": "В музее был выставлен редкий <w>экземпляр</w> древнего жука, найденного в янтаре."
                            }
                        },
                        {
                            "text": "This rock is a perfect <w>specimen</w> of volcanic basalt from the region.",
                            "target_lang_translations": {
                                "ru": "Этот камень — идеальный <w>образец</w> вулканического базальта из данного региона."
                            }
                        }
                    ],
                    "synonyms": [
                        "sample",
                        "individual"
                    ],
                    "common_phrases": [
                        "fine specimen",
                        "rare specimen",
                        "museum specimen",
                        "perfect specimen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный представитель класса или группы; наглядный пример."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экземпляр",
                                "target_lang_sense_clarification": "отдельный представитель вида или предмета"
                            },
                            {
                                "target_lang_word": "образец",
                                "target_lang_sense_clarification": "показательный пример"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70ca6635-9bd0-3ea1-a6e9-8871a0ae46d7",
                    "sense_definition": "A sample, especially one used for diagnostic analysis.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nurse asked the patient to provide a urine <w>specimen</w> for testing.",
                            "target_lang_translations": {
                                "ru": "Медсестра попросила пациента предоставить <w>образец</w> мочи для анализа."
                            }
                        },
                        {
                            "text": "They sent the tissue <w>specimen</w> to the laboratory to check for infection.",
                            "target_lang_translations": {
                                "ru": "Они отправили <w>образец</w> ткани в лабораторию, чтобы проверить наличие инфекции."
                            }
                        }
                    ],
                    "synonyms": [
                        "sample"
                    ],
                    "common_phrases": [
                        "urine specimen",
                        "blood specimen",
                        "stool specimen",
                        "collect a specimen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть материала, взятая для научного или медицинского анализа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "образец",
                                "target_lang_sense_clarification": "для исследования"
                            },
                            {
                                "target_lang_word": "проба",
                                "target_lang_sense_clarification": "материал для анализа"
                            },
                            {
                                "target_lang_word": "препарат",
                                "target_lang_sense_clarification": "подготовленный образец ткани"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f501845-415c-3fd1-b16f-424cb0eb0034",
                    "sense_definition": "An unpleasant or contemptible person.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was a truly pathetic <w>specimen</w> who blamed everyone else for his mistakes.",
                            "target_lang_translations": {
                                "ru": "Он был поистине жалким <w>экземпляром</w>, который винил всех остальных в своих ошибках."
                            }
                        },
                        {
                            "text": "I have never met such a rude <w>specimen</w> in all my years of working here.",
                            "target_lang_translations": {
                                "ru": "За все годы работы здесь я никогда не встречал такого грубого <w>экземпляра</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sorry specimen",
                        "pathetic specimen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used with adjectives like 'sorry' or 'pathetic'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприятный или странный человек (часто употребляется с иронией)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экземпляр",
                                "target_lang_sense_clarification": "иронично о человеке"
                            }
                        ]
                    }
                }
            ]
        }
    ]

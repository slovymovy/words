                },
                {
                    "sense_id": "89644254-3a98-388b-a32e-49d02e428ee5",
                    "sense_definition": "Een groep mensen of organisaties die invloed probeert uit te oefenen op politieke besluitvorming om bepaalde belangen te behartigen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De agrarische <w>lobby</w> heeft veel invloed op het nieuwe overheidsbeleid.",
                            "target_lang_translations": {
                                "en": "The agricultural <w>lobby</w> has a lot of influence on the new government policy.",
                                "ru": "Аграрное <w>лобби</w> имеет большое влияние на новую государственную политику."
                            }
                        },
                        {
                            "text": "Er is een sterke <w>lobby</w> actief om de wetgeving rondom privacy aan te passen.",
                            "target_lang_translations": {
                                "en": "There is a strong <w>lobby</w> active to adjust the legislation regarding privacy.",
                                "ru": "Действует сильное <w>лобби</w> за изменение законодательства о конфиденциальности."
                            }
                        }
                    ],
                    "synonyms": [
                        "belangengroep",
                        "pressiegroep"
                    ],
                    "common_phrases": [
                        "een sterke lobby",
                        "de politieke lobby"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people or organizations trying to influence political decision-making to serve specific interests.",
                        "ru": "Группа людей или организаций, пытающаяся влиять на принятие политических решений для защиты определенных интересов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lobby",
                                "target_lang_sense_clarification": "political influence group"
                            },
                            {
                                "target_lang_word": "pressure group",
                                "target_lang_sense_clarification": "advocacy group"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лобби",
                                "target_lang_sense_clarification": "группа давления"
                            }
                        ]
                    }
                }
            ]

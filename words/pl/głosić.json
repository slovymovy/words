    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "024535f7-49b2-382c-8810-45fc9345a698",
                    "sense_definition": "Publicznie oznajmiać coś ważnego, rozpowszechniać jakieś poglądy, idee lub hasła.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "public_speaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Misjonarze wyjechali do dalekiego kraju, aby <w>głosić</w> swoją wiarę.",
                            "target_lang_translations": {
                                "ru": "Миссионеры отправились в далекую страну, чтобы <w>проповедовать</w> свою веру."
                            }
                        },
                        {
                            "text": "Polityk ten lubi <w>głosić</w> kontrowersyjne poglądy w programach telewizyjnych.",
                            "target_lang_translations": {
                                "ru": "Этот политик любит <w>провозглашать</w> спорные взгляды в телевизионных программах."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogłaszać",
                        "obwieszczać",
                        "oznajmiać",
                        "rozgłaszać",
                        "upowszechniać",
                        "propagować"
                    ],
                    "common_phrases": [
                        "głosić poglądy",
                        "głosić ewangelię",
                        "głosić prawdę",
                        "głosić hasła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публично объявлять о чем-то важном, распространять взгляды, идеи или лозунги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проповедовать",
                                "target_lang_sense_clarification": "веру, идеи"
                            },
                            {
                                "target_lang_word": "провозглашать",
                                "target_lang_sense_clarification": "лозунги, истины"
                            },
                            {
                                "target_lang_word": "распространять",
                                "target_lang_sense_clarification": "взгляды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9699d880-926b-349b-ab1e-6508cdad42a6",
                    "sense_definition": "Zawierać określoną treść lub informację (o tekście, prawie, legendzie).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stara legenda <w>głosi</w>, że w podziemiach zamku ukryty jest skarb.",
                            "target_lang_translations": {
                                "ru": "Старая легенда <w>гласит</w>, что в подземельях замка спрятано сокровище."
                            }
                        },
                        {
                            "text": "Tabliczka na bramie <w>głosiła</w> zakaz wstępu na teren budowy.",
                            "target_lang_translations": {
                                "ru": "Табличка на воротах <w>гласила</w>, что вход на стройплощадку запрещен."
                            }
                        }
                    ],
                    "synonyms": [
                        "oznajmiać"
                    ],
                    "common_phrases": [
                        "jak głosi legenda",
                        "napis głosi",
                        "ustawa głosi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержать определенную информацию (о тексте, законе, легенде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гласить",
                                "target_lang_sense_clarification": "о законе, надписи, легенде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06abd018-92d6-32d3-9ced-a425c2662419",
                    "sense_definition": "Wygłaszać uroczyście mowę, przemówienie lub kazanie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "public_speaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ksiądz proboszcz co niedzielę <w>głosi</w> wiernym poruszające kazanie.",
                            "target_lang_translations": {
                                "ru": "Каждое воскресенье настоятель <w>произносит</w> верующим трогательную проповедь."
                            }
                        },
                        {
                            "text": "Z ambony kapłan <w>głosił</w> słowa otuchy dla wszystkich zgromadzonych.",
                            "target_lang_translations": {
                                "ru": "С амвона священник <w>возвещал</w> слова утешения всем собравшимся."
                            }
                        }
                    ],
                    "synonyms": [
                        "wygłaszać"
                    ],
                    "common_phrases": [
                        "głosić kazanie",
                        "głosić słowo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественно произносить речь или проповедь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "произносить",
                                "target_lang_sense_clarification": "речь, проповедь"
                            },
                            {
                                "target_lang_word": "читать",
                                "target_lang_sense_clarification": "проповедь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2069f45e-5473-3991-b23f-39fd2da956fb",
                    "sense_definition": "Szczekać w charakterystyczny sposób, dając sygnał o znalezieniu tropu (o psie myśliwskim).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myśliwy usłyszał, jak jego pies zaczął <w>głosić</w> w gęstwinie lasu.",
                            "target_lang_translations": {
                                "ru": "Охотник услышал, как его пес начал <w>подавать голос</w> в чаще леса."
                            }
                        },
                        {
                            "text": "Gdy ogary zaczęły <w>głosić</w>, wiedzieliśmy, że zwierzyna jest blisko.",
                            "target_lang_translations": {
                                "ru": "Когда гончие начали <w>подавать голос</w>, мы поняли, что дичь близко."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "język łowiecki"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лаять особым образом, подавая сигнал об обнаружении следа (об охотничьей собаке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подавать голос",
                                "target_lang_sense_clarification": "об охотничьих собаках"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "46feff58-5c43-31a9-81d0-520c1f4f2d8a",
                    "sense_definition": "Niekompletny, swobodny strój domowy lub bielizna, w których zazwyczaj nie pokazuje się publicznie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zastałem ją w kuchni w porannym <w>negliżu</w>, pijącą kawę.",
                            "target_lang_translations": {
                                "ru": "Я застал её на кухне в утреннем <w>неглиже</w>, пьющей кофе."
                            }
                        },
                        {
                            "text": "Aktorka wystąpiła na scenie w odważnym <w>negliżu</w>, co wywołało skandal.",
                            "target_lang_translations": {
                                "ru": "Актриса появилась на сцене в смелом <w>неглиже</w>, что вызвало скандал."
                            }
                        }
                    ],
                    "synonyms": [
                        "dezabil"
                    ],
                    "common_phrases": [
                        "w porannym negliżu",
                        "damski negliż"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лёгкая, свободная домашняя одежда или нижнее бельё, в которых обычно не появляются на людях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неглиже",
                                "target_lang_sense_clarification": "лёгкая домашняя одежда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cda006f0-6e15-382d-9b90-2de1dacce0ce",
                    "sense_definition": "Stan bycia rozebranym lub ubranym bardzo skąpo.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Artysta chciał uchwycić na obrazie całkowity <w>negliż</w> modelki.",
                            "target_lang_translations": {
                                "ru": "Художник хотел запечатлеть на картине полную <w>наготу</w> модели."
                            }
                        },
                        {
                            "text": "Jej przypadkowy <w>negliż</w> wywołał spore zamieszanie wśród gości.",
                            "target_lang_translations": {
                                "ru": "Её случайная <w>обнажённость</w> вызвала немалый переполох среди гостей."
                            }
                        }
                    ],
                    "synonyms": [

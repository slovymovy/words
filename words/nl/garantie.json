{
    "word_family": [
        "garanderen",
        "garant"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c660460-5a70-338a-a135-fbf0683d6778",
                    "sense_definition": "Een schriftelijke belofte van een fabrikant of verkoper om een product te repareren of te vervangen als het binnen een bepaalde periode kapotgaat",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "assurance_concepts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zit er nog <w>garantie</w> op deze nieuwe televisie?",
                            "target_lang_translations": {
                                "en": "Is this new television still under <w>warranty</w>?",
                                "ru": "На этот новый телевизор еще действует <w>гарантия</w>?"
                            }
                        },
                        {
                            "text": "De reparatie valt helaas niet meer onder de <w>garantie</w>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, the repair is no longer covered by the <w>warranty</w>.",
                                "ru": "К сожалению, ремонт больше не подпадает под <w>гарантию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "waarborg"
                    ],
                    "common_phrases": [
                        "onder garantie",
                        "garantie hebben",
                        "twee jaar garantie",
                        "fabrieksgarantie"
                    ],
                    "target_lang_definitions": {
                        "en": "A written promise from a manufacturer or seller to repair or replace a product if it breaks within a certain period",
                        "ru": "Письменное обещание производителя или продавца отремонтировать или заменить товар, если он сломается в течение определенного периода"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "warranty",
                                "target_lang_sense_clarification": "product coverage"
                            },
                            {
                                "target_lang_word": "guarantee",
                                "target_lang_sense_clarification": "product assurance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гарантия",
                                "target_lang_sense_clarification": "на товар"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa673b62-5bef-3897-a98b-a264aeab2cc3",
                    "sense_definition": "Een sterke belofte of zekerheid dat iets waar is of dat iets zeker zal gebeuren",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "assurance_concepts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gaf hem de <w>garantie</w> dat alle schade vergoed zou worden.",
                            "target_lang_translations": {
                                "en": "He gave him the <w>guarantee</w> that all damage would be reimbursed.",
                                "ru": "Он дал ему <w>гарантию</w>, что весь ущерб будет возмещен."
                            }
                        },
                        {
                            "text": "Er is geen enkele <w>garantie</w> dat het morgen mooi weer wordt.",
                            "target_lang_translations": {
                                "en": "There is no <w>guarantee</w> whatsoever that the weather will be nice tomorrow.",
                                "ru": "Нет никакой <w>гарантии</w>, что завтра будет хорошая погода."
                            }
                        }
                    ],
                    "synonyms": [
                        "zekerheid",
                        "toezegging",
                        "waarborg"
                    ],
                    "antonyms": [
                        "onzekerheid"
                    ],
                    "common_phrases": [
                        "garantie geven",
                        "geen garantie",
                        "garantie bieden"
                    ],
                    "target_lang_definitions": {
                        "en": "A strong promise or certainty that something is true or that something will definitely happen",
                        "ru": "Твердое обещание или уверенность в том, что что-то является правдой или обязательно произойдет"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "guarantee",
                                "target_lang_sense_clarification": "firm promise"
                            },
                            {
                                "target_lang_word": "assurance",
                                "target_lang_sense_clarification": "certainty"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гарантия",
                                "target_lang_sense_clarification": "обещание, уверенность"
                            },
                            {
                                "target_lang_word": "ручательство",
                                "target_lang_sense_clarification": "обеспечение обязательства"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
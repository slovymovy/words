            "senses": [
                {
                    "sense_id": "2e4de89c-8c70-3229-9e1a-f4b9684f0e41",
                    "sense_definition": "In a sitting position; not standing.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please remain <w>seated</w> until the bus comes to a complete stop.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, оставайтесь <w>на своих местах</w>, пока автобус полностью не остановится."
                            }
                        },
                        {
                            "text": "The audience was <w>seated</w> and waiting for the show to begin.",
                            "target_lang_translations": {
                                "ru": "Зрители <w>заняли места</w> и ждали начала шоу."
                            }
                        }
                    ],
                    "synonyms": [
                        "sitting"
                    ],
                    "antonyms": [
                        "standing"
                    ],
                    "common_phrases": [
                        "remain seated",
                        "please be seated"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в сидячем положении; не стоящий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидящий",
                                "target_lang_sense_clarification": "находящийся в положении сидя"
                            },
                            {
                                "target_lang_word": "сидя",
                                "target_lang_sense_clarification": "наречие, часто используется в фразах типа 'оставаться сидя'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83468747-f46d-3760-90d2-bbfcc599a901",
                    "sense_definition": "Firmly established or deeply rooted in one's nature or mind.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "fixed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has a deep-<w>seated</w> fear of flying that prevents him from traveling.",

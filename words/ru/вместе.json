    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2fd572a2-932e-395a-8ea3-a5ccabe20f21",
                    "sense_definition": "Совместно, объединившись друг с другом; в одном месте или в одной группе.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "jointly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы часто ходим в кино <w>вместе</w>.",
                            "target_lang_translations": {
                                "en": "We often go to the cinema <w>together</w>."
                            }
                        },
                        {
                            "text": "Они решили жить <w>вместе</w> в новой квартире.",
                            "target_lang_translations": {
                                "en": "They decided to live <w>together</w> in the new apartment."
                            }
                        }
                    ],
                    "synonyms": [
                        "сообща"
                    ],
                    "antonyms": [
                        "отдельно",
                        "раздельно",
                        "порознь"
                    ],
                    "common_phrases": [
                        "все вместе",
                        "жить вместе",
                        "собрать вместе",
                        "быть вместе"
                    ],
                    "target_lang_definitions": {
                        "en": "Jointly, united with each other; in one place or in one group."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "together",
                                "target_lang_sense_clarification": "in company or association"
                            },
                            {
                                "target_lang_word": "jointly",
                                "target_lang_sense_clarification": "in collaboration"
                            },
                            {
                                "target_lang_word": "collectively",
                                "target_lang_sense_clarification": "as a group or whole"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a77f0dc3-5d8f-3685-8e79-343c3db18697",
                    "sense_definition": "В одно и то же время, одновременно с чем-либо (часто используется в сочетании «вместе с тем»).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "simultaneously",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Работа была сложной, но <w>вместе</w> с тем очень интересной.",
                            "target_lang_translations": {
                                "en": "The work was difficult, but <w>at the same time</w> very interesting."
                            }
                        },
                        {
                            "text": "Он был строгим учителем, но <w>вместе</w> с тем справедливым человеком.",
                            "target_lang_translations": {
                                "en": "He was a strict teacher, but <w>at the same time</w> a fair person."
                            }
                        }
                    ],
                    "synonyms": [
                        "одновременно",
                        "в то же время"
                    ],
                    "common_phrases": [
                        "вместе с тем"
                    ],
                    "target_lang_definitions": {
                        "en": "At the same time, simultaneously with something (often used in the phrase 'at the same time')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at the same time",
                                "target_lang_sense_clarification": "simultaneously; however"
                            },
                            {
                                "target_lang_word": "simultaneously",
                                "target_lang_sense_clarification": "occurring at the same instant"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "he",
        "his",
        "himself"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "41858ddc-58c4-3b78-8b42-25577ff57ba2",
                    "sense_definition": "Refers to a male person or animal previously mentioned, functioning as the object of a verb or preposition.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "object_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I saw <w>him</w> at the library yesterday.",
                            "target_lang_translations": {
                                "ru": "Я видел <w>его</w> вчера в библиотеке."
                            }
                        },
                        {
                            "text": "Please give <w>him</w> the book when you see him.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, дай <w>ему</w> книгу, когда увидишь его."
                            }
                        },
                        {
                            "text": "She went to the cinema with <w>him</w>.",
                            "target_lang_translations": {
                                "ru": "Она пошла в кино с <w>ним</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tell him",
                        "give him",
                        "with him",
                        "for him",
                        "ask him"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относится к лицу мужского пола или животному, упомянутому ранее; выступает в роли дополнения глагола или предлога."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "его",
                                "target_lang_sense_clarification": "винительный падеж (кого?)"
                            },
                            {
                                "target_lang_word": "ему",
                                "target_lang_sense_clarification": "дательный падеж (кому?)"
                            },
                            {
                                "target_lang_word": "ним",
                                "target_lang_sense_clarification": "после предлогов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d523dc7a-989e-3613-a4be-8a9e7e7cc17c",
                    "sense_definition": "Used as a predicate after the verb 'to be', or informally as a subject.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "object_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I knocked on the door, but it was not <w>him</w>.",
                            "target_lang_translations": {
                                "ru": "Я постучал в дверь, но это был не <w>он</w>."
                            }
                        },
                        {
                            "text": "That is <w>him</w> in the photograph.",
                            "target_lang_translations": {
                                "ru": "Это <w>он</w> на фотографии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "it is him",
                        "that is him"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Standard in spoken English as a predicate; considered informal or non-standard when used as a subject (e.g., 'Him and I')."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как именная часть сказуемого после глагола 'to be' или неформально как подлежащее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "он",
                                "target_lang_sense_clarification": "в роли сказуемого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7148b75-6b8c-3e1e-9b85-f754ae01503a",
                    "sense_definition": "Refers to God or a deity, typically capitalized.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "object_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Believers pray to <w>Him</w> for guidance and strength.",
                            "target_lang_translations": {
                                "ru": "Верующие молятся <w>Ему</w> о наставлении и силе."
                            }
                        },
                        {
                            "text": "She felt that she could hear <w>Him</w> speaking to her heart.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала, что слышит <w>Его</w>, говорящего с её сердцем."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "capitalized"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относится к Богу или божеству, обычно пишется с заглавной буквы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Его",
                                "target_lang_sense_clarification": "о Боге"
                            },
                            {
                                "target_lang_word": "Ему",
                                "target_lang_sense_clarification": "о Боге"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "120bd08c-0ca7-3885-810f-d59950073ee9",
                    "sense_definition": "A person of elevated skill, dominance, or status in a specific activity.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He scored the winning goal because he is <w>him</w>.",
                            "target_lang_translations": {
                                "ru": "Он забил победный гол, потому что он — <w>тот самый</w>."
                            }
                        },
                        {
                            "text": "Stop trying to be <w>him</w> because you are not on that level yet.",
                            "target_lang_translations": {
                                "ru": "Перестань пытаться быть <w>им</w>, потому что ты ещё не на том уровне."
                            }
                        }
                    ],
                    "synonyms": [
                        "that guy"
                    ],
                    "common_phrases": [
                        "he is him",
                        "I am him"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Used to assert someone is 'the man' or the best."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек выдающегося мастерства, доминирования или статуса в определенной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тот самый",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "лучший",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "778a00aa-0ef4-3ac1-bce6-10baabc1aaec",
                    "sense_definition": "Used reflexively to mean 'himself'.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king arrayed <w>him</w> in royal apparel for the ceremony.",
                            "target_lang_translations": {
                                "ru": "Король облачил <w>себя</w> в королевские одежды для церемонии."
                            }
                        },
                        {
                            "text": "He sat <w>him</w> down by the fire to rest.",
                            "target_lang_translations": {
                                "ru": "Он усадил <w>себя</w> у огня, чтобы отдохнуть."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в возвратном значении, означая 'себя'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ecc3e64a-b98c-3917-9e82-e22ba925a65b",
                    "sense_definition": "A male person or animal.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_gender",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I think this bird is a <w>him</w>, but it might be a female.",
                            "target_lang_translations": {
                                "ru": "Я думаю, что эта птица — <w>самец</w>, но это может быть и самка."
                            }
                        },
                        {
                            "text": "The doctor told us the baby is a <w>him</w>.",
                            "target_lang_translations": {
                                "ru": "Врач сказал нам, что ребёнок — <w>мальчик</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "he"
                    ],
                    "common_phrases": [
                        "a him or a her"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо мужского пола или самец животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самец",
                                "target_lang_sense_clarification": "о животных"
                            },
                            {
                                "target_lang_word": "мальчик",
                                "target_lang_sense_clarification": "о людях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "b461a424-f2e5-354e-a75f-15c5b1da15d4",
                    "sense_definition": "A surname of Khmer origin.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Him</w> arrived at the conference early.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Хим</w> прибыл на конференцию рано."
                            }
                        },
                        {
                            "text": "We are looking for a record of the <w>Him</w> family in this region.",
                            "target_lang_translations": {
                                "ru": "Мы ищем записи о семье <w>Хим</w> в этом регионе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия кхмерского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хим",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
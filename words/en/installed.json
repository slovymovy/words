    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e768121d-e34f-3ebd-92b3-5fe1af62a123",
                    "sense_definition": "The simple past and past participle of <w>install</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>installed</w> the new heating system yesterday.",
                            "target_lang_translations": {
                                "ru": "Мы вчера <w>установили</w> новую систему отопления."
                            }
                        },
                        {
                            "text": "The software was <w>installed</w> on all office computers.",
                            "target_lang_translations": {
                                "ru": "Программное обеспечение было <w>установлено</w> на всех офисных компьютерах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола install."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "установил",
                                "target_lang_sense_clarification": "прошедшее время"
                            },
                            {
                                "target_lang_word": "установленный",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3407a4ee-32d0-368d-ab86-0c2bfeedd81d",
                    "sense_definition": "To have set up a piece of equipment, software, or machinery so that it is ready to be used.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>installed</w> the alarm to improve security.",
                            "target_lang_translations": {
                                "ru": "Они <w>установили</w> сигнализацию для повышения безопасности."
                            }
                        },
                        {
                            "text": "The technician <w>installed</w> the internet router in the living room.",
                            "target_lang_translations": {
                                "ru": "Техник <w>установил</w> интернет-роутер в гостиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "set up",
                        "put in"
                    ],
                    "antonyms": [
                        "removed"
                    ],
                    "common_phrases": [
                        "pre-installed",
                        "easily installed",
                        "newly installed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подготовить к работе оборудование, программное обеспечение или механизмы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "установил",
                                "target_lang_sense_clarification": "оборудование или ПО"
                            },
                            {
                                "target_lang_word": "инсталлировал",
                                "target_lang_sense_clarification": "программное обеспечение"
                            },
                            {
                                "target_lang_word": "смонтировал",
                                "target_lang_sense_clarification": "технические системы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbfcfdb5-8b35-3940-b5bf-28f285d64f70",
                    "sense_definition": "To have placed someone in an official position of authority, usually with a ceremony.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A new chancellor was <w>installed</w> at the university last week.",
                            "target_lang_translations": {
                                "ru": "На прошлой неделе в университете был <w>введен в должность</w> новый ректор."
                            }
                        },
                        {
                            "text": "She was formally <w>installed</w> as the chairperson of the board.",
                            "target_lang_translations": {
                                "ru": "Она была официально <w>введена в должность</w> председателя совета директоров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "formally installed",
                        "installed in office"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально ввести кого-либо в должность, обычно в ходе торжественной церемонии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ввел в должность",
                                "target_lang_sense_clarification": "официально"
                            },
                            {
                                "target_lang_word": "назначил",
                                "target_lang_sense_clarification": "на пост"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89f79bdb-35e0-3f17-a0e2-55ade005a16f",
                    "sense_definition": "To have placed or settled someone or something in a specific place or condition.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was <w>installed</w> at the watch tower for his shift.",
                            "target_lang_translations": {
                                "ru": "Его <w>поставили</w> на дежурство на смотровой вышке."
                            }
                        },
                        {
                            "text": "The cat <w>installed</w> itself comfortably on the sofa.",
                            "target_lang_translations": {
                                "ru": "Кошка удобно <w>устроилась</w> на диване."
                            }
                        }
                    ],
                    "synonyms": [
                        "established",
                        "set up"
                    ],
                    "common_phrases": [
                        "comfortably installed",
                        "installed at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разместить или устроить кого-либо или что-либо в определенном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устроился",
                                "target_lang_sense_clarification": "занять место (о себе)"
                            },
                            {
                                "target_lang_word": "разместил",
                                "target_lang_sense_clarification": "поместить кого-то"
                            },
                            {
                                "target_lang_word": "расположился",
                                "target_lang_sense_clarification": "усесться или улечься"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "collection",
        "collector",
        "collective",
        "collectively",
        "collectible"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1214f643-11a9-3bf4-8c73-5667fd226dce",
                    "sense_definition": "To seek and acquire items of a particular kind as a hobby or interest.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother likes to <w>collect</w> rare stamps from different countries.",
                            "target_lang_translations": {
                                "ru": "Мой брат любит <w>коллекционировать</w> редкие марки из разных стран."
                            }
                        },
                        {
                            "text": "She has <w>collected</w> coins since she was a little girl.",
                            "target_lang_translations": {
                                "ru": "Она <w>собирает</w> монеты с самого детства."
                            }
                        }
                    ],
                    "synonyms": [
                        "accumulate",
                        "amound"
                    ],
                    "common_phrases": [
                        "collect stamps",
                        "collect coins",
                        "collect art"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искать и приобретать предметы определенного типа в качестве хобби или интереса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коллекционировать",
                                "target_lang_sense_clarification": "систематически собирать что-либо (марки, монеты)"
                            },
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "общий термин для накопления предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54cfc506-e02b-321c-bac9-28ca3fa03cd6",
                    "sense_definition": "To bring things or information together from different places; to amass.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher asked us to <w>collect</w> our books and put them in the bag.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил нас <w>собрать</w> книги и положить их в сумку."
                            }
                        },
                        {
                            "text": "Scientists use special tools to <w>collect</w> data about the weather.",
                            "target_lang_translations": {
                                "ru": "Ученые используют специальные инструменты, чтобы <w>собирать</w> данные о погоде."
                            }
                        }
                    ],
                    "synonyms": [
                        "gather",
                        "assemble",
                        "accumulate",
                        "aggregate",
                        "amound"
                    ],
                    "common_phrases": [
                        "collect data",
                        "collect information",
                        "collect evidence",
                        "collect wood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединять вещи или информацию из разных мест; накапливать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "сводить воедино вещи или данные"
                            },
                            {
                                "target_lang_word": "накапливать",
                                "target_lang_sense_clarification": "постепенно собирать большое количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c30933e0-d94a-353a-9e05-90993e0d4e89",
                    "sense_definition": "To go to a place and bring someone or something away from it.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fetch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>collect</w> me from the train station at five o'clock?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>забрать</w> меня с вокзала в пять часов?"
                            }
                        },
                        {
                            "text": "I need to go to the post office to <w>collect</w> a parcel.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти на почту, чтобы <w>забрать</w> посылку."
                            }
                        }
                    ],
                    "synonyms": [
                        "gather up",
                        "round up"
                    ],
                    "common_phrases": [
                        "collect the kids",
                        "collect a parcel",
                        "collect luggage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прийти или приехать куда-либо, чтобы забрать кого-то или что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забирать",
                                "target_lang_sense_clarification": "брать с собой при уходе"
                            },
                            {
                                "target_lang_word": "встречать",
                                "target_lang_sense_clarification": "забирать кого-то с вокзала или аэропорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b502f4b4-f003-3177-a71a-10bdb3d859d1",
                    "sense_definition": "To ask for and receive money that is owed.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The landlord comes every month to <w>collect</w> the rent.",
                            "target_lang_translations": {
                                "ru": "Хозяин квартиры приходит каждый месяц, чтобы <w>забрать</w> арендную плату."
                            }
                        },
                        {
                            "text": "The government <w>collects</w> taxes to pay for public services.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>собирает</w> налоги для оплаты государственных услуг."
                            }
                        }
                    ],
                    "synonyms": [
                        "receive",
                        "secure"
                    ],
                    "common_phrases": [
                        "collect taxes",
                        "collect rent",
                        "collect debt",
                        "collect payment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требовать и получать причитающиеся деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "получать деньги (арендную плату, взносы)"
                            },
                            {
                                "target_lang_word": "взимать",
                                "target_lang_sense_clarification": "официально брать плату или налоги"
                            },
                            {
                                "target_lang_word": "инкассировать",
                                "target_lang_sense_clarification": "банковский термин: получать деньги по документам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f13b8553-9c23-320c-8ec1-b7551e9bb83c",
                    "sense_definition": "To come together to form a group or mass.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the storm, water <w>collected</w> in large puddles on the road.",
                            "target_lang_translations": {
                                "ru": "После шторма вода <w>скопилась</w> в большие лужи на дороге."
                            }
                        },
                        {
                            "text": "A crowd began to <w>collect</w> outside the theater before the show.",
                            "target_lang_translations": {
                                "ru": "Толпа начала <w>собираться</w> у театра перед представлением."
                            }
                        }
                    ],
                    "synonyms": [
                        "gather",
                        "accumulate",
                        "assemble",
                        "coalesce",
                        "group",
                        "mass"
                    ],
                    "common_phrases": [
                        "dust collects",
                        "water collects",
                        "crowd collected"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединяться, образуя группу или массу; скапливаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скапливаться",
                                "target_lang_sense_clarification": "о воде, пыли или предметах"
                            },
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "о людях или группах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03df7367-9c14-3c38-93a0-9c16bd27dec7",
                    "sense_definition": "To collide with or crash into another vehicle or obstacle.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "collision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The truck slid on the ice and <w>collected</w> a parked car.",
                            "target_lang_translations": {
                                "ru": "Грузовик занесло на льду, и он <w>снес</w> припаркованную машину."
                            }
                        },
                        {
                            "text": "He lost control of his bike and <w>collected</w> a fence.",
                            "target_lang_translations": {
                                "ru": "Он потерял управление велосипедом и <w>влетел в</w> забор."
                            }
                        }
                    ],
                    "synonyms": [
                        "bump into",
                        "plough into",
                        "run into"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used to describe a collision"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Столкнуться или врезаться в другое транспортное средство или препятствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снести",
                                "target_lang_sense_clarification": "разг. сбить или врезаться с силой"
                            },
                            {
                                "target_lang_word": "врезаться",
                                "target_lang_sense_clarification": "столкнуться с объектом"
                            },
                            {
                                "target_lang_word": "зацепить",
                                "target_lang_sense_clarification": "задеть при движении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8d13845-4a47-305d-a194-01311bb43ff5",
                    "sense_definition": "To form a conclusion; to deduce or infer.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "inference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "From his silence, I <w>collected</w> that he was not pleased with the idea.",
                            "target_lang_translations": {
                                "ru": "По его молчанию я <w>заключил</w>, что он не в восторге от этой идеи."
                            }
                        },
                        {
                            "text": "I <w>collect</w> from your letter that you intend to visit us soon.",
                            "target_lang_translations": {
                                "ru": "Я <w>делаю вывод</w> из вашего письма, что вы намереваетесь скоро нас посетить."
                            }
                        }
                    ],
                    "synonyms": [
                        "assume",
                        "construe"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly found in older literature"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать вывод; умозаключать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заключать",
                                "target_lang_sense_clarification": "делать вывод из фактов"
                            },
                            {
                                "target_lang_word": "делать вывод",
                                "target_lang_sense_clarification": "приходить к умозаключению"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b1e33389-deaa-3866-bd02-a92980e66ad2",
                    "sense_definition": "With the requirement that the recipient pays the charges.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "payment_mode",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I had no money, so I had to call my parents <w>collect</w>.",
                            "target_lang_translations": {
                                "ru": "У меня не было денег, поэтому мне пришлось звонить родителям <w>за их счёт</w>."
                            }
                        },
                        {
                            "text": "The package was sent <w>collect</w> because the sender refused to pay.",
                            "target_lang_translations": {
                                "ru": "Посылка была отправлена <w>наложенным платежом</w>, так как отправитель отказался платить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "call collect"
                    ],
                    "target_lang_definitions": {
                        "ru": "С условием оплаты получателем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за счёт получателя",
                                "target_lang_sense_clarification": "об оплате звонка или посылки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5ea26053-589a-3120-ae50-7e3e6bca798a",
                    "sense_definition": "Requiring payment by the recipient rather than the sender.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "payment_mode",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He made a <w>collect</w> call to his lawyer from the station.",
                            "target_lang_translations": {
                                "ru": "Он сделал звонок <w>за счёт абонента</w> своему адвокату из участка."
                            }
                        },
                        {
                            "text": "It was a <w>collect</w> shipment, so we had to pay upon arrival.",
                            "target_lang_translations": {
                                "ru": "Это была посылка <w>с оплатой при получении</w>, поэтому нам пришлось заплатить по прибытии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "collect call"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требующий оплаты получателем, а не отправителем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за счёт абонента",
                                "target_lang_sense_clarification": "о телефонном звонке"
                            },
                            {
                                "target_lang_word": "наложенным платежом",
                                "target_lang_sense_clarification": "о почтовом отправлении"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2cc9f8bd-c231-360c-bb9e-bad58e75a089",
                    "sense_definition": "A short prayer said before the reading of the epistle in certain church services.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "prayer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The priest read the <w>collect</w> for the day before starting his sermon.",
                            "target_lang_translations": {
                                "ru": "Священник прочитал <w>коллекту</w> дня перед началом проповеди."
                            }
                        },
                        {
                            "text": "This ancient <w>collect</w> is still used in morning services.",
                            "target_lang_translations": {
                                "ru": "Эта древняя <w>краткая молитва</w> до сих пор используется на утренних службах."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткая молитва, читаемая перед чтением апостольского послания в некоторых церковных службах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коллекта",
                                "target_lang_sense_clarification": "литургический термин"
                            },
                            {
                                "target_lang_word": "краткая молитва",
                                "target_lang_sense_clarification": "общее описание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "fc53651a-9847-35e3-b3fe-206f8a591b25",
                    "sense_definition": "Rzeczywisty, materialny, dostępny dla zmysłów",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reality_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja zabezpieczyła ślady <w>fizyczne</w> na miejscu przestępstwa.",

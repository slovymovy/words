    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3a5421fc-1f33-32a9-89db-6008a6036b97",
                    "sense_definition": "Удалить что-либо с поверхности с помощью трения (тряпкой, губкой или ластиком).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель попросил дежурного <стереть> мел с доски после урока.",
                            "target_lang_translations": {
                                "en": "The teacher asked the student on duty to <wipe> the chalk off the board after the lesson."
                            }
                        },
                        {
                            "text": "Мне нужно <стереть> пыль с полок перед приходом гостей.",
                            "target_lang_translations": {
                                "en": "I need to <wipe> the dust off the shelves before the guests arrive."
                            }
                        }
                    ],
                    "synonyms": [
                        "вытереть"
                    ],
                    "antonyms": [
                        "нанести"
                    ],
                    "common_phrases": [
                        "стереть с доски",
                        "стереть пыль",
                        "стереть пятно",
                        "стереть ластиком"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove something from a surface using friction (with a cloth, sponge, or eraser)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wipe",
                                "target_lang_sense_clarification": "to clean or remove by rubbing with a cloth or hand"
                            },
                            {
                                "target_lang_word": "erase",
                                "target_lang_sense_clarification": "to remove marks or writing, typically with an eraser"
                            },
                            {
                                "target_lang_word": "rub off",
                                "target_lang_sense_clarification": "to remove something by the action of rubbing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a68f131-c1a2-3e3f-b209-54020eb4e052",
                    "sense_definition": "Удалить информацию, файлы или воспоминания (из компьютера, телефона или памяти).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я случайно нажал кнопку и смог <стереть> важный документ.",
                            "target_lang_translations": {
                                "en": "I accidentally pressed a button and managed to <delete> a important document."
                            }
                        },
                        {
                            "text": "Он хотел бы <стереть> этот неприятный день из своей памяти.",
                            "target_lang_translations": {
                                "en": "He would like to <erase> this unpleasant day from his memory."
                            }
                        }
                    ],
                    "antonyms": [
                        "восстановить",
                        "вернуть"
                    ],
                    "common_phrases": [
                        "стереть файлы",
                        "стереть из памяти",
                        "стереть сообщение",
                        "стереть программу"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove information, files, or memories (from a computer, phone, or mind)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "erase",
                                "target_lang_sense_clarification": "to remove recorded data or memories"
                            },
                            {
                                "target_lang_word": "delete",
                                "target_lang_sense_clarification": "to remove digital files or records"
                            },
                            {
                                "target_lang_word": "wipe",
                                "target_lang_sense_clarification": "to completely clear data from a device"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ae6fa87-491f-3fde-b3ec-a439af0bd6a1",
                    "sense_definition": "Повредить кожу до раны или мозоли в результате длительного трения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В новых туфлях я умудрилась <стереть> ноги в кровь за один час.",
                            "target_lang_translations": {
                                "en": "In my new shoes, I managed to <rub> my feet raw in just one hour."
                            }
                        },
                        {
                            "text": "Рабочий боялся <стереть> руки, поэтому надел плотные перчатки.",
                            "target_lang_translations": {
                                "en": "The worker was afraid of <chafing> his hands, so he put on thick gloves."
                            }
                        }
                    ],
                    "synonyms": [
                        "натереть"
                    ],
                    "antonyms": [
                        "вылечить"
                    ],
                    "common_phrases": [
                        "стереть ноги",
                        "стереть в кровь",
                        "стереть кожу",
                        "стереть пятку"
                    ],
                    "target_lang_definitions": {
                        "en": "To damage the skin causing a wound or blister due to prolonged friction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rub raw",
                                "target_lang_sense_clarification": "to chafe the skin until it is sore or bleeding"
                            },
                            {
                                "target_lang_word": "chafe",
                                "target_lang_sense_clarification": "to make skin sore by rubbing against it"
                            },
                            {
                                "target_lang_word": "blister",
                                "target_lang_sense_clarification": "to cause a blister through friction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31b76540-732e-3574-9f9d-222d15baaebf",
                    "sense_definition": "Разрушить или измельчить что-либо до состояния мелких частиц или порошка; часто используется в переносном смысле как угроза полного уничтожения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мощные жернова мельницы могут <стереть> зерно в мягкую муку.",
                            "target_lang_translations": {
                                "en": "The powerful millstones can <grind> the grain into soft flour."
                            }
                        },
                        {
                            "text": "Разъяренный герой пообещал <стереть> своих врагов в порошок.",
                            "target_lang_translations": {
                                "en": "The furious hero promised to <grind> his enemies into dust."
                            }
                        }
                    ],
                    "synonyms": [
                        "истереть"
                    ],
                    "common_phrases": [
                        "стереть в порошок",
                        "стереть в пыль",
                        "стереть с лица земли"
                    ],
                    "target_lang_definitions": {
                        "en": "To destroy or grind something into small particles; often used figuratively as a threat of complete destruction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grind",
                                "target_lang_sense_clarification": "to crush into powder or dust"
                            },
                            {
                                "target_lang_word": "pulverize",
                                "target_lang_sense_clarification": "to reduce to fine particles or destroy completely"
                            },
                            {
                                "target_lang_word": "wipe out",
                                "target_lang_sense_clarification": "figurative: to destroy enemies completely"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
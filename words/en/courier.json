    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "25b59949-df88-3706-8de3-debc6f9a9558",
                    "sense_definition": "A person employed to deliver messages, packages, or important documents.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "delivery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bike <w>courier</w> managed to deliver the urgent parcel through heavy traffic.",
                            "target_lang_translations": {
                                "ru": "Велосипедный <w>курьер</w> сумел доставить срочную посылку сквозь плотный поток машин."
                            }
                        },
                        {
                            "text": "A diplomatic <w>courier</w> carries official government documents across international borders.",
                            "target_lang_translations": {
                                "ru": "Дипломатический <w>курьер</w> перевозит официальные правительственные документы через международные границы."
                            }
                        }
                    ],
                    "synonyms": [
                        "messenger"
                    ],
                    "common_phrases": [
                        "bike courier",
                        "motorcycle courier",
                        "diplomatic courier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, нанятый для доставки сообщений, посылок или важных документов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курьер",
                                "target_lang_sense_clarification": "доставщик"
                            },
                            {
                                "target_lang_word": "посыльный",
                                "target_lang_sense_clarification": "разносчик сообщений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e1dfc01-9afe-3116-844a-e01055b26429",
                    "sense_definition": "A company that transports commercial goods or mail.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "delivery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hired a private <w>courier</w> to ensure the package arrived the next day.",
                            "target_lang_translations": {
                                "ru": "Мы наняли частную <w>курьерскую службу</w>, чтобы посылка прибыла на следующий день."
                            }
                        },
                        {
                            "text": "The <w>courier</w> service offers online tracking for all international shipments.",
                            "target_lang_translations": {
                                "ru": "<w>Курьерская</w> служба предлагает онлайн-отслеживание для всех международных отправлений."
                            }
                        }
                    ],
                    "common_phrases": [
                        "courier service",
                        "courier company",
                        "by courier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компания, которая перевозит коммерческие грузы или почту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курьерская служба",
                                "target_lang_sense_clarification": "компания"
                            },
                            {
                                "target_lang_word": "служба доставки",
                                "target_lang_sense_clarification": "сервис"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "effd5e58-2cdb-3766-a826-b5b6dda5c9ce",
                    "sense_definition": "A person who looks after and guides tourists during a trip.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The holiday <w>courier</w> met the group at the airport to organize their transfer to the hotel.",
                            "target_lang_translations": {
                                "ru": "Туристический <w>гид</w> встретил группу в аэропорту, чтобы организовать их трансфер в отель."
                            }
                        },
                        {
                            "text": "She worked as a <w>courier</w> for a travel agency, assisting guests at the resort.",
                            "target_lang_translations": {
                                "ru": "Она работала <w>сопровождающей</w> в туристическом агентстве, помогая гостям на курорте."
                            }
                        }
                    ],
                    "synonyms": [
                        "guide",
                        "tourist guide",
                        "rep"
                    ],
                    "common_phrases": [
                        "travel courier"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "largely replaced by 'rep' or 'tour guide'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который присматривает за туристами и сопровождает их во время поездки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гид",
                                "target_lang_sense_clarification": "экскурсовод"
                            },
                            {
                                "target_lang_word": "сопровождающий",
                                "target_lang_sense_clarification": "турлидер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12f07518-f161-3d66-b771-7b630a2b8437",
                    "sense_definition": "A user who transfers unauthorized software between servers in the warez scene.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>courier</w> uploaded the cracked software to the topsite within minutes of its release.",
                            "target_lang_translations": {
                                "ru": "<w>Курьер</w> загрузил взломанное ПО на топ-сайт через несколько минут после его релиза."
                            }
                        },
                        {
                            "text": "Site administrators rely on a fast <w>courier</w> to keep their content updated.",
                            "target_lang_translations": {
                                "ru": "Администраторы сайта полагаются на быстрого <w>курьера</w> для обновления контента."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet/warez subculture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пользователь, который передает неавторизованное ПО между серверами в варез-сцене."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курьер",
                                "target_lang_sense_clarification": "сленг, варез-сцена"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5b697250-86bc-3d30-8ad9-bdb10c4904d4",
                    "sense_definition": "To send or deliver something using a courier service.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>courier</w> the signed contracts to your office this afternoon.",
                            "target_lang_translations": {
                                "ru": "Я <w>отправлю курьером</w> подписанные контракты в ваш офис сегодня днем."
                            }
                        },
                        {
                            "text": "They had the replacement parts <w>couriered</w> directly to the factory to save time.",
                            "target_lang_translations": {
                                "ru": "Они распорядились <w>доставить</w> запчасти <w>курьером</w> прямо на завод, чтобы сэкономить время."
                            }
                        }
                    ],
                    "common_phrases": [
                        "courier a package",
                        "courier documents"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправить или доставить что-либо с помощью курьерской службы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправить курьером",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "доставить курьером",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0a664b54-bcbb-3f22-a819-f65dd1b04fff",
                    "sense_definition": "The natural world or ecosystem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "nature_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We must do more to protect the <w>environment</w> from pollution.",
                            "target_lang_translations": {
                                "ru": "Мы должны делать больше для защиты <w>окружающей среды</w> от загрязнения."
                            }
                        },
                        {
                            "text": "Plastic waste is very harmful to the natural <w>environment</w>.",
                            "target_lang_translations": {
                                "ru": "Пластиковые отходы очень вредны для природной <w>среды</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ecosystem"
                    ],
                    "common_phrases": [
                        "protect the environment",
                        "natural environment",
                        "harm the environment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Природный мир, природа или экосистема."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окружающая среда",
                                "target_lang_sense_clarification": "природа, экология"
                            },
                            {
                                "target_lang_word": "экосистема",
                                "target_lang_sense_clarification": "биологическая система"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54be233a-fa8e-3cce-904e-8f9ca746d3c9",
                    "sense_definition": "The surroundings of, and influences on, a particular item of interest.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "conditions_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A noisy office is not a good working <w>environment</w>.",
                            "target_lang_translations": {
                                "ru": "Шумный офис — это не лучшая рабочая <w>обстановка</w>."
                            }
                        },
                        {
                            "text": "The school tries to create a safe <w>environment</w> for all students.",
                            "target_lang_translations": {
                                "ru": "Школа старается создать безопасную <w>среду</w> для всех учеников."
                            }
                        }
                    ],
                    "synonyms": [
                        "surrounding"
                    ],
                    "common_phrases": [
                        "working environment",
                        "safe environment",
                        "home environment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окружение и условия, в которых находится кто-либо или что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обстановка",
                                "target_lang_sense_clarification": "атмосфера, условия"
                            },
                            {
                                "target_lang_word": "среда",
                                "target_lang_sense_clarification": "условия существования"
                            },
                            {
                                "target_lang_word": "окружение",
                                "target_lang_sense_clarification": "то, что находится вокруг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bbd9242-43d1-36a4-8d04-4d4a557f73f2",
                    "sense_definition": "A particular political or social setting, arena or condition.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conditions_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The current political <w>environment</w> is very unstable.",
                            "target_lang_translations": {

                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disbursements",
                                "target_lang_sense_clarification": "legal expenses paid out"
                            },
                            {
                                "target_lang_word": "advances",
                                "target_lang_sense_clarification": "money paid beforehand"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "006e5c44-e30b-3329-8aca-32e74002c027",
                    "sense_definition": "De opening in een molen of gemaal waardoor het water naar buiten wordt gestuwd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij hoogwater stroomt het water met veel geweld door het <w>uitschot</w>.",
                            "target_lang_translations": {
                                "en": "At high water, the water flows with great force through the <w>outlet</w>."
                            }
                        },
                        {
                            "text": "De oude poldermolen heeft een houten <w>uitschot</w> dat aan vervanging toe is.",
                            "target_lang_translations": {
                                "en": "The old polder mill has a wooden <w>outlet</w> that needs replacing."
                            }
                        }
                    ],
                    "target_lang_definitions": {

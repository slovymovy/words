                {
                    "sense_id": "58fe10af-6d4d-3c90-a64a-cdaa8ab9a369",
                    "sense_definition": "Voor dat tijdstip; in de tijd die voorafging aan wat genoemd is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sinds kort woon ik in Utrecht, maar <w>daarvoor</w> woonde ik jarenlang in Amsterdam.",
                            "target_lang_translations": {
                                "ru": "С недавних пор я живу в Утрехте, но <w>до этого</w> я много лет жил в Амстердаме."
                            }
                        },
                        {
                            "text": "Het feest begint om acht uur, maar kort <w>daarvoor</w> gaan we nog even wat eten.",
                            "target_lang_translations": {
                                "ru": "Вечеринка начинается в восемь часов, но незадолго <w>до этого</w> мы перекусим."
                            }
                        }
                    ],
                    "synonyms": [
                        "eerder",
                        "tevoren",
                        "voorheen",
                        "vroeger"
                    ],
                    "antonyms": [
                        "daarna",
                        "later",
                        "vervolgens"
                    ],
                    "common_phrases": [
                        "kort daarvoor",
                        "lang daarvoor",
                        "de dag daarvoor",
                        "jaren daarvoor"
                    ],
                    "target_lang_definitions": {
                        "ru": "До этого момента; в то время, которое предшествовало упомянутому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до этого",
                                "target_lang_sense_clarification": "временное значение"
                            },
                            {
                                "target_lang_word": "раньше",
                                "target_lang_sense_clarification": "ранее"
                            },
                            {
                                "target_lang_word": "перед этим",
                                "target_lang_sense_clarification": "незадолго до"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ada380e4-6504-385c-986a-45f6bcb3b346",
                    "sense_definition": "Ten behoeve van dat doel of die zaak; om die reden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft een ernstige fout gemaakt en <w>daarvoor</w> krijgt hij nu straf.",
                            "target_lang_translations": {
                                "ru": "Он совершил серьезную ошибку, и <w>за это</w> его теперь наказывают."
                            }
                        },
                        {
                            "text": "Als je hulp nodig hebt bij het verhuizen, kan ik <w>daarvoor</w> wel zorgen.",
                            "target_lang_translations": {
                                "ru": "Если тебе нужна помощь с переездом, я могу <w>об этом</w> позаботиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "daartoe"
                    ],
                    "common_phrases": [
                        "daarvoor zorgen",
                        "daarvoor dienen",
                        "daarvoor kiezen",
                        "daarvoor betalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Для этой цели или по этой причине; ради этого."
                    },
                    "translations": {

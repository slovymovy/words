                {
                    "sense_id": "79b73a57-88f6-3cbe-9548-5ed108b5e774",
                    "sense_definition": "Een slordige verzameling voorwerpen die op elkaar liggen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_pile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gooi al het tuinafval maar op de grote <w>hoop</w> achter in de tuin.",
                            "target_lang_translations": {
                                "en": "Just throw all the garden waste on the big <w>pile</w> in the back of the garden.",
                                "ru": "Бросай весь садовый мусор в большую <w>кучу</w> в глубине сада."
                            }
                        },
                        {
                            "text": "Er lag een <w>hoop</w> vuile kleren op de vloer van zijn slaapkamer.",
                            "target_lang_translations": {
                                "en": "There was a <w>pile</w> of dirty clothes on the floor of his bedroom.",
                                "ru": "На полу его спальни лежала <w>куча</w> грязной одежды."
                            }

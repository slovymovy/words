    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "db3941e1-f407-335e-afea-e69e8c0c1749",
                    "sense_definition": "Базовая единица измерения количества информации в двоичной системе счисления; один разряд двоичного кода, принимающий значение 0 или 1.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "computing_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый <w>бит</w> информации в этой системе имеет критическое значение для безопасности.",
                            "target_lang_translations": {
                                "en": "Every <w>bit</w> of information in this system is critical for security."
                            }
                        },
                        {
                            "text": "Для кодирования одного символа в этом тексте используется восемь <w>бит</w>.",
                            "target_lang_translations": {
                                "en": "Eight <w>bits</w> are used to encode one character in this text."
                            }
                        }
                    ],
                    "synonyms": [
                        "разряд"
                    ],
                    "common_phrases": [
                        "один бит",
                        "бит информации",
                        "старший бит",
                        "знаковый бит"
                    ],
                    "target_lang_definitions": {
                        "en": "A basic unit of information in binary notation; a single digit of binary code having the value 0 or 1."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bit",
                                "target_lang_sense_clarification": "computing unit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2df79749-d330-367d-a850-fd6c95fc880a",
                    "sense_definition": "Ритм в музыке, ударная доля такта; в современной музыке (особенно в хип-хопе) — инструментальная основа композиции с четким ритмическим рисунком.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "music_rhythm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рэпер начал читать свой новый текст под энергичный и быстрый <w>бит</w>.",
                            "target_lang_translations": {
                                "en": "The rapper started rapping his new lyrics to an energetic and fast <w>beat</w>."
                            }
                        },
                        {
                            "text": "Музыкант сбился с ритма и не попал в сильный <w>бит</w> барабанов.",
                            "target_lang_translations": {
                                "en": "The musician lost the rhythm and didn't hit the strong <w>beat</w> of the drums."
                            }
                        }
                    ],
                    "synonyms": [
                        "ритм",
                        "такт",
                        "доля"
                    ],
                    "common_phrases": [
                        "тяжёлый бит",
                        "читать под бит",
                        "ломаный бит",
                        "ровный бит"
                    ],
                    "target_lang_definitions": {
                        "en": "Rhythm in music, the downbeat; in modern music (especially hip-hop), the instrumental backing with a distinct rhythmic pattern."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "music rhythm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b103e772-b0ab-3177-bede-bf5de05c9cdf",
                    "sense_definition": "Стиль рок-музыки, возникший в Великобритании в начале 1960-х годов, характеризующийся равномерным ритмом (бит-музыка).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ливерпульская четвёрка сделала британский <w>бит</w> популярным во всём мире.",
                            "target_lang_translations": {
                                "en": "The Liverpool four made British <w>beat</w> popular all over the world."
                            }
                        },
                        {
                            "text": "На вечеринке играл старый добрый <w>бит</w>, и все танцевали твист.",
                            "target_lang_translations": {
                                "en": "Good old <w>beat</w> was playing at the party, and everyone was dancing the twist."
                            }
                        }
                    ],
                    "synonyms": [
                        "бит-рок"
                    ],
                    "common_phrases": [
                        "британский бит",
                        "звёзды бита"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to 1960s music style"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A style of rock music originating in the UK in the early 1960s, characterized by a steady rhythm (beat music)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "music genre"
                            },
                            {
                                "target_lang_word": "beat music",
                                "target_lang_sense_clarification": "music genre"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afb185c8-7ecd-3146-a94c-069db84f602a",
                    "sense_definition": "Сменная насадка для шуруповёрта или отвёртки с определённым типом наконечника.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tool_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мастер попросил подать ему крестовой <w>бит</w> для закручивания саморезов.",
                            "target_lang_translations": {
                                "en": "The craftsman asked to be handed a Phillips <w>bit</w> for driving self-tapping screws."
                            }
                        },
                        {
                            "text": "У меня в наборе инструментов потерялся самый нужный <w>бит</w>.",
                            "target_lang_translations": {
                                "en": "The most necessary <w>bit</w> is missing from my tool set."
                            }
                        }

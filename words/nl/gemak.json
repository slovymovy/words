    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9fc91d1c-48d9-3507-a26b-840d747426b9",
                    "sense_definition": "Een toestand van rust, ontspanning en comfort, zonder haast of zorgen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_ease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zat op zijn <w>gemak</w> de krant te lezen in de tuin.",
                            "target_lang_translations": {
                                "ru": "Он <w>спокойно</w> читал газету в саду."
                            }
                        },
                        {
                            "text": "Na een tijdje begon ze zich meer op haar <w>gemak</w> te voelen.",
                            "target_lang_translations": {
                                "ru": "Через некоторое время она стала чувствовать себя более <w>комфортно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rust",
                        "comfort"
                    ],
                    "antonyms": [
                        "ongemak"
                    ],
                    "common_phrases": [
                        "op je gemak",
                        "zich op zijn gemak voelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние покоя, расслабленности и комфорта, без спешки или забот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спокойствие",
                                "target_lang_sense_clarification": "состояние душевного покоя"
                            },
                            {
                                "target_lang_word": "комфорт",
                                "target_lang_sense_clarification": "ощущение уюта и безопасности"
                            },
                            {
                                "target_lang_word": "непринужденность",
                                "target_lang_sense_clarification": "свобода в поведении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "112c6bad-7953-3a4f-85f8-607cbe631db3",
                    "sense_definition": "De afwezigheid van moeite of inspanning bij het doen van iets.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "facility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met groot <w>gemak</w> slaagde hij voor het moeilijke examen.",
                            "target_lang_translations": {
                                "ru": "Он с большой <w>лёгкостью</w> сдал этот трудный экзамен."
                            }
                        },
                        {
                            "text": "Het <w>gemak</w> waarmee zij de piano speelt is indrukwekkend.",
                            "target_lang_translations": {
                                "ru": "<w>Лёгкость</w>, с которой она играет на пианино, впечатляет."
                            }
                        }
                    ],
                    "synonyms": [
                        "eenvoud"
                    ],
                    "antonyms": [
                        "ongemak"
                    ],
                    "common_phrases": [
                        "met groot gemak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствие трудностей или усилий при выполнении чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лёгкость",
                                "target_lang_sense_clarification": "отсутствие усилий"
                            },
                            {
                                "target_lang_word": "простота",
                                "target_lang_sense_clarification": "отсутствие сложности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "909c121f-1094-3779-a0e2-79ed8c9788eb",
                    "sense_definition": "Een voorziening of hulpmiddel dat het leven makkelijker of aangenamer maakt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "convenience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit moderne vakantiehuis is van alle <w>gemakken</w> voorzien.",
                            "target_lang_translations": {
                                "ru": "Этот современный загородный дом оснащен всеми <w>удобствами</w>."
                            }
                        },
                        {
                            "text": "We kiezen voor het <w>gemak</w> van een kant-en-klare maaltijd.",
                            "target_lang_translations": {
                                "ru": "Мы выбираем <w>удобство</w> готовой еды."
                            }
                        }
                    ],
                    "synonyms": [
                        "gerief",
                        "comfort"
                    ],
                    "antonyms": [
                        "ongemak"
                    ],
                    "common_phrases": [
                        "van alle gemakken voorzien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство или приспособление, делающее жизнь проще или приятнее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удобство",
                                "target_lang_sense_clarification": "комфортные условия или приспособления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8540e0cb-cfcd-3281-92c1-5f3525a0a21a",
                    "sense_definition": "Een ruimte met een wc; het toilet.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "toilet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vroeg de ober waar hij het <w>gemak</w> kon vinden.",
                            "target_lang_translations": {
                                "ru": "Он спросил официанта, где найти <w>уборную</w>."
                            }
                        },
                        {
                            "text": "In oudere huizen bevindt het <w>gemak</w> zich soms nog buiten.",
                            "target_lang_translations": {
                                "ru": "В старых домах <w>туалет</w> иногда всё ещё находится на улице."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het klein gemak"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "wordt als verouderd of eufemistisch beschouwd"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещение с унитазом; туалет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уборная",
                                "target_lang_sense_clarification": "туалет (часто устар. или форм.)"
                            },
                            {
                                "target_lang_word": "туалет",
                                "target_lang_sense_clarification": "санитарный узел"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "timely",
        "timer",
        "timeless",
        "timing",
        "timetable",
        "timeline"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dbf695fe-a6f9-3eea-a8fb-f2e432743c84",
                    "sense_definition": "The specific moment in the day as indicated by a clock.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "temporal_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you tell me what <w>time</w> it is?",
                            "target_lang_translations": {
                                "ru": "Вы можете сказать, сколько сейчас <w>времени</w>?"
                            }
                        },
                        {
                            "text": "The <w>time</w> is exactly four o'clock.",
                            "target_lang_translations": {
                                "ru": "Сейчас ровно четыре <w>часа</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "point in time",
                        "moment"
                    ],
                    "common_phrases": [
                        "what time is it",
                        "on time",
                        "in time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный момент дня, который показывают часы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "время",
                                "target_lang_sense_clarification": "момент"
                            },
                            {
                                "target_lang_word": "час",
                                "target_lang_sense_clarification": "единица времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e8e3250-0cf0-3afb-8b18-77abea984d5a",
                    "sense_definition": "A quantity of availability or duration that can be spent, saved, or wasted.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "temporal_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We do not have much <w>time</w> left to finish the test.",
                            "target_lang_translations": {
                                "ru": "У нас осталось мало <w>времени</w>, чтобы закончить тест."
                            }
                        },
                        {
                            "text": "She spends a lot of <w>time</w> reading books.",
                            "target_lang_translations": {
                                "ru": "Она проводит много <w>времени</w> за чтением книг."
                            }
                        }
                    ],
                    "common_phrases": [
                        "waste time",
                        "spend time",
                        "free time",
                        "plenty of time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество времени или продолжительность, доступные для использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "время",
                                "target_lang_sense_clarification": "продолжительность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de82c3d9-5b6c-38fb-b1c8-33b84d1d11cf",
                    "sense_definition": "An instance or repetition of an action or event.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have visited Paris three <w>times</w>.",
                            "target_lang_translations": {
                                "ru": "Я был в Париже три <w>раза</w>."
                            }
                        },
                        {
                            "text": "This is the first <w>time</w> I have seen this movie.",
                            "target_lang_translations": {
                                "ru": "Я вижу этот фильм в первый <w>раз</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "occasion"
                    ],
                    "common_phrases": [
                        "three times",
                        "many times",
                        "last time",
                        "next time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный случай или повторение действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раз",
                                "target_lang_sense_clarification": "случай"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2adaa176-5a21-3e6b-bb26-2e350949becf",
                    "sense_definition": "The appropriate or expected moment to do something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "temporal_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>time</w> to go home now.",
                            "target_lang_translations": {
                                "ru": "Сейчас <w>пора</w> идти домой."
                            }
                        },
                        {
                            "text": "Is it <w>time</w> for dinner yet?",
                            "target_lang_translations": {
                                "ru": "Уже <w>пора</w> ужинать?"
                            }
                        }
                    ],
                    "synonyms": [
                        "moment"
                    ],
                    "common_phrases": [
                        "time to go",
                        "time for bed",
                        "about time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходящий или ожидаемый момент для совершения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пора",
                                "target_lang_sense_clarification": "подходящий момент"
                            },
                            {
                                "target_lang_word": "время",
                                "target_lang_sense_clarification": "срок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "193e5287-5cad-34e1-afbb-439edc6adb9e",
                    "sense_definition": "A specific experience or period of life characterized by a certain quality.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Did you have a good <w>time</w> at the party?",
                            "target_lang_translations": {
                                "ru": "Ты хорошо провел <w>время</w> на вечеринке?"
                            }
                        },
                        {
                            "text": "He is having a hard <w>time</w> at his new job.",
                            "target_lang_translations": {
                                "ru": "У него сейчас тяжелое <w>время</w> на новой работе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have a good time",
                        "hard time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный опыт или период жизни, характеризующийся особым качеством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "время",
                                "target_lang_sense_clarification": "период"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfebe4f9-8b81-3b41-b707-31408e41ff45",
                    "sense_definition": "A ratio of comparison used to show how much bigger, faster, or more frequent something is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ratio",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My car is three <w>times</w> faster than yours.",
                            "target_lang_translations": {
                                "ru": "Моя машина в три <w>раза</w> быстрее твоей."
                            }
                        },
                        {
                            "text": "This box is four <w>times</w> as heavy as that one.",
                            "target_lang_translations": {
                                "ru": "Эта коробка в четыре <w>раза</w> тяжелее той."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Коэффициент сравнения, показывающий, во сколько раз что-то больше или быстрее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раз",
                                "target_lang_sense_clarification": "кратность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f062895-f1b5-3cb0-8604-e332a03c584b",
                    "sense_definition": "The indefinite continued progress of existence and events in the past, present, and future.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "temporal_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Time</w> passes very quickly when you are busy.",
                            "target_lang_translations": {
                                "ru": "<w>Время</w> летит очень быстро, когда ты занят."
                            }
                        },
                        {
                            "text": "We cannot stop the flow of <w>time</w>.",
                            "target_lang_translations": {
                                "ru": "Мы не можем остановить течение <w>времени</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "time flies",
                        "passage of time",
                        "time travel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопределенное непрерывное течение существования и событий в прошлом, настоящем и будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "время",
                                "target_lang_sense_clarification": "философское понятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "965429ec-7de8-3ac7-854e-71c7ca8b95b0",
                    "sense_definition": "A historical period or era characterized by specific conditions.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Life was very different in medieval <w>times</w>.",
                            "target_lang_translations": {
                                "ru": "Жизнь была совсем другой в средневековые <w>времена</w>."
                            }
                        },
                        {
                            "text": "These are difficult <w>times</w> for the global economy.",
                            "target_lang_translations": {
                                "ru": "Сейчас трудные <w>времена</w> для мировой экономики."
                            }
                        }
                    ],
                    "synonyms": [
                        "ages"
                    ],
                    "common_phrases": [
                        "ancient times",
                        "modern times"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исторический период или эра, характеризующаяся определенными условиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "времена",
                                "target_lang_sense_clarification": "эпоха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ffabd35-1862-3fd7-a94b-e0bfc63caa29",
                    "sense_definition": "The grammatical category of a verb that indicates when an action happens.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher explained the future <w>time</w> of the verb.",
                            "target_lang_translations": {
                                "ru": "Учитель объяснил будущее <w>время</w> глагола."
                            }
                        },
                        {
                            "text": "In this language, the <w>time</w> is marked by a suffix.",
                            "target_lang_translations": {
                                "ru": "В этом языке <w>время</w> обозначается суффиксом."
                            }
                        }
                    ],
                    "synonyms": [
                        "tense"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая категория глагола, указывающая на момент совершения действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "время",
                                "target_lang_sense_clarification": "грамматика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bb44821-34e0-38dc-8778-774225578891",
                    "sense_definition": "The period of a prison sentence.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "prison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is doing <w>time</w> for a crime he committed years ago.",
                            "target_lang_translations": {
                                "ru": "Он отбывает <w>срок</w> за преступление, совершенное много лет назад."
                            }
                        },
                        {
                            "text": "The judge sentenced him to hard <w>time</w>.",
                            "target_lang_translations": {
                                "ru": "Судья приговорил его к реальному <w>сроку</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do time"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Период тюремного заключения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срок",
                                "target_lang_sense_clarification": "заключение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f5c9f86e-fa14-3d52-ac75-7def93e9750f",
                    "sense_definition": "To measure how long something takes.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The coach will <w>time</w> the runners to see who is fastest.",
                            "target_lang_translations": {
                                "ru": "Тренер <w>засечет время</w> бегунов, чтобы узнать, кто самый быстрый."
                            }
                        },
                        {
                            "text": "I need to <w>time</w> myself to ensure I finish the exam.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>засечь время</w>, чтобы убедиться, что я закончу экзамен."
                            }
                        }
                    ],
                    "synonyms": [
                        "clock"
                    ],
                    "target_lang_definitions": {
                        "ru": "Измерять, сколько времени занимает что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засекать время",
                                "target_lang_sense_clarification": "измерять"
                            },
                            {
                                "target_lang_word": "хронометрировать",
                                "target_lang_sense_clarification": "официально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2ba7ccf-f062-3372-a009-2016a9a32dc9",
                    "sense_definition": "To arrange or choose a specific moment for something to happen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "schedule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>timed</w> the launch perfectly to avoid the storm.",
                            "target_lang_translations": {
                                "ru": "Они идеально <w>рассчитали время</w> запуска, чтобы избежать шторма."
                            }
                        },
                        {
                            "text": "The attack was <w>timed</w> to coincide with the holiday.",
                            "target_lang_translations": {
                                "ru": "Атака была <w>приурочена</w> к празднику."
                            }
                        }
                    ],
                    "synonyms": [
                        "set"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбирать или назначать определенный момент для совершения действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассчитывать время",
                                "target_lang_sense_clarification": "планировать"
                            },
                            {
                                "target_lang_word": "приурочивать",
                                "target_lang_sense_clarification": "согласовывать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "f92a0869-d50a-3acf-8c33-f6f6042b1667",
                    "sense_definition": "A call used to warn that a business, usually a bar, is closing.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Time</w>, gentlemen, please finish your drinks!",
                            "target_lang_translations": {
                                "ru": "<w>Закрываемся</w>, господа, пожалуйста, допивайте напитки!"
                            }
                        },
                        {
                            "text": "The bartender shouted <w>time</w> as the clock struck midnight.",
                            "target_lang_translations": {
                                "ru": "Бармен крикнул «<w>Время</w>», когда часы пробили полночь."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, предупреждающий о закрытии заведения, обычно бара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрываемся",
                                "target_lang_sense_clarification": "предупреждение"
                            },
                            {
                                "target_lang_word": "время",
                                "target_lang_sense_clarification": "сигнал"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
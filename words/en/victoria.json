                },
                {
                    "sense_id": "3b257598-ae84-39a7-ace0-0df377b2109b",
                    "sense_definition": "The capital city of British Columbia, Canada.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "place_name_group",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We took a ferry from Vancouver to <w>Victoria</w> for the weekend.",
                            "target_lang_translations": {
                                "ru": "Мы сели на паром из Ванкувера в <w>Викторию</w> на выходные."
                            }
                        },
                        {
                            "text": "The gardens in <w>Victoria</w> are famous for their beautiful flowers.",
                            "target_lang_translations": {

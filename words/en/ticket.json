            "senses": [
                {
                    "sense_id": "812a896a-8799-3257-ab5d-1c72792019a7",
                    "sense_definition": "A pass entitling the holder to admission to a show, concert, sporting event, or similar activity.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "document_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I bought a <w>ticket</w> for the movie tonight.",
                            "target_lang_translations": {
                                "ru": "Я купил <w>билет</w> в кино на вечер."
                            }
                        },
                        {
                            "text": "Do you have your <w>ticket</w> for the concert?",
                            "target_lang_translations": {
                                "ru": "У тебя есть <w>билет</w> на концерт?"
                            }
                        }
                    ],
                    "synonyms": [
                        "license"
                    ],
                    "common_phrases": [
                        "concert ticket",
                        "movie ticket",
                        "buy a ticket"
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c637faac-3a32-36b3-a649-9c4d823aaa6f",
                    "sense_definition": "Командир морского или воздушного судна",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "leader_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш <w>капитан</w> уверенно вёл судно через сильный шторм.",
                            "target_lang_translations": {
                                "en": "Our <w>captain</w> confidently guided the vessel through the strong storm."
                            }
                        },
                        {
                            "text": "<w>Капитан</w> корабля объявил пассажирам о скором прибытии в порт.",
                            "target_lang_translations": {
                                "en": "The ship's <w>captain</w> announced the imminent arrival at the port to the passengers."
                            }
                        }
                    ],
                    "synonyms": [
                        "шкипер"
                    ],
                    "antonyms": [
                        "матрос"
                    ],
                    "common_phrases": [
                        "капитан корабля",
                        "опытный капитан",
                        "приказ капитана",
                        "капитан дальнего плавания"
                    ],
                    "target_lang_definitions": {
                        "en": "A person in command of a ship or an aircraft"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "captain",
                                "target_lang_sense_clarification": "commander of a ship or aircraft"
                            },
                            {
                                "target_lang_word": "skipper",
                                "target_lang_sense_clarification": "informal, usually of a boat"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85f02b66-f041-3ff0-809b-95784253c2c2",
                    "sense_definition": "Воинское звание младшего офицерского состава выше старшего лейтенанта и ниже майора, а также человек, носящий это звание",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "military_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодой <w>капитан</w> отдал честь генералу и вышел из кабинета.",
                            "target_lang_translations": {
                                "en": "The young <w>captain</w> saluted the general and left the office."
                            }
                        },
                        {
                            "text": "В прошлом году мой брат получил звание <w>капитана</w> полиции.",
                            "target_lang_translations": {
                                "en": "Last year my brother received the rank of police <w>captain</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "звание капитана",
                        "товарищ капитан",
                        "погоны капитана"
                    ],
                    "target_lang_definitions": {
                        "en": "A military rank for an officer, usually above lieutenant and below major"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "captain",
                                "target_lang_sense_clarification": "military or police rank"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "548dff10-c05b-3e19-b96e-b8c0f2bf52aa",
                    "sense_definition": "Главный игрок спортивной команды, выполняющий руководящие функции",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "leader_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Капитан</w> футбольной команды первым поднял кубок над головой.",
                            "target_lang_translations": {
                                "en": "The football team <w>captain</w> was the first to lift the cup over his head."
                            }
                        },
                        {
                            "text": "Тренер назначил нового <w>капитана</w> сборной перед началом важного турнира.",
                            "target_lang_translations": {
                                "en": "The coach appointed a new national team <w>captain</w> before the start of the important tournament."
                            }
                        }
                    ],
                    "common_phrases": [
                        "капитан команды",
                        "капитан сборной",
                        "повязка капитана"
                    ],
                    "target_lang_definitions": {
                        "en": "The leader of a sports team"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "captain",
                                "target_lang_sense_clarification": "sports team leader"
                            }
                        ]
                    }
                }
            ]
        }
    ]

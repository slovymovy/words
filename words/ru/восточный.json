                            },
                            {
                                "target_lang_word": "east",
                                "target_lang_sense_clarification": "used in compound nouns or specific phrases"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83efe125-0ab1-359a-ac34-4508d9b58144",

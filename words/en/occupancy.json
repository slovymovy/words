{
    "word_family": [
        "occupy",
        "occupant",
        "occupation",
        "occupational",
        "occupier"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ff588338-b60a-3d04-95f9-01313dab947f",
                    "sense_definition": "The act of living in or using a building, room, or piece of land.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_use",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The apartment is clean and ready for immediate <w>occupancy</w>.",
                            "target_lang_translations": {
                                "ru": "Квартира чистая и готова к немедленному <w>заселению</w>."
                            }
                        },
                        {
                            "text": "Hotels usually experience a lower rate of <w>occupancy</w> during the winter season.",
                            "target_lang_translations": {
                                "ru": "Отели обычно испытывают более низкую <w>заполняемость</w> в зимний сезон."
                            }
                        }
                    ],
                    "synonyms": [
                        "residence",
                        "tenancy",
                        "habitation",
                        "possession"
                    ],
                    "antonyms": [
                        "inoccupancy"
                    ],
                    "common_phrases": [
                        "certificate of occupancy",
                        "immediate occupancy",
                        "double occupancy",
                        "single occupancy",
                        "occupancy rate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт проживания или использования здания, помещения или земельного участка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заселение",
                                "target_lang_sense_clarification": "въезд в помещение для проживания"
                            },
                            {
                                "target_lang_word": "заполняемость",
                                "target_lang_sense_clarification": "коэффициент занятости мест (например, в отеле)"
                            },
                            {
                                "target_lang_word": "занятость",
                                "target_lang_sense_clarification": "состояние, когда помещение занято"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7b117c6-54bf-367f-bab8-0055de5b4539",
                    "sense_definition": "The period of time during which someone rents or uses a place.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They made several major repairs to the property during their <w>occupancy</w>.",
                            "target_lang_translations": {
                                "ru": "Они сделали несколько крупных ремонтов недвижимости за время своего <w>проживания</w>."
                            }
                        },
                        {
                            "text": "The lease agreement covers a five-year <w>occupancy</w> of the office space.",
                            "target_lang_translations": {
                                "ru": "Договор аренды предусматривает пятилетнее <w>пользование</w> офисным помещением."
                            }
                        }
                    ],
                    "synonyms": [
                        "tenure",
                        "tenancy"
                    ],
                    "common_phrases": [
                        "during their occupancy",
                        "length of occupancy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени, в течение которого кто-то арендует или использует помещение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проживание",
                                "target_lang_sense_clarification": "период жительства в помещении"
                            },
                            {
                                "target_lang_word": "пользование",
                                "target_lang_sense_clarification": "период владения или эксплуатации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "800cc3f5-9f5d-3e53-a16e-5d6633d839e9",
                    "sense_definition": "The specific use or function assigned to a building or land, often regarding safety or zoning regulations.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "usage_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This building is designated for residential <w>occupancy</w> only.",
                            "target_lang_translations": {
                                "ru": "Это здание предназначено только для <w>проживания</w>."
                            }
                        },
                        {
                            "text": "The fire department must approve the change of <w>occupancy</w> before the business opens.",
                            "target_lang_translations": {
                                "ru": "Пожарная служба должна утвердить изменение <w>назначения</w> помещения до открытия бизнеса."
                            }
                        }
                    ],
                    "synonyms": [
                        "usage",
                        "classification"
                    ],
                    "common_phrases": [
                        "change of occupancy",
                        "residential occupancy",
                        "commercial occupancy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфическое использование или функция, присвоенная зданию или земле, часто в контексте правил безопасности или зонирования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "назначение",
                                "target_lang_sense_clarification": "функциональная цель использования помещения"
                            },
                            {
                                "target_lang_word": "эксплуатация",
                                "target_lang_sense_clarification": "использование здания согласно техническим нормам"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
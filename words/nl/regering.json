            "senses": [
                {
                    "sense_id": "e2843c95-07b5-30c9-a270-9dff11b3e0cd",
                    "sense_definition": "Het bestuur van een land, bestaande uit ministers en vaak het staatshoofd",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "political_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>regering</w> heeft nieuwe maatregelen aangekondigd om de economie te steunen.",
                            "target_lang_translations": {
                                "en": "The <w>government</w> has announced new measures to support the economy.",
                                "ru": "<w>Правительство</w> объявило о новых мерах для поддержки экономики."
                            }
                        },
                        {
                            "text": "Na de verkiezingen duurde het enkele maanden om een nieuwe <w>regering</w> te vormen.",
                            "target_lang_translations": {
                                "en": "After the elections, it took several months to form a new <w>government</w>.",
                                "ru": "После выборов потребовалось несколько месяцев, чтобы сформировать новое <w>правительство</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kabinet",
                        "gouvernement"
                    ],
                    "common_phrases": [
                        "een regering vormen",
                        "de zittende regering",
                        "in de regering zitten",
                        "de regering vallen"
                    ],
                    "target_lang_definitions": {
                        "en": "The governing body of a country, consisting of ministers and often the head of state",
                        "ru": "Орган управления страной, состоящий из министров и часто главы государства"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "government",
                                "target_lang_sense_clarification": "political body governing a state"
                            },
                            {
                                "target_lang_word": "administration",
                                "target_lang_sense_clarification": "executive branch of government"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "правительство",
                                "target_lang_sense_clarification": "высший исполнительный орган власти"
                            },
                            {
                                "target_lang_word": "кабинет министров",
                                "target_lang_sense_clarification": "состав правительства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a63dcd26-f800-3990-999c-a70ca003edf9",
                    "sense_definition": "De periode waarin een vorst of leider aan de macht is; de heerschappij",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rule_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de <w>regering</w> van de koningin bloeide de kunst en cultuur in het land.",
                            "target_lang_translations": {
                                "en": "During the <w>reign</w> of the queen, art and culture flourished in the country.",
                                "ru": "Во время <w>правления</w> королевы искусство и культура в стране процветали."
                            }
                        },
                        {
                            "text": "Het land kende een lange periode van vrede onder de <w>regering</w> van de wijze vorst.",
                            "target_lang_translations": {
                                "en": "The country experienced a long period of peace under the <w>rule</w> of the wise monarch.",
                                "ru": "Страна пережила долгий период мира под <w>правлением</w> мудрого монарха."
                            }
                        }
                    ],
                    "synonyms": [
                        "bewind",
                        "regime"
                    ],
                    "common_phrases": [
                        "tijdens de regering",
                        "onder de regering van"
                    ],
                    "target_lang_definitions": {
                        "en": "The period during which a monarch or leader is in power; the reign",
                        "ru": "Период, в течение которого монарх или лидер находится у власти"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reign",
                                "target_lang_sense_clarification": "period of rule by a monarch"
                            },
                            {
                                "target_lang_word": "rule",
                                "target_lang_sense_clarification": "exercise of authority or control"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "правление",
                                "target_lang_sense_clarification": "время нахождения у власти"
                            },
                            {
                                "target_lang_word": "царствование",
                                "target_lang_sense_clarification": "период правления монарха"
                            }
                        ]
                    }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c803c30c-0b05-366c-b8d4-c08e27eec04f",
                    "sense_definition": "Iets tot poeder of kleine stukjes vermalen, meestal met een machine of tussen twee harde voorwerpen",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mechanical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De molenaar <w>maalt</w> het graan tot fijn meel.",
                            "target_lang_translations": {
                                "en": "The miller <w>grinds</w> the grain into fine flour.",
                                "ru": "Мельник <w>мелет</w> зерно в муку тонкого помола."
                            }
                        },
                        {
                            "text": "Ik heb vanochtend verse bonen <w>gemalen</w> voor de koffie.",
                            "target_lang_translations": {
                                "en": "I <w>ground</w> fresh beans for the coffee this morning.",
                                "ru": "Сегодня утром я <w>смолол</w> свежие зерна для кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "tobben"
                    ],
                    "common_phrases": [
                        "koffie malen",
                        "graan malen",
                        "fijn malen"
                    ],
                    "target_lang_definitions": {
                        "en": "To reduce something to powder or small pieces, usually with a machine or between two hard objects",
                        "ru": "Измельчать что-либо в порошок или мелкие кусочки, обычно с помощью машины или жерновов"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grind",
                                "target_lang_sense_clarification": "To reduce to smaller pieces by crushing or friction"
                            },
                            {
                                "target_lang_word": "mill",
                                "target_lang_sense_clarification": "To grind or process grain in a machine"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "молоть",
                                "target_lang_sense_clarification": "измельчать зерна или твердые предметы"
                            },
                            {
                                "target_lang_word": "перемалывать",
                                "target_lang_sense_clarification": "процесс полного измельчения"
                            },
                            {
                                "target_lang_word": "измельчать",
                                "target_lang_sense_clarification": "делать мелким"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afdcda15-fa22-3beb-924f-d6a7bd90b0d8",
                    "sense_definition": "Voortdurend onrustig nadenken over vervelende dingen",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij bleef maar <w>malen</w> over de fout die hij had gemaakt.",
                            "target_lang_translations": {
                                "en": "He kept <w>ruminating</w> on the mistake he had made.",
                                "ru": "Он продолжал <w>зацикливаться</w> на ошибке, которую совершил."
                            }
                        },
                        {
                            "text": "Er <w>maalden</w> allerlei sombere gedachten door haar hoofd.",
                            "target_lang_translations": {
                                "en": "All sorts of gloomy thoughts <w>swirled</w> through her head.",
                                "ru": "В её голове <w>крутились</w> всякие мрачные мысли."
                            }
                        }
                    ],
                    "synonyms": [
                        "tobben",
                        "tobben"
                    ],
                    "common_phrases": [
                        "blijven malen",
                        "niet om malen"
                    ],
                    "target_lang_definitions": {
                        "en": "To constantly and restlessly think about unpleasant things",
                        "ru": "Постоянно и беспокойно думать о неприятных вещах, прокручивать мысли в голове"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ruminate",
                                "target_lang_sense_clarification": "To think deeply or repeatedly about something"
                            },
                            {
                                "target_lang_word": "worry",
                                "target_lang_sense_clarification": "To think about problems or unpleasant things"
                            },
                            {
                                "target_lang_word": "dwell",
                                "target_lang_sense_clarification": "To think or speak at length about a particular subject"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "зацикливаться",
                                "target_lang_sense_clarification": "постоянно думать об одном и том же"
                            },
                            {
                                "target_lang_word": "мучиться мыслями",
                                "target_lang_sense_clarification": "переживать"
                            },
                            {
                                "target_lang_word": "крутиться",
                                "target_lang_sense_clarification": "о мыслях в голове"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f3fde72-e352-3854-bd26-c7f23780f7fd",
                    "sense_definition": "Water verplaatsen of wegpompen met behulp van een werktuig",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De poldermolens <w>malen</w> het overtollige water uit het gebied.",
                            "target_lang_translations": {
                                "en": "The polder mills <w>pump</w> the excess water out of the area.",
                                "ru": "Польдерные мельницы <w>откачивают</w> лишнюю воду из этой местности."
                            }
                        },
                        {
                            "text": "Het gemaal moet hard <w>malen</w> om de polder droog te houden.",
                            "target_lang_translations": {
                                "en": "The pumping station must <w>pump</w> hard to keep the polder dry.",
                                "ru": "Насосная станция должна усиленно <w>работать</w>, чтобы держать польдер сухим."
                            }
                        }
                    ],
                    "common_phrases": [
                        "water malen"
                    ],
                    "target_lang_definitions": {
                        "en": "To move or pump away water using a machine or tool",
                        "ru": "Перемещать или откачивать воду с помощью механизма (например, мельницы или насоса)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pump",
                                "target_lang_sense_clarification": "To move fluids using a mechanical device"
                            },
                            {
                                "target_lang_word": "drain",
                                "target_lang_sense_clarification": "To remove water from an area"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "откачивать",
                                "target_lang_sense_clarification": "воду"
                            },
                            {
                                "target_lang_word": "качать",
                                "target_lang_sense_clarification": "работать насосом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4780b613-683a-3e3c-ba4e-5f69254dc43c",
                    "sense_definition": "Een afbeelding maken met verf",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "artistic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude meester <w>maalde</w> een prachtig portret van de koningin.",
                            "target_lang_translations": {
                                "en": "The old master <w>painted</w> a beautiful portrait of the queen.",
                                "ru": "Старый мастер <w>написал</w> прекрасный портрет королевы."
                            }
                        },
                        {
                            "text": "In dat gedicht wordt de natuur zeer levendig <w>gemaald</w>.",
                            "target_lang_translations": {
                                "en": "In that poem, nature is <w>depicted</w> very vividly.",
                                "ru": "В этом стихотворении природа очень живо <w>изображена</w>."
                            }
                        }
                    ],
                    "traits": [

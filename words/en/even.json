                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will see you on Monday, or Tuesday <w>even</w>.",
                            "target_lang_translations": {
                                "ru": "Увидимся в понедельник, или <w>даже</w> во вторник."
                            }
                        },
                        {
                            "text": "My favorite actor is Jack Nicklaus; Jack Nicholson, <w>even</w>.",
                            "target_lang_translations": {
                                "ru": "Мой любимый актёр — Джек Никлаус; <w>точнее</w>, Джек Николсон."
                            }
                        }
                    ],
                    "synonyms": [
                        "in other words"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сигнализирует о поправке к предыдущему высказыванию; вернее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верне́е",
                                "target_lang_sense_clarification": "поправка"
                            },
                            {
                                "target_lang_word": "точне́е",
                                "target_lang_sense_clarification": "уточнение"
                            }
                        ]
                    }
                }
            ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d72ffc2-5ecf-3701-9912-7dd6ee40b54c",
                    "sense_definition": "Silne pragnienie zrealizowania czegoś, co jest dla kogoś ważne, przyjemne lub kuszące; przedmiot takich pragnień.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "desire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podróż dookoła świata to moje największe <w>marzenie</w> od dzieciństwa.",
                            "target_lang_translations": {
                                "ru": "Путешествие вокруг света — это моя самая большая <w>мечта</w> с детства."
                            }
                        },
                        {
                            "text": "W końcu udało mu się spełnić swoje <w>marzenie</w> o własnym domu z ogrodem.",
                            "target_lang_translations": {
                                "ru": "Наконец ему удалось осуществить свою <w>мечту</w> о собственном доме с садом."
                            }
                        }
                    ],
                    "synonyms": [
                        "pragnienie",
                        "sen"
                    ],
                    "antonyms": [
                        "rzeczywistość"
                    ],
                    "common_phrases": [
                        "spełnić marzenie",
                        "szczyt marzeń",
                        "marzenie ściętej głowy",
                        "kraina marzeń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное желание осуществить что-то важное, приятное или заманчивое; предмет таких желаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мечта",
                                "target_lang_sense_clarification": "заветное желание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "340af5cf-cd10-3137-b3db-73c4df287297",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>marzyć</w>; proces oddawania się fantazjom i rozmyślaniom.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciągłe <w>marzenie</w> o sukcesie nie zastąpi ciężkiej pracy potrzebnej do jego osiągnięcia.",
                            "target_lang_translations": {
                                "ru": "Постоянные <w>мечтания</w> об успехе не заменят тяжелой работы, необходимой для его достижения."
                            }
                        },
                        {
                            "text": "Przez to twoje <w>marzenie</w> w obłokach znowu spóźnisz się do szkoły.",
                            "target_lang_translations": {
                                "ru": "Из-за этого твоего <w>витания</w> в облаках ты снова опоздаешь в школу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oddawać się marzeniom"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verbal noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отглагольное существительное от слова «marzyć»; процесс предавания фантазиям и размышлениям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мечтание",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "грёзы",
                                "target_lang_sense_clarification": "фантазии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6d4d3d3-5fdb-39d5-b6b4-bf97ad9b603f",
                    "sense_definition": "Coś niezwykle pięknego, cudownego, idealnego lub trudnego do uwierzenia.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "wonder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta nowa sukienka to po prostu <w>marzenie</w>, wyglądasz w niej świetnie!",
                            "target_lang_translations": {
                                "ru": "Это новое платье — просто <w>мечта</w>, ты выглядишь в нем отлично!"
                            }
                        },
                        {
                            "text": "Pogoda podczas wakacji była jak <w>marzenie</w>, codziennie świeciło słońce.",
                            "target_lang_translations": {
                                "ru": "Погода во время отпуска была как <w>мечта</w>, каждый день светило солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "zjawisko"
                    ],
                    "common_phrases": [
                        "wyglądać jak marzenie",
                        "jak marzenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то невероятно красивое, чудесное, идеальное или такое, во что трудно поверить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мечта",
                                "target_lang_sense_clarification": "нечто идеальное"
                            },
                            {
                                "target_lang_word": "сказка",
                                "target_lang_sense_clarification": "что-то чудесное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60212313-4da6-3e6e-afea-9e5967262041",
                    "sense_definition": "Obrazy, dźwięki i myśli pojawiające się w umyśle podczas snu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sleep_dream",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nocne <w>marzenie</w> było pełne dziwnych i tajemniczych postaci.",
                            "target_lang_translations": {
                                "ru": "Его ночное <w>сновидение</w> было полно странных и таинственных фигур."
                            }
                        },
                        {
                            "text": "Badacze analizowali każde <w>marzenie</w> senne pacjenta, szukając ukrytych znaczeń.",
                            "target_lang_translations": {
                                "ru": "Исследователи анализировали каждое <w>сновидение</w> пациента в поисках скрытых смыслов."
                            }
                        }
                    ],
                    "synonyms": [
                        "sen"
                    ],
                    "common_phrases": [
                        "marzenie senne"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "usually 'sen' is used in this context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Образы, звуки и мысли, возникающие в сознании во время сна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сновидение",
                                "target_lang_sense_clarification": "сон"
                            }
                        ]
                    }
                }
            ]
        }

                {
                    "sense_id": "2c048c3e-2940-3222-90d6-fd3c0ae9ab92",
                    "sense_definition": "Strong negative criticism, especially of a performance or artistic work.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "criticism",
                    "name_type": "no",

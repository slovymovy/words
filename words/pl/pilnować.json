                            {
                                "target_lang_word": "быть осторожным",
                                "target_lang_sense_clarification": "проявлять осмотрительность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45a38b8a-7c50-35c5-beb0-ead18caa5d27",
                    "sense_definition": "Zwracać uwagę na siebie nawzajem, aby zapewnić sobie bezpieczeństwo lub porządek.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "nadzór",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wycieczki w góry musimy się <pilnować>, żeby nikt się nie zgubił.",
                            "target_lang_translations": {
                                "ru": "Во время похода в горы мы должны <держаться вместе>, чтобы никто не потерялся."

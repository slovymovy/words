    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5ffa1057-e5f7-3898-aded-732d48958e98",
                    "sense_definition": "Iets of iemand plotseling of met kracht vastpakken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie wist de vluchtende dief snel te <w>grijpen</w>.",
                            "target_lang_translations": {
                                "en": "The police managed to quickly <w>catch</w> the fleeing thief.",
                                "ru": "Полиции удалось быстро <w>поймать</w> убегающего вора."
                            }
                        },
                        {
                            "text": "Je moet deze unieke kans met beide handen <w>grijpen</w>.",
                            "target_lang_translations": {
                                "en": "You must <w>seize</w> this unique opportunity with both hands.",
                                "ru": "Ты должен обеими руками <w>ухватиться</w> за эту уникальную возможность."
                            }
                        }
                    ],
                    "synonyms": [
                        "pakken",
                        "vastpakken",
                        "nemen"
                    ],
                    "antonyms": [
                        "loslaten"
                    ],
                    "common_phrases": [
                        "de kans grijpen",
                        "de macht grijpen",
                        "naar de fles grijpen"
                    ],
                    "target_lang_definitions": {
                        "en": "To grab or seize someone or something suddenly or forcefully.",
                        "ru": "Схватить или резко взять кого-то или что-то с силой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grab",
                                "target_lang_sense_clarification": "To take hold of suddenly"
                            },
                            {
                                "target_lang_word": "seize",
                                "target_lang_sense_clarification": "To take hold of forcibly or take an opportunity"
                            },
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "To capture someone fleeing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хватать",
                                "target_lang_sense_clarification": "Брать резко или быстро"
                            },
                            {
                                "target_lang_word": "схватить",
                                "target_lang_sense_clarification": "Взять с силой (совершенный вид)"
                            },
                            {
                                "target_lang_word": "поймать",
                                "target_lang_sense_clarification": "Задержать или арестовать кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3bb603d-e8d6-3e44-8cd4-7bff60bcb64e",
                    "sense_definition": "Emotioneel sterk geraakt of geboeid worden door iets.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het publiek was volledig <w>gegrepen</w> door de prachtige muziekvoorstelling.",
                            "target_lang_translations": {
                                "en": "The audience was completely <w>captivated</w> by the beautiful music performance.",
                                "ru": "Публика была полностью <w>захвачена</w> прекрасным музыкальным представлением."
                            }
                        },
                        {
                            "text": "Ik ben diep <w>gegrepen</w> door het trieste verhaal van de vluchtelingen.",
                            "target_lang_translations": {
                                "en": "I am deeply <w>moved</w> by the sad story of the refugees.",
                                "ru": "Я глубоко <w>тронут</w> печальной историей беженцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "raken",
                        "boeien",
                        "ontroeren"
                    ],
                    "antonyms": [
                        "vervelen"
                    ],
                    "common_phrases": [
                        "door angst gegrepen",
                        "door paniek gegrepen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be emotionally moved, touched, or captivated by something.",
                        "ru": "Быть эмоционально тронутым или увлеченным чем-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grip",
                                "target_lang_sense_clarification": "To hold the attention or emotion of"
                            },
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "To affect with emotion"
                            },
                            {
                                "target_lang_word": "captivate",
                                "target_lang_sense_clarification": "To hold the interest of"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "захватывать",
                                "target_lang_sense_clarification": "Увлекать, завладевать вниманием"
                            },
                            {
                                "target_lang_word": "трогать",
                                "target_lang_sense_clarification": "Вызывать эмоциональный отклик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6554fd17-e142-3173-b50a-02e690a2f3bf",
                    "sense_definition": "Iets niet weten te bemachtigen of mislopen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de grote drukte <w>grepen</w> we helaas naast de laatste kaartjes.",
                            "target_lang_translations": {
                                "en": "Due to the large crowd, we unfortunately <w>missed out</w> on the last tickets.",
                                "ru": "Из-за большой толпы мы, к сожалению, <w>упустили</w> последние билеты."
                            }
                        },
                        {
                            "text": "De sporter deed zijn best, maar <w>greep</w> net naast de medailles.",
                            "target_lang_translations": {
                                "en": "The athlete did his best, but just <w>missed out</w> on the medals.",
                                "ru": "Спортсмен старался изо всех сил, но <w>упустил</w> медаль."
                            }
                        }
                    ],
                    "synonyms": [
                        "mislopen",
                        "missen"
                    ],
                    "common_phrases": [
                        "naast de pot grijpen",
                        "mis grijpen",
                        "naast de prijzen grijpen"
                    ],
                    "target_lang_definitions": {
                        "en": "To fail to obtain something; to miss out.",
                        "ru": "Потерпеть неудачу в получении чего-либо; упустить возможность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "miss out",
                                "target_lang_sense_clarification": "To fail to get something desirable"
                            },
                            {
                                "target_lang_word": "miss",
                                "target_lang_sense_clarification": "To fail to obtain"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "упустить",
                                "target_lang_sense_clarification": "Не суметь воспользоваться или получить"
                            },
                            {
                                "target_lang_word": "остаться без",
                                "target_lang_sense_clarification": "Не получить желаемого"
                            }
                        ]
                    }
                }
            ]
        },

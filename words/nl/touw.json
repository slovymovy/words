            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0cf63fa-c129-3382-9b12-e1a2ea3118df",
                    "sense_definition": "Een lang, flexibel koord van in elkaar gedraaide strengen, gebruikt om dingen vast te binden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "rope_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen springen op het schoolplein met een lang <w>touw</w>.",
                            "target_lang_translations": {
                                "en": "The children are jumping in the schoolyard with a long <w>rope</w>.",
                                "ru": "Дети прыгают на школьном дворе через длинную <w>верёвку</w>."
                            }
                        },
                        {
                            "text": "Zonder een stevig <w>touw</w> kunnen we de lading niet vastzetten.",
                            "target_lang_translations": {
                                "en": "Without a sturdy <w>rope</w>, we cannot secure the load.",
                                "ru": "Без прочной <w>верёвки</w> мы не сможем закрепить груз."
                            }
                        }
                    ],
                    "synonyms": [
                        "koord"
                    ],
                    "common_phrases": [
                        "de touwtjes in handen",
                        "op touw zetten",
                        "geen touw aan vast te knopen"
                    ],
                    "target_lang_definitions": {
                        "en": "A long, flexible cord of twisted strands, used for tying things.",
                        "ru": "Длинный гибкий шнур из скрученных прядей, используемый для связывания предметов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rope",
                                "target_lang_sense_clarification": "thick cordage made of twisted strands"
                            },
                            {
                                "target_lang_word": "cord",
                                "target_lang_sense_clarification": "thinner than a rope, often used for binding"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "верёвка",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "канат",
                                "target_lang_sense_clarification": "толстая, прочная верёвка"
                            },
                            {
                                "target_lang_word": "трос",
                                "target_lang_sense_clarification": "металлический или очень прочный канат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e204bcf7-ad88-38c0-93b1-cba2eb9ef98a",
                    "sense_definition": "Verkorte vorm van <w>getouw</w>.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4249aa55-213a-3da2-8015-3df714c30319",
                    "sense_definition": "Een doorgang of smalle ruimte tussen twee gebouwen of kamers waar men doorheen kan lopen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De smalle <w>doorloop</w> naar de achtertuin was overwoekerd met klimop.",
                            "target_lang_translations": {
                                "en": "The narrow <w>passage</w> to the backyard was overgrown with ivy."
                            }
                        },
                        {
                            "text": "Houd de <w>doorloop</w> vrij zodat mensen er gemakkelijk langs kunnen.",
                            "target_lang_translations": {
                                "en": "Keep the <w>walkway</w> clear so people can pass easily."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorgang",
                        "passage",
                        "gangpad"
                    ],
                    "antonyms": [
                        "afsluiting",
                        "blokkade"
                    ],
                    "common_phrases": [
                        "smalle doorloop",
                        "vrije doorloop"
                    ],
                    "target_lang_definitions": {
                        "en": "A passage or narrow space between two buildings or rooms."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "passage",
                                "target_lang_sense_clarification": "walkway"
                            },
                            {
                                "target_lang_word": "walkway",
                                "target_lang_sense_clarification": "path"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5d64e45-8870-35e9-b011-8a5a0788de64",
                    "sense_definition": "Het volledig en zonder onderbreking oefenen van een toneelstuk of muziekuitvoering.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "theater",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de technische <w>doorloop</w> werd vooral gelet op het licht en geluid.",
                            "target_lang_translations": {
                                "en": "During the technical <w>run-through</w>, attention was mainly paid to the lighting and sound."
                            }
                        },
                        {
                            "text": "De regisseur was niet tevreden na de eerste <w>doorloop</w> van de tweede akte.",
                            "target_lang_translations": {
                                "en": "The director was not satisfied after the first <w>run-through</w> of the second act."
                            }
                        }
                    ],
                    "synonyms": [
                        "repetitie",
                        "generale"
                    ],
                    "common_phrases": [
                        "technische doorloop",
                        "eerste doorloop"
                    ],
                    "target_lang_definitions": {
                        "en": "A complete, uninterrupted rehearsal of a performance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run-through",
                                "target_lang_sense_clarification": "rehearsal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f090dc6-c81a-3210-a121-ae06d985cc80",
                    "sense_definition": "Een aandoening waarbij men last heeft van frequente en dunne ontlasting.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "medical_informal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij bleef thuis omdat hij last had van een zware <w>doorloop</w>.",
                            "target_lang_translations": {
                                "en": "He stayed home because he was suffering from a bad case of <w>the runs</w>."
                            }
                        },
                        {
                            "text": "Oude huismiddeltjes tegen de <w>doorloop</w> zijn niet altijd effectief.",
                            "target_lang_translations": {
                                "en": "Old home remedies for <w>the runs</w> are not always effective."
                            }
                        }
                    ],
                    "synonyms": [
                        "diarree",
                        "buikloop"
                    ],
                    "antonyms": [
                        "verstopping",
                        "constipatie"
                    ],
                    "target_lang_definitions": {
                        "en": "An informal term for diarrhea."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the runs",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "diarrhea",
                                "target_lang_sense_clarification": "medical"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0223846a-6aae-33ba-9cb0-9f3fcb082911",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>doorlopen</w> (een proces, ruimte of opleiding geheel doorgaan).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_inseparable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>doorloop</w> momenteel een intensief trainingsprogramma voor mijn nieuwe baan.",
                            "target_lang_translations": {
                                "en": "I am currently <w>going through</w> an intensive training program for my new job."
                            }
                        },
                        {
                            "text": "Elke ochtend <w>doorloop</w> ik mijn vaste routine voordat ik begin met werken.",
                            "target_lang_translations": {
                                "en": "Every morning I <w>go through</w> my fixed routine before I start working."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To go through or undergo a process or program."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go through",
                                "target_lang_sense_clarification": "process"
                            },
                            {
                                "target_lang_word": "undergo",
                                "target_lang_sense_clarification": "experience"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9ac2bd3-1d77-3cdc-a1e2-4deae910b756",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd (in een bijzin) van <w>doorlopen</w> (verder lopen of doorgaan met lopen).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_separable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is belangrijk dat ik stevig <w>doorloop</w> om de trein nog te halen.",
                            "target_lang_translations": {
                                "en": "It is important that I <w>walk on</w> briskly to still catch the train."
                            }
                        },
                        {
                            "text": "Als ik niet <w>doorloop</w>, kom ik zeker te laat op mijn afspraak.",
                            "target_lang_translations": {
                                "en": "If I don't <w>keep walking</w>, I will definitely be late for my appointment."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To walk on or keep walking."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "walk on",
                                "target_lang_sense_clarification": "continue walking"
                            },
                            {
                                "target_lang_word": "keep walking",
                                "target_lang_sense_clarification": "continue movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "678c68df-4e17-3f0d-8e23-828c46ad536d",
                    "sense_definition": "Gebiedende wijs van <w>doorlopen</w> (een proces of stappenplan volgen).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_inseparable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Doorloop</w> de handleiding stap voor stap om fouten te voorkomen.",
                            "target_lang_translations": {
                                "en": "<w>Go through</w> the manual step by step to avoid errors."
                            }
                        },
                        {
                            "text": "<w>Doorloop</w> het menu om bij de juiste instellingen te komen.",
                            "target_lang_translations": {
                                "en": "<w>Go through</w> the menu to get to the correct settings."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To go through or follow specific steps."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go through",
                                "target_lang_sense_clarification": "steps/instructions"

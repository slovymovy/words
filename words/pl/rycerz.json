            "pos": "noun",
            "senses": [
                {
                    "sense_id": "594f389e-20f0-3d96-808b-0e1c7ca87edc",
                    "sense_definition": "Członek uprzywilejowanego stanu żołnierskiego w średniowieczu, walczący zazwyczaj konno",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Średniowieczny <w>rycerz</w> nosił ciężką zbroję i walczył mieczem.",
                            "target_lang_translations": {
                                "ru": "Средневековый <w>рыцарь</w> носил тяжелые доспехи и сражался мечом."
                            }
                        },
                        {
                            "text": "Król postanowił uroczyście pasować dzielnego giermka na <w>rycerza</w>.",
                            "target_lang_translations": {
                                "ru": "Король решил торжественно посвятить храброго оруженосца в <w>рыцари</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "błędny rycerz",
                        "pasowanie na rycerza",
                        "turniej rycerski",
                        "zbroja rycerza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Член привилегированного военного сословия в Средние века, обычно сражавшийся верхом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыцарь",
                                "target_lang_sense_clarification": "средневековый воин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "975c680f-29f4-38f9-b065-2b50c53992db",
                    "sense_definition": "Mężczyzna odznaczający się szlachetnością, odwagą i uprzejmością wobec kobiet",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój mąż zachował się jak prawdziwy <w>rycerz</w>, pomagając mi z ciężkimi zakupami.",
                            "target_lang_translations": {
                                "ru": "Мой муж повел себя как настоящий <w>рыцарь</w>, помогая мне с тяжелыми покупками."
                            }
                        },
                        {
                            "text": "Wiele kobiet wciąż marzy, że przyjedzie po nie <w>rycerz</w> na białym koniu.",
                            "target_lang_translations": {
                                "ru": "Многие женщины все еще мечтают, что за ними приедет <w>рыцарь</w> на белом коне."
                            }
                        }
                    ],
                    "synonyms": [
                        "dżentelmen",
                        "gentleman"
                    ],
                    "common_phrases": [
                        "rycerz na białym koniu",
                        "prawdziwy rycerz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, отличающийся благородством, смелостью и учтивостью по отношению к женщинам"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыцарь",
                                "target_lang_sense_clarification": "благородный человек"
                            },
                            {
                                "target_lang_word": "джентльмен",
                                "target_lang_sense_clarification": "вежливый мужчина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd0f2d3b-87cd-3e78-a1ce-8fe54baff0cb",
                    "sense_definition": "Tytuł nadawany członkom niektórych zakonów lub stowarzyszeń",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Został on mianowany <w>rycerzem</w> Zakonu Maltańskiego za swoje zasługi.",
                            "target_lang_translations": {
                                "ru": "За свои заслуги он стал <w>рыцарем</w> Мальтийского ордена."
                            }
                        },
                        {
                            "text": "W uroczystości wzięli udział <w>rycerze</w> w odświętnych strojach zakonnych.",
                            "target_lang_translations": {
                                "ru": "В церемонии приняли участие <w>рыцари</w> в праздничных орденских одеяниях."
                            }
                        }
                    ],
                    "synonyms": [
                        "kawaler"
                    ],
                    "common_phrases": [
                        "rycerz zakonu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул, присваиваемый членам некоторых орденов или обществ"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыцарь",
                                "target_lang_sense_clarification": "член ордена"
                            },
                            {
                                "target_lang_word": "кавалер",
                                "target_lang_sense_clarification": "обладатель ордена"
                            }
                        ]
                    }
                }
            ]

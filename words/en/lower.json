    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ee7cd87a-2592-3a91-8194-c3618ecb0547",
                    "sense_definition": "To move something to a lower position; to let something go down.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_move_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>lower</w> the blinds to keep the room cool.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>опустите</w> жалюзи, чтобы в комнате было прохладно."
                            }
                        },
                        {
                            "text": "They used a rope to <w>lower</w> the basket to the ground.",
                            "target_lang_translations": {
                                "ru": "Они использовали веревку, чтобы <w>спустить</w> корзину на землю."
                            }
                        }
                    ],
                    "synonyms": [
                        "drop",
                        "bring down"
                    ],
                    "antonyms": [
                        "upper",
                        "higher"
                    ],
                    "common_phrases": [
                        "lower the blinds",
                        "lower a boat",
                        "lower the flag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать что-либо вниз или давать опуститься."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опускать",
                                "target_lang_sense_clarification": "перемещать вниз"
                            },
                            {
                                "target_lang_word": "спускать",
                                "target_lang_sense_clarification": "перемещать вниз, часто плавно или по наклонной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a777c628-1115-3a9b-ae7f-806f2c42ca4b",
                    "sense_definition": "To reduce the amount, degree, intensity, or value of something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_reduce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shop decided to <w>lower</w> the price of all winter clothes.",
                            "target_lang_translations": {
                                "ru": "Магазин решил <w>снизить</w> цены на всю зимнюю одежду."
                            }
                        },
                        {
                            "text": "Please <w>lower</w> your voice because the baby is sleeping.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>понизьте</w> голос, потому что ребенок спит."
                            }
                        }
                    ],
                    "synonyms": [
                        "reduce",
                        "lessen",
                        "turn down",
                        "cut",
                        "shorten"
                    ],
                    "antonyms": [
                        "higher"
                    ],
                    "common_phrases": [
                        "lower the price",
                        "lower the volume",
                        "lower the risk",
                        "lower standards"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшать количество, степень, интенсивность или стоимость чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снижать",
                                "target_lang_sense_clarification": "уменьшать (цену, скорость, высоту)"
                            },
                            {
                                "target_lang_word": "понижать",
                                "target_lang_sense_clarification": "делать более низким (голос, уровень, температуру)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c22e7882-31c7-3532-87f8-d28e24289f02",
                    "sense_definition": "To become less or smaller in amount, level, or intensity.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_decrease_intransitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The water level in the river <w>lowered</w> after the rain stopped.",
                            "target_lang_translations": {
                                "ru": "Уровень воды в реке <w>понизился</w> после того, как дождь прекратился."
                            }
                        },
                        {
                            "text": "As the sun <w>lowered</w> in the sky, the shadows grew longer.",
                            "target_lang_translations": {
                                "ru": "Когда солнце <w>опустилось</w> ниже, тени стали длиннее."
                            }
                        }
                    ],
                    "synonyms": [
                        "drop",
                        "fall",
                        "shrink",
                        "die off"
                    ],
                    "antonyms": [
                        "higher"
                    ],
                    "common_phrases": [
                        "the water lowered",
                        "the sun lowered"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться меньше или ниже по уровню, количеству или интенсивности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опускаться",
                                "target_lang_sense_clarification": "физически перемещаться вниз"
                            },
                            {
                                "target_lang_word": "снижаться",
                                "target_lang_sense_clarification": "уменьшаться (о показателях)"
                            },
                            {
                                "target_lang_word": "понижаться",
                                "target_lang_sense_clarification": "становиться ниже (об уровне)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d0fca86-6e43-387d-bf77-591b1960273b",
                    "sense_definition": "To behave in a way that is considered below one's dignity or standards.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_humble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will not <w>lower</w> myself to argue with someone so rude.",
                            "target_lang_translations": {
                                "ru": "Я не буду <w>унижаться</w> до спора с таким грубияном."
                            }
                        },
                        {
                            "text": "He felt he had to <w>lower</w> his standards to get the job.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, что ему пришлось <w>снизить</w> свои требования, чтобы получить работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "be humble"
                    ],
                    "common_phrases": [
                        "lower oneself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя недостойно, ронять свое достоинство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "унижаться",
                                "target_lang_sense_clarification": "ронять достоинство"
                            },
                            {
                                "target_lang_word": "опускаться",
                                "target_lang_sense_clarification": "морально деградировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b75669dc-ec43-39ee-9923-e489d2d393be",
                    "sense_definition": "To look angry, sullen, or threatening, especially of the sky or a face (alternative spelling of lour).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_lour",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dark clouds <w>lowered</w> over the castle, promising a storm.",
                            "target_lang_translations": {
                                "ru": "Темные тучи <w>нависли</w> над замком, предвещая бурю."
                            }
                        },
                        {
                            "text": "He sat in the corner and <w>lowered</w> at everyone who passed.",
                            "target_lang_translations": {
                                "ru": "Он сидел в углу и <w>хмуро смотрел</w> на всех проходящих."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clouds lowered"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Смотреть угрюмо или угрожающе; нависать (о тучах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нависать",
                                "target_lang_sense_clarification": "о тучах или угрозе"
                            },
                            {
                                "target_lang_word": "хмуриться",
                                "target_lang_sense_clarification": "о лице"
                            }
                        ]
                    }
                }
            ]
        },
        {

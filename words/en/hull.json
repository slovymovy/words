    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6091fe2d-a230-37b0-b5fc-c40ca809d4c8",
                    "sense_definition": "The main body or frame of a ship, boat, or aircraft, excluding masts, sails, engines, and rigging.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The waves crashed violently against the steel <w>hull</w> of the cargo ship.",
                            "target_lang_translations": {
                                "ru": "Волны яростно бились о стальной <w>корпус</w> грузового судна."
                            }
                        },
                        {
                            "text": "Engineers inspected the aircraft <w>hull</w> for any signs of structural damage.",
                            "target_lang_translations": {
                                "ru": "Инженеры осмотрели <w>фюзеляж</w> самолета на предмет структурных повреждений."
                            }
                        }
                    ],
                    "synonyms": [
                        "fuselage"
                    ],
                    "common_phrases": [
                        "ship's hull",
                        "wooden hull",
                        "steel hull",
                        "double hull"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная часть или каркас судна, лодки или самолета, исключая мачты, паруса, двигатели и оснастку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корпус",
                                "target_lang_sense_clarification": "судна или механизма"
                            },
                            {
                                "target_lang_word": "фюзеляж",
                                "target_lang_sense_clarification": "самолета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae03b1df-3603-344c-9003-14493321f578",
                    "sense_definition": "The dry outer covering of a fruit, seed, or nut.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "seed_covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The machine separates the rice grain from its hard protective <w>hull</w>.",
                            "target_lang_translations": {
                                "ru": "Машина отделяет рисовое зерно от его твердой защитной <w>шелухи</w>."
                            }
                        },
                        {
                            "text": "You should remove the green <w>hull</w> from the strawberry before eating it.",
                            "target_lang_translations": {
                                "ru": "Перед едой следует удалить с клубники зеленый <w>хвостик</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "husk",
                        "shell",
                        "peel"
                    ],
                    "common_phrases": [
                        "rice hull",
                        "corn hull",
                        "seed hull"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сухая внешняя оболочка плода, семени или ореха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шелуха",
                                "target_lang_sense_clarification": "зерна или семян"
                            },
                            {
                                "target_lang_word": "скорлупа",
                                "target_lang_sense_clarification": "ореха или яйца"
                            },
                            {
                                "target_lang_word": "лузга",
                                "target_lang_sense_clarification": "семечек"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0cbca41f-63aa-34e0-9c7e-a360ab82c7be",
                    "sense_definition": "To remove the outer covering, stem, or shell from a fruit, seed, or nut.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "seed_covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It took us nearly an hour to <w>hull</w> all the strawberries for the jam.",
                            "target_lang_translations": {
                                "ru": "У нас ушел почти час, чтобы <w>очистить</w> всю клубнику для варенья."
                            }
                        },
                        {
                            "text": "Farmers use specialized equipment to <w>hull</w> the corn efficiently.",
                            "target_lang_translations": {
                                "ru": "Фермеры используют специальное оборудование, чтобы эффективно <w>лущить</w> кукурузу."
                            }
                        }
                    ],
                    "synonyms": [
                        "shell",
                        "husk",
                        "shuck",
                        "peel"
                    ],
                    "common_phrases": [
                        "hull strawberries",
                        "hull peas",
                        "hull corn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалять внешнюю оболочку, стебель или скорлупу с плода, семени или ореха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лущить",
                                "target_lang_sense_clarification": "горох, кукурузу или семечки"
                            },
                            {
                                "target_lang_word": "чистить",
                                "target_lang_sense_clarification": "ягоды или фрукты"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [
        "kwalitatief",
        "kwalificatie",
        "kwalificeren"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "09dfe020-deea-35b0-90aa-1e7608211312",
                    "sense_definition": "De mate waarin iets goed, deugdelijk of uitmuntend is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_excellence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>kwaliteit</w> van het eten in dit restaurant is uitstekend.",
                            "target_lang_translations": {
                                "en": "The <w>quality</w> of the food in this restaurant is excellent."
                            }
                        },
                        {
                            "text": "Wij streven altijd naar een hoge <w>kwaliteit</w> van onze producten.",
                            "target_lang_translations": {
                                "en": "We always strive for high <w>quality</w> in our products."
                            }
                        },
                        {
                            "text": "Na een paar keer wassen ging de trui kapot, want de <w>kwaliteit</w> was slecht.",
                            "target_lang_translations": {
                                "en": "After washing a few times the sweater fell apart, because the <w>quality</w> was poor."
                            }
                        }
                    ],
                    "synonyms": [
                        "niveau",
                        "peil",
                        "klasse",
                        "deugdelijkheid"
                    ],
                    "common_phrases": [
                        "hoge kwaliteit",
                        "goede kwaliteit",
                        "slechte kwaliteit",
                        "prijs en kwaliteit"
                    ],
                    "target_lang_definitions": {
                        "en": "The degree to which something is good, sound, or excellent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quality",
                                "target_lang_sense_clarification": "level of excellence"
                            },
                            {
                                "target_lang_word": "standard",
                                "target_lang_sense_clarification": "level of quality"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d624d125-ddfe-3c94-b257-f9ecb710d312",
                    "sense_definition": "Een kenmerkende, vaak positieve eigenschap of karaktertrek van iemand of iets.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_characteristic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geduld is een belangrijke <w>kwaliteit</w> voor een leraar.",
                            "target_lang_translations": {
                                "en": "Patience is an important <w>quality</w> for a teacher."
                            }
                        },
                        {
                            "text": "Zij heeft veel leidinggevende <w>kwaliteiten</w> die goed van pas komen.",
                            "target_lang_translations": {
                                "en": "She has many leadership <w>qualities</w> that come in handy."
                            }
                        }
                    ],
                    "synonyms": [
                        "eigenschap",
                        "kenmerk",
                        "trek"
                    ],
                    "antonyms": [
                        "gebrek"
                    ],
                    "common_phrases": [
                        "persoonlijke kwaliteiten",
                        "bijzondere kwaliteit"
                    ],
                    "target_lang_definitions": {
                        "en": "A characteristic, often positive attribute or trait of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quality",
                                "target_lang_sense_clarification": "personal attribute"
                            },
                            {
                                "target_lang_word": "characteristic",
                                "target_lang_sense_clarification": "distinguishing feature"
                            },
                            {
                                "target_lang_word": "trait",
                                "target_lang_sense_clarification": "character attribute"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfb3f8a2-a2fd-334b-9632-901ea3387096",
                    "sense_definition": "De functie, rol of hoedanigheid waarin iemand optreedt of handelt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_capacity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij ondertekende het document in zijn <w>kwaliteit</w> als voorzitter.",
                            "target_lang_translations": {
                                "en": "He signed the document in his <w>capacity</w> as chairman."
                            }
                        },
                        {
                            "text": "De burgemeester was aanwezig in haar <w>kwaliteit</w> van hoofd van de politie.",
                            "target_lang_translations": {
                                "en": "The mayor was present in her <w>capacity</w> as head of the police."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoedanigheid",
                        "functie",
                        "rol"
                    ],
                    "common_phrases": [
                        "in de kwaliteit van",
                        "in zijn kwaliteit",
                        "in haar kwaliteit"
                    ],
                    "target_lang_definitions": {
                        "en": "The function, role, or capacity in which someone acts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "capacity",
                                "target_lang_sense_clarification": "role or position"
                            },
                            {
                                "target_lang_word": "role",
                                "target_lang_sense_clarification": "function"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d8b2a24-b4a4-39b7-b032-1b5215ba33a8",
                    "sense_definition": "Het verschil in waarde in het schaakspel tussen een toren en een lichter stuk zoals een loper of paard.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_chess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De grootmeester offerde een <w>kwaliteit</w> om een sterke aanval op te zetten.",
                            "target_lang_translations": {
                                "en": "The grandmaster sacrificed the <w>exchange</w> to set up a strong attack."
                            }
                        },
                        {
                            "text": "Hij stond een <w>kwaliteit</w> voor, maar verloor uiteindelijk toch de partij.",
                            "target_lang_translations": {
                                "en": "He was up the <w>exchange</w>, but eventually lost the game anyway."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kwaliteit offeren",
                        "kwaliteit winnen"
                    ],
                    "target_lang_definitions": {
                        "en": "The difference in value in chess between a rook and a minor piece like a bishop or knight."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exchange",
                                "target_lang_sense_clarification": "chess term"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
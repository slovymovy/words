{
    "word_family": [
        "captive",
        "captor",
        "captivity",
        "recapture"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "83e7ba81-1c8a-372f-90ba-a71d4685db93",
                    "sense_definition": "To take control of a person, place, or thing by force or strategy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The army managed to <w>capture</w> the enemy base after a long battle.",
                            "target_lang_translations": {
                                "ru": "Армии удалось <w>захватить</w> вражескую базу после долгого сражения."
                            }
                        },
                        {
                            "text": "Police hope to <w>capture</w> the suspect before he leaves the city.",
                            "target_lang_translations": {
                                "ru": "Полиция надеется <w>поймать</w> подозреваемого до того, как он покинет город."
                            }
                        }
                    ],
                    "synonyms": [
                        "seize",
                        "apprehend",
                        "catch",
                        "grab",
                        "take hold"
                    ],
                    "antonyms": [
                        "release",
                        "liberate",
                        "let go"
                    ],
                    "common_phrases": [
                        "capture a soldier",
                        "capture the flag",
                        "capture territory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать под контроль человека, место или предмет силой или хитростью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "захватывать",
                                "target_lang_sense_clarification": "силой или стратегией"
                            },
                            {
                                "target_lang_word": "брать",
                                "target_lang_sense_clarification": "город, крепость или базу"
                            },
                            {
                                "target_lang_word": "ловить",
                                "target_lang_sense_clarification": "преступника или беглеца"
                            },
                            {
                                "target_lang_word": "брать в плен",
                                "target_lang_sense_clarification": "солдата или врага"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce21c2b6-6f16-33a7-9b7e-1b379e52db1e",
                    "sense_definition": "To record sound, images, or data to keep them for later use.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The photographer used a special lens to <w>capture</w> the bird in flight.",
                            "target_lang_translations": {
                                "ru": "Фотограф использовал специальный объектив, чтобы <w>запечатлеть</w> птицу в полете."
                            }
                        },
                        {
                            "text": "This software can <w>capture</w> audio from the microphone very clearly.",
                            "target_lang_translations": {
                                "ru": "Это программное обеспечение может очень четко <w>записывать</w> звук с микрофона."
                            }
                        }
                    ],
                    "common_phrases": [
                        "capture an image",
                        "capture video",
                        "capture data"
                    ],
                    "target_lang_definitions": {
                        "ru": "Записывать звук, изображения или данные для последующего использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запечатлевать",
                                "target_lang_sense_clarification": "изображение или момент"
                            },
                            {
                                "target_lang_word": "записывать",
                                "target_lang_sense_clarification": "аудио или данные"
                            },
                            {
                                "target_lang_word": "фиксировать",
                                "target_lang_sense_clarification": "сохранять информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9597c0b4-663f-3a36-a096-ff947a02af2b",
                    "sense_definition": "To represent or describe the character or atmosphere of something accurately in art or speech.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The painting really <w>captures</w> the beauty of the sunset.",
                            "target_lang_translations": {
                                "ru": "Картина действительно <w>передает</w> красоту заката."
                            }
                        },
                        {
                            "text": "Her novel <w>captured</w> the spirit of life in the 1920s.",
                            "target_lang_translations": {
                                "ru": "Ее роман <w>отразил</w> дух жизни 1920-х годов."
                            }
                        }
                    ],
                    "synonyms": [
                        "catch"
                    ],
                    "common_phrases": [
                        "capture the essence",
                        "capture the spirit",
                        "capture the mood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точно передавать или описывать характер или атмосферу чего-либо в искусстве или речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "суть или атмосферу"
                            },
                            {
                                "target_lang_word": "улавливать",
                                "target_lang_sense_clarification": "тонкий смысл или настроение"
                            },
                            {
                                "target_lang_word": "отражать",
                                "target_lang_sense_clarification": "показывать в творчестве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a5db352-4e08-3011-b5f9-40af8ef0ce95",
                    "sense_definition": "To gain and hold someone's interest, attention, or imagination.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The exciting story <w>captured</w> the children's imagination completely.",
                            "target_lang_translations": {
                                "ru": "Захватывающая история полностью <w>захватила</w> воображение детей."
                            }
                        },
                        {
                            "text": "Her speech <w>captured</w> the attention of the entire audience.",
                            "target_lang_translations": {
                                "ru": "Ее речь <w>приковала</w> внимание всей аудитории."
                            }
                        }
                    ],
                    "synonyms": [
                        "grab",
                        "catch",
                        "seize"
                    ],
                    "common_phrases": [
                        "capture attention",
                        "capture imagination",
                        "capture interest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Завладевать чьим-либо интересом, вниманием или воображением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "захватывать",
                                "target_lang_sense_clarification": "воображение или дух"
                            },
                            {
                                "target_lang_word": "приковывать",
                                "target_lang_sense_clarification": "внимание"
                            },
                            {
                                "target_lang_word": "пленять",
                                "target_lang_sense_clarification": "очаровывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "478bc89e-e599-3cd9-b804-72b9f102b2ad",
                    "sense_definition": "To remove an opponent's piece from the board in a game like chess.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He used his knight to <w>capture</w> my bishop.",
                            "target_lang_translations": {
                                "ru": "Он использовал своего коня, чтобы <w>взять</w> моего слона."
                            }
                        },
                        {
                            "text": "In chess, you must think ahead before you <w>capture</w> a piece.",
                            "target_lang_translations": {
                                "ru": "В шахматах нужно думать наперед, прежде чем <w>брать</w> фигуру."
                            }
                        }
                    ],
                    "common_phrases": [
                        "capture a pawn",
                        "capture the queen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снимать фигуру противника с доски в таких играх, как шахматы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брать",
                                "target_lang_sense_clarification": "фигуру в игре"
                            },
                            {
                                "target_lang_word": "съедать",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5b328219-ed77-36eb-a5ce-475ebe377948",
                    "sense_definition": "The act of taking someone or something by force or control.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The criminal managed to evade <w>capture</w> for several weeks.",
                            "target_lang_translations": {
                                "ru": "Преступнику удавалось избегать <w>поимки</w> в течение нескольких недель."
                            }

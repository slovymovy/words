{
    "word_family": [
        "notify",
        "notifiable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0a4e88f4-9ac8-317d-a9a6-22fbaab5fa12",
                    "sense_definition": "A message, alert, or signal displayed by a system or application to inform the user of an event, update, or new message.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>notification</w> popped up on my computer screen while I was working.",
                            "target_lang_translations": {
                                "ru": "Во время работы на экране моего компьютера всплыло <w>уведомление</w>."
                            }
                        },
                        {
                            "text": "You can change your settings if you receive a <w>notification</w> too often.",
                            "target_lang_translations": {
                                "ru": "Вы можете изменить настройки, если получаете <w>уведомления</w> слишком часто."
                            }
                        }
                    ],
                    "synonyms": [
                        "alert"
                    ],
                    "common_phrases": [
                        "push notification",
                        "email notification",
                        "receive a notification",
                        "turn off notifications"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщение, предупреждение или сигнал, отображаемый системой или приложением для информирования пользователя о событии или обновлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уведомление",
                                "target_lang_sense_clarification": "сообщение от программы или устройства"
                            },
                            {
                                "target_lang_word": "оповещение",
                                "target_lang_sense_clarification": "сигнал тревоги или предупреждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75f209d3-b347-3c73-9c92-09d099baea04",
                    "sense_definition": "A specific piece of information or a formal document that serves to notify someone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tenants received a written <w>notification</w> regarding the rent increase.",
                            "target_lang_translations": {
                                "ru": "Жильцы получили письменное <w>уведомление</w> о повышении арендной платы."
                            }
                        },
                        {
                            "text": "Please send an official <w>notification</w> to the bank about your change of address.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отправьте в банк официальное <w>уведомление</w> о смене вашего адреса."
                            }
                        }
                    ],
                    "synonyms": [
                        "notice",
                        "announcement"
                    ],
                    "common_phrases": [
                        "written notification",
                        "official notification",
                        "prior notification"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная информация или официальный документ, служащий для уведомления кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извещение",
                                "target_lang_sense_clarification": "официальный документ, например, почтовое извещение"
                            },
                            {
                                "target_lang_word": "уведомление",
                                "target_lang_sense_clarification": "официальное письменное сообщение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c166739-4ea0-3c48-89df-587d20da34e0",
                    "sense_definition": "The act of formally telling someone about something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>notification</w> of the winner will take place after the votes are counted.",
                            "target_lang_translations": {
                                "ru": "<w>Уведомление</w> победителя произойдёт после подсчёта голосов."
                            }
                        },
                        {
                            "text": "Immediate <w>notification</w> of any safety hazards is required by company policy.",
                            "target_lang_translations": {
                                "ru": "Политика компании требует немедленного <w>оповещения</w> о любых угрозах безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "upon notification",
                        "notification of death"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по официальному сообщению кому-либо о чём-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оповещение",
                                "target_lang_sense_clarification": "процесс массового информирования"
                            },
                            {
                                "target_lang_word": "уведомление",
                                "target_lang_sense_clarification": "процесс информирования конкретных лиц"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as a term of endearment"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A source of joy, happiness, or life (usually referring to a person or phenomenon)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sunshine",
                                "target_lang_sense_clarification": "term of endearment"
                            },
                            {
                                "target_lang_word": "sun",
                                "target_lang_sense_clarification": "source of joy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb21e73f-3fb8-3894-85ac-e7209a87731c",

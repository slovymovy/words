                            {
                                "target_lang_word": "крепкий",
                                "target_lang_sense_clarification": "сильный, надежный"
                            },
                            {
                                "target_lang_word": "надёжный",
                                "target_lang_sense_clarification": "на который можно положиться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "sturdy",
                                "target_lang_sense_clarification": "of objects, buildings, or materials"
                            },
                            {
                                "target_lang_word": "firm",
                                "target_lang_sense_clarification": "secure, tight, or unyielding"
                            },
                            {
                                "target_lang_word": "solid",
                                "target_lang_sense_clarification": "substantial and reliable"
                            }
                        ]

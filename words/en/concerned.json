    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6ab43d0d-5d96-33eb-8fec-e815a97a7a7b",
                    "sense_definition": "Worried, troubled, or anxious about something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was a <w>concerned</w> expression on her face as I told her the news.",
                            "target_lang_translations": {
                                "ru": "Когда я сообщил ей новости, на её лице было <w>обеспокоенное</w> выражение."
                            }
                        },
                        {
                            "text": "She is deeply <w>concerned</w> about the health of her elderly parents.",
                            "target_lang_translations": {
                                "ru": "Она глубоко <w>обеспокоена</w> здоровьем своих пожилых родителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "worried",
                        "anxious",
                        "troubled"
                    ],
                    "antonyms": [
                        "unconcerned",
                        "indifferent"
                    ],
                    "common_phrases": [
                        "concerned about",
                        "deeply concerned",
                        "concerned citizen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий беспокойство, тревогу или волнение по какому-либо поводу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обеспокоенный",
                                "target_lang_sense_clarification": "испытывающий чувство тревоги или беспокойства"
                            },
                            {
                                "target_lang_word": "встревоженный",
                                "target_lang_sense_clarification": "сильно беспокоящийся, взволнованный"
                            },
                            {
                                "target_lang_word": "озабоченный",
                                "target_lang_sense_clarification": "поглощённый заботами или проблемами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0af41782-4360-32a9-999c-61e3da383a5b",
                    "sense_definition": "Involved in something or having a responsibility regarding it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "involvement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The people <w>concerned</w> have been punished for their actions.",
                            "target_lang_translations": {
                                "ru": "<w>Причастные</w> к этому люди были наказаны за свои действия."
                            }
                        },
                        {
                            "text": "We need to arrange a meeting with all the parties <w>concerned</w>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно организовать встречу со всеми <w>заинтересованными</w> сторонами."
                            }
                        }
                    ],
                    "synonyms": [
                        "involved",
                        "implicated"
                    ],
                    "antonyms": [
                        "unrelated",
                        "uninvolved"
                    ],
                    "common_phrases": [
                        "parties concerned",
                        "people concerned",
                        "everyone concerned"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вовлечённый во что-либо или имеющий отношение к делу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заинтересованный",
                                "target_lang_sense_clarification": "имеющий интерес или долю в деле (о сторонах, лицах)"
                            },
                            {
                                "target_lang_word": "причастный",
                                "target_lang_sense_clarification": "имеющий непосредственное отношение к событию или действию"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [

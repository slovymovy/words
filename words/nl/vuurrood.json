            "senses": [
                {
                    "sense_id": "325b2a7e-6a34-3d1c-8073-8b8a2c86d9e6",
                    "sense_definition": "Zeer fel rood van kleur, vergelijkbaar met de kleur van vuur.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "color_shade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij rijdt in een <w>vuurrode</w> auto.",
                            "target_lang_translations": {
                                "en": "He drives a <w>fire-red</w> car.",
                                "ru": "Он ездит на <w>огненно-красной</w> машине."
                            }
                        },
                        {
                            "text": "De zijkant van het gebouw was een egaal <w>vuurrood</w> vlak.",
                            "target_lang_translations": {
                                "en": "The side of the building was a solid <w>fire-red</w> surface.",
                                "ru": "Боковая сторона здания представляла собой сплошную <w>огненно-красную</w> поверхность."
                            }
                        }
                    ],
                    "synonyms": [
                        "brandrood"
                    ],
                    "common_phrases": [
                        "een vuurrode auto",
                        "vuurrood haar"
                    ],
                    "target_lang_definitions": {
                        "en": "Very bright red in color, comparable to the color of fire.",
                        "ru": "Очень яркий красный цвет, напоминающий цвет огня."
                    },
                    "translations": {

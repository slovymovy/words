                {
                    "sense_id": "7bd63aa4-2797-37e7-9b56-154b03c66c6a",
                    "sense_definition": "System umownych znaków graficznych, służący do utrwalania mowy i przekazywania myśli.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "writing_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chińskie <pismo> składa się z tysięcy skomplikowanych znaków.",
                            "target_lang_translations": {
                                "ru": "Китайское <письмо> состоит из тысяч сложных знаков."
                            }
                        },
                        {
                            "text": "Dzieci w pierwszej klasie poznają <pismo> i uczą się czytać.",
                            "target_lang_translations": {
                                "ru": "Дети в первом классе изучают <письмо> и учатся читать."
                            }
                        }
                    ],
                    "synonyms": [
                        "alfabet"
                    ],
                    "antonyms": [
                        "mowa"
                    ],
                    "common_phrases": [
                        "pismo ręczne",
                        "pismo drukowane",
                        "pismo obrazkowe",
                        "pismo techniczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система условных графических знаков для фиксации речи и передачи мыслей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "письмо",
                                "target_lang_sense_clarification": "система знаков"
                            },
                            {
                                "target_lang_word": "письменность",
                                "target_lang_sense_clarification": "совокупность письменных памятников или система письма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b698bb80-91c1-3e5b-9df3-974bc60c9291",
                    "sense_definition": "Indywidualny, charakterystyczny dla danej osoby sposób kształtowania liter podczas pisania ręcznego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "handwriting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz miał tak niewyraźne <pismo>, że farmaceuta nie mógł odczytać recepty.",
                            "target_lang_translations": {
                                "ru": "У врача был такой неразборчивый <почерк>, что фармацевт не мог прочитать рецепт."
                            }
                        },
                        {
                            "text": "Po latach rozpoznałem jej charakterystyczne <pismo> na kopercie.",
                            "target_lang_translations": {
                                "ru": "Спустя годы я узнал её характерный <почерк> на конверте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czytelne pismo",
                        "niewyraźne pismo",
                        "charakter pisma",
                        "ćwiczyć pismo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Индивидуальная манера начертания букв при ручном письме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почерк",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "80e8b04e-824e-3933-82b8-74f7ba2b9092",
                    "sense_definition": "De inhoud, zin of bedoeling van iets weergeven; ergens voor staan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je me uitleggen wat dit moeilijke woord precies <betekent>?",
                            "target_lang_translations": {
                                "en": "Can you explain to me exactly what this difficult word <w>means</w>?",
                                "ru": "Можешь объяснить мне, что именно <w>означает</w> это сложное слово?"
                            }
                        },
                        {
                            "text": "Het rode licht bij het stoplicht <betekent> dat je moet stoppen.",
                            "target_lang_translations": {
                                "en": "The red light at the traffic light <w>means</w> that you have to stop.",
                                "ru": "Красный свет светофора <w>означает</w>, что нужно остановиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "willen zeggen"
                    ],
                    "common_phrases": [
                        "dat betekent dat",
                        "wat betekent dit"
                    ],
                    "target_lang_definitions": {
                        "en": "To represent the content, meaning, or intention of something.",
                        "ru": "Обозначать содержание, смысл или намерение; символизировать что-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mean",
                                "target_lang_sense_clarification": "to convey meaning"
                            },
                            {
                                "target_lang_word": "signify",
                                "target_lang_sense_clarification": "to stand for"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "означать",
                                "target_lang_sense_clarification": "иметь смысл (о словах, знаках)"
                            },
                            {
                                "target_lang_word": "значить",
                                "target_lang_sense_clarification": "иметь смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0da5b9e4-2c82-35a7-939e-2a7a27f50b5d",
                    "sense_definition": "Van belang zijn of een bepaalde emotionele waarde hebben voor iemand.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn steun heeft in die moeilijke periode heel veel voor mij <betekend>.",
                            "target_lang_translations": {
                                "en": "His support <w>meant</w> a lot to me during that difficult period.",
                                "ru": "Его поддержка очень много <w>значила</w> для меня в тот трудный период."
                            }
                        },
                        {
                            "text": "Dat oude horloge van haar grootvader <betekent> alles voor haar.",
                            "target_lang_translations": {
                                "en": "That old watch of her grandfather's <w>means</w> everything to her.",
                                "ru": "Эти старые часы её дедушки <w>значат</w> для неё всё."
                            }
                        }
                    ],
                    "synonyms": [
                        "willen zeggen",
                        "willen zeggen"
                    ],
                    "common_phrases": [
                        "veel betekenen voor",
                        "niets betekenen",
                        "alles betekenen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be of importance or have emotional value to someone.",
                        "ru": "Быть важным или иметь эмоциональную ценность для кого-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mean",
                                "target_lang_sense_clarification": "to be important"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "значить",
                                "target_lang_sense_clarification": "иметь важность, ценность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc938f24-0df1-314b-bd2f-29bd59df39e7",
                    "sense_definition": "Als noodzakelijk gevolg hebben of met zich meebrengen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze nieuwe maatregel <betekent> helaas dat we moeten bezuinigen.",
                            "target_lang_translations": {
                                "en": "Unfortunately, this new measure <w>means</w> that we have to cut costs.",
                                "ru": "Эта новая мера, к сожалению, <w>означает</w>, что нам придется экономить."
                            }
                        },
                        {
                            "text": "Als het misgaat met het project, zou dat een grote ramp <betekenen>.",
                            "target_lang_translations": {
                                "en": "If the project goes wrong, that would <w>mean</w> a major disaster.",
                                "ru": "Если с проектом что-то пойдет не так, это будет <w>означать</w> большую катастрофу."
                            }
                        }
                    ],
                    "synonyms": [
                        "willen zeggen",
                        "willen zeggen"
                    ],
                    "common_phrases": [
                        "het einde betekenen",
                        "oorlog betekenen"
                    ],
                    "target_lang_definitions": {
                        "en": "To have as a necessary consequence or result.",
                        "ru": "Иметь неизбежным следствием или влечь за собой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mean",
                                "target_lang_sense_clarification": "to result in"
                            },
                            {
                                "target_lang_word": "entail",
                                "target_lang_sense_clarification": "to involve as a consequence"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "означать",
                                "target_lang_sense_clarification": "влечь за собой последствия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "29c61404-b113-3e69-898e-89a9dac59fa5",
                    "sense_definition": "Groźna, masowa choroba zakaźna, która szybko się rozprzestrzenia i powoduje śmierć wielu ludzi lub zwierząt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "disease_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych czasach czarna <zaraza> dziesiątkowała populację europejskich miast.",
                            "target_lang_translations": {
                                "ru": "В давние времена черная <чума> косила население европейских городов."
                            }
                        },
                        {
                            "text": "Lekarze robią wszystko, aby powstrzymać rozprzestrzenianie się tej śmiertelnej <zarazy>.",
                            "target_lang_translations": {
                                "ru": "Врачи делают все, чтобы остановить распространение этой смертельной <заразы>."
                            }
                        }
                    ],
                    "synonyms": [
                        "epidemia",
                        "mór",
                        "plaga"
                    ],
                    "common_phrases": [
                        "wybuch zarazy",
                        "panująca zaraza",
                        "uciekać przed zarazą",
                        "ognisko zarazy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опасная, массовая инфекционная болезнь, которая быстро распространяется и приводит к гибели людей или животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зараза",
                                "target_lang_sense_clarification": "инфекция, болезнь"

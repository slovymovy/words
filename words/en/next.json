{
    "word_family": [],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f7b88080-5bda-384f-957f-bdc7dce46de2",
                    "sense_definition": "Nearest in order, succession, or rank; immediately following.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sequence_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please turn to the <w>next</w> page to continue reading.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, переверните на <w>следующую</w> страницу, чтобы продолжить чтение."
                            }
                        },
                        {
                            "text": "The <w>next</w> chapter of the book is very exciting.",
                            "target_lang_translations": {
                                "ru": "<w>Следующая</w> глава книги очень захватывающая."
                            }
                        }
                    ],
                    "synonyms": [
                        "subsequent"
                    ],
                    "antonyms": [
                        "previous"
                    ],
                    "common_phrases": [
                        "next time",
                        "next week",
                        "next year",
                        "next generation",
                        "next level"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий по порядку или очереди; идущий сразу за текущим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следующий",
                                "target_lang_sense_clarification": "идущий следом по порядку"
                            },
                            {
                                "target_lang_word": "будущий",
                                "target_lang_sense_clarification": "предстоящий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44b4049b-7190-3015-9135-20c7e22f4d91",
                    "sense_definition": "Nearest in place or position; having nothing similar intervening.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "position_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My friend lives in the <w>next</w> house down the street.",
                            "target_lang_translations": {
                                "ru": "Мой друг живет в <w>соседнем</w> доме вниз по улице."
                            }
                        },
                        {
                            "text": "She went into the <w>next</w> room to answer the phone.",
                            "target_lang_translations": {
                                "ru": "Она пошла в <w>соседнюю</w> комнату, чтобы ответить на звонок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "next door",
                        "next room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ближайший по месту или положению; соседний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соседний",
                                "target_lang_sense_clarification": "находящийся рядом, смежный"
                            },
                            {
                                "target_lang_word": "ближайший",
                                "target_lang_sense_clarification": "самый близкий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59c47e25-eedc-3d95-a9a1-6089541af69d",
                    "sense_definition": "Nearest in relationship.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police notified his <w>next</w> of kin immediately after the accident.",
                            "target_lang_translations": {
                                "ru": "Полиция уведомила его <w>ближайших</w> родственников сразу после аварии."
                            }
                        },
                        {
                            "text": "The property will pass to her <w>next</w> cousin if she has no children.",
                            "target_lang_translations": {
                                "ru": "Имущество перейдет к ее <w>ближайшему</w> кузену, если у нее нет детей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "next of kin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ближайший по степени родства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ближайший",
                                "target_lang_sense_clarification": "о родственниках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6fec75f-2864-345a-94d1-edbf2ec511f2",
                    "sense_definition": "Most direct, shortest, or nearest in distance or time.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "position_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to take the <w>next</w> way home to arrive sooner.",
                            "target_lang_translations": {
                                "ru": "Он решил пойти <w>кратчайшим</w> путем домой, чтобы прийти поскорее."
                            }
                        },
                        {
                            "text": "I speak the truth the <w>next</w> way without any deception.",
                            "target_lang_translations": {
                                "ru": "Я говорю правду <w>напрямик</w>, без какого-либо обмана."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'nearest' or 'most direct'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый прямой или короткий (о пути или времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кратчайший",
                                "target_lang_sense_clarification": "самый короткий путь"
                            },
                            {
                                "target_lang_word": "прямой",
                                "target_lang_sense_clarification": "без отклонений"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "e21666d9-91f5-3618-9a1c-3faf797f93af",
                    "sense_definition": "Denotes the one immediately following the current or most recent one.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sequence_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Next</w> week would be a good time for us to meet.",
                            "target_lang_translations": {
                                "ru": "<w>Следующая</w> неделя была бы хорошим временем для нашей встречи."
                            }
                        },
                        {
                            "text": "I will try to do better <w>next</w> time.",
                            "target_lang_translations": {
                                "ru": "Я постараюсь сделать лучше в <w>следующий</w> раз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "next time",
                        "next week",
                        "next month"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает тот, который следует сразу за текущим или последним."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следующий",
                                "target_lang_sense_clarification": "идущий следом"
                            },
                            {
                                "target_lang_word": "будущий",
                                "target_lang_sense_clarification": "предстоящий (о времени)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b889e5a6-808c-3d7e-ba94-fbab11c9a936",
                    "sense_definition": "Closest in the future, referring to a specific day of the week.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The party is <w>next</w> Tuesday, not the one coming tomorrow.",
                            "target_lang_translations": {
                                "ru": "Вечеринка в <w>следующий</w> вторник, а не в тот, что будет завтра."
                            }
                        },
                        {
                            "text": "We are planning to leave for our trip <w>next</w> Thursday.",
                            "target_lang_translations": {
                                "ru": "Мы планируем отправиться в поездку в <w>следующий</w> четверг."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ближайший в будущем (о дне недели)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следующий",
                                "target_lang_sense_clarification": "ближайший по календарю"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "62227e13-e320-3aa7-bb78-a04264bb0dba",
                    "sense_definition": "So as to follow in time or sequence something previously mentioned.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sequence_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "First we removed the handles, and <w>next</w> we stripped off the paint.",
                            "target_lang_translations": {
                                "ru": "Сначала мы сняли ручки, а <w>затем</w> содрали краску."
                            }
                        },
                        {
                            "text": "I wonder what will happen <w>next</w> in the story.",
                            "target_lang_translations": {
                                "ru": "Интересно, что произойдет <w>дальше</w> в этой истории."
                            }
                        }
                    ],
                    "antonyms": [
                        "previously"
                    ],
                    "common_phrases": [
                        "what comes next"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вслед за чем-то, после этого; затем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затем",
                                "target_lang_sense_clarification": "после этого"
                            },
                            {
                                "target_lang_word": "дальше",
                                "target_lang_sense_clarification": "в продолжение"
                            },
                            {
                                "target_lang_word": "потом",
                                "target_lang_sense_clarification": "позже"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9410a9c-d7ce-3fc7-8ab7-52d95994f816",
                    "sense_definition": "In a rank or sequence closest to the one mentioned.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is the <w>next</w> best thing to ice cream.",
                            "target_lang_translations": {
                                "ru": "Это <w>следующее</w> по качеству лакомство после мороженого."
                            }
                        },
                        {
                            "text": "They live in the <w>next</w> closest house to the river.",
                            "target_lang_translations": {
                                "ru": "Они живут в <w>следующем</w> ближайшем доме к реке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "next best"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий по порядку или степени после упомянутого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следующий",
                                "target_lang_sense_clarification": "идущий за чем-то в рейтинге"
                            },
                            {
                                "target_lang_word": "второй",
                                "target_lang_sense_clarification": "второй по значимости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ee678f3-343e-31fd-9ef0-4cb4e615415a",
                    "sense_definition": "On the first subsequent occasion.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When we <w>next</w> meet, you will be married.",
                            "target_lang_translations": {
                                "ru": "Когда мы встретимся в <w>следующий</w> раз, ты уже будешь женат."
                            }
                        },
                        {
                            "text": "I will tell him the news when I <w>next</w> see him.",
                            "target_lang_translations": {
                                "ru": "Я расскажу ему новости, когда увижу его в <w>следующий</w> раз."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В следующий раз; при первой возможности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в следующий раз",
                                "target_lang_sense_clarification": "при следующей встрече"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a4afd711-006e-33f6-a5d2-89157c214fae",
                    "sense_definition": "The one that follows after this one.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Who is <w>next</w> in the queue?",
                            "target_lang_translations": {
                                "ru": "Кто <w>следующий</w> в очереди?"
                            }
                        },
                        {
                            "text": "There is no time to rest before moving from one place to the <w>next</w>.",
                            "target_lang_translations": {
                                "ru": "Нет времени отдыхать перед перемещением из одного места в <w>другое</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто (или то, что) следует дальше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следующий",
                                "target_lang_sense_clarification": "субстантивированное прилагательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "995f7892-4110-3ad7-8138-78ae804cf3ec",
                    "sense_definition": "The next match or game.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sequence_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That was a good game, do you want to play <w>next</w>?",
                            "target_lang_translations": {
                                "ru": "Это была хорошая игра, хочешь сыграть <w>следующую</w>?"
                            }
                        },
                        {
                            "text": "We lost this round, but we will win the <w>next</w>.",
                            "target_lang_translations": {
                                "ru": "Мы проиграли этот раунд, но выиграем <w>следующий</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gaming context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующая игра или матч."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следующая",
                                "target_lang_sense_clarification": "подразумевается игра"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "72392af2-2a38-3bcc-9dfb-ed0760175c17",
                    "sense_definition": "On the side of; nearest or adjacent to.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "position_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sat <w>next</w> the wall to avoid the crowd.",
                            "target_lang_translations": {
                                "ru": "Он сел <w>возле</w> стены, чтобы избежать толпы."
                            }
                        },
                        {
                            "text": "The letter D admits no other consonant <w>next</w> her but R.",
                            "target_lang_translations": {
                                "ru": "Буква D не допускает никакой другой согласной <w>рядом с</w> собой, кроме R."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage typically requires 'to' (next to)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рядом с; возле (устаревшее употребление без 'to')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возле",
                                "target_lang_sense_clarification": "около"
                            },
                            {
                                "target_lang_word": "рядом с",
                                "target_lang_sense_clarification": "поблизости"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
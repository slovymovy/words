                            },
                            {
                                "target_lang_word": "каталог",
                                "target_lang_sense_clarification": "систематизированный перечень"
                            },
                            {
                                "target_lang_word": "справочник",
                                "target_lang_sense_clarification": "книга со сведениями"
                            }
                        ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "abc08588-344f-350b-b39c-8c0373b8046f",
                    "sense_definition": "To cause someone to become acquainted with someone else.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let me <w>introduce</w> you to my friends.",
                            "target_lang_translations": {
                                "ru": "Позволь мне <w>познакомить</w> тебя с моими друзьями."
                            }
                        },
                        {
                            "text": "Please <w>introduce</w> yourself to the class.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>представьтесь</w> классу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "introduce yourself",
                        "introduce a friend",
                        "introduce someone to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Познакомить кого-либо с кем-либо другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "познакомить",
                                "target_lang_sense_clarification": "сделать знакомыми"
                            },
                            {
                                "target_lang_word": "представить",
                                "target_lang_sense_clarification": "официально или вежливо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "719da1c1-e389-3f95-81a2-5741707f9ff5",
                    "sense_definition": "To bring something into use or practice for the first time.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company decided to <w>introduce</w> a new dress code.",
                            "target_lang_translations": {
                                "ru": "Компания решила <w>ввести</w> новый дресс-код."
                            }
                        },
                        {
                            "text": "Wheeled transport was <w>introduced</w> long ago.",
                            "target_lang_translations": {
                                "ru": "Колёсный транспорт был <w>внедрён</w> очень давно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "introduce a system",
                        "introduce a law",
                        "introduce a change",
                        "introduce a product"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать использовать или применять что-либо впервые."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ввести",
                                "target_lang_sense_clarification": "в действие, в практику"
                            },
                            {
                                "target_lang_word": "внедрить",
                                "target_lang_sense_clarification": "новую систему, технологию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3337b4b6-37b3-33dc-9a4b-bbb80da2439a",
                    "sense_definition": "To make something or someone known by formal announcement or recommendation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let me <w>introduce</w> our guest speaker for tonight.",
                            "target_lang_translations": {
                                "ru": "Позвольте мне <w>представить</w> нашего сегодняшнего приглашённого докладчика."
                            }
                        },
                        {
                            "text": "The senator plans to <w>introduce</w> the bill in the next session.",
                            "target_lang_translations": {
                                "ru": "Сенатор планирует <w>внести</w> законопроект на следующей сессии."
                            }
                        }
                    ],
                    "synonyms": [
                        "announce"
                    ],
                    "common_phrases": [
                        "introduce a speaker",
                        "introduce a bill",
                        "introduce a topic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать кого-либо или что-либо известным посредством официального объявления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представить",
                                "target_lang_sense_clarification": "публике, аудитории"
                            },
                            {
                                "target_lang_word": "внести",
                                "target_lang_sense_clarification": "законопроект, предложение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b59ed3de-c637-3955-a632-6d47c9bcca0c",
                    "sense_definition": "To add something to a system, a mixture, or a container.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Various pollutants were <w>introduced</w> into the atmosphere.",
                            "target_lang_translations": {
                                "ru": "В атмосферу были <w>выброшены</w> различные загрязняющие вещества."
                            }
                        },
                        {
                            "text": "Be careful not to <w>introduce</w> bacteria into the wound.",
                            "target_lang_translations": {
                                "ru": "Будьте осторожны, чтобы не <w>занести</w> бактерии в рану."
                            }
                        }
                    ],
                    "common_phrases": [
                        "introduce into",
                        "introduce a virus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавить что-либо в систему, смесь или ёмкость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ввести",
                                "target_lang_sense_clarification": "вещество, данные"
                            },
                            {
                                "target_lang_word": "занести",
                                "target_lang_sense_clarification": "инфекцию, грязь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
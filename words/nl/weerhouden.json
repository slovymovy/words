                    "sense_id": "bc5b6c68-de81-321d-a232-921546c8ee79",
                    "sense_definition": "Iemand of iets beletten een bepaalde handeling uit te voeren of tegenhouden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niets kon hem ervan <w>weerhouden</w> om de waarheid te vertellen.",
                            "target_lang_translations": {
                                "en": "Nothing could <w>prevent</w> him from telling the truth.",
                                "ru": "Ничто не могло <w>удержать</w> его от того, чтобы сказать правду."
                            }
                        },
                        {
                            "text": "Je moet je niet laten <w>weerhouden</w> door angst voor het onbekende.",
                            "target_lang_translations": {
                                "en": "You should not let yourself be <w>held back</w> by fear of the unknown.",
                                "ru": "Ты не должен позволять страху перед неизвестным <w>останавливать</w> тебя."
                            }
                        }
                    ],
                    "synonyms": [
                        "tegenhouden",
                        "tegenhouden"
                    ],
                    "antonyms": [
                        "aansporen",
                        "aanmoedigen"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "004f41db-39fc-34b1-a193-79ca41aeb990",
                    "sense_definition": "Drobne ziarna minerałów i skał, najczęściej kwarcu, tworzące plaże, pustynie i dno rzek",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci uwielbiają budować wysoki zamek z <w>piasku</w> na plaży.",
                            "target_lang_translations": {
                                "ru": "Дети обожают строить высокий замок из <w>песка</w> на пляже."
                            }
                        },
                        {
                            "text": "Muszę wytrzepać buty, bo po spacerze mam w nich pełno <w>piasku</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно вытряхнуть обувь, потому что после прогулки в ней полно <w>песка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "piach"
                    ],
                    "common_phrases": [
                        "zamek z piasku",
                        "chować głowę w piasek",
                        "ziarenko piasku",
                        "sypać piaskiem w oczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкие зерна минералов и горных пород, чаще всего кварца, образующие пляжи, пустыни и дно рек"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "песок",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "331f17c3-b26a-30c7-8257-81c3a966b163",
                    "sense_definition": "Teren lub gleba składająca się głównie z piasku, zazwyczaj mało urodzajna",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "terrain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnicy narzekają, że na tych suchych <w>piaskach</w> nic nie urośnie.",
                            "target_lang_translations": {
                                "ru": "Фермеры жалуются, что на этих сухих <w>песках</w> ничего не вырастет."
                            }
                        },
                        {
                            "text": "Wiele lasów sosnowych w Polsce rośnie na słabych <w>piaskach</w>.",
                            "target_lang_translations": {
                                "ru": "Многие сосновые леса в Польше растут на бедных <w>песчаных почвах</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "na piaskach",
                        "budować na piasku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Местность или почва, состоящая в основном из песка, обычно малоплодородная"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "песчаная почва",
                                "target_lang_sense_clarification": "тип грунта"
                            },
                            {
                                "target_lang_word": "пески",
                                "target_lang_sense_clarification": "местность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e33cec0a-e1a7-330c-a30b-40f1d66d56f8",
                    "sense_definition": "Drobne złogi mineralne gromadzące się w narządach wewnętrznych, na przykład w nerkach",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zalecił picie dużej ilości wody, aby wypłukać <w>piasek</w> z nerek.",
                            "target_lang_translations": {
                                "ru": "Врач порекомендовал пить много воды, чтобы вымыть <w>песок</w> из почек."
                            }
                        },
                        {
                            "text": "Badanie wykazało drobny <w>piasek</w> w drogach moczowych pacjenta.",
                            "target_lang_translations": {
                                "ru": "Обследование показало мелкий <w>песок</w> в мочевыводящих путях пациента."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piasek w nerkach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкие минеральные отложения, скапливающиеся во внутренних органах, например, в почках"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "песок",
                                "target_lang_sense_clarification": "медицина"
                            }
                        ]
                    }
                }
            ]
        },

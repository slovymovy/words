                    "semantic_group_id": "hunting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный охотник сразу заметил рыжую <w>трубу</w> лисы в кустах.",
                            "target_lang_translations": {
                                "en": "The experienced hunter immediately noticed the fox's red <w>brush</w> in the bushes."
                            }

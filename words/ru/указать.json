{
    "word_family": [
        "указ",
        "указка",
        "указание",
        "указатель"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "53fdb866-6088-32be-81f2-1a883d027241",
                    "sense_definition": "Жестом или движением привлечь внимание к кому-либо или чему-либо, показать направление.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pointing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он молча <указал> рукой на дверь.",
                            "target_lang_translations": {
                                "en": "He silently <w>pointed</w> at the door with his hand."
                            }
                        },
                        {
                            "text": "Прохожий любезно <указал> нам дорогу к музею.",
                            "target_lang_translations": {
                                "en": "A passerby kindly <w>showed</w> us the way to the museum."
                            }
                        }
                    ],
                    "synonyms": [
                        "показать"
                    ],
                    "common_phrases": [
                        "указать пальцем",
                        "указать рукой",
                        "указать на дверь",
                        "указать направление"
                    ],
                    "target_lang_definitions": {
                        "en": "To attract attention to someone or something by gesture or movement; to show a direction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "point",
                                "target_lang_sense_clarification": "to direct attention with a finger or gesture"
                            },
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "to indicate a direction or path"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e337db30-ee11-34db-a513-d8139b64715d",
                    "sense_definition": "Точно назвать, отметить или вписать необходимые сведения (в документе, списке и т. п.).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "specifying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В анкете необходимо <указать> точный домашний адрес.",
                            "target_lang_translations": {
                                "en": "It is necessary to <w>specify</w> your exact home address in the form."
                            }
                        },
                        {
                            "text": "Мы забыли <указать> дату рождения в заявлении.",
                            "target_lang_translations": {
                                "en": "We forgot to <w>indicate</w> the date of birth in the application."
                            }
                        }
                    ],
                    "synonyms": [
                        "отметить",
                        "обозначить",
                        "назвать",
                        "прописать"
                    ],
                    "common_phrases": [
                        "указать адрес",
                        "указать цену",
                        "указать сроки",
                        "указать данные"
                    ],
                    "target_lang_definitions": {
                        "en": "To state, mark, or enter specific information (in a document, list, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "specify",
                                "target_lang_sense_clarification": "to state a fact or requirement clearly"
                            },
                            {
                                "target_lang_word": "indicate",
                                "target_lang_sense_clarification": "to state or express briefly"
                            },
                            {
                                "target_lang_word": "state",
                                "target_lang_sense_clarification": "to express something definitely or clearly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98e70b49-e76b-3120-8686-d7ab0c5dddca",
                    "sense_definition": "Обратить чьё-либо внимание на какое-либо обстоятельство, факт или ошибку.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pointing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель <указал> ученику на грубые ошибки в тексте.",
                            "target_lang_translations": {
                                "en": "The teacher <w>pointed out</w> the gross errors in the text to the student."
                            }
                        },
                        {
                            "text": "Я хочу <указать> на важность этой проблемы для всех нас.",
                            "target_lang_translations": {
                                "en": "I want to <w>point out</w> the importance of this problem for all of us."
                            }
                        }
                    ],
                    "synonyms": [
                        "отметить",
                        "подчеркнуть",
                        "заметить"
                    ],
                    "antonyms": [
                        "проигнорировать"
                    ],
                    "common_phrases": [
                        "указать на ошибки",
                        "указать на недостатки",
                        "указать причину"
                    ],
                    "target_lang_definitions": {
                        "en": "To draw someone's attention to a circumstance, fact, or mistake."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "point out",
                                "target_lang_sense_clarification": "to direct attention to a fact or mistake"
                            },
                            {
                                "target_lang_word": "indicate",
                                "target_lang_sense_clarification": "to point out or show"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d589862-521c-3340-9ede-199ffe20a253",
                    "sense_definition": "Дать наставление, распорядиться или научить, как следует действовать.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "instructing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Никто не имеет права <указать> мне, как жить.",
                            "target_lang_translations": {
                                "en": "No one has the right to <w>tell</w> me how to live."
                            }
                        },
                        {
                            "text": "Начальник строго <указал> сотрудникам закончить проект сегодня.",
                            "target_lang_translations": {
                                "en": "The boss strictly <w>instructed</w> the employees to finish the project today."
                            }
                        }
                    ],
                    "synonyms": [
                        "приказать",
                        "велеть",
                        "распорядиться"
                    ],
                    "common_phrases": [
                        "указать место",
                        "строго указать"
                    ],
                    "target_lang_definitions": {
                        "en": "To give instructions, orders, or teach someone how to act."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "to give an order or instruction"
                            },
                            {
                                "target_lang_word": "instruct",
                                "target_lang_sense_clarification": "to give a directive or command"
                            },
                            {
                                "target_lang_word": "order",
                                "target_lang_sense_clarification": "to command someone to do something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
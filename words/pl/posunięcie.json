                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gwałtowne <w>posunięcie</w> dźwigni spowodowało natychmiastowe zatrzymanie maszyny.",
                            "target_lang_translations": {
                                "ru": "Резкое <w>перемещение</w> рычага привело к немедленной остановке машины."

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "44fd0b09-66ac-3c52-8d8c-4ab6d2d2a7db",
                    "sense_definition": "A food mixture used inside a pie, sandwich, cake, or pastry.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The apple pie has a sweet and delicious <w>filling</w>.",
                            "target_lang_translations": {
                                "ru": "У этого яблочного пирога сладкая и вкусная <w>начинка</w>."
                            }
                        },
                        {
                            "text": "I prefer a sandwich with a simple cheese <w>filling</w>.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю сэндвич с простой сырной <w>начинкой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pie filling",
                        "sandwich filling",
                        "creamy filling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пищевая смесь, используемая внутри пирога, сэндвича, торта или выпечки."

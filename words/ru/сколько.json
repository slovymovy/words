    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "f2356559-7faa-305e-9115-c6ddf0d7e6ed",
                    "sense_definition": "Вопросительное или относительное слово, обозначающее количество, число или меру чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_interrogative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Скажите, пожалуйста, <w>сколько</w> стоит этот билет на поезд до Москвы?"
                        },
                        {
                            "text": "Я даже не представляю, <w>сколько</w> времени нам потребуется на эту работу."
                        }
                    ],
                    "synonyms": [
                        "как много"
                    ],
                    "common_phrases": [
                        "сколько стоит",
                        "сколько времени",
                        "сколько лет",
                        "сколько угодно"
                    ]
                },
                {
                    "sense_id": "fc7d41d8-f525-3bb0-9fc9-9c857cf8b774",
                    "sense_definition": "Употребляется в восклицательных предложениях для выражения удивления по поводу большого количества кого-либо или чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_exclamatory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Боже мой, <w>сколько</w> цветов ты принёс на наш праздник!"
                        },
                        {
                            "text": "Я уже забыл, <w>сколько</w> раз мы обсуждали эту сложную тему."
                        }
                    ],
                    "synonyms": [
                        "много"
                    ],
                    "common_phrases": [
                        "сколько лет сколько зим",
                        "сколько можно"
                    ]
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1073254e-5380-35de-a571-8a3491e6c065",
                    "sense_definition": "Обозначает длительность или продолжительность действия во времени.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты не знаешь, <w>сколько</w> нам ещё ждать начала концерта?"
                        },
                        {
                            "text": "Врач не сказал, <w>сколько</w> дней мне нужно принимать это лекарство."
                        }
                    ],
                    "synonyms": [
                        "как долго"
                    ],
                    "common_phrases": [
                        "сколько ждать",
                        "сколько идти",
                        "сколько ехать"
                    ]
                },
                {
                    "sense_id": "70d3714a-9790-3107-b0c1-dcabadc45639",
                    "sense_definition": "Используется для обозначения степени проявления признака или меры действия, часто в сопоставлении (обычно в паре со словом «столько»).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "degree_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не <w>столько</w> устал после работы, <w>сколько</w> просто хочу побыть в тишине."
                        },
                        {
                            "text": "Он помог нам ровно <w>столько</w>, <w>сколько</w> позволяли его скромные возможности."
                        }
                    ],
                    "synonyms": [
                        "насколько"
                    ],
                    "common_phrases": [
                        "не столько сколько",
                        "столько же сколько"
                    ]
                }
            ]
        }
    ]

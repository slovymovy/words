            "senses": [
                {
                    "sense_id": "3f053c25-8f69-3cda-9d8c-04adc43ecfb8",
                    "sense_definition": "Zestawić ze sobą osoby, rzeczy lub zjawiska w celu znalezienia między nimi podobieństw lub różnic.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "comparison_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed zakupem nowego komputera warto <porównać> ceny w kilku sklepach.",
                            "target_lang_translations": {
                                "ru": "Перед покупкой нового компьютера стоит <сравнить> цены в нескольких магазинах."
                            }
                        },
                        {
                            "text": "Nauczyciel <porównał> twoją pracę do wypracowania najlepszego ucznia w klasie.",
                            "target_lang_translations": {
                                "ru": "Учитель <сравнил> твою работу с сочинением лучшего ученика в классе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zestawić",
                        "skonfrontować"
                    ],
                    "common_phrases": [
                        "porównać ceny",
                        "porównać oferty",
                        "porównać wyniki",
                        "porównać z oryginałem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сопоставить людей, вещи или явления с целью нахождения сходств или различий между ними."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сравнить",
                                "target_lang_sense_clarification": "найти сходства или различия"
                            },
                            {
                                "target_lang_word": "сопоставить",
                                "target_lang_sense_clarification": "рассмотреть вместе для сравнения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8718058-a164-3a4e-b3af-b73fcd92d97a",
                    "sense_definition": "Ocenić własną wartość, umiejętności lub sytuację w odniesieniu do innych osób.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "comparison_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym <porównać> się z najlepszymi zawodnikami w tej dyscyplinie.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <сравнить> себя с лучшими спортсменами в этой дисциплине."
                            }
                        },
                        {
                            "text": "Kiedy <porównał> się ze swoim bratem, poczuł motywację do cięższej pracy.",
                            "target_lang_translations": {
                                "ru": "Когда он <сравнил> себя со своим братом, он почувствовал мотивацию работать усерднее."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmierzyć się"
                    ],
                    "common_phrases": [
                        "porównać się z kimś",
                        "porównać się do kogoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценить собственную значимость, навыки или ситуацию по отношению к другим людям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сравнить",

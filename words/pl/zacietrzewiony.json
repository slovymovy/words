                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Страдательное причастие от глагола zacietrzewić (раззадорить, ожесточить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раззадоренный",
                                "target_lang_sense_clarification": "побужденный к действию, возбужденный"
                            },
                            {
                                "target_lang_word": "ожесточённый",
                                "target_lang_sense_clarification": "доведенный до состояния гнева или упорства"
                            }
                        ]
                    }
                }
            ]
        }
    ]

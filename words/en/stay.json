    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0fb9d4c6-0145-393c-91bd-1ee4ef9281fc",
                    "sense_definition": "To remain in a particular place rather than leaving.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "remain_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>stay</w> here until I come back.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>оставайся</w> здесь, пока я не вернусь."
                            }
                        },
                        {
                            "text": "We decided to <w>stay</w> at home and watch a movie.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>остаться</w> дома и посмотреть фильм."
                            }
                        }
                    ],
                    "synonyms": [
                        "remain",
                        "wait",
                        "linger",
                        "tarry"
                    ],
                    "antonyms": [
                        "leave",
                        "depart",
                        "go"
                    ],
                    "common_phrases": [
                        "stay at home",
                        "stay here",
                        "stay put",
                        "stay up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставаться в определенном месте, не уходя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остава́ться",
                                "target_lang_sense_clarification": "не уходить"
                            },
                            {
                                "target_lang_word": "оста́ться",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41bfe70a-a93e-37d9-b541-a26dab376409",
                    "sense_definition": "To continue to have a particular quality or be in a particular state.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "remain_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should wear a coat to <w>stay</w> warm outside.",
                            "target_lang_translations": {
                                "ru": "Тебе следует надеть пальто, чтобы <w>сохранить</w> тепло на улице."
                            }
                        },
                        {
                            "text": "Please try to <w>stay</w> calm during the emergency.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, постарайтесь <w>сохранять</w> спокойствие во время чрезвычайной ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "remain",
                        "keep",
                        "continue"
                    ],
                    "antonyms": [
                        "change",
                        "become"
                    ],
                    "common_phrases": [

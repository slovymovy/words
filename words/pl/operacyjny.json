                {
                    "sense_id": "98a55238-5028-3366-b44f-f248312bd93e",
                    "sense_definition": "Związany z zabiegiem chirurgicznym, dotyczący operacji medycznej lub używany w jej trakcie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent został natychmiast przewieziony na salę <w>operacyjną</w> w celu ratowania życia.",
                            "target_lang_translations": {
                                "ru": "Пациента немедленно перевезли в <w>операционную</w> для спасения жизни."
                            }
                        },
                        {
                            "text": "Lekarz prowadzący zdecydował, że konieczny będzie zabieg <w>operacyjny</w>.",
                            "target_lang_translations": {
                                "ru": "Лечащий врач решил, что необходимо <w>оперативное</w> вмешательство."
                            }
                        }
                    ],
                    "synonyms": [
                        "chirurgiczny",
                        "zabiegowy"
                    ],
                    "antonyms": [
                        "zachowawczy",
                        "nieoperacyjny"
                    ],
                    "common_phrases": [

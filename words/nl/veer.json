    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e77f62fb-1e2e-3eed-995f-6afbf5727159",
                    "sense_definition": "Een van de lichte, platte uitgroeisels die de huid van vogels bedekken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "feather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De pauw liet zijn prachtige gekleurde <w>veren</w> zien.",
                            "target_lang_translations": {
                                "ru": "Павлин показал свои прекрасные цветные <w>перья</w>.",
                                "en": "The peacock showed its beautiful colored <w>feathers</w>."
                            }
                        },
                        {
                            "text": "We moesten vanochtend heel vroeg uit de <w>veren</w>.",
                            "target_lang_translations": {
                                "ru": "Сегодня утром нам пришлось очень рано встать с <w>постели</w>.",
                                "en": "We had to get out of <w>bed</w> very early this morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "pluim"
                    ],
                    "common_phrases": [
                        "uit de veren",
                        "veren laten",
                        "met andermans veren pronken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одно из легких плоских роговых образований, покрывающих кожу птиц.",
                        "en": "One of the light, flat growths that cover the skin of birds."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перо",
                                "target_lang_sense_clarification": "птичье"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "feather",
                                "target_lang_sense_clarification": "bird plumage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4524f59-0df7-30c6-9ea4-381cde208573",
                    "sense_definition": "Een boot of schip dat mensen en voertuigen naar de overkant van een water brengt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ferry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moesten rennen om het laatste <w>veer</w> nog te halen.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось бежать, чтобы успеть на последний <w>паром</w>.",
                                "en": "We had to run to catch the last <w>ferry</w>."
                            }
                        },
                        {
                            "text": "De auto reed voorzichtig de klep van het <w>veer</w> op.",
                            "target_lang_translations": {
                                "ru": "Машина осторожно заехала на аппарель <w>парома</w>.",
                                "en": "The car drove carefully onto the ramp of the <w>ferry</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pont",
                        "veerpont",
                        "veerboot"
                    ],
                    "common_phrases": [
                        "het veer nemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лодка или судно, которое переправляет людей и транспортные средства на другую сторону водоема.",
                        "en": "A boat or ship that transports people and vehicles to the other side of a body of water."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паром",
                                "target_lang_sense_clarification": "судно"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "ferry",
                                "target_lang_sense_clarification": "boat"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1465613c-ff15-390b-87e5-5ff56e7b3d38",
                    "sense_definition": "Een elastisch, meestal metalen onderdeel dat na indrukken of uitrekken terugkeert in de oorspronkelijke vorm.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>veer</w> in het mechanisme was gebroken door roest.",
                            "target_lang_translations": {
                                "ru": "<w>Пружина</w> в механизме сломалась из-за ржавчины.",
                                "en": "The <w>spring</w> in the mechanism was broken due to rust."
                            }
                        },
                        {
                            "text": "Er zit een kleine <w>veer</w> in de pen die de punt terugduwt.",
                            "target_lang_translations": {
                                "ru": "В ручке есть маленькая <w>пружина</w>, которая выталкивает стержень обратно.",
                                "en": "There is a small <w>spring</w> in the pen that pushes the tip back."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Упругая, обычно металлическая деталь, которая возвращается в исходную форму после сжатия или растяжения.",
                        "en": "An elastic, usually metal part that returns to its original shape after being compressed or stretched."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пружина",
                                "target_lang_sense_clarification": "деталь"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "spring",
                                "target_lang_sense_clarification": "mechanical part"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1f7e431a-c5dd-398e-a2aa-10b64d5347f3",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>veren</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>veer</w> mee op het ritme van de muziek.",
                            "target_lang_translations": {
                                "ru": "Я <w>пружиню</w> в такт музыке.",
                                "en": "I <w>bounce</w> along to the rhythm of the music."
                            }
                        },
                        {
                            "text": "Als ik op de trampoline spring, <w>veer</w> ik hoog de lucht in.",
                            "target_lang_translations": {
                                "ru": "Когда я прыгаю на батуте, я высоко <w>подпрыгиваю</w> в воздух.",
                                "en": "When I jump on the trampoline, I <w>spring</w> high into the air."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени глагола 'veren' (пружинить).",
                        "en": "First person singular present tense of the verb 'veren' (to spring or bounce)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пружиню",
                                "target_lang_sense_clarification": "форма глагола"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "bounce",
                                "target_lang_sense_clarification": "movement"
                            },
                            {
                                "target_lang_word": "spring",
                                "target_lang_sense_clarification": "movement"
                            }
                        ]
                    }
                }
            ]
        }

                    "sense_id": "04b1c979-ad52-3399-8ed5-b96114299462",
                    "sense_definition": "Personen of goederen van de ene plaats naar de andere brengen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vrachtwagenchauffeur <w>vervoert</w> verse groenten naar de supermarkt.",
                            "target_lang_translations": {
                                "ru": "Водитель грузовика <w>перевозит</w> свежие овощи в супермаркет.",
                                "en": "The truck driver <w>transports</w> fresh vegetables to the supermarket."
                            }
                        },
                        {
                            "text": "Wij <w>vervoeren</w> passagiers met bussen door de hele stad.",
                            "target_lang_translations": {
                                "ru": "Мы <w>перевозим</w> пассажиров на автобусах по всему городу.",
                                "en": "We <w>transport</w> passengers by bus throughout the city."
                            }
                        }
                    ],
                    "synonyms": [
                        "transporteren",
                        "overbrengen",
                        "brengen"
                    ],
                    "common_phrases": [
                        "goederen vervoeren",
                        "passagiers vervoeren",
                        "gratis vervoeren",
                        "met de trein vervoeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать людей или грузы из одного места в другое",
                        "en": "To move people or goods from one place to another"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевозить",
                                "target_lang_sense_clarification": "доставлять с помощью транспорта"
                            },
                            {
                                "target_lang_word": "транспортировать",
                                "target_lang_sense_clarification": "перемещать грузы или пассажиров"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "transport",
                                "target_lang_sense_clarification": "to carry people or goods from one place to another"
                            },
                            {
                                "target_lang_word": "convey",
                                "target_lang_sense_clarification": "to transport or carry to a place"
                            },
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "to take from one place to another"
                            }

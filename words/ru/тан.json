    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "be285e6d-e65e-32ce-9812-a85bae6b20a0",
                    "sense_definition": "Прохладительный кисломолочный напиток, традиционный для кавказской и среднеазиатской кухни, приготовленный из мацони или катыка с добавлением воды и соли.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "beverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В жаркую погоду холодный <тан> отлично утоляет жажду.",
                            "target_lang_translations": {
                                "en": "In hot weather, cold <w>tan</w> quenches thirst perfectly."
                            }
                        },
                        {
                            "text": "Мы купили в магазине свежий <тан> и горячий лаваш.",
                            "target_lang_translations": {
                                "en": "We bought fresh <w>tan</w> and hot lavash at the store."
                            }
                        }
                    ],
                    "common_phrases": [
                        "газированный тан",
                        "пить тан",
                        "холодный тан"
                    ],
                    "target_lang_definitions": {
                        "en": "A refreshing fermented milk drink, traditional in Caucasian and Central Asian cuisine, made from matsoni or katyk with added water and salt."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tan",
                                "target_lang_sense_clarification": "fermented milk drink"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f389a30-9d57-3dae-903c-5f166a9b0963",
                    "sense_definition": "Исторический дворянский титул в средневековой Шотландии, соответствующий графу, а также лицо, носящее этот титул.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Макбет, <тан> Гламиса, является главным героем знаменитой трагедии Шекспира.",
                            "target_lang_translations": {
                                "en": "Macbeth, <w>Thane</w> of Glamis, is the main character of Shakespeare's famous tragedy."
                            }
                        },
                        {
                            "text": "Король наградил верного воина землями и провозгласил его новым <таном>.",
                            "target_lang_translations": {
                                "en": "The king rewarded the loyal warrior with lands and proclaimed him the new <w>thane</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical title"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A historical noble title in medieval Scotland, equivalent to an earl, as well as a person holding this title."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thane",
                                "target_lang_sense_clarification": "Scottish historical title"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "438fed83-c558-3e0a-989c-ff24c92e98a7",
                    "sense_definition": "Титул правителя Шира в произведениях Дж. Р. Р. Толкина.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "title",
                    "name_type": "fictional_name",
                    "examples": [
                        {
                            "text": "Пиппин стал тридцать вторым <таном> Шира после Войны Кольца.",
                            "target_lang_translations": {
                                "en": "Pippin became the thirty-second <w>Thain</w> of the Shire after the War of the Ring."
                            }
                        },
                        {
                            "text": "На большом совете присутствовал сам <тан> и другие уважаемые хоббиты.",
                            "target_lang_translations": {
                                "en": "The <w>Thain</w> himself and other respected hobbits were present at the great council."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The title of the ruler of the Shire in the works of J.R.R. Tolkien."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Thain",
                                "target_lang_sense_clarification": "title in Tolkien's legendarium"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28fe37d3-10bf-36e5-bac9-6df7b77b48c1",
                    "sense_definition": "Традиционная японская мера земельной площади, равная приблизительно 9,92 арам.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Крестьянин получил в аренду участок размером в один <тан>.",
                            "target_lang_translations": {
                                "en": "The peasant leased a plot of land one <w>tan</w> in size."
                            }
                        },
                        {
                            "text": "В старых документах площадь рисовых полей часто измерялась в <танах>.",
                            "target_lang_translations": {
                                "en": "In old documents, the area of rice paddies was often measured in <w>tans</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Japan"
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A traditional Japanese unit of land area, approximately equal to 9.92 ares."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tan",
                                "target_lang_sense_clarification": "Japanese unit of area"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "aanmerken",
        "aanmerkelijk"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "be691e1e-86fa-3545-a8fb-05e91023bf24",
                    "sense_definition": "De mogelijkheid om ergens voor gekozen te worden of mee te tellen; wordt bijna altijd gebruikt in vaste uitdrukkingen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "overweging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Helaas komt u niet in <w>aanmerking</w> voor deze subsidie.",
                            "target_lang_translations": {
                                "en": "Unfortunately, you are not <w>eligible</w> for this subsidy.",
                                "ru": "К сожалению, вы не можете <w>претендовать</w> на эту субсидию."
                            }
                        },
                        {
                            "text": "We zullen uw voorstel zeker in <w>aanmerking</w> nemen bij de vergadering.",
                            "target_lang_translations": {
                                "en": "We will certainly take your proposal into <w>consideration</w> at the meeting.",
                                "ru": "Мы обязательно примем ваше предложение во <w>внимание</w> на собрании."
                            }
                        }
                    ],
                    "synonyms": [
                        "overweging"
                    ],
                    "common_phrases": [
                        "in aanmerking komen",
                        "in aanmerking nemen"
                    ],
                    "target_lang_definitions": {
                        "en": "The possibility of being chosen for something or counting towards something; almost always used in fixed expressions.",
                        "ru": "Возможность быть выбранным или учтенным; почти всегда используется в устойчивых выражениях"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "consideration",
                                "target_lang_sense_clarification": "used in phrases like 'take into consideration'"
                            },
                            {
                                "target_lang_word": "account",
                                "target_lang_sense_clarification": "used in phrases like 'take into account'"
                            },
                            {
                                "target_lang_word": "eligibility",
                                "target_lang_sense_clarification": "the state of qualifying for something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "внимание",
                                "target_lang_sense_clarification": "в выражении \"принимать во внимание\""
                            },
                            {
                                "target_lang_word": "расчёт",
                                "target_lang_sense_clarification": "в выражении \"принимать в расчёт\""
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a9dc236-2a54-354f-909c-85fbd223d2ae",
                    "sense_definition": "Een kritische opmerking over iets dat niet goed is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "kritiek",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar had geen enkele <w>aanmerking</w> op mijn huiswerk.",
                            "target_lang_translations": {
                                "en": "The teacher had not a single <w>remark</w> about my homework.",
                                "ru": "У учителя не было ни одного <w>замечания</w> к моей домашней работе."
                            }
                        },
                        {
                            "text": "Ondanks een kleine <w>aanmerking</w> was het rapport van goede kwaliteit.",
                            "target_lang_translations": {
                                "en": "Despite a small <w>criticism</w>, the report was of good quality.",
                                "ru": "Несмотря на небольшое <w>замечание</w>, отчет был хорошего качества."
                            }
                        }
                    ],
                    "synonyms": [
                        "kritiek",
                        "commentaar",
                        "opmerking"
                    ],
                    "antonyms": [
                        "lof",
                        "compliment"
                    ],
                    "common_phrases": [
                        "aanmerkingen maken",
                        "aanmerkingen hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "A critical remark about something that is not good.",
                        "ru": "Критическое замечание о том, что что-то сделано неправильно или плохо"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remark",
                                "target_lang_sense_clarification": "a critical comment or observation"
                            },
                            {
                                "target_lang_word": "criticism",
                                "target_lang_sense_clarification": "expression of disapproval"
                            },
                            {
                                "target_lang_word": "comment",
                                "target_lang_sense_clarification": "feedback or note on something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "замечание",
                                "target_lang_sense_clarification": "критика, указание на ошибку"
                            },
                            {
                                "target_lang_word": "нарекание",
                                "target_lang_sense_clarification": "выражение недовольства"
                            }
                        ]
                    }
                }
            ]
        }
    ]

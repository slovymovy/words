    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cb85324a-32fb-3fb4-b969-0cba9970ec7c",
                    "sense_definition": "Een punt waar twee of meer wegen samenkomen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "traffic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op die <w>kruising</w> gebeuren veel ongelukken.",
                            "target_lang_translations": {
                                "ru": "На этом <w>перекрёстке</w> происходит много аварий."
                            }
                        },
                        {
                            "text": "Je moet bij de volgende <w>kruising</w> rechtsaf slaan.",
                            "target_lang_translations": {
                                "ru": "На следующем <w>перекрёстке</w> тебе нужно повернуть направо."
                            }
                        }
                    ],
                    "synonyms": [
                        "kruispunt"
                    ],
                    "common_phrases": [
                        "drukke kruising",
                        "gevaarlijke kruising",
                        "op de kruising"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где соединяются две или более дороги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перекрёсток",
                                "target_lang_sense_clarification": "дорог"
                            },
                            {
                                "target_lang_word": "пересечение",
                                "target_lang_sense_clarification": "путей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4caf4ab5-424e-3baf-924e-88b707427a81",
                    "sense_definition": "Een dier of plant dat is ontstaan uit twee verschillende soorten of rassen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biology_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een muildier is een <w>kruising</w> tussen een paard en een ezel.",
                            "target_lang_translations": {
                                "ru": "Мул — это <w>помесь</w> лошади и осла."
                            }
                        },
                        {
                            "text": "Deze hond is een <w>kruising</w> en geen rashond.",
                            "target_lang_translations": {
                                "ru": "Эта собака — <w>помесь</w>, а не породистая."
                            }
                        }
                    ],
                    "synonyms": [
                        "hybride"
                    ],
                    "common_phrases": [
                        "genetische kruising",
                        "bijzondere kruising"
                    ],
                    "target_lang_definitions": {
                        "ru": "Животное или растение, полученное от двух разных видов или пород."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помесь",
                                "target_lang_sense_clarification": "животных"
                            },
                            {
                                "target_lang_word": "гибрид",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f883c017-524a-34fe-9914-ce2557882278",
                    "sense_definition": "De bovenhoek van een doel waar de paal en de lat samenkomen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij schoot de bal precies in de <w>kruising</w>.",
                            "target_lang_translations": {
                                "ru": "Он пробил мячом точно в <w>девятку</w>."
                            }
                        },
                        {
                            "text": "De doelman kon de bal niet uit de <w>kruising</w> tikken.",
                            "target_lang_translations": {
                                "ru": "Вратарь не смог вытащить мяч из <w>девятки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de kruising",
                        "bal in de kruising"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхний угол ворот, где соединяются штанга и перекладина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девятка",
                                "target_lang_sense_clarification": "футбольный термин"
                            },
                            {
                                "target_lang_word": "крестовина",
                                "target_lang_sense_clarification": "конструкция ворот"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c06372f-bed2-367b-b29a-c941c14c7c2d",
                    "sense_definition": "Een combinatie van eigenschappen van twee verschillende personen of zaken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De film is een <w>kruising</w> tussen horror en komedie.",
                            "target_lang_translations": {
                                "ru": "Этот фильм — <w>смесь</w> ужасов и комедии."
                            }
                        },
                        {
                            "text": "Dat kind was een <w>kruising</w> tussen zijn moeder en zijn vader.",
                            "target_lang_translations": {
                                "ru": "Этот ребёнок был <w>смесью</w> матери и отца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een vreemde kruising",
                        "kruising tussen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сочетание свойств двух разных людей или предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смесь",
                                "target_lang_sense_clarification": "комбинация"
                            },
                            {
                                "target_lang_word": "гибрид",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfbd6959-405d-30c2-ac94-887b141a690a",
                    "sense_definition": "Het proces waarbij verschillende rassen of soorten worden samengebracht om nageslacht te produceren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biology_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door middel van <w>kruising</w> hebben we dit nieuwe soort kunnen maken.",
                            "target_lang_translations": {
                                "ru": "С помощью <w>скрещивания</w> мы смогли создать этот новый вид."
                            }
                        },
                        {
                            "text": "De <w>kruising</w> van deze twee plantensoorten duurde jaren.",
                            "target_lang_translations": {
                                "ru": "<w>Скрещивание</w> этих двух видов растений заняло годы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "door middel van kruising"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс соединения разных пород или видов для получения потомства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрещивание",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                }
            ]
        }

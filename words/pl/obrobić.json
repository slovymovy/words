                        "obrobić działkę",
                        "obrobić ziemię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подготовить землю к выращиванию растений путем выполнения полевых работ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обработать",
                                "target_lang_sense_clarification": "землю, почву"
                            },
                            {
                                "target_lang_word": "возделать",
                                "target_lang_sense_clarification": "сельскохозяйственный термин"
                            }
                        ]
                    }
                }
            ]
        }

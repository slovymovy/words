                    "frequency": "Low",
                    "semantic_group_id": "preservation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn grootvader <w>rookte</w> vroeger zelf zijn gevangen paling.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка раньше сам <w>коптил</w> пойманного угря."
                            }
                        },
                        {
                            "text": "Ze hebben de ham urenlang op traditionele wijze <w>gerookt</w>.",
                            "target_lang_translations": {
                                "ru": "Они часами <w>коптили</w> ветчину традиционным способом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "paling roken",
                        "gerookte zalm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Консервировать продукты и придавать им вкус, выдерживая в дыму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коптить",
                                "target_lang_sense_clarification": "продукты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8264d702-ddc2-3f06-bff8-aefbaa15a164",

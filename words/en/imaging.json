                {
                    "sense_id": "53e780e7-283a-3868-a446-d0fb0e0e877e",
                    "sense_definition": "The process of producing images of the inside of a body or object, especially for medical use.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor ordered diagnostic <w>imaging</w> to check for internal injuries.",
                            "target_lang_translations": {
                                "ru": "Врач назначил диагностическую <w>визуализацию</w>, чтобы проверить наличие внутренних повреждений."
                            }
                        },
                        {
                            "text": "Advances in medical <w>imaging</w> allow us to detect diseases much earlier than before.",
                            "target_lang_translations": {
                                "ru": "Достижения в области медицинской <w>визуализации</w> позволяют нам выявлять болезни гораздо раньше, чем прежде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "medical imaging",
                        "diagnostic imaging",
                        "thermal imaging",
                        "satellite imaging"
                    ],
                    "target_lang_definitions": {

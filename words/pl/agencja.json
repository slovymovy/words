                {
                    "sense_id": "fd0f4d91-0aba-3f65-b250-0045bdd29929",
                    "sense_definition": "Firma lub przedsiębiorstwo świadczące specjalistyczne usługi na rzecz innych firm lub osób prywatnych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "company_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza <agencja> reklamowa przygotowała nową kampanię dla klienta.",
                            "target_lang_translations": {
                                "ru": "Наше рекламное <агентство> подготовило новую кампанию для клиента."
                            }
                        },
                        {
                            "text": "Szukamy mieszkania do wynajęcia, więc pomaga nam <agencja> nieruchomości.",
                            "target_lang_translations": {
                                "ru": "Мы ищем квартиру в аренду, поэтому нам помогает <агентство> недвижимости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "agencja reklamowa",
                        "agencja nieruchomości",
                        "agencja turystyczna",
                        "agencja pracy",
                        "agencja modelek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фирма или предприятие, предоставляющее специализированные услуги другим компаниям или частным лицам."

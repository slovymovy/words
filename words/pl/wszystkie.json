{
    "word_family": [
        "wszyscy",
        "wszystko",
        "wszechświat",
        "powszechny"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "08ae7717-b786-3315-b0e8-50c2425d3b99",
                    "sense_definition": "Oznacza każdy element z danego zbioru; określa pełny ogół przedmiotów, zjawisk, zwierząt lub osób (w rodzaju niemęskoosobowym).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantifiers",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W moim pokoju <w>wszystkie</w> okna są szeroko otwarte.",
                            "target_lang_translations": {
                                "ru": "В моей комнате <w>все</w> окна широко открыты."
                            }
                        },
                        {
                            "text": "Musisz dokładnie umyć <w>wszystkie</w> brudne naczynia.",
                            "target_lang_translations": {
                                "ru": "Ты должен тщательно вымыть <w>всю</w> грязную посуду."
                            }
                        }
                    ],
                    "antonyms": [
                        "żadne"
                    ],
                    "common_phrases": [
                        "wszystkie dzieci",
                        "wszystkie strony",
                        "po wszystkie czasy",
                        "we wszystkie strony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает каждый элемент из данной группы; определяет полную совокупность предметов, явлений или живых существ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "все",
                                "target_lang_sense_clarification": "определительное местоимение, мн. ч."
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "43eceea1-75bc-35de-a418-a89fb40c09ad",
                    "sense_definition": "Zastępuje rzeczowniki, oznaczając ogół wspomnianych osób (kobiet, dzieci), zwierząt lub rzeczy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantifiers",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje siostry są w domu i <w>wszystkie</w> czytają książki.",
                            "target_lang_translations": {
                                "ru": "Мои сестры дома, и <w>все</w> читают книги."
                            }
                        },
                        {
                            "text": "Kupiłam jabłka i zjadłam <w>wszystkie</w> jeszcze w drodze.",
                            "target_lang_translations": {
                                "ru": "Я купила яблоки и съела <w>все</w> ещё по дороге."
                            }
                        }
                    ],
                    "antonyms": [
                        "żadna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заменяет существительные, обозначая совокупность упомянутых лиц, животных или предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "все",
                                "target_lang_sense_clarification": "местоимение-существительное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
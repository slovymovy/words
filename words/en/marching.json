        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1e731177-4164-3967-8e03-1dac234cb601",
                    "sense_definition": "Relating to, suited for, or performed while marching.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He plays the drums in the school <w>marching</w> band.",
                            "target_lang_translations": {
                                "ru": "Он играет на барабанах в школьном <w>маршевом</w> оркестре."
                            }
                        },
                        {
                            "text": "The soldiers received their <w>marching</w> orders early in the morning.",
                            "target_lang_translations": {
                                "ru": "Солдаты получили приказ о <w>выступлении</w> рано утром."
                            }
                        }
                    ],
                    "common_phrases": [
                        "marching band",
                        "marching orders",
                        "marching song"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к маршу, предназначенный для него или выполняемый во время марша."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маршевый",
                                "target_lang_sense_clarification": "относящийся к музыке или шагу"
                            },
                            {
                                "target_lang_word": "походный",
                                "target_lang_sense_clarification": "относящийся к передвижению войск"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9142e442-658f-36e6-b985-6c63ef51dd08",
                    "sense_definition": "Arranged or situated in a line or sequence, resembling soldiers in formation.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>marching</w> ants carried small crumbs back to their colony.",
                            "target_lang_translations": {
                                "ru": "<w>Марширующие</w> муравьи несли маленькие крошки обратно в свою колонию."
                            }
                        },
                        {
                            "text": "We looked out at the <w>marching</w> fence posts stretching across the field.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели на <w>выстроившиеся</w> столбы забора, тянущиеся через поле."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный в линию или последовательность, напоминающий солдат в строю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идущий строем",
                                "target_lang_sense_clarification": "движущийся организованно"
                            },
                            {
                                "target_lang_word": "выстроившийся",
                                "target_lang_sense_clarification": "расположенный в ряд"
                            }
                        ]
                    }
                }
            ]
        },
        {

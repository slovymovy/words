            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e6a90505-e6bc-3a10-b8a0-0015d077272e",
                    "sense_definition": "Zespół ludzi pochodzących od jednego przodka, połączonych więzami krwi i wspólnym nazwiskiem, często o ustalonym statusie społecznym lub historycznym.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "lineage_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary <w>ród</w> zamieszkuje zamek na wzgórzu od wielu wieków.",
                            "target_lang_translations": {
                                "ru": "Этот древний <w>род</w> обитает в замке на холме уже много веков."
                            }
                        },
                        {
                            "text": "Głowa <w>rodu</w> podjęła decyzję o podziale majątku między synów.",
                            "target_lang_translations": {
                                "ru": "Глава <w>рода</w> принял решение о разделе имущества между сыновьями."
                            }
                        }
                    ],
                    "synonyms": [
                        "klan",
                        "klan",
                        "klan"
                    ],
                    "common_phrases": [
                        "głowa rodu",
                        "ród królewski",
                        "ród szlachecki",
                        "wywodzić się z rodu",
                        "starożytny ród"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, происходящих от одного предка, связанных кровными узами и общей фамилией, часто имеющая определенный социальный или исторический статус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "род",
                                "target_lang_sense_clarification": "династия, семья"
                            },
                            {
                                "target_lang_word": "династия",
                                "target_lang_sense_clarification": "правящий или знатный род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cee72a0b-cf36-36af-9057-6c008a622e13",
                    "sense_definition": "Samodzielna społeczność w obrębie plemienia, obejmująca rodziny wywodzące się od wspólnego przodka, charakterystyczna dla ludów pierwotnych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "lineage_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy <w>ród</w> w tym plemieniu miał swoje własne totemy i święte miejsca.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>род</w> в этом племени имел свои собственные тотемы и священные места."
                            }
                        },
                        {
                            "text": "Konflikty między zwaśnionymi <w>rodami</w> trwały przez wiele pokoleń.",
                            "target_lang_translations": {
                                "ru": "Конфликты между враждующими <w>родами</w> длились много поколений."
                            }
                        }
                    ],
                    "synonyms": [
                        "klan"
                    ],
                    "common_phrases": [
                        "starszyzna rodu",
                        "ród plemienny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обособленная социальная группа внутри племени, состоящая из семей, ведущих происхождение от общего предка; характерна для первобытных обществ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "род",
                                "target_lang_sense_clarification": "племенная община"
                            },
                            {
                                "target_lang_word": "клан",
                                "target_lang_sense_clarification": "родовая община"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e1ce009-54ba-31e1-9511-6cef12f99c5b",
                    "sense_definition": "Pochodzenie lub miejsce urodzenia; współcześnie używane niemal wyłącznie w formie narzędnika w utartych zwrotach określających skąd coś lub ktoś się wywodzi.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest pomysł <w>rodem</w> z taniego filmu science-fiction.",
                            "target_lang_translations": {
                                "ru": "Это идея <w>родом</w> из дешевого научно-фантастического фильма."
                            }
                        },
                        {
                            "text": "Mój dziadek był <w>rodem</w> z małej wioski na wschodzie kraju.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был <w>родом</w> из маленькой деревни на востоке страны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rodem z",
                        "rodem z piekła"
                    ],
                    "traits": [

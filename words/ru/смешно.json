    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e5f3048d-48a4-314e-a7b4-2359ed5d5e09",
                    "sense_definition": "Таким образом, что это вызывает смех; забавно, комично.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Клоун <смешно> двигался по арене, развлекая детей.",
                            "target_lang_translations": {
                                "en": "The clown moved <comically> around the arena, entertaining the children."
                            }
                        },
                        {
                            "text": "Она <смешно> сморщила нос, когда попробовала лимон.",
                            "target_lang_translations": {
                                "en": "She wrinkled her nose <comically> when she tasted the lemon."
                            }
                        }
                    ],
                    "synonyms": [
                        "забавно"
                    ],
                    "antonyms": [
                        "несмешно"
                    ],
                    "common_phrases": [
                        "смешно выглядеть",
                        "смешно одеваться"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that causes laughter; amusingly or comically."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "comically",
                                "target_lang_sense_clarification": "in a funny or amusing manner"
                            },
                            {
                                "target_lang_word": "funny",
                                "target_lang_sense_clarification": "informal adverbial use"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e0615c5-0f72-3304-9b2f-d420e3695566",
                    "sense_definition": "О чувстве веселья, желании смеяться, которое испытывает кто-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне было <смешно> слушать эту старую историю.",
                            "target_lang_translations": {
                                "en": "It was <funny> for me to listen to this old story."
                            }
                        },
                        {
                            "text": "Ему стало <смешно>, когда он увидел своё отражение в зеркале.",
                            "target_lang_translations": {
                                "en": "He found it <funny> when he saw his reflection in the mirror."
                            }
                        }
                    ],
                    "synonyms": [
                        "весело"
                    ],
                    "antonyms": [
                        "грустно",
                        "печально"
                    ],
                    "common_phrases": [
                        "мне смешно",
                        "ему смешно",
                        "до слёз смешно"
                    ],
                    "target_lang_definitions": {
                        "en": "Describes a feeling of amusement or the state of finding something humorous."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "funny",
                                "target_lang_sense_clarification": "causing amusement"
                            },
                            {
                                "target_lang_word": "amusing",
                                "target_lang_sense_clarification": "entertaining"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63910af2-da08-39b0-b5df-bbf1fe8f6a41",
                    "sense_definition": "Оценка ситуации как нелепой, странной или бессмысленной.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "absurdity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Просто <смешно> обижаться на такие пустяки.",
                            "target_lang_translations": {
                                "en": "It is simply <ridiculous> to get offended by such trifles."
                            }
                        },
                        {
                            "text": "Было бы <смешно> надеяться на быстрый успех в этом деле.",
                            "target_lang_translations": {
                                "en": "It would be <laughable> to hope for quick success in this matter."
                            }
                        }
                    ],
                    "synonyms": [
                        "нелепо",
                        "странно"
                    ],
                    "common_phrases": [
                        "просто смешно",
                        "смешно сказать"
                    ],
                    "target_lang_definitions": {
                        "en": "Describes a situation as absurd, senseless, or unreasonable."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ridiculous",
                                "target_lang_sense_clarification": "absurd or unreasonable"
                            },
                            {
                                "target_lang_word": "laughable",
                                "target_lang_sense_clarification": "so silly it deserves to be laughed at"
                            }
                        ]
                    }
                }
            ]
        },

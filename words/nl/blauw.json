    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "36598dcf-24d9-3d34-b67e-40e081010d27",
                    "sense_definition": "Met de kleur van een onbewolkte lucht; een van de primaire kleuren.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "color_blue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De lucht is vandaag helder en <w>blauw</w>.",
                            "target_lang_translations": {
                                "ru": "Небо сегодня ясное и <w>голубое</w>."
                            }
                        },
                        {
                            "text": "Zij heeft een prachtige <w>blauwe</w> jurk gekocht voor het feest.",
                            "target_lang_translations": {
                                "ru": "Она купила красивое <w>синее</w> платье для вечеринки."
                            }
                        },
                        {
                            "text": "Ik heb gisteren een <w>blauwe</w> brief van de belastingdienst ontvangen.",
                            "target_lang_translations": {
                                "ru": "Вчера я получил <w>синее</w> письмо из налоговой службы."
                            }
                        }
                    ],
                    "synonyms": [
                        "azuur",
                        "hemelsblauw"
                    ],
                    "antonyms": [
                        "oranje"
                    ],
                    "common_phrases": [
                        "blauw bloed",
                        "een blauwe maandag",
                        "blauwe brief"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий цвет безоблачного неба; один из основных цветов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синий",
                                "target_lang_sense_clarification": "основной цвет"
                            },
                            {
                                "target_lang_word": "голубой",
                                "target_lang_sense_clarification": "светло-синий, цвет неба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2fb0c11-447e-3f82-988a-b70735179c2a",
                    "sense_definition": "Zichtbaar gekneusd; met donkere verkleuringen op de huid door een klap of stoot.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de val was zijn knie dik en <w>blauw</w>.",
                            "target_lang_translations": {
                                "ru": "После падения его колено распухло и стало <w>синим</w>."
                            }
                        },
                        {
                            "text": "Hij liep een <w>blauw</w> oog op tijdens de ruzie.",
                            "target_lang_translations": {
                                "ru": "Он получил <w>подбитый</w> глаз во время ссоры."
                            }
                        },
                        {
                            "text": "De bokser was na de wedstrijd <w>bont en blauw</w> geslagen.",
                            "target_lang_translations": {
                                "ru": "После боя боксер был весь в <w>синяках</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gekneusd"
                    ],
                    "common_phrases": [
                        "bont en blauw",
                        "een blauw oog",
                        "blauwe plek"
                    ],
                    "target_lang_definitions": {
                        "ru": "С видимыми ушибами; с темным изменением цвета кожи от удара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синий",
                                "target_lang_sense_clarification": "о цвете кожи при ушибе"
                            },
                            {
                                "target_lang_word": "в синяках",
                                "target_lang_sense_clarification": "покрытый ушибами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0d54b7a-e580-3bc4-b89f-1b8024147cd3",
                    "sense_definition": "In een toestand van dronkenschap.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was gisteravond behoorlijk <w>blauw</w> na al dat bier.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером он был изрядно <w>пьян</w> после всего этого пива."
                            }
                        },
                        {
                            "text": "Ze kwamen midden in de nacht <w>blauw</w> thuis van het feest.",
                            "target_lang_translations": {
                                "ru": "Они вернулись домой с вечеринки посреди ночи <w>синими</w>."
                            }

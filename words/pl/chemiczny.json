                    }
                },
                {
                    "sense_id": "f4c9f0e1-45e9-3c75-b4f8-f326786ace2f",
                    "sense_definition": "nienaturalny, zawierający sztuczne dodatki lub wytworzony w procesie przemysłowym; często w odniesieniu do żywności",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten napój ma bardzo <w>chemiczny</w> posmak i mi nie smakuje.",

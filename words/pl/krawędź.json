    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8cbf356e-c60d-3c27-ad0d-59be6cc06d08",
                    "sense_definition": "Linia wyznaczająca koniec powierzchni przedmiotu lub granica, za którą teren gwałtownie opada.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Postawił kubek z gorącą kawą na samej <w>krawędzi</w> biurka.",
                            "target_lang_translations": {
                                "ru": "Он поставил кружку с горячим кофе на самый <w>край</w> письменного стола."
                            }
                        },
                        {
                            "text": "Turyści ostrożnie podeszli do <w>krawędzi</w> stromego urwiska, aby zrobić zdjęcie.",
                            "target_lang_translations": {
                                "ru": "Туристы осторожно подошли к <w>краю</w> крутого обрыва, чтобы сделать фотографию."
                            }
                        }
                    ],
                    "synonyms": [
                        "brzeg",
                        "skraj",
                        "kraniec",
                        "obrzeże",
                        "kant",
                        "rant"
                    ],
                    "antonyms": [
                        "środek",
                        "wnętrze"
                    ],
                    "common_phrases": [
                        "krawędź stołu",
                        "krawędź urwiska",
                        "ostra krawędź",
                        "krawędź jezdni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Линия, обозначающая конец поверхности предмета, или граница, за которой местность резко обрывается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "граница поверхности"
                            },
                            {
                                "target_lang_word": "кромка",
                                "target_lang_sense_clarification": "острый край или обрез"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3144c067-64cb-37c4-86eb-ac4042c0bbda",
                    "sense_definition": "Stan krytyczny lub moment bezpośrednio poprzedzający jakieś ważne, często negatywne wydarzenie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z powodu złego zarządzania firma znalazła się na <w>krawędzi</w> bankructwa.",
                            "target_lang_translations": {
                                "ru": "Из-за плохого управления фирма оказалась на <w>грани</w> банкротства."
                            }
                        },
                        {
                            "text": "Po wielu godzinach pracy bez przerwy był na <w>krawędzi</w> wyczerpania.",
                            "target_lang_translations": {
                                "ru": "После многих часов работы без перерыва он был на <w>грани</w> истощения."
                            }
                        }
                    ],
                    "synonyms": [
                        "skraj"
                    ],
                    "common_phrases": [
                        "na krawędzi bankructwa",
                        "na krawędzi wytrzymałości",
                        "na krawędzi życia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Критическое состояние или момент, непосредственно предшествующий какому-либо важному, часто негативному событию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грань",
                                "target_lang_sense_clarification": "критическая черта"
                            },
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "предел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2aa31be7-3de3-3fff-bf76-92920084b9c3",
                    "sense_definition": "Odcinek łączący dwa sąsiednie wierzchołki wielościanu, będący częścią wspólną dwóch ścian.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "math_geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy sześcian posiada osiem wierzchołków i dwanaście równych <w>krawędzi</w>.",
                            "target_lang_translations": {
                                "ru": "У каждого куба восемь вершин и двенадцать равных <w>рёбер</w>."
                            }
                        },
                        {
                            "text": "Zadanie polega na obliczeniu długości <w>krawędzi</w> podstawy ostrosłupa.",
                            "target_lang_translations": {
                                "ru": "Задача состоит в вычислении длины <w>ребра</w> основания пирамиды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "krawędź sześcianu",
                        "krawędź boczna",
                        "krawędź podstawy",
                        "długość krawędzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрезок, соединяющий две соседние вершины многогранника и являющийся общей частью двух граней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ребро",
                                "target_lang_sense_clarification": "геометрия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d97f7696-5a2f-39eb-977e-c19e52d82cdc",
                    "sense_definition": "Połączenie między dwoma wierzchołkami w grafie matematycznym.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_graph",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W grafie nieskierowanym każda <w>krawędź</w> łączy dwa wierzchołki bez określania kierunku.",
                            "target_lang_translations": {
                                "ru": "В неориентированном графе каждое <w>ребро</w> соединяет две вершины без определения направления."
                            }
                        },
                        {
                            "text": "Algorytm sprawdza, czy dana <w>krawędź</w> należy do najkrótszej ścieżki w grafie.",
                            "target_lang_translations": {
                                "ru": "Алгоритм проверяет, принадлежит ли данное <w>ребро</w> кратчайшему пути в графе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "waga krawędzi",
                        "zbiór krawędzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединение между двумя вершинами в математическом графе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ребро",
                                "target_lang_sense_clarification": "теория графов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

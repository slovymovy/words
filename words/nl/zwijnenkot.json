    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "153faa7b-6a2e-37f8-876f-a1d33d17a6ce",
                    "sense_definition": "Een erg vieze en rommelige plek, vaak gebruikt om een onopgeruimde kamer of huis te beschrijven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "messy_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet je kamer opruimen, want het is echt een <w>zwijnenkot</w>.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно убраться в комнате, потому что это настоящий <w>свинарник</w>."
                            }
                        },
                        {
                            "text": "Na het feestje zag de woonkamer eruit als een regelrecht <w>zwijnenkot</w>.",
                            "target_lang_translations": {
                                "ru": "После вечеринки гостиная выглядела как самый настоящий <w>свинарник</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "varkensstal",
                        "puinhoop",
                        "bende",
                        "troep"
                    ],
                    "common_phrases": [
                        "een regelrecht zwijnenkot",
                        "lijken op een zwijnenkot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень грязное и беспорядочное место; часто используется для описания неубранной комнаты или дома."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свинарник",
                                "target_lang_sense_clarification": "перен. грязное место"
                            },
                            {
                                "target_lang_word": "хлев",
                                "target_lang_sense_clarification": "перен. грязное помещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07d9ba1e-580d-3759-9587-365cf6d816d7",
                    "sense_definition": "Een verblijfplaats of hok waarin varkens worden gehouden.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "animal_housing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer maakte het <w>zwijnenkot</w> schoon en gaf de dieren eten.",
                            "target_lang_translations": {
                                "ru": "Фермер вычистил <w>свинарник</w> и покормил животных."
                            }
                        },
                        {
                            "text": "De varkens lagen lekker te slapen in het modderige <w>zwijnenkot</w>.",
                            "target_lang_translations": {
                                "ru": "Свиньи сладко спали в грязном <w>свинарнике</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "varkensstal",
                        "varkenshok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещение или загон, где содержатся свиньи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свинарник",
                                "target_lang_sense_clarification": "помещение для свиней"
                            },
                            {
                                "target_lang_word": "хлев",
                                "target_lang_sense_clarification": "помещение для скота"
                            }
                        ]
                    }
                }
            ]
        }

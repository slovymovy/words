                {
                    "sense_id": "9175cb60-070a-3484-853a-bc5521925fe5",
                    "sense_definition": "Rezultat czyjegoś działania, pracy, procesu lub wyobraźni; coś, co zostało stworzone.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten poemat to <twór> jego młodzieńczej i bujnej fantazji.",
                            "target_lang_translations": {
                                "ru": "Эта поэма — <творение> его юной и буйной фантазии."
                            }
                        },
                        {
                            "text": "Cała ta historia to tylko <twór> twojej wyobraźni, nic takiego się nie stało.",
                            "target_lang_translations": {
                                "ru": "Вся эта история — лишь <плод> твоего воображения, ничего такого не случилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytwór",
                        "dzieło",
                        "produkt",
                        "rezultat"
                    ],
                    "common_phrases": [
                        "twór wyobraźni",
                        "dziwny twór",
                        "sztuczny twór"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e3425123-e217-38de-b019-e5a79cf9b0f5",
                    "sense_definition": "Een voortzetting van iets dat eerder is begonnen, of een deel dat na een eerder deel komt (zoals bij een boek of film).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We wachten allemaal vol spanning op het <w>vervolg</w> van de serie.",
                            "target_lang_translations": {
                                "en": "We are all eagerly waiting for the <w>sequel</w> to the series.",
                                "ru": "Мы все с нетерпением ждем <w>продолжения</w> сериала."
                            }
                        },
                        {
                            "text": "Dit dikke boek is een <w>vervolg</w> op zijn eerste succesvolle roman.",
                            "target_lang_translations": {
                                "en": "This thick book is a <w>sequel</w> to his first successful novel.",
                                "ru": "Эта толстая книга — <w>продолжение</w> его первого успешного романа."
                            }
                        }
                    ],
                    "synonyms": [
                        "voortzetting",
                        "vervolgdeel",
                        "continuering"
                    ],
                    "antonyms": [
                        "begin",
                        "start",
                        "inleiding"
                    ],
                    "common_phrases": [
                        "wordt vervolgd",
                        "in het vervolg",
                        "als vervolg op"
                    ],
                    "target_lang_definitions": {
                        "en": "A continuation of something started earlier, or a part that comes after an earlier part (such as with a book or film).",
                        "ru": "Продолжение чего-либо, начатого ранее, или часть, следующая за предыдущей (например, в книге или фильме)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sequel",
                                "target_lang_sense_clarification": "subsequent part of a book or film"
                            },
                            {
                                "target_lang_word": "continuation",
                                "target_lang_sense_clarification": "act of continuing or extending"
                            },
                            {
                                "target_lang_word": "follow-up",
                                "target_lang_sense_clarification": "subsequent action or work"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "продолжение",
                                "target_lang_sense_clarification": "следующая часть или развитие событий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4881bb9a-64d2-3595-9482-6bae0e39054f",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>vervolgen</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>vervolg</w> mijn weg naar huis na een korte pauze.",
                            "target_lang_translations": {
                                "en": "I <w>continue</w> my way home after a short break.",
                                "ru": "Я <w>продолжаю</w> свой путь домой после короткого перерыва."
                            }
                        },
                        {
                            "text": "Ik <w>vervolg</w> mijn studie zodra ik weer beter ben.",
                            "target_lang_translations": {
                                "en": "I will <w>resume</w> my studies as soon as I am better.",
                                "ru": "Я <w>продолжу</w> учебу, как только поправлюсь."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of <w>vervolgen</w>.",
                        "ru": "Первое лицо единственного числа настоящего времени от глагола <w>vervolgen</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "continue",
                                "target_lang_sense_clarification": "to proceed with"
                            },
                            {
                                "target_lang_word": "resume",
                                "target_lang_sense_clarification": "to start again after a pause"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "продолжаю",
                                "target_lang_sense_clarification": "действие продолжения"
                            },
                            {
                                "target_lang_word": "преследую",
                                "target_lang_sense_clarification": "действие преследования"
                            }
                        ]
                    }
                },
                {

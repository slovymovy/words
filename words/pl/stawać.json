            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2dc68364-01c8-3acc-94b0-a5d2faa5b49b",
                    "sense_definition": "Zmieniać się w kogoś lub w coś; nabywać nową cechę lub właściwość (używane z zaimkiem zwrotnym „się”)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z każdym dniem pogoda <staje> się coraz ładniejsza.",
                            "target_lang_translations": {
                                "ru": "С каждым днём погода <становится> всё лучше."
                            }
                        },
                        {
                            "text": "Kiedy dorosnę, chcę <stawać> się lepszym człowiekiem każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Когда я вырасту, я хочу с каждым днём <становиться> лучше."
                            }
                        },
                        {
                            "text": "To zagadnienie <staje> się jasne dopiero po głębszej analizie.",
                            "target_lang_translations": {
                                "ru": "Этот вопрос <становится> ясным только после глубокого анализа."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatrzymywać się",
                        "zatrzymywać się"
                    ],
                    "common_phrases": [
                        "stawać się dorosłym",
                        "stawać się jasne",
                        "stawać się modne",
                        "stawać się faktem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превращаться в кого-то или во что-то; приобретать новое качество или свойство (используется с возвратным местоимением «się»)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "становиться",
                                "target_lang_sense_clarification": "делаться кем-то или каким-то"
                            },
                            {
                                "target_lang_word": "делаться",
                                "target_lang_sense_clarification": "разг. становиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c50d0b39-5ed1-326d-b1ca-589d333968d6",
                    "sense_definition": "Przerywać poruszanie się; zatrzymywać się w miejscu",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pociąg zawsze <staje> na tej małej stacji.",
                            "target_lang_translations": {
                                "ru": "Поезд всегда <останавливается> на этой маленькой станции."
                            }
                        },
                        {
                            "text": "Musisz <stawać>, gdy widzisz czerwone światło.",
                            "target_lang_translations": {
                                "ru": "Ты должен <останавливаться>, когда видишь красный свет."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatrzymywać się"
                    ],
                    "antonyms": [
                        "ruszać"
                    ],
                    "common_phrases": [
                        "stawać na światłach",
                        "stawać dęba",
                        "stawać w miejscu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекращать движение; останавливаться на месте"

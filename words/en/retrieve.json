{
    "word_family": [
        "retrieval",
        "retriever",
        "retrievable",
        "irretrievable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3954d858-6219-3476-acd0-88b6a314a79f",
                    "sense_definition": "To find and get something back that was lost, taken, or left somewhere.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "recovery_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bent down to <w>retrieve</w> her earring from the carpet.",
                            "target_lang_translations": {
                                "ru": "Она наклонилась, чтобы <w>достать</w> свою сережку с ковра."
                            }
                        },
                        {
                            "text": "Police managed to <w>retrieve</w> the stolen goods from the warehouse.",
                            "target_lang_translations": {
                                "ru": "Полиции удалось <w>вернуть</w> украденные товары со склада."
                            }
                        }
                    ],
                    "synonyms": [
                        "recover",
                        "regain",
                        "reclaim",
                        "get back"
                    ],
                    "antonyms": [
                        "lose"
                    ],
                    "common_phrases": [
                        "retrieve luggage",
                        "retrieve a bag",
                        "retrieve items"
                    ],
                    "target_lang_definitions": {
                        "ru": "Найти и вернуть что-то, что было потеряно, взято или оставлено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вернуть",
                                "target_lang_sense_clarification": "получить обратно"
                            },
                            {
                                "target_lang_word": "отыскать",
                                "target_lang_sense_clarification": "найти в результате поисков"
                            },
                            {
                                "target_lang_word": "достать",
                                "target_lang_sense_clarification": "извлечь откуда-либо, дотянуться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9525fd5e-6ac9-34bd-b8d9-70597506ac6f",
                    "sense_definition": "To find and access information stored in a computer or system.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "recovery_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can easily <w>retrieve</w> deleted emails from the trash folder.",
                            "target_lang_translations": {
                                "ru": "Вы можете легко <w>восстановить</w> удаленные письма из папки «Корзина»."
                            }
                        },
                        {
                            "text": "The system allows users to <w>retrieve</w> data in seconds.",
                            "target_lang_translations": {
                                "ru": "Система позволяет пользователям <w>извлекать</w> данные за считанные секунды."
                            }
                        }
                    ],
                    "synonyms": [
                        "access",
                        "fetch"
                    ],
                    "antonyms": [
                        "save",
                        "store"
                    ],
                    "common_phrases": [
                        "retrieve data",
                        "retrieve information",
                        "retrieve a file"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Находить и получать доступ к информации, хранящейся в компьютере или системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восстановить",
                                "target_lang_sense_clarification": "вернуть удаленные данные"
                            },
                            {
                                "target_lang_word": "извлечь",
                                "target_lang_sense_clarification": "получить данные из системы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "317830f0-d52b-3dd9-82ee-7c96a0249527",
                    "sense_definition": "To run after and bring back a thrown object or killed game, typically done by a dog.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The golden retriever swam into the lake to <w>retrieve</w> the stick.",
                            "target_lang_translations": {
                                "ru": "Золотистый ретривер поплыл в озеро, чтобы <w>принести</w> палку."
                            }
                        },
                        {
                            "text": "Hunting dogs are trained to <w>retrieve</w> birds without damaging them.",
                            "target_lang_translations": {
                                "ru": "Охотничьих собак обучают <w>приносить</w> птиц, не повреждая их."
                            }
                        }
                    ],
                    "synonyms": [
                        "fetch"
                    ],
                    "common_phrases": [
                        "retrieve a ball",
                        "retrieve game",
                        "retrieve a stick"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бежать за брошенным предметом и приносить его обратно (обычно о собаках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принести",
                                "target_lang_sense_clarification": "доставить брошенный предмет"
                            },
                            {
                                "target_lang_word": "апортировать",
                                "target_lang_sense_clarification": "термин в дрессировке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f68fe77-d85c-31b7-92cd-0711fa0b8046",
                    "sense_definition": "To improve a bad situation or correct a mistake.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "remediation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was too late to <w>retrieve</w> the situation after the scandal broke.",
                            "target_lang_translations": {
                                "ru": "Было слишком поздно <w>исправить</w> ситуацию после того, как разразился скандал."
                            }
                        },
                        {
                            "text": "He tried to <w>retrieve</w> his honor by apologizing publicly.",
                            "target_lang_translations": {
                                "ru": "Он попытался <w>спасти</w> свою честь, публично извинившись."
                            }
                        }
                    ],
                    "synonyms": [
                        "rectify",
                        "salvage",
                        "remedy"
                    ],
                    "common_phrases": [
                        "retrieve the situation",
                        "retrieve one's reputation",
                        "retrieve a loss"
                    ],
                    "target_lang_definitions": {
                        "ru": "Улучшить плохую ситуацию или исправить ошибку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исправить",
                                "target_lang_sense_clarification": "положение или ошибку"
                            },
                            {
                                "target_lang_word": "спасти",
                                "target_lang_sense_clarification": "уберечь от провала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93e7d506-d75c-39ef-9508-9e01f8d71fb6",
                    "sense_definition": "To bring information back into one's mind from memory.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "recovery_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I struggled to <w>retrieve</w> the name of the actor I saw yesterday.",
                            "target_lang_translations": {
                                "ru": "Я с трудом пытался <w>вспомнить</w> имя актера, которого видел вчера."
                            }
                        },
                        {
                            "text": "The brain can <w>retrieve</w> distant memories when triggered by a smell.",
                            "target_lang_translations": {
                                "ru": "Мозг может <w>восстанавливать</w> давние воспоминания, когда срабатывает триггер на запах."
                            }
                        }
                    ],
                    "synonyms": [
                        "recall",
                        "remember"
                    ],
                    "antonyms": [
                        "forget"
                    ],
                    "common_phrases": [
                        "retrieve a memory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восстанавливать информацию в памяти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вспомнить",
                                "target_lang_sense_clarification": "восстановить в памяти"
                            },
                            {
                                "target_lang_word": "восстановить",
                                "target_lang_sense_clarification": "оживить воспоминания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7540b38a-1462-3bee-ba98-5fbc011cdfb2",
                    "sense_definition": "The act of successfully returning a difficult ball in sports like tennis or squash.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd cheered at his incredible <w>retrieve</w> of the smash.",
                            "target_lang_translations": {
                                "ru": "Толпа приветствовала его невероятный <w>приём</w> смэша."
                            }
                        },
                        {
                            "text": "Making a successful <w>retrieve</w> from the back of the court requires speed.",
                            "target_lang_translations": {
                                "ru": "Успешный <w>возврат</w> мяча с задней линии требует скорости."
                            }
                        }
                    ],
                    "synonyms": [
                        "return"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Успешный возврат трудного мяча в таких видах спорта, как теннис или сквош."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "мяча"
                            },
                            {
                                "target_lang_word": "возврат",
                                "target_lang_sense_clarification": "ответный удар"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "463377bf-3629-3edc-adcd-6f32cbd2dc65",
                    "sense_definition": "The act of a hunting dog finding and bringing back game.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "animal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dog performed a perfect <w>retrieve</w> of the pheasant.",
                            "target_lang_translations": {
                                "ru": "Собака выполнила идеальную <w>подачу</w> фазана."
                            }
                        },
                        {
                            "text": "We watched the spaniel's <w>retrieve</w> with admiration.",
                            "target_lang_translations": {
                                "ru": "Мы с восхищением наблюдали за <w>апортировкой</w> спаниеля."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "hunting"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие охотничьей собаки по поиску и подносу дичи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подача",
                                "target_lang_sense_clarification": "дичи охотнику"
                            },
                            {
                                "target_lang_word": "апортировка",
                                "target_lang_sense_clarification": "команда или действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
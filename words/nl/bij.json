                {
                    "sense_id": "6332c8a5-2173-3bac-9421-a84a6b941014",
                    "sense_definition": "In de directe omgeving van; op korte afstand van.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locatie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wacht op je <w>bij</w> de ingang van de winkel.",
                            "target_lang_translations": {
                                "en": "I'll wait for you <w>at</w> the entrance of the store.",
                                "ru": "Я жду тебя <w>у</w> входа в магазин."
                            }
                        },
                        {
                            "text": "Het station is vlak <w>bij</w> mijn huis.",
                            "target_lang_translations": {

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1345c0b6-f33c-3be7-a470-a50d654384d5",
                    "sense_definition": "De lengte van een touw of kabel waaraan iets vastzit een beetje langer maken.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zeiler moest de schoot voorzichtig <w>bijvieren</w> om de druk op het zeil te verminderen.",
                            "target_lang_translations": {
                                "en": "The sailor had to carefully <w>ease</w> the sheet to reduce the pressure on the sail.",
                                "ru": "Яхтсмену пришлось осторожно <w>потравить</w> шкот, чтобы уменьшить давление на парус."
                            }
                        },
                        {
                            "text": "Je moet het touw langzaam <w>bijvieren</w> zodat de lading gecontroleerd naar beneden zakt.",
                            "target_lang_translations": {
                                "en": "You must slowly <w>pay out</w> the rope so the load descends in a controlled manner.",
                                "ru": "Нужно медленно <w>отпускать</w> веревку, чтобы груз опускался под контролем."
                            }
                        }
                    ],
                    "synonyms": [
                        "vieren"
                    ],
                    "antonyms": [
                        "aanhalen"
                    ],
                    "common_phrases": [
                        "de schoot bijvieren",
                        "het touw bijvieren"
                    ],
                    "target_lang_definitions": {
                        "en": "To slightly increase the length of a rope or cable to which something is attached.",
                        "ru": "Немного увеличить длину веревки или троса, к которому что-то прикреплено, чтобы ослабить натяжение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slacken",
                                "target_lang_sense_clarification": "to make a rope less tight"
                            },
                            {
                                "target_lang_word": "pay out",
                                "target_lang_sense_clarification": "to let out more length of a rope"
                            },
                            {
                                "target_lang_word": "ease",
                                "target_lang_sense_clarification": "nautical: to loosen a line carefully"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "травить",
                                "target_lang_sense_clarification": "выпускать трос или веревку (морской термин)"
                            },
                            {
                                "target_lang_word": "ослаблять",
                                "target_lang_sense_clarification": "делать веревку менее натянутой"
                            },
                            {
                                "target_lang_word": "отпускать",
                                "target_lang_sense_clarification": "давать слабину"
                            }
                        ]
                    }
                }
            ]

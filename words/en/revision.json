            "senses": [
                {
                    "sense_id": "d3fda3c6-6a19-3629-b43d-59153372cea7",
                    "sense_definition": "The action of reviewing notes or materials previously learned, especially in preparation for a test or examination.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She spent the entire weekend doing <w>revision</w> for her final chemistry exam.",
                            "target_lang_translations": {
                                "ru": "Она провела все выходные за <w>повторением</w> материала к выпускному экзамену по химии."
                            }
                        },
                        {
                            "text": "My <w>revision</w> notes are messy, but they help me remember the key facts.",
                            "target_lang_translations": {
                                "ru": "Мои конспекты для <w>повторения</w> небрежные, но они помогают мне запомнить ключевые факты."
                            }
                        }
                    ],
                    "synonyms": [
                        "review"
                    ],
                    "common_phrases": [
                        "do revision",
                        "exam revision",
                        "revision notes",
                        "last-minute revision"
                    ],
                    "traits": [

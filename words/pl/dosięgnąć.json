                },
                {
                    "sense_id": "c7849793-6ded-3595-a62f-5a3ed7eb2eba",
                    "sense_definition": "Osiągnąć pewien wysoki poziom, wartość lub granicę w jakimś procesie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_level",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego popularność jako pisarza <w>dosięgła</w> szczytu w latach dwudziestych ubiegłego wieku.",
                            "target_lang_translations": {
                                "ru": "Его популярность как писателя <w>достигла</w> пика в двадцатые годы прошлого века."
                            }

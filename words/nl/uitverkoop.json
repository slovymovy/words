{
    "word_family": [
        "uitverkopen",
        "verkoop",
        "verkopen",
        "verkoper"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c0169b8-6672-306b-bf27-c48a3b90fdfd",
                    "sense_definition": "Een periode waarin winkels producten tegen verlaagde prijzen verkopen, vaak om van oude voorraad af te komen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>uitverkoop</w> begint morgen en duurt twee weken.",
                            "target_lang_translations": {
                                "ru": "<w>Распродажа</w> начинается завтра и продлится две недели."
                            }
                        },
                        {
                            "text": "Ik heb deze schoenen met veel korting gekocht in de <w>uitverkoop</w>.",
                            "target_lang_translations": {
                                "ru": "Я купил эти туфли с большой скидкой на <w>распродаже</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "opruiming",
                        "sale",
                        "solden"
                    ],
                    "common_phrases": [
                        "in de uitverkoop",
                        "uitverkoop houden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период, когда магазины продают товары по сниженным ценам, часто чтобы избавиться от старых запасов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распродажа",
                                "target_lang_sense_clarification": "продажа товаров со скидками"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3b5108cf-af45-3491-9538-d29d8e732c13",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>uitverkopen</w> (in een bijzin).",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is de bedoeling dat ik de oude collectie snel <w>uitverkoop</w>.",
                            "target_lang_translations": {
                                "ru": "Цель в том, чтобы я быстро <w>распродал</w> старую коллекцию."
                            }
                        },
                        {
                            "text": "Als ik alles <w>uitverkoop</w>, kan ik ruimte maken voor nieuwe producten.",
                            "target_lang_translations": {
                                "ru": "Если я всё <w>распродам</w>, я смогу освободить место для новых товаров."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола uitverkopen (в придаточном предложении)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распродаю",
                                "target_lang_sense_clarification": "действие по полной продаже запасов"
                            },
                            {
                                "target_lang_word": "распродам",
                                "target_lang_sense_clarification": "совершенный вид, часто используется в условных предложениях"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "455ec17e-de4a-3995-9e67-03b124c3d9a8",
                    "sense_definition": "A surname, most notably associated with Harry S. Truman, the 33rd President of the United States.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_name_1",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "President <w>Truman</w> took office after the death of Franklin D. Roosevelt.",
                            "target_lang_translations": {
                                "ru": "Президент <w>Трумэн</w> вступил в должность после смерти Франклина Д. Рузвельта."
                            }
                        },
                        {
                            "text": "Mr. <w>Truman</w> signed the documents at the bank yesterday.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Труман</w> подписал документы в банке вчера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Harry S. Truman",
                        "Truman Doctrine"
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, наиболее известная благодаря Гарри С. Трумэну, 33-му президенту США."
                    },
                    "translations": {
                        "ru": [
                            {

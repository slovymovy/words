                {
                    "sense_id": "ddd41efb-a701-3dc0-9df7-1bed7930953f",
                    "sense_definition": "Of or relating to a specific territory or geographic area.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two nations finally resolved their long-standing <w>territorial</w> dispute.",
                            "target_lang_translations": {
                                "ru": "Две страны наконец разрешили свой давний <w>территориальный</w> спор."
                            }
                        },
                        {
                            "text": "Fishing boats are not allowed to enter the country's <w>territorial</w> waters without permission.",
                            "target_lang_translations": {
                                "ru": "Рыболовецким судам запрещено входить в <w>территориальные</w> воды страны без разрешения."
                            }
                        }
                    ],
                    "synonyms": [
                        "geographic",
                        "regional"
                    ],
                    "antonyms": [
                        "extraterritorial"
                    ],
                    "common_phrases": [
                        "territorial waters",
                        "territorial integrity",
                        "territorial claims",
                        "territorial dispute"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к определённой территории или географическому району."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "территориальный",
                                "target_lang_sense_clarification": "относящийся к территории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32d39bdb-45f7-3959-aa2f-b7e8bf64ac6b",
                    "sense_definition": "Protective of one's personal space, ownership, or status; unwilling to share control.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Male lions are extremely <w>territorial</w> and will aggressively defend their domain against intruders.",
                            "target_lang_translations": {
                                "ru": "Самцы львов чрезвычайно <w>территориальны</w> и агрессивно защищают свои владения от чужаков."
                            }
                        },
                        {
                            "text": "The manager became very <w>territorial</w> about his department when the consultants arrived.",
                            "target_lang_translations": {
                                "ru": "Менеджер стал вести себя очень <w>собственнически</w> по отношению к своему отделу, когда прибыли консультанты."
                            }
                        }
                    ],
                    "synonyms": [
                        "possessive",
                        "defensive",
                        "protective"
                    ],
                    "common_phrases": [
                        "territorial behavior",
                        "territorial instincts",
                        "be territorial about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защищающий своё пространство, права или статус; не желающий делиться контролем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "территориальный",
                                "target_lang_sense_clarification": "о животных: защищающий свою территорию"
                            },
                            {
                                "target_lang_word": "собственнический",
                                "target_lang_sense_clarification": "ревностно относящийся к своему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56510b51-de04-31b8-b6be-5e1c67024378",
                    "sense_definition": "Relating to a volunteer reserve military force organized for home defence.",

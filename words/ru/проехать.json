            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17001e7d-a710-32b9-9b4b-bf5f2434e090",
                    "sense_definition": "Проследовать мимо чего-либо или через что-либо на транспортном средстве.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы быстро <w>проехали</w> через центр города, так как не было пробок."
                        },
                        {
                            "text": "Автобус <w>проехал</w> мимо остановки, не сбавляя скорости."
                        }
                    ],
                    "synonyms": [
                        "объехать"
                    ],
                    "common_phrases": [
                        "проехать мимо",
                        "проехать через мост",
                        "проехать на красный"
                    ]
                },
                {
                    "sense_id": "5e742f3c-78d5-351b-b49a-c7daa99dd38e",
                    "sense_definition": "Преодолеть определённое расстояние, передвигаясь на транспорте.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За сегодня мы <w>проехали</w> пятьсот километров и очень устали."
                        },
                        {
                            "text": "Нам осталось <w>проехать</w> совсем немного до дома."
                        }
                    ],
                    "common_phrases": [
                        "проехать сто километров",
                        "проехать полпути",
                        "проехать много"
                    ]
                },
                {
                    "sense_id": "1450a353-c4d4-3f56-bde2-12f312cec72e",
                    "sense_definition": "Случайно пропустить нужное место остановки или назначения во время поездки.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mistake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я зачитался интересной книгой и <w>проехал</w> свою станцию метро."
                        },
                        {
                            "text": "Водитель такси случайно <w>проехал</w> нужный поворот."
                        }
                    ],
                    "synonyms": [
                        "пропустить"
                    ],
                    "common_phrases": [
                        "проехать остановку",
                        "проехать поворот",
                        "проехать мимо цели"
                    ]
                },
                {
                    "sense_id": "a37161e1-1e2b-3173-8f30-4187882c3b49",
                    "sense_definition": "Перестать обсуждать какую-либо тему; забыть о сказанном или сделанном (обычно используется как призыв прекратить разговор).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Давай не будем снова начинать этот спор, <w>проехали</w>."
                        },
                        {
                            "text": "Ты извинился, так что всё нормально, <w>проехали</w> эту тему."
                        }
                    ],
                    "synonyms": [
                        "забить"
                    ],
                    "common_phrases": [
                        "ладно проехали",
                        "всё проехали",
                        "проехали тему"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ]
                }
            ]
        }

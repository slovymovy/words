                },
                {
                    "sense_id": "997274ca-ab96-3edc-8c84-85659fa88b8b",
                    "sense_definition": "uroczystość rodzinna organizowana z okazji pierwszych urodzin dziecka",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {

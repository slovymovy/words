{
    "word_family": [
        "просить",
        "прошение",
        "проситель",
        "просительный",
        "запрос",
        "выпрашивать"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "61b2a767-cc4a-337e-b708-314a7f73e364",
                    "sense_definition": "Обращение к кому-либо с целью побудить сделать что-либо, исполнить какое-либо желание",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "request_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня к тебе есть одна небольшая <w>просьба</w>.",
                            "target_lang_translations": {
                                "en": "I have a small <w>request</w> for you."
                            }
                        },
                        {
                            "text": "Он всегда готов выполнить любую мою <w>просьбу</w>.",
                            "target_lang_translations": {
                                "en": "He is always ready to fulfill any <w>request</w> of mine."
                            }
                        }
                    ],
                    "synonyms": [
                        "мольба",
                        "упрашивание"
                    ],
                    "antonyms": [
                        "приказ",
                        "требование"
                    ],
                    "common_phrases": [
                        "выполнить просьбу",
                        "обратиться с просьбой",
                        "по просьбе",
                        "откликнуться на просьбу"
                    ],
                    "target_lang_definitions": {
                        "en": "An appeal to someone to do something or fulfill a wish"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "request",
                                "target_lang_sense_clarification": "act of asking for something"
                            },
                            {
                                "target_lang_word": "favor",
                                "target_lang_sense_clarification": "an act of kindness asked of someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dcd57eae-3a5e-30f2-9d4c-3de38e75b57d",
                    "sense_definition": "Вежливая форма требования или предписания, часто используемая в объявлениях (обычно в конструкции с инфинитивом)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "request_impersonal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Убедительная <w>просьба</w> не шуметь в читальном зале.",
                            "target_lang_translations": {
                                "en": "It is an earnest <w>request</w> not to make noise in the reading room."
                            }
                        },
                        {
                            "text": "Пассажирам <w>просьба</w> пройти на посадку.",
                            "target_lang_translations": {
                                "en": "Passengers are <w>requested</w> to proceed to boarding."
                            }
                        }
                    ],
                    "common_phrases": [
                        "убедительная просьба",
                        "большая просьба",
                        "просьба не беспокоить"
                    ],
                    "target_lang_definitions": {
                        "en": "A polite form of demand or instruction, often used in announcements"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "request",
                                "target_lang_sense_clarification": "formal instruction or requirement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ce4f811-4104-341d-92c9-11ca71287950",
                    "sense_definition": "Официальное письменное заявление или ходатайство, подаваемое в государственные органы или организации",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "request_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Комиссия рассмотрела <w>просьбу</w> о предоставлении политического убежища.",
                            "target_lang_translations": {
                                "en": "The commission considered the <w>request</w> for political asylum."
                            }
                        },
                        {
                            "text": "Адвокат подал <w>просьбу</w> о пересмотре судебного решения.",
                            "target_lang_translations": {
                                "en": "The lawyer filed a <w>petition</w> for a review of the court decision."
                            }
                        }
                    ],
                    "synonyms": [
                        "прошение",
                        "ходатайство",
                        "заявление"
                    ],
                    "common_phrases": [
                        "просьба о помиловании",
                        "подать просьбу",
                        "удовлетворить просьбу"
                    ],
                    "target_lang_definitions": {
                        "en": "An official written statement or petition submitted to authorities"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "petition",
                                "target_lang_sense_clarification": "formal written request to authority"
                            },
                            {
                                "target_lang_word": "application",
                                "target_lang_sense_clarification": "formal request for a right or benefit"
                            },
                            {
                                "target_lang_word": "plea",
                                "target_lang_sense_clarification": "urgent or legal appeal"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
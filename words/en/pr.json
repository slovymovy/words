            "senses": [
                {
                    "sense_id": "712b65ad-fb20-3d24-8b2c-434df82c83cb",
                    "sense_definition": "Initialism of public relations; the professional maintenance of a favorable public image by a company, organization, or famous person.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "public_relations",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company hired a new <w>PR</w> firm to improve its reputation.",
                            "target_lang_translations": {
                                "ru": "Компания наняла новую <w>пиар</w>-фирму, чтобы улучшить свою репутацию."
                            }
                        },
                        {
                            "text": "The scandal was a <w>PR</w> disaster for the government.",
                            "target_lang_translations": {
                                "ru": "Этот скандал стал <w>пиар</w>-катастрофой для правительства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bad PR",
                        "good PR",
                        "PR stunt",
                        "PR firm",
                        "PR campaign"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связи с общественностью; деятельность по формированию благоприятного имиджа компании, организации или известного лица."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пиар",
                                "target_lang_sense_clarification": "разговорное, транслитерация PR"
                            },
                            {
                                "target_lang_word": "связи с общественностью",
                                "target_lang_sense_clarification": "официальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9028f54-dcdb-3668-903f-0fc847979170",
                    "sense_definition": "Initialism of personal record; the best performance one has ever achieved in a specific activity, especially sport.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She set a new <w>PR</w> in the marathon last weekend.",
                            "target_lang_translations": {
                                "ru": "Она установила новый <w>личный рекорд</w> в марафоне на прошлых выходных."
                            }
                        },
                        {
                            "text": "I am training hard to beat my <w>PR</w> in the deadlift.",
                            "target_lang_translations": {
                                "ru": "Я усердно тренируюсь, чтобы побить свой <w>личный рекорд</w> в становой тяге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "set a PR",
                        "beat a PR",
                        "new PR",
                        "hit a PR"
                    ],
                    "target_lang_definitions": {
                        "ru": "Личный рекорд; лучший результат, когда-либо достигнутый человеком в определенной деятельности, особенно в спорте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "личный рекорд",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "639364bc-e4a7-3a46-8328-40ad93335a88",
                    "sense_definition": "Initialism of permanent residence; the legal status of a person allowed to reside indefinitely within a country of which they are not a citizen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "immigration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He finally received his <w>PR</w> card after years of waiting.",
                            "target_lang_translations": {

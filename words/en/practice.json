                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клиника",
                                "target_lang_sense_clarification": "медучреждение"
                            },
                            {
                                "target_lang_word": "кабинет",
                                "target_lang_sense_clarification": "помещение врача"
                            },
                            {
                                "target_lang_word": "практика",
                                "target_lang_sense_clarification": "бизнес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "495cb0f9-d262-3383-8c4e-70da5422899a",
                    "sense_definition": "The observance of religious duties that a church requires of its members.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "habit_custom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regular prayer is an essential part of their religious <w>practice</w>.",

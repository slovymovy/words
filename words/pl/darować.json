    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a5d92856-aaa6-3a2b-894d-b08dc74f5c89",
                    "sense_definition": "Przebaczyć komuś wyrządzoną krzywdę, winę lub obrazę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "forgive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie potrafię mu <darować> tego, że okłamał mnie w tak ważnej sprawie.",
                            "target_lang_translations": {
                                "ru": "Я не могу <простить> ему того, что он солгал мне в таком важном деле."
                            }
                        },
                        {
                            "text": "Proszę, <daruj> mi moje wczorajsze zachowanie, byłem bardzo zmęczony.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <прости> мне мое вчерашнее поведение, я был очень уставшим."
                            }
                        }
                    ],
                    "synonyms": [
                        "przebaczyć",
                        "wybaczyć"
                    ],
                    "antonyms": [
                        "ukarać"
                    ],
                    "common_phrases": [
                        "darować winy",
                        "darować urazy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простить кому-то причиненное зло, вину или обиду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простить",
                                "target_lang_sense_clarification": "извинить за проступок"
                            },
                            {
                                "target_lang_word": "извинить",
                                "target_lang_sense_clarification": "не держать зла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53b5aff4-0c96-31ca-b96d-b392a119d2c1",
                    "sense_definition": "Zrezygnować z robienia czegoś lub mówienia o czymś; dać sobie z czymś spokój.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "refrain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powinieneś <darować> sobie te złośliwe uwagi, bo nikogo one nie bawią.",
                            "target_lang_translations": {
                                "ru": "Тебе стоит <оставить> эти ехидные замечания, потому что они никого не веселят."
                            }
                        },
                        {
                            "text": "Dzisiaj <darujemy> sobie sprzątanie i idziemy na długi spacer.",
                            "target_lang_translations": {
                                "ru": "Сегодня мы <пропустим> уборку и пойдем на долгую прогулку."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpuścić"
                    ],
                    "common_phrases": [
                        "darować sobie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used frequently in the phrase 'darować sobie'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказаться от выполнения чего-либо или разговора о чем-либо; перестать заниматься чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "перестать делать или говорить (в сочетании с 'sobie')"
                            },
                            {
                                "target_lang_word": "пропустить",
                                "target_lang_sense_clarification": "не делать чего-то запланированного"
                            },
                            {
                                "target_lang_word": "воздержаться",
                                "target_lang_sense_clarification": "отказаться от действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c6a49c0-4969-3f77-8a6b-58aa787474ec",
                    "sense_definition": "Przekazać coś komuś na własność bezpłatnie; dać prezent.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "give",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice postanowili <darować> dzieciom stary dom na wsi.",
                            "target_lang_translations": {
                                "ru": "Родители решили <подарить> детям старый дом в деревне."
                            }
                        },
                        {
                            "text": "Chciałbym ci coś <darować> na pamiątkę naszego spotkania.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы тебе кое-что <подарить> на память о нашей встрече."
                            }
                        }
                    ],
                    "synonyms": [
                        "podarować"
                    ],
                    "antonyms": [
                        "odebrać",
                        "zabrać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать что-то кому-то в собственность бесплатно; сделать подарок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подарить",
                                "target_lang_sense_clarification": "дать в дар"
                            },
                            {
                                "target_lang_word": "даровать",
                                "target_lang_sense_clarification": "торжественно вручить (устар./книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "863b2b1d-2110-33b1-864a-0a3aeb434bf0",
                    "sense_definition": "Zrezygnować z wyegzekwowania czegoś, co się komuś należy (np. długu, kary), lub odstąpić od odebrania komuś życia.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "remit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Król w akcie łaski postanowił <darować> życie wszystkim jeńcom.",
                            "target_lang_translations": {
                                "ru": "Король в знак милости решил <даровать> жизнь всем пленникам."
                            }
                        },
                        {
                            "text": "Bank nie chciał <darować> mu odsetek od zaciągniętego kredytu.",
                            "target_lang_translations": {
                                "ru": "Банк не хотел <прощать> ему проценты по взятому кредиту."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpuścić"
                    ],
                    "antonyms": [
                        "wyegzekwować"
                    ],
                    "common_phrases": [
                        "darować życie",
                        "darować dług",
                        "darować karę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказаться от взыскания чего-либо причитающегося (долга, наказания) или сохранить кому-то жизнь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простить",
                                "target_lang_sense_clarification": "долг или вину"
                            },
                            {
                                "target_lang_word": "даровать",
                                "target_lang_sense_clarification": "жизнь или свободу"
                            },
                            {
                                "target_lang_word": "пощадить",
                                "target_lang_sense_clarification": "сохранить жизнь"
                            }
                        ]
                    }
                }
            ]
        }
    ]

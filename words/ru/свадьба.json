    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8e6d16ab-5d3d-3c8f-952f-e73a5a36da53",
                    "sense_definition": "Обряд бракосочетания и праздничное торжество по случаю вступления в брак.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы пригласили всех близких друзей на нашу <w>свадьбу</w>.",
                            "target_lang_translations": {
                                "en": "We invited all our close friends to our <w>wedding</w>."
                            }
                        },
                        {
                            "text": "Их <w>свадьба</w> была очень красивой и весёлой.",
                            "target_lang_translations": {
                                "en": "Their <w>wedding</w> was very beautiful and fun."
                            }
                        }
                    ],
                    "synonyms": [
                        "бракосочетание"
                    ],
                    "antonyms": [
                        "развод"
                    ],
                    "common_phrases": [
                        "играть свадьбу",
                        "день свадьбы",
                        "пышная свадьба",
                        "до свадьбы заживёт"
                    ],
                    "target_lang_definitions": {
                        "en": "The ceremony of getting married and the celebration that follows."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wedding",
                                "target_lang_sense_clarification": "ceremony and celebration"
                            },
                            {
                                "target_lang_word": "marriage",
                                "target_lang_sense_clarification": "ceremony (formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ebace93-4054-3f33-9600-ae88d99083d3",
                    "sense_definition": "Годовщина вступления в брак (обычно употребляется с прилагательным, обозначающим срок совместной жизни).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "anniversary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Завтра мои родители отмечают серебряную <w>свадьбу</w>.",
                            "target_lang_translations": {
                                "en": "Tomorrow my parents are celebrating their silver <w>wedding</w>."
                            }
                        },
                        {
                            "text": "На золотую <w>свадьбу</w> дедушке и бабушке подарили путешествие.",
                            "target_lang_translations": {
                                "en": "For their golden <w>wedding</w>, grandfather and grandmother were given a trip."
                            }
                        }
                    ],
                    "synonyms": [
                        "годовщина свадьбы",
                        "годовщина"
                    ],
                    "common_phrases": [
                        "серебряная свадьба",
                        "золотая свадьба",
                        "бриллиантовая свадьба"
                    ],
                    "target_lang_definitions": {
                        "en": "An anniversary of a marriage, usually used with an adjective indicating the number of years."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wedding",
                                "target_lang_sense_clarification": "used in phrases like 'silver wedding'"
                            },
                            {
                                "target_lang_word": "anniversary",
                                "target_lang_sense_clarification": "celebration of a date"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9ae0e1e-0cbb-372a-9a84-fa8f9b07d2cf",
                    "sense_definition": "Участники брачного торжества, гости и молодожёны как единая группа людей.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вся <w>свадьба</w> пела и танцевала до самого утра.",
                            "target_lang_translations": {
                                "en": "The whole <w>wedding</w> sang and danced until the very morning."
                            }
                        },
                        {
                            "text": "Когда мы вошли в зал, шумная <w>свадьба</w> уже сидела за столами.",
                            "target_lang_translations": {
                                "en": "When we entered the hall, the noisy <w>wedding</w> was already sitting at the tables."
                            }
                        }
                    ],
                    "common_phrases": [
                        "шумная свадьба",
                        "свадьба гуляет"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to refer to the crowd of people attending the event"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The people attending a wedding, including guests and the couple, viewed as a group."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wedding",
                                "target_lang_sense_clarification": "group of people (metonymy)"
                            },
                            {
                                "target_lang_word": "wedding party",
                                "target_lang_sense_clarification": "participants and guests"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "a02fbb18-6ce8-31ac-9499-2658cb7d3cda",
                    "sense_definition": "A cigarette.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "consumption_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He stopped at the shop to buy a pack of <w>smokes</w>.",
                            "target_lang_translations": {
                                "ru": "Он остановился у магазина, чтобы купить пачку <w>сигарет</w>."

            "senses": [
                {
                    "sense_id": "2ff4e76f-a95e-3d7b-8aa4-99a843a0b9f1",
                    "sense_definition": "Een geneesmiddel dat wordt gebruikt om infecties door schimmels te bestrijden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anti_fungal_agents",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts heeft een <w>antischimmelmiddel</w> voorgeschreven voor mijn hardnekkige voetschimmel.",
                            "target_lang_translations": {
                                "ru": "Врач прописал <w>противогрибковое средство</w> от моего стойкого грибка стопы."
                            }
                        },
                        {
                            "text": "Het is belangrijk om het <w>antischimmelmiddel</w> precies volgens de instructies van de dokter te gebruiken.",
                            "target_lang_translations": {
                                "ru": "Важно использовать <w>противогрибковый препарат</w> в точном соответствии с инструкциями врача."
                            }
                        }
                    ],
                    "synonyms": [
                        "antimycoticum"
                    ],
                    "common_phrases": [
                        "een antischimmelmiddel voorschrijven",
                        "lokaal antischimmelmiddel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лекарственное средство, используемое для борьбы с грибковыми инфекциями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противогрибковое средство",
                                "target_lang_sense_clarification": "лекарство"
                            },
                            {
                                "target_lang_word": "антимикотик",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77e318d2-bf8a-3370-beb8-d3643f6cee32",
                    "sense_definition": "Een stof die de groei van schimmels remt of stopt, vaak gebruikt op materialen of planten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anti_fungal_agents",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben een krachtig <w>antischimmelmiddel</w> gekocht om de muren in de badkamer schoon te maken.",
                            "target_lang_translations": {
                                "ru": "Мы купили мощное <w>средство от плесени</w>, чтобы очистить стены в ванной."
                            }
                        },
                        {
                            "text": "Dit <w>antischimmelmiddel</w> beschermt het hout in de tuin tegen vocht en bederf.",
                            "target_lang_translations": {
                                "ru": "Это <w>противогрибковое средство</w> защищает садовую древесину от влаги и гниения."
                            }
                        }
                    ],
                    "synonyms": [
                        "schimmeldoder",
                        "fungicide"
                    ],
                    "common_phrases": [
                        "krachtig antischimmelmiddel",
                        "biologisch antischimmelmiddel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество, которое замедляет или останавливает рост плесени, часто используется для обработки материалов или растений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средство от плесени",
                                "target_lang_sense_clarification": "бытовая химия"
                            },
                            {
                                "target_lang_word": "фунгицид",
                                "target_lang_sense_clarification": "для растений или материалов"
                            }
                        ]
                    }
                }
            ]
        }

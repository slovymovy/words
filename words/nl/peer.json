                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>peer</w> 'm, want ik moet de laatste trein halen.",
                            "target_lang_translations": {
                                "en": "I'm <w>taking off</w>, because I have to catch the last train.",
                                "ru": "Я <w>сваливаю</w>, так как мне нужно успеть на последний поезд."
                            }

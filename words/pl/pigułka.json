    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a6d89fd0-695c-3d23-a240-8b3370a1ec53",
                    "sense_definition": "Mała porcja lekarstwa w zwartej, stałej postaci, zwykle o okrągłym lub owalnym kształcie, przeznaczona do połknięcia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "medicine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz przepisał mi nowe <w>pigułki</w> na ból głowy, które muszę brać rano.",
                            "target_lang_translations": {
                                "ru": "Врач прописал мне новые <w>таблетки</w> от головной боли, которые я должен принимать по утрам."
                            }
                        },
                        {
                            "text": "Ten lek jest sprzedawany też w płynie, ale pani, zdaje się, woli łykać <w>pigułki</w>?",
                            "target_lang_translations": {
                                "ru": "Это лекарство продается также в жидком виде, но вы, кажется, предпочитаете глотать <w>таблетки</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "tabletka",
                        "pastylka",
                        "drażetka"
                    ],
                    "common_phrases": [
                        "łykać pigułki",
                        "gorzka pigułka",
                        "w pigułce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая порция лекарства в твердой форме, обычно круглой или овальной, предназначенная для проглатывания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пилюля",
                                "target_lang_sense_clarification": "лекарственная форма в виде шарика"
                            },
                            {
                                "target_lang_word": "таблетка",
                                "target_lang_sense_clarification": "твердая дозированная лекарственная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09630f92-6363-3b83-8468-72fe2ab8fafe",
                    "sense_definition": "Potoczne określenie na doustny środek antykoncepcyjny stosowany przez kobiety.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "medicine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele młodych kobiet decyduje się na stosowanie <w>pigułki</w> jako metody antykoncepcji.",
                            "target_lang_translations": {
                                "ru": "Многие молодые женщины выбирают <w>противозачаточные таблетки</w> в качестве метода контрацепции."
                            }
                        },
                        {
                            "text": "Należy pamiętać, aby przyjmować <w>pigułkę</w> codziennie o tej samej porze.",
                            "target_lang_translations": {
                                "ru": "Нужно помнить о том, чтобы принимать <w>таблетку</w> каждый день в одно и то же время."
                            }
                        }
                    ],
                    "synonyms": [
                        "tabletka"
                    ],
                    "common_phrases": [
                        "brać pigułkę",
                        "odstawić pigułkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common term for contraceptive pills"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название орального контрацептива, используемого женщинами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противозачаточная таблетка",
                                "target_lang_sense_clarification": "средство контрацепции"
                            },
                            {
                                "target_lang_word": "таблетка",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec07e01c-8278-3baf-9d2b-1c7152e0960a",
                    "sense_definition": "Mała kulka ulepiona z miękkiej masy lub substancji.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więzień ulepił małą <w>pigułkę</w> z chleba, aby ukryć w niej wiadomość.",
                            "target_lang_translations": {
                                "ru": "Заключенный слепил маленький <w>шарик</w> из хлеба, чтобы спрятать в нем записку."
                            }
                        },
                        {
                            "text": "Dziecko bawiło się przy stole, tocząc w palcach małe <w>pigułki</w> z wosku.",
                            "target_lang_translations": {
                                "ru": "Ребенок играл за столом, скатывая пальцами маленькие <w>шарики</w> из воска."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pigułka z chleba"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький шарик, слепленный из мягкой массы или вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шарик",
                                "target_lang_sense_clarification": "небольшое круглое образование"
                            },
                            {
                                "target_lang_word": "катышек",
                                "target_lang_sense_clarification": "маленький комочек"
                            }
                        ]
                    }
                }

{
    "word_family": [
        "потеря",
        "утеря",
        "растерянность",
        "потерянный",
        "растерянный",
        "затеряться",
        "растеряться",
        "потеряться",
        "утерять"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7f600628-a9bb-394d-bd16-a9310388bcd2",
                    "sense_definition": "Пропадать, исчезать, оказываться в неизвестном месте (о предметах).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня постоянно <теряются> ручки и карандаши.",
                            "target_lang_translations": {
                                "en": "My pens and pencils constantly <get lost>."
                            }
                        },
                        {
                            "text": "Важные документы не должны <теряться> при переезде в новый офис.",
                            "target_lang_translations": {
                                "en": "Important documents must not <get lost> when moving to a new office."
                            }
                        }
                    ],
                    "synonyms": [
                        "пропадать",
                        "исчезать"
                    ],
                    "antonyms": [
                        "находиться"
                    ],
                    "common_phrases": [
                        "вечно теряться",
                        "где-то теряться",
                        "постоянно теряться"
                    ],
                    "target_lang_definitions": {
                        "en": "To go missing, disappear, or end up in an unknown place (referring to objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get lost",
                                "target_lang_sense_clarification": "become unable to be found"
                            },
                            {
                                "target_lang_word": "go missing",
                                "target_lang_sense_clarification": "disappear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af986311-85f3-387d-a28b-44f36744e527",
                    "sense_definition": "Переставать понимать, где находишься; сбиваться с пути.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы часто <теряются> на узких улочках старого города.",
                            "target_lang_translations": {
                                "en": "Tourists often <get lost> in the narrow streets of the old city."
                            }
                        },
                        {
                            "text": "Маленький ребёнок боялся <теряться> в огромном магазине.",
                            "target_lang_translations": {
                                "en": "The small child was afraid of <getting lost> in the huge store."
                            }
                        }
                    ],
                    "synonyms": [
                        "плутать"
                    ],
                    "antonyms": [
                        "находиться"
                    ],
                    "common_phrases": [
                        "легко теряться",
                        "теряться в лесу",
                        "теряться в городе",
                        "теряться в толпе"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop understanding where one is; to lose one's way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get lost",
                                "target_lang_sense_clarification": "lose one's way spatially"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c248f617-8752-3e36-8e38-f5d5faacdb58",
                    "sense_definition": "Чувствовать неуверенность, волнение или страх, не зная, как поступить или что сказать.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент начал <теряться>, когда профессор задал ему сложный вопрос.",
                            "target_lang_translations": {
                                "en": "The student began to <get flustered> when the professor asked him a difficult question."
                            }
                        },
                        {
                            "text": "Она привыкла выступать на сцене и никогда не <теряется> перед публикой.",
                            "target_lang_translations": {
                                "en": "She is used to performing on stage and never <loses her nerve> in front of an audience."
                            }
                        }
                    ],
                    "synonyms": [
                        "смущаться",
                        "робеть",
                        "тушеваться"
                    ],
                    "common_phrases": [
                        "теряться в догадках",
                        "совсем теряться",
                        "немного теряться",
                        "теряться при виде"
                    ],
                    "target_lang_definitions": {
                        "en": "To feel uncertain, agitated, or afraid, not knowing how to act or what to say."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get flustered",
                                "target_lang_sense_clarification": "become agitated and confused"
                            },
                            {
                                "target_lang_word": "get confused",
                                "target_lang_sense_clarification": "lose mental clarity"
                            },
                            {
                                "target_lang_word": "be at a loss",
                                "target_lang_sense_clarification": "not know what to do"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d9e1a9f-9b5c-336d-a48a-a3663a9db219",
                    "sense_definition": "Страдательная форма от глагола <w>терять</w> (утрачиваться, лишаться чего-либо).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При плохом переводе часто <теряется> истинный смысл шутки.",
                            "target_lang_translations": {
                                "en": "With a bad translation, the true meaning of the joke is often <lost>."
                            }
                        },
                        {
                            "text": "В этой суматохе зря <теряется> драгоценное время.",
                            "target_lang_translations": {
                                "en": "Precious time is <lost> in vain in this commotion."
                            }
                        }
                    ],
                    "synonyms": [
                        "утрачиваться"
                    ],
                    "common_phrases": [
                        "теряться из виду",
                        "смысл теряется",
                        "связь теряется"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Passive form of the verb 'to lose' (to be lost, to be deprived of something)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be lost",
                                "target_lang_sense_clarification": "passive state of loss"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36baaaad-3058-3df5-8ec9-abcd4204abaf",
                    "sense_definition": "Становиться плохо различимым, исчезать из виду или сливаться с окружением.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его тихий голос начал <теряться> в шуме сильного ветра.",
                            "target_lang_translations": {
                                "en": "His quiet voice began to <be lost> in the noise of the strong wind."
                            }
                        },
                        {
                            "text": "Маленький деревянный домик почти <терялся> среди высоких сосен.",
                            "target_lang_translations": {
                                "en": "The small wooden house was almost <lost> among the tall pines."
                            }
                        }
                    ],
                    "synonyms": [
                        "растворяться",
                        "меркнуть"
                    ],
                    "common_phrases": [
                        "теряться в тумане",
                        "теряться на фоне",
                        "теряться вдали",
                        "теряться в темноте"
                    ],
                    "target_lang_definitions": {
                        "en": "To become barely distinguishable, disappear from sight, or blend into the surroundings."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fade",
                                "target_lang_sense_clarification": "become faint or indistinct"
                            },
                            {
                                "target_lang_word": "be lost",
                                "target_lang_sense_clarification": "blend into background"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
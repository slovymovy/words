    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4bcd85be-ea9e-3f0a-b92e-05b1db0ea516",
                    "sense_definition": "In a state of deep unhappiness, discomfort, or suffering.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "unhappiness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She started to feel <w>miserable</w> after hearing the bad news about her job application.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала себя <w>несчастной</w> после того, как услышала плохие новости по поводу её заявления на работу."
                            }
                        },
                        {
                            "text": "The flu made him feel weak and <w>miserable</w> for the entire week.",
                            "target_lang_translations": {
                                "ru": "Из-за гриппа он чувствовал себя слабым и <w>разбитым</w> всю неделю."
                            }
                        }
                    ],
                    "synonyms": [
                        "unhappy",
                        "sad",
                        "depressed",
                        "dejected",
                        "wretched",
                        "gloomy",
                        "downcast",
                        "sorrowful",
                        "despondent"
                    ],
                    "antonyms": [
                        "happy",
                        "cheerful",
                        "joyful",
                        "content"
                    ],
                    "common_phrases": [
                        "feel miserable",
                        "look miserable",
                        "make someone miserable",
                        "miserable life"
                    ],
                    "target_lang_definitions": {
                        "ru": "В состоянии глубокого несчастья, дискомфорта или страдания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несчастный",
                                "target_lang_sense_clarification": "испытывающий горе или страдание"
                            },
                            {
                                "target_lang_word": "жалкий",
                                "target_lang_sense_clarification": "вызывающий жалость своим видом или состоянием"
                            },
                            {
                                "target_lang_word": "бедный",
                                "target_lang_sense_clarification": "в значении 'несчастный', вызывающий сочувствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "130f47aa-061a-31c7-a8d0-3a7303b900c8",
                    "sense_definition": "Of the weather, extremely unpleasant due to being cold, wet, or overcast.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to cancel the picnic because of the cold and <w>miserable</w> weather.",
                            "target_lang_translations": {
                                "ru": "Мы решили отменить пикник из-за холодной и <w>скверной</w> погоды."
                            }
                        },
                        {
                            "text": "It was a <w>miserable</w> day with heavy rain and strong winds blowing constantly.",
                            "target_lang_translations": {
                                "ru": "Это был <w>мерзкий</w> день с проливным дождём и постоянно дующим сильным ветром."
                            }
                        }
                    ],
                    "synonyms": [
                        "gloomy",
                        "dreary",
                        "bleak",
                        "dismal",
                        "gray",
                        "wet",
                        "inclement"
                    ],
                    "antonyms": [
                        "sunny",
                        "pleasant",
                        "bright",
                        "fine"
                    ],
                    "common_phrases": [
                        "miserable weather",
                        "miserable day",
                        "miserable rain",
                        "miserable morning"
                    ],
                    "target_lang_definitions": {
                        "ru": "О погоде: крайне неприятная из-за холода, сырости или облачности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скверный",
                                "target_lang_sense_clarification": "плохой, неприятный (о погоде)"
                            },
                            {
                                "target_lang_word": "ненастный",
                                "target_lang_sense_clarification": "дождливый, пасмурный"
                            },
                            {
                                "target_lang_word": "мерзкий",
                                "target_lang_sense_clarification": "разг. отвратительный, очень неприятный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fd0d5eb-939f-3f1d-80e4-263e648ff757",
                    "sense_definition": "Very bad, unskilled, or of extremely poor quality.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His first attempt at baking a birthday cake was a <w>miserable</w> failure.",
                            "target_lang_translations": {
                                "ru": "Его первая попытка испечь торт на день рождения обернулась <w>жалким</w> провалом."
                            }
                        },
                        {
                            "text": "I am absolutely <w>miserable</w> at remembering names when I meet new people.",
                            "target_lang_translations": {
                                "ru": "Я совершенно <w>никудышный</w> в запоминании имён, когда знакомлюсь с новыми людьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "terrible",
                        "incompetent",
                        "inept",
                        "poor",
                        "pathetic",
                        "hopeless",
                        "worthless"
                    ],
                    "antonyms": [
                        "excellent",
                        "superb",
                        "skilled",
                        "great"
                    ],
                    "common_phrases": [
                        "miserable failure",
                        "miserable attempt",
                        "miserable performance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень плохой, неумелый или крайне низкого качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалкий",
                                "target_lang_sense_clarification": "ничтожный, неудачный (о попытке или результате)"
                            },
                            {
                                "target_lang_word": "никудышный",
                                "target_lang_sense_clarification": "неумелый, негодный"

                {
                    "sense_id": "de3ded72-b879-3c6a-a141-b9d6fd16d51f",
                    "sense_definition": "Een rij struiken die als afscheiding dient.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tuinman snoeit de <w>haag</w> elk voorjaar.",
                            "target_lang_translations": {
                                "ru": "Садовник подстригает <w>живую изгородь</w> каждую весну."
                            }
                        },
                        {
                            "text": "We hebben een hoge <w>haag</w> geplant voor meer privacy in de tuin.",
                            "target_lang_translations": {
                                "ru": "Мы посадили высокую <w>живую изгородь</w> для большей уединенности в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "heg"
                    ],
                    "common_phrases": [
                        "de haag snoeien",
                        "een levende haag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ряд кустарников, служащий ограждением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живая изгородь",
                                "target_lang_sense_clarification": "ряд кустарников"
                            },
                            {
                                "target_lang_word": "изгородь",
                                "target_lang_sense_clarification": "ограда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67943cd8-9b94-326b-b405-e66a2fbd1e77",
                    "sense_definition": "Een rij mensen die aan weerszijden van een pad staan, vaak als eerbetoon.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leerlingen vormden een <w>haag</w> voor de bruid en bruidegom.",
                            "target_lang_translations": {
                                "ru": "Ученики выстроились в <w>живой коридор</w> для жениха и невесты."
                            }
                        },
                        {
                            "text": "De spelers liepen door een <w>haag</w> van juichende supporters het veld op.",
                            "target_lang_translations": {
                                "ru": "Игроки вышли на поле сквозь <w>живой коридор</w> из ликующих болельщиков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een haag vormen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ряд людей, стоящих по обе стороны пути, часто в знак почтения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живой коридор",
                                "target_lang_sense_clarification": "строй людей"
                            },
                            {
                                "target_lang_word": "шпалера",
                                "target_lang_sense_clarification": "военный или торжественный строй"
                            }
                        ]

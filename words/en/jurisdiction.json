{
    "word_family": [
        "jurisdictional"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc6c265e-d89d-396d-a590-a9b873c58ba1",
                    "sense_definition": "The official power or right to make legal decisions and judgments.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "legal_authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local court does not have <w>jurisdiction</w> over serious federal crimes.",
                            "target_lang_translations": {
                                "ru": "Местный суд не имеет <w>юрисдикции</w> в отношении тяжких федеральных преступлений."
                            }
                        },
                        {
                            "text": "The police officer explained that the incident was outside his <w>jurisdiction</w>.",
                            "target_lang_translations": {
                                "ru": "Полицейский объяснил, что происшествие находится вне его <w>юрисдикции</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "power",
                        "power"
                    ],
                    "common_phrases": [
                        "have jurisdiction",
                        "lack jurisdiction",
                        "claim jurisdiction",
                        "under jurisdiction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальные полномочия или право принимать правовые решения и выносить суждения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юрисдикция",
                                "target_lang_sense_clarification": "правомочие производить суд"
                            },
                            {
                                "target_lang_word": "подсудность",
                                "target_lang_sense_clarification": "компетенция суда рассматривать дела"
                            },
                            {
                                "target_lang_word": "полномочия",
                                "target_lang_sense_clarification": "право власти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14f44eaf-8208-30f9-8f54-842f6538fbe1",
                    "sense_definition": "An area, country, or territory in which a specific system of laws and legal authority applies.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_territory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company moved its headquarters to a <w>jurisdiction</w> with lower corporate taxes.",
                            "target_lang_translations": {
                                "ru": "Компания перенесла свою штаб-квартиру в <w>юрисдикцию</w> с более низкими корпоративными налогами."
                            }
                        },
                        {
                            "text": "Criminal laws can vary significantly from one <w>jurisdiction</w> to another.",
                            "target_lang_translations": {
                                "ru": "Уголовные законы могут существенно различаться от одной <w>юрисдикции</w> к другой."
                            }
                        }
                    ],
                    "synonyms": [
                        "power",
                        "power"
                    ],
                    "common_phrases": [
                        "tax jurisdiction",
                        "foreign jurisdiction",
                        "within the jurisdiction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория, страна или область, в которой применяется определенная система законов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юрисдикция",
                                "target_lang_sense_clarification": "территория действия законов"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
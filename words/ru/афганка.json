            "senses": [
                {
                    "sense_id": "d488142b-4c8d-3938-aaaa-aad70214db76",
                    "sense_definition": "Жительница или уроженка Афганистана.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "Начар не туркменка, она — <w>афганка</w>, которую украли у родителей в детстве."
                        },
                        {
                            "text": "Молодая красивая <w>афганка</w> уверенно управляла городским троллейбусом."
                        }
                    ],
                    "common_phrases": [
                        "молодая афганка",
                        "красивая афганка",
                        "старая афганка"
                    ]
                },
                {
                    "sense_id": "28cb00d4-46fd-3e87-a817-844c77507b0f",
                    "sense_definition": "Собака породы афганская борзая (самка).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На выставке собак грациозная <w>афганка</w> получила золотую медаль за экстерьер."
                        },
                        {
                            "text": "У наших соседей живёт длинношёрстная <w>афганка</w>, которую нужно часто расчёсывать."
                        }
                    ],
                    "synonyms": [
                        "афга́нская борза́я"
                    ],
                    "common_phrases": [
                        "породистая афганка",
                        "грациозная афганка"
                    ],
                    "traits": [

                },
                {
                    "sense_id": "365511c0-3b9c-3744-aea9-020910806e28",
                    "sense_definition": "Zonder toevoegingen, versiering of emotie; puur en feitelijk.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "stijl",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je de feiten <w>sec</w> bekijkt, is er geen reden tot paniek.",
                            "target_lang_translations": {

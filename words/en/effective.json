    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "92f6bc92-7f40-390a-af22-7d74a6ae81b9",
                    "sense_definition": "Producing the intended or desired result; successful in achieving a purpose.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "producing_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new medicine proved to be highly <w>effective</w> in treating the infection.",
                            "target_lang_translations": {
                                "ru": "Новое лекарство оказалось очень <w>эффективным</w> при лечении инфекции."
                            }
                        },
                        {
                            "text": "We need to find an <w>effective</w> solution to this problem before it gets worse.",
                            "target_lang_translations": {
                                "ru": "Нам нужно найти <w>действенное</w> решение этой проблемы, пока она не усугубилась."
                            }
                        }
                    ],
                    "synonyms": [
                        "efficacious"
                    ],
                    "common_phrases": [
                        "highly effective",
                        "cost-effective",
                        "effective method",
                        "effective way"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводящий к желаемому результату; успешный в достижении цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эффективный",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "действенный",
                                "target_lang_sense_clarification": "о мерах, лекарствах или способах"
                            },
                            {
                                "target_lang_word": "результативный",
                                "target_lang_sense_clarification": "дающий высокие результаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "744aa830-5303-3ac1-949a-b70f740055d7",
                    "sense_definition": "Officially operative, active, or in force (used for laws, rules, or agreements).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "operative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new parking regulations become <w>effective</w> starting next Monday.",
                            "target_lang_translations": {
                                "ru": "Новые правила парковки <w>вступают в силу</w> со следующего понедельника."
                            }
                        },
                        {
                            "text": "Her resignation is <w>effective</w> immediately, so she will not return to the office.",
                            "target_lang_translations": {
                                "ru": "Ее отставка <w>вступает в силу</w> немедленно, поэтому она не вернется в офис."
                            }
                        }
                    ],
                    "common_phrases": [
                        "effective immediately",
                        "effective date",
                        "effective from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально действующий или имеющий юридическую силу (о законах, правилах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "действующий",
                                "target_lang_sense_clarification": "находящийся в действии"
                            },
                            {
                                "target_lang_word": "вступающий в силу",
                                "target_lang_sense_clarification": "начинающий действовать"
                            },
                            {
                                "target_lang_word": "имеющий силу",
                                "target_lang_sense_clarification": "юридически значимый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffac2383-28d0-3b7a-bc89-73be0ff0c496",
                    "sense_definition": "Existing in fact or reality, although not officially stated as such; actual.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "actual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Although he is not the manager, he has <w>effective</w> control over the daily operations.",
                            "target_lang_translations": {
                                "ru": "Хотя он и не менеджер, он осуществляет <w>фактический</w> контроль над повседневными операциями."
                            }
                        },
                        {
                            "text": "Her <w>effective</w> income is much lower after paying all the mandatory fees.",
                            "target_lang_translations": {
                                "ru": "Ее <w>реальный</w> доход намного ниже после уплаты всех обязательных сборов."
                            }
                        }
                    ],
                    "antonyms": [
                        "nominal"
                    ],
                    "common_phrases": [
                        "effective control",
                        "effective tax rate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий в действительности, а не формально; фактический."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фактический",
                                "target_lang_sense_clarification": "реально существующий"
                            },
                            {
                                "target_lang_word": "реальный",
                                "target_lang_sense_clarification": "настоящий, не номинальный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f760f64-8fa0-3390-99f5-460f2d0dc0b3",
                    "sense_definition": "A soldier who is fit for duty and available for service.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general reported that he had five thousand <w>effectives</w> ready for the upcoming battle.",
                            "target_lang_translations": {
                                "ru": "Генерал доложил, что у него пять тысяч <w>боеспособных солдат</w>, готовых к предстоящей битве."
                            }
                        },
                        {
                            "text": "Due to the outbreak of illness, the number of <w>effectives</w> in the unit dropped significantly.",
                            "target_lang_translations": {
                                "ru": "Из-за вспышки болезни число <w>бойцов в строю</w> в подразделении значительно сократилось."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Военнослужащий, годный к службе и готовый к выполнению задач."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "боеспособный солдат",
                                "target_lang_sense_clarification": "военный термин"
                            },
                            {
                                "target_lang_word": "боец",
                                "target_lang_sense_clarification": "солдат в строю"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "907e2063-4830-3c33-86a9-a7ba3f45b75d",
                    "sense_definition": "Op enig moment in de tijd; wordt vaak gebruikt in vragen of voorwaardelijke zinnen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb jij <w>ooit</w> zo'n vreemd dier gezien?",
                            "target_lang_translations": {
                                "en": "Have you <w>ever</w> seen such a strange animal?",
                                "ru": "Ты <w>когда-нибудь</w> видел такое странное животное?"
                            }
                        },
                        {
                            "text": "Als je <w>ooit</w> hulp nodig hebt, mag je me bellen.",
                            "target_lang_translations": {
                                "en": "If you <w>ever</w> need help, you can call me.",
                                "ru": "Если тебе <w>когда-нибудь</w> понадобится помощь, можешь мне позвонить."
                            }
                        }
                    ],
                    "synonyms": [
                        "eens"
                    ],
                    "antonyms": [
                        "nooit",
                        "nimmer"
                    ],
                    "common_phrases": [
                        "heb je ooit",
                        "als ik ooit",
                        "iemand ooit"
                    ],
                    "target_lang_definitions": {
                        "en": "At any moment in time; often used in questions or conditional sentences.",
                        "ru": "В любой момент времени; часто используется в вопросах или условных предложениях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ever",

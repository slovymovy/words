            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "280b51ef-df9c-3403-aafd-242fdbaa35d3",
                    "sense_definition": "Zich neerslachtig, somber en zonder energie voelend.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die man is erg <w>depressief</w> na het onverwachte overlijden van zijn vrouw.",
                            "target_lang_translations": {
                                "en": "That man is very <w>depressed</w> after the unexpected death of his wife.",
                                "ru": "Этот мужчина очень <w>подавлен</w> после неожиданной смерти своей жены."
                            }
                        },
                        {
                            "text": "Door het donkere en natte weer voel ik me vandaag een beetje <w>depressief</w>.",
                            "target_lang_translations": {
                                "en": "Because of the dark and wet weather, I feel a bit <w>depressed</w> today.",
                                "ru": "Из-за темной и сырой погоды я чувствую себя сегодня немного <w>подавленным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "neerslachtig",
                        "somber",
                        "terneergeslagen",
                        "lusteloos"
                    ],
                    "antonyms": [
                        "opgewekt",
                        "vrolijk",
                        "blij",
                        "levenslustig"
                    ],
                    "common_phrases": [
                        "zich depressief voelen",
                        "een depressieve bui"
                    ],
                    "target_lang_definitions": {
                        "en": "Feeling downhearted, gloomy, and lacking energy.",
                        "ru": "Чувствующий уныние, мрачность и отсутствие энергии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "depressed",
                                "target_lang_sense_clarification": "feeling sad or despondent"
                            },
                            {
                                "target_lang_word": "down",
                                "target_lang_sense_clarification": "informal: feeling low spirits"
                            },
                            {
                                "target_lang_word": "gloomy",
                                "target_lang_sense_clarification": "feeling pessimistic or sad"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подавленный",
                                "target_lang_sense_clarification": "о настроении"
                            },
                            {
                                "target_lang_word": "унылый",
                                "target_lang_sense_clarification": "грустный, безрадостный"

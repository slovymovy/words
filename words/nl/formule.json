                },
                {
                    "sense_id": "d4c4f726-6032-3c61-8725-43584b1b25e5",
                    "sense_definition": "Een vaste bewoording of uitdrukking die men in bepaalde situaties gebruikt",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brief eindigde met een standaard beleefde <w>formule</w>.",
                            "target_lang_translations": {

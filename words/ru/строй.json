                    "common_phrases": [
                        "выйти из строя",
                        "ввести в строй",
                        "вернуться в строй",
                        "вывести из строя"
                    ],
                    "target_lang_definitions": {
                        "en": "Operational state or working condition (mostly used in idioms)."

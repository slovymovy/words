                {
                    "sense_id": "aafeed7c-8b1c-3f69-a2e5-b6a5186478cd",
                    "sense_definition": "A member or descendant of any of various ethnic groups which typically have dark pigmentation of the skin.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ethnicity_noun_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The organization supports the rights of <w>blacks</w> in the community.",
                            "target_lang_translations": {
                                "ru": "Организация поддерживает права <w>чернокожих</w> в общине."
                            }

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грохот",
                                "target_lang_sense_clarification": "громкий шум от удара или падения"
                            },
                            {
                                "target_lang_word": "раскаты",
                                "target_lang_sense_clarification": "громкий, прерывистый звук (например, грома)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

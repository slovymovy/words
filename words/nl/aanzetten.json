    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "22b71c88-9719-34dd-b5d2-e3db0956bbcf",
                    "sense_definition": "In werking stellen; zorgen dat een apparaat of machine begint te werken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "start_activate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is koud hier, dus ik ga de verwarming <w>aanzetten</w>.",
                            "target_lang_translations": {
                                "en": "It is cold here, so I am going to <w>turn on</w> the heating.",
                                "ru": "Здесь холодно, поэтому я <w>включу</w> отопление."
                            }
                        },
                        {
                            "text": "Heb jij de radio <w>aangezet</w> om naar het nieuws te luisteren?",
                            "target_lang_translations": {
                                "en": "Did you <w>switch on</w> the radio to listen to the news?",
                                "ru": "Ты <w>включил</w> радио, чтобы послушать новости?"
                            }
                        }
                    ],
                    "synonyms": [
                        "inschakelen",
                        "aandoen",
                        "starten"
                    ],
                    "antonyms": [
                        "uitzetten",
                        "uitschakelen"
                    ],
                    "common_phrases": [
                        "de tv aanzetten",
                        "het licht aanzetten",
                        "de computer aanzetten"
                    ],
                    "target_lang_definitions": {
                        "en": "To put into operation; to ensure a device or machine starts working.",
                        "ru": "Приводить в действие; заставлять прибор или машину начать работать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn on",
                                "target_lang_sense_clarification": "general term for activating devices"
                            },
                            {
                                "target_lang_word": "switch on",
                                "target_lang_sense_clarification": "often implies using a switch"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "активировать устройство (свет, прибор)"
                            },
                            {
                                "target_lang_word": "запускать",
                                "target_lang_sense_clarification": "приводить механизм в движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c00b2a7-6aca-3dbf-a3d0-185a2d304f8a",
                    "sense_definition": "Iets vastmaken aan iets anders, vaak door naaien of monteren.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "attach",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleermaker moet een nieuwe knoop aan mijn jas <w>aanzetten</w>.",
                            "target_lang_translations": {
                                "en": "The tailor needs to <w>sew</w> a new button <w>on</w> my coat.",
                                "ru": "Портной должен <w>пришить</w> новую пуговицу к моему пальто."
                            }
                        },
                        {
                            "text": "Ze hebben een extra stuk aan het gebouw <w>aangezet</w>.",
                            "target_lang_translations": {
                                "en": "They have <w>attached</w> an extra section to the building.",
                                "ru": "Они <w>пристроили</w> дополнительную часть к зданию."
                            }

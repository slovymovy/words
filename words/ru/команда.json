            "senses": [
                {
                    "sense_id": "b3de1a91-1b3d-392c-8b4d-1b0db793ef4e",
                    "sense_definition": "Спортивный коллектив, группа спортсменов во главе с капитаном, выступающих вместе в соревнованиях.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша футбольная <w>команда</w> выиграла важный матч.",
                            "target_lang_translations": {
                                "en": "Our football <w>team</w> won an important match."
                            }
                        },
                        {
                            "text": "Мой брат мечтает попасть в сборную <w>команду</w> страны.",
                            "target_lang_translations": {
                                "en": "My brother dreams of getting into the national <w>team</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "футбольная команда",
                        "капитан команды",
                        "играть в команде",
                        "сборная команда"
                    ],
                    "target_lang_definitions": {
                        "en": "A sports group, a group of athletes led by a captain, competing together."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "team",
                                "target_lang_sense_clarification": "sports group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b97747e4-cb75-36b0-aeeb-cf0505546fbd",
                    "sense_definition": "Устный приказ или сигнал начать какое-либо действие.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдаты побежали вперёд по <w>команде</w> офицера.",
                            "target_lang_translations": {
                                "en": "The soldiers ran forward at the officer's <w>command</w>."
                            }
                        },
                        {
                            "text": "Собака хорошо знает <w>команду</w> «сидеть».",
                            "target_lang_translations": {
                                "en": "The dog knows the 'sit' <w>command</w> well."
                            }

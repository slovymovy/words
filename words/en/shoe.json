    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb6fd558-6598-3579-a91d-51f9b46694a0",
                    "sense_definition": "A protective covering for the foot, usually made of leather or synthetic material, having a sturdy sole and not extending above the ankle.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "footwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Get your <w>shoes</w> on now, or you'll be late for school.",
                            "target_lang_translations": {
                                "ru": "Надевай <w>ботинки</w> сейчас же, иначе опоздаешь в школу."
                            }
                        },
                        {
                            "text": "She bought a comfortable pair of running <w>shoes</w> for the marathon.",
                            "target_lang_translations": {
                                "ru": "Она купила пару удобных беговых <w>кроссовок</w> для марафона."
                            }
                        }
                    ],
                    "synonyms": [
                        "kick"
                    ],
                    "common_phrases": [
                        "pair of shoes",
                        "put on shoes",
                        "tie one's shoes",
                        "take off shoes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитное покрытие для ноги, обычно сделанное из кожи или синтетического материала, имеющее прочную подошву и не закрывающее щиколотку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туфля",
                                "target_lang_sense_clarification": "низкая обувь, не закрывающая щиколотку"
                            },
                            {
                                "target_lang_word": "ботинок",
                                "target_lang_sense_clarification": "часто используется как общее название для закрытой обуви"
                            },
                            {
                                "target_lang_word": "обувь",
                                "target_lang_sense_clarification": "общее понятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14d1d4b9-67df-3727-8890-992d9f680f42",
                    "sense_definition": "A U-shaped piece of metal nailed to a horse's hoof to protect it from wear.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metal_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The blacksmith replaced the loose <w>shoe</w> on the horse's left hoof.",
                            "target_lang_translations": {
                                "ru": "Кузнец заменил расшатавшуюся <w>подкову</w> на левом копыте лошади."
                            }
                        },
                        {
                            "text": "It is considered good luck to hang a horse <w>shoe</w> over a doorway.",
                            "target_lang_translations": {
                                "ru": "Считается, что повесить <w>подкову</w> над дверным проёмом — к удаче."
                            }
                        }
                    ],
                    "common_phrases": [
                        "throw a shoe",
                        "horse shoe"
                    ],
                    "target_lang_definitions": {
                        "ru": "U-образная металлическая пластина, прибиваемая к копыту лошади для защиты от износа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подкова",
                                "target_lang_sense_clarification": "металлическая набойка на копыто"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10b019e8-a1b6-34ab-a450-428678615523",
                    "sense_definition": "A part of a braking system, especially in drum brakes, that presses against a wheel or drum to create friction and stop motion.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic said the brake <w>shoes</w> were worn down and needed replacing.",
                            "target_lang_translations": {
                                "ru": "Механик сказал, что тормозные <w>колодки</w> износились и требуют замены."
                            }
                        },
                        {
                            "text": "Friction from the <w>shoe</w> against the drum slows down the vehicle.",
                            "target_lang_translations": {
                                "ru": "Трение <w>колодки</w> о барабан замедляет транспортное средство."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brake shoe"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть тормозной системы, особенно в барабанных тормозах, которая прижимается к колесу или барабану для создания трения и остановки движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тормозная колодка",
                                "target_lang_sense_clarification": "в автомобилях и механизмах"
                            },
                            {
                                "target_lang_word": "башмак",
                                "target_lang_sense_clarification": "технический термин, часто на железной дороге"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c96b822f-c96a-3e93-96ea-9e99021b60c3",
                    "sense_definition": "A device used in casinos to hold multiple decks of playing cards for dealing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "gambling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dealer drew the next card from the <w>shoe</w> with a smooth motion.",
                            "target_lang_translations": {
                                "ru": "Крупье плавным движением вытянул следующую карту из <w>шуза</w>."
                            }
                        },
                        {
                            "text": "Using a <w>shoe</w> allows the casino to deal more hands between shuffles.",
                            "target_lang_translations": {
                                "ru": "Использование <w>башмака</w> позволяет казино раздавать больше рук между тасовками."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dealing shoe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приспособление, используемое в казино для хранения нескольких колод игральных карт при раздаче."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "башмак",
                                "target_lang_sense_clarification": "устройство для раздачи карт"
                            },
                            {
                                "target_lang_word": "шуз",
                                "target_lang_sense_clarification": "профессиональный жаргон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27dd8529-7083-3382-866e-866be49a4946",
                    "sense_definition": "An ingot of gold or silver shaped somewhat like a boat, formerly used as currency in East Asia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Merchants in the 19th century often traded using a silver <w>shoe</w>.",
                            "target_lang_translations": {
                                "ru": "Торговцы в XIX веке часто вели торговлю, используя серебряный <w>ямб</w>."
                            }
                        },
                        {
                            "text": "The museum exhibit featured a rare gold <w>shoe</w> from the Qing dynasty.",
                            "target_lang_translations": {
                                "ru": "На музейной выставке был представлен редкий золотой <w>ямб</w> династии Цин."
                            }
                        }
                    ],
                    "synonyms": [
                        "boat money",
                        "shoe money"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слиток золота или серебра, формой напоминающий лодку, ранее использовавшийся как валюта в Восточной Азии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ямб",
                                "target_lang_sense_clarification": "китайский серебряный слиток"
                            },
                            {
                                "target_lang_word": "слиток",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a5dc8720-8351-3361-b98c-f4f7c50fe4c2",
                    "sense_definition": "To fit a person or animal, especially a horse, with shoes.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_fitting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farrier came to the stable to <w>shoe</w> the horses before the race.",
                            "target_lang_translations": {
                                "ru": "Кузнец пришел в конюшню, чтобы <w>подковать</w> лошадей перед скачками."
                            }
                        },
                        {
                            "text": "The hikers were well clothed and <w>shod</w> for the difficult ascent.",
                            "target_lang_translations": {
                                "ru": "Туристы были хорошо одеты и <w>обуты</w> для трудного восхождения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shoe a horse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обувать человека или подковывать животное, особенно лошадь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подковывать",
                                "target_lang_sense_clarification": "лошадей"
                            },
                            {
                                "target_lang_word": "обувать",
                                "target_lang_sense_clarification": "людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "597a3d2a-d1c4-3704-b133-43c4e2f6dac0",
                    "sense_definition": "To cover the tip or bottom of an object with a protective layer of metal or other material.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The antique walking stick was <w>shod</w> with silver at the base.",
                            "target_lang_translations": {
                                "ru": "Антикварная трость была <w>окована</w> серебром у основания."
                            }
                        },
                        {
                            "text": "Builders <w>shod</w> the wooden piles with iron to help them penetrate the ground.",
                            "target_lang_translations": {
                                "ru": "Строители <w>оковали</w> деревянные сваи железом, чтобы помочь им войти в грунт."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать наконечник или основание предмета защитным слоем металла или другого материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оковывать",
                                "target_lang_sense_clarification": "укреплять металлом"
                            },
                            {
                                "target_lang_word": "обшивать",
                                "target_lang_sense_clarification": "покрывать материалом"
                            }
                        ]
                    }
                }
            ]
        },

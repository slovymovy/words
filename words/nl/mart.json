            "senses": [
                {
                    "sense_id": "45d74bb4-2756-360e-9864-05a3644ae948",
                    "sense_definition": "Derde persoon enkelvoud tegenwoordige tijd van <w>marren</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>mart</w> niet langer en gaat direct aan de slag.",
                            "target_lang_translations": {
                                "ru": "Он больше не <w>медлит</w> и сразу приступает к работе."
                            }
                        },
                        {
                            "text": "De zeeman <w>mart</w> het touw stevig vast aan de bolder.",
                            "target_lang_translations": {
                                "ru": "Моряк крепко <w>привязывает</w> трос к кнехту."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Третье лицо единственного числа настоящего времени от глагола marren (медлить или привязывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медлить",
                                "target_lang_sense_clarification": "мешкать, задерживаться"
                            },
                            {
                                "target_lang_word": "привязывать",
                                "target_lang_sense_clarification": "крепить, швартовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76d86b53-124d-3273-bb30-18a5bb70b861",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>marren</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jij <w>mart</w> te veel in plaats van door te werken.",
                            "target_lang_translations": {
                                "ru": "Ты слишком много <w>мешкаешь</w> вместо того, чтобы работать."
                            }
                        },
                        {
                            "text": "Als jij het touw <w>mart</w>, zorg ik voor de rest van de lading.",
                            "target_lang_translations": {
                                "ru": "Если ты <w>закрепишь</w> веревку, я позабочусь об остальном грузе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }

            "senses": [
                {
                    "sense_id": "5b978ead-f4fa-310b-bacc-74fdcd387543",
                    "sense_definition": "Potoczne, często lekceważące określenie przybysza lub handlarza pochodzącego zza wschodniej granicy (z Ukrainy, Rosji lub Białorusi).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na miejskim bazarze można było spotkać niejednego <w>wańkę</w> sprzedającego towary ze Wschodu.",
                            "target_lang_translations": {
                                "ru": "На городском рынке можно было встретить не одного <w>Ваньку</w>, продающего товары с Востока."
                            }
                        },
                        {
                            "text": "Miejscowi nazywali każdego wędrownego handlarza <w>wańką</w>, niezależnie od jego prawdziwego imienia.",
                            "target_lang_translations": {
                                "ru": "Местные называли каждого бродячего торговца <w>Ванькой</w>, независимо от его настоящего имени."
                            }
                        }
                    ],
                    "synonyms": [
                        "wania"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, часто пренебрежительное название приезжего или торговца из-за восточной границы (из Украины, России или Беларуси)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ванька",
                                "target_lang_sense_clarification": "собирательное прозвище выходцев с Востока"
                            },
                            {
                                "target_lang_word": "челнок",
                                "target_lang_sense_clarification": "торговец, привозящий товар из-за границы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bf16f87-96ff-3e83-baa8-304de4ca0321",
                    "sense_definition": "Regionalne określenie przedsionka lub ganku w domu, używane dawniej na Kresach Wschodnich.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zostawiliśmy zabłocone buty w <w>wańce</w>, zanim weszliśmy do ciepłej kuchni.",
                            "target_lang_translations": {
                                "ru": "Мы оставили грязные ботинки в <w>сенях</w>, прежде чем войти в теплую кухню."
                            }
                        },
                        {
                            "text": "Stara, drewniana <w>wańka</w> chroniła wejście do domu przed mroźnym wiatrem.",
                            "target_lang_translations": {
                                "ru": "Старые деревянные <w>сени</w> защищали вход в дом от морозного ветра."
                            }
                        }
                    ],
                    "synonyms": [
                        "wania",
                        "wania"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Kresy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное название прихожей или крыльца в доме, использовавшееся ранее на Восточных Кресах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сени",
                                "target_lang_sense_clarification": "входное помещение в традиционном доме"
                            },
                            {
                                "target_lang_word": "крыльцо",
                                "target_lang_sense_clarification": "пристройка перед входом в дом"

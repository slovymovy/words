            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2697bbb1-5e37-362f-bb70-0d22dabed3a2",
                    "sense_definition": "Przechowywać informacje w pamięci i potrafić je odtworzyć; mieć obraz czegoś w umyśle",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "memory_recall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety nie <w>pamiętam</w> twojego numeru telefonu.",
                            "target_lang_translations": {
                                "ru": "К сожалению, я не <w>помню</w> твоего номера телефона."
                            }
                        },
                        {
                            "text": "Czy <w>pamiętasz</w>, gdzie zostawiliśmy samochód?",
                            "target_lang_translations": {
                                "ru": "Ты <w>помнишь</w>, где мы оставили машину?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wspominać"
                    ],
                    "antonyms": [
                        "zapominać"
                    ],
                    "common_phrases": [
                        "dobrze pamiętać",
                        "pamiętać wszystko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хранить информацию в памяти и уметь её воспроизвести; иметь образ чего-либо в уме"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помнить",
                                "target_lang_sense_clarification": "сохранять в памяти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab54c7eb-1374-3666-a604-f1c402985ec2",
                    "sense_definition": "Nie zapomnieć o zrobieniu czegoś; mieć coś na uwadze",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "memory_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <w>pamiętać</w> o zamknięciu okien przed wyjściem.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>помнить</w> о том, что нужно закрыть окна перед уходом."
                            }
                        },
                        {
                            "text": "Proszę, <w>pamiętaj</w> o naszym spotkaniu jutro rano.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>помни</w> о нашей встрече завтра утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "uważać",
                        "zważać"
                    ],
                    "antonyms": [
                        "zapominać"
                    ],
                    "common_phrases": [
                        "pamiętać o czymś",
                        "pamiętać o kimś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не забыть сделать что-то; иметь что-то в виду"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не забывать",
                                "target_lang_sense_clarification": "о необходимости сделать что-то"
                            },
                            {
                                "target_lang_word": "помнить",
                                "target_lang_sense_clarification": "держать в уме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "473bbfb0-f68b-319a-abe3-20b5d4ef1eaf",
                    "sense_definition": "Dbać o kogoś lub o coś; troszczyć się i brać pod uwagę czyjeś dobro",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dobry przyjaciel zawsze <w>pamięta</w> o twoich urodzinach.",
                            "target_lang_translations": {
                                "ru": "Хороший друг всегда <w>помнит</w> о твоем дне рождения."
                            }
                        },
                        {
                            "text": "Warto <w>pamiętać</w> o zdrowiu i regularnie ćwiczyć.",
                            "target_lang_translations": {
                                "ru": "Стоит <w>помнить</w> о здоровье и регулярно заниматься спортом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pamiętać o rodzinie",
                        "pamiętać o bliskich"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заботиться о ком-то или о чем-то; принимать во внимание чье-то благо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помнить",
                                "target_lang_sense_clarification": "проявлять заботу"
                            },
                            {
                                "target_lang_word": "не забывать",
                                "target_lang_sense_clarification": "уделять внимание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21d3d27c-cf75-3635-a534-dc5748c0fc7f",
                    "sense_definition": "Pochodzić z dawnych czasów; być świadkiem przeszłości (o przedmiotach, budynkach)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten zabytkowy kościół <w>pamięta</w> czasy średniowiecza.",
                            "target_lang_translations": {
                                "ru": "Эта старинная церковь <w>помнит</w> времена средневековья."
                            }
                        },
                        {
                            "text": "Stare drzewo w parku <w>pamięta</w> historię wielu pokoleń.",
                            "target_lang_translations": {
                                "ru": "Старое дерево в парке <w>помнит</w> историю многих поколений."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pamiętać czasy",
                        "pamiętać wieki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить из давних времен; быть свидетелем прошлого (о предметах, зданиях)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помнить",
                                "target_lang_sense_clarification": "быть свидетелем событий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c2ebcd5-5019-3ad3-b2a4-8d517f6c69cb",
                    "sense_definition": "Okazywać szacunek i czcić pamięć o kimś lub o czymś",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "honor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy zgromadzeni <w>pamiętają</w> o bohaterach narodowych.",
                            "target_lang_translations": {
                                "ru": "Все собравшиеся <w>помнят</w> о национальных героях."
                            }
                        },
                        {
                            "text": "Należy <w>pamiętać</w> o ważnych rocznicach historycznych.",
                            "target_lang_translations": {
                                "ru": "Следует <w>помнить</w> о важных исторических годовщинах."
                            }
                        }
                    ],
                    "synonyms": [
                        "czcić",
                        "świętować"
                    ],
                    "common_phrases": [
                        "pamiętać o zmarłych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявлять уважение и чтить память о ком-то или о чем-то"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чтить",
                                "target_lang_sense_clarification": "память о ком-то"
                            },
                            {
                                "target_lang_word": "помнить",
                                "target_lang_sense_clarification": "с уважением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8baf4f7f-a833-3473-b154-7cbd3c8f4fc6",
                    "sense_definition": "Chować urazę; nie wybaczać wyrządzonej krzywdy mimo upływu czasu",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "grudge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "On długo <w>pamięta</w> złe słowa, które usłyszał.",
                            "target_lang_translations": {
                                "ru": "Он долго <w>помнит</w> плохие слова, которые услышал."
                            }
                        },
                        {
                            "text": "Nie warto <w>pamiętać</w> starych sporów i kłótni.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>помнить</w> старые споры и ссоры."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpamiętywać"
                    ],
                    "antonyms": [
                        "zapominać"
                    ],
                    "common_phrases": [
                        "pamiętać urazę",
                        "pamiętać krzywdy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Таить обиду; не прощать причиненное зло, несмотря на прошедшее время"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помнить",
                                "target_lang_sense_clarification": "не забывать обиды"
                            },
                            {
                                "target_lang_word": "припоминать",
                                "target_lang_sense_clarification": "быть злопамятным"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "7aac1b8c-4abd-3aff-b6cc-d805600d21f1",
                    "sense_definition": "O cenie, wadze lub wynagrodzeniu: zawierający podatek, opakowanie lub inne obciążenia; całkowity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podana cena <w>brutto</w> zawiera już doliczony podatek VAT.",
                            "target_lang_translations": {
                                "ru": "Указанная цена <w>брутто</w> уже включает добавленный НДС."
                            }
                        },
                        {
                            "text": "Jego pensja <w>brutto</w> jest znacznie wyższa niż kwota, którą otrzymuje na rękę.",
                            "target_lang_translations": {
                                "ru": "Его зарплата <w>брутто</w> значительно выше суммы, которую он получает на руки."
                            }
                        }
                    ],
                    "antonyms": [
                        "netto"
                    ],
                    "common_phrases": [
                        "cena brutto",
                        "waga brutto",
                        "kwota brutto",
                        "wynagrodzenie brutto"
                    ],
                    "target_lang_definitions": {
                        "ru": "О цене, весе или зарплате: включающий налог, упаковку или другие начисления; полный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брутто",
                                "target_lang_sense_clarification": "о весе, цене или зарплате"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f1a9729c-557f-36a1-aea6-c1985649902a",
                    "sense_definition": "W sposób uwzględniający wszystkie składniki kosztów, bez odliczeń; łącznie z opakowaniem lub podatkiem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie koszty projektu musimy policzyć <w>brutto</w>, żeby uniknąć niespodzianek.",
                            "target_lang_translations": {
                                "ru": "Все расходы по проекту мы должны посчитать <w>брутто</w>, чтобы избежать сюрпризов."
                            }
                        },
                        {
                            "text": "Czy ta stawka godzinowa jest podana <w>brutto</w> czy netto?",
                            "target_lang_translations": {
                                "ru": "Эта почасовая ставка указана <w>брутто</w> или нетто?"
                            }
                        }
                    ],
                    "antonyms": [
                        "netto"
                    ],
                    "common_phrases": [
                        "zarabiać brutto",
                        "liczyć brutto",
                        "podawać brutto"
                    ],
                    "target_lang_definitions": {
                        "ru": "С учетом всех составляющих стоимости, без вычетов; вместе с упаковкой или налогом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брутто",
                                "target_lang_sense_clarification": "без вычетов"
                            },
                            {
                                "target_lang_word": "грязными",
                                "target_lang_sense_clarification": "о зарплате (разг.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f6b552ba-fab5-35d1-a9cc-80f920df2d8c",
                    "sense_definition": "Pociąg towarowy, zazwyczaj o dużej masie przewożonego ładunku.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na bocznicę kolejową wjechało właśnie ciężkie <w>brutto</w> z węglem.",
                            "target_lang_translations": {
                                "ru": "На железнодорожный путь только что въехал тяжелый <w>товарняк</w> с углем."
                            }
                        },
                        {
                            "text": "Dyżurny ruchu wstrzymał <w>brutto</w>, aby przepuścić pociąg pasażerski.",
                            "target_lang_translations": {
                                "ru": "Дежурный по станции задержал <w>товарняк</w>, чтобы пропустить пассажирский поезд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ciężkie brutto"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "kolejnictwo"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Товарный поезд, обычно перевозящий тяжелый груз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "товарняк",
                                "target_lang_sense_clarification": "разг."
                            },
                            {
                                "target_lang_word": "грузовой состав",
                                "target_lang_sense_clarification": "ж/д термин"

                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "missing_target",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Снаряд пролетел <w>мимо</w> цели и взорвался в поле.",
                            "target_lang_translations": {
                                "en": "The shell flew <w>past</w> the target and exploded in the field."
                            }
                        },
                        {
                            "text": "Он пропустил мои слова <w>мимо</w> ушей.",
                            "target_lang_translations": {
                                "en": "He let my words go <w>past</w> his ears."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мимо цели",
                        "мимо ушей",
                        "мимо ворот"
                    ],
                    "target_lang_definitions": {

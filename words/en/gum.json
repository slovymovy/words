{
    "word_family": [
        "gummy",
        "gumdrop",
        "gumshoe",
        "gumwood",
        "gumminess"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "432267dd-6c77-31ac-b409-e342a6592039",
                    "sense_definition": "A sweet, flavored substance made for chewing but not for swallowing.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "substance_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She popped a piece of <w>gum</w> into her mouth after lunch.",
                            "target_lang_translations": {
                                "ru": "Она закинула в рот <w>жвачку</w> после обеда."
                            }
                        },
                        {
                            "text": "Please wrap your chewing <w>gum</w> in paper before throwing it in the bin.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, заверните вашу <w>жевательную резинку</w> в бумагу, прежде чем выбросить её в мусорное ведро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chewing gum",
                        "bubble gum",
                        "stick of gum",
                        "sugar-free gum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкая, ароматизированная субстанция, предназначенная для жевания, но не для проглатывания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жевательная резинка",
                                "target_lang_sense_clarification": "нейтральное"
                            },
                            {
                                "target_lang_word": "жвачка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed6f81a2-7730-3992-8eb2-514e8b8fa635",
                    "sense_definition": "The firm, pink flesh inside the mouth that covers the jawbones and surrounds the base of the teeth.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should brush your teeth and <w>gums</w> gently twice a day.",
                            "target_lang_translations": {
                                "ru": "Вам следует аккуратно чистить зубы и <w>дёсны</w> дважды в день."
                            }
                        },
                        {
                            "text": "The dentist said that healthy <w>gums</w> do not bleed when you floss.",
                            "target_lang_translations": {
                                "ru": "Стоматолог сказал, что здоровые <w>дёсны</w> не кровоточат, когда вы пользуетесь зубной нитью."
                            }
                        }
                    ],
                    "synonyms": [
                        "gingiva"
                    ],
                    "common_phrases": [
                        "gum disease",
                        "bleeding gums",
                        "sore gums",
                        "receding gums"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плотная розовая ткань внутри рта, покрывающая челюстные кости и окружающая основание зубов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "десна",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03465f6e-a334-3d0c-8912-9fd021551afc",
                    "sense_definition": "A sticky substance produced by some trees and plants that hardens when dry, often used in glues, thickeners, or candy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "substance_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The back of the stamp is coated with <w>gum</w> so it sticks to the envelope.",
                            "target_lang_translations": {
                                "ru": "Оборотная сторона марки покрыта <w>клеем</w>, чтобы она приклеивалась к конверту."
                            }
                        },
                        {
                            "text": "Manufacturers often use vegetable <w>gum</w> to thicken sauces and dressings.",
                            "target_lang_translations": {
                                "ru": "Производители часто используют растительную <w>камедь</w> для загущения соусов и заправок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gum arabic",
                        "xanthan gum",
                        "guar gum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Липкое вещество, выделяемое некоторыми деревьями и растениями, которое затвердевает при высыхании; часто используется в клеях, загустителях или конфетах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "камедь",
                                "target_lang_sense_clarification": "растительный сок, гуммиарабик"
                            },
                            {
                                "target_lang_word": "смола",
                                "target_lang_sense_clarification": "древесная смола"
                            },
                            {
                                "target_lang_word": "клей",
                                "target_lang_sense_clarification": "клейкое вещество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c21ef9ca-1c37-30df-86ec-c756df2282ff",
                    "sense_definition": "A tree that produces a sticky substance or resin, particularly a eucalyptus tree.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nature_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The koala spent most of the day sleeping high in the <w>gum</w> tree.",
                            "target_lang_translations": {
                                "ru": "Коала провела большую часть дня, спя высоко на <w>эвкалипте</w>."
                            }
                        },
                        {
                            "text": "We planted a large blue <w>gum</w> in the garden for shade.",
                            "target_lang_translations": {
                                "ru": "Мы посадили в саду большой голубой <w>эвкалипт</w> для тени."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gum tree",
                        "blue gum",
                        "red gum"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "commonly used in Australia and parts of the US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дерево, выделяющее липкое вещество или смолу, в частности эвкалипт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эвкалипт",
                                "target_lang_sense_clarification": "распространенное название"
                            },
                            {
                                "target_lang_word": "камедное дерево",
                                "target_lang_sense_clarification": "буквальный перевод"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c0cff92a-64fe-3fad-9781-8c36a563ae22",
                    "sense_definition": "To cause a machine or process to stop working properly, often by clogging it with a sticky substance.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "substance_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Using the wrong type of oil can <w>gum</w> up the engine valves.",
                            "target_lang_translations": {
                                "ru": "Использование неподходящего масла может <w>засорить</w> клапаны двигателя."
                            }
                        },
                        {
                            "text": "Too much bureaucracy tends to <w>gum</w> up the works and slow down progress.",
                            "target_lang_translations": {
                                "ru": "Излишняя бюрократия имеет свойство <w>тормозить</w> работу и замедлять прогресс."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gum up",
                        "gum up the works"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить машину или процесс перестать работать должным образом, часто путем засорения липким веществом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засорять",
                                "target_lang_sense_clarification": "механически"
                            },
                            {
                                "target_lang_word": "тормозить",
                                "target_lang_sense_clarification": "переносно, о процессе"
                            },
                            {
                                "target_lang_word": "стопорить",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4cebe49-532a-3fac-9bff-715ff2abc112",
                    "sense_definition": "To cover, fasten, or stiffen something with a sticky substance like glue.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "substance_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>gummed</w> the label onto the package securely.",
                            "target_lang_translations": {
                                "ru": "Он надежно <w>приклеил</w> этикетку к посылке."
                            }
                        },
                        {
                            "text": "The pages of the old book were <w>gummed</w> together by the spilled juice.",
                            "target_lang_translations": {
                                "ru": "Страницы старой книги <w>склеились</w> из-за пролитого сока."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gum down",
                        "gum together"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать, скреплять или делать жестким с помощью липкого вещества, например клея."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склеивать",
                                "target_lang_sense_clarification": "соединять вместе"
                            },
                            {
                                "target_lang_word": "приклеивать",
                                "target_lang_sense_clarification": "прикреплять к чему-либо"
                            },
                            {
                                "target_lang_word": "гуммировать",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55fd621e-2cfa-3c31-858a-81e28ccb9365",
                    "sense_definition": "To chew something using the gums rather than teeth, often done by babies or toothless elderly people.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baby sat in the high chair <w>gumming</w> a piece of soft bread.",
                            "target_lang_translations": {
                                "ru": "Малыш сидел на высоком стульчике, <w>жамкая</w> кусочек мягкого хлеба."
                            }
                        },
                        {
                            "text": "Without his dentures, the old man had to <w>gum</w> his food slowly.",
                            "target_lang_translations": {
                                "ru": "Без зубных протезов старику приходилось медленно <w>жевать деснами</w> свою еду."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жевать что-либо деснами, а не зубами, что часто делают младенцы или пожилые люди без зубов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жамкать",
                                "target_lang_sense_clarification": "просторечное"
                            },
                            {
                                "target_lang_word": "жевать деснами",
                                "target_lang_sense_clarification": "описательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "db28111b-41bc-3c13-88c2-000b7d35ac74",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Gum</w> is a character in a popular series of children's books.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Гам</w> — персонаж популярной серии детских книг."
                            }
                        },
                        {
                            "text": "We looked up the name <w>Gum</w> in the local telephone directory.",
                            "target_lang_translations": {
                                "ru": "Мы искали фамилию <w>Гам</w> в местном телефонном справочнике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "family name"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гам",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "diamantine"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bc5df1ea-12c4-3c49-bf61-1c1a9b94ffb6",
                    "sense_definition": "A very hard, transparent precious stone made of pure carbon, often used in jewelry.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "gem_mineral",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wears a beautiful <w>diamond</w> on her finger.",
                            "target_lang_translations": {
                                "ru": "Она носит красивый <w>бриллиант</w> на пальце."
                            }
                        },
                        {
                            "text": "The dozen loose <w>diamonds</w> sparkled in the light.",
                            "target_lang_translations": {
                                "ru": "Дюжина незакрепленных <w>бриллиантов</w> сверкала на свету."
                            }
                        }
                    ],
                    "synonyms": [
                        "sparkler"
                    ],
                    "common_phrases": [
                        "diamond ring",
                        "rough diamond",
                        "diamond necklace",
                        "diamond mine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Драгоценный камень из чистого углерода, самый твердый из известных минералов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бриллиант",
                                "target_lang_sense_clarification": "ограненный камень, используемый в украшениях"
                            },
                            {
                                "target_lang_word": "алмаз",
                                "target_lang_sense_clarification": "минерал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a263c3e9-552a-34d5-8fa6-1ee76ace60c9",
                    "sense_definition": "A figure with four straight sides of equal length forming two acute and two obtuse angles; a rhombus.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pattern on the sweater consists of red and blue <w>diamonds</w>.",
                            "target_lang_translations": {
                                "ru": "Узор на свитере состоит из красных и синих <w>ромбов</w>."
                            }
                        },
                        {
                            "text": "I can see the <w>diamonds</w> on the snake's back.",
                            "target_lang_translations": {
                                "ru": "Я вижу <w>ромбы</w> на спине змеи."
                            }
                        }
                    ],
                    "synonyms": [
                        "rhomb",
                        "lozenge"
                    ],
                    "common_phrases": [
                        "diamond shape",
                        "diamond pattern"
                    ],
                    "target_lang_definitions": {
                        "ru": "Геометрическая фигура с четырьмя равными сторонами; ромб."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ромб",
                                "target_lang_sense_clarification": "геометрическая фигура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2625a58-53c0-3ef0-be01-874413d5ea52",
                    "sense_definition": "A playing card of the suit marked with red diamond shapes.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have only one <w>diamond</w> in my hand.",
                            "target_lang_translations": {
                                "ru": "У меня на руках только одна <w>бубна</w>."
                            }
                        },
                        {
                            "text": "He played the ten of <w>diamonds</w> to win the trick.",
                            "target_lang_translations": {
                                "ru": "Он пошел с десятки <w>бубен</w>, чтобы взять взятку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ace of diamonds",
                        "king of diamonds",
                        "queen of diamonds",
                        "jack of diamonds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Карточная масть, обозначаемая красными ромбами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бубны",
                                "target_lang_sense_clarification": "масть"
                            },
                            {
                                "target_lang_word": "бубна",
                                "target_lang_sense_clarification": "карта этой масти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "201a8c80-93c9-30df-93eb-ffbf03589191",
                    "sense_definition": "The square area of a baseball field enclosed by the four bases; the infield.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "baseball",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teams met on the <w>diamond</w> to start the game.",
                            "target_lang_translations": {
                                "ru": "Команды встретились на <w>площадке</w>, чтобы начать игру."
                            }
                        },
                        {
                            "text": "The groundskeepers prepared the <w>diamond</w> before the match.",
                            "target_lang_translations": {
                                "ru": "Смотрители подготовили <w>площадку</w> перед матчем."
                            }
                        }
                    ],
                    "synonyms": [
                        "infield",
                        "baseball diamond",
                        "baseball field",
                        "ball field"
                    ],
                    "antonyms": [
                        "outfield"
                    ],
                    "common_phrases": [
                        "baseball diamond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренняя часть бейсбольного поля, ограниченная четырьмя базами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бейсбольная площадка",
                                "target_lang_sense_clarification": "часть поля"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e1864dd2-0580-3863-86f3-5cfa621fe932",
                    "sense_definition": "Made of or containing diamond.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "gem_mineral",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He gave her <w>diamond</w> earrings for her birthday.",
                            "target_lang_translations": {
                                "ru": "Он подарил ей <w>бриллиантовые</w> серьги на день рождения."
                            }
                        },
                        {
                            "text": "The tool has a <w>diamond</w> tip for cutting glass.",
                            "target_lang_translations": {
                                "ru": "Инструмент имеет <w>алмазный</w> наконечник для резки стекла."
                            }
                        }
                    ],
                    "synonyms": [
                        "diamantine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный из алмаза или содержащий алмаз."

        "nadawczy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6ac8bc43-7604-3522-ae2f-7ebb24796aa0",
                    "sense_definition": "Przekazać przesyłkę, list lub paczkę pracownikowi poczty lub kurierowi w celu dostarczenia jej do adresata.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść rano na pocztę, aby <w>nadać</w> list polecony do urzędu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно утром пойти на почту, чтобы <w>отправить</w> заказное письмо в ведомство."
                            }
                        },
                        {
                            "text": "Wczoraj <w>nadałem</w> dużą paczkę z prezentami dla rodziny za granicą.",
                            "target_lang_translations": {
                                "ru": "Вчера я <w>отправил</w> большую посылку с подарками для семьи за границей."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomóc"
                    ],
                    "antonyms": [
                        "odebrać"
                    ],
                    "common_phrases": [
                        "nadać list polecony",
                        "nadać paczkę",
                        "nadać bagaż",
                        "nadać przesyłkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать посылку, письмо или бандероль почтовому работнику или курьеру для доставки адресату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправить",
                                "target_lang_sense_clarification": "почтой или курьером"
                            },
                            {
                                "target_lang_word": "сдать",
                                "target_lang_sense_clarification": "багаж или груз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "549a5a99-29ef-3ac2-bd28-8b41989c2cf7",
                    "sense_definition": "Przypisać komuś lub czemuś nazwę, tytuł, uprawnienie lub określoną cechę charakterystyczną.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice długo zastanawiali się, jakie imię <w>nadać</w> swojemu nowo narodzonemu dziecku.",
                            "target_lang_translations": {
                                "ru": "Родители долго думали, какое имя <w>дать</w> своему новорожденному ребенку."
                            }
                        },
                        {
                            "text": "Nowy menedżer chce <w>nadać</w> naszej pracy szybsze tempo i lepszą organizację.",
                            "target_lang_translations": {
                                "ru": "Новый менеджер хочет <w>придать</w> нашей работе более быстрый темп и лучшую организацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "zadać"
                    ],
                    "antonyms": [
                        "odebrać"
                    ],
                    "common_phrases": [
                        "nadać imię",
                        "nadać tytuł",
                        "nadać kształt",
                        "nadać ton",
                        "nadać tempo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Присвоить кому-либо или чему-либо имя, титул, право или определенную характерную черту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "придать",
                                "target_lang_sense_clarification": "форму, вид, характер, темп"
                            },
                            {
                                "target_lang_word": "дать",
                                "target_lang_sense_clarification": "имя"
                            },
                            {
                                "target_lang_word": "присвоить",
                                "target_lang_sense_clarification": "звание, титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f589728-35ce-36c5-894d-e6ca8d8d1e0e",
                    "sense_definition": "Być odpowiednim, użytecznym lub wystarczająco dobrym do jakiegoś celu lub zadania (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "utility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary płaszcz może się jeszcze <w>nadać</w> do pracy w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Это старое пальто может еще <w>сгодиться</w> для работы в саду."
                            }
                        },
                        {
                            "text": "Nie jestem pewien, czy ten kandydat <w>nada</w> się na stanowisko kierownika.",
                            "target_lang_translations": {
                                "ru": "Я не уверен, <w>подойдет</w> ли этот кандидат на должность руководителя."
                            }
                        }
                    ],
                    "synonyms": [
                        "spodobać się"
                    ],
                    "common_phrases": [
                        "nadać się do użytku",
                        "nadać się na stanowisko",
                        "to się nie nada"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть подходящим, полезным или достаточно хорошим для какой-либо цели или задачи (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сгодиться",
                                "target_lang_sense_clarification": "быть полезным"
                            },
                            {
                                "target_lang_word": "подойти",
                                "target_lang_sense_clarification": "быть подходящим"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94d85504-4bc6-360c-97c1-0efb7da381f2",
                    "sense_definition": "Przesłać sygnał radiowy, telewizyjny lub internetowy do odbiorców; wyemitować program.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "media_broadcast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Telewizja publiczna ma obowiązek <w>nadać</w> oficjalny komunikat prezydenta.",
                            "target_lang_translations": {
                                "ru": "Общественное телевидение обязано <w>передать</w> официальное сообщение президента."
                            }
                        },
                        {
                            "text": "Stacja radiowa <w>nadała</w> wczoraj powtórkę wywiadu ze znanym pisarzem.",
                            "target_lang_translations": {
                                "ru": "Радиостанция вчера <w>передала</w> повтор интервью с известным писателем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nadać komunikat",
                        "nadać audycję",
                        "nadać sygnał",
                        "nadać program"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать радио-, теле- или интернет-сигнал получателям; выпустить программу в эфир."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передать",
                                "target_lang_sense_clarification": "сообщение, сигнал"
                            },
                            {
                                "target_lang_word": "выпустить в эфир",
                                "target_lang_sense_clarification": "программу"
                            }
                        ]
                    }
                }
            ]
        }
    ]

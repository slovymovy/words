    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7be9e877-05f8-388c-9f6f-9ebf43c3b23c",
                    "sense_definition": "To officially force a rule, tax, punishment, or belief to be accepted or obeyed.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "enforce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government decided to <w>impose</w> a new tax on luxury goods to increase revenue."
                        },
                        {
                            "text": "Strict sanctions were <w>imposed</w> on the country following the violation of the treaty."
                        }
                    ],
                    "synonyms": [
                        "force",
                        "inflict",
                        "enjoin"
                    ],
                    "antonyms": [
                        "lift",
                        "revoke",
                        "remove"
                    ],
                    "common_phrases": [
                        "impose a ban",
                        "impose a fine",
                        "impose restrictions",
                        "impose sanctions"
                    ]
                },
                {

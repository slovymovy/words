    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "04401a5b-0c56-3211-bb44-bf5ee251d51c",
                    "sense_definition": "Drunk or tipsy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "intoxication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>ebrious</w> man stumbled out of the tavern late at night.",
                            "target_lang_translations": {
                                "ru": "<w>Пьяный</w> мужчина, спотыкаясь, вышел из таверны поздно ночью."
                            }
                        },
                        {
                            "text": "Police officers detained the <w>ebrious</w> driver for safety reasons.",
                            "target_lang_translations": {
                                "ru": "Сотрудники полиции задержали <w>нетрезвого</w> водителя в целях безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "drunk"
                    ],
                    "common_phrases": [
                        "ebrious state",
                        "ebrious condition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пьяный или подвыпивший."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пьяный",
                                "target_lang_sense_clarification": "находящийся под воздействием алкоголя"
                            },
                            {
                                "target_lang_word": "нетрезвый",
                                "target_lang_sense_clarification": "формальное или вежливое обозначение состояния опьянения"
                            },
                            {
                                "target_lang_word": "опьяневший",
                                "target_lang_sense_clarification": "пришедший в состояние опьянения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "180aae4e-8d88-3ec8-a87f-1843ad25648f",
                    "sense_definition": "Similar to that of a drunk person.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "intoxication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The neurologist noted the patient's <w>ebrious</w> step during the examination.",
                            "target_lang_translations": {
                                "ru": "Невролог отметил <w>нетвёрдую</w> походку пациента во время осмотра."
                            }
                        },
                        {
                            "text": "He walked with an <w>ebrious</w> sway despite being completely sober.",
                            "target_lang_translations": {
                                "ru": "Он шёл с <w>пьяным</w> покачиванием, хотя был совершенно трезв."
                            }
                        }
                    ],
                    "synonyms": [
                        "drunklike",
                        "intoxicated-like"
                    ],
                    "common_phrases": [
                        "ebrious gait",
                        "ebrious step"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий поведение или движения пьяного человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нетвёрдый",
                                "target_lang_sense_clarification": "о походке или шагах: неуверенный, шатающийся"
                            },
                            {
                                "target_lang_word": "пьяный",
                                "target_lang_sense_clarification": "в переносном смысле: шатающийся, как у пьяного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52baffa2-3549-34b2-8e25-e809acc0ce6c",
                    "sense_definition": "Having a tendency towards alcohol abuse.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>ebrious</w> habits eventually led to serious health complications.",
                            "target_lang_translations": {
                                "ru": "Его <w>алкогольные</w> привычки в конечном итоге привели к серьезным осложнениям со здоровьем."
                            }
                        },
                        {
                            "text": "The family tried to intervene before his <w>ebrious</w> nature caused more harm.",
                            "target_lang_translations": {
                                "ru": "Семья пыталась вмешаться, пока его <w>склонная к пьянству</w> натура не причинила больше вреда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ebrious habits"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный к злоупотреблению алкоголем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "алкогольный",
                                "target_lang_sense_clarification": "связанный с употреблением спиртного (о привычках)"
                            },
                            {
                                "target_lang_word": "склонный к пьянству",
                                "target_lang_sense_clarification": "имеющий тягу к спиртному"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
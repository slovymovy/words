        "service",
        "uitserveren"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8feed9b7-ed18-3dfe-9dc6-03b723f62924",
                    "sense_definition": "Iemand of iets hard bekritiseren en vervolgens negeren of als waardeloos beschouwen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dismissal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De journalist besloot het slechte boek in zijn recensie volledig af te <w>serveren</w>.",
                            "target_lang_translations": {
                                "en": "The journalist decided to completely <w>slam</w> the bad book in his review."
                            }
                        },
                        {
                            "text": "Na de grote fout werd de politicus door zijn eigen partij <w>afgeserveerd</w>.",
                            "target_lang_translations": {
                                "en": "After the big mistake, the politician was <w>written off</w> by his own party."
                            }
                        }
                    ],
                    "synonyms": [
                        "afkraken",
                        "afdanken"
                    ],
                    "common_phrases": [
                        "iemand publiekelijk afserveren",
                        "een plan afserveren"
                    ],
                    "target_lang_definitions": {
                        "en": "To criticize someone or something harshly and subsequently ignore them or regard them as worthless."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "write off",
                                "target_lang_sense_clarification": "to decide that someone or something is useless or not worth paying attention to"
                            },
                            {
                                "target_lang_word": "dismiss",
                                "target_lang_sense_clarification": "to treat something as unworthy of serious consideration"
                            },
                            {
                                "target_lang_word": "slam",
                                "target_lang_sense_clarification": "to criticize severely (often used in journalism)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da8f7416-65bc-3558-952b-e1736d9b0b82",
                    "sense_definition": "Een wedstrijd of game winnend afsluiten door een goede opslag te geven.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tennisser wist de spannende set met een ace <w>af</w> te <w>serveren</w>.",
                            "target_lang_translations": {
                                "en": "The tennis player managed to <w>serve out</w> the exciting set with an ace."
                            }
                        },
                        {
                            "text": "Hij heeft de wedstrijd overtuigend <w>afgeserveerd</w>.",
                            "target_lang_translations": {
                                "en": "He convincingly <w>served out</w> the match."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de wedstrijd afserveren",
                        "de set afserveren"
                    ],
                    "target_lang_definitions": {
                        "en": "To win a match or game by delivering a winning serve."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve out",
                                "target_lang_sense_clarification": "to win a game or match in tennis while serving"
                            }
                        ]
                    }
                }
            ]
        }
    ]

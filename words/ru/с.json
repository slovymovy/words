{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "b8b6a63a-c2ee-33d2-9bc3-71db8346723e",
                    "sense_definition": "Употребляется для обозначения совместности действия, сопровождения или наличия предмета (используется с творительным падежом).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я люблю гулять в парке <с> моей собакой.",
                            "target_lang_translations": {
                                "en": "I like walking in the park <with> my dog."
                            }
                        },
                        {
                            "text": "Мы часто пьём чай <с> лимоном и сахаром.",
                            "target_lang_translations": {
                                "en": "We often drink tea <with> lemon and sugar."
                            }
                        }
                    ],
                    "common_phrases": [
                        "с удовольствием",
                        "с уважением",
                        "с кем-то",
                        "с чем-то"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to denote joint action, accompaniment, or the presence of an object (used with the instrumental case)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "with",
                                "target_lang_sense_clarification": "indicating accompaniment or possession"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7322d32-750b-387f-af7e-56f9c31ccbd0",
                    "sense_definition": "Указывает на место или поверхность, откуда удаляется предмет или происходит движение (используется с родительным падежом).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, убери посуду <со> стола.",
                            "target_lang_translations": {
                                "en": "Please clear the dishes <from> the table."
                            }
                        },
                        {
                            "text": "Отец возвращается <с> работы в шесть часов.",
                            "target_lang_translations": {
                                "en": "Father returns <from> work at six o'clock."
                            }
                        }
                    ],
                    "antonyms": [
                        "на"
                    ],
                    "common_phrases": [
                        "с работы",
                        "с улицы",
                        "с места"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the place or surface from which an object is removed or movement occurs (used with the genitive case)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "indicating source or removal"
                            },
                            {
                                "target_lang_word": "off",
                                "target_lang_sense_clarification": "indicating removal from a surface"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ac15991-267e-34f5-8361-9aa4ef84f25f",
                    "sense_definition": "Обозначает начальный момент времени действия или состояния (используется с родительным падежом).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот магазин открыт <с> девяти утра.",
                            "target_lang_translations": {
                                "en": "This store is open <from> nine in the morning."
                            }
                        },
                        {
                            "text": "Мы знаем друг друга <с> самого детства.",
                            "target_lang_translations": {
                                "en": "We have known each other <since> early childhood."
                            }
                        }
                    ],
                    "antonyms": [
                        "до"
                    ],
                    "common_phrases": [
                        "с утра",
                        "с детства",
                        "с тех пор"
                    ],
                    "target_lang_definitions": {
                        "en": "Denotes the starting moment in time of an action or state (used with the genitive case)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "indicating a starting point in time"
                            },
                            {
                                "target_lang_word": "since",
                                "target_lang_sense_clarification": "from a past time until now"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17ef3970-f93d-33ab-9d8d-510199089398",
                    "sense_definition": "Указывает на причину действия или состояния (используется с родительным падежом).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок громко заплакал <с> испуга.",
                            "target_lang_translations": {
                                "en": "The child started crying loudly <from> fright."
                            }
                        },
                        {
                            "text": "Она сделала эту ошибку <с> непривычки.",
                            "target_lang_translations": {
                                "en": "She made this mistake <out of> lack of habit."
                            }
                        }
                    ],
                    "common_phrases": [
                        "с испуга",
                        "с горя",
                        "со скуки"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the cause of an action or state (used with the genitive case)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "indicating cause"
                            },
                            {
                                "target_lang_word": "out of",
                                "target_lang_sense_clarification": "indicating motivation or cause"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91342c11-30b8-3ad7-95fd-cb9ea62da85b",
                    "sense_definition": "Указывает на происхождение или место прибытия, заменяя предлог «из» (используется с родительным падежом).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой новый знакомый приехал <с> Москвы.",
                            "target_lang_translations": {
                                "en": "My new acquaintance arrived <from> Moscow."
                            }
                        },
                        {
                            "text": "Эти ребята <с> нашего двора очень дружные.",
                            "target_lang_translations": {
                                "en": "These guys <from> our yard are very friendly."
                            }
                        }
                    ],
                    "common_phrases": [
                        "с района"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often replaces 'из' in casual speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates origin or place of arrival, replacing the preposition 'из' (used with the genitive case)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "indicating origin"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca9a1a68-a4a4-3775-8be7-b8b001e8b402",
                    "sense_definition": "Обозначает приблизительную меру, размер или сравнение (используется с винительным падежом).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы поймали рыбу размером <с> мою руку.",
                            "target_lang_translations": {
                                "en": "We caught a fish the size <of> my hand."
                            }
                        },
                        {
                            "text": "Там было грязи <с> километр.",
                            "target_lang_translations": {
                                "en": "There was <about> a kilometer of mud."
                            }
                        }
                    ],
                    "common_phrases": [
                        "с гулькин нос"
                    ],
                    "target_lang_definitions": {
                        "en": "Denotes an approximate measure, size, or comparison (used with the accusative case)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "about",
                                "target_lang_sense_clarification": "indicating approximation"
                            },
                            {
                                "target_lang_word": "the size of",
                                "target_lang_sense_clarification": "indicating comparison in size"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "96d4abfd-efef-343c-b4ff-69d9d3bb2f59",
                    "sense_definition": "Сокращение от слова «север», используемое на картах и компасах.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Стрелка компаса всегда указывает на <с>.",
                            "target_lang_translations": {
                                "en": "The compass needle always points to <N>."
                            }
                        },
                        {
                            "text": "Ветер дул <с>, принося холодный воздух.",
                            "target_lang_translations": {
                                "en": "The wind blew from the <N>, bringing cold air."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for the word 'North', used on maps and compasses."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "N",
                                "target_lang_sense_clarification": "abbreviation for North"
                            },
                            {
                                "target_lang_word": "North",
                                "target_lang_sense_clarification": "cardinal direction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8adfaac-7f69-34ef-891f-72434de6ad90",
                    "sense_definition": "Сокращение от слова «слон» в шахматной нотации.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В записи партии этот ход обозначен как <с>e4.",
                            "target_lang_translations": {
                                "en": "In the game record, this move is marked as <B>e4."
                            }
                        },
                        {
                            "text": "Фигура <с> может ходить по диагонали.",
                            "target_lang_translations": {
                                "en": "The <Bishop> piece can move diagonally."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for the word 'Bishop' (elephant) in chess notation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "B",
                                "target_lang_sense_clarification": "abbreviation for Bishop"
                            },
                            {
                                "target_lang_word": "Bishop",
                                "target_lang_sense_clarification": "chess piece"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
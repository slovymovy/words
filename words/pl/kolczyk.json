                            },
                            {
                                "target_lang_word": "ушная бирка",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2695d78f-42c4-3313-b459-f5940e175387",

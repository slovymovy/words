                {
                    "sense_id": "334a4344-8300-3fb0-9612-b33be104e96b",
                    "sense_definition": "A failing academic grade, usually the lowest possible mark given to a student.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "grades",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was very upset because he got an <w>F</w> on his math test.",
                            "target_lang_translations": {
                                "ru": "Он был очень расстроен, так как получил <w>двойку</w> за контрольную по математике."
                            }
                        },
                        {
                            "text": "If you do not submit the assignment, you will receive an <w>F</w> for the course.",
                            "target_lang_translations": {
                                "ru": "Если вы не сдадите задание, вы получите <w>неуд</w> за курс."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get an F",
                        "straight Fs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неудовлетворительная оценка, обычно самая низкая отметка, выставляемая студенту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двойка",
                                "target_lang_sense_clarification": "школьная оценка"
                            },
                            {
                                "target_lang_word": "неуд",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },

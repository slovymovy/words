    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b5e5de5d-e2e9-397b-ab0f-35bbab3cdd6f",
                    "sense_definition": "Тайная, нелегальная деятельность, скрытая от властей, или положение людей, занимающихся такой деятельностью",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Революционеры были вынуждены уйти в глубокое <w>подполье</w>.",
                            "target_lang_translations": {
                                "en": "The revolutionaries were forced to go deep <w>underground</w>."
                            }
                        },
                        {
                            "text": "Партия долгие годы работала в <w>подполье</w>, скрываясь от полиции.",
                            "target_lang_translations": {
                                "en": "The party worked <w>underground</w> for many years, hiding from the police."
                            }
                        }
                    ],
                    "common_phrases": [
                        "уйти в подполье",
                        "выйти из подполья",
                        "работа в подполье"
                    ],
                    "target_lang_definitions": {
                        "en": "Secret, illegal activity hidden from authorities, or the state of being in hiding"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "underground",
                                "target_lang_sense_clarification": "state of secrecy or hiding"
                            },
                            {
                                "target_lang_word": "hiding",
                                "target_lang_sense_clarification": "act of concealing oneself"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ded95266-27ff-3e75-8d99-aa0a72f3a07b",
                    "sense_definition": "Совокупность людей или организаций, ведущих нелегальную политическую борьбу",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Городское <w>подполье</w> распространяло листовки с призывами к забастовке.",
                            "target_lang_translations": {
                                "en": "The city <w>underground</w> distributed leaflets calling for a strike."
                            }
                        },
                        {
                            "text": "Полиция арестовала нескольких активных участников местного <w>подполья</w>.",
                            "target_lang_translations": {
                                "en": "The police arrested several active members of the local <w>underground</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "лидеры подполья",
                        "разгром подполья"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people or organizations engaged in illegal political struggle"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "underground",
                                "target_lang_sense_clarification": "resistance organization"
                            },
                            {
                                "target_lang_word": "resistance",
                                "target_lang_sense_clarification": "political movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "867681a8-3317-3d3a-a310-0d90df30311a",
                    "sense_definition": "Пространство или помещение под полом дома, обычно используемое для хранения продуктов",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка хранила картошку и банки с соленьями в холодном <w>подполье</w>.",
                            "target_lang_translations": {
                                "en": "Grandma kept potatoes and jars of pickles in the cold <w>cellar</w>."
                            }
                        },
                        {
                            "text": "Кот случайно провалился в открытое <w>подполье</w> и не мог выбраться.",
                            "target_lang_translations": {
                                "en": "The cat accidentally fell into the open <w>subfloor</w> and couldn't get out."
                            }
                        }
                    ],
                    "synonyms": [
                        "подпол",
                        "подвал"
                    ],
                    "common_phrases": [
                        "спуститься в подполье",
                        "достать из подполья",
                        "сырое подполье"
                    ],
                    "target_lang_definitions": {
                        "en": "A space or room under the floor of a house, usually used for storing food"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cellar",
                                "target_lang_sense_clarification": "storage space under a house"
                            },
                            {
                                "target_lang_word": "subfloor",
                                "target_lang_sense_clarification": "space beneath floorboards"
                            },
                            {
                                "target_lang_word": "crawl space",
                                "target_lang_sense_clarification": "low space under a floor"
                            }
                        ]
                    }
                }
            ]
        }
    ]

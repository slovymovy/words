    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fd326d66-2483-3725-be43-21957f3447f3",
                    "sense_definition": "Форма родительного падежа единственного числа существительного <w>хор</w>",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Голос церковного <хора> звучал торжественно под сводами старого собора.",
                            "target_lang_translations": {
                                "en": "The voice of the church <choir> sounded solemn under the vaults of the old cathedral."
                            }
                        },
                        {
                            "text": "Без поддержки <хора> солист чувствовал себя на сцене неуверенно.",
                            "target_lang_translations": {
                                "en": "Without the support of the <chorus>, the soloist felt insecure on stage."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пение хора",
                        "участник хора",
                        "дирижёр хора",
                        "солист хора"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive singular form of the noun 'хор' (choir/chorus)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "choir",
                                "target_lang_sense_clarification": "group of singers"
                            },
                            {
                                "target_lang_word": "chorus",
                                "target_lang_sense_clarification": "musical ensemble or part of a song"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9ee1b80-0cf1-3b97-9731-167f3b161780",
                    "sense_definition": "Народный танец-хоровод у южных славян, румын и молдаван, а также музыка к этому танцу",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На городской площади заиграла музыка, и люди начали весело танцевать <хору>.",
                            "target_lang_translations": {
                                "en": "Music began to play in the town square, and people started to merrily dance the <hora>."
                            }
                        },
                        {
                            "text": "Молдавская <хора> требует от танцоров хорошего чувства ритма и сплочённости.",
                            "target_lang_translations": {
                                "en": "The Moldovan <hora> requires a good sense of rhythm and cohesion from the dancers."
                            }
                        }
                    ],
                    "synonyms": [
                        "хоровод"
                    ],
                    "common_phrases": [
                        "танцевать хору",
                        "молдавская хора",
                        "весёлая хора"
                    ],
                    "target_lang_definitions": {
                        "en": "A folk circle dance of the southern Slavs, Romanians, and Moldovans, as well as the music for this dance"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hora",
                                "target_lang_sense_clarification": "folk dance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6938778c-da23-307b-a1b5-5a14c26f033e",
                    "sense_definition": "Сельскохозяйственная территория, прилегающая к древнегреческому полису и используемая его гражданами",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Афинская <хора> обеспечивала город необходимым зерном и другими продуктами.",
                            "target_lang_translations": {
                                "en": "The Athenian <chora> provided the city with necessary grain and other products."
                            }
                        },
                        {
                            "text": "Археологи обнаружили остатки древних усадеб на территории, где раньше располагалась <хора>.",
                            "target_lang_translations": {
                                "en": "Archaeologists discovered the remains of ancient estates in the territory where the <chora> used to be located."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical term"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Agricultural territory adjacent to an Ancient Greek polis and used by its citizens"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chora",
                                "target_lang_sense_clarification": "historical term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abc82bcf-2991-3931-a1a8-79f764e1c02b",
                    "sense_definition": "Название лисицы или самки лисицы в некоторых местных говорах",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animals_dialect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охотник заметил, как рыжая <хора> быстро скрылась в густых зарослях кустарника.",
                            "target_lang_translations": {
                                "en": "The hunter noticed how the red <fox> quickly disappeared into the thick bushes."
                            }
                        },
                        {
                            "text": "В сказках этого региона хитрая <хора> часто обманывает других зверей.",
                            "target_lang_translations": {
                                "en": "In the fairy tales of this region, the cunning <fox> often deceives other animals."
                            }
                        }
                    ],
                    "synonyms": [
                        "лисица"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Name for a fox or vixen in certain local dialects"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fox",
                                "target_lang_sense_clarification": "dialectal"
                            },
                            {
                                "target_lang_word": "vixen",
                                "target_lang_sense_clarification": "female fox"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9ed9c54-a4ef-3bd5-8c9a-b795acb256c5",
                    "sense_definition": "Название самца оленя в некоторых местных говорах",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animals_dialect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На опушку леса вышел крупный <хора> с огромными ветвистыми рогами.",
                            "target_lang_translations": {
                                "en": "A large <stag> with huge branching antlers stepped out onto the edge of the forest."
                            }
                        },
                        {
                            "text": "Старый <хора> внимательно прислушивался к каждому шороху в лесу.",
                            "target_lang_translations": {
                                "en": "The old <stag> listened attentively to every rustle in the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "олень"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Name for a male deer in certain local dialects"
                    },
                    "translations": {

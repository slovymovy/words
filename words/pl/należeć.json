{
    "word_family": [
        "należność",
        "należny",
        "przynależeć",
        "przynależność"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b8ec4d2b-bc04-319c-9257-19e2437b6dd4",
                    "sense_definition": "Być czyjąś własnością, stanowić czyjeś mienie",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten piękny dom z ogrodem <należy> do moich dziadków.",
                            "target_lang_translations": {
                                "ru": "Этот красивый дом с садом <принадлежит> моим бабушке и дедушке."
                            }
                        },
                        {
                            "text": "Wszystkie te książki <należą> teraz do biblioteki miejskiej.",
                            "target_lang_translations": {
                                "ru": "Все эти книги теперь <принадлежат> городской библиотеке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "należeć do mnie",
                        "należeć do państwa",
                        "należeć do rodziny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть чьей-либо собственностью, составлять чье-либо имущество"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принадлежать",
                                "target_lang_sense_clarification": "быть собственностью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20414a2f-6364-3015-8bf1-2bda7a8b196e",
                    "sense_definition": "Być członkiem jakiejś organizacji, grupy społecznej lub elementem zbioru",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "membership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój starszy brat od lat <należy> do klubu szachowego.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат уже много лет <состоит> в шахматном клубе."
                            }
                        },
                        {
                            "text": "Polska <należy> do Unii Europejskiej od dwóch tysięcy czwartego roku.",
                            "target_lang_translations": {
                                "ru": "Польша <входит> в Европейский союз с две тысячи четвертого года."
                            }
                        }
                    ],
                    "synonyms": [
                        "uczestniczyć w czymś"
                    ],
                    "common_phrases": [
                        "należeć do unii",
                        "należeć do partii",
                        "należeć do grupy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть членом организации, социальной группы или частью множества"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "входить",
                                "target_lang_sense_clarification": "быть частью группы или объединения"
                            },
                            {
                                "target_lang_word": "состоять",
                                "target_lang_sense_clarification": "быть членом организации"
                            },
                            {
                                "target_lang_word": "принадлежать",
                                "target_lang_sense_clarification": "относиться к классу или группе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9048366c-6c73-39ff-9487-ee1f81d20ead",
                    "sense_definition": "Być koniecznym, właściwym lub wymaganym (używane w formie bezosobowej)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W bibliotece <należy> zachować ciszę i nie rozmawiać głośno.",
                            "target_lang_translations": {
                                "ru": "В библиотеке <следует> соблюдать тишину и не разговаривать громко."
                            }
                        },
                        {
                            "text": "Do ciasta <należy> dodać dwa jajka i szklankę mąki.",
                            "target_lang_translations": {
                                "ru": "В тесто <нужно> добавить два яйца и стакан муки."
                            }
                        }
                    ],
                    "synonyms": [
                        "polegać"
                    ],
                    "common_phrases": [
                        "jak należy",
                        "należy pamiętać",
                        "nie należy",
                        "należy dodać"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane bezosobowo"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть необходимым, правильным или требуемым (используется в безличной форме)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следует",
                                "target_lang_sense_clarification": "рекомендуется или должно"
                            },
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "необходимо"
                            },
                            {
                                "target_lang_word": "надлежит",
                                "target_lang_sense_clarification": "официальное требование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fe2e45a-ebc1-33c7-a9f1-55127333f600",
                    "sense_definition": "Przysługiwać komuś, być czymś zasłużonym (używane z zaimkiem „się”)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "entitlement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po tak ciężkiej pracy każdemu <należy> się chwila odpoczynku.",
                            "target_lang_translations": {
                                "ru": "После такой тяжелой работы каждому <полагается> минутка отдыха."
                            }
                        },
                        {
                            "text": "Zgodnie z umową, pracownikowi <należy> się dodatkowe wynagrodzenie.",
                            "target_lang_translations": {
                                "ru": "Согласно договору, работнику <причитается> дополнительное вознаграждение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "należy mi się",
                        "nagroda się należy",
                        "to się należy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причитаться кому-либо, быть заслуженным (используется с «się»)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полагаться",
                                "target_lang_sense_clarification": "по праву или закону"
                            },
                            {
                                "target_lang_word": "причитаться",
                                "target_lang_sense_clarification": "обычно о деньгах или воздаянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd26096f-2d3c-3bd0-9523-9af85160273e",
                    "sense_definition": "Zależeć od czyjejś woli, być w czyjejś gestii lub obowiązku",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ostateczna decyzja w tej sprawie <należy> tylko do ciebie.",
                            "target_lang_translations": {
                                "ru": "Окончательное решение по этому делу <зависит> только от тебя."
                            }
                        },
                        {
                            "text": "Wybór odpowiedniego kandydata <należy> do komisji rekrutacyjnej.",
                            "target_lang_translations": {
                                "ru": "Выбор подходящего кандидата <лежит> на приемной комиссии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "decyzja należy do",
                        "wybór należy do",
                        "przyszłość należy do"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зависеть от чьей-либо воли, быть в чьем-либо ведении или обязанности"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зависеть",
                                "target_lang_sense_clarification": "определяться кем-то"
                            },
                            {
                                "target_lang_word": "принадлежать",
                                "target_lang_sense_clarification": "относиться к компетенции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "543ba0e7-4550-3925-99cf-a800272246df",
                    "sense_definition": "Leżeć wystarczająco długo lub do woli (zwykle z zaimkiem „się”)",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_colloquial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W niedzielę wreszcie <należałem> się w łóżku za wszystkie czasy.",
                            "target_lang_translations": {
                                "ru": "В воскресенье я наконец-то <належался> в кровати за все времена."
                            }
                        },
                        {
                            "text": "Chory pacjent <należał> się w szpitalu przez dwa tygodnie.",
                            "target_lang_translations": {
                                "ru": "Больной пациент <належался> в больнице за две недели."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "dotyczy leżenia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лежать достаточно долго или вдоволь (обычно с возвратным местоимением)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "належаться",
                                "target_lang_sense_clarification": "полежать вдоволь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
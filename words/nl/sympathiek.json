    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6cf2bba5-4722-3a02-b28c-bf9515659262",
                    "sense_definition": "Aangenaam en vriendelijk in de omgang.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een <w>sympathieke</w> kerel die altijd voor iedereen klaarstaat.",
                            "target_lang_translations": {
                                "ru": "Он <w>симпатичный</w> парень, который всегда готов всем помочь."
                            }
                        },
                        {
                            "text": "Mijn nieuwe buren zijn erg <w>sympathiek</w> en hebben ons meteen uitgenodigd.",
                            "target_lang_translations": {
                                "ru": "Мои новые соседи очень <w>приятные</w>, они сразу же нас пригласили."
                            }
                        }
                    ],
                    "synonyms": [
                        "aardig",
                        "vriendelijk",
                        "prettig"
                    ],
                    "antonyms": [
                        "onsympathiek"
                    ],
                    "common_phrases": [
                        "een sympathieke man",
                        "heel sympathiek",
                        "sympathiek overkomen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приятный и дружелюбный в общении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "симпатичный",
                                "target_lang_sense_clarification": "располагающий к себе, приятный человек"
                            },
                            {
                                "target_lang_word": "приятный",
                                "target_lang_sense_clarification": "вызывающий положительные эмоции"
                            },
                            {
                                "target_lang_word": "милый",
                                "target_lang_sense_clarification": "славный, хороший в общении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "874923e8-8046-381d-a16a-e6d5660788bd",
                    "sense_definition": "Bereid zich in de zorgen van een ander te verplaatsen; begripvol.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kreeg geen <w>sympathiek</w> gehoor toen hij zijn problemen uitlegde.",
                            "target_lang_translations": {
                                "ru": "Он не нашел <w>сочувственного</w> отклика, когда объяснял свои проблемы."
                            }
                        },
                        {
                            "text": "De directeur stond <w>sympathiek</w> tegenover het voorstel van de werknemers.",
                            "target_lang_translations": {
                                "ru": "Директор <w>благожелательно</w> отнесся к предложению сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "begripvol",
                        "medelevend"
                    ],
                    "antonyms": [
                        "onsympathiek"
                    ],
                    "common_phrases": [
                        "sympathiek staan tegenover"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовый понять проблемы другого; полный сочувствия и понимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сочувственный",
                                "target_lang_sense_clarification": "выражающий сострадание или понимание"
                            },
                            {
                                "target_lang_word": "благожелательный",
                                "target_lang_sense_clarification": "проявляющий доброе отношение или поддержку (о позиции)"
                            },
                            {
                                "target_lang_word": "понимающий",
                                "target_lang_sense_clarification": "относящийся с пониманием"
                            }
                        ]
                    }
                }
            ]
        }
    ]

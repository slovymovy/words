    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b372ac11-2377-365e-b67b-f5f62490801c",
                    "sense_definition": "Een computerprogramma of softwaretoepassing, meestal ontworpen voor mobiele apparaten zoals smartphones of tablets.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "software",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb een nieuwe <w>app</w> gedownload om Nederlands te leren.",
                            "target_lang_translations": {
                                "ru": "Я скачал новое <w>приложение</w>, чтобы учить нидерландский.",
                                "en": "I downloaded a new <w>app</w> to learn Dutch."
                            }
                        },
                        {
                            "text": "Deze <w>app</w> werkt niet goed op mijn oude telefoon.",
                            "target_lang_translations": {
                                "ru": "Это <w>приложение</w> плохо работает на моем старом телефоне.",
                                "en": "This <w>app</w> doesn't work well on my old phone."
                            }
                        }
                    ],
                    "synonyms": [
                        "applicatie",
                        "toepassing"
                    ],
                    "common_phrases": [
                        "app downloaden",
                        "app installeren",
                        "app openen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа или программное обеспечение, обычно разработанное для мобильных устройств, таких как смартфоны или планшеты.",
                        "en": "A computer program or software application, usually designed for mobile devices like smartphones or tablets."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приложение",
                                "target_lang_sense_clarification": "программа для смартфона"
                            },
                            {
                                "target_lang_word": "апп",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "app",
                                "target_lang_sense_clarification": "software application"
                            },
                            {
                                "target_lang_word": "application",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b68671e2-55eb-3154-8731-0afdcb9fd46c",
                    "sense_definition": "Een digitaal berichtje dat verstuurd is via een berichtendienst zoals WhatsApp.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "message",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stuur me even een <w>app</w> als je veilig thuis bent.",
                            "target_lang_translations": {
                                "ru": "Отправь мне <w>сообщение</w>, когда благополучно доберешься домой.",
                                "en": "Send me a <w>text</w> when you get home safely."
                            }
                        },
                        {
                            "text": "Ik heb je <w>app</w> gisteren helemaal niet gezien.",
                            "target_lang_translations": {
                                "ru": "Я вчера вообще не видел твоего <w>сообщения</w>.",
                                "en": "I didn't see your <w>message</w> at all yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "appje",
                        "berichtje"
                    ],
                    "common_phrases": [
                        "een app sturen",
                        "een app krijgen",
                        "een app lezen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Цифровое сообщение, отправленное через службу обмена сообщениями, такую как WhatsApp.",
                        "en": "A digital message sent via a messaging service like WhatsApp."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообщение",
                                "target_lang_sense_clarification": "в мессенджере"
                            },
                            {
                                "target_lang_word": "смс",
                                "target_lang_sense_clarification": "разговорное обобщение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "text",
                                "target_lang_sense_clarification": "informal message"
                            },
                            {
                                "target_lang_word": "message",
                                "target_lang_sense_clarification": "digital communication"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9103853-455a-3103-b9d9-e60576cd55f9",
                    "sense_definition": "Een digitaal gesprek of chatgroep binnen een berichtenapplicatie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "message",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet de foto's van het feestje maar even in de <w>app</w>.",
                            "target_lang_translations": {
                                "ru": "Скинь фотки с вечеринки в <w>чат</w>.",
                                "en": "Just put the photos from the party in the <w>group chat</w>."
                            }
                        },
                        {
                            "text": "Er werd druk overlegd in de <w>app</w> van het studentenhuis.",
                            "target_lang_translations": {
                                "ru": "В <w>чате</w> студенческого общежития шло бурное обсуждение.",
                                "en": "There was a busy discussion in the student house <w>group chat</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "groepsapp",
                        "groepschat"
                    ],
                    "common_phrases": [
                        "in de app",
                        "uit de app"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "refers to the group chat context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Цифровая беседа или чат-группа в приложении для обмена сообщениями.",
                        "en": "A digital conversation or chat group within a messaging application."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чат",
                                "target_lang_sense_clarification": "групповая переписка"
                            },
                            {
                                "target_lang_word": "группа",
                                "target_lang_sense_clarification": "беседа в мессенджере"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "group chat",
                                "target_lang_sense_clarification": "conversation group"
                            },
                            {
                                "target_lang_word": "chat",
                                "target_lang_sense_clarification": "digital conversation"
                            }
                        ]
                    }
                }
            ]
        },
        {

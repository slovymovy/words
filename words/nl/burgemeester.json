            "senses": [
                {
                    "sense_id": "9c8f9b17-8290-3b03-b622-7cdf34507a36",
                    "sense_definition": "Het hoofd van het gemeentebestuur en de voorzitter van de gemeenteraad.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "political_office",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>burgemeester</w> opende gisteren feestelijk het nieuwe buurthuis.",
                            "target_lang_translations": {
                                "en": "The <w>mayor</w> festively opened the new community centre yesterday."
                            }
                        },
                        {
                            "text": "Samen met de wethouders maakt de <w>burgemeester</w> plannen voor de stad.",
                            "target_lang_translations": {
                                "en": "Together with the aldermen, the <w>mayor</w> makes plans for the city."
                            }
                        }
                    ],
                    "synonyms": [
                        "burgervader"
                    ],
                    "common_phrases": [
                        "burgemeester en wethouders",
                        "waarnemend burgemeester",
                        "de nieuwe burgemeester"
                    ],
                    "target_lang_definitions": {
                        "en": "The head of the municipal executive and the chairman of the municipal council."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mayor",
                                "target_lang_sense_clarification": "head of city government"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "763a2430-0b93-35bc-a8c3-c31f21a7d231",
                    "sense_definition": "Een benaming voor specifieke grote meeuwensoorten, met name de grote en kleine burgemeester.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "birds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Langs de kust werd een zeldzame <w>burgemeester</w> gespot door vogelaars.",
                            "target_lang_translations": {
                                "en": "A rare <w>Glaucous Gull</w> was spotted by birdwatchers along the coast."
                            }
                        },
                        {
                            "text": "In de winter kun je hier soms een jonge <w>burgemeester</w> zien vliegen.",
                            "target_lang_translations": {
                                "en": "In winter you can sometimes see a young <w>Glaucous Gull</w> flying here."
                            }
                        }
                    ],
                    "synonyms": [
                        "kleine burgemeester",
                        "grote burgemeester"
                    ],
                    "target_lang_definitions": {
                        "en": "A name for specific large gull species, particularly the Glaucous Gull and Iceland Gull."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Glaucous Gull",
                                "target_lang_sense_clarification": "bird species"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "132d2260-adb7-3ed8-b508-af826652fa61",
                    "sense_definition": "Het steunpunt of houten bord dat zich onder de spil van een molen bevindt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mill_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De molenaar moest de <w>burgemeester</w> vervangen omdat het hout was aangetast.",
                            "target_lang_translations": {
                                "en": "The miller had to replace the <w>bearing block</w> because the wood was affected."
                            }
                        },
                        {
                            "text": "Onder de zware spil zorgt de <w>burgemeester</w> voor de nodige ondersteuning.",
                            "target_lang_translations": {
                                "en": "Under the heavy spindle, the <w>bearing block</w> provides the necessary support."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical term in molinology"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The support point or wooden board located under the spindle of a mill."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bearing block",
                                "target_lang_sense_clarification": "part of a windmill"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bac4656-6c34-3bf2-ab83-1991721b8877",
                    "sense_definition": "Een verouderde benaming voor een Romeins consul.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude geschriften wordt de Romeinse consul soms aangeduid als <w>burgemeester</w>.",
                            "target_lang_translations": {
                                "en": "In old writings, the Roman consul is sometimes referred to as <w>mayor</w>."
                            }
                        },
                        {
                            "text": "De macht van de <w>burgemeester</w> in het oude Rome was aanzienlijk.",
                            "target_lang_translations": {
                                "en": "The power of the <w>consul</w> in ancient Rome was considerable."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An archaic term for a Roman consul."
                    },
                    "translations": {
                        "en": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2851f69a-a2f5-3e17-93de-37edf883e88a",
                    "sense_definition": "Sprawić, że coś trwa dłużej; zwiększyć czas ważności lub trwania czegoś",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść do urzędu, aby <w>przedłużyć</w> ważność mojego dowodu osobistego.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти в администрацию, чтобы <w>продлить</w> срок действия моего удостоверения личности."
                            }
                        },
                        {
                            "text": "Lekarze robili wszystko, co w ich mocy, by <w>przedłużyć</w> życie pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врачи делали всё возможное, чтобы <w>продлить</w> жизнь пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydłużyć",
                        "prolongować",
                        "odroczyć"
                    ],
                    "antonyms": [
                        "skrócić",
                        "przyspieszyć"
                    ],
                    "common_phrases": [
                        "przedłużyć umowę",
                        "przedłużyć termin",
                        "przedłużyć życie",
                        "przedłużyć pobyt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы что-то длилось дольше; увеличить срок действия или продолжительность чего-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продлить",
                                "target_lang_sense_clarification": "срок действия, договор, жизнь"
                            },
                            {
                                "target_lang_word": "пролонгировать",
                                "target_lang_sense_clarification": "официально (договор, кредит)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dcbca107-657e-388f-b4eb-eee7aa443dee",
                    "sense_definition": "Zwiększyć wymiary fizyczne czegoś; uczynić coś dłuższym w przestrzeni",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krawcowa obiecała <w>przedłużyć</w> moją sukienkę o kilka centymetrów.",
                            "target_lang_translations": {
                                "ru": "Портниха обещала <w>удлинить</w> моё платье на несколько сантиметров."
                            }
                        },
                        {
                            "text": "Musimy <w>przedłużyć</w> ten kabel, żeby sięgnął do gniazdka w drugim pokoju.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>удлинить</w> этот кабель, чтобы он дотянулся до розетки в другой комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydłużyć"
                    ],
                    "antonyms": [
                        "skrócić",
                        "obciąć"
                    ],
                    "common_phrases": [
                        "przedłużyć kabel",
                        "przedłużyć rzęsy",
                        "przedłużyć włosy",
                        "przedłużyć rękawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличить физические размеры чего-либо; сделать что-то более длинным в пространстве"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удлинить",
                                "target_lang_sense_clarification": "сделать длиннее (одежду, предмет)"
                            },
                            {
                                "target_lang_word": "нарастить",
                                "target_lang_sense_clarification": "волосы, ресницы, ногти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed15bf92-53d2-32f7-aa1e-fcbfda974bd5",
                    "sense_definition": "Trwać dłużej niż planowano lub przewidywano (używane z zaimkiem zwrotnym „się”)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety nasze zebranie w biurze znacznie się <w>przedłużyło</w>.",
                            "target_lang_translations": {
                                "ru": "К сожалению, наше собрание в офисе значительно <w>затянулось</w>."
                            }
                        },
                        {
                            "text": "Zima w tym roku niespodziewanie się <w>przedłużyła</w> i śnieg leżał aż do kwietnia.",
                            "target_lang_translations": {
                                "ru": "Зима в этом году неожиданно <w>затянулась</w>, и снег лежал аж до апреля."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeciągnąć się",
                        "wydłużyć się"
                    ],
                    "antonyms": [
                        "skończyć się"
                    ],
                    "common_phrases": [
                        "spotkanie się przedłużyło",
                        "wizyta się przedłużyła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длиться дольше, чем планировалось или ожидалось"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затянуться",
                                "target_lang_sense_clarification": "длиться слишком долго"
                            },
                            {
                                "target_lang_word": "продлиться",
                                "target_lang_sense_clarification": "продолжаться какое-то время"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e9bc7e73-63ad-3fd1-9809-1ccd6ba21fb9",
                    "sense_definition": "Заниматься делом, затрачивать усилия и время для создания или выполнения чего-либо; трудиться.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "labor_effort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат любит <w>работать</w> в саду по выходным.",
                            "target_lang_translations": {
                                "en": "My brother loves to <w>work</w> in the garden on weekends."
                            }
                        },
                        {
                            "text": "Мы должны <w>работать</w> вместе, чтобы закончить проект вовремя.",
                            "target_lang_translations": {
                                "en": "We must <w>work</w> together to finish the project on time."
                            }
                        }
                    ],
                    "synonyms": [
                        "пользоваться",
                        "служить",
                        "пыхтеть"
                    ],
                    "antonyms": [
                        "отдыхать",
                        "бездействовать"
                    ],
                    "common_phrases": [
                        "много работать",
                        "работать руками",
                        "работать головой",
                        "тяжело работать"
                    ],
                    "target_lang_definitions": {
                        "en": "To engage in activity, expend effort and time to create or perform something; to labor."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "to exert effort"
                            },
                            {
                                "target_lang_word": "labor",
                                "target_lang_sense_clarification": "to toil"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d382a39-593b-340c-ad7d-47c4d7e5be7f",
                    "sense_definition": "Иметь постоянное занятие, состоять на службе, исполнять обязанности сотрудника.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "employment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя мама <w>работает</w> врачом в городской больнице.",
                            "target_lang_translations": {
                                "en": "My mom <w>works</w> as a doctor in the city hospital."
                            }
                        },
                        {
                            "text": "Я хочу <w>работать</w> в крупной международной компании.",
                            "target_lang_translations": {
                                "en": "I want to <w>work</w> in a large international company."
                            }
                        }
                    ],
                    "synonyms": [
                        "служить"
                    ],
                    "common_phrases": [
                        "работать учителем",
                        "работать в офисе",
                        "работать на заводе",
                        "работать по специальности"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a permanent occupation, to be employed, to perform the duties of an employee."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "to have a job"
                            },
                            {
                                "target_lang_word": "be employed",
                                "target_lang_sense_clarification": "formal status"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa1c4ebf-f5ae-3791-b3c7-bf1d8fd9400e",
                    "sense_definition": "Находиться в действии, быть в исправном состоянии (о механизмах, органах); быть открытым для посетителей (об учреждениях).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот старый компьютер больше не <w>работает</w>.",
                            "target_lang_translations": {
                                "en": "This old computer doesn't <w>work</w> anymore."
                            }
                        },
                        {
                            "text": "Продуктовый магазин <w>работает</w> до десяти вечера.",
                            "target_lang_translations": {
                                "en": "The grocery store <w>is open</w> until ten in the evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "действовать",
                        "фунциклировать",
                        "фурычить"
                    ],
                    "antonyms": [
                        "бездействовать"
                    ],
                    "common_phrases": [
                        "телефон не работает",
                        "лифт работает",
                        "сердце работает",
                        "магазин работает"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in action, to be in working order (about mechanisms); to be open for visitors (about institutions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "function correctly"
                            },
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "operate (machines/software)"
                            },
                            {
                                "target_lang_word": "be open",
                                "target_lang_sense_clarification": "for business/institutions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df61f5db-1c56-3dc9-b30e-f791d2f60dce",
                    "sense_definition": "Усердно заниматься чем-либо с целью улучшения, изучения или создания (обычно используется с предлогом «над»).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "labor_effort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Писатель сейчас <w>работает</w> над своим новым романом.",
                            "target_lang_translations": {
                                "en": "The writer is currently <w>working</w> on his new novel."
                            }
                        },
                        {
                            "text": "Тебе нужно больше <w>работать</w> над произношением.",
                            "target_lang_translations": {
                                "en": "You need to <w>work</w> more on your pronunciation."
                            }
                        }
                    ],
                    "common_phrases": [
                        "работать над собой",
                        "работать над ошибками",
                        "работать над проектом"
                    ],
                    "target_lang_definitions": {
                        "en": "To work hard on something with the aim of improvement, study, or creation (usually used with the preposition \"on\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "on something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d21ec1c3-82cd-33dd-b936-c199329cad9e",
                    "sense_definition": "Вести огонь по противнику, атаковать цель (военный жаргон).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Снайпер <w>работает</w> по целям на крыше здания.",
                            "target_lang_translations": {
                                "en": "The sniper is <w>working</w> on targets on the roof of the building."
                            }
                        },
                        {
                            "text": "Артиллерия начала <w>работать</w> по квадрату противника.",
                            "target_lang_translations": {
                                "en": "The artillery started <w>working</w> on the enemy grid."
                            }
                        }
                    ],
                    "common_phrases": [
                        "работать по цели",
                        "снайпер работает"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "военный жаргон"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To fire at an enemy, to attack a target (military slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "engage",
                                "target_lang_sense_clarification": "military target"
                            },
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "slang"
                            }
                        ]
                    }
                }
            ]
        }
    ]

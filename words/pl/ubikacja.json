    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5526191e-da95-31c7-8f30-8b01cfbfe008",
                    "sense_definition": "Pomieszczenie sanitarne wyposażone w urządzenia do załatwiania potrzeb fizjologicznych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sanitary_room",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przepraszam, czy wie pan, gdzie znajduje się <w>ubikacja</w>?",
                            "target_lang_translations": {
                                "ru": "Извините, вы знаете, где находится <w>туалет</w>?"
                            }
                        },
                        {
                            "text": "W tym budynku jest tylko jedna wspólna <w>ubikacja</w> na korytarzu.",
                            "target_lang_translations": {
                                "ru": "В этом здании есть только один общий <w>туалет</w> в коридоре."
                            }
                        }
                    ],
                    "synonyms": [
                        "toaleta",
                        "WC",
                        "ustęp"
                    ],
                    "common_phrases": [
                        "iść do ubikacji",
                        "publiczna ubikacja",
                        "zajęta ubikacja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Санитарное помещение, оборудованное для отправления естественных надобностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туалет",
                                "target_lang_sense_clarification": "помещение"
                            },
                            {
                                "target_lang_word": "уборная",
                                "target_lang_sense_clarification": "разговорное или официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fce4f7ba-57e3-3f3b-9343-c4d94819ced8",
                    "sense_definition": "Urządzenie sanitarne, zazwyczaj w formie ceramicznej muszli, służące do załatwiania potrzeb fizjologicznych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sanitary_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hydraulik musiał wymienić pękniętą <w>ubikację</w> w naszej łazience.",
                            "target_lang_translations": {
                                "ru": "Сантехнику пришлось заменить треснувший <w>унитаз</w> в нашей ванной."
                            }
                        },
                        {
                            "text": "Do czyszczenia <w>ubikacji</w> należy używać silnych środków dezynfekujących.",
                            "target_lang_translations": {
                                "ru": "Для чистки <w>унитаза</w> следует использовать сильные дезинфицирующие средства."
                            }
                        }
                    ],
                    "synonyms": [
                        "muszla",
                        "sedes",
                        "toaleta"
                    ],
                    "common_phrases": [
                        "zatkana ubikacja",
                        "montaż ubikacji",
                        "myć ubikację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Санитарно-техническое устройство, обычно в форме керамической чаши."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "унитаз",
                                "target_lang_sense_clarification": "сантехническое устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d19b8e86-63d0-326e-a759-cb1266a3ff2a",
                    "sense_definition": "W dawnym języku urzędowym: dowolne wyodrębnione pomieszczenie w budynku, na przykład pokój lub kuchnia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "room_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkanie składało się z trzech widnych <w>ubikacji</w> oraz przedpokoju.",
                            "target_lang_translations": {
                                "ru": "Квартира состояла из трех светлых <w>помещений</w> и прихожей."
                            }
                        },
                        {
                            "text": "W starym akcie notarialnym kuchnię opisano jako <w>ubikację</w> gospodarczą.",
                            "target_lang_translations": {
                                "ru": "В старом нотариальном акте кухня была описана как хозяйственное <w>помещение</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomieszczenie",
                        "izba",
                        "pokój"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane dawniej w terminologii budowlanej i urzędowej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В старом официальном языке: любое отдельное помещение в здании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помещение",
                                "target_lang_sense_clarification": "комната, пространство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

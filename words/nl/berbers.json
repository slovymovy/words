{
    "word_family": [
        "Berber"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "134ac091-9684-3805-9257-72d21def4944",
                    "sense_definition": "Met betrekking tot de Berbers, hun taal of hun cultuur.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij kocht een prachtig <w>Berbers</w> tapijt voor in de woonkamer.",
                            "target_lang_translations": {
                                "ru": "Она купила красивый <w>берберский</w> ковер для гостиной."
                            }
                        },
                        {
                            "text": "De <w>Berberse</w> cultuur heeft een lange en rijke geschiedenis in Noord-Afrika.",
                            "target_lang_translations": {
                                "ru": "<w>Берберская</w> культура имеет долгую и богатую историю в Северной Африке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Berbers tapijt",
                        "Berberse taal",
                        "Berberse cultuur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к берберам, их языку или культуре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "берберский",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "78671c79-5440-3fe1-92ac-a880f35ee676",
                    "sense_definition": "De taal die door de Berbers in Noord-Afrika wordt gesproken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Mijn buurman spreekt vloeiend <w>Berbers</w> en Arabisch.",
                            "target_lang_translations": {
                                "ru": "Мой сосед свободно говорит на <w>берберском</w> и арабском."
                            }
                        },
                        {
                            "text": "Het <w>Berbers</w> wordt in verschillende dialecten gesproken in Marokko en Algerije.",
                            "target_lang_translations": {
                                "ru": "На <w>берберском</w> говорят на различных диалектах в Марокко и Алжире."
                            }
                        }
                    ],
                    "synonyms": [
                        "Tamazight"
                    ],
                    "common_phrases": [
                        "in het Berbers",
                        "Berbers spreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Язык, на котором говорят берберы в Северной Африке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "берберский язык",
                                "target_lang_sense_clarification": "язык"
                            },
                            {
                                "target_lang_word": "берберский",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "61667dc4-7333-340c-8148-4b91adcc7a80",
                    "sense_definition": "Een bevolkingsgroep die oorspronkelijk in Noord-Afrika leeft.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>Berbers</w> zijn de inheemse bewoners van Noord-Afrika.",
                            "target_lang_translations": {
                                "ru": "<w>Берберы</w> — коренные жители Северной Африки."
                            }
                        },
                        {
                            "text": "Veel <w>Berbers</w> hebben hun eigen tradities en gebruiken behouden.",
                            "target_lang_translations": {
                                "ru": "Многие <w>берберы</w> сохранили свои традиции и обычаи."
                            }
                        }
                    ],
                    "synonyms": [
                        "Imazighen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа народов, коренное население Северной Африки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "берберы",
                                "target_lang_sense_clarification": "народ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25ef816a-cddf-3524-94d0-da11cb8a23c8",
                    "sense_definition": "Meervoud van <w>Berber</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb gisteren met twee <w>Berbers</w> gesproken over hun reis.",
                            "target_lang_translations": {
                                "ru": "Вчера я говорил с двумя <w>берберами</w> об их путешествии."
                            }
                        },
                        {
                            "text": "Er wonen veel <w>Berbers</w> in deze regio van het land.",
                            "target_lang_translations": {
                                "ru": "В этом регионе страны живет много <w>берберов</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, принадлежащие к народу берберов (множественное число)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "берберы",
                                "target_lang_sense_clarification": "люди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13c1b900-c70d-309c-aa40-f911c685500d",
                    "sense_definition": "Meervoud van <w>berber</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rugs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de winkel lagen stapels handgeknoopte <w>berbers</w>.",
                            "target_lang_translations": {
                                "ru": "В магазине лежали стопки <w>берберских ковров</w> ручной работы."
                            }
                        },
                        {
                            "text": "Onze oude <w>berbers</w> moeten nodig gereinigd worden.",
                            "target_lang_translations": {
                                "ru": "Наши старые <w>берберские ковры</w> нужно срочно почистить."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ковры в берберском стиле (обычно светлые с темным узором и высоким ворсом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "берберские ковры",
                                "target_lang_sense_clarification": "тип ковров"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
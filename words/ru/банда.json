    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d68f0331-5119-32b6-a2ab-142f3eadf5ad",
                    "sense_definition": "Организованная группа людей, объединившихся для совершения преступлений.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "criminal_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиции удалось обезвредить опасную <w>банду</w>.",
                            "target_lang_translations": {
                                "en": "The police managed to neutralize a dangerous <w>gang</w>."
                            }
                        },
                        {
                            "text": "Эта <w>банда</w> грабила банки по всей стране.",
                            "target_lang_translations": {
                                "en": "This <w>gang</w> robbed banks all over the country."
                            }
                        }
                    ],
                    "synonyms": [
                        "шайка",
                        "ватага"
                    ],
                    "common_phrases": [
                        "главарь банды",
                        "вооружённая банда",
                        "член банды"
                    ],
                    "target_lang_definitions": {
                        "en": "An organized group of people united to commit crimes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gang",
                                "target_lang_sense_clarification": "criminal group"
                            },
                            {
                                "target_lang_word": "mob",
                                "target_lang_sense_clarification": "organized crime group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "652c72eb-e4bf-3f5f-a875-e65ceeb8c227",
                    "sense_definition": "Музыкальная группа, коллектив музыкантов (обычно исполняющих рок, джаз или популярную музыку).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "music_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша <w>банда</w> репетирует в гараже каждый вечер.",
                            "target_lang_translations": {
                                "en": "Our <w>band</w> rehearses in the garage every evening."
                            }
                        },
                        {
                            "text": "На фестивале выступала известная рок-<w>банда</w>.",
                            "target_lang_translations": {
                                "en": "A famous rock <w>band</w> performed at the festival."
                            }
                        }
                    ],
                    "synonyms": [
                        "бэнд",
                        "бенд"
                    ],
                    "common_phrases": [
                        "собрать банду",
                        "играть в банде"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "musicians"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A musical group or collective (usually performing rock, jazz, or pop music)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "band",
                                "target_lang_sense_clarification": "musical group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed9b6b11-7296-31d2-a15f-ce6aa9561d0d",
                    "sense_definition": "Вооружённый отряд, обычно незаконный или принадлежащий противнику (часто с негативным оттенком).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В лесах скрывались остатки разбитой <w>банды</w>.",
                            "target_lang_translations": {
                                "en": "The remnants of a defeated <w>gang</w> were hiding in the forests."
                            }
                        },
                        {
                            "text": "Партизаны вступили в бой с вражеской <w>бандой</w>.",
                            "target_lang_translations": {
                                "en": "The partisans engaged in battle with an enemy <w>gang</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "фашистские банды"
                    ],
                    "target_lang_definitions": {
                        "en": "An armed detachment, usually illegal or belonging to the enemy."

            "senses": [
                {
                    "sense_id": "e359ea00-c269-396d-a078-a517a9f6d0b2",
                    "sense_definition": "Zonder rook; geen rook producerend.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit zijn gelukkig <w>rookloze</w> kaarsen die niet walmen.",
                            "target_lang_translations": {
                                "en": "Fortunately, these are <w>smokeless</w> candles that do not give off soot.",
                                "ru": "К счастью, это <w>бездымные</w> свечи, которые не коптят."
                            }
                        },
                        {
                            "text": "Hij gebruikt <w>rookloos</w> buskruit voor zijn historische geweer.",
                            "target_lang_translations": {
                                "en": "He uses <w>smokeless</w> gunpowder for his historical rifle.",
                                "ru": "Он использует <w>бездымный</w> порох для своей старинной винтовки."
                            }
                        }
                    ],
                    "antonyms": [
                        "rokerig"
                    ],
                    "common_phrases": [
                        "rookloos buskruit",
                        "rookloos vuur"
                    ],
                    "target_lang_definitions": {
                        "en": "Without smoke; producing no smoke.",
                        "ru": "Без дыма; не производящий дыма."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smokeless",
                                "target_lang_sense_clarification": "producing no smoke"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бездымный",
                                "target_lang_sense_clarification": "не выделяющий дыма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94834c33-335b-34f8-af34-3066340717f7",
                    "sense_definition": "Waar niet gerookt wordt of mag worden; vrij van tabaksrook.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "environment_policy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij gingen op een <w>rookloze</w> vakantie waar niemand mocht roken.",
                            "target_lang_translations": {
                                "en": "We went on a <w>smoke-free</w> holiday where no one was allowed to smoke.",
                                "ru": "Мы отправились в <w>некурящий</w> отпуск, где никому не разрешалось курить."
                            }
                        },
                        {
                            "text": "De overheid streeft naar een <w>rookloze</w> generatie in de toekomst.",
                            "target_lang_translations": {
                                "en": "The government is striving for a <w>smoke-free</w> generation in the future.",
                                "ru": "Правительство стремится к <w>некурящему</w> поколению в будущем."
                            }
                        }
                    ],
                    "synonyms": [
                        "rookvrij"
                    ],
                    "common_phrases": [
                        "rookloze generatie",
                        "rookloze werkplek"
                    ],
                    "target_lang_definitions": {
                        "en": "Where smoking is not done or allowed; free of tobacco smoke.",
                        "ru": "Где не курят или курение запрещено; свободный от табачного дыма."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smoke-free",
                                "target_lang_sense_clarification": "environment where smoking is prohibited"
                            },
                            {
                                "target_lang_word": "smokeless",
                                "target_lang_sense_clarification": "free of tobacco smoke"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "свободный от табачного дыма",
                                "target_lang_sense_clarification": "где запрещено курение"
                            },
                            {

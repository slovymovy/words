            "senses": [
                {
                    "sense_id": "43729307-ed8e-3b57-85f2-ec196fa0440b",
                    "sense_definition": "Mieć określoną cenę, wymagać zapłaty pieniężnej za towar lub usługę",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cost_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ile <kosztuje> bilet na pociąg do Warszawy?",
                            "target_lang_translations": {
                                "ru": "Сколько <стоит> билет на поезд до Варшавы?"
                            }
                        },
                        {
                            "text": "Ten nowy samochód musiał dużo <kosztować>.",
                            "target_lang_translations": {
                                "ru": "Этот новый автомобиль, должно быть, много <стоил>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ile to kosztuje",
                        "dużo kosztować",
                        "mało kosztować",
                        "nic nie kosztować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь определенную цену, требовать денежной оплаты за товар или услугу"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоить",
                                "target_lang_sense_clarification": "иметь цену"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5f37347-54b5-315c-afbb-65dfae980b44",
                    "sense_definition": "Wymagać poświęcenia, wysiłku lub utraty czegoś cennego (np. zdrowia, czasu, nerwów)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cost_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zdobycie tego medalu <kosztowało> go wiele lat ciężkich treningów.",
                            "target_lang_translations": {
                                "ru": "Завоевание этой медали <стоило> ему многих лет тяжелых тренировок."
                            }
                        },
                        {
                            "text": "Ta pomyłka może nas <kosztować> utratę ważnego klienta.",
                            "target_lang_translations": {
                                "ru": "Эта ошибка может <стоить> нам потери важного клиента."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drogo kosztować",
                        "kosztować wiele zdrowia",
                        "kosztować dużo nerwów",
                        "kosztować życia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требовать жертв, усилий или потери чего-то ценного (например, здоровья, времени)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоить",
                                "target_lang_sense_clarification": "требовать жертв или усилий"
                            },
                            {
                                "target_lang_word": "обходиться",
                                "target_lang_sense_clarification": "дорого даваться"
                            }

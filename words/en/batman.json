            "pos": "name",
            "senses": [
                {
                    "sense_id": "d125bda3-9c90-38b9-98cc-09ebb1ddb3ab",
                    "sense_definition": "A fictional superhero known for his bat-themed costume, high intellect, and use of advanced gadgets to fight crime.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hero_context",
                    "name_type": "fictional_name",
                    "examples": [
                        {
                            "text": "Every child at the party wanted to dress up as <w>Batman</w>.",
                            "target_lang_translations": {
                                "ru": "Каждый ребенок на вечеринке хотел нарядиться <w>Бэтменом</w>."
                            }
                        },
                        {
                            "text": "He has a large collection of <w>Batman</w> comics in his room.",
                            "target_lang_translations": {
                                "ru": "У него в комнате большая коллекция комиксов о <w>Бэтмене</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Batman and Robin",
                        "I'm Batman"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вымышленный супергерой, известный своим костюмом в стиле летучей мыши, высоким интеллектом и использованием высокотехнологичных гаджетов для борьбы с преступностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бэтмен",
                                "target_lang_sense_clarification": "супергерой"
                            }
                        ]
                    }
                },
                {

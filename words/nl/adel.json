            "senses": [
                {
                    "sense_id": "62d5f1dd-aae7-34b6-9b1e-2d50b1b470b7",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>adelen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>adel</w> hem vanwege zijn uitzonderlijke verdiensten voor het land.",
                            "target_lang_translations": {
                                "en": "I <w>ennoble</w> him because of his exceptional merits for the country.",
                                "ru": "Я <w>возвожу</w> его <w>в дворянство</w> за его исключительные заслуги перед страной."
                            }
                        },
                        {
                            "text": "Als vorst <w>adel</w> ik slechts zelden nieuwe personen.",
                            "target_lang_translations": {
                                "en": "As a monarch, I rarely <w>ennoble</w> new people.",
                                "ru": "Как монарх, я крайне редко <w>возвожу в дворянство</w> новых людей."
                            }
                        }
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "44d1a2d2-d813-3fa7-973c-a218b973e99b",
                    "sense_definition": "Een rechthoekig stuk stof, meestal van katoen, dat op een matras wordt gelegd of waaronder men slaapt",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "bedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet de <w>lakens</w> van mijn bed vandaag verschonen.",
                            "target_lang_translations": {
                                "en": "I need to change the <w>sheets</w> on my bed today.",
                                "ru": "Мне нужно сегодня сменить <w>простыни</w> на кровати."
                            }
                        },
                        {
                            "text": "Het kind kroop lekker warm onder het <w>laken</w>.",
                            "target_lang_translations": {
                                "en": "The child crawled snugly under the <w>sheet</w>.",
                                "ru": "Ребенок уютно устроился под <w>простыней</w>."
                            }
                        },
                        {
                            "text": "In dit huis is zij degene die de <w>lakens</w> uitdeelt.",
                            "target_lang_translations": {
                                "en": "In this house, she is the one who calls the <w>shots</w>.",
                                "ru": "В этом доме она всем <w>заправляет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "beddenlaken"
                    ],
                    "common_phrases": [
                        "de lakens uitdelen",
                        "onder de lakens",
                        "witte lakens",
                        "tussen de lakens"
                    ],
                    "target_lang_definitions": {
                        "en": "A rectangular piece of fabric, usually cotton, placed on a mattress or used as a covering while sleeping",
                        "ru": "Прямоугольный кусок ткани, обычно из хлопка, который кладут на матрас или под которым спят"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sheet",
                                "target_lang_sense_clarification": "bedding"
                            },
                            {
                                "target_lang_word": "bed sheet",
                                "target_lang_sense_clarification": "bedding"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "простыня",
                                "target_lang_sense_clarification": "постельное белье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4c4fdff-3655-3a1b-a834-d44165b600fa",
                    "sense_definition": "Een geweven en daarna vervilte wollen stof, vroeger veel gebruikt voor kleding en nu nog voor biljarttafels",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "textile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het biljart in het café is bedekt met groen <w>laken</w>.",
                            "target_lang_translations": {
                                "en": "The billiard table in the pub is covered with green <w>baize</w>.",
                                "ru": "Бильярдный стол в кафе покрыт зеленым <w>сукном</w>."
                            }
                        },
                        {
                            "text": "In de middeleeuwen was de handel in <w>laken</w> erg belangrijk voor de stad.",
                            "target_lang_translations": {
                                "en": "In the Middle Ages, the trade in <w>broadcloth</w> was very important for the city.",
                                "ru": "В Средние века торговля <w>сукном</w> была очень важна для города."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A woven and subsequently felted woolen fabric, historically used for clothing and still used for billiard tables",
                        "ru": "Плотная шерстяная ткань с ворсом, исторически использовавшаяся для одежды, а ныне — для покрытия бильярдных столов"
                    },
                    "translations": {
                        "en": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bdd9d53e-7fdc-38cf-b210-0bdfa923e9e2",
                    "sense_definition": "Met de kenmerken van een wereldstad of een groot verstedelijkt gebied.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "urban",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stad heeft de laatste jaren een echt <w>metropolitaans</w> karakter gekregen.",
                            "target_lang_translations": {
                                "ru": "За последние годы город приобрел настоящий характер <w>мегаполиса</w>.",
                                "en": "The city has acquired a truly <w>metropolitan</w> character in recent years."
                            }
                        },
                        {
                            "text": "Zij wonen en werken in het drukke <w>metropolitaans</w> gebied rondom de hoofdstad.",
                            "target_lang_translations": {
                                "ru": "Они живут и работают в оживленном <w>столичном</w> регионе вокруг столицы.",
                                "en": "They live and work in the busy <w>metropolitan</w> area around the capital."
                            }
                        }
                    ],
                    "synonyms": [
                        "grootstedelijk"
                    ],
                    "antonyms": [
                        "kleinsteeds",
                        "landelijk",
                        "provinciaal"
                    ],
                    "common_phrases": [
                        "metropolitaans gebied",
                        "metropolitaanse regio",
                        "metropolitaans karakter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий чертами мегаполиса или крупной городской агломерации.",
                        "en": "Possessing the characteristics of a metropolis or a large urban area."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крупногородской",
                                "target_lang_sense_clarification": "характерный для большого города"
                            },
                            {
                                "target_lang_word": "мегаполисный",
                                "target_lang_sense_clarification": "относящийся к мегаполису"
                            },
                            {
                                "target_lang_word": "столичный",
                                "target_lang_sense_clarification": "в значении 'присущий столице/мегаполису'"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "metropolitan",
                                "target_lang_sense_clarification": "relating to a large city"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "796bc3b6-ee1d-3b12-a06f-e9f97485ac5c",
                    "sense_definition": "Betrekking hebbend op een aartsbisschop of een kerkprovincie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>metropolitaans</w> kapittel kwam bijeen in de historische kathedraal.",
                            "target_lang_translations": {
                                "ru": "<w>Митрополичий</w> капитул собрался в историческом соборе.",
                                "en": "The <w>metropolitan</w> chapter met in the historic cathedral."
                            }
                        },
                        {
                            "text": "Deze beslissing werd genomen op <w>metropolitaans</w> niveau binnen de kerkelijke hiërarchie.",
                            "target_lang_translations": {
                                "ru": "Это решение было принято на <w>митрополичьем</w> уровне в церковной иерархии.",
                                "en": "This decision was made at the <w>metropolitan</w> level within the church hierarchy."
                            }
                        }
                    ],
                    "synonyms": [
                        "aartsbisschoppelijk"
                    ],
                    "common_phrases": [
                        "metropolitaans kapittel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к митрополиту или церковной провинции.",
                        "en": "Relating to a metropolitan bishop or an ecclesiastical province."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "митрополичий",
                                "target_lang_sense_clarification": "относящийся к сану митрополита"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "metropolitan",
                                "target_lang_sense_clarification": "ecclesiastical title or rank"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
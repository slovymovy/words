            "senses": [
                {
                    "sense_id": "54f092d0-4f68-3bbf-a50b-e5e8716500d5",
                    "sense_definition": "Провести какое-либо определённое время, занимаясь рытьём земли или какой-либо медленной, кропотливой работой.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <прокопались> в огороде до самого вечера, сажая картошку.",
                            "target_lang_translations": {
                                "en": "We <dug around> in the garden until the very evening, planting potatoes."
                            }
                        },
                        {
                            "text": "Отец <прокопался> с ремонтом старого радиоприёмника всё утро.",
                            "target_lang_translations": {
                                "en": "Father <tinkered> with the repair of the old radio all morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "провозиться",
                        "проковыряться"
                    ],
                    "common_phrases": [
                        "долго прокопаться",
                        "прокопаться целый день"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "в значении «медленно заниматься чем-либо»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To spend a certain amount of time digging or doing some slow, painstaking work."
                    },

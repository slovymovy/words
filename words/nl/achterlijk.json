                {
                    "sense_id": "900dccb9-5bbe-341e-a64a-e6857bc34836",
                    "sense_definition": "Heel erg dom, idioot of belachelijk.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stupid_ridiculous",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik vind het echt een <w>achterlijk</w> idee om met dit slechte weer te gaan zwemmen.",
                            "target_lang_translations": {
                                "ru": "Я считаю, что это действительно <w>идиотская</w> идея — идти плавать в такую плохую погоду."
                            }
                        },
                        {
                            "text": "Doe niet zo <w>achterlijk</w> en luister gewoon even naar wat ik te zeggen heb.",
                            "target_lang_translations": {
                                "ru": "Не веди себя так <w>по-дурацки</w> и просто послушай, что я хочу сказать."
                            }
                        }
                    ],
                    "synonyms": [
                        "dom",
                        "idioot",
                        "stom",
                        "belachelijk",
                        "dwaas"
                    ],
                    "antonyms": [
                        "slim",
                        "intelligent",
                        "verstandig"
                    ],
                    "common_phrases": [
                        "doe niet zo achterlijk",
                        "een achterlijk idee"
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2489329f-623b-355b-bb8e-98b560f9e311",
                    "sense_definition": "Bescherming zoeken tegen slechte weersomstandigheden (zoals regen) of gevaar; zich op een veilige plek bevinden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shelter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moesten onder een afdakje <w>schuilen</w> voor de plotselinge regenbui.",
                            "target_lang_translations": {
                                "en": "We had to <w>take shelter</w> under a small canopy from the sudden downpour."
                            }
                        },
                        {
                            "text": "De bange kat <w>school</w> onder de auto toen het vuurwerk begon.",
                            "target_lang_translations": {
                                "en": "The scared cat <w>took shelter</w> under the car when the fireworks started."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich verbergen",
                        "beschutting zoeken"
                    ],
                    "antonyms": [
                        "tevoorschijn komen"
                    ],
                    "common_phrases": [
                        "schuilen voor de regen",
                        "ergens gaan schuilen"
                    ],
                    "target_lang_definitions": {
                        "en": "To seek protection against bad weather conditions (such as rain) or danger; to be in a safe place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take shelter",
                                "target_lang_sense_clarification": "seek protection from weather or danger"
                            },
                            {
                                "target_lang_word": "seek cover",
                                "target_lang_sense_clarification": "find a protected place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f91e799-d152-34e6-b064-b1d4264bcd82",
                    "sense_definition": "Onzichtbaar aanwezig zijn; verborgen zitten in iets of iemand (vaak figuurlijk gebruikt).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hidden_presence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In die onschuldige opmerking <w>schuilt</w> een diepere betekenis.",
                            "target_lang_translations": {
                                "en": "A deeper meaning <w>lies hidden</w> in that innocent remark."
                            }
                        },
                        {
                            "text": "Er <w>schuilt</w> een groot gevaar in het nemen van onnodige risico's.",
                            "target_lang_translations": {
                                "en": "Great danger <w>lurks</w> in taking unnecessary risks."
                            }
                        }
                    ],
                    "synonyms": [
                        "verborgen zijn",
                        "zitten"
                    ],
                    "antonyms": [
                        "zich openbaren",
                        "blijken"
                    ],
                    "common_phrases": [
                        "er schuilt gevaar in",
                        "er schuilt waarheid in"
                    ],
                    "target_lang_definitions": {
                        "en": "To be invisibly present; to exist hidden within something or someone (often used figuratively)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lie hidden",
                                "target_lang_sense_clarification": "exist underneath the surface"
                            },
                            {
                                "target_lang_word": "lurk",
                                "target_lang_sense_clarification": "be present latently, often implying danger"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
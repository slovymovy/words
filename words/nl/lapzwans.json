            "senses": [
                {
                    "sense_id": "91e1aa60-df68-3352-847b-63877995228a",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>lapzwansen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>lapzwans</w> vandaag een beetje omdat ik gisteren heel hard gewerkt heb.",
                            "target_lang_translations": {
                                "en": "I am <w>loafing around</w> a bit today because I worked very hard yesterday.",
                                "ru": "Я сегодня немного <w>бездельничаю</w>, потому что вчера очень много работал."
                            }
                        },
                        {
                            "text": "Als ik vrij ben, <w>lapzwans</w> ik het liefst de hele middag in de tuin.",
                            "target_lang_translations": {
                                "en": "When I am off, I prefer to <w>laze about</w> in the garden all afternoon.",
                                "ru": "Когда я свободен, я предпочитаю <w>лодырничать</w> весь день в саду."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "conjugation"
                        }
                    ],
                    "target_lang_definitions": {

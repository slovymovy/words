                    "sense_id": "3587294c-58dc-3e8d-822e-7398462aba34",
                    "sense_definition": "Dokument, wydruk lub kod uprawniający posiadacza do zniżki, rabatu lub wymiany na określony towar czy usługę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dostałem <w>kupon</w> na darmową kawę w nowej kawiarni.",
                            "target_lang_translations": {
                                "ru": "Я получил <w>купон</w> на бесплатный кофе в новой кофейне."
                            }
                        },
                        {
                            "text": "Wytnij ten <w>kupon</w> z gazety, aby otrzymać zniżkę na zakupy.",
                            "target_lang_translations": {
                                "ru": "Вырежи этот <w>купон</w> из газеты, чтобы получить скидку на покупки."
                            }
                        }
                    ],
                    "synonyms": [
                        "bon",
                        "talon",
                        "voucher"
                    ],
                    "common_phrases": [
                        "kupon rabatowy",
                        "kupon zniżkowy",
                        "kupon podarunkowy",
                        "kupon promocyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ, распечатка или код, дающий право на скидку или получение товара/услуги."
                    },
                    "translations": {

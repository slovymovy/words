                    ],
                    "synonyms": [
                        "bedrijfsmatig",
                        "commercieel"
                    ],
                    "antonyms": [
                        "privé",
                        "persoonlijk"
                    ],
                    "common_phrases": [
                        "zakelijk leider",
                        "zakelijk instinct",
                        "zakelijk contact",
                        "zakelijk verkeer"
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "eter",
        "etentje",
        "eetbaar"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ad7b353-1782-3d57-8bc3-c4fcba2cf22c",
                    "sense_definition": "Een eenvoudige eetgelegenheid, vaak informeel, waar men maaltijden kan kopen en nuttigen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "establishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben gisteren heerlijk gegeten in een gezellige <w>eterij</w> in het centrum.",
                            "target_lang_translations": {
                                "ru": "Вчера мы вкусно поели в уютном <w>кафе</w> в центре."
                            }
                        },
                        {
                            "text": "Deze <w>eterij</w> staat bekend om zijn verse en betaalbare gerechten.",
                            "target_lang_translations": {
                                "ru": "Это <w>бистро</w> известно своими свежими и доступными блюдами."
                            }
                        }
                    ],
                    "synonyms": [
                        "eetcafé",
                        "bistro",
                        "eetsalon",
                        "restaurant",
                        "cafetaria",
                        "bodega",
                        "snackbar",
                        "friture"
                    ],
                    "common_phrases": [
                        "gezellige eterij",
                        "lokale eterij"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простое заведение общественного питания, часто неформальное, где можно купить и съесть еду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кафе",
                                "target_lang_sense_clarification": "небольшое уютное заведение"
                            },
                            {
                                "target_lang_word": "закусочная",
                                "target_lang_sense_clarification": "простое место для еды"
                            },
                            {
                                "target_lang_word": "бистро",
                                "target_lang_sense_clarification": "маленький ресторанчик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75064b9d-2fa9-3c01-b0eb-e0efc2a4c4f4",
                    "sense_definition": "Zaken die men kan eten; voedsel of proviand.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bezorger zette de zak met <w>eterij</w> voor de deur en vertrok.",
                            "target_lang_translations": {
                                "ru": "Курьер поставил пакет с <w>едой</w> перед дверью и ушел."
                            }
                        },
                        {
                            "text": "Er was genoeg <w>eterij</w> in huis om de hele groep te voeden.",
                            "target_lang_translations": {
                                "ru": "В доме было достаточно <w>съестного</w>, чтобы накормить всю группу."
                            }
                        }
                    ],
                    "synonyms": [
                        "eten",
                        "voedsel",
                        "voeding",
                        "maaltijd"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что можно есть; пища или провизия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "еда",
                                "target_lang_sense_clarification": "пища в общем смысле"
                            },
                            {
                                "target_lang_word": "съестное",
                                "target_lang_sense_clarification": "разговорное, собирательное"
                            },
                            {
                                "target_lang_word": "провизия",
                                "target_lang_sense_clarification": "запасы еды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c267bba5-0989-3210-9b84-e4195ea15f4b",
                    "sense_definition": "Het voortdurend of overmatig eten; het gedoe rondom de maaltijd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na al die <w>eterij</w> tijdens het kerstdiner konden we geen pap meer zeggen.",
                            "target_lang_translations": {
                                "ru": "После всей этой <w>объедаловки</w> за рождественским ужином мы и слова вымолвить не могли."
                            }
                        },
                        {
                            "text": "De hele <w>eterij</w> duurde de hele middag en was erg vermoeiend.",
                            "target_lang_translations": {
                                "ru": "Всё это <w>застолье</w> длилось весь день и было очень утомительным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "na de eterij"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс длительного или чрезмерного приема пищи; суета вокруг еды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объедаловка",
                                "target_lang_sense_clarification": "чрезмерное употребление пищи"
                            },
                            {
                                "target_lang_word": "застолье",
                                "target_lang_sense_clarification": "длительный прием пищи в компании"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "metric",
        "diameter",
        "perimeter",
        "centimeter",
        "kilometer",
        "millimeter",
        "thermometer",
        "speedometer",
        "barometer",
        "altimeter",
        "metrology"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e972c944-fc66-30ee-9c7b-b216f00e922e",
                    "sense_definition": "The fundamental unit of length in the metric system, equal to 100 centimeters.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "unit_of_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The swimming pool is fifty <w>meters</w> long.",
                            "target_lang_translations": {
                                "ru": "Длина бассейна составляет пятьдесят <w>метров</w>."
                            }
                        },
                        {
                            "text": "He ran the one hundred <w>meters</w> in record time.",
                            "target_lang_translations": {
                                "ru": "Он пробежал сто <w>метров</w> за рекордное время."
                            }
                        }
                    ],
                    "synonyms": [
                        "metre"
                    ],
                    "common_phrases": [
                        "square meter",
                        "cubic meter",
                        "per meter"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "US spelling"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная единица длины в метрической системе, равная 100 сантиметрам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "метр",
                                "target_lang_sense_clarification": "единица измерения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8881b769-3c95-383e-bf19-d28aa466b418",
                    "sense_definition": "A device used to measure and record the quantity, degree, or rate of something, such as electricity, gas, or time.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "measuring_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to put some coins in the parking <w>meter</w> before we leave the car.",
                            "target_lang_translations": {
                                "ru": "Мне нужно опустить монеты в парковочный <w>счётчик</w>, прежде чем мы оставим машину."
                            }
                        },
                        {
                            "text": "The taxi <w>meter</w> showed a much higher fare than I expected.",
                            "target_lang_translations": {
                                "ru": "<w>Счётчик</w> такси показал гораздо более высокую плату, чем я ожидал."
                            }
                        }
                    ],
                    "synonyms": [
                        "gauge"
                    ],
                    "common_phrases": [
                        "parking meter",
                        "gas meter",
                        "water meter",
                        "smart meter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибор, используемый для измерения и регистрации количества, степени или тарифа чего-либо (например, электричества, газа или времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "счётчик",
                                "target_lang_sense_clarification": "прибор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4640688-4520-3c96-ad15-f795cfee1f9a",
                    "sense_definition": "The rhythmic arrangement of syllables in poetry or the pattern of beats in music.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rhythm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poem is written in a very strict and traditional <w>meter</w>.",
                            "target_lang_translations": {
                                "ru": "Стихотворение написано в очень строгом и традиционном <w>размере</w>."
                            }
                        },
                        {
                            "text": "Musicians must learn to play in different types of <w>meter</w> to handle complex pieces.",
                            "target_lang_translations": {
                                "ru": "Музыканты должны учиться играть в различных музыкальных <w>размерах</w>, чтобы справляться со сложными произведениями."
                            }
                        }
                    ],
                    "synonyms": [
                        "rhythm",
                        "beat",
                        "cadence"
                    ],
                    "common_phrases": [
                        "poetic meter"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "US spelling"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ритмическая структура стиха или схема тактов в музыке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размер",
                                "target_lang_sense_clarification": "стихотворный или музыкальный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "db580bd2-367d-3c7d-ae17-0756c1dcb63c",
                    "sense_definition": "To measure the quantity of something using a specific device.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "action_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The electricity usage is <w>metered</w> separately for each apartment in the building.",
                            "target_lang_translations": {
                                "ru": "Потребление электроэнергии <w>измеряется</w> отдельно для каждой квартиры в здании."
                            }
                        },
                        {
                            "text": "They decided to <w>meter</w> the water consumption to encourage conservation.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>измерять</w> потребление воды, чтобы поощрить экономию."
                            }
                        }
                    ],
                    "synonyms": [
                        "measure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Измерять количество чего-либо с помощью специального прибора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "измерять",
                                "target_lang_sense_clarification": "определять величину"
                            },
                            {
                                "target_lang_word": "учитывать",
                                "target_lang_sense_clarification": "вести учёт потребления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e0953a3-845e-393d-9697-4213f661c0c7",
                    "sense_definition": "To control or regulate the flow or amount of something being supplied.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_regulate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pump <w>meters</w> the fuel into the engine very precisely for maximum efficiency.",
                            "target_lang_translations": {
                                "ru": "Насос очень точно <w>дозирует</w> топливо в двигатель для максимальной эффективности."
                            }
                        },
                        {
                            "text": "He tried to <w>meter</w> out his energy so he could finish the long race.",
                            "target_lang_translations": {
                                "ru": "Он пытался <w>дозировать</w> свою энергию, чтобы закончить длинную гонку."
                            }
                        }
                    ],
                    "synonyms": [
                        "regulate",
                        "dose"
                    ],
                    "common_phrases": [
                        "meter out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Контролировать или регулировать поток или количество чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дозировать",
                                "target_lang_sense_clarification": "отмерять порциями"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "input",
        "output",
        "throughput"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c0996222-8676-3f68-9c44-050ad48efa0f",
                    "sense_definition": "To move something or someone into a specific place or position.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "placement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>put</w> your dirty clothes in the washing machine.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>положи</w> грязную одежду в стиральную машину."
                            }
                        },
                        {
                            "text": "She <w>put</w> the keys on the kitchen table.",
                            "target_lang_translations": {
                                "ru": "Она <w>положила</w> ключи на кухонный стол."
                            }
                        }
                    ],
                    "synonyms": [
                        "programmable UJT"
                    ],
                    "antonyms": [
                        "remove"
                    ],
                    "common_phrases": [
                        "put away",
                        "put back",
                        "put down",
                        "put on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать что-либо или кого-либо в определенное место или положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "поместить горизонтально или внутрь чего-либо (сов. вид)"
                            },
                            {
                                "target_lang_word": "поставить",
                                "target_lang_sense_clarification": "поместить вертикально или на опору (сов. вид)"
                            },
                            {
                                "target_lang_word": "класть",
                                "target_lang_sense_clarification": "помещать горизонтально (несов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e01958ad-d89e-3811-b308-69d5d40def25",
                    "sense_definition": "To cause someone or something to be in a particular state, situation, or condition.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "placement_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soft music helped <w>put</w> the baby to sleep.",
                            "target_lang_translations": {
                                "ru": "Тихая музыка помогла <w>уложить</w> ребенка спать."
                            }
                        },
                        {
                            "text": "We need to <w>put</w> these new ideas into practice immediately.",
                            "target_lang_translations": {
                                "ru": "Нам нужно немедленно <w>применить</w> эти новые идеи на практике."
                            }
                        }
                    ],
                    "synonyms": [
                        "programmable UJT"
                    ],
                    "common_phrases": [
                        "put to sleep",
                        "put in charge",
                        "put to use",
                        "put at risk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить кого-либо или что-либо в определенное состояние, ситуацию или условие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставить",
                                "target_lang_sense_clarification": "приводить в какое-либо положение или состояние"
                            },
                            {
                                "target_lang_word": "приводить",
                                "target_lang_sense_clarification": "доводить до определенного состояния"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ff9f60f-d859-3a4b-86e2-4e6b4cf70bd5",
                    "sense_definition": "To assign or attribute a quality, value, burden, or responsibility to someone or something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "placement_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should not <w>put</w> all the blame on him for the mistake.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <w>возлагать</w> на него всю вину за эту ошибку."
                            }
                        },
                        {
                            "text": "The school decides to <w>put</w> great emphasis on learning foreign languages.",
                            "target_lang_translations": {
                                "ru": "Школа решила <w>сделать</w> большой упор на изучение иностранных языков."
                            }
                        }
                    ],
                    "synonyms": [
                        "programmable UJT"
                    ],
                    "common_phrases": [
                        "put the blame",
                        "put pressure on",
                        "put trust in",
                        "put a price on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приписывать качество, ценность, бремя или ответственность кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возлагать",
                                "target_lang_sense_clarification": "назначать ответственность или вину"
                            },
                            {
                                "target_lang_word": "ставить",
                                "target_lang_sense_clarification": "делать акцент или придавать значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca2f9d19-8323-30a5-b699-82c917a20bc1",
                    "sense_definition": "To express a thought, idea, or feeling in words.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>put</w> it simply, we cannot afford to buy a new car right now.",
                            "target_lang_translations": {
                                "ru": "<w>Попросту говоря</w>, мы не можем позволить себе купить новую машину прямо сейчас."
                            }
                        },
                        {
                            "text": "I do not know how to <w>put</w> my feelings into words.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, как <w>выразить</w> свои чувства словами."
                            }
                        }
                    ],
                    "synonyms": [
                        "programmable UJT"
                    ],
                    "common_phrases": [
                        "put it simply",
                        "put it mildly",
                        "put into words",
                        "how shall I put it"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать мысль, идею или чувство словами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выражать",
                                "target_lang_sense_clarification": "передавать словами или действиями"
                            },
                            {
                                "target_lang_word": "формулировать",
                                "target_lang_sense_clarification": "облекать в словесную форму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe282048-0cf2-30a3-8e97-f07426ae1a54",
                    "sense_definition": "To present a proposal, question, or situation to someone for consideration or judgment.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "proposal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I would like to <w>put</w> a question to the guest speaker regarding his research.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>задать</w> вопрос приглашенному докладчику касательно его исследования."
                            }
                        },
                        {
                            "text": "The chairman decided to <w>put</w> the motion to a vote after the debate.",
                            "target_lang_translations": {
                                "ru": "Председатель решил <w>поставить</w> предложение на голосование после дебатов."
                            }
                        }
                    ],
                    "synonyms": [
                        "programmable UJT"
                    ],
                    "common_phrases": [
                        "put a question",
                        "put to the vote",
                        "put it to someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представлять предложение, вопрос или ситуацию кому-либо для рассмотрения или суждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставить",
                                "target_lang_sense_clarification": "выносить на обсуждение или голосование"
                            },
                            {
                                "target_lang_word": "задавать",
                                "target_lang_sense_clarification": "обращаться с вопросом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bebd97d7-7207-3b4c-a0a4-4f835f953322",
                    "sense_definition": "To estimate an amount, cost, or value at a certain level.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "estimation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police <w>put</w> the number of protesters at around five thousand.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>оценила</w> число протестующих примерно в пять тысяч."
                            }
                        },
                        {
                            "text": "Experts <w>put</w> the cost of the damage at several million dollars.",
                            "target_lang_translations": {
                                "ru": "Эксперты <w>оценили</w> стоимость ущерба в несколько миллионов долларов."
                            }
                        }
                    ],
                    "synonyms": [
                        "programmable UJT"
                    ],
                    "common_phrases": [
                        "put the cost at",
                        "put the figure at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценивать количество, стоимость или значение на определенном уровне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "определять стоимость или количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9449a4a4-ba5c-336b-9cde-e87891c0ea13",
                    "sense_definition": "To throw a heavy object, such as a shot, with a pushing motion from the shoulder.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The athlete prepared to <w>put</w> the shot with immense power.",
                            "target_lang_translations": {
                                "ru": "Спортсмен приготовился <w>толкнуть</w> ядро с огромной силой."
                            }
                        },
                        {
                            "text": "He managed to <w>put</w> the heavy ball further than any other competitor.",
                            "target_lang_translations": {
                                "ru": "Ему удалось <w>толкнуть</w> тяжелый шар дальше, чем любому другому участнику."
                            }
                        }
                    ],
                    "synonyms": [
                        "programmable UJT"
                    ],
                    "common_phrases": [
                        "put the shot"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бросать тяжелый предмет, например ядро, толкающим движением от плеча."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толкать",
                                "target_lang_sense_clarification": "спортивный термин (ядро)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "da961a45-d57b-34bc-971b-2d7a979260fc",
                    "sense_definition": "A financial contract giving the holder the right to sell an asset at a specified price by a certain date.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "finance_option",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The investor bought a <w>put</w> to protect his portfolio against a market decline.",
                            "target_lang_translations": {
                                "ru": "Инвестор купил <w>пут-опцион</w>, чтобы защитить свой портфель от спада на рынке."
                            }
                        },
                        {
                            "text": "Understanding the difference between a call and a <w>put</w> is essential for trading options.",
                            "target_lang_translations": {
                                "ru": "Понимание разницы между колл-опционом и <w>пут-опционом</w> необходимо для торговли опционами."
                            }
                        }
                    ],
                    "synonyms": [
                        "programmable UJT"
                    ],
                    "antonyms": [
                        "call"
                    ],
                    "common_phrases": [
                        "buy a put",
                        "put option"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "finance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Финансовый контракт, дающий владельцу право продать актив по определенной цене к определенной дате."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пут-опцион",
                                "target_lang_sense_clarification": "финансовый инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff27d9e1-6b72-3242-bf65-ca3066163132",
                    "sense_definition": "The act of throwing the heavy metal ball in the sport of shot put.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sports_action_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His final <w>put</w> secured him the gold medal in the championship.",
                            "target_lang_translations": {
                                "ru": "Его последний <w>толчок</w> обеспечил ему золотую медаль на чемпионате."
                            }
                        },
                        {
                            "text": "The technique for the <w>put</w> requires strength and coordination.",
                            "target_lang_translations": {
                                "ru": "Техника <w>толчка</w> требует силы и координации."
                            }
                        }
                    ],
                    "synonyms": [
                        "programmable UJT"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по метанию тяжелого металлического шара в толкании ядра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толчок",
                                "target_lang_sense_clarification": "спортивное действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
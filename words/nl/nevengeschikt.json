{
    "word_family": [
        "nevenschikken",
        "nevenschikking"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b8153217-caf5-3474-9183-975b84a3a251",
                    "sense_definition": "Op hetzelfde niveau staand, zonder dat er sprake is van een hiërarchisch verschil of gezagsverhouding.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gelijkwaardigheid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze organisatie zijn de afdelingshoofden volledig <w>nevengeschikt</w> aan elkaar.",
                            "target_lang_translations": {
                                "ru": "В этой организации руководители отделов полностью <w>равноправны</w> по отношению друг к другу."
                            }
                        },
                        {
                            "text": "De leden van het bestuur zijn <w>nevengeschikt</w> en hebben evenveel invloed op de besluitvorming.",
                            "target_lang_translations": {
                                "ru": "Члены правления <w>равноправны</w> и имеют одинаковое влияние на принятие решений."
                            }
                        }
                    ],
                    "synonyms": [
                        "gelijkwaardig"
                    ],
                    "antonyms": [
                        "ondergeschikt"
                    ],
                    "common_phrases": [
                        "nevengeschikte partijen",
                        "nevengeschikte functies"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся на одном уровне, без иерархических различий или отношений подчинения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равноправный",
                                "target_lang_sense_clarification": "о статусе или людях"
                            },
                            {
                                "target_lang_word": "равный по рангу",
                                "target_lang_sense_clarification": "об иерархии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f359f25e-aba3-394d-891a-0f578d52454e",
                    "sense_definition": "Verbonden met iets anders als een gelijkwaardig en onlosmakelijk deel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gelijkwaardigheid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De commerciële belangen zijn in dit project <w>nevengeschikt</w> aan de maatschappelijke doelen.",
                            "target_lang_translations": {
                                "ru": "Коммерческие интересы в этом проекте <w>равноценны</w> общественным целям."
                            }
                        },
                        {
                            "text": "In de zin zijn de twee hoofdzinnen <w>nevengeschikt</w> verbonden door een voegwoord.",
                            "target_lang_translations": {
                                "ru": "В предложении два главных предложения связаны <w>сочинительной связью</w> с помощью союза."
                            }
                        }
                    ],
                    "synonyms": [
                        "gelijkwaardig"
                    ],
                    "antonyms": [
                        "ondergeschikt"
                    ],
                    "common_phrases": [
                        "nevengeschikt aan",
                        "nevengeschikte doelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединенный с чем-то другим как равноценная часть; в лингвистике — сочинённый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равноценный",
                                "target_lang_sense_clarification": "о значимости или целях"
                            },
                            {
                                "target_lang_word": "сочинённый",
                                "target_lang_sense_clarification": "грамматический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "122edfc9-81d7-36f2-89ef-521f239fa672",
                    "sense_definition": "Een slim plan of truc om iemand te misleiden of een doel te bereiken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "trick",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het houten paard was een <w>list</w> om Troje binnen te komen.",
                            "target_lang_translations": {
                                "ru": "Деревянный конь был <w>хитростью</w>, чтобы проникнуть в Трою."
                            }
                        },
                        {
                            "text": "De dief gebruikte een slimme <w>list</w> om de bewakers af te leiden.",
                            "target_lang_translations": {
                                "ru": "Вор использовал умную <w>уловку</w>, чтобы отвлечь охранников."
                            }
                        }
                    ],
                    "synonyms": [
                        "misleiding",
                        "slimmigheid"
                    ],
                    "common_phrases": [
                        "list en bedrog",
                        "een list verzinnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хитрый план или прием, используемый для обмана кого-либо или достижения цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уловка",
                                "target_lang_sense_clarification": "конкретный прием или трюк"
                            },
                            {
                                "target_lang_word": "хитрость",
                                "target_lang_sense_clarification": "поступок или план"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce25c731-b5cc-340f-9e1d-c1d744b3dfdb",
                    "sense_definition": "Het vermogen om slimme plannen te bedenken; sluwheid.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cunning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met veel <w>list</w> wist de vos aan de jagers te ontsnappen.",
                            "target_lang_translations": {
                                "ru": "Благодаря большой <w>хитрости</w> лисе удалось ускользнуть от охотников."
                            }
                        },
                        {
                            "text": "Zijn overwinning was meer te danken aan <w>list</w> dan aan brute kracht.",
                            "target_lang_translations": {
                                "ru": "Его победа была заслугой скорее <w>хитрости</w>, чем грубой силы."
                            }
                        }
                    ],
                    "synonyms": [
                        "sluwheid",
                        "slimheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность придумывать умные планы; изворотливость и лукавство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хитрость",
                                "target_lang_sense_clarification": "свойство характера"
                            },
                            {
                                "target_lang_word": "изворотливость",
                                "target_lang_sense_clarification": "умение выходить из трудных ситуаций"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
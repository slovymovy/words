{
    "word_family": [
        "overleving",
        "overlevende",
        "overleefbaar"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "70329907-426a-3c0a-b37f-c098fa8c5533",
                    "sense_definition": "In leven blijven ondanks moeilijke omstandigheden, gevaar, ziekte of een ramp.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stay_alive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zonder water kun je niet lang <w>overleven</w> in de woestijn.",
                            "target_lang_translations": {
                                "en": "You cannot <w>survive</w> long in the desert without water.",
                                "ru": "Без воды в пустыне долго не <w>выжить</w>."
                            }
                        },
                        {
                            "text": "Twintig mensen <w>overleefden</w> het zware ongeluk op de snelweg.",
                            "target_lang_translations": {
                                "en": "Twenty people <w>survived</w> the severe accident on the highway.",
                                "ru": "Двадцать человек <w>выжили</w> в тяжелой аварии на шоссе."
                            }
                        }
                    ],
                    "synonyms": [
                        "overleving"
                    ],
                    "antonyms": [
                        "sterven",
                        "omkomen",
                        "bezwijken"
                    ],
                    "common_phrases": [
                        "de oorlog overleven",
                        "nauwelijks overleven",
                        "een ramp overleven"
                    ],
                    "target_lang_definitions": {
                        "en": "To remain alive despite difficult circumstances, danger, illness, or a disaster.",
                        "ru": "Оставаться в живых, несмотря на тяжелые условия, опасность, болезнь или катастрофу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "survive",
                                "target_lang_sense_clarification": "stay alive"
                            },
                            {
                                "target_lang_word": "live through",
                                "target_lang_sense_clarification": "endure and survive"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выживать",
                                "target_lang_sense_clarification": "оставаться в живых"
                            },
                            {
                                "target_lang_word": "выжить",
                                "target_lang_sense_clarification": "сохранить жизнь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57ac937e-cac7-337d-a281-2ca8d4f152b1",
                    "sense_definition": "Langer leven dan iemand anders, meestal een familielid of partner.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "outlive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is erg verdrietig als ouders hun eigen kinderen <w>overleven</w>.",
                            "target_lang_translations": {
                                "en": "It is very sad when parents <w>outlive</w> their own children.",
                                "ru": "Очень грустно, когда родители <w>переживают</w> своих собственных детей."
                            }
                        },
                        {
                            "text": "Zij heeft haar man uiteindelijk tien jaar <w>overleefd</w>.",
                            "target_lang_translations": {
                                "en": "She eventually <w>outlived</w> her husband by ten years.",
                                "ru": "В итоге она <w>пережила</w> своего мужа на десять лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zijn vrouw overleven",
                        "zijn ouders overleven"
                    ],
                    "target_lang_definitions": {
                        "en": "To live longer than someone else, usually a family member or partner.",
                        "ru": "Прожить дольше кого-то другого, обычно родственника или партнера."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "outlive",
                                "target_lang_sense_clarification": "live longer than"
                            },
                            {
                                "target_lang_word": "survive",
                                "target_lang_sense_clarification": "live after someone's death"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пережить",
                                "target_lang_sense_clarification": "прожить дольше кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e5881c9-1a5b-31a7-b885-98ec6a32c418",
                    "sense_definition": "Blijven bestaan of functioneren, vaak gezegd van organisaties, ideeën of apparaten.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "persist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het kleine bedrijf zal de economische crisis waarschijnlijk niet <w>overleven</w>.",
                            "target_lang_translations": {
                                "en": "The small company will probably not <w>survive</w> the economic crisis.",
                                "ru": "Маленькая компания, вероятно, не <w>переживет</w> экономический кризис."
                            }
                        },
                        {
                            "text": "Mijn oude auto heeft de strenge winter gelukkig <w>overleefd</w>.",
                            "target_lang_translations": {
                                "en": "Luckily, my old car <w>survived</w> the harsh winter.",
                                "ru": "Моя старая машина, к счастью, <w>пережила</w> суровую зиму."
                            }
                        }
                    ],
                    "synonyms": [
                        "overleving",
                        "overleving"
                    ],
                    "antonyms": [
                        "verdwijnen",
                        "kapotgaan"
                    ],
                    "common_phrases": [
                        "de crisis overleven",
                        "de winter overleven"
                    ],
                    "target_lang_definitions": {
                        "en": "To continue to exist or function, often said of organizations, ideas, or devices.",
                        "ru": "Продолжать существовать или функционировать, часто говорится об организациях, идеях или устройствах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "survive",
                                "target_lang_sense_clarification": "continue to exist"
                            },
                            {
                                "target_lang_word": "last",
                                "target_lang_sense_clarification": "endure over time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пережить",
                                "target_lang_sense_clarification": "справиться с трудностями"
                            },
                            {
                                "target_lang_word": "уцелеть",
                                "target_lang_sense_clarification": "сохраниться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa2d09b5-d774-3e81-ba91-039ca8578141",
                    "sense_definition": "Het feit dat iemand of iets in leven blijft of blijft bestaan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "survival_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Schoon drinkwater is noodzakelijk voor het <w>overleven</w> van de mens.",
                            "target_lang_translations": {
                                "en": "Clean drinking water is necessary for human <w>survival</w>.",
                                "ru": "Чистая питьевая вода необходима для <w>выживания</w> человека."
                            }
                        },
                        {
                            "text": "De artsen vechten voor het <w>overleven</w> van de patiënt.",
                            "target_lang_translations": {
                                "en": "The doctors are fighting for the patient's <w>survival</w>.",
                                "ru": "Врачи борются за <w>выживание</w> пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "overleving"
                    ],
                    "antonyms": [
                        "ondergang",
                        "dood"
                    ],
                    "common_phrases": [
                        "strijd om te overleven",
                        "kans op overleven"
                    ],
                    "target_lang_definitions": {
                        "en": "The fact that someone or something remains alive or continues to exist.",
                        "ru": "Факт того, что кто-то или что-то остается в живых или продолжает существовать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "survival",
                                "target_lang_sense_clarification": "state of surviving"
                            },
                            {
                                "target_lang_word": "surviving",
                                "target_lang_sense_clarification": "act of staying alive"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выживание",
                                "target_lang_sense_clarification": "процесс сохранения жизни"
                            }
                        ]
                    }
                }
            ]
        }
    ]

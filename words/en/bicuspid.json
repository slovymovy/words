    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1681a827-43ba-3bb3-8c0c-a3da45ddd9cb",
                    "sense_definition": "A tooth with two cusps located between the canine and the molars.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "teeth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dentist said I might need to have a <w>bicuspid</w> removed before getting braces.",
                            "target_lang_translations": {
                                "ru": "Стоматолог сказал, что мне, возможно, придется удалить <w>премоляр</w> перед установкой брекетов."
                            }
                        },
                        {
                            "text": "Each <w>bicuspid</w> helps to crush and grind food during chewing.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>малый коренной зуб</w> помогает раздавливать и перемалывать пищу при жевании."
                            }
                        }
                    ],
                    "synonyms": [
                        "premolar"
                    ],
                    "common_phrases": [
                        "first bicuspid",
                        "second bicuspid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зуб с двумя бугорками, расположенный между клыком и молярами"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "премоляр",
                                "target_lang_sense_clarification": "стоматологический термин"
                            },
                            {
                                "target_lang_word": "малый коренной зуб",
                                "target_lang_sense_clarification": "общеупотребительное название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "10fbc50d-77df-379f-8103-ac37861d5aa0",
                    "sense_definition": "Having two points or prominences, such as on a leaf, tooth, or heart valve.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The surgeon repaired the patient's <w>bicuspid</w> aortic valve to improve blood flow.",
                            "target_lang_translations": {
                                "ru": "Хирург восстановил <w>двустворчатый</w> аортальный клапан пациента, чтобы улучшить кровоток."
                            }
                        },
                        {
                            "text": "In botany, a <w>bicuspid</w> leaf ends in two distinct sharp points.",
                            "target_lang_translations": {
                                "ru": "В ботанике <w>двуострый</w> лист заканчивается двумя отчетливыми острыми кончиками."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bicuspid valve"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий два острия или выступа (например, о клапане сердца или листе)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двустворчатый",
                                "target_lang_sense_clarification": "о клапанах сердца"
                            },
                            {
                                "target_lang_word": "двуострый",
                                "target_lang_sense_clarification": "о форме или листьях"
                            },
                            {
                                "target_lang_word": "бикуспидальный",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

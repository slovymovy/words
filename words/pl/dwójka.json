                    }
                },
                {
                    "sense_id": "55ca9781-9a92-346b-8679-403758ecb09e",
                    "sense_definition": "Karta do gry lub ścianka kostki mająca dwa oczka.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej grze <w>dwójka</w> jest najniższą możliwą kartą.",
                            "target_lang_translations": {
                                "ru": "В этой игре <w>двойка</w> — самая младшая карта."
                            }
                        },
                        {
                            "text": "Rzucił kostką i niestety wypadła mu tylko <w>dwójka</w>.",
                            "target_lang_translations": {
                                "ru": "Он бросил кубик, и, к сожалению, выпала только <w>двойка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dwójka trefl",
                        "dwójka kier",
                        "wyrzucić dwójkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игральная карта или грань кубика с двумя очками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двойка",

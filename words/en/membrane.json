    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00e69636-0303-322d-add1-b5b4e1a6753d",
                    "sense_definition": "The thin outer layer that surrounds a cell or organelle, controlling the movement of substances in and out.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "biological_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nutrients must pass through the cell <w>membrane</w> to provide energy."
                        },
                        {
                            "text": "The plasma <w>membrane</w> protects the interior of the cell from the outside environment."
                        }
                    ],
                    "synonyms": [
                        "wall",
                        "envelope",
                        "boundary"
                    ],
                    "common_phrases": [
                        "cell membrane",
                        "plasma membrane"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "biology"
                        }
                    ]
                },
                {
                    "sense_id": "ddd4cace-4c47-3937-991b-e69624723506",
                    "sense_definition": "A thin layer of tissue that covers a surface, lines a cavity, or divides an organ in the body.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "biological_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The virus causes inflammation of the mucous <w>membrane</w> in the nose and throat."
                        },
                        {
                            "text": "A delicate <w>membrane</w> surrounds the heart to reduce friction during beats."
                        }
                    ],
                    "synonyms": [
                        "tissue",
                        "lining",
                        "film",
                        "skin"
                    ],
                    "common_phrases": [
                        "mucous membrane",
                        "basement membrane"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "anatomy"
                        }
                    ]
                },
                {
                    "sense_id": "0ea349a1-460f-346c-ab53-450a11d28677",
                    "sense_definition": "A thin, flexible sheet of material used as a barrier to separate substances or to transmit vibrations.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Builders installed a waterproof <w>membrane</w> on the roof to prevent leaks."
                        },
                        {
                            "text": "The loudspeaker produces sound when its thin <w>membrane</w> vibrates rapidly."
                        }
                    ],
                    "synonyms": [
                        "sheet",
                        "film",
                        "diaphragm",
                        "barrier"
                    ],
                    "common_phrases": [
                        "waterproof membrane",
                        "vibrating membrane"

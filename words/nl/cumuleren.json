    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "99466ab2-1906-3f6b-b097-5b8e41fc300b",
                    "sense_definition": "Zich opstapelen of steeds meer worden door voortdurende toevoeging.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleine problemen begonnen te <w>cumuleren</w> tot een grote crisis.",
                            "target_lang_translations": {
                                "en": "The small problems began to <w>accumulate</w> into a major crisis.",
                                "ru": "Маленькие проблемы начали <w>накапливаться</w>, перерастая в крупный кризис."
                            }
                        },
                        {
                            "text": "Al is de rente laag, het bedrag groeit snel doordat het <w>cumuleert</w>.",
                            "target_lang_translations": {
                                "en": "Although the interest rate is low, the amount grows quickly because it <w>accumulates</w>.",
                                "ru": "Хотя процентная ставка низкая, сумма быстро растет, так как она <w>накапливается</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "opstapelen",
                        "ophopen",
                        "vermeerderen"
                    ],
                    "antonyms": [
                        "verminderen",
                        "afnemen"
                    ],
                    "common_phrases": [
                        "rente-cumuleren",
                        "schulden-cumuleren"
                    ],
                    "target_lang_definitions": {
                        "en": "To pile up or increase through continuous addition.",
                        "ru": "Накапливаться или увеличиваться в объеме за счет постоянного добавления."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accumulate",
                                "target_lang_sense_clarification": "general sense"
                            },
                            {
                                "target_lang_word": "pile up",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "накапливаться",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "суммироваться",
                                "target_lang_sense_clarification": "о числах или эффектах"
                            },
                            {
                                "target_lang_word": "скапливаться",
                                "target_lang_sense_clarification": "собираться в кучу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf8d90ad-28fd-30b6-b26b-0d4e72f84e37",
                    "sense_definition": "Het gelijktijdig uitoefenen van meerdere functies, ambten of banen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "combination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In dit land mogen ministers hun functie niet <w>cumuleren</w> met een lokaal mandaat.",
                            "target_lang_translations": {
                                "en": "In this country, ministers are not allowed to <w>combine</w> their position with a local mandate.",
                                "ru": "В этой стране министрам не разрешается <w>совмещать</w> свою должность с местным мандатом."
                            }
                        },
                        {
                            "text": "De politicus kreeg kritiek omdat hij te veel bestuursfuncties wilde <w>cumuleren</w>.",
                            "target_lang_translations": {
                                "en": "The politician received criticism because he wanted to <w>hold</w> too many board positions <w>concurrently</w>.",
                                "ru": "Политик подвергся критике, так как хотел <w>совмещать</w> слишком много руководящих должностей."
                            }
                        }
                    ],
                    "synonyms": [
                        "combineren"
                    ],
                    "common_phrases": [
                        "functies-cumuleren",
                        "mandaten-cumuleren"
                    ],
                    "target_lang_definitions": {
                        "en": "To hold multiple positions, offices, or jobs simultaneously.",
                        "ru": "Одновременное выполнение нескольких функций, должностей или работ."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "combine",
                                "target_lang_sense_clarification": "roles or jobs"
                            },
                            {
                                "target_lang_word": "hold concurrently",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "совмещать",
                                "target_lang_sense_clarification": "должности или функции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1a29f3f-74ec-3831-82fb-9342407589f6",
                    "sense_definition": "Het samenvoegen van verschillende stukken grond of kavels tot één geheel.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "combination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De notaris hielp de eigenaar om de verschillende percelen te <w>cumuleren</w>.",
                            "target_lang_translations": {
                                "en": "The notary helped the owner to <w>consolidate</w> the different plots.",
                                "ru": "Нотариус помог владельцу <w>объединить</w> различные участки."
                            }
                        },
                        {
                            "text": "Door de kleine akkers te <w>cumuleren</w> ontstond er een groot bouwterrein.",
                            "target_lang_translations": {
                                "en": "By <w>merging</w> the small fields, a large building site was created.",
                                "ru": "<w>Объединив</w> небольшие пашни, создали большую строительную площадку."
                            }
                        }
                    ],
                    "synonyms": [
                        "samenvoegen"
                    ],
                    "antonyms": [
                        "verkavelen",
                        "opsplitsen"
                    ],
                    "target_lang_definitions": {
                        "en": "To merge different pieces of land or plots into one whole.",
                        "ru": "Объединение различных земельных участков или наделов в единое целое."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "consolidate",
                                "target_lang_sense_clarification": "land or property"
                            },
                            {
                                "target_lang_word": "merge",
                                "target_lang_sense_clarification": "general"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "объединять",
                                "target_lang_sense_clarification": "земельные участки"
                            },
                            {
                                "target_lang_word": "консолидировать",
                                "target_lang_sense_clarification": "юридический/формальный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

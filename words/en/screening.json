                    "sense_id": "627f6eb2-82df-37f9-8854-9b0fa9264ca0",
                    "sense_definition": "Present participle and gerund of <w>screen</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They are currently <w>screening</w> applicants for the new management position.",
                            "target_lang_translations": {
                                "ru": "В данный момент они <w>отбирают</w> кандидатов на новую руководящую должность."
                            }
                        },
                        {
                            "text": "The nurse is <w>screening</w> patients for flu symptoms in the waiting room.",
                            "target_lang_translations": {
                                "ru": "Медсестра <w>проверяет</w> пациентов на наличие симптомов гриппа в приемной."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {

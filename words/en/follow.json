{
    "word_family": [
        "follower",
        "following",
        "followership"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2882b7a-4d79-3461-a1a3-57e14749e34c",
                    "sense_definition": "To move behind someone or something in the same direction, often to go where they go.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>follow</w> me to the conference room.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>следуйте</w> за мной в конференц-зал."
                            }
                        },
                        {
                            "text": "The little ducklings always <w>follow</w> their mother to the pond.",
                            "target_lang_translations": {
                                "ru": "Маленькие утята всегда <w>следуют</w> за мамой к пруду."
                            }
                        }
                    ],
                    "synonyms": [
                        "pursue",
                        "trail",
                        "tail"
                    ],
                    "antonyms": [
                        "lead",
                        "guide"
                    ],
                    "common_phrases": [
                        "follow me",
                        "follow him",
                        "follow the car"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти или двигаться за кем-то или чем-то в том же направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "идти следом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3aaab2b-505d-3bdc-9c3b-46189120f6c5",
                    "sense_definition": "To come after something else in a sequence, time, or order.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the alphabet, the letter B <w>follows</w> the letter A.",
                            "target_lang_translations": {
                                "ru": "В алфавите буква Б <w>следует</w> за буквой А."
                            }
                        },
                        {
                            "text": "Summer will <w>follow</w> spring as the weather gets warmer.",
                            "target_lang_translations": {
                                "ru": "За весной <w>последует</w> лето, когда погода станет теплее."
                            }
                        }
                    ],
                    "synonyms": [
                        "succeed",
                        "come after",
                        "ensue"
                    ],
                    "antonyms": [
                        "precede"
                    ],
                    "common_phrases": [
                        "follow closely",
                        "follow after"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти следом, наступать после чего-либо во времени или порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "наступать после"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b27f6f0-65a7-38f4-a833-df16b508b404",
                    "sense_definition": "To act according to an instruction, rule, or advice.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_compliance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>follow</w> the instructions carefully to build the shelf.",
                            "target_lang_translations": {
                                "ru": "Вы должны внимательно <w>следовать</w> инструкциям, чтобы собрать полку."
                            }
                        },
                        {
                            "text": "It is important to <w>follow</w> the rules of the road while driving.",
                            "target_lang_translations": {
                                "ru": "Важно <w>соблюдать</w> правила дорожного движения во время вождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "come after",
                        "pursue"
                    ],
                    "antonyms": [
                        "lead"
                    ],
                    "common_phrases": [
                        "follow instructions",
                        "follow the rules",
                        "follow advice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действовать в соответствии с инструкцией, правилом или советом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "руководствоваться чем-либо"
                            },
                            {
                                "target_lang_word": "соблюдать",
                                "target_lang_sense_clarification": "исполнять в точности"
                            },
                            {
                                "target_lang_word": "придерживаться",
                                "target_lang_sense_clarification": "соблюдать что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6caa9475-97cc-349d-8145-d7b430f8b560",
                    "sense_definition": "To subscribe to an account on a social media platform to see its updates.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "digital_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I decided to <w>follow</w> my favorite actor on Instagram.",
                            "target_lang_translations": {
                                "ru": "Я решил <w>подписаться</w> на моего любимого актера в Instagram."
                            }
                        },
                        {
                            "text": "If you like our content, please <w>follow</w> us for more updates.",
                            "target_lang_translations": {
                                "ru": "Если вам нравится наш контент, пожалуйста, <w>подпишитесь</w> на нас для получения обновлений."
                            }
                        }
                    ],
                    "antonyms": [
                        "unfollow"
                    ],
                    "common_phrases": [
                        "follow on Twitter",
                        "follow on Instagram",
                        "like and follow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подписаться на обновления аккаунта в социальной сети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписаться",
                                "target_lang_sense_clarification": "оформить подписку"
                            },
                            {
                                "target_lang_word": "фолловить",
                                "target_lang_sense_clarification": "интернет-сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0606c7c-8362-32a9-a06a-33cbe43a46e0",
                    "sense_definition": "To understand or grasp the meaning of an explanation or argument.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_grasp",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am sorry, but I do not <w>follow</w> what you are saying.",
                            "target_lang_translations": {
                                "ru": "Извините, но я не <w>понимаю</w>, что вы говорите."
                            }
                        },
                        {
                            "text": "Do you <w>follow</w> the logic behind this mathematical proof?",
                            "target_lang_translations": {
                                "ru": "Вы <w>понимаете</w> логику этого математического доказательства?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "do you follow",
                        "follow the logic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать или улавливать смысл объяснения или аргумента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "постигать смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dabd289-2daf-3f1c-af4d-6c9577aa382f",
                    "sense_definition": "To watch the development of something or keep track of current events.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "observation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My father likes to <w>follow</w> the news every morning.",
                            "target_lang_translations": {
                                "ru": "Мой отец любит <w>следить</w> за новостями каждое утро."
                            }
                        },
                        {
                            "text": "We <w>follow</w> the local football team throughout the entire season.",
                            "target_lang_translations": {
                                "ru": "Мы <w>следим</w> за местной футбольной командой на протяжении всего сезона."
                            }
                        }
                    ],
                    "synonyms": [
                        "trail"
                    ],
                    "common_phrases": [
                        "follow the news",
                        "follow sports"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следить за развитием чего-либо или быть в курсе текущих событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следить",
                                "target_lang_sense_clarification": "наблюдать, интересоваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44096512-21c7-33de-ab74-727623a9b3e5",
                    "sense_definition": "To happen as a logical result or consequence of something else.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "logical_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It <w>follows</w> that if you do not study, you will likely fail.",
                            "target_lang_translations": {
                                "ru": "Из этого <w>следует</w>, что если вы не будете учиться, то, скорее всего, провалите экзамен."
                            }
                        },
                        {
                            "text": "If we ignore the problem now, serious trouble is sure to <w>follow</w>.",
                            "target_lang_translations": {
                                "ru": "Если мы проигнорируем проблему сейчас, за этим обязательно <w>последуют</w> серьезные неприятности."
                            }
                        }
                    ],
                    "synonyms": [
                        "ensue"
                    ],
                    "common_phrases": [
                        "it follows that",
                        "sure to follow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть логическим результатом или следствием чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "вытекать как следствие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b6e212b8-f194-37e2-bd2d-eb2cead6c626",
                    "sense_definition": "The act of subscribing to a user's updates on a social media platform.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was excited to get a new <w>follow</w> from a famous musician.",
                            "target_lang_translations": {
                                "ru": "Он был рад получить новую <w>подписку</w> от известного музыканта."
                            }
                        },
                        {
                            "text": "Posting good photos is a great way to get more likes and <w>follows</w>.",
                            "target_lang_translations": {
                                "ru": "Публикация хороших фотографий — отличный способ получить больше лайков и <w>подписок</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "likes and follows",
                        "give a follow"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "internet context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие подписки на обновления пользователя в социальной сети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписка",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "фолловинг",
                                "target_lang_sense_clarification": "интернет-сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
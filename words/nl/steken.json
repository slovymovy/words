                            {
                                "target_lang_word": "поджигать",
                                "target_lang_sense_clarification": "заставлять гореть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3211619c-783f-3836-8e07-85340b19e9cb",
                    "sense_definition": "Tijd, moeite of geld aan iets besteden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "invest",
                    "name_type": "no",
                    "examples": [

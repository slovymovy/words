    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e9ab48f5-0806-3e63-b568-c32a50ddeb12",
                    "sense_definition": "Отлично, великолепно, очень хорошо; используется для выражения высокой оценки.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "evaluation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты очень <круто> играешь на гитаре.",
                            "target_lang_translations": {
                                "en": "You play the guitar really <cool>."
                            }
                        },
                        {
                            "text": "На вечеринке вчера было реально <круто>.",
                            "target_lang_translations": {
                                "en": "It was really <cool> at the party yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "клёво",
                        "здо́рово"
                    ],
                    "common_phrases": [
                        "выглядеть круто",
                        "звучит круто",
                        "очень круто"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common among youth and in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Excellent, wonderful, very good; used to express high appreciation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cool",
                                "target_lang_sense_clarification": "slang, informal"
                            },
                            {
                                "target_lang_word": "awesome",
                                "target_lang_sense_clarification": "slang, enthusiastic"
                            },
                            {
                                "target_lang_word": "great",
                                "target_lang_sense_clarification": "general positive evaluation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70af415b-1800-34d3-98fd-875a66cd262b",
                    "sense_definition": "С резким наклоном, почти вертикально (о подъёме или спуске).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Узкая тропинка <круто> поднималась в гору.",
                            "target_lang_translations": {
                                "en": "The narrow path climbed <steeply> up the mountain."
                            }
                        },
                        {
                            "text": "Берег реки <круто> обрывался прямо у воды.",
                            "target_lang_translations": {
                                "en": "The river bank dropped off <steeply> right by the water."
                            }
                        }
                    ],
                    "antonyms": [
                        "полого"
                    ],
                    "common_phrases": [
                        "круто вверх",
                        "круто вниз",
                        "круто спускаться"
                    ],
                    "target_lang_definitions": {
                        "en": "With a sharp incline, almost vertically (regarding an ascent or descent)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steeply",
                                "target_lang_sense_clarification": "physical slope"
                            },
                            {
                                "target_lang_word": "sharply",
                                "target_lang_sense_clarification": "abrupt drop or rise"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3843a1e-acf4-32cf-9004-5efa486a683d",
                    "sense_definition": "Резко, внезапно (об изменении направления движения или хода событий).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Машина <круто> свернула налево, чтобы избежать аварии.",
                            "target_lang_translations": {
                                "en": "The car turned <sharply> to the left to avoid an accident."
                            }
                        },
                        {
                            "text": "Его жизнь <круто> изменилась после переезда в другой город.",
                            "target_lang_translations": {
                                "en": "His life changed <drastically> after moving to another city."
                            }
                        }
                    ],
                    "synonyms": [
                        "резко",
                        "внезапно"
                    ],
                    "antonyms": [
                        "плавно"
                    ],
                    "common_phrases": [
                        "круто повернуть",
                        "круто изменить",
                        "круто развернуться"
                    ],
                    "target_lang_definitions": {
                        "en": "Sharply, suddenly (regarding a change in direction or course of events)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sharply",
                                "target_lang_sense_clarification": "sudden turn"
                            },
                            {
                                "target_lang_word": "drastically",
                                "target_lang_sense_clarification": "significant change in situation"
                            },
                            {
                                "target_lang_word": "abruptly",
                                "target_lang_sense_clarification": "sudden action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5752fd4-4aaa-365d-9dcd-a7fbabf988d6",
                    "sense_definition": "Густо, плотно (о консистенции теста) или сильно (о добавлении соли).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "consistency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тесто для пельменей нужно замесить <круто>.",
                            "target_lang_translations": {
                                "en": "The dumpling dough needs to be kneaded <stiffly>."
                            }
                        },
                        {
                            "text": "Повар слишком <круто> посолил суп, и его невозможно было есть.",
                            "target_lang_translations": {
                                "en": "The cook salted the soup too <heavily>, and it was impossible to eat."
                            }
                        }
                    ],
                    "common_phrases": [
                        "круто посолить",
                        "круто замесить"
                    ],
                    "target_lang_definitions": {
                        "en": "Thickly, densely (regarding dough consistency) or strongly (regarding adding salt)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stiffly",
                                "target_lang_sense_clarification": "dough consistency"
                            },
                            {
                                "target_lang_word": "heavily",
                                "target_lang_sense_clarification": "amount of salt/seasoning"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d05b023-5032-3182-96db-f76a6c20c2b4",
                    "sense_definition": "Строго, сурово, решительно и без снисхождения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник <круто> обошёлся с сотрудником, который опоздал.",
                            "target_lang_translations": {
                                "en": "The boss treated the employee who was late <harshly>."
                            }
                        },
                        {
                            "text": "Отец решил <круто> поговорить с сыном о его поведении.",
                            "target_lang_translations": {
                                "en": "The father decided to talk <sternly> with his son about his behavior."
                            }
                        }
                    ],
                    "synonyms": [
                        "сурово",
                        "строго",
                        "жёстко",
                        "решительно"
                    ],
                    "common_phrases": [
                        "круто обойтись",
                        "круто поговорить"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Strictly, severely, decisively, and without leniency."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "harshly",
                                "target_lang_sense_clarification": "severe treatment"
                            },
                            {
                                "target_lang_word": "sternly",
                                "target_lang_sense_clarification": "strict manner"
                            },
                            {
                                "target_lang_word": "toughly",
                                "target_lang_sense_clarification": "uncompromisingly"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "6881c382-8ba4-39ca-bd1d-01eca6fda3df",
                    "sense_definition": "Выражение одобрения, восхищения или радости.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы выиграли этот сложный матч! — <Круто>!",
                            "target_lang_translations": {
                                "en": "We won this difficult match! — <Cool>!"
                            }
                        },
                        {
                            "text": "— Я купил билеты на концерт. — <Круто>, я давно хотел пойти!",
                            "target_lang_translations": {
                                "en": "— I bought tickets to the concert. — <Awesome>, I've wanted to go for a long time!"
                            }
                        }
                    ],
                    "synonyms": [
                        "клёво",
                        "здо́рово"
                    ],
                    "common_phrases": [
                        "как круто",
                        "просто круто"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An expression of approval, admiration, or joy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Cool!",
                                "target_lang_sense_clarification": "exclamation of approval"
                            },
                            {
                                "target_lang_word": "Awesome!",
                                "target_lang_sense_clarification": "exclamation of admiration"
                            },
                            {
                                "target_lang_word": "Great!",
                                "target_lang_sense_clarification": "positive reaction"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9ef2d8e8-c433-350e-9ae4-2ad083704d46",
                    "sense_definition": "Краткая форма среднего рода прилагательного <крутой>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это решение было слишком <круто> и неожиданно для всех.",
                            "target_lang_translations": {
                                "en": "This decision was too <harsh> and unexpected for everyone."
                            }
                        },
                        {
                            "text": "Всё это выглядит очень <круто> со стороны.",
                            "target_lang_translations": {
                                "en": "All this looks very <cool> from the outside."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short adjective"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short neuter form of the adjective 'tough', 'cool', or 'steep'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cool",
                                "target_lang_sense_clarification": "impressive"
                            },
                            {
                                "target_lang_word": "harsh",
                                "target_lang_sense_clarification": "severe/drastic"
                            },
                            {
                                "target_lang_word": "steep",
                                "target_lang_sense_clarification": "high price or angle"
                            }
                        ]
                    }
                }
            ]
        }
    ]

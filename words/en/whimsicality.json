            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a6c4f5f6-ca26-3a73-9e87-9ad85f0a60e2",
                    "sense_definition": "The quality of being playful, fanciful, or unusual in an amusing way.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist's work is loved for its charm and <w>whimsicality</w>.",
                            "target_lang_translations": {
                                "ru": "Работы художника любят за их шарм и <w>причудливость</w>."
                            }
                        },
                        {
                            "text": "She added a touch of <w>whimsicality</w> to the serious presentation.",
                            "target_lang_translations": {
                                "ru": "Она добавила нотку <w>затейливости</w> в серьезную презентацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "playfulness",
                        "fancifulness",
                        "quirkiness",
                        "capriciousness"
                    ],
                    "antonyms": [
                        "seriousness",
                        "solemnity",
                        "practicality"
                    ],
                    "common_phrases": [
                        "touch of whimsicality",
                        "sense of whimsicality"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство быть игривым, фантазийным или необычным в забавной манере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "причудливость",
                                "target_lang_sense_clarification": "свойство чего-то необычного и забавного"
                            },
                            {
                                "target_lang_word": "затейливость",
                                "target_lang_sense_clarification": "сложность и оригинальность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6b7a77c-8219-3760-a4a1-fef53bee30e2",
                    "sense_definition": "A strange, funny, or fanciful idea, object, or action.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The garden was filled with stone statues and other <w>whimsicalities</w>.",
                            "target_lang_translations": {
                                "ru": "Сад был наполнен каменными статуями и другими <w>причудами</w>."
                            }
                        },
                        {
                            "text": "We were amused by the various <w>whimsicalities</w> of the old architecture.",
                            "target_lang_translations": {
                                "ru": "Нас позабавили различные <w>странности</w> старинной архитектуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "caprice",
                        "quirk",
                        "oddity",
                        "whim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Странная, забавная или фантазийная идея, предмет или действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "причуда",
                                "target_lang_sense_clarification": "необычный поступок, идея или вещь"
                            },
                            {
                                "target_lang_word": "странность",
                                "target_lang_sense_clarification": "необычная особенность"
                            }
                        ]
                    }
                }
            ]
        }
    ]

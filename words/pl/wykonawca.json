            "senses": [
                {
                    "sense_id": "9b24ae46-2ebd-39a4-bdb4-49127eabd31b",
                    "sense_definition": "Artysta wykonujący utwór muzyczny, teatralny lub inne dzieło przed publicznością.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody <w>wykonawca</w> zachwycił publiczność swoją interpretacją piosenki.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>исполнитель</w> восхитил публику своей интерпретацией песни."
                            }
                        },
                        {
                            "text": "Na scenie pojawił się znany <w>wykonawca</w> muzyki pop.",
                            "target_lang_translations": {
                                "ru": "На сцене появился известный <w>исполнитель</w> поп-музыки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wykonawca utworu",
                        "znany wykonawca",
                        "oryginalny wykonawca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Артист, исполняющий музыкальное, театральное или иное произведение перед публикой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исполнитель",
                                "target_lang_sense_clarification": "артист, музыкант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9a3e654-5243-3271-bc08-eded5119449c",
                    "sense_definition": "Osoba lub firma, która realizuje określone zadanie, projekt lub zlecenie, często na podstawie umowy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Główny <w>wykonawca</w> zobowiązał się do ukończenia budowy w terminie.",
                            "target_lang_translations": {
                                "ru": "Генеральный <w>подрядчик</w> обязался завершить строительство в срок."
                            }
                        },
                        {
                            "text": "Jako <w>wykonawca</w> testamentu, prawnik musiał skontaktować się z rodziną zmarłego.",
                            "target_lang_translations": {
                                "ru": "Как <w>исполнитель</w> завещания, юрист должен был связаться с семьей покойного."
                            }
                        }
                    ],
                    "synonyms": [
                        "realizator"
                    ],
                    "antonyms": [
                        "zleceniodawca"
                    ],
                    "common_phrases": [
                        "główny wykonawca",
                        "wykonawca robót",
                        "wykonawca testamentu",
                        "wykonawca zlecenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или организация, выполняющие определенную работу, заказ или проект, часто по договору."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исполнитель",
                                "target_lang_sense_clarification": "лицо, выполняющее работу или заказ"
                            },
                            {
                                "target_lang_word": "подрядчик",
                                "target_lang_sense_clarification": "строительство, договорные отношения"
                            }
                        ]
                    }
                }
            ]
        }

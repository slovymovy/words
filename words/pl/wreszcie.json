{
    "word_family": [
        "reszta",
        "resztka",
        "zresztą"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1d85d994-0261-3252-9590-f0608c8d902b",
                    "sense_definition": "Słowo wyrażające ulgę lub zniecierpliwienie, że oczekiwane zdarzenie nastąpiło po długim czasie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wreszcie</w> przyjechałeś! Tak długo na ciebie czekaliśmy!",
                            "target_lang_translations": {
                                "ru": "<w>Наконец</w> ты приехал! Мы так долго тебя ждали!"
                            }
                        },
                        {
                            "text": "Czy <w>wreszcie</w> powiesz mi, o co ci właściwie chodzi?",
                            "target_lang_translations": {
                                "ru": "Ты <w>наконец</w> скажешь мне, в чём собственно дело?"
                            }
                        }
                    ],
                    "synonyms": [
                        "nareszcie",
                        "w końcu"
                    ],
                    "common_phrases": [
                        "no wreszcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, выражающее облегчение или нетерпение от того, что ожидаемое событие произошло спустя долгое время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наконец",
                                "target_lang_sense_clarification": "после долгого ожидания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99c08e74-61f9-35bf-ae2e-24cd36415020",
                    "sense_definition": "Słowo używane do wprowadzenia ostatniego elementu w wyliczeniu lub ciągu zdarzeń.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "enumeration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Najpierw zrobiliśmy zakupy, potem posprzątaliśmy, a <w>wreszcie</w> odpoczęliśmy.",
                            "target_lang_translations": {
                                "ru": "Сначала мы сделали покупки, потом убрались и, <w>наконец</w>, отдохнули."
                            }
                        },
                        {
                            "text": "Dyskutowaliśmy o kosztach, terminach i <w>wreszcie</w> o wykonawcach.",
                            "target_lang_translations": {
                                "ru": "Мы обсуждали расходы, сроки и, <w>наконец</w>, исполнителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "w końcu"
                    ],
                    "common_phrases": [
                        "i wreszcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое для введения последнего элемента в перечислении или последовательности событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наконец",
                                "target_lang_sense_clarification": "при перечислении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
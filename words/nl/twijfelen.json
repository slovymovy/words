{
    "word_family": [
        "twijfel",
        "twijfelaar",
        "twijfelachtig",
        "ongetwijfeld",
        "betwijfelen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8ea46180-2406-36d6-a8bd-be2432c54f1e",
                    "sense_definition": "Niet zeker weten wat je moet doen of kiezen; aarzelen tussen verschillende mogelijkheden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "indecision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze <w>twijfelde</w> lang tussen de rode en de blauwe jurk.",
                            "target_lang_translations": {
                                "en": "She <w>hesitated</w> for a long time between the red and the blue dress."
                            }
                        },
                        {
                            "text": "Wij <w>twijfelen</w> nog steeds over de bestemming van onze vakantie.",
                            "target_lang_translations": {
                                "en": "We are still <w>hesitating</w> about the destination of our vacation."
                            }
                        }
                    ],
                    "synonyms": [
                        "aarzelen",
                        "weifelen"
                    ],
                    "antonyms": [
                        "beslissen",
                        "kiezen"
                    ],
                    "common_phrases": [
                        "twijfelen tussen twee dingen",
                        "twijfelen over een beslissing",
                        "blijven twijfelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be unsure what to do or choose; to hesitate between different possibilities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hesitate",
                                "target_lang_sense_clarification": "to pause or hold back in uncertainty"
                            },
                            {
                                "target_lang_word": "waver",
                                "target_lang_sense_clarification": "to be undecided between two opinions or courses of action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3c56e4c-243d-35ef-af26-503df8d19d1a",
                    "sense_definition": "Het vermoeden hebben dat iets niet waar is of dat iemand niet eerlijk of capabel is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "skepticism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie <w>twijfelt</w> aan de waarheid van zijn verklaring.",
                            "target_lang_translations": {
                                "en": "The police <w>doubt</w> the truth of his statement."
                            }
                        },
                        {
                            "text": "Je moet niet aan jezelf <w>twijfelen</w> als het even tegenzit.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>doubt</w> yourself when things go against you."
                            }
                        }
                    ],
                    "synonyms": [
                        "wantrouwen"
                    ],
                    "antonyms": [
                        "geloven",
                        "vertrouwen"
                    ],
                    "common_phrases": [
                        "twijfelen aan de waarheid",
                        "twijfelen aan jezelf",
                        "niet aan te twijfelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To suspect that something is not true or that someone is not honest or capable."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "doubt",
                                "target_lang_sense_clarification": "to lack confidence in or disbelieve"
                            },
                            {
                                "target_lang_word": "question",
                                "target_lang_sense_clarification": "to have or express doubts about the validity or truth of something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
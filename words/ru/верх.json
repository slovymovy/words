                {
                    "sense_id": "f9ac979b-ad2e-34b2-9ee4-f1075a3e2f14",
                    "sense_definition": "Высшая степень какого-либо качества или состояния.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_degree",
                    "name_type": "no",
                    "examples": [

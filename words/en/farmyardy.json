    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ed2e014c-80f1-3568-a919-7aed54d26854",
                    "sense_definition": "Resembling or characteristic of a farmyard, especially regarding its smell or appearance.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "resembling_farmyard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The muddy boots left a <w>farmyardy</w> smell in the hallway after the walk.",
                            "target_lang_translations": {
                                "ru": "Грязные сапоги после прогулки оставили в прихожей запах <w>скотного двора</w>."
                            }
                        },
                        {
                            "text": "She wrinkled her nose at the <w>farmyardy</w> odor drifting from the nearby cattle pens.",
                            "target_lang_translations": {
                                "ru": "Она поморщилась от запаха <w>хлева</w>, доносившегося из соседних загонов для скота."
                            }
                        }
                    ],
                    "synonyms": [
                        "barnyard"
                    ],
                    "common_phrases": [
                        "farmyardy smell",
                        "farmyardy odor",
                        "farmyardy scent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий скотный двор или характерный для него, особенно в отношении запаха или внешнего вида."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напоминающий скотный двор",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "хлевный",
                                "target_lang_sense_clarification": "преимущественно о запахе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7b6e2bd-cd8c-3c9d-8494-6dbdfd4211c9",
                    "sense_definition": "Having a rustic, earthy flavor or aroma, often used to describe wine, cheese, or cider.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "resembling_farmyard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sommelier pointed out the complex, <w>farmyardy</w> notes in the aged red wine.",
                            "target_lang_translations": {
                                "ru": "Сомелье отметил сложные нотки <w>скотного двора</w> в выдержанном красном вине."
                            }
                        },
                        {
                            "text": "Some people love the <w>farmyardy</w> flavor of this traditional goat cheese, while others find it too strong.",
                            "target_lang_translations": {
                                "ru": "Некоторым нравится <w>хлевный</w> привкус этого традиционного козьего сыра, в то время как другие находят его слишком сильным."
                            }
                        }
                    ],
                    "synonyms": [
                        "barnyard"
                    ],
                    "common_phrases": [
                        "farmyardy notes",
                        "farmyardy flavor",
                        "farmyardy character"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий деревенским, землистым вкусом или ароматом; часто используется для описания вина, сыра или сидра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с тонами скотного двора",
                                "target_lang_sense_clarification": "о вине или сыре"
                            },
                            {
                                "target_lang_word": "животный",
                                "target_lang_sense_clarification": "термин сомелье"
                            }
                        ]
                    }
                }
            ]
        }
    ]

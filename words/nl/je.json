                {
                    "sense_id": "e0c736a5-722f-37c2-add8-9603c49d412a",
                    "sense_definition": "De onbenadrukte vorm van het persoonlijk voornaamwoord <w>jij</w> (tweede persoon enkelvoud, onderwerp).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "subject_sg",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ga <w>je</w> vanavond mee naar de bioscoop?",
                            "target_lang_translations": {
                                "en": "Are <w>you</w> coming to the cinema tonight?"
                            }
                        },
                        {
                            "text": "Wat heb <w>je</w> gisteren eigenlijk gedaan?",
                            "target_lang_translations": {
                                "en": "What did <w>you</w> actually do yesterday?"
                            }
                        }
                    ],
                    "synonyms": [
                        "jij"
                    ],
                    "common_phrases": [
                        "weet je",
                        "heb je",
                        "kun je",
                        "wil je"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "onbenadrukte vorm"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The unstressed form of the personal pronoun 'jij' (second person singular, subject)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you",
                                "target_lang_sense_clarification": "subject pronoun"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0698a74c-ed86-3711-b355-bd8b1a84535b",
                    "sense_definition": "De onbenadrukte vorm van het persoonlijk voornaamwoord <w>jou</w> (tweede persoon enkelvoud, voorwerp).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "object_sg",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal <w>je</w> morgen even bellen.",
                            "target_lang_translations": {
                                "en": "I will give <w>you</w> a call tomorrow."
                            }
                        },
                        {
                            "text": "Hij heeft <w>je</w> gisteren in de stad gezien.",
                            "target_lang_translations": {
                                "en": "He saw <w>you</w> in town yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "jou"
                    ],
                    "common_phrases": [
                        "ik zie je",
                        "voor je",
                        "met je",
                        "aan je"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "onbenadrukte vorm"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The unstressed form of the personal pronoun 'jou' (second person singular, object)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you",
                                "target_lang_sense_clarification": "object pronoun"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74ef53ca-2848-34c1-9571-8e34d3c2c782",
                    "sense_definition": "Wederkerend voornaamwoord dat verwijst naar het onderwerp van de zin (tweede persoon).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb je <w>je</w> vanochtend al gewassen?",
                            "target_lang_translations": {
                                "en": "Have you washed <w>yourself</w> already this morning?"
                            }
                        },
                        {
                            "text": "Je moet <w>je</w> niet zo druk maken over kleine dingen.",
                            "target_lang_translations": {
                                "en": "You shouldn't work <w>yourself</w> up so much about small things."
                            }
                        }
                    ],
                    "synonyms": [
                        "jezelf"
                    ],
                    "common_phrases": [
                        "je vergissen",
                        "je wassen",
                        "je voelen",
                        "je herinneren"
                    ],
                    "target_lang_definitions": {
                        "en": "Reflexive pronoun referring to the subject of the sentence (second person)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "yourself",
                                "target_lang_sense_clarification": "reflexive pronoun"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6aeb72e2-afb7-3424-9e36-f9d72f6c3568",
                    "sense_definition": "Verwijzing naar mensen in het algemeen, inclusief de spreker en luisteraar; informeel alternatief voor 'men'.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "generic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Nederland kun <w>je</w> overal veilig fietsen.",
                            "target_lang_translations": {
                                "en": "In the Netherlands <w>you</w> can cycle safely everywhere."
                            }
                        },
                        {
                            "text": "<w>Je</w> weet nooit wat er in de toekomst gaat gebeuren.",
                            "target_lang_translations": {
                                "en": "<w>You</w> never know what is going to happen in the future."
                            }
                        }
                    ],
                    "synonyms": [
                        "men"
                    ],
                    "common_phrases": [
                        "je weet maar nooit",
                        "dat heb je",
                        "zo zie je maar"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informeler dan 'men'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Reference to people in general, including the speaker and listener; informal alternative for 'one'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you",
                                "target_lang_sense_clarification": "generic pronoun"
                            },
                            {
                                "target_lang_word": "one",
                                "target_lang_sense_clarification": "formal generic pronoun"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "c8341bdb-919c-3750-b43c-cb62dd9ae833",
                    "sense_definition": "De onbenadrukte vorm van het bezittelijk voornaamwoord <w>jouw</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possessive_sg",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vergeet niet <w>je</w> jas mee te nemen als je weggaat.",
                            "target_lang_translations": {
                                "en": "Don't forget to take <w>your</w> coat when you leave."
                            }
                        },
                        {
                            "text": "Hoe gaat het met <w>je</w> moeder?",
                            "target_lang_translations": {
                                "en": "How is <w>your</w> mother?"
                            }
                        }
                    ],
                    "synonyms": [
                        "jouw"
                    ],
                    "common_phrases": [
                        "je moeder",
                        "je vader",
                        "je eigen",
                        "op je gemak"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "onbenadrukte vorm"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The unstressed form of the possessive pronoun 'jouw'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "your",
                                "target_lang_sense_clarification": "possessive determiner"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65021d43-13dc-3008-8301-5a5b2d4f4cf2",
                    "sense_definition": "Vervanging van het bezittelijk voornaamwoord <w>jullie</w> wanneer 'jullie' al als onderwerp in de zin staat (om herhaling te voorkomen).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "possessive_pl",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hebben jullie <w>je</w> huiswerk al afgemaakt?",
                            "target_lang_translations": {
                                "en": "Have you finished <w>your</w> homework yet?"
                            }
                        },
                        {
                            "text": "Jullie moeten <w>je</w> schoenen uitdoen bij de voordeur.",
                            "target_lang_translations": {
                                "en": "You must take off <w>your</w> shoes at the front door."
                            }
                        }
                    ],
                    "synonyms": [
                        "jullie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "vervangt 'jullie' om herhaling te vermijden"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Replacement for the possessive pronoun 'jullie' when 'jullie' is already the subject (to avoid repetition)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "your",
                                "target_lang_sense_clarification": "possessive determiner (plural context)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "ówczesny",
        "ówcześnie"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "cee30a4f-0142-3c14-bf76-7106a00d102e",
                    "sense_definition": "Zaimek wskazujący na osobę, rzecz lub czas, o których była mowa wcześniej; w języku współczesnym ma charakter nieco książkowy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "demonstrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <w>owym</w> czasie w całym kraju panował spokój.",
                            "target_lang_translations": {
                                "ru": "В <w>то</w> время во всей стране царило спокойствие."
                            }
                        },
                        {
                            "text": "Nie znałem wcześniej <w>owego</w> człowieka.",
                            "target_lang_translations": {
                                "ru": "Я раньше не знал <w>того</w> человека."
                            }
                        }
                    ],
                    "synonyms": [
                        "tamten"
                    ],
                    "common_phrases": [
                        "w owym czasie",
                        "do owego dnia",
                        "po owe czasy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указательное местоимение, относящееся к лицу, предмету или времени, о которых шла речь ранее; в современном языке имеет несколько книжный оттенок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тот",
                                "target_lang_sense_clarification": "указывает на отдаленный или упомянутый объект"
                            },
                            {
                                "target_lang_word": "этот",
                                "target_lang_sense_clarification": "указывает на упомянутый объект (в повествовании)"
                            },
                            {
                                "target_lang_word": "оный",
                                "target_lang_sense_clarification": "устаревшее/книжное; тот самый, вышеупомянутый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d490357f-cb4f-3578-97bc-8e41288a4731",
                    "sense_definition": "Zaimek nieokreślony oznaczający kogoś lub coś bliżej nieznanego, używany zazwyczaj w utartych zwrotach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "indefinite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przychodził tu <w>ten i ów</w>, żeby zapytać o drogę.",
                            "target_lang_translations": {
                                "ru": "Сюда приходил <w>кое-кто</w>, чтобы спросить дорогу."
                            }
                        },
                        {
                            "text": "Z czasem <w>ten i ów</w> zaczął głośno narzekać.",
                            "target_lang_translations": {
                                "ru": "Со временем <w>кое-кто</w> начал громко жаловаться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ten i ów",
                        "to i owo",
                        "ta i owa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопределенное местоимение, обозначающее кого-то или что-то неизвестное; обычно используется в устойчивых выражениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кое-кто",
                                "target_lang_sense_clarification": "в значении «некоторые люди»"
                            },
                            {
                                "target_lang_word": "тот и этот",
                                "target_lang_sense_clarification": "разные, всякие люди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d2c2fd6-4426-3865-8b8c-79c087393173",
                    "sense_definition": "Zaimek pełniący funkcję podmiotu, zastępujący imię lub nazwę wspomnianego wcześniej mężczyzny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "personal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spojrzał na przyjaciela, a <w>ów</w> tylko się uśmiechnął.",
                            "target_lang_translations": {
                                "ru": "Он посмотрел на друга, а <w>тот</w> лишь улыбнулся."
                            }
                        },
                        {
                            "text": "Zapytałem go o zdanie, lecz <w>ów</w> milczał.",
                            "target_lang_translations": {
                                "ru": "Я спросил его мнения, но <w>тот</w> молчал."
                            }
                        }
                    ],
                    "synonyms": [
                        "on"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary style"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Местоимение в роли подлежащего, заменяющее имя или название упомянутого ранее мужчины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тот",
                                "target_lang_sense_clarification": "заменяет существительное, чтобы избежать повтора"
                            },
                            {
                                "target_lang_word": "он",
                                "target_lang_sense_clarification": "личное местоимение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
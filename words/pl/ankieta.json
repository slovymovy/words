                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "komisja",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rządowa <w>ankieta</w> miała za zadanie ocenić stan rolnictwa w kraju.",
                            "target_lang_translations": {
                                "ru": "Правительственная <w>комиссия</w> имела задачу оценить состояние сельского хозяйства в стране."

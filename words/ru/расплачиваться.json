    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d49e1959-da96-39bf-bc8f-879fb073cd0c",
                    "sense_definition": "Отдавать деньги за покупку, услугу или возвращать долг",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом ресторане можно <w>расплачиваться</w> банковской картой.",
                            "target_lang_translations": {
                                "en": "You can <w>pay</w> with a bank card in this restaurant."
                            }
                        },
                        {
                            "text": "Он постепенно <w>расплачивается</w> с долгами, которые взял на покупку квартиры.",
                            "target_lang_translations": {
                                "en": "He is gradually <w>paying off</w> the debts he took on to buy the apartment."
                            }
                        }
                    ],
                    "synonyms": [
                        "платить",
                        "рассчитываться",
                        "оплачивать"
                    ],
                    "common_phrases": [
                        "расплачиваться наличными",
                        "расплачиваться картой",
                        "расплачиваться с долгами"
                    ],
                    "target_lang_definitions": {
                        "en": "To give money for goods, services, or to settle a debt"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay",
                                "target_lang_sense_clarification": "give money for goods or services"
                            },
                            {
                                "target_lang_word": "pay off",
                                "target_lang_sense_clarification": "finish paying a debt completely"
                            },
                            {
                                "target_lang_word": "settle",
                                "target_lang_sense_clarification": "pay a bill or debt"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d5debff-5519-3d88-905e-34c73b2392ce",
                    "sense_definition": "Нести наказание или страдать из-за своих поступков или ошибок",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам всем приходится <w>расплачиваться</w> за ошибки молодости.",
                            "target_lang_translations": {
                                "en": "We all have to <w>pay</w> for the mistakes of our youth."
                            }
                        },
                        {
                            "text": "Спортсмен <w>расплачивается</w> своим здоровьем за олимпийские медали.",
                            "target_lang_translations": {
                                "en": "The athlete <w>pays</w> with his health for Olympic medals."
                            }
                        }
                    ],
                    "synonyms": [
                        "поплатиться",
                        "отвечать"
                    ],
                    "common_phrases": [
                        "расплачиваться за ошибки",
                        "дорого расплачиваться",
                        "расплачиваться здоровьем"
                    ],
                    "target_lang_definitions": {
                        "en": "To suffer negative consequences or be punished for one's actions"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay",
                                "target_lang_sense_clarification": "suffer consequences for actions"
                            },
                            {
                                "target_lang_word": "pay the price",
                                "target_lang_sense_clarification": "idiom: suffer the consequences"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50165c87-f53c-3527-9aee-5e38c1abd125",
                    "sense_definition": "Мстить кому-либо, сводить счёты за нанесённую обиду",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "revenge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Главный герой фильма жестоко <w>расплачивается</w> со своими врагами.",
                            "target_lang_translations": {
                                "en": "The movie's main character cruelly <w>settles scores</w> with his enemies."
                            }
                        },
                        {
                            "text": "Не стоит <w>расплачиваться</w> злом за причинённое зло.",
                            "target_lang_translations": {
                                "en": "One should not <w>pay back</w> evil with evil."
                            }
                        }
                    ],
                    "synonyms": [
                        "мстить",
                        "квитаться"
                    ],
                    "common_phrases": [
                        "расплачиваться с обидчиком",
                        "расплачиваться с врагами"
                    ],
                    "target_lang_definitions": {
                        "en": "To take revenge or settle scores with someone"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get even",
                                "target_lang_sense_clarification": "inflict similar trouble on a rival"
                            },
                            {
                                "target_lang_word": "settle scores",
                                "target_lang_sense_clarification": "avenge a past wrong"
                            },
                            {
                                "target_lang_word": "pay back",
                                "target_lang_sense_clarification": "take revenge on someone"
                            }
                        ]
                    }
                }
            ]
        }

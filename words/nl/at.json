                {
                    "sense_id": "18eb860b-97ff-3e23-ab18-a1b0a1379120",
                    "sense_definition": "Enkelvoud verleden tijd van <w>eten</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_eten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gisteren <w>at</w> ik een heerlijke pizza in dat nieuwe restaurant.",
                            "target_lang_translations": {
                                "en": "Yesterday I <w>ate</w> a delicious pizza at that new restaurant.",
                                "ru": "Вчера я <w>ел</w> вкусную пиццу в том новом ресторане."
                            }
                        },
                        {
                            "text": "Hij <w>at</w> zijn bord niet helemaal leeg omdat hij al vol zat.",
                            "target_lang_translations": {
                                "en": "He didn't <w>eat</w> everything on his plate because he was already full.",
                                "ru": "Он не <w>съел</w> всё, что было на тарелке, так как уже наелся."
                            }
                        }
                    ],
                    "common_phrases": [
                        "at op",
                        "at mee"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Singular past tense of the verb 'to eat'.",
                        "ru": "Прошедшее время единственного числа глагола \"eten\" (есть)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ate",
                                "target_lang_sense_clarification": "past tense of 'to eat'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ел",
                                "target_lang_sense_clarification": "прошедшее время, мужской род"
                            },
                            {
                                "target_lang_word": "ела",
                                "target_lang_sense_clarification": "прошедшее время, женский род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94ee1585-8e26-38c0-bf42-ca36964debd3",
                    "sense_definition": "Eerste, tweede en derde persoon enkelvoud tegenwoordige tijd van <w>atten</w> (in één keer leegdrinken).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_atten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>at</w> zijn biertje in één teug leeg om de weddenschap te winnen.",
                            "target_lang_translations": {
                                "en": "He <w>chugs</w> his beer in one go to win the bet.",
                                "ru": "Он <w>пьёт залпом</w> своё пиво, чтобы выиграть пари."
                            }
                        },
                        {
                            "text": "Als straf <w>at</w> de student zijn glas water direct leeg.",
                            "target_lang_translations": {
                                "en": "As punishment, the student immediately <w>downs</w> his glass of water.",
                                "ru": "В наказание студент сразу же <w>выпивает залпом</w> стакан воды."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present tense"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "student language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugated form of the verb 'atten' (to drink something in one go).",
                        "ru": "Форма настоящего времени единственного числа глагола \"atten\" (пить залпом, осушать до дна)."
                    },
                    "translations": {

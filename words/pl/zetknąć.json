{
    "word_family": [
        "styk",
        "styczność",
        "styczny",
        "styczna",
        "stykowy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "842ccfe2-20ca-398d-af5c-d8f7e1aea2cc",
                    "sense_definition": "Zbliżyć do siebie dwa przedmioty lub ich części tak, aby się dotknęły.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Elektryk musiał <zetknąć> końcówki przewodów, aby popłynął prąd.",
                            "target_lang_translations": {
                                "ru": "Электрик должен был <соединить> концы проводов, чтобы пошел ток."
                            }
                        },
                        {
                            "text": "Ostrożnie <zetknął> brzegi rany, zanim nałożył na nią opatrunek.",
                            "target_lang_translations": {
                                "ru": "Он осторожно <сомкнул> края раны, прежде чем наложить повязку."
                            }
                        }
                    ],
                    "synonyms": [
                        "przytykać",
                        "przykładać",
                        "zbliżać"
                    ],
                    "antonyms": [
                        "oddalać"
                    ],
                    "common_phrases": [
                        "zetknąć dłonie",
                        "zetknąć kable",
                        "zetknąć krawędzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сблизить два предмета или их части так, чтобы они коснулись друг друга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединить",
                                "target_lang_sense_clarification": "привести в контакт (напр. провода)"
                            },
                            {
                                "target_lang_word": "сомкнуть",
                                "target_lang_sense_clarification": "сблизить края чего-либо"
                            },
                            {
                                "target_lang_word": "сблизить",
                                "target_lang_sense_clarification": "придвинуть друг к другу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17b25754-a84e-320c-8599-66744a68bf87",
                    "sense_definition": "O obiektach lub częściach ciała: dotknąć się wzajemnie lub znaleźć się w bezpośredniej bliskości.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ich dłonie przypadkiem <zetknęły się> podczas przekazywania dokumentów.",
                            "target_lang_translations": {
                                "ru": "Их ладони случайно <соприкоснулись> при передаче документов."
                            }
                        },
                        {
                            "text": "Dwie rzeki <zetknęły się> w dolinie, tworząc potężny nurt.",
                            "target_lang_translations": {
                                "ru": "Две реки <сошлись> в долине, образуя мощный поток."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbliżać się"
                    ],
                    "common_phrases": [
                        "zetknąć się ramionami",
                        "zetknąć się powierzchniami",
                        "zetknąć się nosami"
                    ],
                    "target_lang_definitions": {
                        "ru": "О предметах или частях тела: коснуться друг друга или оказаться в непосредственной близости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соприкоснуться",
                                "target_lang_sense_clarification": "коснуться поверхностями"
                            },
                            {
                                "target_lang_word": "коснуться",
                                "target_lang_sense_clarification": "дотронуться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "121197aa-91fd-3f5c-a20d-f4e70826ae42",
                    "sense_definition": "Napotkać kogoś lub coś bezpośrednio; doświadczyć czegoś nowego, trudnego lub interesującego (np. problemu, kultury, zjawiska).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "encounter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas podróży po Azji po raz pierwszy <zetknąłem się> z tak odmienną kulturą.",
                            "target_lang_translations": {
                                "ru": "Во время путешествия по Азии я впервые <столкнулся> со столь отличной культурой."
                            }
                        },
                        {
                            "text": "W nowej pracy szybko <zetknęła się> z problemami organizacyjnymi.",
                            "target_lang_translations": {
                                "ru": "На новой работе она быстро <столкнулась> с организационными проблемами."
                            }
                        }
                    ],
                    "synonyms": [
                        "spotykać się"
                    ],
                    "common_phrases": [
                        "zetknąć się z problemem",
                        "zetknąć się z krytyką",
                        "zetknąć się z rzeczywistością",
                        "zetknąć się osobiście"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встретить кого-то или что-то непосредственно; испытать что-то новое, трудное или интересное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столкнуться",
                                "target_lang_sense_clarification": "с проблемой, реальностью или неожиданностью"
                            },
                            {
                                "target_lang_word": "познакомиться",
                                "target_lang_sense_clarification": "узнать что-то новое (напр. культуру)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdd57798-15a1-3212-bab5-f5e6ba048994",
                    "sense_definition": "Doprowadzić do spotkania lub nawiązania znajomości między dwiema osobami.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To niesamowity przypadek <zetknął> ich ze sobą na dworcu w obcym mieście.",
                            "target_lang_translations": {
                                "ru": "Невероятный случай <свел> их вместе на вокзале в чужом городе."
                            }
                        },
                        {
                            "text": "Wspólne interesy <zetknęły> mnie z wieloma ciekawymi ludźmi z branży.",
                            "target_lang_translations": {
                                "ru": "Общие интересы <свели> меня со многими интересными людьми из этой сферы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaznajamiać"
                    ],
                    "common_phrases": [
                        "los ich zetknął",
                        "przypadek nas zetknął"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести к встрече или знакомству двух людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свести",
                                "target_lang_sense_clarification": "познакомить или соединить людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fe48a99-ddad-3a4f-93de-e0513ea914b3",
                    "sense_definition": "Wejść w gwałtowny konflikt, walkę lub rywalizację z przeciwnikiem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wrogie armie <zetknęły się> o świcie na przedpolach stolicy.",
                            "target_lang_translations": {
                                "ru": "Вражеские армии <столкнулись> на рассвете на подступах к столице."
                            }
                        },
                        {
                            "text": "Dwaj mistrzowie szachowi <zetkną się> w finale turnieju w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Два шахматных мастера <сразятся> в финале турнира на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "ścierać się",
                        "potykać się"
                    ],
                    "common_phrases": [
                        "zetknąć się w walce",
                        "zetknąć się z wrogiem",
                        "zetknąć się w boju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вступить в конфликт, борьбу или соперничество с противником."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столкнуться",
                                "target_lang_sense_clarification": "в бою или конфликте"
                            },
                            {
                                "target_lang_word": "сразиться",
                                "target_lang_sense_clarification": "в состязании или битве"
                            },
                            {
                                "target_lang_word": "сойтись",
                                "target_lang_sense_clarification": "в поединке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
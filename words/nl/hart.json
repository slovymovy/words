                },
                {
                    "sense_id": "197a763a-35d3-3677-89bc-c6e7e8f62e6a",
                    "sense_definition": "De plaats waar men gevoelens en emoties ervaart; het innerlijke karakter.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

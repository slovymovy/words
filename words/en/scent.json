{
    "word_family": [
        "scented",
        "unscented",
        "scentless"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "776540d2-1214-3e1f-82c0-694d0d0666c8",
                    "sense_definition": "A distinctive smell, especially a pleasant one.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "smell_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The air is thick with the unexpected <w>scent</w> of rain.",
                            "target_lang_translations": {
                                "ru": "Воздух наполнен неожиданным <w>запахом</w> дождя."
                            }
                        },
                        {
                            "text": "She loves the sweet <w>scent</w> of fresh roses in the garden.",
                            "target_lang_translations": {
                                "ru": "Она любит сладкий <w>аромат</w> свежих роз в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "aroma",
                        "fragrance",
                        "smell",
                        "odor",
                        "perfume",
                        "bouquet",
                        "redolence"
                    ],
                    "common_phrases": [
                        "scent of flowers",
                        "sweet scent",
                        "fresh scent",
                        "give off a scent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличительный запах, особенно приятный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за́пах",
                                "target_lang_sense_clarification": "общее понятие"
                            },
                            {
                                "target_lang_word": "арома́т",
                                "target_lang_sense_clarification": "приятный запах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4241666-4222-3ba7-8ee9-a766e241f7d8",
                    "sense_definition": "A liquid cosmetic created to provide a pleasant smell, often worn on the body.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "smell_product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He put a few drops of <w>scent</w> on his handkerchief.",
                            "target_lang_translations": {
                                "ru": "Он нанес несколько капель <w>духов</w> на свой носовой платок."
                            }
                        },
                        {
                            "text": "She bought a bottle of expensive <w>scent</w> for her mother.",
                            "target_lang_translations": {
                                "ru": "Она купила флакон дорогих <w>духов</w> для своей матери."
                            }
                        }
                    ],
                    "synonyms": [
                        "perfume",
                        "cologne",
                        "aftershave",
                        "fragrance"
                    ],
                    "common_phrases": [
                        "bottle of scent",
                        "wear scent",
                        "cheap scent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкое косметическое средство для придания приятного запаха, наносимое на тело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "духи́",
                                "target_lang_sense_clarification": "парфюмерия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9103f411-c1d7-3e71-97a0-ac200c63a627",
                    "sense_definition": "A smell left by an animal that allows it to be tracked by other animals.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "tracking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dogs picked up the <w>scent</w> but then quickly lost it.",
                            "target_lang_translations": {
                                "ru": "Собаки взяли <w>след</w>, но затем быстро потеряли его."
                            }
                        },
                        {
                            "text": "The hounds followed the fox's <w>scent</w> through the woods.",
                            "target_lang_translations": {
                                "ru": "Гончие шли по <w>следу</w> лисы через лес."
                            }
                        }
                    ],
                    "synonyms": [
                        "smell"
                    ],
                    "common_phrases": [
                        "pick up the scent",
                        "lose the scent",
                        "follow a scent",
                        "cold scent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запах, оставленный животным, позволяющий другим животным выслеживать его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "след",
                                "target_lang_sense_clarification": "запаховый след животного"
                            },
                            {
                                "target_lang_word": "за́пах",
                                "target_lang_sense_clarification": "запах животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "611d2697-f832-34ed-b692-92a7dd2258f2",
                    "sense_definition": "A trail of clues or evidence that can be followed to find someone or something.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "tracking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tip put the detectives on a false <w>scent</w> during the investigation.",
                            "target_lang_translations": {
                                "ru": "Наводка пустила детективов по ложному <w>следу</w> в ходе расследования."
                            }
                        },
                        {
                            "text": "The journalist is on the <w>scent</w> of a major political scandal.",
                            "target_lang_translations": {
                                "ru": "Журналист напал на <w>след</w> крупного политического скандала."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the scent",
                        "throw off the scent",
                        "false scent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Череда улик или доказательств, по которым можно найти кого-то или что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "след",
                                "target_lang_sense_clarification": "переносное значение: улики"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e0c1051-cf1a-348f-bf7c-baa04b7fb2a9",
                    "sense_definition": "The ability to detect smells, especially in animals.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "faculty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I believe the bloodhound has the best <w>scent</w> of all dogs.",
                            "target_lang_translations": {
                                "ru": "Я считаю, что у бладхаунда лучшее <w>чутьё</w> из всех собак."
                            }
                        },
                        {
                            "text": "The animal uses its keen <w>scent</w> to find food in the forest.",
                            "target_lang_translations": {
                                "ru": "Животное использует свой острый <w>нюх</w>, чтобы находить пищу в лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "olfaction",
                        "smell"
                    ],
                    "common_phrases": [
                        "keen scent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность воспринимать запахи, особенно у животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чутьё",
                                "target_lang_sense_clarification": "охотничье/животное обоняние"
                            },
                            {
                                "target_lang_word": "нюх",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "обоня́ние",
                                "target_lang_sense_clarification": "физиологическое чувство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ed760b71-9c31-3885-baf3-98cc22d72b6b",
                    "sense_definition": "To give a particular smell to something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "impart_smell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>scent</w> the air with burning sage before you begin.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>ароматизировать</w> воздух горящим шалфеем, прежде чем начать."
                            }
                        },
                        {
                            "text": "Blooming flowers <w>scent</w> the garden heavily in spring.",
                            "target_lang_translations": {
                                "ru": "Цветущие цветы сильно <w>наполняют ароматом</w> сад весной."
                            }
                        }
                    ],
                    "synonyms": [
                        "perfume",
                        "odorize"
                    ],
                    "common_phrases": [
                        "scent the air",
                        "scent the room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать чему-либо определенный запах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ароматизи́ровать",
                                "target_lang_sense_clarification": "придавать аромат"
                            },
                            {
                                "target_lang_word": "наполня́ть арома́том",
                                "target_lang_sense_clarification": "насыщать запахом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51c651c8-f246-358d-9a89-6a1a7669b250",
                    "sense_definition": "To recognize or find something by its smell.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "detect_smell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hounds <w>scented</w> the fox in the woods.",
                            "target_lang_translations": {
                                "ru": "Гончие <w>почуяли</w> лису в лесу."
                            }
                        },
                        {
                            "text": "I paused to <w>scent</w> the breeze as I entered the valley.",
                            "target_lang_translations": {
                                "ru": "Я остановился, чтобы <w>ощутить запах</w> ветерка, когда вошел в долину."
                            }
                        }
                    ],
                    "synonyms": [
                        "smell",
                        "sniff",
                        "detect",
                        "sense"
                    ],
                    "common_phrases": [
                        "scent the prey"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распознавать или обнаруживать что-либо по запаху."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чу́ять",
                                "target_lang_sense_clarification": "ощущать запах (о животных)"
                            },
                            {
                                "target_lang_word": "почу́ять",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "027e03d7-31e8-3baf-a6b7-ee844fe66d85",
                    "sense_definition": "To suspect or become aware of a situation or fact that is not obvious.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "detect_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>scented</w> trouble when I saw them running down the hill.",
                            "target_lang_translations": {
                                "ru": "Я <w>почуял</w> неладное, когда увидел, как они бегут с холма."
                            }
                        },
                        {
                            "text": "She could <w>scent</w> a challenge immediately and accepted it.",
                            "target_lang_translations": {
                                "ru": "Она могла сразу <w>почуять</w> вызов и принимала его."
                            }
                        }
                    ],
                    "synonyms": [
                        "sense",
                        "detect",
                        "perceive",
                        "discern"
                    ],
                    "common_phrases": [
                        "scent trouble",
                        "scent danger",
                        "scent a challenge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подозревать или осознавать ситуацию или факт, которые не очевидны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чу́ять",
                                "target_lang_sense_clarification": "интуитивно чувствовать"
                            },
                            {
                                "target_lang_word": "почу́ять",
                                "target_lang_sense_clarification": "почувствовать неладное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "welken",
        "verwelken",
        "verwelking"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "85934db8-653b-31c8-8d27-cd984a177b9b",
                    "sense_definition": "Vragend woord dat gebruikt wordt voor een zelfstandig naamwoord om te vragen naar een specifieke keuze uit een groep.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "interrogative_det",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In <w>welke</w> stad ben jij geboren?",
                            "target_lang_translations": {
                                "en": "In <w>which</w> city were you born?",
                                "ru": "В <w>каком</w> городе ты родился?"
                            }
                        },
                        {
                            "text": "<w>Welk</w> boek ben je op dit moment aan het lezen?",
                            "target_lang_translations": {
                                "en": "<w>Which</w> book are you reading at the moment?",
                                "ru": "<w>Какую</w> книгу ты сейчас читаешь?"
                            }
                        },
                        {
                            "text": "Ik weet niet <w>welke</w> kleur ik moet kiezen voor de muur.",
                            "target_lang_translations": {
                                "en": "I don't know <w>which</w> color to choose for the wall.",
                                "ru": "Я не знаю, <w>какой</w> цвет выбрать для стены."
                            }
                        }
                    ],
                    "synonyms": [
                        "wat voor"
                    ],
                    "common_phrases": [
                        "om het even welk",
                        "welk soort",
                        "op welk moment"
                    ],
                    "target_lang_definitions": {
                        "en": "Interrogative word used before a noun to ask for a specific choice from a group.",
                        "ru": "Вопросительное слово, которое ставится перед существительным, чтобы спросить о конкретном выборе из группы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "which",
                                "target_lang_sense_clarification": "interrogative determiner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "какой",
                                "target_lang_sense_clarification": "общий вопрос о качестве или выборе"
                            },
                            {
                                "target_lang_word": "который",
                                "target_lang_sense_clarification": "вопрос о выборе из определенного ряда или последовательности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "497469e1-2367-31bd-ab72-6be87fe1cde4",
                    "sense_definition": "Vragend voornaamwoord dat zelfstandig gebruikt wordt om te verwijzen naar een persoon of zaak uit een groep.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "interrogative_pron",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er staan hier drie fietsen; <w>welke</w> is van jou?",
                            "target_lang_translations": {
                                "en": "There are three bikes here; <w>which</w> one is yours?",
                                "ru": "Здесь стоят три велосипеда; <w>который</w> из них твой?"
                            }
                        },
                        {
                            "text": "<w>Welke</w> van de twee vind je het mooist?",
                            "target_lang_translations": {
                                "en": "<w>Which</w> of the two do you like best?",
                                "ru": "<w>Какой</w> из двух тебе нравится больше всего?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Interrogative pronoun used independently to refer to a person or thing from a group.",
                        "ru": "Вопросительное местоимение, используемое самостоятельно для указания на лицо или предмет из группы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "which",
                                "target_lang_sense_clarification": "interrogative pronoun"
                            },
                            {
                                "target_lang_word": "which one",
                                "target_lang_sense_clarification": "specific choice"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "который",
                                "target_lang_sense_clarification": "выбор конкретного объекта из группы"
                            },
                            {
                                "target_lang_word": "какой",
                                "target_lang_sense_clarification": "вопрос о выборе (какой именно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a7600f4-b0c7-30ed-9bc1-9b05a62a368b",
                    "sense_definition": "Betrekkelijk voornaamwoord dat terugverwijst naar een eerder genoemd woord (vooral in formele taal of na een voorzetsel).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relative_pron",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte had een wapen bij zich, <w>welke</w> hij in de struiken had verstopt.",
                            "target_lang_translations": {
                                "en": "The suspect had a weapon with him, <w>which</w> he had hidden in the bushes.",
                                "ru": "У подозреваемого было при себе оружие, <w>которое</w> он спрятал в кустах."
                            }
                        },
                        {
                            "text": "Dit zijn de voorwaarden onder <w>welke</w> wij akkoord gaan.",
                            "target_lang_translations": {
                                "en": "These are the conditions under <w>which</w> we agree.",
                                "ru": "Это условия, на <w>которых</w> мы согласны."
                            }
                        }
                    ],
                    "synonyms": [
                        "die",
                        "dat",
                        "hetgeen"
                    ],
                    "common_phrases": [
                        "de redenen om welke"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "in spreektaal vaak vervangen door 'die' of 'dat'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Relative pronoun referring back to a previously mentioned word (often formal or after a preposition).",
                        "ru": "Относительное местоимение, отсылающее к ранее упомянутому слову (часто используется в формальном стиле или после предлогов)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "which",
                                "target_lang_sense_clarification": "relative pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "который",
                                "target_lang_sense_clarification": "связующее слово в придаточном предложении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "198bc07d-a0d3-3639-810f-311e66c0112b",
                    "sense_definition": "Uitroepend voornaamwoord om verbazing, bewondering of afkeer uit te drukken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "exclamatory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Welk</w> een prachtig uitzicht hebben we hier!",
                            "target_lang_translations": {
                                "en": "<w>What</w> a beautiful view we have here!",
                                "ru": "<w>Какой</w> прекрасный вид открывается отсюда!"
                            }
                        },
                        {
                            "text": "O, <w>welk</w> een ellende heeft deze oorlog veroorzaakt.",
                            "target_lang_translations": {
                                "en": "Oh, <w>what</w> misery this war has caused.",
                                "ru": "О, <w>какое</w> горе принесла эта война."
                            }
                        }
                    ],
                    "synonyms": [
                        "wat",
                        "wat een"
                    ],
                    "common_phrases": [
                        "welk een"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literair of plechtig"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamatory pronoun to express surprise, admiration, or aversion.",
                        "ru": "Восклицательное местоимение для выражения удивления, восхищения или неприязни."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "what",
                                "target_lang_sense_clarification": "exclamatory"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "какой",
                                "target_lang_sense_clarification": "для усиления эмоциональной окраски (какой же, что за)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2dc41c3e-6e4b-3f70-a20e-70722795f8fb",
                    "sense_definition": "Vervoeging van <w>welken</w>: slap worden en uitdrogen (van planten).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>welk</w> helemaal weg in deze ondraaglijke hitte.",
                            "target_lang_translations": {
                                "en": "I am <w>wilting</w> completely in this unbearable heat.",
                                "ru": "Я совсем <w>вяну</w> на этой невыносимой жаре."
                            }
                        },
                        {
                            "text": "<w>Welk</w> niet, kleine bloem, de regen komt eraan.",
                            "target_lang_translations": {
                                "en": "Do not <w>wilt</w>, little flower, the rain is coming.",
                                "ru": "Не <w>вянь</w>, маленький цветок, дождь уже близко."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwelken",
                        "uitdrogen"
                    ],
                    "antonyms": [
                        "bloeien"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "eerste persoon enkelvoud / gebiedende wijs"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugation of 'welken': to become limp and dry out (of plants).",
                        "ru": "Форма глагола 'welken': становиться вялым и высыхать (о растениях)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wilt",
                                "target_lang_sense_clarification": "verb form"
                            },
                            {
                                "target_lang_word": "wither",
                                "target_lang_sense_clarification": "verb form"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вянуть",
                                "target_lang_sense_clarification": "терять свежесть, сохнуть"
                            },
                            {
                                "target_lang_word": "увядать",
                                "target_lang_sense_clarification": "становиться вялым, отцветать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
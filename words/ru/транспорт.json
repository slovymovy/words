                        "транспортировка",
                        "доставка"
                    ],
                    "common_phrases": [
                        "транспорт грузов"
                    ],
                    "target_lang_definitions": {
                        "en": "The act or process of moving goods or people from one place to another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transportation",
                                "target_lang_sense_clarification": "act of moving"
                            },
                            {
                                "target_lang_word": "transport",
                                "target_lang_sense_clarification": "act of moving"
                            },
                            {
                                "target_lang_word": "shipping",
                                "target_lang_sense_clarification": "commercial transport of goods"

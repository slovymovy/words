        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17f8070e-72c4-31fe-bf3d-e597da88d9fd",
                    "sense_definition": "Vervoeging van <w>balen</w>: eerste persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>baal</w> ervan dat de trein weer vertraging heeft.",
                            "target_lang_translations": {
                                "ru": "Я <w>расстроен</w> тем, что поезд снова опаздывает.",
                                "en": "I <w>am bummed</w> that the train is delayed again."
                            }
                        },
                        {
                            "text": "Soms <w>baal</w> ik echt van al dat regenachtige weer.",
                            "target_lang_translations": {
                                "ru": "Иногда я действительно <w>устаю</w> от всей этой дождливой погоды.",
                                "en": "Sometimes I really <w>get fed up</w> with all that rainy weather."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik baal",
                        "stevig balen",
                        "er van balen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа глагола \"balen\": быть недовольным, расстроенным или сытым по горло чем-либо.",
                        "en": "First-person singular form of the verb \"balen\": to be fed up, disappointed, or annoyed by a situation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расстраиваться",
                                "target_lang_sense_clarification": "быть недовольным ситуацией"
                            },
                            {
                                "target_lang_word": "быть сытым по горло",
                                "target_lang_sense_clarification": "сильно устать от чего-либо"
                            },
                            {
                                "target_lang_word": "беситься",
                                "target_lang_sense_clarification": "разговорное: злиться, раздражаться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "be fed up",
                                "target_lang_sense_clarification": "to be tired of or angry about something"
                            },
                            {
                                "target_lang_word": "be bummed out",
                                "target_lang_sense_clarification": "informal: to be disappointed or upset"
                            }
                        ]
                    }
                }
            ]
        },
        {

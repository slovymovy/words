    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f738edff-6274-3a44-aa30-ccb1ffd373b3",
                    "sense_definition": "Сесть рядом с кем-либо или чем-либо, занять место поблизости.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я решил <w>подсесть</w> к ним за столик в кафе, чтобы поговорить.",
                            "target_lang_translations": {
                                "en": "I decided to <w>join</w> them at the table in the cafe to have a talk."
                            }
                        },
                        {
                            "text": "Она тихо <w>подсела</w> к камину, чтобы немного согреться после прогулки.",
                            "target_lang_translations": {
                                "en": "She quietly <w>sat down by</w> the fireplace to warm up a bit after the walk."
                            }
                        }
                    ],
                    "synonyms": [
                        "присесть",
                        "сесть"
                    ],
                    "antonyms": [
                        "отсесть"
                    ],
                    "common_phrases": [
                        "подсесть к столу",
                        "подсесть поближе",
                        "можно к вам подсесть"
                    ],
                    "target_lang_definitions": {
                        "en": "To sit down next to someone or something, or to take a seat nearby."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sit down next to",
                                "target_lang_sense_clarification": "physical action of taking a seat beside someone"
                            },
                            {
                                "target_lang_word": "join",
                                "target_lang_sense_clarification": "to sit with a group or person"
                            },
                            {
                                "target_lang_word": "sit by",
                                "target_lang_sense_clarification": "to sit near something (e.g., a fire)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f67b4047-5fbc-392d-bc00-9ae42904dab1",
                    "sense_definition": "Пристраститься к чему-либо, приобрести сильную зависимость или увлеченность (наркотиками, играми, сериалами и т. п.).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "dependency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат конкретно <w>подсел</w> на новые компьютерные игры.",
                            "target_lang_translations": {
                                "en": "My brother got totally <w>hooked</w> on new computer games."
                            }
                        },
                        {
                            "text": "Многие люди боятся <w>подсесть</w> на лекарства и стараются лечиться народными средствами.",
                            "target_lang_translations": {
                                "en": "Many people are afraid to <w>get addicted</w> to medication and try to treat themselves with folk remedies."
                            }
                        }
                    ],
                    "antonyms": [
                        "слезть"
                    ],
                    "common_phrases": [
                        "подсесть на иглу",
                        "подсесть на сериал",
                        "подсесть на игру"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used to describe addiction or strong habit"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become addicted to something or develop a strong habit or fascination (drugs, games, TV series, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get hooked on",
                                "target_lang_sense_clarification": "slang/informal, for hobbies or addictions"
                            },
                            {
                                "target_lang_word": "get addicted to",
                                "target_lang_sense_clarification": "formal or serious dependency"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ab7f30a-6dfc-367e-b54a-64e0d9eadfb0",
                    "sense_definition": "Потерять часть заряда (об аккумуляторе) или физических сил; ослабеть.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "energy_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня в телефоне сильно <w>подсел</w> аккумулятор, поэтому я не мог позвонить.",
                            "target_lang_translations": {
                                "en": "The battery in my phone <w>ran down</w> significantly, so I couldn't make a call."
                            }
                        },
                        {
                            "text": "К концу третьего раунда боксер заметно <w>подсел</w> и стал пропускать удары.",
                            "target_lang_translations": {
                                "en": "By the end of the third round, the boxer noticeably <w>weakened</w> and started missing punches."
                            }
                        }
                    ],
                    "antonyms": [
                        "подзарядиться"
                    ],
                    "common_phrases": [
                        "батарейка подсела",
                        "голос подсел",
                        "аккумулятор подсел"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To lose some charge (regarding a battery) or physical strength; to weaken."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run down",
                                "target_lang_sense_clarification": "regarding batteries or energy levels"
                            },
                            {
                                "target_lang_word": "weaken",
                                "target_lang_sense_clarification": "regarding physical strength or voice"
                            },
                            {
                                "target_lang_word": "get low",
                                "target_lang_sense_clarification": "regarding battery charge"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "008968e0-1d00-3d49-b72a-6ac42c1efece",
                    "sense_definition": "Сесть на какой-либо попутный транспорт или войти в транспортное средство по пути.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы успели <w>подсесть</w> в последний вагон уходящего поезда.",
                            "target_lang_translations": {
                                "en": "We managed to <w>hop into</w> the last carriage of the departing train."
                            }
                        },
                        {
                            "text": "Туристы попросили водителя грузовика разрешить им <w>подсесть</w> в кабину.",
                            "target_lang_translations": {
                                "en": "The tourists asked the truck driver to let them <w>hop into</w> the cab."
                            }
                        }
                    ],
                    "antonyms": [
                        "сойти",
                        "выйти"
                    ],
                    "common_phrases": [
                        "подсесть в машину",
                        "подсесть на поезд"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To get on a passing vehicle or enter a vehicle along the way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hop on",
                                "target_lang_sense_clarification": "informal, boarding a vehicle quickly"
                            },
                            {
                                "target_lang_word": "jump in",
                                "target_lang_sense_clarification": "entering a car or enclosed vehicle"
                            },
                            {
                                "target_lang_word": "get on",
                                "target_lang_sense_clarification": "general boarding of transport"
                            }
                        ]
                    }
                }
            ]
        }

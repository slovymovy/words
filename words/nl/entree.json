{
    "word_family": [
        "entreegeld",
        "entreebewijs",
        "entreekaart"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5525e6d3-9a40-3265-be59-68ffd8ea2977",
                    "sense_definition": "De plaats waar men een gebouw binnenkomt, vaak een hal of gang direct achter de voordeur.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_entrance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>entree</w> van het nieuwe kantoor is erg licht en ruim.",
                            "target_lang_translations": {
                                "ru": "<w>Вход</w> в новый офис очень светлый и просторный."
                            }
                        },
                        {
                            "text": "Wacht je op mij bij de <w>entree</w> van de bioscoop?",
                            "target_lang_translations": {
                                "ru": "Ты подождешь меня у <w>входа</w> в кинотеатр?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ingang",
                        "hal",
                        "vestibule"
                    ],
                    "common_phrases": [
                        "bij de entree",
                        "de hoofdentree",
                        "ruime entree"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место входа в здание, часто холл или коридор сразу за входной дверью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вход",
                                "target_lang_sense_clarification": "место входа"
                            },
                            {
                                "target_lang_word": "вестибюль",
                                "target_lang_sense_clarification": "просторное помещение при входе"
                            },
                            {
                                "target_lang_word": "прихожая",
                                "target_lang_sense_clarification": "в жилом доме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed0c7ae7-f4ee-3c9d-8157-757e8ecea161",
                    "sense_definition": "Het bedrag dat betaald moet worden om ergens naar binnen te mogen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "price",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kinderen tot twaalf jaar hoeven geen <w>entree</w> te betalen.",
                            "target_lang_translations": {
                                "ru": "Детям до двенадцати лет не нужно платить за <w>вход</w>."
                            }
                        },
                        {
                            "text": "De <w>entree</w> voor het concert was behoorlijk duur.",
                            "target_lang_translations": {
                                "ru": "<w>Входная плата</w> за концерт была довольно высокой."
                            }
                        }
                    ],
                    "synonyms": [
                        "toegangsprijs"
                    ],
                    "common_phrases": [
                        "entree betalen",
                        "gratis entree",
                        "inclusief entree"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумма, которую необходимо заплатить, чтобы войти куда-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "входная плата",
                                "target_lang_sense_clarification": "стоимость посещения"
                            },
                            {
                                "target_lang_word": "плата за вход",
                                "target_lang_sense_clarification": "деньги за вход"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fee74581-be64-3613-a58e-b22a00a9a19f",
                    "sense_definition": "Het recht of de mogelijkheid om ergens binnen te gaan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "access_right",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zonder uitnodiging heb je geen <w>entree</w> tot het feest.",
                            "target_lang_translations": {
                                "ru": "Без приглашения у тебя нет <w>доступа</w> на вечеринку."
                            }
                        },
                        {
                            "text": "De bewaker weigerde hem de <w>entree</w> omdat hij geen pasje had.",
                            "target_lang_translations": {
                                "ru": "Охранник отказал ему во <w>входе</w>, так как у него не было пропуска."
                            }
                        }
                    ],
                    "synonyms": [
                        "toegang"
                    ],
                    "common_phrases": [
                        "vrije entree",
                        "entree weigeren",
                        "entree verschaffen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Право или возможность войти куда-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вход",
                                "target_lang_sense_clarification": "право доступа"
                            },
                            {
                                "target_lang_word": "доступ",
                                "target_lang_sense_clarification": "возможность войти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d15040a5-a2b9-34d3-a940-fad1633cafa6",
                    "sense_definition": "Een gerecht dat aan het begin van de maaltijd, voor het hoofdgerecht, wordt geserveerd.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bestelden een lichte salade als <w>entree</w>.",
                            "target_lang_translations": {
                                "ru": "Мы заказали легкий салат в качестве <w>закуски</w>."
                            }
                        },
                        {
                            "text": "Het menu bestond uit een <w>entree</w>, een hoofdgerecht en een toetje.",
                            "target_lang_translations": {
                                "ru": "Меню состояло из <w>закуски</w>, основного блюда и десерта."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorgerecht",
                        "voorspijs"
                    ],
                    "common_phrases": [
                        "als entree",
                        "koude entree",
                        "warme entree"
                    ],
                    "target_lang_definitions": {
                        "ru": "Блюдо, которое подается в начале трапезы, перед основным блюдом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закуска",
                                "target_lang_sense_clarification": "первое блюдо"
                            },
                            {
                                "target_lang_word": "антре",
                                "target_lang_sense_clarification": "кулинарный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
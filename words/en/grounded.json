{
    "word_family": [
        "grounding",
        "groundless",
        "ungrounded",
        "groundwork"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1bed75a3-aa67-3cfe-9f31-97d12cc30dd2",
                    "sense_definition": "Not allowed to leave the house or participate in social activities as a punishment, usually applied to children or teenagers.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was <grounded> for a week because he failed his math test.",
                            "target_lang_translations": {
                                "ru": "Его <наказали> на неделю, потому что он провалил тест по математике."
                            }
                        },
                        {
                            "text": "My parents said I am <grounded> until I clean my room.",
                            "target_lang_translations": {
                                "ru": "Родители сказали, что я <наказан> (не выйду из дома), пока не уберусь в своей комнате."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grounded for life",
                        "grounded for a week",
                        "get grounded"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наказанный родителями (обычно запретом выходить из дома или гулять с друзьями)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наказан",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "под домашним арестом",
                                "target_lang_sense_clarification": "ироничное или сленговое выражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ae75a23-d8cb-3233-ac50-67fac9053613",
                    "sense_definition": "Prohibited from flying, usually due to bad weather, mechanical issues, or regulations.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "All flights were <grounded> due to the heavy fog.",
                            "target_lang_translations": {
                                "ru": "Все самолеты <остались на земле> из-за сильного тумана."
                            }
                        },
                        {
                            "text": "The pilot was <grounded> pending a medical examination.",
                            "target_lang_translations": {
                                "ru": "Пилот был <отстранён от полётов> до прохождения медицинского осмотра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grounded flight",
                        "grounded aircraft",
                        "stay grounded"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не допущенный к полетам или оставленный на земле (о самолетах или пилотах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отстранённый от полётов",
                                "target_lang_sense_clarification": "о пилоте"
                            },
                            {
                                "target_lang_word": "оставленный на земле",
                                "target_lang_sense_clarification": "о самолёте или рейсе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0becea1e-0dbd-3913-9987-e0c385e04c4b",
                    "sense_definition": "Connected to the earth as a safety measure for electricity.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure the washing machine is plugged into a <grounded> outlet.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что стиральная машина подключена к <заземлённой> розетке."
                            }
                        },
                        {
                            "text": "The electrician checked to ensure the wiring was properly <grounded>.",
                            "target_lang_translations": {
                                "ru": "Электрик проверил, правильно ли <заземлена> проводка."
                            }
                        }
                    ],
                    "synonyms": [
                        "earthed"
                    ],
                    "common_phrases": [
                        "grounded outlet",
                        "grounded wire"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in US; 'earthed' is common in UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединенный с землей проводником для безопасности (об электричестве)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заземлённый",
                                "target_lang_sense_clarification": "электрика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fbb79f4-1121-39bd-85df-970f1b123316",
                    "sense_definition": "Having a sensible, realistic, and stable attitude toward life; not arrogant or flighty.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Despite her fame, she remained a very <grounded> person.",
                            "target_lang_translations": {
                                "ru": "Несмотря на свою славу, она оставалась очень <уравновешенным> человеком."
                            }
                        },
                        {
                            "text": "He is a <grounded> leader who always listens to his team.",
                            "target_lang_translations": {
                                "ru": "Он <здравомыслящий> лидер, который всегда прислушивается к своей команде."
                            }
                        }
                    ],
                    "synonyms": [
                        "sensible",
                        "down-to-earth",
                        "level-headed"
                    ],
                    "antonyms": [
                        "flighty",
                        "pretentious",
                        "spacy",
                        "out of touch"
                    ],
                    "common_phrases": [
                        "stay grounded",
                        "remain grounded",
                        "keep someone grounded"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий здравым смыслом, реалистичный и устойчивый психологически."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уравновешенный",
                                "target_lang_sense_clarification": "о характере"
                            },
                            {
                                "target_lang_word": "здравомыслящий",
                                "target_lang_sense_clarification": "реалистичный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1901a7b2-216f-3b09-bc37-e0bcccbb818d",
                    "sense_definition": "Stuck on the bottom of a body of water; unable to move because the water is too shallow.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <grounded> ship had to wait for the high tide to float again.",
                            "target_lang_translations": {
                                "ru": "<Севший на мель> корабль должен был ждать прилива, чтобы снова поплыть."
                            }
                        },
                        {
                            "text": "Rescue boats were sent to help the <grounded> vessel near the reef.",
                            "target_lang_translations": {
                                "ru": "Спасательные катера были отправлены на помощь <севшему на мель> судну у рифа."
                            }
                        }
                    ],
                    "synonyms": [
                        "aground"
                    ],
                    "antonyms": [
                        "flighty"
                    ],
                    "common_phrases": [
                        "grounded vessel",
                        "grounded ship"
                    ],
                    "target_lang_definitions": {
                        "ru": "Застрявший на дне водоема, неспособный двигаться из-за мелководья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "севший на мель",
                                "target_lang_sense_clarification": "о судне"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9647d8f4-2bc1-3451-9c48-43c98425ab4c",
                    "sense_definition": "Simple past and past participle of <w>ground</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The airline <grounded> the plane for repairs.",
                            "target_lang_translations": {
                                "ru": "Авиакомпания <запретила вылет> самолету для проведения ремонта."
                            }
                        },
                        {
                            "text": "His theory was <grounded> in years of scientific research.",
                            "target_lang_translations": {
                                "ru": "Его теория была <основана> на годах научных исследований."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense / participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прошедшего времени глагола ground (основывать, запрещать вылет, заземлять)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основан",
                                "target_lang_sense_clarification": "базироваться на чем-то"
                            },
                            {
                                "target_lang_word": "запретил вылет",
                                "target_lang_sense_clarification": "авиация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
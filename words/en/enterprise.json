    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "76caecff-0f05-32fb-b9f9-535d12c38767",
                    "sense_definition": "A company, business, organization, or other purposeful commercial endeavor.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She started a small <w>enterprise</w> selling handmade jewelry online.",
                            "target_lang_translations": {
                                "ru": "Она основала небольшое <w>предприятие</w> по продаже украшений ручной работы в интернете."
                            }
                        },
                        {
                            "text": "The state-owned <w>enterprise</w> provides electricity to the entire region.",
                            "target_lang_translations": {
                                "ru": "Это государственное <w>предприятие</w> обеспечивает электричеством весь регион."
                            }
                        }
                    ],
                    "synonyms": [
                        "business",
                        "company",
                        "firm"
                    ],
                    "common_phrases": [
                        "private enterprise",
                        "commercial enterprise",
                        "state-owned enterprise",
                        "small and medium enterprise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компания, бизнес, организация или иная целенаправленная коммерческая деятельность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предприятие",
                                "target_lang_sense_clarification": "организация, завод, фирма"
                            },
                            {
                                "target_lang_word": "компания",
                                "target_lang_sense_clarification": "коммерческая организация"
                            },
                            {
                                "target_lang_word": "бизнес",
                                "target_lang_sense_clarification": "дело, предпринимательство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d9dbdb6-edde-3b81-81a4-5bae4919a134",
                    "sense_definition": "A difficult or important project, undertaking, or venture, especially one that requires effort and courage.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "project_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Building the bridge was a massive engineering <w>enterprise</w> that took ten years.",
                            "target_lang_translations": {
                                "ru": "Строительство моста было грандиозным инженерным <w>предприятием</w>, которое заняло десять лет."
                            }
                        },
                        {
                            "text": "They agreed to fund the joint <w>enterprise</w> between the two research institutes.",
                            "target_lang_translations": {
                                "ru": "Они согласились финансировать совместное <w>начинание</w> двух научно-исследовательских институтов."
                            }
                        }
                    ],
                    "synonyms": [
                        "undertaking",
                        "venture",
                        "endeavor",
                        "pursuit",
                        "activity"
                    ],
                    "common_phrases": [
                        "joint enterprise",
                        "criminal enterprise",
                        "vast enterprise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сложный или важный проект, начинание или авантюра, особенно требующие усилий и смелости."

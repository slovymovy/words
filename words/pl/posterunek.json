            "pos": "noun",
            "senses": [
                {
                    "sense_id": "27a07442-cc55-3fa5-b99c-911c3ccf23b7",
                    "sense_definition": "Siedziba jednostki policji lub innej służby mundurowej, zwykle mniejsza niż komisariat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz udać się na <w>posterunek</w>, aby zgłosić kradzież dokumentów.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно пойти в <w>участок</w>, чтобы заявить о краже документов."
                            }
                        },
                        {
                            "text": "Policjanci zabrali podejrzanego na <w>posterunek</w> w celu przesłuchania.",
                            "target_lang_translations": {
                                "ru": "Полицейские доставили подозреваемого в <w>участок</w> для допроса."
                            }
                        }
                    ],
                    "synonyms": [
                        "komisariat"
                    ],
                    "common_phrases": [
                        "posterunek policji",
                        "komendant posterunku",
                        "zgłosić na posterunek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место расположения подразделения полиции или другой службы, обычно меньше комиссариата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "полицейский"
                            },
                            {
                                "target_lang_word": "отделение",
                                "target_lang_sense_clarification": "полиции"
                            },
                            {
                                "target_lang_word": "пост",
                                "target_lang_sense_clarification": "полиции или охраны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1ffc6f7-9102-34ef-aa53-e3f5990405d0",
                    "sense_definition": "Wyznaczone miejsce, w którym żołnierz lub funkcjonariusz pełni straż, obserwację albo dyżur.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kapral surowo zabronił żołnierzom opuszczania wyznaczonego <w>posterunku</w>.",
                            "target_lang_translations": {
                                "ru": "Капрал строго запретил солдатам покидать назначенный <w>пост</w>."
                            }
                        },
                        {
                            "text": "Przez całą noc harcerze stali na <w>posterunku</w> przy obozowym ognisku.",
                            "target_lang_translations": {
                                "ru": "Всю ночь скауты стояли на <w>посту</w> у лагерного костра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stać na posterunku",
                        "trwać na posterunku",
                        "opuścić posterunek",
                        "posterunek graniczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специально отведенное место, где солдат или должностное лицо несет караул, ведет наблюдение или дежурство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пост",
                                "target_lang_sense_clarification": "место дежурства"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "9a8317b3-40af-3c5e-9f7e-59e7cdb47dd1",
                    "sense_definition": "Lacking earnestness, depth, or genuine commitment in behavior or attitude.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is an <w>unserious</w> candidate who has no real plan for the future.",
                            "target_lang_translations": {
                                "ru": "Он <w>несерьёзный</w> кандидат, у которого нет реального плана на будущее."
                            }
                        },
                        {
                            "text": "The band played a delightfully <w>unserious</w> song that made everyone smile.",
                            "target_lang_translations": {
                                "ru": "Группа сыграла восхитительно <w>несерьёзную</w> песню, которая заставила всех улыбнуться."
                            }
                        }
                    ],
                    "synonyms": [
                        "frivolous",
                        "facetious",
                        "flippant",
                        "nonserious"
                    ],
                    "antonyms": [
                        "serious",
                        "earnest",
                        "committed"
                    ],
                    "common_phrases": [
                        "unserious person",
                        "unserious attitude"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишённый серьёзности, глубины или искренней приверженности в поведении или отношении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несерьёзный",
                                "target_lang_sense_clarification": "об отношении или человеке"
                            },
                            {
                                "target_lang_word": "легкомысленный",
                                "target_lang_sense_clarification": "поверхностный, ветреный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71355b6c-691d-36ca-88e3-adbff9578580",
                    "sense_definition": "Not severe, dangerous, or critical.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fortunately, the injury was <w>unserious</w> and required only a bandage.",
                            "target_lang_translations": {

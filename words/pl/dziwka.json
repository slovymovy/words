                {
                    "sense_id": "ae36bd83-f2a9-3fa3-9999-ff6f0f48edb9",
                    "sense_definition": "Wulgarne określenie kobiety świadczącej usługi seksualne za pieniądze.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sex_worker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej dzielnicy często można spotkać <w>dziwki</w> czekające na klientów.",
                            "target_lang_translations": {
                                "ru": "В этом районе часто можно встретить <w>шлюх</w>, ожидающих клиентов."
                            }
                        },
                        {
                            "text": "Mieszkańcy skarżyli się, że pod ich oknami stoją <w>dziwki</w>.",
                            "target_lang_translations": {
                                "ru": "Жители жаловались, что под их окнами стоят <w>проститутки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "prostytutka",
                        "kurwa",
                        "ladacznica",
                        "nierządnica",
                        "córa Koryntu"
                    ],
                    "common_phrases": [
                        "tania dziwka",
                        "luksusowa dziwka"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "wulgarnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное обозначение женщины, оказывающей сексуальные услуги за деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлюха",
                                "target_lang_sense_clarification": "вульгарно"
                            },
                            {
                                "target_lang_word": "блядь",
                                "target_lang_sense_clarification": "грубое ругательство"
                            },
                            {
                                "target_lang_word": "проститутка",
                                "target_lang_sense_clarification": "нейтральное/медицинское"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c33edab7-1e2a-346a-8d88-d1cca9cb72fb",
                    "sense_definition": "Wulgarnie o kobiecie rozwiązłej seksualnie lub ogólne, bardzo obraźliwe wyzwisko pod adresem nielubianej kobiety.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę mieć z tą <w>dziwką</w> nic wspólnego po tym, jak mnie oszukała.",
                            "target_lang_translations": {
                                "ru": "Я не хочу иметь ничего общего с этой <w>сукой</w> после того, как она меня обманула."
                            }
                        },
                        {
                            "text": "Ta wredna <w>dziwka</w> znowu robi awanturę o byle co.",
                            "target_lang_translations": {
                                "ru": "Эта злобная <w>стерва</w> снова устраивает скандал из-за пустяков."
                            }
                        }
                    ],
                    "synonyms": [
                        "szmata",
                        "wywłoka",
                        "zdzira",
                        "lafirynda",
                        "kurwa"
                    ],
                    "common_phrases": [
                        "głupia dziwka"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "wulgarnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное слово для распутной женщины или общее, очень оскорбительное ругательство в адрес женщины."
                    },
                    "translations": {

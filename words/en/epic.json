{
    "word_family": [
        "epical",
        "epically"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "65dea8e5-93ee-3030-8e67-0f3720950a49",
                    "sense_definition": "Momentously heroic or grand in scale and character.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "grandeur",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The explorers set off on an <w>epic</w> journey across the frozen continent.",
                            "target_lang_translations": {
                                "ru": "Исследователи отправились в <w>эпическое</w> путешествие по ледяному континенту."
                            }
                        },
                        {
                            "text": "The movie depicts an <w>epic</w> battle between the forces of good and evil.",
                            "target_lang_translations": {
                                "ru": "Фильм изображает <w>эпическую</w> битву между силами добра и зла."
                            }
                        }
                    ],
                    "synonyms": [
                        "extraordinary",
                        "momentous",
                        "remarkable"
                    ],
                    "common_phrases": [
                        "epic scale",
                        "epic proportions",
                        "epic journey",
                        "epic struggle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грандиозный по масштабу или характеру; героический."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эпический",
                                "target_lang_sense_clarification": "величественный, героический"
                            },
                            {
                                "target_lang_word": "грандиозный",
                                "target_lang_sense_clarification": "огромный по масштабу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ba7080a-14be-3e6d-88f0-d183e59654b2",
                    "sense_definition": "Extremely impressive, enjoyable, or intense; extending beyond the usual or ordinary.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We had an <w>epic</w> time at the concert last night.",
                            "target_lang_translations": {
                                "ru": "Мы <w>эпично</w> провели время на концерте вчера вечером."
                            }
                        },
                        {
                            "text": "He made an <w>epic</w> mistake by forgetting to save his work.",
                            "target_lang_translations": {
                                "ru": "Он совершил <w>грандиозную</w> ошибку, забыв сохранить свою работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "epic fail",
                        "epic win"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used to describe something extremely good or extreme in nature"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "common in informal speech and internet culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно впечатляющий, невероятный или интенсивный (разг.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эпичный",
                                "target_lang_sense_clarification": "сленг: впечатляющий, крутой"
                            },
                            {
                                "target_lang_word": "грандиозный",
                                "target_lang_sense_clarification": "используется для усиления (напр., провал)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25490d23-c0b2-39cb-b931-84b73affdad9",
                    "sense_definition": "Of or relating to an epic poem or legend.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "literature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor specializes in <w>epic</w> poetry from the medieval period.",
                            "target_lang_translations": {
                                "ru": "Профессор специализируется на <w>эпической</w> поэзии средневекового периода."
                            }
                        },
                        {
                            "text": "Beowulf is a classic example of the <w>epic</w> tradition in literature.",
                            "target_lang_translations": {
                                "ru": "«Беовульф» — классический пример <w>эпической</w> традиции в литературе."
                            }
                        }
                    ],
                    "synonyms": [
                        "epical"
                    ],
                    "common_phrases": [
                        "epic poetry",
                        "epic hero",
                        "epic tradition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к эпосу или героическим сказаниям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эпический",
                                "target_lang_sense_clarification": "литературный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec00bd65-e883-3062-9b39-bc828a44acad",
                    "sense_definition": "An extended narrative poem in elevated language, celebrating the feats of a legendary hero.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Odyssey is a famous Greek <w>epic</w> attributed to Homer.",
                            "target_lang_translations": {
                                "ru": "«Одиссея» — знаменитый греческий <w>эпос</w>, приписываемый Гомеру."
                            }
                        },
                        {
                            "text": "Students must read at least one ancient <w>epic</w> during the course.",
                            "target_lang_translations": {
                                "ru": "Студенты должны прочитать хотя бы один древний <w>эпос</w> во время курса."
                            }
                        }
                    ],
                    "synonyms": [
                        "epos"
                    ],
                    "common_phrases": [
                        "epic poem",
                        "national epic",
                        "ancient epic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная повествовательная поэма, воспевающая подвиги легендарного героя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эпос",
                                "target_lang_sense_clarification": "жанр или произведение"
                            },
                            {
                                "target_lang_word": "эпическая поэма",
                                "target_lang_sense_clarification": "конкретное произведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6651322a-267c-3674-9963-73b0bb7ee1d0",
                    "sense_definition": "A long film, book, or series of events portraying heroic deeds and adventures.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "grandeur",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The director is filming a historical <w>epic</w> set in ancient Rome.",
                            "target_lang_translations": {
                                "ru": "Режиссёр снимает историческую <w>эпопею</w>, действие которой происходит в Древнем Риме."
                            }
                        },
                        {
                            "text": "Her survival in the wilderness was a true <w>epic</w> of endurance.",
                            "target_lang_translations": {
                                "ru": "Её выживание в дикой природе стало настоящей <w>эпопеей</w> выносливости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "historical epic",
                        "Hollywood epic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Масштабное произведение (фильм, книга) или длинная череда событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эпопея",
                                "target_lang_sense_clarification": "длинная история или серия событий"
                            },
                            {
                                "target_lang_word": "киноэпопея",
                                "target_lang_sense_clarification": "масштабный фильм"
                            }
                        ]
                    }
                }
            ]
        }
    ]

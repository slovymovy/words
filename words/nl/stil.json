                            {
                                "target_lang_word": "silently",
                                "target_lang_sense_clarification": "without making any sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тихо",
                                "target_lang_sense_clarification": "с малым количеством шума"
                            },
                            {
                                "target_lang_word": "бесшумно",
                                "target_lang_sense_clarification": "без звука"
                            }
                        ]

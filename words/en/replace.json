                    "frequency": "Middle",
                    "semantic_group_id": "restoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When you have finished using the telephone, please <w>replace</w> the handset."
                        },
                        {
                            "text": "She carefully <w>replaced</w> the book on the shelf after reading it."
                        }
                    ],
                    "common_phrases": [

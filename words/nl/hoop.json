                {
                    "sense_id": "d00281cc-32bf-355c-a708-f03fb40c6e04",
                    "sense_definition": "Uitwerpselen, meestal van een dier.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "excrement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pas op dat je niet in die <w>hoop</w> van de hond stapt.",
                            "target_lang_translations": {
                                "en": "Watch out you don't step in that <w>pile</w> from the dog.",
                                "ru": "Осторожно, не наступи в эту собачью <w>кучу</w>."
                            }
                        },
                        {
                            "text": "De boer schepte de <w>hoop</w> mest op de kar om het land te bemesten.",
                            "target_lang_translations": {
                                "en": "The farmer shoveled the <w>heap</w> of manure onto the cart to fertilize the land.",
                                "ru": "Фермер погрузил <w>кучу</w> навоза на телегу, чтобы удобрить землю."
                            }

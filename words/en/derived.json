        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "407dfb44-ad60-3875-ac8d-e37acbf9d5b7",
                    "sense_definition": "Formed or developed from a specific source or origin rather than being original.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author had to prove that his book was not a <w>derived</w> work.",
                            "target_lang_translations": {
                                "ru": "Автору пришлось доказывать, что его книга не является <w>производным</w> произведением."
                            }
                        },
                        {
                            "text": "In computer programming, a <w>derived</w> class inherits properties from a base class.",
                            "target_lang_translations": {
                                "ru": "В программировании <w>производный</w> класс наследует свойства базового класса."
                            }
                        }
                    ],
                    "synonyms": [
                        "secondary",
                        "resultant"
                    ],
                    "antonyms": [
                        "original",
                        "primary",
                        "fundamental"
                    ],
                    "common_phrases": [
                        "derived from",
                        "derived work",
                        "derived demand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сформированный или развитый из определенного источника, не являющийся оригинальным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "производный",
                                "target_lang_sense_clarification": "образованный от другого объекта или понятия"
                            },
                            {
                                "target_lang_word": "вторичный",
                                "target_lang_sense_clarification": "не основной, возникший на базе чего-то другого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37a47957-8a3b-309a-81ad-f00fd1783f61",
                    "sense_definition": "Relating to features or traits in a species that evolved later and were not present in the ancestral species.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Feathers are considered a <w>derived</w> characteristic unique to birds.",
                            "target_lang_translations": {
                                "ru": "Перья считаются <w>производным</w> признаком, уникальным для птиц."
                            }
                        },
                        {
                            "text": "The scientist analyzed the <w>derived</w> traits to understand the evolutionary path.",
                            "target_lang_translations": {
                                "ru": "Ученый проанализировал <w>производные</w> черты, чтобы понять путь эволюции."
                            }
                        }
                    ],
                    "synonyms": [
                        "evolved",
                        "advanced"
                    ],
                    "antonyms": [

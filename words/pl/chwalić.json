    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a8b6570a-8082-3262-b5f3-17aaa2e5fb9e",
                    "sense_definition": "Mówić o kimś lub o czymś dobrze, wyrażając uznanie, podziw lub aprobatę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "praise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel często <chwali> uczniów za dobre wyniki w nauce i aktywność.",
                            "target_lang_translations": {
                                "ru": "Учитель часто <хвалит> учеников за хорошие результаты в учебе и активность."
                            }
                        },
                        {
                            "text": "Wszyscy goście <chwalili> przygotowany przez nią obiad, prosząc o dokładkę.",
                            "target_lang_translations": {
                                "ru": "Все гости <хвалили> приготовленный ею обед, прося добавки."
                            }
                        }
                    ],
                    "synonyms": [
                        "doceniać",
                        "zachwalać"
                    ],
                    "antonyms": [
                        "ganić",
                        "krytykować",
                        "potępiać"
                    ],
                    "common_phrases": [
                        "chwalić pod niebiosa",
                        "chwalić dzień przed zachodem słońca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить о ком-то или чем-то хорошо, выражая признание, восхищение или одобрение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвалить",
                                "target_lang_sense_clarification": "высказывать одобрение, говорить комплименты"
                            },
                            {
                                "target_lang_word": "восхвалять",
                                "target_lang_sense_clarification": "превозносить, горячо хвалить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98ded242-258b-3a2c-afc4-1f79065a7cc2",
                    "sense_definition": "Mówić z dumą o własnych sukcesach, zaletach lub stanie posiadania, często w sposób przesadny (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "boast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat ciągle <chwali> się swoim nowym, szybkim samochodem przed sąsiadami.",
                            "target_lang_translations": {
                                "ru": "Мой брат постоянно <хвастается> перед соседями своей новой быстрой машиной."
                            }
                        },
                        {
                            "text": "Nieładnie jest tak <chwalić> się pieniędzmi, gdy inni mają problemy finansowe.",
                            "target_lang_translations": {
                                "ru": "Некрасиво так <хвастаться> деньгами, когда у других финансовые проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "chlubić się",
                        "chełpić się",
                        "przechwalać się",
                        "popisywać się",
                        "afiszować się"
                    ],
                    "common_phrases": [
                        "chwalić się czymś"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "С гордостью говорить о собственных успехах, достоинствах или имуществе, часто преувеличивая (используется с «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвастаться",
                                "target_lang_sense_clarification": "говорить с чрезмерной гордостью о себе"
                            },
                            {
                                "target_lang_word": "хвалиться",
                                "target_lang_sense_clarification": "выставлять напоказ свои успехи или имущество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0eb73b6b-18f7-30ca-8bd6-460c865c9132",
                    "sense_definition": "Być zadowolonym z czegoś, co się posiada lub czego się doświadcza; doceniać coś (używane z zaimkiem „sobie”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi dziadkowie bardzo <chwalą> sobie życie na spokojnej i cichej wsi.",
                            "target_lang_translations": {
                                "ru": "Мои дедушка и бабушка очень <ценят> жизнь в тихой и спокойной деревне."
                            }
                        },
                        {
                            "text": "Pracownicy tej dużej firmy <chwalą> sobie elastyczne godziny pracy.",
                            "target_lang_translations": {
                                "ru": "Сотрудники этой крупной компании <ценят> гибкий график работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "doceniać"
                    ],
                    "antonyms": [
                        "narzekać"
                    ],
                    "common_phrases": [
                        "chwalić sobie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem „sobie”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть довольным тем, что имеешь или испытываешь; ценить что-то (используется с «sobie»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ценить",
                                "target_lang_sense_clarification": "признавать ценность чего-либо, дорожить"
                            },
                            {
                                "target_lang_word": "быть довольным",
                                "target_lang_sense_clarification": "испытывать удовлетворение от чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fc28b82-b8e9-3884-be67-16cfa827e856",
                    "sense_definition": "Oddawać cześć religijną bóstwu lub świętym; wielbić.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "worship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierni zebrali się w świątyni, aby <chwalić> Pana radosnymi pieśniami.",
                            "target_lang_translations": {
                                "ru": "Верующие собрались в храме, чтобы <славить> Господа радостными песнями."
                            }
                        },
                        {
                            "text": "Psalmista wzywa nas, byśmy <chwalili> imię Pańskie każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Псалмопевец призывает нас, чтобы мы <славили> имя Господне каждый день."
                            }
                        }
                    ],
                    "synonyms": [
                        "czcić",
                        "wielbić",
                        "sławić",
                        "gloryfikować",
                        "oddawać cześć"
                    ],
                    "common_phrases": [
                        "chwalić Pana",
                        "chwalić Boga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздавать религиозные почести божеству или святым; прославлять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "славить",
                                "target_lang_sense_clarification": "прославлять, воспевать (обычно в религиозном контексте)"
                            },
                            {
                                "target_lang_word": "восхвалять",
                                "target_lang_sense_clarification": "торжественно славить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
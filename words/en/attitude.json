                    "synonyms": [
                        "stance"
                    ],
                    "common_phrases": [
                        "positive attitude",
                        "negative attitude",
                        "attitude towards",
                        "change of attitude"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мнение, чувство или настроение по отношению к кому-либо или чему-либо; душевный настрой."
                    },
                    "translations": {
                        "ru": [

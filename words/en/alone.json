                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "лишь"
                            },
                            {
                                "target_lang_word": "один",
                                "target_lang_sense_clarification": "сам по себе"

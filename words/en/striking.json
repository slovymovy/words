    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "11c69771-fd99-3e12-a213-c8932107fe16",
                    "sense_definition": "Making a strong impression; attracting attention by being unusual, extreme, or prominent.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "impression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a <w>striking</w> resemblance between the two sisters.",
                            "target_lang_translations": {
                                "ru": "Между двумя сестрами существует <w>поразительное</w> сходство."
                            }
                        },
                        {
                            "text": "He looked quite <w>striking</w> in his new suit and tie.",
                            "target_lang_translations": {
                                "ru": "Он выглядел весьма <w>эффектно</w> в своем новом костюме и галстуке."
                            }
                        }
                    ],
                    "synonyms": [
                        "arresting"
                    ],
                    "antonyms": [
                        "non-striking"
                    ],
                    "common_phrases": [
                        "striking resemblance",
                        "striking contrast",
                        "striking feature",
                        "striking distance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производящий сильное впечатление; привлекающий внимание своей необычностью или яркостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поразительный",
                                "target_lang_sense_clarification": "о сходстве, контрасте или перемене"
                            },
                            {
                                "target_lang_word": "броский",
                                "target_lang_sense_clarification": "о внешности или чертах, привлекающих внимание"
                            },
                            {
                                "target_lang_word": "впечатляющий",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b173f366-3d84-365b-ab4b-a57856131f6e",
                    "sense_definition": "Participating in a strike; refusing to work as a form of protest.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "labor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>striking</w> workers gathered outside the factory gates to protest.",
                            "target_lang_translations": {
                                "ru": "<w>Бастующие</w> рабочие собрались у ворот фабрики в знак протеста."
                            }
                        },
                        {
                            "text": "Negotiations between the management and the <w>striking</w> employees have failed.",
                            "target_lang_translations": {
                                "ru": "Переговоры между руководством и <w>бастующими</w> сотрудниками провалились."
                            }
                        }
                    ],
                    "antonyms": [
                        "non-striking"
                    ],
                    "common_phrases": [
                        "striking workers",
                        "striking miners",
                        "striking teachers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участвующий в забастовке; отказывающийся работать в знак протеста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бастующий",
                                "target_lang_sense_clarification": "о рабочих или служащих"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cc8ff246-7673-38cb-a959-40f5c9c44e5b",
                    "sense_definition": "Present participle and gerund of <w>strike</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The clock was <w>striking</w> midnight when we finally arrived home.",
                            "target_lang_translations": {
                                "ru": "Часы <w>били</w> полночь, когда мы наконец вернулись домой."
                            }
                        },
                        {
                            "text": "He is <w>striking</w> a match to light the campfire.",
                            "target_lang_translations": {
                                "ru": "Он <w>чиркает</w> спичкой, чтобы разжечь костер."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола strike (ударять, бить, чиркать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бьющий",
                                "target_lang_sense_clarification": "о часах или физическом ударе"
                            },
                            {
                                "target_lang_word": "ударяющий",
                                "target_lang_sense_clarification": "наносящий удар"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4fb5a98a-d5b2-3630-95ac-3dc1808d2486",
                    "sense_definition": "The act by which something hits or impacts something else.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The repeated <w>strikings</w> of the hammer could be heard throughout the building.",
                            "target_lang_translations": {
                                "ru": "Повторяющиеся <w>удары</w> молотка были слышны по всему зданию."
                            }
                        },
                        {
                            "text": "Careful observation of the match <w>strikings</w> revealed a pattern in how they lit.",
                            "target_lang_translations": {
                                "ru": "Внимательное наблюдение за <w>чирканьем</w> спичек выявило закономерность в том, как они загорались."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие удара или столкновения чего-либо с чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удар",
                                "target_lang_sense_clarification": "единичное действие"
                            },
                            {
                                "target_lang_word": "битье",
                                "target_lang_sense_clarification": "процесс нанесения ударов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

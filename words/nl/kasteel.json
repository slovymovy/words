            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a5e7e4e3-cbb9-33d1-9191-0b761a230deb",
                    "sense_definition": "Een groot, versterkt gebouw waar vroeger edelen of vorsten woonden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koning en koningin woonden jarenlang in een groot <w>kasteel</w>.",
                            "target_lang_translations": {
                                "ru": "Король и королева много лет жили в большом <w>замке</w>."
                            }
                        },
                        {
                            "text": "Tijdens onze vakantie in Frankrijk hebben we een mooi middeleeuws <w>kasteel</w> bezocht.",
                            "target_lang_translations": {
                                "ru": "Во время нашего отпуска во Франции мы посетили красивый средневековый <w>замок</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "burcht",
                        "slot"
                    ],
                    "common_phrases": [
                        "kasteel bezoeken",
                        "middeleeuws kasteel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое укрепленное здание, где раньше жили дворяне или монархи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замок",
                                "target_lang_sense_clarification": "укрепленное жилище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fb65233-7b77-376b-aeb8-dd15b04d0a61",
                    "sense_definition": "Een schaakstuk dat in rechte lijnen horizontaal of verticaal beweegt.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij verplaatste zijn <w>kasteel</w> naar de andere kant van het bord.",
                            "target_lang_translations": {
                                "ru": "Он переместил свою <w>ладью</w> на другую сторону доски."
                            }
                        },
                        {
                            "text": "Met het <w>kasteel</w> kun je de koning van de tegenstander schaakmat zetten.",
                            "target_lang_translations": {
                                "ru": "С помощью <w>ладьи</w> можно поставить мат королю противника."
                            }
                        }
                    ],
                    "synonyms": [
                        "toren",
                        "schaaktoren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шахматная фигура, которая ходит по прямым линиям по горизонтали или вертикали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ладья",
                                "target_lang_sense_clarification": "шахматная фигура"
                            },
                            {
                                "target_lang_word": "тура",
                                "target_lang_sense_clarification": "разговорное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "094f6cd9-75ae-3c5f-8c5f-e386c3499ff5",
                    "sense_definition": "Een verhoogde opbouw op het dek van historische zeilschepen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kapitein stond op het achterste <w>kasteel</w> en keek uit over de oceaan.",
                            "target_lang_translations": {
                                "ru": "Капитан стоял на кормовой <w>надстройке</w> и смотрел на океан."
                            }
                        },
                        {
                            "text": "Tijdens de zeeslag werden soldaten op het <w>kasteel</w> geplaatst om te schieten.",
                            "target_lang_translations": {
                                "ru": "Во время морского сражения солдат размещали на <w>надстройке</w> для стрельбы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвышенная надстройка на палубе исторических парусных кораблей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надстройка",
                                "target_lang_sense_clarification": "часть корабля"
                            },
                            {
                                "target_lang_word": "кастль",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "00120975-9b0f-35a0-9d60-cc54ecc3be27",
                    "sense_definition": "Mieszkanie, pokój lub budynek wynajmowane lub zajmowane na pobyt czasowy, często przez turystów, żołnierzy lub studentów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "accommodation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wakacji nad morzem wynajęliśmy tanią <w>kwaterę</w> u rybaka.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска на море мы сняли дешевую <w>комнату</w> у рыбака."
                            }
                        },
                        {
                            "text": "Żołnierze wrócili na <w>kwatery</w> po długich ćwiczeniach w terenie.",
                            "target_lang_translations": {
                                "ru": "Солдаты вернулись в <w>казармы</w> после долгих учений в поле."
                            }
                        }
                    ],
                    "synonyms": [
                        "lokum",
                        "nocleg",
                        "stancja",
                        "zakwaterowanie"
                    ],
                    "common_phrases": [
                        "kwatera prywatna",
                        "kwatera główna",
                        "szukać kwatery",
                        "zmienić kwaterę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жилье, комната или здание, арендуемое или занимаемое для временного проживания, часто туристами, солдатами или студентами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жильё",
                                "target_lang_sense_clarification": "временное проживание"
                            },
                            {
                                "target_lang_word": "комната",
                                "target_lang_sense_clarification": "съемная"
                            },
                            {
                                "target_lang_word": "квартира",
                                "target_lang_sense_clarification": "место постоя (воен.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9087a4b9-e06d-30cf-bc4a-54f83fb1aa1a",
                    "sense_definition": "Wydzielona część terenu, najczęściej na cmentarzu, przeznaczona na groby.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plot",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego dziadek został pochowany w wojskowej <w>kwaterze</w> na Powązkach.",
                            "target_lang_translations": {
                                "ru": "Его дедушка был похоронен на военном <w>участке</w> кладбища Повонзки."
                            }
                        },
                        {
                            "text": "Zarządca cmentarza otworzył nową <w>kwaterę</w> dla zasłużonych obywateli.",
                            "target_lang_translations": {
                                "ru": "Управляющий кладбищем открыл новый <w>сектор</w> для заслуженных граждан."
                            }
                        }
                    ],
                    "synonyms": [
                        "sektor",
                        "działka",
                        "pole"
                    ],
                    "common_phrases": [
                        "kwatera cmentarna",
                        "kwatera zasłużonych",
                        "kwatera wojskowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделенная часть территории, чаще всего на кладбище, предназначенная для захоронений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "на кладбище"
                            },
                            {
                                "target_lang_word": "сектор",
                                "target_lang_sense_clarification": "зона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce0ab419-51e1-393b-88f9-5e4d69fe1580",
                    "sense_definition": "Część okna lub drzwi stanowiąca odrębną całość, zwykle oszklona i ujęta w ramy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny wiatr otworzył górną <w>kwaterę</w> starego okna.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер открыл верхнюю <w>фрамугу</w> старого окна."
                            }
                        },
                        {
                            "text": "Witraż w kościele składa się z wielu kolorowych <w>kwater</w>.",
                            "target_lang_translations": {
                                "ru": "Витраж в костеле состоит из множества цветных <w>секций</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrzydło"
                    ],
                    "common_phrases": [
                        "kwatera okienna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть окна или двери, представляющая собой отдельное целое, обычно застекленная и вставленная в раму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "створка",

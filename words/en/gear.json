            "senses": [
                {
                    "sense_id": "6f38046f-4b2b-3718-8774-0cba0e4fbc9f",
                    "sense_definition": "Excellent, fantastic, or very stylish.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That new band we saw last night was absolutely <w>gear</w>.",

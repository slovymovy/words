                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одичал",
                                "target_lang_sense_clarification": "стал диким"
                            },
                            {
                                "target_lang_word": "зарос",
                                "target_lang_sense_clarification": "покрылся дикой растительностью"
                            }

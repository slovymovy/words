    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "83957862-b258-3da9-b82a-e2b26f725d03",
                    "sense_definition": "Intensywne uczucie strachu, przerażenia lub lęku, często pojawiające się w obliczu nagłego zagrożenia lub w oczekiwaniu na coś złego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krzyknął w nagłej <w>trwodze</w>, widząc zbliżający się samochód.",
                            "target_lang_translations": {
                                "ru": "Он вскрикнул в внезапном <w>ужасе</w>, увидев приближающуюся машину."
                            }
                        },
                        {
                            "text": "Z <w>trwogą</w> i wahaniem myślała o nadchodzących zmianach w firmie.",
                            "target_lang_translations": {
                                "ru": "С <w>тревогой</w> и колебаниями она думала о грядущих переменах в фирме."
                            }
                        }
                    ],
                    "synonyms": [
                        "strach",
                        "lęk",
                        "panika",
                        "obawa",
                        "niepokój",
                        "groza"
                    ],
                    "antonyms": [
                        "spokój",
                        "odwaga",
                        "opanowanie",
                        "bezpieczeństwo"
                    ],
                    "common_phrases": [
                        "blady z trwogi",
                        "śmiertelna trwoga",
                        "patrzeć z trwogą",
                        "napełniać trwogą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное чувство страха, ужаса или беспокойства, часто возникающее перед лицом внезапной опасности или в ожидании чего-то плохого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страх",
                                "target_lang_sense_clarification": "сильная эмоция боязни"
                            },
                            {
                                "target_lang_word": "ужас",
                                "target_lang_sense_clarification": "крайняя степень страха"
                            },
                            {
                                "target_lang_word": "тревога",
                                "target_lang_sense_clarification": "беспокойство, ожидание опасности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17b3d811-139f-3e67-a430-0f5c3c5e5ccc",
                    "sense_definition": "Rzecz, sytuacja, zjawisko lub żywioł, które są bardzo groźne, przerażające i wywołują silny lęk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cause_of_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szalejąca za oknem burza była prawdziwą <w>trwogą</w> dla mieszkańców drewnianych domów.",
                            "target_lang_translations": {
                                "ru": "Бушующая за окном буря была настоящим <w>кошмаром</w> для жителей деревянных домов."
                            }
                        },
                        {
                            "text": "Wojna to wielka <w>trwoga</w>, która niszczy życie zwykłych ludzi.",
                            "target_lang_translations": {
                                "ru": "Война — это великий <w>ужас</w>, разрушающий жизни простых людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "koszmar",
                        "zagrożenie",
                        "groza",
                        "horrendum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещь, ситуация или явление, представляющие собой большую угрозу и вызывающие сильный страх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ужас",
                                "target_lang_sense_clarification": "то, что вызывает страх"
                            },
                            {
                                "target_lang_word": "кошмар",
                                "target_lang_sense_clarification": "нечто пугающее или мучительное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
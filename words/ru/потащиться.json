    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "59e0b570-b4e1-34dc-8900-3db6c2b4845c",
                    "sense_definition": "Медленно, с трудом или без желания пойти, отправиться куда-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_reluctant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В такую дождливую погоду нам пришлось <w>потащиться</w> в магазин пешком.",
                            "target_lang_translations": {
                                "en": "In such rainy weather, we had to <w>drag ourselves</w> to the store on foot."
                            }
                        },
                        {
                            "text": "Я не хотел вставать с дивана, но всё же <w>потащился</w> открывать дверь.",
                            "target_lang_translations": {
                                "en": "I didn't want to get off the couch, but still <w>dragged myself</w> to open the door."
                            }
                        }
                    ],
                    "synonyms": [
                        "поплестись",
                        "побрести",
                        "отправиться"
                    ],
                    "antonyms": [
                        "побежать",
                        "помчаться"
                    ],
                    "common_phrases": [
                        "еле потащиться",
                        "потащиться домой",
                        "потащиться на работу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies reluctance or difficulty"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To go somewhere slowly, with difficulty, or reluctantly"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drag oneself",
                                "target_lang_sense_clarification": "to go somewhere with great effort or reluctance"
                            },
                            {
                                "target_lang_word": "trudge",
                                "target_lang_sense_clarification": "to walk slowly and with heavy steps, typically because of exhaustion or harsh conditions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d27c664-3af8-31b4-a135-8b3ef617d169",
                    "sense_definition": "Пойти следом за кем-либо (часто отставая, с трудом или будучи незваным).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_follow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький щенок <w>потащился</w> за нами до самого дома.",
                            "target_lang_translations": {
                                "en": "The little puppy <w>trailed</w> after us all the way home."
                            }
                        },
                        {
                            "text": "Он не хотел оставаться один и <w>потащился</w> следом за старшим братом.",
                            "target_lang_translations": {
                                "en": "He didn't want to stay alone and <w>tagged along</w> after his older brother."
                            }
                        }
                    ],
                    "synonyms": [
                        "увязаться",
                        "поплестись"
                    ],
                    "common_phrases": [
                        "потащиться следом",
                        "потащиться за кем-то"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To follow someone, often lagging behind or uninvited"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tag along",
                                "target_lang_sense_clarification": "to accompany someone, especially when uninvited or with reluctance"
                            },
                            {
                                "target_lang_word": "trail after",
                                "target_lang_sense_clarification": "to follow behind someone, often slowly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35231e99-0fc1-307f-b9c6-7927e122cd2b",
                    "sense_definition": "Страдательный залог к глаголу <w>потащить</w>; начать перемещаться волоком или под действием тяги.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Трактор дёрнул трос, и тяжёлое бревно <w>потащилось</w> по земле.",
                            "target_lang_translations": {
                                "en": "The tractor jerked the cable, and the heavy log <w>started dragging</w> along the ground."
                            }
                        },
                        {
                            "text": "Сломанная ветка зацепилась за бампер и <w>потащилась</w> за машиной.",
                            "target_lang_translations": {
                                "en": "A broken branch caught on the bumper and <w>dragged</w> behind the car."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To start moving by being dragged or pulled along a surface"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drag",
                                "target_lang_sense_clarification": "to move along the ground due to being pulled"
                            },
                            {
                                "target_lang_word": "be dragged",
                                "target_lang_sense_clarification": "passive form indicating being pulled along"
                            }
                        ]
                    }
                }
            ]
        }
    ]

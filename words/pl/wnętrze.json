            "senses": [
                {
                    "sense_id": "c12c7033-5962-36b7-bf1d-e5df556e3a4e",
                    "sense_definition": "Przestrzeń znajdująca się w środku czegoś, ograniczona ścianami lub powłoką.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "space_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda dostała się do <w>wnętrza</w> moich butów podczas deszczu.",
                            "target_lang_translations": {
                                "ru": "Вода попала <w>внутрь</w> моих ботинок во время дождя."
                            }
                        },
                        {
                            "text": "Dziecko z ciekawością zaglądało do <w>wnętrza</w> pudełka.",
                            "target_lang_translations": {
                                "ru": "Ребенок с любопытством заглядывал <w>внутрь</w> коробки."
                            }
                        }
                    ],
                    "synonyms": [
                        "środek"
                    ],
                    "antonyms": [
                        "zewnętrze"
                    ],
                    "common_phrases": [
                        "do wnętrza",
                        "wnętrze dłoni",
                        "wnętrze ziemi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пространство, находящееся внутри чего-либо, ограниченное стенками или оболочкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутренность",
                                "target_lang_sense_clarification": "пространство внутри предмета"
                            },
                            {
                                "target_lang_word": "внутренняя часть",
                                "target_lang_sense_clarification": "содержимое или изнанка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "459eb91b-4b88-39d2-a9f3-1a611bff060a",
                    "sense_definition": "Pomieszczenie w środku budowli wraz z jego wystrojem, układem i wyposażeniem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Architekt zaprojektował bardzo nowoczesne <w>wnętrze</w> tego domu.",
                            "target_lang_translations": {
                                "ru": "Архитектор спроектировал очень современный <w>интерьер</w> этого дома."
                            }
                        },
                        {
                            "text": "Jasne kolory sprawiają, że <w>wnętrze</w> wydaje się większe.",
                            "target_lang_translations": {
                                "ru": "Светлые тона делают <w>интерьер</w> визуально больше."
                            }
                        }
                    ],
                    "antonyms": [
                        "zewnętrze"
                    ],
                    "common_phrases": [
                        "projektowanie wnętrz",
                        "wystrój wnętrza",
                        "architektura wnętrz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутреннее пространство здания или помещения, включая его убранство, планировку и обстановку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интерьер",
                                "target_lang_sense_clarification": "архитектура и дизайн помещений"

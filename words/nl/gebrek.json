    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ef71a6c2-0288-3cdb-b9c6-a422b29e50dc",
                    "sense_definition": "Een toestand waarin er niet genoeg van iets is of waarin iets volledig ontbreekt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tekort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door een <w>gebrek</w> aan bewijs werd de verdachte uiteindelijk vrijgesproken.",
                            "target_lang_translations": {
                                "en": "Due to a <w>lack</w> of evidence, the suspect was eventually acquitted.",
                                "ru": "Из-за <w>недостатка</w> доказательств подозреваемый был в конечном итоге оправдан."
                            }
                        },
                        {
                            "text": "We konden het project niet afmaken bij <w>gebrek</w> aan financiële middelen.",
                            "target_lang_translations": {
                                "en": "We could not finish the project for <w>lack</w> of financial means.",
                                "ru": "Мы не смогли завершить проект из-за <w>нехватки</w> финансовых средств."
                            }
                        }
                    ],
                    "synonyms": [
                        "deficiëntie"
                    ],
                    "antonyms": [
                        "overvloed",
                        "teveel"
                    ],
                    "common_phrases": [
                        "gebrek aan",
                        "bij gebrek aan"
                    ],
                    "target_lang_definitions": {
                        "en": "A state in which there is not enough of something or it is completely missing.",
                        "ru": "Состояние, при котором чего-то недостаточно или что-то полностью отсутствует."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lack",
                                "target_lang_sense_clarification": "state of being without something"
                            },
                            {
                                "target_lang_word": "shortage",
                                "target_lang_sense_clarification": "insufficient amount available"
                            },
                            {
                                "target_lang_word": "deficiency",
                                "target_lang_sense_clarification": "inadequacy or incompleteness"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "недостаток",
                                "target_lang_sense_clarification": "дефицит чего-либо"
                            },
                            {
                                "target_lang_word": "нехватка",
                                "target_lang_sense_clarification": "недостаточное количество"
                            },
                            {
                                "target_lang_word": "отсутствие",
                                "target_lang_sense_clarification": "полное отсутствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21d4fac3-e5e2-3dd3-b03a-2dfcb8fe2daf",
                    "sense_definition": "Een onvolkomenheid, fout of mankement aan een voorwerp, systeem of persoon.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "defect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het huis bleek een verborgen <w>gebrek</w> te hebben dat pas na de koop werd ontdekt.",
                            "target_lang_translations": {
                                "en": "The house turned out to have a hidden <w>defect</w> that was only discovered after the purchase.",
                                "ru": "Оказалось, что у дома был скрытый <w>дефект</w>, который обнаружили только после покупки."
                            }
                        },
                        {
                            "text": "Ondanks een klein technisch <w>gebrek</w> functioneert de machine nog steeds goed.",
                            "target_lang_translations": {
                                "en": "Despite a small technical <w>defect</w>, the machine still functions well.",
                                "ru": "Несмотря на небольшой технический <w>дефект</w>, машина всё ещё работает хорошо."
                            }
                        }
                    ],
                    "antonyms": [
                        "perfectie"
                    ],
                    "common_phrases": [
                        "verborgen gebrek",
                        "in gebreke blijven"
                    ],
                    "target_lang_definitions": {
                        "en": "An imperfection, fault, or flaw in an object, system, or person.",
                        "ru": "Несовершенство, ошибка или изъян в предмете, системе или человеке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defect",
                                "target_lang_sense_clarification": "fault or malfunction"
                            },

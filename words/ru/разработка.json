    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "abd62507-a1a6-350f-8136-77af6b227679",
                    "sense_definition": "Действие по значению глагола <w>разрабатывать</w>; процесс создания, проектирования или тщательной подготовки чего-либо нового (плана, устройства, программы и т. п.).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creation_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Южнокорейские ученые заняты <w>разработкой</w> новой модели робота.",
                            "target_lang_translations": {
                                "en": "South Korean scientists are busy with the <w>development</w> of a new robot model."
                            }
                        },
                        {
                            "text": "На <w>разработку</w> этого лекарства ушло более десяти лет.",
                            "target_lang_translations": {
                                "en": "The <w>development</w> of this medicine took over ten years."
                            }
                        }
                    ],
                    "synonyms": [
                        "подготовка",
                        "дизайн",
                        "подготовка",
                        "дизайн",
                        "эксплуатация"
                    ],
                    "common_phrases": [
                        "разработка проекта",
                        "разработка плана",
                        "стадия разработки",
                        "разработка программного обеспечения"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of creating, designing, or preparing something new, such as a plan, device, or software."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "development",
                                "target_lang_sense_clarification": "process of creating"
                            },
                            {
                                "target_lang_word": "design",
                                "target_lang_sense_clarification": "technical creation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fec33c4-7b05-3d62-9959-d4ec5efcf251",
                    "sense_definition": "Добыча полезных ископаемых; эксплуатация природных месторождений.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания получила лицензию на <w>разработку</w> нефтяного месторождения.",
                            "target_lang_translations": {
                                "en": "The company received a license for the <w>development</w> of an oil field."
                            }
                        },
                        {
                            "text": "При <w>разработке</w> недр необходимо строго соблюдать экологические нормы.",
                            "target_lang_translations": {
                                "en": "During the <w>exploitation</w> of mineral resources, it is necessary to strictly observe environmental standards."
                            }
                        }
                    ],
                    "synonyms": [
                        "эксплуатация"
                    ],
                    "common_phrases": [
                        "разработка месторождения",
                        "разработка недр",
                        "открытая разработка"
                    ],
                    "target_lang_definitions": {
                        "en": "The extraction of minerals or the exploitation of natural deposits."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exploitation",
                                "target_lang_sense_clarification": "of natural resources"
                            },
                            {
                                "target_lang_word": "mining",
                                "target_lang_sense_clarification": "of minerals"
                            },
                            {
                                "target_lang_word": "extraction",
                                "target_lang_sense_clarification": "process of removing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44df6134-d483-3390-be8c-a1a87cc12da8",
                    "sense_definition": "Результат научной или конструкторской работы; конкретные исследования, идеи, документы или конструкции.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "creation_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Научные <w>разработки</w> этого института применяются в современной медицине.",
                            "target_lang_translations": {
                                "en": "The scientific <w>developments</w> of this institute are used in modern medicine."
                            }
                        },
                        {
                            "text": "Автор представил свои последние методические <w>разработки</w> на конференции.",
                            "target_lang_translations": {
                                "en": "The author presented his latest methodological <w>developments</w> at the conference."
                            }
                        }
                    ],
                    "common_phrases": [
                        "научные разработки",
                        "методические разработки",
                        "собственная разработка"
                    ],
                    "target_lang_definitions": {
                        "en": "The result of scientific or engineering work; specific research, ideas, or designs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "development",
                                "target_lang_sense_clarification": "result or product"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6d86e30-c8a1-3cc6-a848-f3f9d5a16f1b",
                    "sense_definition": "Часть музыкального произведения (обычно в сонатной форме), в которой развиваются и видоизменяются основные темы.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В <w>разработке</w> симфонии композитор мастерски соединил две главные темы.",
                            "target_lang_translations": {
                                "en": "In the <w>development</w> of the symphony, the composer masterfully combined two main themes."
                            }
                        },
                        {
                            "text": "Эта соната отличается сложной и драматичной <w>разработкой</w>.",
                            "target_lang_translations": {
                                "en": "This sonata is distinguished by a complex and dramatic <w>development</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A section of a musical piece (usually in sonata form) where the main themes are developed and modified."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "development",
                                "target_lang_sense_clarification": "musical term"
                            }
                        ]
                    }
                }
            ]
        }
    ]

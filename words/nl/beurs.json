    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e7f97e5a-a5f7-35f7-916e-71fa78b2af53",
                    "sense_definition": "Een kleine tas of zak om muntgeld en bankbiljetten in te bewaren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "money_container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben mijn <w>beurs</w> vergeten mee te nemen naar de winkel.",
                            "target_lang_translations": {
                                "ru": "Я забыл взять свой <w>кошелёк</w> в магазин."
                            }
                        },
                        {
                            "text": "Hij moest de <w>beurs</w> trekken om de dure rekening te betalen.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось достать <w>кошелёк</w>, чтобы оплатить дорогой счёт."
                            }
                        }
                    ],
                    "synonyms": [
                        "portemonnee",
                        "geldbuidel"
                    ],
                    "common_phrases": [
                        "de beurs trekken",
                        "een lege beurs",
                        "met gesloten beurs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькая сумка или мешочек для хранения монет и банкнот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кошелёк",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "портмоне",
                                "target_lang_sense_clarification": "обычно мужской или складной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ce84d18-9dd6-3de8-9772-7fe198fb22fa",
                    "sense_definition": "De markt of het gebouw waar aandelen, obligaties en andere waardepapieren worden verhandeld.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "financial_market",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het grote technologiebedrijf gaat volgend jaar naar de <w>beurs</w>.",
                            "target_lang_translations": {
                                "ru": "Крупная технологическая компания в следующем году выходит на <w>биржу</w>."
                            }
                        },
                        {
                            "text": "Door het slechte nieuws zijn de prijzen op de <w>beurs</w> gedaald.",
                            "target_lang_translations": {
                                "ru": "Из-за плохих новостей цены на <w>бирже</w> упали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naar de beurs gaan",
                        "op de beurs",
                        "de beurs crasht"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "financial"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рынок или здание, где торгуют акциями, облигациями и другими ценными бумагами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "биржа",
                                "target_lang_sense_clarification": "финансовый рынок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ea13510-a543-315d-8f61-8db672e8ca13",
                    "sense_definition": "Een geldbedrag dat iemand krijgt om een studie of onderzoek te betalen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "stipend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij heeft een <w>beurs</w> gekregen om in het buitenland te studeren.",
                            "target_lang_translations": {
                                "ru": "Она получила <w>стипендию</w>, чтобы учиться за границей."
                            }
                        },
                        {
                            "text": "Studenten kunnen een <w>beurs</w> aanvragen als hun ouders weinig verdienen.",
                            "target_lang_translations": {
                                "ru": "Студенты могут подать заявку на <w>стипендию</w>, если их родители мало зарабатывают."
                            }
                        }
                    ],
                    "synonyms": [
                        "studiebeurs"
                    ],
                    "common_phrases": [
                        "een beurs aanvragen",
                        "een beurs krijgen",
                        "een volledige beurs"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "education"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная сумма, выделяемая кому-либо для оплаты обучения или исследований."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стипендия",

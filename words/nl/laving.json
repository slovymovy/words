    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "13e5c003-aeb6-30b9-8cb6-d18f0885ce15",
                    "sense_definition": "Het lessen van de dorst of het bieden van verfrissing, vaak door het geven van drinken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de lange wandeling in de hitte zochten de reizigers naar <w>laving</w> bij de waterput.",
                            "target_lang_translations": {
                                "ru": "После долгой прогулки по жаре путники искали <w>утоления жажды</w> у колодца."
                            }
                        },
                        {
                            "text": "Het koele bronwater bood de uitgeputte sporters de nodige <w>laving</w>.",
                            "target_lang_translations": {
                                "ru": "Прохладная родниковая вода дала изнуренным спортсменам необходимое <w>освежение</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verfrissing",
                        "lafenis"
                    ],
                    "common_phrases": [
                        "laving zoeken",
                        "laving bieden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утоление жажды или освежение, часто посредством питья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утоление жажды",
                                "target_lang_sense_clarification": "действие по устранению жажды"
                            },
                            {
                                "target_lang_word": "освежение",
                                "target_lang_sense_clarification": "восстановление сил прохладой или питьем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3a998f7-1296-3417-a02f-f12e5fbc2f9d",
                    "sense_definition": "Geestelijke troost, verlichting of bemoediging voor iemand die verdrietig of angstig is.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vriendelijke woorden van de vreemdeling waren een <w>laving</w> voor haar bedroefde ziel.",
                            "target_lang_translations": {
                                "ru": "Добрые слова незнакомца стали <w>бальзамом</w> для ее опечаленной души."
                            }
                        },
                        {
                            "text": "In tijden van grote rouw kan muziek dienen tot <w>laving</w> van de geest.",
                            "target_lang_translations": {
                                "ru": "Во времена глубокого траура музыка может служить <w>утешением</w> для духа."
                            }
                        }
                    ],
                    "synonyms": [
                        "verkwikking",
                        "lafenis"
                    ],
                    "common_phrases": [
                        "laving van de ziel",
                        "tot laving zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Духовное утешение, облегчение или поддержка в печали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утешение",
                                "target_lang_sense_clarification": "облегчение горя или страдания"
                            },
                            {
                                "target_lang_word": "отрада",
                                "target_lang_sense_clarification": "то, что доставляет удовольствие или успокоение"
                            },
                            {
                                "target_lang_word": "бальзам",
                                "target_lang_sense_clarification": "переносное значение: целительное, успокаивающее средство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

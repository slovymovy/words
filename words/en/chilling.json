                {
                    "sense_id": "028bf6c7-8778-3a9c-a15b-8bb3d4ac0e45",
                    "sense_definition": "Causing a sudden feeling of fear, horror, or dread.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The witness gave a <w>chilling</w> account of the crime scene.",
                            "target_lang_translations": {
                                "ru": "Свидетель дал <w>леденящее кровь</w> описание места преступления."
                            }
                        },
                        {
                            "text": "It was a <w>chilling</w> story that kept everyone awake at night.",
                            "target_lang_translations": {
                                "ru": "Это была <w>жуткая</w> история, из-за которой никто не мог уснуть ночью."
                            }
                        }
                    ],
                    "synonyms": [
                        "frightening",
                        "terrifying",
                        "scary",
                        "creepy",
                        "eerie"
                    ],
                    "antonyms": [
                        "comforting",
                        "reassuring",
                        "heartwarming"
                    ],
                    "common_phrases": [
                        "chilling effect",
                        "chilling reminder",
                        "spine-chilling",
                        "chilling silence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий внезапное чувство страха, ужаса или трепета."
                    },
                    "translations": {
                        "ru": [
                            {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b3c3fcb3-e1d0-3e57-9249-fc0dc0fe419a",
                    "sense_definition": "Relating to, characteristic of, or situated on the outskirts of a city.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many families choose to live in a <w>suburban</w> area because it is safer for children.",
                            "target_lang_translations": {
                                "ru": "Многие семьи предпочитают жить в <w>пригородном</w> районе, потому что там безопаснее для детей."
                            }
                        },
                        {
                            "text": "She enjoys the quiet pace of <w>suburban</w> life compared to the busy city center.",
                            "target_lang_translations": {
                                "ru": "Ей нравится спокойный ритм <w>пригородной</w> жизни по сравнению с оживленным центром города."
                            }
                        }
                    ],
                    "antonyms": [
                        "nonsuburban"
                    ],
                    "common_phrases": [
                        "suburban area",
                        "suburban life",
                        "suburban home",
                        "suburban sprawl"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к пригороду, характерный для него или расположенный на окраине города."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пригородный",
                                "target_lang_sense_clarification": "относящийся к пригороду"
                            },
                            {
                                "target_lang_word": "загородный",
                                "target_lang_sense_clarification": "находящийся за чертой города"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fae96826-1b1f-30cc-b078-574d4fe3cf66",
                    "sense_definition": "A person who lives in a suburb.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The early train is always crowded with <w>suburbans</w> commuting to their offices.",
                            "target_lang_translations": {
                                "ru": "Утренний поезд всегда переполнен <w>жителями пригорода</w>, едущими в свои офисы."
                            }
                        },
                        {
                            "text": "City dwellers often have different priorities than the <w>suburbans</w> living nearby.",
                            "target_lang_translations": {
                                "ru": "У горожан часто иные приоритеты, чем у <w>жителей пригорода</w>, живущих неподалеку."
                            }
                        }
                    ],
                    "synonyms": [
                        "suburbanite"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, проживающий в пригороде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "житель пригорода",
                                "target_lang_sense_clarification": "человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "602f0acb-2240-38ff-9ee1-4382fd8803eb",
                    "sense_definition": "An automobile with a station wagon body on a truck chassis.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They packed all their camping gear into the back of the old <w>suburban</w>.",
                            "target_lang_translations": {
                                "ru": "Они погрузили всё туристическое снаряжение в багажник старого <w>субурбана</w>."
                            }
                        },
                        {
                            "text": "He drove a large <w>suburban</w> that could fit the entire team.",
                            "target_lang_translations": {
                                "ru": "Он вел большой <w>субурбан</w>, в который могла поместиться вся команда."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Автомобиль с кузовом универсал на шасси грузовика (часто о модели Chevrolet Suburban)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "субурбан",
                                "target_lang_sense_clarification": "тип автомобиля"
                            },
                            {
                                "target_lang_word": "внедорожник",
                                "target_lang_sense_clarification": "тип кузова"
                            }
                        ]
                    }

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "4037131a-01ad-3ff7-98b0-fcef23dcfcab",
                    "sense_definition": "Partykuła nadająca wypowiedzi charakter życzenia, rozkazu, zachęty lub przyzwolenia; podniosły lub książkowy wariant słowa <w>niech</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "particle_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A niechaj narodowie wżdy postronni znają, iż Polacy nie gęsi, iż swój język mają!",
                            "target_lang_translations": {
                                "ru": "А <пусть> знают все окрестные народы, что поляки не гуси, что у них свой язык!"
                            }
                        },
                        {
                            "text": "<w>Niechaj</w> ten dzień pozostanie w naszej pamięci na zawsze.",
                            "target_lang_translations": {
                                "ru": "<Пусть> этот день останется в нашей памяти навсегда."
                            }
                        },
                        {
                            "text": "<w>Niechaj</w> każdy robi to, co uważa za słuszne i sprawiedliwe.",
                            "target_lang_translations": {
                                "ru": "<Пусть> каждый делает то, что считает правильным и справедливым."
                            }
                        }
                    ],
                    "synonyms": [
                        "niech",
                        "niechajże"
                    ],
                    "common_phrases": [
                        "niechaj będzie",
                        "niechaj się stanie",
                        "niechaj żyje"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "książkowy, podniosły"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Частица, придающая высказыванию характер пожелания, приказа, побуждения или разрешения; возвышенный или книжный вариант слова \"niech\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пусть",
                                "target_lang_sense_clarification": "частица, выражающая побуждение или пожелание"
                            },
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "торжественная частица (напр., да здравствует)"
                            },
                            {
                                "target_lang_word": "пускай",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c2a759f5-946e-3b65-b9cb-1575a5b9eb0e",
                    "sense_definition": "Forma trybu rozkazującego (2. osoba liczby pojedynczej) czasownika <w>niechać</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_imperative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Niechaj</w> płonnych nadziei, gdyż one prowadzą tylko do rozczarowania.",
                            "target_lang_translations": {
                                "ru": "<Оставь> пустые надежды, так как они ведут лишь к разочарованию."
                            }
                        },
                        {
                            "text": "<w>Niechaj</w> waśni i sporów, bo zgoda jest najważniejsza.",
                            "target_lang_translations": {
                                "ru": "<Оставь> распри и споры, потому что согласие важнее всего."
                            }
                        }
                    ],
                    "synonyms": [
                        "niech",
                        "niechajże"

    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e5eaf37-179e-3c7d-8b87-be41ced7bb12",
                    "sense_definition": "Budowla inżynieryjna w postaci długiego korytarza przebiegającego pod ziemią, wodą lub przez górę, służąca zazwyczaj do celów komunikacyjnych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pociąg zwolnił, wjeżdżając w ciemny <w>tunel</w> pod miastem.",
                            "target_lang_translations": {
                                "ru": "Поезд замедлил ход, въезжая в темный <w>тоннель</w> под городом."
                            }
                        },
                        {
                            "text": "Dzięki nowym <w>tunelom</w> podróż przez góry jest znacznie szybsza.",
                            "target_lang_translations": {
                                "ru": "Благодаря новым <w>тоннелям</w> путешествие через горы стало значительно быстрее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tunel kolejowy",
                        "wjazd do tunelu",
                        "tunel podziemny",
                        "światełko w tunelu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инженерное сооружение в виде длинного коридора, проходящего под землей, водой или сквозь гору, обычно служащее для транспортных целей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тоннель",
                                "target_lang_sense_clarification": "сооружение"
                            },
                            {
                                "target_lang_word": "туннель",
                                "target_lang_sense_clarification": "вариант написания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b537ec62-8b8d-349d-8393-889a7976fd38",
                    "sense_definition": "Lekka konstrukcja ogrodnicza pokryta folią, służąca do uprawy roślin w warunkach chroniących przed zimnem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiosną sadzimy pomidory i ogórki w <w>tunelu</w> na działce.",
                            "target_lang_translations": {
                                "ru": "Весной мы сажаем помидоры и огурцы в <w>туннеле</w> на даче."
                            }
                        },
                        {
                            "text": "Silny wiatr uszkodził foliowy <w>tunel</w> stojący w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер повредил пленочный <w>туннель</w>, стоящий в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "foliak"
                    ],
                    "common_phrases": [
                        "tunel foliowy",
                        "uprawa w tunelu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкая садовая конструкция, покрытая пленкой, служащая для выращивания растений в условиях защиты от холода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плёночный туннель",
                                "target_lang_sense_clarification": "садоводство"
                            },
                            {
                                "target_lang_word": "парник",
                                "target_lang_sense_clarification": "теплица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91088e49-f602-3f69-af82-f3a2752a7306",
                    "sense_definition": "Przestrzeń ograniczona po bokach i od góry, przypominająca wyglądem zamknięty korytarz.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jechaliśmy samochodem przez piękny, zielony <w>tunel</w> utworzony przez korony starych drzew.",
                            "target_lang_translations": {
                                "ru": "Мы ехали на машине сквозь красивый зеленый <w>тоннель</w>, образованный кронами старых деревьев."
                            }
                        },
                        {
                            "text": "Reflektory tworzyły we mgle świetlny <w>tunel</w>.",
                            "target_lang_translations": {
                                "ru": "Фары создавали в тумане световой <w>тоннель</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tunel aerodynamiczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пространство, ограниченное с боков и сверху, напоминающее видом закрытый коридор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тоннель",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "коридор",
                                "target_lang_sense_clarification": "метафора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74501234-534c-3a1a-bb23-b9ec61113c91",
                    "sense_definition": "Wirtualne połączenie sieciowe, w którym dane są przesyłane wewnątrz innego protokołu, często w celu zapewnienia bezpieczeństwa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Haker próbował wykorzystać <w>tunel</w>, ale został szybko namierzony przez administratora.",
                            "target_lang_translations": {
                                "ru": "Хакер пытался использовать <w>туннель</w>, но был быстро обнаружен администратором."
                            }
                        },
                        {
                            "text": "Skonfigurowaliśmy bezpieczny <w>tunel</w> VPN do łączenia się z biurem.",
                            "target_lang_translations": {
                                "ru": "Мы настроили безопасный VPN-<w>туннель</w> для соединения с офисом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tunel VPN",
                        "tunel SSH"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "informatyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Виртуальное сетевое соединение, в котором данные передаются внутри другого протокола, часто для обеспечения безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туннель",
                                "target_lang_sense_clarification": "информатика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50ea3747-854d-3891-9075-c89bdcb9d208",
                    "sense_definition": "Rodzaj biżuterii w kształcie pustego walca, wkładany w celowo rozciągnięty otwór w płatku ucha.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek nosi w lewym uchu duży, czarny <w>tunel</w>.",
                            "target_lang_translations": {
                                "ru": "Марек носит в левом ухе большой черный <w>тоннель</w>."
                            }
                        },
                        {
                            "text": "Kupiła sobie nowy, drewniany <w>tunel</w> o średnicy dziesięciu milimetrów.",
                            "target_lang_translations": {
                                "ru": "Она купила себе новый деревянный <w>тоннель</w> диаметром десять миллиметров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tunel w uchu",
                        "rozpychanie ucha pod tunel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид украшения в форме полого цилиндра, вставляемый в намеренно растянутое отверстие в мочке уха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тоннель",
                                "target_lang_sense_clarification": "украшение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d611789f-bbc9-3890-8802-c2d20cc3b7f1",
                    "sense_definition": "Poziome wyrobisko korytarzowe w kopalni.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Górnicy drążyli nowy <w>tunel</w>, aby dostać się do złoża węgla.",
                            "target_lang_translations": {
                                "ru": "Шахтеры прокладывали новый <w>тоннель</w>, чтобы добраться до угольного пласта."
                            }
                        },
                        {
                            "text": "Stary <w>tunel</w> wentylacyjny wymagał natychmiastowego remontu.",
                            "target_lang_translations": {
                                "ru": "Старый вентиляционный <w>тоннель</w> требовал немедленного ремонта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drążenie tunelu"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "górnictwo"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Горизонтальная горная выработка коридорного типа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штольня",
                                "target_lang_sense_clarification": "горное дело"
                            },
                            {
                                "target_lang_word": "тоннель",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d168f84c-fe37-37b4-8d70-ab45d2cbd402",
                    "sense_definition": "Zatrzymywać kogoś lub coś w miejscu, nie pozwalać na wykonanie jakiejś czynności, dalszy ruch lub rozwój sytuacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_stop_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wysoki mur miał za zadanie skutecznie <w>powstrzymywać</w> wrogie wojska przed atakiem na zamek.",
                            "target_lang_translations": {
                                "ru": "Высокая стена должна была эффективно <w>сдерживать</w> вражеские войска от атаки на замок."
                            }
                        },
                        {
                            "text": "Policja musiała użyć siły, aby <w>powstrzymywać</w> napierający tłum demonstrantów.",
                            "target_lang_translations": {
                                "ru": "Полиция была вынуждена применить силу, чтобы <w>сдерживать</w> напирающую толпу демонстрантов."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatrzymywać",
                        "wstrzymywać",
                        "blokować",
                        "hamować",
                        "zapobiegać"
                    ],
                    "common_phrases": [
                        "powstrzymywać atak",
                        "powstrzymywać inwazję",
                        "powstrzymywać napastnika",
                        "powstrzymywać zmiany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не давать кому-либо или чему-либо двигаться, действовать или развиваться; останавливать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдерживать",
                                "target_lang_sense_clarification": "натиск, толпу, развитие"
                            },
                            {
                                "target_lang_word": "останавливать",
                                "target_lang_sense_clarification": "прекращать движение или действие"
                            },
                            {
                                "target_lang_word": "удерживать",
                                "target_lang_sense_clarification": "не пускать, держать на месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b452e01c-7275-328a-ad77-379158785d8e",
                    "sense_definition": "Nie pozwalać sobie na zrobienie czegoś, rezygnować z jakiegoś działania mimo chęci jego wykonania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_refrain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem się bardzo <w>powstrzymywać</w>, żeby nie powiedzieć mu prawdy prosto w oczy.",
                            "target_lang_translations": {
                                "ru": "Мне приходилось сильно <w>сдерживаться</w>, чтобы не высказать ему правду прямо в глаза."
                            }
                        },
                        {
                            "text": "Lekarz zalecił pacjentowi, aby ten zaczął <w>powstrzymywać</w> się od jedzenia tłustych potraw.",
                            "target_lang_translations": {
                                "ru": "Врач порекомендовал пациенту начать <w>воздерживаться</w> от употребления жирной пищи."
                            }
                        }
                    ],
                    "synonyms": [
                        "pohamowywać się"
                    ],
                    "common_phrases": [
                        "powstrzymywać się od głosu",
                        "powstrzymywać się od komentarza",
                        "powstrzymywać się od palenia"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не позволять себе совершить какое-либо действие; отказываться от чего-либо вопреки желанию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдерживаться",
                                "target_lang_sense_clarification": "контролировать свои порывы"
                            },
                            {
                                "target_lang_word": "воздерживаться",
                                "target_lang_sense_clarification": "отказываться от привычки или действия"
                            },
                            {
                                "target_lang_word": "удерживаться",
                                "target_lang_sense_clarification": "не поддаваться желанию сделать что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f02a3310-ccc7-3d0f-ba34-78817a0c6fb4",
                    "sense_definition": "Panować nad gwałtownymi emocjami lub reakcjami fizjologicznymi, nie pozwalając im się uzewnętrznić.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_suppress_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo smutnej wiadomości, starała się <w>powstrzymywać</w> łzy przy swoich małych dzieciach.",
                            "target_lang_translations": {
                                "ru": "Несмотря на печальную новость, она старалась <w>сдерживать</w> слезы при своих маленьких детях."
                            }
                        },
                        {
                            "text": "Uczeń nie mógł <w>powstrzymywać</w> głośnego śmiechu podczas zabawnej sytuacji w klasie.",
                            "target_lang_translations": {
                                "ru": "Ученик не мог <w>сдерживать</w> громкий смех во время забавной ситуации в классе."
                            }
                        }
                    ],
                    "synonyms": [
                        "tłumić",
                        "dusić",
                        "dławić"
                    ],
                    "common_phrases": [
                        "powstrzymywać łzy",
                        "powstrzymywać śmiech",
                        "powstrzymywać gniew",
                        "powstrzymywać emocje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Усилием воли не давать проявиться эмоциям или физическим реакциям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдерживать",
                                "target_lang_sense_clarification": "слезы, смех, гнев"
                            },
                            {
                                "target_lang_word": "подавлять",
                                "target_lang_sense_clarification": "заглушать чувства"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "forever"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "0a771c92-13e7-3dcc-93b8-070e73481baf",
                    "sense_definition": "In order to help, benefit, or be suitable for someone or something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "beneficiary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I bought a special gift <w>for</w> my mother.",
                            "target_lang_translations": {
                                "ru": "Я купил особый подарок <w>для</w> мамы."
                            }
                        },
                        {
                            "text": "Exercise is very good <w>for</w> your health.",
                            "target_lang_translations": {
                                "ru": "Упражнения очень полезны <w>для</w> вашего здоровья."
                            }
                        },
                        {
                            "text": "Everything I do, I do <w>for</w> you.",
                            "target_lang_translations": {
                                "ru": "Всё, что я делаю, я делаю <w>для</w> тебя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "good for you",
                        "just for you",
                        "do for me"
                    ],
                    "target_lang_definitions": {
                        "ru": "Для того, чтобы помочь, принести пользу или подойти кому-то или чему-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для",
                                "target_lang_sense_clarification": "указывает на адресата действия или получателя пользы"
                            },
                            {
                                "target_lang_word": "ради",
                                "target_lang_sense_clarification": "в интересах кого-либо, для блага"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5af39b4-37dc-32de-8869-58fd259e5d84",
                    "sense_definition": "Used to show the purpose or function of an object or action.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This sharp knife is used <w>for</w> cutting bread.",
                            "target_lang_translations": {
                                "ru": "Этот острый нож используется <w>для</w> резки хлеба."
                            }
                        },
                        {
                            "text": "What is this red button <w>for</w>?",
                            "target_lang_translations": {
                                "ru": "<w>Для</w> чего эта красная кнопка?"
                            }
                        },
                        {
                            "text": "These apples are <w>for</w> eating, not for cooking.",
                            "target_lang_translations": {
                                "ru": "Эти яблоки <w>для</w> еды, а не для готовки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "for sale",
                        "for rent",
                        "what for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания цели или функции объекта или действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для",
                                "target_lang_sense_clarification": "указывает на предназначение предмета"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "указывает на цель расходования средств или усилий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b10b121-0f1d-34dc-a4fb-f697c1569d92",
                    "sense_definition": "Used to indicate a duration or period of time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We have lived in this house <w>for</w> ten years.",
                            "target_lang_translations": {
                                "ru": "Мы живем в этом доме десять лет."
                            }
                        },
                        {
                            "text": "Please wait here <w>for</w> a moment.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подождите здесь минутку."
                            }
                        },
                        {
                            "text": "The store is closed <w>for</w> the day.",
                            "target_lang_translations": {
                                "ru": "Магазин закрыт <w>на</w> сегодня."
                            }
                        }
                    ],
                    "common_phrases": [
                        "for a while",
                        "for a long time",
                        "for years"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания продолжительности или периода времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в тече́ние",
                                "target_lang_sense_clarification": "на протяжении определенного периода"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "указывает на запланированный срок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13b5d9e7-c9ca-328c-91c4-ccfd05538c13",
                    "sense_definition": "Towards; in the direction of a destination.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "destination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train <w>for</w> London leaves in five minutes.",
                            "target_lang_translations": {
                                "ru": "Поезд <w>на</w> Лондон отправляется через пять минут."
                            }
                        },
                        {
                            "text": "She left home and headed <w>for</w> the office.",
                            "target_lang_translations": {
                                "ru": "Она вышла из дома и направилась <w>в</w> офис."
                            }
                        },
                        {
                            "text": "Run <w>for</w> the hills!",
                            "target_lang_translations": {
                                "ru": "Бегите <w>в</w> горы!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "head for",
                        "leave for",
                        "bound for"
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении; в сторону пункта назначения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "указывает на конечный пункт движения"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "указывает направление движения (транспорт, стороны света)"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "по направлению к кому-либо или чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64bf1281-a69d-34b5-ab16-016f9ab20ace",
                    "sense_definition": "In order to obtain, acquire, or achieve something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you want to go out <w>for</w> coffee?",
                            "target_lang_translations": {
                                "ru": "Хочешь пойти <w>за</w> кофе?"
                            }
                        },
                        {
                            "text": "Can you go to the store <w>for</w> some eggs?",
                            "target_lang_translations": {
                                "ru": "Можешь сходить в магазин <w>за</w> яйцами?"
                            }
                        },
                        {
                            "text": "I am saving up money <w>for</w> a new car.",
                            "target_lang_translations": {
                                "ru": "Я коплю деньги <w>на</w> новую машину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ask for",
                        "look for",
                        "wait for"
                    ],
                    "target_lang_definitions": {
                        "ru": "С целью получить, приобрести или достичь чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "с целью сходить и принести что-то"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "с целью накопления или приобретения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dad5e410-13c6-3d4a-81a3-1940bddea1d1",
                    "sense_definition": "In exchange for; in place of.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "exchange",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I paid five dollars <w>for</w> this sandwich.",
                            "target_lang_translations": {
                                "ru": "Я заплатил пять долларов <w>за</w> этот сэндвич."
                            }
                        },
                        {
                            "text": "He traded his apple <w>for</w> a banana.",
                            "target_lang_translations": {
                                "ru": "Он обменял свое яблоко <w>на</w> банан."
                            }
                        },
                        {
                            "text": "I got a very good price <w>for</w> that old car.",
                            "target_lang_translations": {
                                "ru": "Я получил очень хорошую цену <w>за</w> ту старую машину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "value for money",
                        "word for word",
                        "eye for an eye"
                    ],
                    "target_lang_definitions": {
                        "ru": "В обмен на; вместо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "указывает на цену или предмет обмена"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "при обмене одного предмета на другой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5630f8a-3c66-39c6-aa1d-7caeaea87cec",
                    "sense_definition": "Because of; as a result of.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was famous <w>for</w> his delicious cooking.",
                            "target_lang_translations": {
                                "ru": "Он был известен своей вкусной стряпней."
                            }
                        },
                        {
                            "text": "I cannot see the road <w>for</w> the thick fog.",
                            "target_lang_translations": {
                                "ru": "Я не вижу дороги <w>из-за</w> густого тумана."
                            }
                        },
                        {
                            "text": "She refused to help him just <w>for</w> that reason.",
                            "target_lang_translations": {
                                "ru": "Она отказалась помочь ему только <w>по</w> этой причине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "famous for",
                        "sorry for",
                        "for this reason"
                    ],
                    "target_lang_definitions": {
                        "ru": "Из-за; в результате."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и́з-за",
                                "target_lang_sense_clarification": "по причине, вследствие чего-либо"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "основание для благодарности, наказания или ответственности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74e014f2-d935-36cb-9c61-bcf6ed3aeb11",
                    "sense_definition": "Supporting or in favour of a person, group, or idea.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Are you <w>for</w> or against the new plan?",
                            "target_lang_translations": {
                                "ru": "Вы <w>за</w> или против нового плана?"
                            }
                        },
                        {
                            "text": "I voted <w>for</w> the candidate who promised change.",
                            "target_lang_translations": {
                                "ru": "Я голосовал <w>за</w> кандидата, который обещал перемены."
                            }
                        },
                        {
                            "text": "Who is <w>for</w> having ice cream after dinner?",
                            "target_lang_translations": {
                                "ru": "Кто <w>за</w> то, чтобы съесть мороженое после ужина?"
                            }
                        }
                    ],
                    "antonyms": [
                        "against"
                    ],
                    "common_phrases": [
                        "vote for",
                        "all for it"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддерживающий или выступающий в пользу человека, группы или идеи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "в поддержку, на стороне кого-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a46faa02-c2d6-3421-9208-42d19d275326",
                    "sense_definition": "Considering the nature or circumstances of; by the standards of.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "standard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is very tall <w>for</w> her age.",
                            "target_lang_translations": {
                                "ru": "Она очень высокая <w>для</w> своего возраста."
                            }
                        },
                        {
                            "text": "It is quite warm outside <w>for</w> a day in January.",
                            "target_lang_translations": {
                                "ru": "На улице довольно тепло <w>для</w> январского дня."
                            }
                        },
                        {
                            "text": "He is very mature <w>for</w> a two-year-old child.",
                            "target_lang_translations": {
                                "ru": "Он очень развит <w>для</w> двухлетнего ребенка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "for his age",
                        "good for a beginner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учитывая характер или обстоятельства; по стандартам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для",
                                "target_lang_sense_clarification": "при сопоставлении с нормой для данной категории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c40b5464-9c45-3a75-a762-5872137ff3ba",
                    "sense_definition": "Used to introduce the subject of an infinitive verb clause.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is important <w>for</w> you to study hard.",
                            "target_lang_translations": {
                                "ru": "<w>Для</w> тебя важно усердно учиться."
                            }
                        },
                        {
                            "text": "There is no need <w>for</w> him to shout so loudly.",
                            "target_lang_translations": {
                                "ru": "Ему нет нужды так громко кричать."
                            }
                        },
                        {
                            "text": "It seems unreasonable <w>for</w> the boss to withhold our wages.",
                            "target_lang_translations": {
                                "ru": "Кажется неразумным <w>для</w> начальника задерживать нам зарплату."
                            }
                        }
                    ],
                    "common_phrases": [
                        "time for us",
                        "wait for it"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения подлежащего инфинитивного оборота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для",
                                "target_lang_sense_clarification": "вводит лицо или предмет, к которому относится действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11169966-68ce-3d68-9724-0b26ecc63b09",
                    "sense_definition": "Despite; in spite of.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "concession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>For</w> all his expensive education, he didn't seem very bright.",
                            "target_lang_translations": {
                                "ru": "<w>Несмотря на</w> всё его дорогое образование, он не казался очень умным."
                            }
                        },
                        {
                            "text": "She was lonely, <w>for</w> all her many friends.",
                            "target_lang_translations": {
                                "ru": "Она была одинока, <w>несмотря на</w> множество друзей."
                            }
                        },
                        {
                            "text": "<w>For</w> all his faults, he was a great leader.",
                            "target_lang_translations": {
                                "ru": "<w>При</w> всех его недостатках, он был великим лидером."
                            }
                        }
                    ],
                    "common_phrases": [
                        "for all that",
                        "for all his faults"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несмотря на; вопреки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несмотря на",
                                "target_lang_sense_clarification": "указывает на уступку"
                            },
                            {
                                "target_lang_word": "при",
                                "target_lang_sense_clarification": "в сочетании 'при всём...'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24fac453-6060-337d-b2bb-c5bf9d050389",
                    "sense_definition": "To be, or regarded as being.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do not take me <w>for</w> a fool.",
                            "target_lang_translations": {
                                "ru": "Не принимай меня <w>за</w> дурака."
                            }
                        },
                        {
                            "text": "They knew him <w>for</w> a stranger immediately.",
                            "target_lang_translations": {
                                "ru": "Они сразу приняли его <w>за</w> чужака."
                            }
                        },
                        {
                            "text": "We often take our health <w>for</w> granted.",
                            "target_lang_translations": {
                                "ru": "Мы часто принимаем наше здоровье <w>как</w> должное."
                            }
                        }
                    ],
                    "common_phrases": [
                        "take for granted",
                        "take for a fool"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть или считаться кем-то/чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "принимать в качестве кого-то"
                            },
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "в устойчивых выражениях (как должное)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "0d43d519-b7cf-339a-bdfb-e1912d86bc26",
                    "sense_definition": "Because; seeing that.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "causation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I had to stay with my relatives, <w>for</w> I had nowhere else to go.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось остаться у родственников, <w>потому что</w> мне некуда было больше идти."
                            }
                        },
                        {
                            "text": "He felt very guilty, <w>for</w> he knew he had lied to his friend.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя очень виноватым, <w>ибо</w> знал, что солгал своему другу."
                            }
                        }
                    ],
                    "synonyms": [
                        "because",
                        "seeing that",
                        "given that"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Потому что; так как."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потому́ что",
                                "target_lang_sense_clarification": "союз причины"
                            },
                            {
                                "target_lang_word": "ибо",
                                "target_lang_sense_clarification": "книжный или устаревший союз"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "dd7cf802-87ae-310e-9f48-bfd2ef17457c",
                    "sense_definition": "To; used to mark the following verb as an infinitive.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "infinitive_marker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am going <w>for</w> get some food now.",
                            "target_lang_translations": {
                                "ru": "Я иду, <w>чтобы</w> раздобыть еды."
                            }
                        },
                        {
                            "text": "He was not able <w>for</w> come to the party.",
                            "target_lang_translations": {
                                "ru": "Он не смог прийти на вечеринку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Caribbean or AAVE"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чтобы; используется для маркировки следующего глагола как инфинитива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чтобы",
                                "target_lang_sense_clarification": "для обозначения цели (в диалектах)"

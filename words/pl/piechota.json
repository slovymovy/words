{
    "word_family": [
        "piechur",
        "pieszy",
        "piechociarz"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "59af67db-fdfa-3e93-99ea-7025267f397a",
                    "sense_definition": "Sposób przemieszczania się o własnych siłach na nogach; słowo używane niemal wyłącznie w wyrażeniach „na piechotę” lub „piechotą”.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "walking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mieliśmy pieniędzy na taksówkę, więc wróciliśmy do domu <w>piechotą</w>.",
                            "target_lang_translations": {
                                "ru": "У нас не было денег на такси, поэтому мы вернулись домой <w>пешком</w>."
                            }
                        },
                        {
                            "text": "Do szkoły mam blisko, więc zawsze chodzę tam na <w>piechotę</w>.",
                            "target_lang_translations": {
                                "ru": "До школы мне близко, поэтому я всегда хожу туда <w>пешком</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pieszo"
                    ],
                    "common_phrases": [
                        "na piechotę",
                        "iść piechotą",
                        "chodzić piechotą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ передвижения собственными силами, на ногах; слово используется почти исключительно в выражениях «na piechotę» или «piechotą»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пешком",
                                "target_lang_sense_clarification": "способ передвижения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc78dbd3-ce84-314d-a18f-1aaada57b9ae",
                    "sense_definition": "Rodzaj wojsk lądowych, których żołnierze walczą pieszo, a nie konno czy w pojazdach bojowych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Piechota</w> ruszyła do ataku zaraz po ostrzale artyleryjskim.",
                            "target_lang_translations": {
                                "ru": "<w>Пехота</w> пошла в атаку сразу после артиллерийского обстрела."
                            }
                        },
                        {
                            "text": "Współczesna <w>piechota</w> morska jest elitarną formacją wojskową.",
                            "target_lang_translations": {
                                "ru": "Современная морская <w>пехота</w> является элитным воинским формированием."
                            }
                        }
                    ],
                    "synonyms": [
                        "infanteria"
                    ],
                    "antonyms": [
                        "kawaleria"
                    ],
                    "common_phrases": [
                        "piechota morska",
                        "lekka piechota",
                        "piechota zmechanizowana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Род сухопутных войск, солдаты которых сражаются в пешем строю, а не верхом или на боевых машинах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пехота",
                                "target_lang_sense_clarification": "род войск"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48faa869-76e1-3580-a75b-9e41ac829ded",
                    "sense_definition": "Odmiana fasoli karłowej, która nie pnie się wysoko i nie wymaga stosowania tyczek do podpierania.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym roku w naszym ogrodzie posadziliśmy fasolę <w>piechotę</w>.",
                            "target_lang_translations": {
                                "ru": "В этом году в нашем саду мы посадили <w>кустовую фасоль</w>."
                            }
                        },
                        {
                            "text": "<w>Piechota</w> jest łatwiejsza w uprawie, ponieważ nie trzeba przygotowywać dla niej podpór.",
                            "target_lang_translations": {
                                "ru": "<w>Кустовая фасоль</w> проще в выращивании, так как для неё не нужно готовить опоры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fasola piechota"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность кустовой фасоли, которая не вьется высоко и не требует использования шестов для опоры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кустовая фасоль",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a97b93b-1372-34ba-b654-dbc4f2df1b4e",
                    "sense_definition": "Potoczne lub żartobliwe określenie drobiu domowego, takiego jak kury, gęsi czy kaczki.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodyni wypuściła rano całą <w>piechotę</w> na podwórko.",
                            "target_lang_translations": {
                                "ru": "Хозяйка утром выпустила всю <w>домашнюю птицу</w> во двор."
                            }
                        },
                        {
                            "text": "Muszę iść nakarmić moją <w>piechotę</w>, bo głośno domaga się ziarna.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти покормить мою <w>птицу</w>, так как она громко требует зерна."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное или шутливое название домашней птицы, такой как куры, гуси или утки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "домашняя птица",
                                "target_lang_sense_clarification": "собирательное"
                            },
                            {
                                "target_lang_word": "живность",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "23b0a7f0-3092-3ca9-ac2f-8c4099e702ce",
                    "sense_definition": "Polskie nazwisko męskie lub żeńskie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Piechota</w> jest znanym lekarzem w naszym szpitalu.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Пехота</w> — известный врач в нашей больнице."
                            }
                        },
                        {
                            "text": "Nie znam nikogo o nazwisku <w>Piechota</w> w tej okolicy.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого по фамилии <w>Пехота</w> в этой округе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская мужская или женская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пехота",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
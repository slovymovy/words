                {
                    "sense_id": "1c334073-7b59-3487-abba-c50645a9c3cf",
                    "sense_definition": "Ledemaat (been of arm) van een dier.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "limb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hond gaf braaf een <w>poot</w> aan zijn baasje.",

            "senses": [
                {
                    "sense_id": "23b77145-fa39-39c9-8e5e-105071c693fc",
                    "sense_definition": "De opstelling van de troepen tijdens een mars",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "military_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten marcheerden in een strakke <w>marsorde</w> over het plein.",
                            "target_lang_translations": {
                                "en": "The soldiers marched across the square in tight <w>marching order</w>.",
                                "ru": "Солдаты маршировали по площади в строгом <w>походном порядке</w>."
                            }
                        },
                        {
                            "text": "De generaal controleerde de <w>marsorde</w> voordat het leger vertrok.",
                            "target_lang_translations": {
                                "en": "The general checked the <w>order of march</w> before the army departed.",
                                "ru": "Генерал проверил <w>походный порядок</w> перед выступлением армии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in marsorde"
                    ],
                    "target_lang_definitions": {
                        "en": "The specific arrangement or sequence of troops during a march",
                        "ru": "Построение войск во время марша"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "order of march",
                                "target_lang_sense_clarification": "sequence of units"
                            },
                            {
                                "target_lang_word": "marching order",
                                "target_lang_sense_clarification": "formation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "походный порядок",
                                "target_lang_sense_clarification": "военный термин, обозначающий построение для передвижения"
                            },
                            {
                                "target_lang_word": "строй",
                                "target_lang_sense_clarification": "общее название для размещения военнослужащих"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f91dbf88-104d-3e07-8b9b-1ac2af0b14eb",
                    "sense_definition": "De richting waarin de troepen zich bewegen",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "military_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bataljon veranderde van <w>marsorde</w> toen de vijand in zicht kwam.",
                            "target_lang_translations": {
                                "en": "The battalion changed <w>formation</w> when the enemy came into view.",
                                "ru": "Батальон сменил <w>порядок движения</w>, когда показался враг."
                            }
                        },
                        {
                            "text": "De troepen naderden in <w>marsorde</w> vanuit het oosten.",
                            "target_lang_translations": {
                                "en": "The troops approached in <w>marching order</w> from the east.",
                                "ru": "Войска приближались в <w>походном порядке</w> с востока."
                            }
                        }
                    ],
                    "target_lang_definitions": {

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "782de4f9-4a92-3b74-806f-7c97c52de543",
                    "sense_definition": "Обнаруживать кого-либо или что-либо в результате поисков или случайно",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "discovery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он умеет быстро <w>находить</w> потерянные вещи в доме.",
                            "target_lang_translations": {
                                "en": "He knows how to quickly <w>find</w> lost things in the house."
                            }
                        },
                        {
                            "text": "Мне нравится гулять по лесу и <w>находить</w> красивые цветы.",
                            "target_lang_translations": {
                                "en": "I like walking in the forest and <w>finding</w> beautiful flowers."
                            }
                        }
                    ],
                    "synonyms": [
                        "обнаруживать",
                        "отыскивать"
                    ],
                    "antonyms": [
                        "терять"
                    ],
                    "common_phrases": [
                        "находить время",
                        "находить дорогу",
                        "находить друзей",
                        "находить работу"
                    ],
                    "target_lang_definitions": {
                        "en": "To discover or locate someone or something physically"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find",
                                "target_lang_sense_clarification": "general term for discovering something"
                            },
                            {
                                "target_lang_word": "locate",
                                "target_lang_sense_clarification": "to find the exact position of something"
                            },
                            {
                                "target_lang_word": "discover",
                                "target_lang_sense_clarification": "to find something unexpectedly or for the first time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "918ca03b-669f-3e64-afe7-e4dd7bcd9e42",
                    "sense_definition": "Приходить к чему-либо в результате размышлений или усилий (получать решение, ответ)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "discovery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В любой сложной ситуации нужно стараться <w>находить</w> компромисс.",
                            "target_lang_translations": {
                                "en": "In any difficult situation, one must try to <w>find</w> a compromise."
                            }
                        },
                        {
                            "text": "Умный студент всегда сможет <w>находить</w> правильные ответы на экзамене.",
                            "target_lang_translations": {
                                "en": "A smart student will always be able to <w>find</w> the right answers on the exam."
                            }
                        }
                    ],
                    "synonyms": [
                        "отыскивать"
                    ],
                    "common_phrases": [
                        "находить решение",
                        "находить выход",
                        "находить компромисс",
                        "находить общий язык"
                    ],
                    "target_lang_definitions": {
                        "en": "To reach a solution, answer, or result through effort or thought"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find",
                                "target_lang_sense_clarification": "to obtain a solution or answer"
                            },
                            {
                                "target_lang_word": "come up with",
                                "target_lang_sense_clarification": "to produce a solution or idea"
                            },
                            {
                                "target_lang_word": "reach",
                                "target_lang_sense_clarification": "to arrive at an agreement or compromise"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95364a1e-5e19-3094-acd8-9883df38c6b5",
                    "sense_definition": "Считать, признавать каким-либо, иметь мнение о чём-либо",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я начинаю <w>находить</w> эту книгу довольно скучной и затянутой.",
                            "target_lang_translations": {
                                "en": "I am starting to <w>find</w> this book rather boring and dragged out."
                            }
                        },
                        {
                            "text": "Критики могут <w>находить</w> этот фильм странным, но зрителям он нравится.",
                            "target_lang_translations": {
                                "en": "Critics might <w>find</w> this movie strange, but the audience likes it."
                            }
                        }
                    ],
                    "synonyms": [
                        "считать",
                        "полагать",
                        "думать",
                        "рассматривать"
                    ],
                    "common_phrases": [
                        "находить странным",
                        "находить возможным",
                        "находить интересным",
                        "находить нужным"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a specific opinion about something; to consider something to be a certain way"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find",
                                "target_lang_sense_clarification": "to perceive or experience something in a certain way"
                            },
                            {
                                "target_lang_word": "consider",
                                "target_lang_sense_clarification": "to think of someone or something in a particular way"
                            },
                            {
                                "target_lang_word": "deem",
                                "target_lang_sense_clarification": "formal; to judge or consider"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16eefd19-2f47-377d-89bd-c30e6689ccd7",
                    "sense_definition": "Надвигаясь, закрывать или покрывать собой (обычно о тучах, тени)",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "motion_cover",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы видели, как на солнце медленно <w>находит</w> тёмная грозовая туча.",
                            "target_lang_translations": {
                                "en": "We saw a dark storm cloud slowly <w>coming over</w> the sun."
                            }
                        },
                        {
                            "text": "Вечером тень от горы <w>находит</w> на нашу деревню.",
                            "target_lang_translations": {
                                "en": "In the evening, the shadow from the mountain <w>comes over</w> our village."
                            }
                        }
                    ],
                    "synonyms": [
                        "надвигаться"
                    ],
                    "antonyms": [
                        "сходить"
                    ],
                    "common_phrases": [
                        "туча находит",
                        "тень находит",
                        "коса нашла на камень"
                    ],
                    "target_lang_definitions": {
                        "en": "To move over and cover something (typically of clouds or shadows)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come over",
                                "target_lang_sense_clarification": "to move into a position covering something"
                            },
                            {
                                "target_lang_word": "cover",
                                "target_lang_sense_clarification": "to extend over an area"
                            },
                            {
                                "target_lang_word": "descend upon",
                                "target_lang_sense_clarification": "to come down on or cover"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9b6d128-173f-3700-862f-e0dcae808e22",
                    "sense_definition": "Внезапно появляться, овладевать кем-либо (о мыслях, чувствах, состояниях)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "state_mood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Иногда на поэта <w>находит</w> вдохновение посреди ночи.",
                            "target_lang_translations": {
                                "en": "Sometimes inspiration <w>comes upon</w> the poet in the middle of the night."
                            }
                        },
                        {
                            "text": "На него часто <w>находит</w> грусть, когда идёт осенний дождь.",
                            "target_lang_translations": {
                                "en": "Sadness often <w>comes over</w> him when the autumn rain falls."
                            }
                        }
                    ],
                    "antonyms": [
                        "отпускать"
                    ],
                    "common_phrases": [
                        "находит вдохновение",
                        "находит грусть",
                        "находит страх",
                        "находит стих"
                    ],
                    "target_lang_definitions": {
                        "en": "To suddenly affect or take control of someone (of feelings, thoughts, or states)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come over",
                                "target_lang_sense_clarification": "to affect someone suddenly (of a feeling)"
                            },
                            {
                                "target_lang_word": "come upon",
                                "target_lang_sense_clarification": "to affect or strike someone"
                            },
                            {
                                "target_lang_word": "strike",
                                "target_lang_sense_clarification": "to occur suddenly (like inspiration)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d22e885-bc07-3c72-a8e3-c44b23373eb3",
                    "sense_definition": "Вычислять, получать результат математического действия",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "calculation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Школьники учатся <w>находить</w> площадь треугольника на уроках геометрии.",
                            "target_lang_translations": {
                                "en": "Schoolchildren learn to <w>find</w> the area of a triangle in geometry lessons."
                            }
                        },
                        {
                            "text": "Программа помогает быстро <w>находить</w> сумму всех чисел в таблице.",
                            "target_lang_translations": {
                                "en": "The program helps to quickly <w>find</w> the sum of all numbers in the table."
                            }
                        }
                    ],
                    "common_phrases": [
                        "находить сумму",
                        "находить корень",
                        "находить значение"
                    ],
                    "target_lang_definitions": {
                        "en": "To calculate or determine a mathematical result"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find",
                                "target_lang_sense_clarification": "to determine a value by calculation"
                            },
                            {
                                "target_lang_word": "calculate",
                                "target_lang_sense_clarification": "to determine mathematically"
                            },
                            {
                                "target_lang_word": "work out",
                                "target_lang_sense_clarification": "to solve or calculate"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88e8e328-2805-3bc4-88fe-49749056782a",
                    "sense_definition": "Проходить пешком определённое расстояние",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "walking_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За день туристы успели <w>находить</w> около двадцати километров.",
                            "target_lang_translations": {
                                "en": "During the day, the tourists managed to <w>walk</w> about twenty kilometers."
                            }
                        },
                        {
                            "text": "Мой шагомер показывает, сколько я могу <w>находить</w> за неделю.",
                            "target_lang_translations": {
                                "en": "My pedometer shows how much I can <w>walk</w> in a week."
                            }
                        }
                    ],
                    "synonyms": [
                        "нашагать"
                    ],
                    "common_phrases": [
                        "находить километры"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cover a certain distance by walking"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "walk",
                                "target_lang_sense_clarification": "to travel a distance on foot"
                            },
                            {
                                "target_lang_word": "cover",
                                "target_lang_sense_clarification": "to travel a specific distance"
                            },
                            {
                                "target_lang_word": "clock up",
                                "target_lang_sense_clarification": "informal; to accumulate distance"
                            }
                        ]
                    }
                }
            ]
        }
    ]

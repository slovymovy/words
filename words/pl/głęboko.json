{
    "word_family": [
        "głęboki",
        "głębia",
        "głębokość",
        "pogłębiać",
        "zagłębiać"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "cf4462a5-dcb0-3468-a1a7-bc6c6f7ccc34",
                    "sense_definition": "Znajdując się w dużej odległości od powierzchni, brzegu lub wejścia, głęboko wewnątrz czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_depth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nurek zszedł <głęboko> pod wodę, aby obejrzeć rafę.",
                            "target_lang_translations": {
                                "ru": "Дайвер опустился <глубоко> под воду, чтобы осмотреть риф."
                            }
                        },
                        {
                            "text": "Korzenie tego starego dębu sięgają bardzo <głęboko> w ziemię.",
                            "target_lang_translations": {
                                "ru": "Корни этого старого дуба уходят очень <глубоко> в землю."
                            }
                        }
                    ],
                    "antonyms": [
                        "płytko"
                    ],
                    "common_phrases": [
                        "głęboko pod wodą",
                        "głęboko w lesie",
                        "głęboko w ziemi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находясь на большом расстоянии от поверхности, края или входа, глубоко внутри чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глубоко",
                                "target_lang_sense_clarification": "о физическом расстоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "079eedc5-c60e-39e7-ad90-452ffe2f370b",
                    "sense_definition": "W stopniu bardzo dużym, intensywnym lub przepełnionym emocjami; w sposób świadczący o dużym zaangażowaniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem <głęboko> przekonany, że podjęliśmy właściwą decyzję.",
                            "target_lang_translations": {
                                "ru": "Я <глубоко> убежден, что мы приняли правильное решение."
                            }
                        },
                        {
                            "text": "Jej smutna historia <głęboko> mnie poruszyła.",
                            "target_lang_translations": {
                                "ru": "Её печальная история <глубоко> меня тронула."
                            }
                        }
                    ],
                    "synonyms": [
                        "dogłębnie"
                    ],
                    "common_phrases": [
                        "głęboko wierzyć",
                        "głęboko przekonany",
                        "głęboko wzruszony"
                    ],
                    "target_lang_definitions": {
                        "ru": "В очень сильной степени, интенсивно или с большим чувством; с полной отдачей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глубоко",
                                "target_lang_sense_clarification": "о чувствах или убеждениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "409314ab-b24a-3043-bad4-5cb3e3201f64",
                    "sense_definition": "Wciągając dużą ilość powietrza do płuc podczas oddychania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "breathing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <głęboko> oddychać, żeby uspokoić nerwy przed egzaminem.",
                            "target_lang_translations": {
                                "ru": "Ты должен <глубоко> дышать, чтобы успокоить нервы перед экзаменом."
                            }
                        },
                        {
                            "text": "Pacjent spał spokojnie i oddychał <głęboko>.",
                            "target_lang_translations": {
                                "ru": "Пациент спал спокойно и дышал <глубоко>."
                            }
                        }
                    ],
                    "antonyms": [
                        "płytko"
                    ],
                    "common_phrases": [
                        "głęboko oddychać",
                        "głęboko wciągnąć powietrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вбирая большое количество воздуха в легкие при дыхании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глубоко",
                                "target_lang_sense_clarification": "о дыхании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "377f10c6-bede-3ddd-8c73-8900cf1ff68d",
                    "sense_definition": "W odległym punkcie czasu, na przykład późno w nocy lub dawno w przeszłości.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Siedzieliśmy przy ognisku i rozmawialiśmy <głęboko> w noc.",
                            "target_lang_translations": {
                                "ru": "Мы сидели у костра и разговаривали <глубоко> за полночь."
                            }
                        },
                        {
                            "text": "Tradycje tego regionu sięgają <głęboko> w przeszłość.",
                            "target_lang_translations": {
                                "ru": "Традиции этого региона уходят <глубоко> в прошлое."
                            }
                        }
                    ],
                    "common_phrases": [
                        "głęboko w nocy",
                        "sięgać głęboko w przeszłość"
                    ],
                    "target_lang_definitions": {
                        "ru": "В отдаленный момент времени, например, поздно ночью или давно в прошлом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глубоко",
                                "target_lang_sense_clarification": "о времени (поздно или давно)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
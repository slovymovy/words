                    ],
                    "antonyms": [
                        "oosters",
                        "niet-westers"
                    ],
                    "common_phrases": [
                        "westerse wereld",
                        "westerse cultuur",
                        "westerse samenleving",
                        "westerse waarden"
                    ],
                    "target_lang_definitions": {

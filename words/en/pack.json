{
    "word_family": [
        "package",
        "packet",
        "packaging",
        "packer",
        "backpack",
        "unpack",
        "prepack"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "12c0fdec-fe22-37a6-a49e-d8909558ae51",
                    "sense_definition": "A small cardboard or paper container and the items contained within it.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "container_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He went to the shop to buy a <w>pack</w> of gum.",
                            "target_lang_translations": {
                                "ru": "Он пошел в магазин, чтобы купить <w>пачку</w> жвачки."
                            }
                        },
                        {
                            "text": "She smokes a <w>pack</w> of cigarettes every single day.",
                            "target_lang_translations": {
                                "ru": "Она выкуривает <w>пачку</w> сигарет каждый божий день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pack of cigarettes",
                        "pack of gum",
                        "six-pack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая картонная или бумажная упаковка и предметы, содержащиеся в ней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пачка",
                                "target_lang_sense_clarification": "сигарет, жвачки"
                            },
                            {
                                "target_lang_word": "упаковка",
                                "target_lang_sense_clarification": "общая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c920afad-c70b-3537-9c59-a8dd1f6d20d8",
                    "sense_definition": "A bag or bundle of items prepared to be carried, especially on the back.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hiker adjusted the heavy <w>pack</w> on his back before climbing.",
                            "target_lang_translations": {
                                "ru": "Турист поправил тяжелый <w>рюкзак</w> на спине перед восхождением."
                            }
                        },
                        {
                            "text": "The horses carried the <w>packs</w> across the dusty plain.",
                            "target_lang_translations": {
                                "ru": "Лошади несли <w>тюки</w> через пыльную равнину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "heavy pack",
                        "back pack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумка или связка вещей, подготовленная для переноски, особенно на спине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рюкзак",
                                "target_lang_sense_clarification": "туристический"
                            },
                            {
                                "target_lang_word": "тюк",
                                "target_lang_sense_clarification": "связка вещей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30708bea-3db8-3eaf-b6a4-06920c9e22ee",
                    "sense_definition": "A group of wild animals, such as wolves or dogs, that live and hunt together.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>pack</w> of wolves howled loudly in the distance.",
                            "target_lang_translations": {
                                "ru": "<w>Стая</w> волков громко выла вдалеке."
                            }
                        },
                        {
                            "text": "The hunting dogs followed the scent as a tight <w>pack</w>.",
                            "target_lang_translations": {
                                "ru": "Охотничьи собаки шли по следу плотной <w>сворой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pack of wolves",
                        "leader of the pack",
                        "hunting pack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа диких животных, таких как волки или собаки, которые живут и охотятся вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стая",
                                "target_lang_sense_clarification": "волков"
                            },
                            {
                                "target_lang_word": "свора",
                                "target_lang_sense_clarification": "собак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5e814e4-2d18-36a1-98aa-bee806ca7c4e",
                    "sense_definition": "A complete set of playing cards used for games.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We wanted to play poker but nobody brought a <w>pack</w>.",
                            "target_lang_translations": {
                                "ru": "Мы хотели поиграть в покер, но никто не принес <w>колоду</w>."
                            }
                        },
                        {
                            "text": "Please shuffle the <w>pack</w> thoroughly before you deal the cards.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, тщательно перетасуй <w>колоду</w> перед раздачей карт."
                            }
                        }
                    ],
                    "synonyms": [
                        "deck"
                    ],
                    "common_phrases": [
                        "pack of cards",
                        "shuffle the pack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный набор игральных карт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колода",
                                "target_lang_sense_clarification": "карт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cdea7aa-d6eb-37a2-830c-ced6d7ad9eb2",
                    "sense_definition": "A bag, pad, or wrap containing something hot or cold, used to treat pain or swelling.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor told him to put an ice <w>pack</w> on his swollen ankle.",
                            "target_lang_translations": {
                                "ru": "Врач сказал ему приложить <w>пакет</w> со льдом к опухшей лодыжке."
                            }
                        },
                        {
                            "text": "She applied a hot <w>pack</w> to her sore back muscles.",
                            "target_lang_translations": {
                                "ru": "Она приложила горячий <w>компресс</w> к больным мышцам спины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ice pack",
                        "cold pack",
                        "heat pack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мешок, подушечка или повязка с чем-то горячим или холодным, используемая для лечения боли или отеков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пакет",
                                "target_lang_sense_clarification": "со льдом"
                            },
                            {
                                "target_lang_word": "компресс",
                                "target_lang_sense_clarification": "медицинский"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c50a8bd-2085-36eb-a08c-7580fa613809",
                    "sense_definition": "A group of people or things, often regarded negatively or as a large amount.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't believe him because everything he says is a <w>pack</w> of lies.",
                            "target_lang_translations": {
                                "ru": "Не верь ему, потому что все, что он говорит, — это <w>куча</w> лжи."
                            }
                        },
                        {
                            "text": "A <w>pack</w> of thieves managed to steal the jewels from the safe.",
                            "target_lang_translations": {
                                "ru": "<w>Шайке</w> воров удалось украсть драгоценности из сейфа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pack of lies",
                        "pack of thieves",
                        "pack of trouble"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей или предметов, часто воспринимаемая негативно или как большое количество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куча",
                                "target_lang_sense_clarification": "лжи, неприятностей"
                            },
                            {
                                "target_lang_word": "шайка",
                                "target_lang_sense_clarification": "воров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d257140-7222-3cc7-9a65-40448cef5517",
                    "sense_definition": "The group of forward players in a rugby team.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain had to replace a player in the <w>pack</w> due to injury.",
                            "target_lang_translations": {
                                "ru": "Капитану пришлось заменить игрока <w>нападения</w> из-за травмы."
                            }
                        },
                        {
                            "text": "Our <w>pack</w> pushed forward with great strength during the scrum.",
                            "target_lang_translations": {
                                "ru": "Наши <w>нападающие</w> с большой силой давили вперед во время схватки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа нападающих в команде по регби."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нападающие",
                                "target_lang_sense_clarification": "в регби"
                            },
                            {
                                "target_lang_word": "схватка",
                                "target_lang_sense_clarification": "элемент игры"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "495c6a7d-f31b-386d-bb9c-d6d4fadddb21",
                    "sense_definition": "To put clothes and other possessions into bags, boxes, or suitcases, especially for travel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "container_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>pack</w> my suitcase before we leave for the airport.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>собрать</w> чемодан перед отъездом в аэропорт."
                            }
                        },
                        {
                            "text": "She <w>packed</w> her lunch in a small plastic box.",
                            "target_lang_translations": {
                                "ru": "Она <w>упаковала</w> свой обед в маленькую пластиковую коробку."
                            }
                        }
                    ],
                    "antonyms": [
                        "unpack"
                    ],
                    "common_phrases": [
                        "pack a bag",
                        "pack for a trip",
                        "pack up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Складывать одежду и другие вещи в сумки, коробки или чемоданы, особенно для путешествия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "вещи, чемодан"
                            },
                            {
                                "target_lang_word": "упаковывать",
                                "target_lang_sense_clarification": "багаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f4bcba7-a631-377a-a1b4-571ed2208cc4",
                    "sense_definition": "To fill a space or container completely; to crowd together in large numbers.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "crowd_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thousands of excited fans <w>packed</w> the stadium for the concert.",
                            "target_lang_translations": {
                                "ru": "Тысячи взволнованных фанатов <w>заполнили</w> стадион ради концерта."
                            }
                        },
                        {
                            "text": "The morning bus was <w>packed</w> with commuters going to work.",
                            "target_lang_translations": {
                                "ru": "Утренний автобус был <w>битком набит</w> пассажирами, едущими на работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pack the stadium",
                        "packed with people",
                        "pack into"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью заполнять пространство или контейнер; собираться в большом количестве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заполнять",
                                "target_lang_sense_clarification": "пространство"
                            },
                            {
                                "target_lang_word": "набиваться",
                                "target_lang_sense_clarification": "битком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2fd01c7-b70a-30f4-8784-0c9a3de2dd3a",
                    "sense_definition": "To put food or goods into containers or materials for storage, preservation, or transport.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "container_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory <w>packs</w> fresh fish in cans for export.",
                            "target_lang_translations": {
                                "ru": "Фабрика <w>фасует</w> свежую рыбу в банки на экспорт."
                            }
                        },
                        {
                            "text": "These fragile glass items must be <w>packed</w> very carefully.",
                            "target_lang_translations": {
                                "ru": "Эти хрупкие стеклянные предметы должны быть <w>упакованы</w> очень осторожно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pack in boxes",
                        "meat packing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать продукты или товары в контейнеры или материалы для хранения или транспортировки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упаковывать",
                                "target_lang_sense_clarification": "товары"
                            },
                            {
                                "target_lang_word": "фасовать",
                                "target_lang_sense_clarification": "продукты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc21f75c-487f-309b-ab44-33323364ddc7",
                    "sense_definition": "To carry a gun, usually concealed on one's person.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weapon_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer was <w>packing</w> a pistol under his jacket.",
                            "target_lang_translations": {
                                "ru": "Полицейский <w>носил</w> пистолет под курткой."
                            }
                        },
                        {
                            "text": "He was arrested because he was <w>packing</w> a weapon without a permit.",
                            "target_lang_translations": {
                                "ru": "Его арестовали, потому что он <w>носил</w> оружие без разрешения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pack heat",
                        "packing a gun"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Носить с собой оружие, обычно скрытно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "носить оружие",
                                "target_lang_sense_clarification": "при себе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df75f6d7-46b3-3fe3-8021-65076a4650bb",
                    "sense_definition": "To arrange or select people or things unfairly to achieve a desired result.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cheat_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The corrupt leader tried to <w>pack</w> the committee with his loyal supporters.",
                            "target_lang_translations": {
                                "ru": "Коррумпированный лидер пытался <w>наводнить</w> комитет своими верными сторонниками."
                            }
                        },
                        {
                            "text": "It is illegal to <w>pack</w> a jury to ensure a specific verdict.",
                            "target_lang_translations": {
                                "ru": "Незаконно <w>подтасовывать состав</w> присяжных, чтобы обеспечить определенный вердикт."
                            }
                        }
                    ],
                    "synonyms": [
                        "stack"
                    ],
                    "common_phrases": [
                        "pack the court",
                        "pack a jury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечестно подбирать людей или организовывать что-либо для достижения желаемого результата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подтасовывать",
                                "target_lang_sense_clarification": "состав"
                            },
                            {
                                "target_lang_word": "наполнять",
                                "target_lang_sense_clarification": "своими людьми"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
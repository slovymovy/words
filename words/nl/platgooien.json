                        "de site platgooien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренное выведение из строя компьютерной системы, сервера или веб-сайта.",
                        "en": "To cause a computer system, server, or website to stop functioning."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обрушить",
                                "target_lang_sense_clarification": "сломать сервер или сайт"
                            },
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "сленг: вывести из строя"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "take down",
                                "target_lang_sense_clarification": "server or website"
                            },
                            {
                                "target_lang_word": "crash",
                                "target_lang_sense_clarification": "system or network"
                            }
                        ]
                    }
                }
            ]
        }
    ]

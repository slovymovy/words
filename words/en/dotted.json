{
    "word_family": [
        "dot",
        "dotty",
        "undotted"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "489f26d5-f43b-38c8-bb77-a955d3ae6a79",
                    "sense_definition": "Made up of a series of small round marks or dots.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pattern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please print your name and address clearly on the <w>dotted</w> line.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, разборчиво напишите ваше имя и адрес на <w>пунктирной</w> линии."
                            }
                        },
                        {
                            "text": "She wore a white dress with a <w>dotted</w> pattern.",
                            "target_lang_translations": {
                                "ru": "На ней было белое платье с узором <w>в горошек</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "spotted",
                        "stippled"
                    ],
                    "antonyms": [
                        "solid",
                        "continuous"
                    ],
                    "common_phrases": [
                        "dotted line",
                        "dotted pattern"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий из ряда маленьких круглых отметок или точек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пунктирный",
                                "target_lang_sense_clarification": "о линии или начертании"
                            },
                            {
                                "target_lang_word": "в горошек",
                                "target_lang_sense_clarification": "об узоре на ткани"
                            },
                            {
                                "target_lang_word": "точечный",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5c4bef3-2fbe-3f5c-a234-269bd23ba2d8",
                    "sense_definition": "Covered with irregularly scattered objects.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scattered",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The green hillside was <w>dotted</w> with white sheep.",
                            "target_lang_translations": {
                                "ru": "Зеленый склон холма был <w>усеян</w> белыми овцами."
                            }
                        },
                        {
                            "text": "Small islands are <w>dotted</w> throughout the large bay.",
                            "target_lang_translations": {
                                "ru": "Маленькие острова <w>разбросаны</w> по всему большому заливу."
                            }
                        }
                    ],
                    "synonyms": [
                        "speckled",
                        "sprinkled",
                        "scattered"
                    ],
                    "antonyms": [
                        "empty",
                        "bare"
                    ],
                    "common_phrases": [
                        "dotted with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытый неравномерно разбросанными объектами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усеянный",
                                "target_lang_sense_clarification": "покрытый множеством мелких объектов"
                            },
                            {
                                "target_lang_word": "разбросанный",
                                "target_lang_sense_clarification": "расположенный в разных местах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71c42bfc-6be2-3f41-91a7-3e9c9b398c40",
                    "sense_definition": "Containing dots as separators, often used in technical or computing contexts.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The server address is written in <w>dotted</w> decimal notation.",
                            "target_lang_translations": {
                                "ru": "Адрес сервера записан в <w>десятично-точечной</w> нотации."
                            }
                        },
                        {
                            "text": "Programmers often use <w>dotted</w> names to organize code libraries.",
                            "target_lang_translations": {
                                "ru": "Программисты часто используют имена <w>с точками</w> для организации библиотек кода."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dotted decimal",
                        "dotted quad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий точки в качестве разделителей (в техническом контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с точками",
                                "target_lang_sense_clarification": "об именах или адресах"
                            },
                            {
                                "target_lang_word": "точечный",
                                "target_lang_sense_clarification": "о нотации (напр. десятично-точечная)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70169ee6-202f-3846-811e-f8d8d758c561",
                    "sense_definition": "Having a dot written after a note to increase its duration by half.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>dotted</w> quarter note lasts longer than a regular one.",
                            "target_lang_translations": {
                                "ru": "Четвертная нота <w>с точкой</w> длится дольше обычной."
                            }
                        },
                        {
                            "text": "The melody is characterized by a lively <w>dotted</w> rhythm.",
                            "target_lang_translations": {
                                "ru": "Мелодия характеризуется живым <w>пунктирным</w> ритмом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dotted note",
                        "dotted rhythm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий точку после ноты, увеличивающую её длительность наполовину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с точкой",
                                "target_lang_sense_clarification": "о музыкальной ноте"
                            },
                            {
                                "target_lang_word": "пунктирный",
                                "target_lang_sense_clarification": "о ритме"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4b4a6bf9-75fb-3f11-a933-c884616d5f19",

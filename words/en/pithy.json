    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "73231014-5ce9-35de-8581-61e605c54303",
                    "sense_definition": "Concise and full of meaning.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "meaningful_conciseness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The journalist wrote a <w>pithy</w> headline that captured the entire story.",
                            "target_lang_translations": {
                                "ru": "Журналист написал <w>ёмкий</w> заголовок, который отразил суть всей истории."
                            }
                        },
                        {
                            "text": "He is famous for his <w>pithy</w> remarks during interviews.",
                            "target_lang_translations": {
                                "ru": "Он известен своими <w>лаконичными</w> замечаниями во время интервью."
                            }
                        }
                    ],
                    "synonyms": [
                        "concise",
                        "succinct",
                        "terse",
                        "laconic"
                    ],
                    "antonyms": [
                        "verbose",
                        "long-winded",
                        "rambling"
                    ],
                    "common_phrases": [
                        "pithy remark",
                        "pithy observation",
                        "pithy quote",
                        "pithy saying"
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткий, но глубокий по смыслу; содержательный и выразительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ёмкий",
                                "target_lang_sense_clarification": "содержательный, вмещающий много смысла в краткую форму"
                            },
                            {
                                "target_lang_word": "лаконичный",
                                "target_lang_sense_clarification": "краткий и чёткий в изложении мыслей"
                            },
                            {
                                "target_lang_word": "сжатый",
                                "target_lang_sense_clarification": "краткий, без лишних слов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1705697-b968-352b-ae3e-4b38e75af6bc",
                    "sense_definition": "Resembling or containing pith; having a dry, spongy texture.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old radishes had become <w>pithy</w> and were no longer good to eat.",
                            "target_lang_translations": {
                                "ru": "Старый редис стал <w>рыхлым</w> и больше не годился в пищу."
                            }
                        },
                        {
                            "text": "Inside the dry orange, the flesh was white and <w>pithy</w>.",
                            "target_lang_translations": {
                                "ru": "Внутри сухого апельсина мякоть была белой и <w>губчатой</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "juicy",
                        "solid",
                        "dense"
                    ],
                    "common_phrases": [
                        "pithy fruit",
                        "pithy texture",
                        "pithy radish",
                        "pithy stem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий сердцевину растения; имеющий сухую, рыхлую или губчатую структуру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыхлый",
                                "target_lang_sense_clarification": "о структуре плодов, потерявших сочность (например, редис)"
                            },
                            {
                                "target_lang_word": "губчатый",
                                "target_lang_sense_clarification": "пористый, напоминающий губку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b16aec4-def1-3746-b6b6-1386e08fc58f",
                    "sense_definition": "Vigorous, powerful, or substantial.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "strength",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villagers brewed a <w>pithy</w> ale that was known for its strength.",
                            "target_lang_translations": {
                                "ru": "Деревенские жители варили <w>крепкий</w> эль, который славился своей силой."
                            }
                        },
                        {
                            "text": "He was a man of <w>pithy</w> character and strong resolve.",
                            "target_lang_translations": {
                                "ru": "Это был человек <w>сильного</w> характера и твёрдой решимости."
                            }
                        }
                    ],
                    "antonyms": [
                        "weak",
                        "feeble"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used to describe strength or substance"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "often found in Scottish English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Энергичный, сильный или существенный (часто используется в архаичном или диалектном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крепкий",
                                "target_lang_sense_clarification": "сильный, насыщенный (о напитках или словах)"
                            },
                            {
                                "target_lang_word": "сильный",
                                "target_lang_sense_clarification": "обладающий мощью или значительностью"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
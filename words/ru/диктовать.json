    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b29499f3-cbf7-35e8-a28b-c79f24d5dee5",
                    "sense_definition": "Произносить текст или слова вслух, чтобы кто-то другой мог их записать.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель начал медленно <w>диктовать</w> текст, чтобы все ученики успели записать.",
                            "target_lang_translations": {
                                "en": "The teacher began to slowly <w>dictate</w> the text so that all the students could write it down in time."
                            }
                        },
                        {
                            "text": "Пожалуйста, можешь <w>диктовать</w> номер телефона по цифрам?",
                            "target_lang_translations": {
                                "en": "Could you please <w>dictate</w> the phone number digit by digit?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "диктовать письмо",
                        "диктовать текст",
                        "диктовать по буквам",
                        "диктовать адрес"
                    ],
                    "target_lang_definitions": {
                        "en": "To speak text or words aloud so that someone else can write them down."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dictate",
                                "target_lang_sense_clarification": "speak aloud for writing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d8b4b7a-ebeb-326e-9680-58edec1d9d60",
                    "sense_definition": "Навязывать свою волю, условия или правила; заставлять поступать определённым образом.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Крупные компании часто пытаются <w>диктовать</w> свои условия на рынке.",
                            "target_lang_translations": {
                                "en": "Large companies often try to <w>dictate</w> their terms in the market."
                            }
                        },
                        {
                            "text": "Никто не имеет права <w>диктовать</w> нам, как мы должны жить и работать.",
                            "target_lang_translations": {
                                "en": "No one has the right to <w>dictate</w> to us how we should live and work."
                            }
                        }
                    ],
                    "synonyms": [
                        "навязывать",
                        "предписывать"
                    ],
                    "common_phrases": [
                        "диктовать условия",
                        "диктовать правила",
                        "диктовать моду",
                        "диктовать свою волю"
                    ],
                    "target_lang_definitions": {
                        "en": "To impose one's will, conditions, or rules; to force someone to act in a certain way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dictate",
                                "target_lang_sense_clarification": "impose will or rules"
                            },
                            {
                                "target_lang_word": "impose",
                                "target_lang_sense_clarification": "force upon others"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
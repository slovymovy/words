                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "surnames",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Sokół</w> mieszka w tym bloku od wielu lat.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Сокол</w> живет в этом доме много лет."
                            }
                        },
                        {
                            "text": "Czy widziałaś dzisiaj panią <w>Sokół</w> w sklepie spożywczym?",
                            "target_lang_translations": {
                                "ru": "Ты видела сегодня госпожу <w>Сокол</w> в продуктовом магазине?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Популярная польская фамилия."
                    },
                    "translations": {

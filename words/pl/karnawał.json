{
    "word_family": [
        "karnawałowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "84ac52bb-8d13-3ac0-b208-2a83065b427f",
                    "sense_definition": "Okres od święta Trzech Króli do Środy Popielcowej, przeznaczony na bale, maskarady i zabawy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "karnawał_def",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym roku <w>karnawał</w> trwa wyjątkowo krótko.",
                            "target_lang_translations": {
                                "ru": "В этом году <w>карнавал</w> длится исключительно недолго."
                            }
                        },
                        {
                            "text": "Wszyscy szykują się na wielki bal na koniec <w>karnawału</w>.",
                            "target_lang_translations": {
                                "ru": "Все готовятся к большому балу в конце <w>карнавала</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapusty",
                        "mięsopust"
                    ],
                    "antonyms": [
                        "post"
                    ],
                    "common_phrases": [
                        "koniec karnawału",
                        "w czasie karnawału",
                        "bal karnawałowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период от праздника Богоявления (Трёх Королей) до Пепельной среды, предназначенный для балов, маскарадов и увеселений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карнавал",
                                "target_lang_sense_clarification": "сезон праздников перед постом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4be706fe-3048-33fd-99cb-8d2356fea8d7",
                    "sense_definition": "Wielkie, barwne pochody i masowe zabawy uliczne, charakterystyczne zwłaszcza dla krajów Ameryki Łacińskiej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "karnawał_def",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słynny <w>karnawał</w> w Rio de Janeiro przyciąga miliony turystów.",
                            "target_lang_translations": {
                                "ru": "Знаменитый <w>карнавал</w> в Рио-де-Жанейро привлекает миллионы туристов."
                            }
                        },
                        {
                            "text": "W telewizji pokazywano relację z brazylijskiego <w>karnawału</w>.",
                            "target_lang_translations": {
                                "ru": "По телевизору показывали репортаж с бразильского <w>карнавала</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "festiwal",
                        "fiesta"
                    ],
                    "common_phrases": [
                        "karnawał w Rio",
                        "karnawał w Wenecji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грандиозные красочные шествия и массовые уличные гулянья, характерные особенно для стран Латинской Америки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карнавал",
                                "target_lang_sense_clarification": "праздничное шествие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b120513c-b135-38b2-8943-3dd6368c31d2",
                    "sense_definition": "Zabawy i uroczystości odbywające się w okresie zapustów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "karnawał_def",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młodzież uwielbia nocne szaleństwa <w>karnawału</w>.",
                            "target_lang_translations": {
                                "ru": "Молодежь обожает ночные безумства <w>карнавала</w>."
                            }
                        },
                        {
                            "text": "Ten huczny <w>karnawał</w> będziemy wspominać przez lata.",
                            "target_lang_translations": {
                                "ru": "Этот шумный <w>карнавал</w> мы будем вспоминать долгие годы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapusty"
                    ],
                    "common_phrases": [
                        "szaleństwa karnawału"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеселения и торжества, проходящие в период масленицы (запуст)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карнавал",
                                "target_lang_sense_clarification": "празднество, гулянье"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
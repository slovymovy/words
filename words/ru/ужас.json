    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab55c532-e880-371a-8508-c7b9c69d60c8",
                    "sense_definition": "Сильное чувство страха, испуга или паники, доводящее до оцепенения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Всех присутствующих в зале охватил <w>ужас</w>.",
                            "target_lang_translations": {
                                "en": "<w>Horror</w> seized everyone present in the hall."
                            }
                        },
                        {
                            "text": "Она в <w>ужасе</w> закрыла лицо руками, увидев аварию.",
                            "target_lang_translations": {
                                "en": "She covered her face in <w>horror</w> when she saw the accident."
                            }
                        }
                    ],
                    "synonyms": [
                        "страх",
                        "паника",
                        "испуг",
                        "жуть",
                        "кошмар",
                        "боязнь"
                    ],
                    "common_phrases": [
                        "прийти в ужас",
                        "в ужасе",
                        "охватил ужас",
                        "наводить ужас"
                    ],
                    "target_lang_definitions": {
                        "en": "A strong feeling of fear, shock, or panic."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "horror",
                                "target_lang_sense_clarification": "intense fear or shock"
                            },
                            {
                                "target_lang_word": "terror",
                                "target_lang_sense_clarification": "extreme fear or panic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4e3d7af-dcc0-38e1-96d4-c5c19ee293f8",
                    "sense_definition": "Очень плохая, неприятная или трагическая ситуация; что-то, вызывающее страх или отвращение.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На дорогах города творится полный <w>ужас</w> из-за снегопада.",
                            "target_lang_translations": {
                                "en": "The traffic in the city is a total <w>nightmare</w> because of the snowfall."
                            }
                        },
                        {
                            "text": "Это был настоящий <w>ужас</w>, а не отпуск.",
                            "target_lang_translations": {
                                "en": "It was a real <w>nightmare</w>, not a vacation."
                            }
                        }
                    ],
                    "synonyms": [
                        "кошмар",
                        "беда",
                        "жуть",
                        "жесть"
                    ],
                    "antonyms": [
                        "восторг"
                    ],
                    "common_phrases": [
                        "тихий ужас",
                        "какой ужас"
                    ],
                    "target_lang_definitions": {
                        "en": "A very bad, unpleasant, or chaotic situation; something causing dismay."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nightmare",
                                "target_lang_sense_clarification": "a terrible or chaotic situation"
                            },
                            {
                                "target_lang_word": "horror",
                                "target_lang_sense_clarification": "something terrible"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14a197e2-0d8e-3f72-b5b0-2afe6f2d82aa",
                    "sense_definition": "Жанр фильмов, книг или игр, цель которых — вызвать у аудитории чувство страха.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы вчера весь вечер смотрели <w>ужасы</w>.",
                            "target_lang_translations": {
                                "en": "We watched <w>horror movies</w> all evening yesterday."
                            }
                        },
                        {
                            "text": "Я не люблю читать <w>ужасы</w> на ночь, потом плохо сплю.",
                            "target_lang_translations": {
                                "en": "I don't like reading <w>horror stories</w> at night; I sleep badly afterwards."
                            }
                        }
                    ],
                    "synonyms": [
                        "ужастик"
                    ],
                    "common_phrases": [
                        "фильм ужасов",
                        "смотреть ужасы",
                        "любить ужасы"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for 'фильм ужасов'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A genre of movies, books, or games designed to scare the audience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "horror",
                                "target_lang_sense_clarification": "genre of fiction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da632f00-28a9-31bc-a690-c0a6ba7bb523",
                    "sense_definition": "Крайняя степень бедственности, трагичности или безвыходности положения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Трудно представить весь <w>ужас</w> их положения после пожара.",
                            "target_lang_translations": {
                                "en": "It is hard to imagine the full <w>horror</w> of their situation after the fire."
                            }
                        },
                        {
                            "text": "Он вдруг осознал весь <w>ужас</w> своего одиночества.",
                            "target_lang_translations": {
                                "en": "He suddenly realized the full <w>horror</w> of his loneliness."
                            }
                        }
                    ],
                    "synonyms": [
                        "беда",
                        "кошмар"
                    ],
                    "common_phrases": [
                        "весь ужас"
                    ],
                    "target_lang_definitions": {
                        "en": "The extreme degree of distress, tragedy, or hopelessness of a situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "horror",
                                "target_lang_sense_clarification": "tragic or distressing quality"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "6ae7c5e2-5d6c-3ea6-bf19-86601c9f1746",
                    "sense_definition": "Эмоциональный возглас, выражающий сильное возмущение, удивление или страх.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ужас</w>, как холодно сегодня на улице!",
                            "target_lang_translations": {
                                "en": "<w>It's terrible</w> how cold it is outside today!"
                            }
                        },
                        {
                            "text": "Ты видел цены в магазине? Просто <w>ужас</w>!",
                            "target_lang_translations": {
                                "en": "Did you see the prices in the store? It's just <w>awful</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "кошмар",
                        "жуть",
                        "жесть"
                    ],
                    "common_phrases": [

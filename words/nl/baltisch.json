                    "common_phrases": [
                        "de Baltische Zee",
                        "het Baltische gebied",
                        "het Baltische schild"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Балтийскому морю или прилегающим территориям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балтийский",
                                "target_lang_sense_clarification": "относящийся к морю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "959a4d04-6b70-3dd1-8c43-654e7a0abc1d",
                    "sense_definition": "Met betrekking tot de Balten, een historisch geslacht van Visigotische vorsten.",

            "senses": [
                {
                    "sense_id": "ba2df2ff-32ad-3061-b6e9-1082f10fa552",
                    "sense_definition": "Urzędnik stojący na czele sołectwa (jednostki pomocniczej gminy, zazwyczaj wsi), będący przedstawicielem lokalnej społeczności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "modern_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkańcy wsi wybrali nowego <w>sołtysa</w> na czteroletnią kadencję.",
                            "target_lang_translations": {
                                "ru": "Жители деревни выбрали нового <w>солтыса</w> на четырехлетний срок."
                            }
                        },
                        {
                            "text": "Muszę iść do <w>sołtysa</w>, żeby zapłacić podatek gruntowy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти к <w>солтысу</w>, чтобы заплатить земельный налог."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wybory sołtysa",
                        "funkcja sołtysa",
                        "zebranie z sołtysem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чиновник, возглавляющий солецтво (административную единицу, обычно деревню), являющийся представителем местного сообщества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солтыс",
                                "target_lang_sense_clarification": "глава деревни в Польше"
                            },
                            {
                                "target_lang_word": "староста",
                                "target_lang_sense_clarification": "функциональный аналог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f382f614-8062-385c-ba3b-1696de1bb12a",
                    "sense_definition": "W dawnej Polsce: zarządca wsi lokowanej na prawie niemieckim, reprezentujący pana feudalnego i posiadający przywileje sądowe oraz ekonomiczne.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "historical_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W średniowieczu <w>sołtys</w> miał obowiązek stawać do służby wojskowej konno.",
                            "target_lang_translations": {
                                "ru": "В Средневековье <w>солтыс</w> был обязан являться на военную службу верхом."
                            }
                        },
                        {
                            "text": "Bogaty <w>sołtys</w> często posiadał we wsi własną karczmę i młyn.",
                            "target_lang_translations": {
                                "ru": "Богатый <w>солтыс</w> часто владел в деревне собственной корчмой и мельницей."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "znaczenie historyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В старой Польше: управляющий деревней, основанной на немецком праве, представляющий феодала и обладающий судебными и экономическими привилегиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солтыс",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                }
            ]
        },

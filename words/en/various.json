    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "644d8373-0155-3357-9b13-bae91e4c4b1e",
                    "sense_definition": "Having a broad range of different elements or qualities.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_diversity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are <w>various</w> reasons for his decision to leave.",
                            "target_lang_translations": {
                                "ru": "Существуют <w>различные</w> причины его решения уйти."
                            }
                        },
                        {
                            "text": "The museum displays <w>various</w> types of ancient pottery.",
                            "target_lang_translations": {
                                "ru": "В музее представлены <w>разнообразные</w> виды древней керамики."
                            }
                        }
                    ],
                    "synonyms": [
                        "diverse",
                        "miscellaneous",
                        "manifold",
                        "multifarious",
                        "sundry",
                        "motley"
                    ],
                    "antonyms": [
                        "monotonous"
                    ],
                    "common_phrases": [
                        "various types",
                        "various kinds",
                        "various forms",
                        "various reasons"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий широкий спектр различных элементов или качеств; разнообразный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разнообразный",
                                "target_lang_sense_clarification": "содержащий много разных элементов"
                            },
                            {
                                "target_lang_word": "различный",
                                "target_lang_sense_clarification": "отличающийся по качеству или виду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66d0f60b-4268-3a9b-83bc-dcdff9bb9f43",
                    "sense_definition": "That varies or differs from others; variant.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scholar noted a <w>various</w> reading of the text in the footnote.",
                            "target_lang_translations": {
                                "ru": "Ученый отметил <w>вариантное</w> прочтение текста в сноске."
                            }
                        },
                        {
                            "text": "This edition includes <w>various</w> spellings found in older manuscripts.",
                            "target_lang_translations": {
                                "ru": "Это издание включает <w>отличающиеся</w> написания, найденные в старых рукописях."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly used in literary or textual criticism contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменяющийся или отличающийся от других; вариантный (обычно в контексте текстов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вариантный",
                                "target_lang_sense_clarification": "являющийся вариантом чего-либо"
                            },
                            {
                                "target_lang_word": "отличающийся",
                                "target_lang_sense_clarification": "не совпадающий с основным"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "7cbb5971-e359-377a-88ff-2692f9d26f70",
                    "sense_definition": "More than one of an indeterminate set of things; several different.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_diversity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are <w>various</w> ways to solve this math problem.",
                            "target_lang_translations": {
                                "ru": "Существуют <w>разные</w> способы решения этой математической задачи."
                            }
                        },
                        {
                            "text": "<w>Various</w> people have complained about the noise in the building.",
                            "target_lang_translations": {
                                "ru": "<w>Разные</w> люди жаловались на шум в здании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "various ways",
                        "various people",
                        "various things",
                        "at various times"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несколько разных предметов или людей из неопределенного множества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разный",
                                "target_lang_sense_clarification": "неодинаковый, всякий"
                            },
                            {
                                "target_lang_word": "различный",
                                "target_lang_sense_clarification": "всякого рода, всевозможный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "7a036a1c-bba3-3f6e-8351-c22e4634ec95",
                    "sense_definition": "De achtergrond, wanden en voorwerpen die op een toneel of filmset worden geplaatst om de omgeving van het verhaal uit te beelden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "scenery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De acteurs liepen voorzichtig tussen de onderdelen van het <w>decor</w> door.",
                            "target_lang_translations": {
                                "en": "The actors walked carefully between the parts of the <w>scenery</w>.",
                                "ru": "Актёры осторожно проходили между частями <w>декорации</w>."
                            }
                        },
                        {
                            "text": "Voor de nieuwe voorstelling hebben ze een prachtig <w>decor</w> gebouwd.",
                            "target_lang_translations": {
                                "en": "They have built a beautiful <w>set</w> for the new show.",
                                "ru": "Для нового спектакля построили прекрасную <w>декорацию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "toneelbeeld",
                        "toneeldecor"
                    ],
                    "common_phrases": [
                        "het decor bouwen",
                        "een prachtig decor",
                        "achter het decor"
                    ],
                    "target_lang_definitions": {
                        "en": "The scenery, walls, and objects placed on a stage or film set to portray the environment of the story.",
                        "ru": "Оформление сцены или съёмочной площадки (задний план, предметы), создающее иллюзию места действия."
                    },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "59236d8f-9a98-32c5-a055-03a023b22026",
                    "sense_definition": "Aandacht die door de media aan een zaak of persoon wordt geschonken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "media_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het schandaal zorgde voor veel negatieve <w>publiciteit</w> in de landelijke kranten.",
                            "target_lang_translations": {
                                "ru": "Скандал вызвал широкую негативную <w>огласку</w> в национальных газетах."
                            }
                        },
                        {
                            "text": "De zanger mijdt de <w>publiciteit</w> liever om zijn privéleven te beschermen.",
                            "target_lang_translations": {
                                "ru": "Певец предпочитает избегать <w>публичности</w>, чтобы защитить свою личную жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "media-aandacht",
                        "persaandacht"
                    ],
                    "antonyms": [
                        "antipubliciteit"
                    ],
                    "common_phrases": [
                        "negatieve publiciteit",
                        "publiciteit zoeken",
                        "in de publiciteit komen",
                        "slechte publiciteit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внимание, уделяемое СМИ какому-либо делу или человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "огласка",
                                "target_lang_sense_clarification": "широкое обсуждение, часто в контексте скандалов"
                            },
                            {
                                "target_lang_word": "публичность",
                                "target_lang_sense_clarification": "пребывание на виду у общества"
                            },
                            {
                                "target_lang_word": "паблисити",
                                "target_lang_sense_clarification": "популярность, создаваемая СМИ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7faf51e0-8c14-3f29-8201-b19ed8f3c664",
                    "sense_definition": "Het maken van reclame om iets te verkopen of aan te prijzen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "advertising",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf maakt veel <w>publiciteit</w> voor de nieuwe zomercollectie.",
                            "target_lang_translations": {
                                "ru": "Компания создает широкую <w>рекламу</w> для новой летней коллекции."
                            }
                        },
                        {
                            "text": "Zonder goede <w>publiciteit</w> is het lastig om nieuwe klanten te werven.",
                            "target_lang_translations": {
                                "ru": "Без хорошей <w>рекламы</w> трудно привлечь новых клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "reclame",
                        "promotie"
                    ],
                    "common_phrases": [
                        "publiciteit maken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создание рекламы для продажи или продвижения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реклама",
                                "target_lang_sense_clarification": "коммерческое продвижение"
                            },
                            {
                                "target_lang_word": "продвижение",
                                "target_lang_sense_clarification": "маркетинговая активность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8870ff67-7e5a-3c64-9356-237554efdf04",
                    "sense_definition": "De toestand van openbaar en algemeen bekend zijn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "openness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na jaren van stilte trad de schrijver weer in de <w>publiciteit</w> met een nieuw boek.",
                            "target_lang_translations": {
                                "ru": "После многих лет молчания писатель снова появился на <w>публике</w> с новой книгой."
                            }
                        },
                        {
                            "text": "De plannen werden in de volle <w>publiciteit</w> besproken door de gemeenteraad.",
                            "target_lang_translations": {
                                "ru": "Планы обсуждались городским советом при полной <w>гласности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "openbaarheid",
                        "bekendheid"
                    ],
                    "common_phrases": [
                        "in de publiciteit treden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние открытости и общеизвестности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гласность",
                                "target_lang_sense_clarification": "открытость для общества"
                            },
                            {
                                "target_lang_word": "публичность",
                                "target_lang_sense_clarification": "общественный характер"
                            }
                        ]
                    }
                }
            ]
        }
    ]

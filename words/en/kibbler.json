    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c7322dab-d1a3-3dbf-8f6b-a54e0860a428",
                    "sense_definition": "A machine used to coarsely grind or crush grain, beans, or similar materials.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "kibbler_machine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer used a <w>kibbler</w> to break down the corn for animal feed.",
                            "target_lang_translations": {
                                "ru": "Фермер использовал <w>зернодробилку</w>, чтобы измельчить кукурузу на корм скоту."
                            }
                        },
                        {
                            "text": "Maintenance on the <w>kibbler</w> is essential to ensure the grain is ground evenly.",
                            "target_lang_translations": {
                                "ru": "Техническое обслуживание <w>дробилки</w> необходимо для обеспечения равномерного помола зерна."
                            }
                        }
                    ],
                    "synonyms": [
                        "grinder",
                        "crusher"
                    ],
                    "common_phrases": [
                        "grain kibbler",
                        "corn kibbler"
                    ],
                    "target_lang_definitions": {
                        "ru": "Машина, используемая для грубого помола или дробления зерна, бобов или подобных материалов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зернодробилка",
                                "target_lang_sense_clarification": "машина для измельчения зерна"
                            },
                            {
                                "target_lang_word": "дробилка",
                                "target_lang_sense_clarification": "общее название устройства для дробления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28329634-c9cc-31a4-9233-1f90b102745c",
                    "sense_definition": "A person whose job is to operate a machine that grinds grain.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "kibbler_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>kibbler</w> adjusted the settings on the mill to produce a finer meal.",
                            "target_lang_translations": {
                                "ru": "<w>Дробильщик</w> отрегулировал настройки мельницы для получения более мелкого помола."
                            }
                        },
                        {
                            "text": "In the past, a <w>kibbler</w> was a common worker in agricultural processing plants.",
                            "target_lang_translations": {
                                "ru": "В прошлом <w>дробильщик</w> был обычным рабочим на сельскохозяйственных перерабатывающих предприятиях."
                            }
                        }
                    ],
                    "synonyms": [
                        "operator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, чья работа заключается в управлении машиной для дробления зерна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дробильщик",
                                "target_lang_sense_clarification": "рабочий, обслуживающий дробильную установку"
                            }
                        ]
                    }
                }
            ]
        }
    ]

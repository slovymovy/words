            "senses": [
                {
                    "sense_id": "b7ba9ca9-7e5c-3b6f-8156-03837be01acf",
                    "sense_definition": "taki, który zginął, został zgubiony i nie można go znaleźć",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "lost_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <zagubiony> bagaż dotarł do mnie dopiero po dwóch dniach oczekiwania.",
                            "target_lang_translations": {
                                "ru": "Мой <потерянный> багаж прибыл ко мне только спустя два дня ожидания."
                            }
                        },
                        {
                            "text": "Przez całe popołudnie szukaliśmy w parku <zagubionego> telefonu.",
                            "target_lang_translations": {
                                "ru": "Весь день мы искали в парке <потерянный> телефон."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgubiony",
                        "stracony"
                    ],
                    "antonyms": [
                        "znaleziony",
                        "odnaleziony"
                    ],
                    "common_phrases": [
                        "zagubiony bagaż",
                        "zagubiony przedmiot",
                        "biuro rzeczy zagubionych"
                    ],
                    "target_lang_definitions": {
                        "ru": "такой, который пропал, был утерян и его невозможно найти"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потерянный",
                                "target_lang_sense_clarification": "о вещах"
                            },
                            {
                                "target_lang_word": "утерянный",
                                "target_lang_sense_clarification": "официально или о документах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b82fdfc-fbff-3529-9aca-247e501ada2e",
                    "sense_definition": "taki, który stracił orientację w terenie i nie wie, gdzie się znajduje ani dokąd iść",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "lost_orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Zagubiony> turysta poprosił policjanta o wskazanie drogi do centrum.",
                            "target_lang_translations": {
                                "ru": "<Заблудившийся> турист попросил полицейского показать дорогу в центр."
                            }
                        },
                        {
                            "text": "Poczułem się kompletnie <zagubiony> w labiryncie wąskich uliczek starego miasta.",
                            "target_lang_translations": {
                                "ru": "Я чувствовал себя совершенно <потерянным> в лабиринте узких улочек старого города."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbłąkany"
                    ],
                    "common_phrases": [
                        "zagubiony w lesie",
                        "zagubiony turysta",
                        "zagubiony w tłumie"
                    ],
                    "target_lang_definitions": {
                        "ru": "такой, который сбился с пути и не знает, где находится или куда идти"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заблудившийся",
                                "target_lang_sense_clarification": "сбившийся с пути"
                            },
                            {
                                "target_lang_word": "потерявшийся",
                                "target_lang_sense_clarification": "о человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55c3e623-3212-3bba-a93c-05e8bccf2042",
                    "sense_definition": "taki, który czuje się niepewnie, jest zdezorientowany i nie wie, jak postępować w danej sytuacji życiowej",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "confused_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po zmianie pracy Marek czuł się nieco <zagubiony> w nowym środowisku.",
                            "target_lang_translations": {
                                "ru": "После смены работы Марек чувствовал себя немного <потерянным> в новой обстановке."
                            }
                        },
                        {
                            "text": "Jej <zagubiony> wzrok świadczył o tym, że zupełnie nie zrozumiała zadanego pytania.",
                            "target_lang_translations": {
                                "ru": "Её <растерянный> взгляд свидетельствовал о том, что она совершенно не поняла заданного вопроса."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdezorientowany",
                        "bezradny",
                        "zmieszany"
                    ],
                    "antonyms": [
                        "pewny siebie",
                        "zdecydowany"
                    ],
                    "common_phrases": [

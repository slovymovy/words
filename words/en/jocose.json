            "senses": [
                {
                    "sense_id": "f1d7c662-cb92-3062-a8d2-0ee6fa882091",
                    "sense_definition": "Describing a person who is fond of joking and has a cheerful, lively nature.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>jocose</w> host kept the dinner party entertained with endless funny stories.",
                            "target_lang_translations": {
                                "ru": "Этот <w>веселый</w> хозяин развлекал гостей за ужином бесконечными смешными историями."
                            }
                        },
                        {
                            "text": "He was in a <w>jocose</w> mood after hearing the good news about his promotion.",
                            "target_lang_translations": {
                                "ru": "Услышав хорошие новости о своем повышении, он пребывал в <w>игривом</w> настроении."
                            }
                        }
                    ],
                    "synonyms": [
                        "jovial",
                        "jocular",
                        "merry",
                        "cheerful"
                    ],
                    "antonyms": [
                        "morose",
                        "gloomy",
                        "serious"
                    ],
                    "common_phrases": [
                        "jocose mood",
                        "jocose nature",
                        "jocose spirit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывает человека, который любит шутить и обладает веселым, живым характером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игривый",
                                "target_lang_sense_clarification": "о характере или настроении"
                            },
                            {
                                "target_lang_word": "веселый",
                                "target_lang_sense_clarification": "о человеке, склонном к веселью"
                            },
                            {
                                "target_lang_word": "шутливый",
                                "target_lang_sense_clarification": "о настроении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e697916-25b9-3f10-953a-5114df3dba35",
                    "sense_definition": "Characterized by being humorous, playful, or done in a joking manner.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She made a <w>jocose</w> remark about the weather to help break the ice.",
                            "target_lang_translations": {
                                "ru": "Она сделала <w>шутливое</w> замечание о погоде, чтобы разрядить обстановку."
                            }
                        },
                        {
                            "text": "His <w>jocose</w> style of writing makes even dry subjects interesting to read.",
                            "target_lang_translations": {
                                "ru": "Его <w>шутливый</w> стиль письма делает интересным чтение даже о скучных предметах."
                            }
                        }
                    ],
                    "synonyms": [
                        "humorous",
                        "playful",
                        "facetious",
                        "witty"
                    ],
                    "antonyms": [
                        "solemn",
                        "earnest",
                        "serious"
                    ],
                    "common_phrases": [
                        "jocose remark",
                        "jocose style",
                        "jocose manner"
                    ],
                    "target_lang_definitions": {

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c438b519-21be-36be-9a7e-102d8c26c170",
                    "sense_definition": "Zatwierdzony przez urząd lub władzę, mający moc prawną i urzędową.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "urzedowy_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd opublikował <w>oficjalny</w> komunikat dotyczący zmian w podatkach.",
                            "target_lang_translations": {
                                "ru": "Правительство опубликовало <w>официальное</w> сообщение, касающееся изменений в налогах."
                            }
                        },
                        {
                            "text": "To jest <w>oficjalny</w> dokument, którego nie można kopiować bez zgody urzędu.",
                            "target_lang_translations": {
                                "ru": "Это <w>официальный</w> документ, который нельзя копировать без разрешения учреждения."
                            }
                        }
                    ],
                    "synonyms": [
                        "urzędowy",
                        "formalny",
                        "prawny"
                    ],
                    "antonyms": [
                        "nieoficjalny"
                    ],
                    "common_phrases": [
                        "oficjalny komunikat",
                        "język oficjalny",
                        "oficjalna wersja",
                        "oficjalny dokument"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утвержденный органом власти, имеющий юридическую и служебную силу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "официальный",
                                "target_lang_sense_clarification": "исходящий от власти, документальный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35f5b93c-419c-3eb2-bf76-92fbcc688a0b",
                    "sense_definition": "Występujący w charakterze reprezentanta instytucji lub państwa, a nie prywatnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "urzedowy_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prezydent udał się z <w>oficjalną</w> wizytą do sąsiedniego kraju.",
                            "target_lang_translations": {
                                "ru": "Президент отправился с <w>официальным</w> визитом в соседнюю страну."
                            }
                        },
                        {
                            "text": "Jutro czeka mnie ważne <w>oficjalne</w> spotkanie z dyrektorem generalnym.",
                            "target_lang_translations": {
                                "ru": "Завтра меня ждет важная <w>официальная</w> встреча с генеральным директором."
                            }
                        }
                    ],
                    "synonyms": [
                        "służbowy",
                        "reprezentacyjny"
                    ],
                    "antonyms": [
                        "nieoficjalny"
                    ],
                    "common_phrases": [
                        "oficjalna wizyta",
                        "oficjalne spotkanie",
                        "podróż oficjalna",
                        "wystąpienie oficjalne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступающий в качестве представителя учреждения или государства, а не частное лицо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "официальный",
                                "target_lang_sense_clarification": "служебный, представительский"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91f026d4-4330-3048-a9a8-6d3380f54ca1",
                    "sense_definition": "Cechujący się powagą, sztywnością i ścisłym przestrzeganiem ustalonych reguł zachowania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>oficjalny</w> ton sprawił, że wszyscy poczuli się nieco skrępowani.",
                            "target_lang_translations": {
                                "ru": "Его <w>официальный</w> тон заставил всех почувствовать себя немного скованно."
                            }
                        },
                        {
                            "text": "Na uroczystą galę obowiązuje bardzo <w>oficjalny</w> strój wieczorowy.",
                            "target_lang_translations": {
                                "ru": "На торжественный гала-вечер требуется очень <w>официальный</w> вечерний наряд."
                            }

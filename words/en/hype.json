{
    "word_family": [
        "overhype",
        "hypeman"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "97d416a7-ec25-33e2-8703-eb80e71a9e5d",
                    "sense_definition": "Promotion or propaganda, especially exaggerated claims.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "promotion_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie did not live up to the <w>hype</w> generated by the media.",
                            "target_lang_translations": {
                                "ru": "Фильм не оправдал <w>шумихи</w>, поднятой в СМИ."
                            }
                        },
                        {
                            "text": "Don't believe the <w>hype</w> about this new diet plan.",
                            "target_lang_translations": {
                                "ru": "Не верьте <w>хайпу</w> вокруг этой новой диеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "publicity",
                        "promotion",
                        "buildup"
                    ],
                    "common_phrases": [
                        "media hype",
                        "live up to the hype",
                        "believe the hype"
                    ],
                    "target_lang_definitions": {
                        "ru": "Агрессивная реклама, шумиха или ажиотаж, часто с преувеличением достоинств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шумиха",
                                "target_lang_sense_clarification": "громкое обсуждение, часто раздутое искусственно"
                            },
                            {
                                "target_lang_word": "хайп",
                                "target_lang_sense_clarification": "модный сленг: ажиотаж вокруг чего-либо"
                            },
                            {
                                "target_lang_word": "ажиотаж",
                                "target_lang_sense_clarification": "сильное возбуждение, волнение вокруг события"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6e67954-5838-39bf-b6ca-25872ccbb24c",
                    "sense_definition": "Clipping of <w>hypodermic</w> needle.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "drug_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He found a used <w>hype</w> on the ground in the alley.",

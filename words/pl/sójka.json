            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ea675f4-ba10-3d82-85e8-cc98c9916ae6",
                    "sense_definition": "Średniej wielkości ptak z rodziny krukowatych o rdzawobrązowym upierzeniu i charakterystycznych, niebieskich piórach na skrzydłach.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lesie głośno skrzeczała <w>sójka</w>, ostrzegając inne zwierzęta przed niebezpieczeństwem.",
                            "target_lang_translations": {
                                "ru": "В лесу громко кричала <w>сойка</w>, предупреждая других животных об опасности."
                            }
                        },
                        {
                            "text": "Mój brat ciągle wybiera się do nas jak <w>sójka</w> za morze i nie może dojechać.",
                            "target_lang_translations": {
                                "ru": "Мой брат всё собирается к нам, как <w>сойка</w> за море, и никак не может доехать."
                            }
                        }
                    ],
                    "synonyms": [
                        "sójka zwyczajna",
                        "sójka żołędziówka"
                    ],
                    "common_phrases": [
                        "wybierać się jak sójka za morze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Птица семейства врановых с рыжевато-коричневым оперением и характерными голубыми перьями на крыльях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сойка",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b12e7e23-fa64-36fb-af76-ba00dd0aad6f",
                    "sense_definition": "Uderzenie kogoś w bok, zazwyczaj łokciem lub pięścią; szturchaniec.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kolega dał mi <w>sójkę</w> w bok, żebym przestał się głośno śmiać.",
                            "target_lang_translations": {
                                "ru": "Приятель дал мне <w>тычка</w> в бок, чтобы я перестал громко смеяться."
                            }
                        },
                        {
                            "text": "Za ten głupi żart należała mu się porządna <w>sójka</w>.",
                            "target_lang_translations": {
                                "ru": "За эту глупую шутку ему полагался хороший <w>тычок</w>."
                            }

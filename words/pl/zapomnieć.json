    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "75a1d7be-7625-3ecd-9f51-f119ed9a72c7",
                    "sense_definition": "Przestać pamiętać o czymś lub o kimś; utracić wspomnienie na dany temat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "memory_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety <w>zapomniałem</w> twojego imienia, czy możesz mi przypomnieć?",
                            "target_lang_translations": {
                                "ru": "К сожалению, я <w>забыл</w> твое имя, можешь мне напомнить?"
                            }
                        },
                        {
                            "text": "Musisz <w>zapomnieć</w> o przeszłości i skupić się na tym, co będzie jutro.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>забыть</w> о прошлом и сосредоточиться на том, что будет завтра."
                            }
                        }
                    ],
                    "antonyms": [
                        "zapamiętać",
                        "przypomnieć (sobie)"
                    ],
                    "common_phrases": [
                        "zapomnieć o wszystkim",
                        "zapomnieć o bożym świecie",
                        "zapomnieć na śmierć",
                        "szybko zapomnieć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать помнить о чем-то или о ком-то; утратить воспоминание на данную тему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забыть",
                                "target_lang_sense_clarification": "перестать помнить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8769835f-0bce-37c6-b9e1-ab13f6284cd0",
                    "sense_definition": "Nie wykonać planowanej czynności z powodu braku pamięci.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "negligence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wychodząc z domu, <w>zapomniałam</w> zamknąć okno w sypialni.",
                            "target_lang_translations": {
                                "ru": "Выходя из дома, я <w>забыла</w> закрыть окно в спальне."
                            }
                        },
                        {
                            "text": "Proszę, nie <w>zapomnij</w> kupić mleka i chleba, gdy będziesz wracać z pracy.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>забудь</w> купить молока и хлеба, когда будешь возвращаться с работы."
                            }
                        }
                    ],
                    "antonyms": [
                        "przypomnieć (sobie)"
                    ],
                    "common_phrases": [
                        "zapomnieć zrobić",
                        "zapomnieć zadzwonić",
                        "zapomnieć o spotkaniu",
                        "zapomnieć o obowiązkach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не выполнить запланированное действие из-за того, что оно вылетело из головы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забыть",
                                "target_lang_sense_clarification": "не сделать что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef2989e3-5ed2-3fd7-897d-7565f2a77c3a",
                    "sense_definition": "Pozostawić coś w jakimś miejscu nieumyślnie, przez roztargnienie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "negligence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W pośpiechu <w>zapomniałem</w> kluczyków do samochodu i musiałem się wracać.",
                            "target_lang_translations": {
                                "ru": "В спешке я <w>забыл</w> ключи от машины, и мне пришлось возвращаться."
                            }
                        },
                        {
                            "text": "Czy kiedykolwiek <w>zapomniałeś</w> portfela, idąc na ważne zakupy?",
                            "target_lang_translations": {
                                "ru": "Ты когда-нибудь <w>забывал</w> кошелек, идя за важными покупками?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "zapomnieć kluczy",
                        "zapomnieć telefonu",
                        "zapomnieć portfela",
                        "zapomnieć dokumentów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставить что-то в каком-то месте непреднамеренно, по рассеянности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забыть",
                                "target_lang_sense_clarification": "оставить по рассеянности"
                            },
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "забыть взять с собой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec41135b-7fd1-35a9-94b9-825d61b1a19d",
                    "sense_definition": "Przestać myśleć o kimś lub o czymś, często w kontekście emocjonalnym, przebaczenia lub zakończenia relacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "memory_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu latach w końcu <w>zapomniała</w> o wyrządzonych jej krzywdach.",
                            "target_lang_translations": {
                                "ru": "Спустя много лет она наконец <w>забыла</w> о причиненных ей обидах."
                            }
                        },
                        {
                            "text": "Nie chcę o nim <w>zapomnieć</w>, mimo że nasze drogi się rozeszły.",
                            "target_lang_translations": {
                                "ru": "Я не хочу о нем <w>забывать</w>, несмотря на то, что наши пути разошлись."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zapomnieć o urazach",
                        "zapomnieć o kłótni",
                        "zapomnieć o sprawie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать думать о ком-то или о чем-то, часто в контексте эмоций, прощения или окончания отношений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забыть",
                                "target_lang_sense_clarification": "выкинуть из головы, простить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f7ac994-7adf-3680-b0d2-74d1b66a4bc9",
                    "sense_definition": "Stracić panowanie nad sobą i zachować się niestosownie do sytuacji lub swojej pozycji.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas ostrej wymiany zdań dyrektor <w>zapomniał się</w> i podniósł głos na pracownika.",
                            "target_lang_translations": {
                                "ru": "Во время резкого обмена мнениями директор <w>забылся</w> и повысил голос на сотрудника."
                            }
                        },
                        {
                            "text": "Przepraszam, <w>zapomniałem się</w>, to nie powinno było się wydarzyć.",
                            "target_lang_translations": {
                                "ru": "Извините, я <w>забылся</w>, этого не должно было случиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "zagalopować się",
                        "przeholować",
                        "posunąć się za daleko"
                    ],
                    "common_phrases": [
                        "zapomnieć się w gniewie",
                        "zapomnieć się w dyskusji"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерять самообладание и повести себя неподобающим образом для данной ситуации или своего положения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забыться",
                                "target_lang_sense_clarification": "потерять контроль над собой"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
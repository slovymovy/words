            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e3e6f940-510f-3c27-8401-bc6591d6959f",
                    "sense_definition": "Деепричастие настоящего времени от глагола <w>говорить</w>: в процессе речи, высказывая мысли вслух.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Честно <w>говоря</w>, я не ожидал такого результата.",
                            "target_lang_translations": {
                                "en": "Honestly <w>speaking</w>, I didn't expect such a result."
                            }
                        },
                        {
                            "text": "Иначе <w>говоря</w>, нам нужно начать всё сначала.",
                            "target_lang_translations": {
                                "en": "In other words (otherwise <w>speaking</w>), we need to start all over again."
                            }
                        },
                        {
                            "text": "Он вышел из комнаты, не <w>говоря</w> ни слова.",
                            "target_lang_translations": {
                                "en": "He left the room without <w>saying</w> a word."
                            }
                        }
                    ],
                    "antonyms": [
                        "молчание"
                    ],
                    "common_phrases": [
                        "честно говоря",
                        "иначе говоря",
                        "короче говоря",
                        "собственно говоря",
                        "откровенно говоря"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "деепричастие"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Present gerund of the verb 'to speak': in the process of speech, expressing thoughts aloud."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speaking",
                                "target_lang_sense_clarification": "general action or in fixed phrases like 'honestly speaking'"
                            },
                            {
                                "target_lang_word": "saying",
                                "target_lang_sense_clarification": "uttering words"
                            }
                        ]
                    }
                }
            ]
        },

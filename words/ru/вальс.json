    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ddd7e7c-eb5e-322a-b3e9-5f1588633eb8",
                    "sense_definition": "Парный бальный танец, для которого характерно плавное кружение пар и музыкальный размер три четверти.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "dance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На выпускном вечере мы танцевали наш последний школьный <w>вальс</w>.",
                            "target_lang_translations": {
                                "en": "At the prom, we danced our last school <w>waltz</w>."
                            }
                        },
                        {
                            "text": "Молодые пары красиво кружились в <w>вальсе</w> по всему залу.",
                            "target_lang_translations": {
                                "en": "Young couples spun beautifully in a <w>waltz</w> all around the hall."
                            }
                        }
                    ],
                    "common_phrases": [
                        "танцевать вальс",
                        "венский вальс",
                        "кружиться в вальсе",
                        "пригласить на вальс"
                    ],
                    "target_lang_definitions": {
                        "en": "A ballroom dance for couples, characterized by smooth spinning and a three-quarter time signature."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "waltz",
                                "target_lang_sense_clarification": "dance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d85f051-968e-3002-ae82-7ab546ce3607",
                    "sense_definition": "Музыкальное произведение в ритме три четверти, под которое исполняется этот танец.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Оркестр заиграл старинный <w>вальс</w>, и гости оживились.",
                            "target_lang_translations": {
                                "en": "The orchestra began playing an old <w>waltz</w>, and the guests livened up."
                            }
                        },
                        {
                            "text": "Композитор написал этот <w>вальс</w> специально для фортепиано.",
                            "target_lang_translations": {
                                "en": "The composer wrote this <w>waltz</w> specifically for the piano."
                            }
                        }
                    ],
                    "common_phrases": [
                        "играть вальс",
                        "звуки вальса",
                        "ритм вальса",
                        "слушать вальс"
                    ],
                    "target_lang_definitions": {
                        "en": "A musical composition in three-quarter time, to which this dance is performed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "waltz",
                                "target_lang_sense_clarification": "music"
                            }
                        ]
                    }
                }
            ]
        }
    ]

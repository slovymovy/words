                    "sense_id": "5b576e87-98d1-3434-8b95-9f04451c0a23",
                    "sense_definition": "Zmienić kolor na zielony lub pokryć się zielenią (o roślinach, przedmiotach lub twarzy człowieka).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wiosnę łąka za domem pięknie <zzieleniała>.",
                            "target_lang_translations": {
                                "ru": "Весной луг за домом красиво <зазеленел>."
                            }
                        },
                        {
                            "text": "Pasażer statku <zzieleniał> na twarzy z powodu choroby morskiej.",
                            "target_lang_translations": {
                                "ru": "Пассажир корабля <позеленел> лицом из-за морской болезни."
                            }
                        }
                    ],
                    "synonyms": [
                        "zazielenić się"
                    ],
                    "antonyms": [
                        "zżółknąć"
                    ],
                    "common_phrases": [
                        "zzielenieć na twarzy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать зеленым или покрыться зеленью (о растениях, предметах или лице человека)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позеленеть",
                                "target_lang_sense_clarification": "изменить цвет на зеленый"
                            },
                            {
                                "target_lang_word": "зазеленеть",
                                "target_lang_sense_clarification": "покрыться зеленью, о растениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab560bab-a132-3d64-a5d0-639036335812",
                    "sense_definition": "Okazać bardzo silne emocje, najczęściej zazdrość lub złość.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad <zzieleniał> z zazdrości, gdy zobaczył mój nowy samochód.",
                            "target_lang_translations": {
                                "ru": "Мой сосед <позеленел> от зависти, когда увидел мою новую машину."
                            }
                        },
                        {
                            "text": "Dyrektor <zzieleniał> ze złości na wieść o opóźnieniu projektu.",
                            "target_lang_translations": {
                                "ru": "Директор <позеленел> от злости, узнав о задержке проекта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zzielenieć z zazdrości",
                        "zzielenieć ze złości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявить очень сильные эмоции, чаще всего зависть или злость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позеленеть",
                                "target_lang_sense_clarification": "от эмоций"
                            }
                        ]
                    }
                }
            ]

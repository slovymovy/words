                },
                {
                    "sense_id": "f236a4ec-aef4-3865-bb8a-9f12a5294ee4",
                    "sense_definition": "Proces kierowania czymś, sprawowania władzy lub kontroli nad majątkiem albo instytucją.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_act",
                    "name_type": "no",
                    "examples": [
                        {

                            "target_lang_translations": {
                                "ru": "Небольшой городок <w>Иден</w> находится в штате Техас."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Географическое название, которое носят различные города и населенные пункты в мире"

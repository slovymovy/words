            "senses": [
                {
                    "sense_id": "6f1971a3-e4f2-3d5e-9c77-13a4ff249aa8",
                    "sense_definition": "Een gebouw, voertuig of andere constructie volledig afbreken of vernietigen",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat oude kantoorgebouw wordt overmorgen eindelijk <w>gesloopt</w>.",
                            "target_lang_translations": {
                                "en": "That old office building will finally be <w>demolished</w> the day after tomorrow."
                            }
                        },
                        {
                            "text": "De gemeente heeft besloten de hele wijk te <w>slopen</w> voor nieuwbouw.",
                            "target_lang_translations": {
                                "en": "The municipality has decided to <w>tear down</w> the entire neighborhood for new construction."
                            }
                        }
                    ],
                    "synonyms": [
                        "afbreken",
                        "neerhalen",
                        "ontmantelen",
                        "vernietigen"
                    ],
                    "antonyms": [
                        "bouwen",
                        "opbouwen",
                        "construeren"
                    ],
                    "common_phrases": [
                        "een gebouw slopen",
                        "een huis slopen",
                        "de boel slopen"
                    ],
                    "target_lang_definitions": {
                        "en": "To completely tear down or destroy a building, vehicle, or other construction"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demolish",
                                "target_lang_sense_clarification": "for buildings and large structures"
                            },
                            {
                                "target_lang_word": "tear down",
                                "target_lang_sense_clarification": "common for buildings"
                            },
                            {
                                "target_lang_word": "scrap",
                                "target_lang_sense_clarification": "for vehicles or machinery"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6593aee-85ce-3b0c-a2b5-5f9dcba839c1",
                    "sense_definition": "Iemand lichamelijk of geestelijk volledig uitputten",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "exhaustion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was wekenlang compleet <w>gesloopt</w> na de zware marathon.",
                            "target_lang_translations": {
                                "en": "He was completely <w>exhausted</w> for weeks after the tough marathon."
                            }
                        },
                        {
                            "text": "De stress en de lange werkdagen <w>slopen</w> haar gezondheid.",
                            "target_lang_translations": {
                                "en": "The stress and long working days are <w>ruining</w> her health."
                            }
                        }

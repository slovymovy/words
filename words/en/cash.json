                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please take your items to the <w>cash</w> to pay for them.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, пройдите с товарами на <w>кассу</w> для оплаты."
                            }

{
    "word_family": [
        "demonstration",
        "demonstrator",
        "demonstrative",
        "demonstrable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4e2f4202-a0d1-34e6-900d-27cbfe11e160",
                    "sense_definition": "To show clearly how to do something or how something works.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_show",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The instructor will <w>demonstrate</w> the correct breathing technique for the class.",
                            "target_lang_translations": {
                                "ru": "Инструктор <w>продемонстрирует</w> классу правильную технику дыхания."
                            }
                        },
                        {
                            "text": "Can you <w>demonstrate</w> how to operate this new coffee machine?",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>показать</w>, как пользоваться этой новой кофемашиной?"
                            }
                        }
                    ],
                    "synonyms": [
                        "show",
                        "illustrate",
                        "display"
                    ],
                    "common_phrases": [
                        "demonstrate how to",
                        "demonstrate a technique",
                        "demonstrate a product"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наглядно показывать, как что-то делается или работает."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "объяснять на примере"
                            },
                            {
                                "target_lang_word": "демонстрировать",
                                "target_lang_sense_clarification": "наглядно показывать"
                            },
                            {
                                "target_lang_word": "продемонстрировать",
                                "target_lang_sense_clarification": "показать действие или результат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e7e009a-4d0e-37b9-88f1-a469c5806547",
                    "sense_definition": "To prove something clearly by giving evidence or reasoning.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_show",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Recent statistics <w>demonstrate</w> a significant improvement in the local economy.",
                            "target_lang_translations": {
                                "ru": "Последние статистические данные <w>демонстрируют</w> значительное улучшение местной экономики."
                            }
                        },
                        {
                            "text": "The lawyer tried to <w>demonstrate</w> that his client was innocent.",
                            "target_lang_translations": {
                                "ru": "Адвокат пытался <w>доказать</w>, что его клиент невиновен."
                            }
                        }
                    ],
                    "synonyms": [
                        "prove",
                        "show",
                        "substantiate",
                        "validate",
                        "establish"
                    ],
                    "antonyms": [
                        "disprove",
                        "refute"
                    ],
                    "common_phrases": [
                        "clearly demonstrate",
                        "demonstrate a link",
                        "demonstrate ability"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доказывать что-либо, приводя факты или доводы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доказывать",
                                "target_lang_sense_clarification": "подтверждать фактами"
                            },
                            {
                                "target_lang_word": "свидетельствовать",
                                "target_lang_sense_clarification": "служить доказательством"
                            },
                            {
                                "target_lang_word": "демонстрировать",
                                "target_lang_sense_clarification": "проявлять, делать явным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6362f091-c483-3dd4-a197-7b53d63a2c04",
                    "sense_definition": "To participate in a public event to show support for or opposition to something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thousands of people gathered to <w>demonstrate</w> against the new government policy.",
                            "target_lang_translations": {
                                "ru": "Тысячи людей собрались, чтобы <w>протестовать</w> против новой политики правительства."
                            }
                        },
                        {
                            "text": "The students plan to <w>demonstrate</w> peacefully outside the university administration building.",
                            "target_lang_translations": {
                                "ru": "Студенты планируют мирно <w>провести демонстрацию</w> у здания администрации университета."
                            }
                        }
                    ],
                    "synonyms": [
                        "protest",
                        "march",
                        "rally"
                    ],
                    "common_phrases": [
                        "demonstrate against",
                        "demonstrate in favor",
                        "demonstrate peacefully"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участвовать в публичном мероприятии в знак поддержки или протеста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участвовать в демонстрации",
                                "target_lang_sense_clarification": "выходить на демонстрацию"
                            },
                            {
                                "target_lang_word": "протестовать",
                                "target_lang_sense_clarification": "выражать несогласие"
                            },
                            {
                                "target_lang_word": "митинговать",
                                "target_lang_sense_clarification": "участвовать в митинге"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
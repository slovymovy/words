    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "184d9199-d236-3063-be44-89026e149d68",
                    "sense_definition": "Any substance that can cause asphyxia.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Carbon monoxide is a dangerous <w>asphyxiant</w> that is difficult to detect without equipment.",
                            "target_lang_translations": {
                                "ru": "Угарный газ — это опасное <w>удушающее вещество</w>, которое трудно обнаружить без оборудования."
                            }
                        },
                        {
                            "text": "Workers in the chemical plant must wear masks to protect against any potential <w>asphyxiant</w>.",
                            "target_lang_translations": {
                                "ru": "Работники химического завода должны носить маски для защиты от любого потенциального <w>удушающего вещества</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "simple asphyxiant",
                        "chemical asphyxiant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любое вещество, способное вызвать удушье (асфиксию)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удушающее вещество",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "асфиксиант",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "67b2904e-6e36-31c7-a656-a875666efe3c",
                    "sense_definition": "That can cause asphyxiation.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fire produced thick, <w>asphyxiant</w> smoke that forced everyone to evacuate immediately.",
                            "target_lang_translations": {
                                "ru": "Пожар вызвал густой <w>удушающий</w> дым, который заставил всех немедленно эвакуироваться."
                            }
                        },
                        {
                            "text": "Certain industrial processes can create an <w>asphyxiant</w> atmosphere if ventilation fails.",
                            "target_lang_translations": {
                                "ru": "Некоторые промышленные процессы могут создать <w>удушающую</w> атмосферу, если вентиляция выйдет из строя."
                            }
                        }
                    ],
                    "synonyms": [
                        "suffocating",
                        "choking",
                        "smothering"
                    ],
                    "common_phrases": [
                        "asphyxiant gas",
                        "asphyxiant effect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий удушье или асфиксию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удушающий",
                                "target_lang_sense_clarification": "о дыме, газе или атмосфере"
                            }
                        ]
                    }
                }
            ]
        }
    ]

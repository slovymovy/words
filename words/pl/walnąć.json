            "pos": "verb",
            "senses": [
                {
                    "sense_id": "405854b8-9587-3f83-b46c-0dbd5b4ea1e4",
                    "sense_definition": "Uderzyć kogoś lub coś z dużą siłą, często wydając przy tym głośny dźwięk.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "impact_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodziej próbował <walnąć> policjanta ciężką torbą.",
                            "target_lang_translations": {
                                "ru": "Вор пытался <ударить> полицейского тяжелой сумкой."
                            }
                        },
                        {
                            "text": "Musisz mocniej <walnąć> w te drzwi, żeby się zamknęły.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно сильнее <стукнуть> по этой двери, чтобы она закрылась."
                            }
                        }
                    ],
                    "synonyms": [
                        "uderzyć",
                        "rąbnąć",
                        "grzmotnąć",
                        "przyłożyć"
                    ],
                    "common_phrases": [
                        "walnąć pięścią w stół",
                        "walnąć kogoś w twarz",
                        "walnąć głową w mur"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ударить кого-то или что-то с большой силой, часто издавая при этом громкий звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ударить",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "стукнуть",
                                "target_lang_sense_clarification": "разговорное, часто подразумевает звук удара"
                            },
                            {
                                "target_lang_word": "врезать",
                                "target_lang_sense_clarification": "сленг, ударить очень сильно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24c4ca65-71f1-3963-b142-a129e4bcc897",
                    "sense_definition": "Położyć coś ciężkiego lub rzucić czymś w konkretne miejsce w sposób niedbały.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "placement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po powrocie ze szkoły Tomek <walnął> plecak na środku pokoju.",
                            "target_lang_translations": {
                                "ru": "Вернувшись из школы, Томек <бросил> рюкзак посреди комнаты."
                            }
                        },
                        {
                            "text": "Nie możesz tak po prostu <walnąć> tych dokumentów na biurko szefa.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь просто так <швырнуть> эти документы на стол начальника."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzucić",
                        "cisnąć"
                    ],
                    "antonyms": [
                        "położyć ostrożnie"
                    ],
                    "common_phrases": [
                        "walnąć coś w kąt"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небрежно положить что-то тяжелое или бросить что-то в определенное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросить",
                                "target_lang_sense_clarification": "кинуть что-то куда-то"
                            },
                            {
                                "target_lang_word": "швырнуть",
                                "target_lang_sense_clarification": "бросить с силой или пренебрежением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5d9ed34-43cf-3719-a69c-a1be00828b6f",
                    "sense_definition": "Gwałtownie wybuchnąć, powodując hałas i zniszczenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "explosion_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uciekajcie stąd, bo ta butla z gazem zaraz <walnie>!",
                            "target_lang_translations": {
                                "ru": "Бегите отсюда, потому что этот газовый баллон сейчас <рванёт>!"
                            }
                        },
                        {
                            "text": "Na poligonie coś głośno <walnęło> i w niebo uniósł się dym.",
                            "target_lang_translations": {
                                "ru": "На полигоне что-то громко <бахнуло>, и в небо поднялся дым."
                            }
                        }
                    ],
                    "synonyms": [
                        "wybuchnąć",
                        "eksplodować"
                    ],
                    "common_phrases": [
                        "coś zaraz walnie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно взорваться, вызывая шум и разрушения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рвануть",
                                "target_lang_sense_clarification": "разговорное, о взрыве"
                            },
                            {
                                "target_lang_word": "бахнуть",
                                "target_lang_sense_clarification": "издать громкий звук, взорваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16a9a262-90dd-393f-a131-701c34f6c481",
                    "sense_definition": "Wypić porcję alkoholu, zazwyczaj szybko i jednym haustem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "consumption_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na imprezie postanowili <walnąć> po kieliszku wódki za zdrowie gospodarza.",
                            "target_lang_translations": {
                                "ru": "На вечеринке они решили <бахнуть> по рюмке водки за здоровье хозяина."
                            }
                        },
                        {
                            "text": "Marek musiał <walnąć> szybkie piwo na odwagi przed rozmową.",
                            "target_lang_translations": {
                                "ru": "Мареку нужно было <опрокинуть> пива для храбрости перед разговором."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypić",
                        "chlapnąć",
                        "obalić"
                    ],
                    "common_phrases": [
                        "walnąć kielicha",
                        "walnąć piwo",
                        "walnąć lufę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпить порцию алкоголя, обычно быстро и залпом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бахнуть",
                                "target_lang_sense_clarification": "выпить залпом (разговорное)"
                            },
                            {
                                "target_lang_word": "опрокинуть",
                                "target_lang_sense_clarification": "выпить рюмку или стакан"
                            },
                            {
                                "target_lang_word": "выпить",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22ce8805-44ad-36ef-849c-a6083a75de14",
                    "sense_definition": "Zrobić coś szybko, niedbale lub popełnić błąd (np. językowy).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_performance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Student <walnął> straszną gafę podczas egzaminu ustnego.",
                            "target_lang_translations": {
                                "ru": "Студент <ляпнул> ужасную глупость во время устного экзамена."
                            }
                        },
                        {
                            "text": "Nudziło jej się, więc <walnęła> sobie szybki rysunek w zeszycie.",
                            "target_lang_translations": {
                                "ru": "Ей было скучно, поэтому она <набросала> себе быстрый рисунок в тетради."
                            }
                        }
                    ],
                    "synonyms": [

                    ],
                    "antonyms": [
                        "lichamelijk",
                        "fysiek"
                    ],
                    "common_phrases": [
                        "geestelijke gezondheid",
                        "geestelijk gehandicapt",
                        "geestelijk welzijn"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c930329-2bdc-388d-b32e-2273b8b83004",
                    "sense_definition": "Исторический придворный чин и должность в Русском государстве XIII–XVII веков, второй по значимости после боярина.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "rank_and_holder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В боярской думе <w>окольничий</w> занимал высокое положение и участвовал в управлении государством.",
                            "target_lang_translations": {
                                "en": "In the Boyar Duma, the <w>okolnichy</w> held a high position and participated in the administration of the state."
                            }
                        },
                        {
                            "text": "За верную службу царь пожаловал ему чин <w>окольничего</w>.",
                            "target_lang_translations": {
                                "en": "For faithful service, the tsar granted him the rank of <w>okolnichy</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "чин окольничего",
                        "пожаловать в окольничие"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Russia"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "historical term (XIII–XVII centuries)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A historical court rank and position in the Russian state from the 13th to the 17th centuries, second in importance only to a boyar."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "okolnichy",
                                "target_lang_sense_clarification": "historical rank"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfae802b-eec2-3767-bb75-2272bdb3e5d5",
                    "sense_definition": "Человек, имеющий этот чин; придворный сановник.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "rank_and_holder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На торжественном приёме старый <w>окольничий</w> стоял рядом с царским троном.",
                            "target_lang_translations": {
                                "en": "At the ceremonial reception, the old <w>okolnichy</w> stood next to the tsar's throne."
                            }
                        },
                        {
                            "text": "Этот <w>окольничий</w> возглавлял один из важных государственных приказов.",
                            "target_lang_translations": {
                                "en": "This <w>okolnichy</w> headed one of the important state departments."
                            }
                        }
                    ],
                    "synonyms": [
                        "сановник",
                        "вельможа",
                        "придворный"
                    ],
                    "common_phrases": [
                        "думный окольничий"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Russia"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "historical term"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person holding this rank; a court dignitary."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "okolnichy",
                                "target_lang_sense_clarification": "person holding the rank"
                            }
                        ]
                    }
                }
            ]
        }
    ]

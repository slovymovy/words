    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2aa42a69-8245-3c49-a3fb-776eea6b7c2b",
                    "sense_definition": "Съедобный сладковатый корнеплод оранжевого цвета, богатый витаминами.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В суп нужно добавить лук и <w>морковь</w>.",
                            "target_lang_translations": {
                                "en": "You need to add onion and <w>carrot</w> to the soup."
                            }
                        },
                        {
                            "text": "Кролики очень любят есть свежую <w>морковь</w>.",
                            "target_lang_translations": {
                                "en": "Rabbits love eating fresh <w>carrots</w>."
                            }
                        },
                        {
                            "text": "На завтрак мама приготовила салат, в который входит тертая <w>морковь</w>.",
                            "target_lang_translations": {
                                "en": "For breakfast, Mom made a salad that includes grated <w>carrot</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "морковка"
                    ],
                    "common_phrases": [
                        "свежая морковь",
                        "тертая морковь",
                        "вареная морковь",
                        "сок из моркови"
                    ],
                    "target_lang_definitions": {
                        "en": "An edible, sweetish, orange root vegetable rich in vitamins."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carrot",
                                "target_lang_sense_clarification": "root vegetable"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f4eee88-211c-3e91-9373-afd3497d455e",
                    "sense_definition": "Огородное растение семейства зонтичных с перистыми листьями, дающее этот корнеплод.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На нашем огороде хорошо растёт <w>морковь</w> и свекла.",
                            "target_lang_translations": {
                                "en": "<w>Carrots</w> and beets grow well in our garden."
                            }
                        },
                        {
                            "text": "Весной фермеры начинают сеять <w>морковь</w> на полях.",
                            "target_lang_translations": {
                                "en": "In the spring, farmers start sowing <w>carrots</w> in the fields."
                            }
                        },
                        {
                            "text": "Высокая грядка с <w>морковью</w> уже зазеленела.",
                            "target_lang_translations": {
                                "en": "The raised bed with <w>carrots</w> has already turned green."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сажать морковь",
                        "грядка с морковью",
                        "урожай моркови"
                    ],
                    "target_lang_definitions": {
                        "en": "A garden plant of the umbrella family with feathery leaves that produces this root vegetable."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carrot",
                                "target_lang_sense_clarification": "plant"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "43bb960b-e22e-3888-aa80-35c4b4344e5e",
                    "sense_definition": "Человек, который совершает полёты в космическое пространство и проводит там исследования.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "space_profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Юрий Гагарин — первый <w>космонавт</w> в истории человечества.",
                            "target_lang_translations": {
                                "en": "Yuri Gagarin was the first <w>cosmonaut</w> in human history."
                            }
                        },
                        {
                            "text": "Каждый <w>космонавт</w> должен обладать отличным здоровьем и выносливостью.",
                            "target_lang_translations": {
                                "en": "Every <w>cosmonaut</w> must possess excellent health and stamina."
                            }
                        }
                    ],
                    "synonyms": [
                        "звездоплаватель",
                        "космоплаватель"
                    ],
                    "antonyms": [
                        "астронавт",
                        "тайконавт"
                    ],
                    "common_phrases": [
                        "первый космонавт",
                        "лётчик-космонавт",
                        "отряд космонавтов",
                        "советский космонавт"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who flies into outer space and conducts research there."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cosmonaut",
                                "target_lang_sense_clarification": "specifically a Russian or Soviet space traveler"
                            },
                            {
                                "target_lang_word": "astronaut",
                                "target_lang_sense_clarification": "general term for a space traveler"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02bbb611-fbed-3078-960a-9156ca42f7f8",
                    "sense_definition": "Сотрудник силовых структур (обычно ОМОН) в защитной экипировке и шлеме, работающий на массовых мероприятиях.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "police_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Площадь была оцеплена, и <w>космонавты</w> никого не пропускали за ограждение.",
                            "target_lang_translations": {
                                "en": "The square was cordoned off, and the <w>riot cops</w> wouldn't let anyone past the barrier."
                            }
                        },
                        {
                            "text": "В толпе шутили, что <w>космонавты</w> в своих шлемах ничего не слышат.",
                            "target_lang_translations": {
                                "en": "The crowd joked that the <w>riot cops</w> couldn't hear anything inside their helmets."
                            }
                        }
                    ],
                    "antonyms": [
                        "тихарь"
                    ],
                    "common_phrases": [
                        "атака космонавтов",
                        "шлемы космонавтов"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "ironic usage due to resemblance of riot gear to space suits"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A law enforcement officer (usually riot police) in protective gear and a helmet, working at mass gatherings."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "riot cop",
                                "target_lang_sense_clarification": "slang/informal"
                            },
                            {
                                "target_lang_word": "riot police officer",
                                "target_lang_sense_clarification": "formal equivalent"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
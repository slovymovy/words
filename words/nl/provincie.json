{
    "word_family": [
        "provinciaal",
        "provincialisme"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1a235ceb-d34a-3f47-a566-08b2c48cceea",
                    "sense_definition": "Een staatkundig onderdeel van een land, vaak met een eigen bestuur",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "administrative_division",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nederland heeft in totaal twaalf verschillende <w>provincies</w>.",
                            "target_lang_translations": {
                                "ru": "В Нидерландах всего двенадцать разных <w>провинций</w>."
                            }
                        },
                        {
                            "text": "Het bestuur van de <w>provincie</w> beslist over de aanleg van nieuwe wegen.",
                            "target_lang_translations": {
                                "ru": "Управление <w>провинции</w> принимает решение о прокладке новых дорог."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de provincie",
                        "de provincie Utrecht",
                        "gedeputeerde staten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Административно-территориальная единица страны, часто имеющая собственное управление"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провинция",
                                "target_lang_sense_clarification": "административная единица"
                            },
                            {
                                "target_lang_word": "область",
                                "target_lang_sense_clarification": "регион"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "532b0696-bc66-3321-a5b1-3a6679a6eaa3",
                    "sense_definition": "Het gebied buiten de grote steden of de hoofdstad; het platteland",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "countryside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij verhuisden van het drukke Amsterdam naar de rustige <w>provincie</w>.",
                            "target_lang_translations": {
                                "ru": "Они переехали из шумного Амстердама в тихую <w>провинцию</w>."
                            }
                        },
                        {
                            "text": "In de <w>provincie</w> zijn de huizenprijzen vaak lager dan in de stad.",
                            "target_lang_translations": {
                                "ru": "В <w>провинции</w> цены на дома часто ниже, чем в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "platteland",
                        "buitengebied"
                    ],
                    "antonyms": [
                        "stad",
                        "hoofdstad"
                    ],
                    "common_phrases": [
                        "de diepe provincie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория за пределами крупных городов или столицы; сельская местность"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провинция",
                                "target_lang_sense_clarification": "местность вне столицы"
                            },
                            {
                                "target_lang_word": "глубинка",
                                "target_lang_sense_clarification": "удаленная от центра местность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверка",
                                "target_lang_sense_clarification": "инспекция"
                            },
                            {
                                "target_lang_word": "осмотр",
                                "target_lang_sense_clarification": "обследование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75674adb-1533-36fb-afbc-436fa3f63edd",
                    "sense_definition": "A document showing the amount of money owed for goods or services, especially in a restaurant.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "8",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Could we have the <w>check</w>, please?",
                            "target_lang_translations": {
                                "ru": "Можно нам <w>счёт</w>, пожалуйста?"
                            }
                        },
                        {
                            "text": "My friend insisted on paying the <w>check</w> after our dinner.",
                            "target_lang_translations": {
                                "ru": "Мой друг настоял на оплате <w>счёта</w> после нашего ужина."
                            }
                        }

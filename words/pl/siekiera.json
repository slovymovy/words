{
    "word_family": [
        "siekierka",
        "siekierowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d6f81d9-fd22-34b7-be97-70c36941cfb5",
                    "sense_definition": "Ręczne narzędzie składające się z metalowego ostrza osadzonego na drewnianym trzonku, służące głównie do rąbania drewna.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drwal wziął ostrą <w>siekierę</w> i poszedł do lasu ścinać drzewa.",
                            "target_lang_translations": {
                                "ru": "Лесоруб взял острый <w>топор</w> и пошел в лес валить деревья."
                            }
                        },
                        {
                            "text": "Muszę naostrzyć <w>siekierę</w>, żeby porąbać drewno do kominka.",
                            "target_lang_translations": {
                                "ru": "Мне нужно наточить <w>топор</w>, чтобы нарубить дров для камина."
                            }
                        }
                    ],
                    "synonyms": [
                        "topór",
                        "rąbanica"
                    ],
                    "common_phrases": [
                        "ostra siekiera",
                        "rąbać siekierą",
                        "ostrze siekiery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ручной инструмент, состоящий из металлического лезвия, насаженного на деревянную рукоятку, служащий в основном для рубки дров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "топор",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5d66bc4-41cd-3b99-b361-f97f525cc95f",
                    "sense_definition": "Bardzo mocna kawa lub herbata; rzadziej gęste, duszne powietrze nasycone dymem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta poranna kawa to prawdziwa <w>siekiera</w>, od razu stawia na nogi.",
                            "target_lang_translations": {
                                "ru": "Этот утренний кофе — настоящий <w>чифир</w>, сразу ставит на ноги."
                            }
                        },
                        {
                            "text": "W małym pokoju panowała straszna <w>siekiera</w> od dymu papierosowego.",
                            "target_lang_translations": {
                                "ru": "В маленькой комнате стоял жуткий <w>кумар</w> от сигаретного дыма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prawdziwa siekiera",
                        "ale siekiera"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень крепкий кофе или чай; реже — густой, душный воздух, насыщенный дымом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чифир",
                                "target_lang_sense_clarification": "очень крепкий чай или кофе (разг.)"
                            },
                            {
                                "target_lang_word": "кумар",
                                "target_lang_sense_clarification": "густой дым, духота (жарг.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f4437d96-eb88-3309-b2f5-cc6e39ecee6c",
                    "sense_definition": "Nazwisko używane w Polsce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Siekiera</w> jest naszym nowym sąsiadem z parteru.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Секера</w> — наш новый сосед с первого этажа."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie słyszałem o kimś, kto nazywa się <w>Siekiera</w>.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не слышал о ком-то по фамилии <w>Секера</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, встречающаяся в Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Секера",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
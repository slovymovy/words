                                "target_lang_word": "червяк",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "earthworm",
                                "target_lang_sense_clarification": "animal"
                            },
                            {
                                "target_lang_word": "worm",
                                "target_lang_sense_clarification": "common term"
                            }
                        ]
                    }

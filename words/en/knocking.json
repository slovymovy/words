        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9dfae786-6ab1-3776-92ed-21747c4d7bdf",
                    "sense_definition": "An act of striking a surface or the sound produced by such an action.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We heard a loud <w>knocking</w> on the front door late at night.",
                            "target_lang_translations": {
                                "ru": "Поздно ночью мы услышали громкий <w>стук</w> во входную дверь."
                            }
                        },
                        {
                            "text": "The persistent <w>knocking</w> finally woke him from his deep sleep.",
                            "target_lang_translations": {
                                "ru": "Настойчивый <w>стук</w> наконец разбудил его от глубокого сна."
                            }
                        }
                    ],
                    "synonyms": [
                        "rapping",
                        "banging",
                        "pounding",
                        "tapping"
                    ],
                    "common_phrases": [
                        "loud knocking",
                        "knocking at the door",
                        "sudden knocking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук удара о поверхность или действие, производящее такой звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стук",
                                "target_lang_sense_clarification": "звук или действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64bff667-452f-3cad-9a38-bc9b37e76253",
                    "sense_definition": "A repetitive metallic noise from an engine caused by improper combustion.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "engine_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic identified the problem immediately after hearing the engine <w>knocking</w>.",
                            "target_lang_translations": {
                                "ru": "Механик сразу определил проблему, услышав <w>стук</w> в двигателе."
                            }
                        },
                        {
                            "text": "Using low-quality fuel can sometimes cause <w>knocking</w> in older cars.",
                            "target_lang_translations": {
                                "ru": "Использование некачественного топлива иногда может вызвать <w>детонацию</w> в старых автомобилях."
                            }
                        }
                    ],
                    "synonyms": [
                        "pinging",
                        "pinking"
                    ],
                    "common_phrases": [
                        "engine knocking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повторяющийся металлический шум в двигателе, вызванный неправильным сгоранием топлива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стук",
                                "target_lang_sense_clarification": "в двигателе"
                            },
                            {
                                "target_lang_word": "детонация",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

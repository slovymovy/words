            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "859c3eb8-0b99-319b-9f6a-302c061764e9",
                    "sense_definition": "Taki, który ma u kogoś dług pieniężny lub zobowiązanie moralne (np. wdzięczność, przeprosiny).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jesteś mi <w>winny</w> pięćdziesiąt złotych za wczorajszą kolację.",
                            "target_lang_translations": {
                                "ru": "Ты мне <w>должен</w> пятьдесят злотых за вчерашний ужин."
                            }
                        },
                        {
                            "text": "Uważam, że jestem jej <w>winny</w> szczere przeprosiny za moje zachowanie.",
                            "target_lang_translations": {
                                "ru": "Я считаю, что <w>должен</w> принести ей искренние извинения за своё поведение."
                            }
                        }
                    ],
                    "synonyms": [
                        "dłużny",
                        "zobowiązany"
                    ],
                    "common_phrases": [
                        "być winnym pieniądze",
                        "winny przysługę",
                        "winny wyjaśnienia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий денежный долг или моральное обязательство перед кем-либо (например, благодарность, извинения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "должен",
                                "target_lang_sense_clarification": "о долге или обязательстве"
                            },
                            {
                                "target_lang_word": "должный",
                                "target_lang_sense_clarification": "имеющий долг (устар.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea011605-3354-38c1-ab6c-e6edc166992a",
                    "sense_definition": "Taki, który popełnił przestępstwo, wykroczenie lub zrobił coś niewłaściwego; ponoszący odpowiedzialność za zło.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "guilt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd orzekł, że oskarżony jest <w>winny</w> kradzieży samochodu.",
                            "target_lang_translations": {
                                "ru": "Суд постановил, что обвиняемый <w>виновен</w> в краже автомобиля."
                            }
                        },
                        {
                            "text": "Mimo braku dowodów, on wciąż czuł się <w>winny</w> zaistniałej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Несмотря на отсутствие улик, он всё ещё чувствовал себя <w>виноватым</w> в сложившейся ситуации."
                            }
                        }
                    ],
                    "antonyms": [
                        "niewinny",
                        "bezwinny",
                        "czysty"
                    ],
                    "common_phrases": [
                        "uznać za winnego",
                        "czuć się winnym",
                        "winny zarzutów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершивший преступление, проступок или сделавший что-то неправильное; несущий ответственность за что-то плохое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виновный",
                                "target_lang_sense_clarification": "юридический или формальный термин"
                            },
                            {
                                "target_lang_word": "виноватый",
                                "target_lang_sense_clarification": "в бытовом смысле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5f283ff-0708-319f-aa27-073f07c95019",
                    "sense_definition": "Odnoszący się do wina, powstały z wina lub mający jego charakterystyczny, kwaskowaty smak.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "wine_relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do tej sałatki najlepiej dodać dobrej jakości ocet <w>winny</w>.",
                            "target_lang_translations": {
                                "ru": "В этот салат лучше всего добавить качественный <w>винный</w> уксус."
                            }
                        },
                        {
                            "text": "Te jabłka mają specyficzny, lekko <w>winny</w> posmak, który bardzo lubię.",
                            "target_lang_translations": {
                                "ru": "У этих яблок специфический, слегка <w>винный</w> привкус, который мне очень нравится."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ocet winny",
                        "kwas winny",
                        "kamień winny",
                        "latorośl winna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к вину, сделанный из вина или имеющий его характерный кисловатый вкус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "винный",
                                "target_lang_sense_clarification": "относящийся к вину"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43b0eb59-01ba-36fa-996b-4b399f5bb48f",
                    "sense_definition": "Mający ciemnoczerwony kolor wpadający w fiolet, przypominający barwę czerwonego wina.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na uroczystość założyła elegancką sukienkę w kolorze <w>winnym</w>.",
                            "target_lang_translations": {
                                "ru": "На торжество она надела элегантное платье <w>винного</w> цвета."
                            }
                        },
                        {
                            "text": "Ściany w salonie pomalowaliśmy na głęboki, <w>winny</w> odcień.",
                            "target_lang_translations": {
                                "ru": "Мы покрасили стены в гостиной в глубокий <w>винный</w> оттенок."
                            }
                        }
                    ],
                    "synonyms": [
                        "bordo",
                        "bordowy"
                    ],
                    "common_phrases": [
                        "winna czerwień",
                        "kolor winny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий тёмно-красный цвет с фиолетовым оттенком, напоминающий цвет красного вина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "винный",
                                "target_lang_sense_clarification": "о цвете"
                            },
                            {
                                "target_lang_word": "бордовый",
                                "target_lang_sense_clarification": "название цвета"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2ed4ac66-bec9-3c9b-9b34-b2a492e01801",
                    "sense_definition": "Защищаясь, успешно отбить нападение или удар, заставив противника отступить.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Армия была готова <отразить> внезапную атаку врага.",
                            "target_lang_translations": {
                                "en": "The army was ready to <repel> a sudden enemy attack."
                            }
                        },
                        {
                            "text": "Вратарь сумел <отразить> сильный удар по воротам.",
                            "target_lang_translations": {
                                "en": "The goalkeeper managed to <parry> a strong shot on goal."
                            }
                        }
                    ],
                    "synonyms": [
                        "отзеркалить",
                        "отзеркалить"
                    ],
                    "common_phrases": [
                        "отразить атаку",
                        "отразить удар",
                        "отразить нападение",
                        "отразить угрозу"
                    ],
                    "target_lang_definitions": {
                        "en": "To successfully fight off an attack or blow in self-defense, forcing the opponent to retreat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "repel",
                                "target_lang_sense_clarification": "an attack or enemy force"
                            },
                            {
                                "target_lang_word": "ward off",
                                "target_lang_sense_clarification": "a threat or attack"
                            },
                            {
                                "target_lang_word": "parry",
                                "target_lang_sense_clarification": "a physical blow or strike"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9637ad3b-b0ca-373a-ae99-aee4f1b62355",
                    "sense_definition": "Воспроизвести изображение предмета на гладкой или блестящей поверхности (например, в зеркале или воде).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спокойное озеро смогло идеально <отразить> облака.",
                            "target_lang_translations": {
                                "en": "The calm lake was able to perfectly <reflect> the clouds."
                            }
                        },
                        {
                            "text": "Старинное зеркало <отразило> её испуганное лицо.",
                            "target_lang_translations": {
                                "en": "The antique mirror <reflected> her frightened face."
                            }
                        }
                    ],
                    "synonyms": [
                        "отзеркалить"
                    ],
                    "common_phrases": [
                        "отразить в зеркале",
                        "отразить лицо",
                        "отразить небо"
                    ],
                    "target_lang_definitions": {
                        "en": "To reproduce the image of an object on a smooth or shiny surface (e.g., in a mirror or water)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reflect",
                                "target_lang_sense_clarification": "an image"
                            },
                            {
                                "target_lang_word": "mirror",
                                "target_lang_sense_clarification": "to show a reflection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "088563b9-b118-3cea-a017-db5626f00759",
                    "sense_definition": "Показать, зафиксировать или передать информацию, события или чувства (в документе, творчестве и т.п.).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бухгалтер должен правильно <отразить> все расходы в отчёте.",
                            "target_lang_translations": {
                                "en": "The accountant must correctly <record> all expenses in the report."
                            }
                        },
                        {
                            "text": "Художник хотел <отразить> в картине красоту осени.",
                            "target_lang_translations": {
                                "en": "The artist wanted to <capture> the beauty of autumn in the painting."
                            }
                        }
                    ],
                    "synonyms": [
                        "показать",
                        "изобразить"
                    ],
                    "common_phrases": [
                        "отразить в отчёте",
                        "отразить суть",
                        "отразить действительность",
                        "отразить мнение"
                    ],
                    "target_lang_definitions": {
                        "en": "To show, record, or convey information, events, or feelings (in a document, creative work, etc.)."
                    },
                    "translations": {

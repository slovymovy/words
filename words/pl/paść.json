                },
                {
                    "sense_id": "b668c499-8204-3041-a31c-f41b53d19e4d",
                    "sense_definition": "Nagle opanować kogoś lub wpłynąć na kogoś (o silnych, zwykle negatywnych emocjach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotion_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wszystkich zebranych <w>padł</w> blady strach.",
                            "target_lang_translations": {
                                "ru": "На всех собравшихся <w>напал</w> дикий страх."
                            }
                        },
                        {
                            "text": "Cień podejrzenia <w>padł</w> na niewinnego pracownika.",
                            "target_lang_translations": {
                                "ru": "Тень подозрения <w>пала</w> на невиновного сотрудника."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogarnąć",
                        "pochłonąć"
                    ],
                    "common_phrases": [
                        "blady strach padł",
                        "padł cień podejrzenia"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "styl literacki lub książkowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно охватить кого-то или повлиять на кого-то (о сильных, обычно негативных эмоциях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "о тени, подозрении"
                            },
                            {
                                "target_lang_word": "напасть",
                                "target_lang_sense_clarification": "о страхе, тоске"
                            },
                            {
                                "target_lang_word": "овладеть",
                                "target_lang_sense_clarification": "об эмоциях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c4b8ec5-338c-3695-833f-f588f0c24115",
                    "sense_definition": "Przypaść komuś w udziale; stać się czyimś zadaniem w wyniku wyboru lub losowania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "choice_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym tygodniu <w>padło</w> na mnie, żeby posprzątać biuro.",
                            "target_lang_translations": {
                                "ru": "На этой неделе <w>выпало</w> мне убирать офис."
                            }
                        },
                        {
                            "text": "Losowanie sprawiło, że wybór <w>padł</w> na najmłodszego kandydata.",
                            "target_lang_translations": {
                                "ru": "Жребий определил, что выбор <w>пал</w> на самого молодого кандидата."
                            }
                        }
                    ],
                    "common_phrases": [
                        "padło na mnie",
                        "wybór padł na"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достаться кому-то; стать чьей-то задачей в результате выбора или жребия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "о выборе"
                            },
                            {
                                "target_lang_word": "выпасть",
                                "target_lang_sense_clarification": "о жребии, доле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbc4c6bd-a24a-37de-806b-22b29736fe7b",
                    "sense_definition": "Bardzo mocno się roześmiać lub być czymś niezwykle zaskoczonym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy opowiedział ten żart, po prostu <w>padłem</w> ze śmiechu.",
                            "target_lang_translations": {
                                "ru": "Когда он рассказал эту шутку, я просто <w>умер</w> со смеху."
                            }
                        },
                        {
                            "text": "<w>Padłem</w>, jak zobaczyłem jego nowe przebranie.",
                            "target_lang_translations": {
                                "ru": "Я <w>выпал в осадок</w>, когда увидел его новый костюм."
                            }
                        }
                    ],
                    "common_phrases": [
                        "paść ze śmiechu",
                        "paść z wrażenia"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень сильно рассмеяться или быть крайне удивленным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умереть",
                                "target_lang_sense_clarification": "со смеху (перен.)"
                            },
                            {
                                "target_lang_word": "упасть",
                                "target_lang_sense_clarification": "от удивления или смеха"
                            },
                            {
                                "target_lang_word": "выпасть в осадок",
                                "target_lang_sense_clarification": "сильно удивиться (слэнг)"
                            }
                        ]
                    }
                }
            ]
        },

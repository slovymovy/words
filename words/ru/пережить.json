            "senses": [
                {
                    "sense_id": "1fcd66bd-00ce-3f7e-977e-0d59af3dee5b",
                    "sense_definition": "Выдержать, перенести какое-либо тяжелое событие, испытание или чувство; остаться в живых после опасности.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "endure_survive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эту трудную неделю надо просто <w>пережить</w>, а дальше будет легче.",
                            "target_lang_translations": {
                                "en": "We just need to <w>get through</w> this difficult week, and then it will be easier."
                            }
                        },
                        {
                            "text": "Нам удалось <w>пережить</w> суровую зиму в горах без электричества.",
                            "target_lang_translations": {
                                "en": "We managed to <w>survive</w> the harsh winter in the mountains without electricity."
                            }
                        },
                        {
                            "text": "Он не знал, как <w>пережить</w> горе после потери близкого друга.",
                            "target_lang_translations": {
                                "en": "He didn't know how to <w>endure</w> the grief after losing a close friend."
                            }
                        }
                    ],
                    "synonyms": [
                        "выдержать",
                        "перенести",
                        "вытерпеть",
                        "выжить"
                    ],
                    "antonyms": [
                        "погибнуть",
                        "сломаться",
                        "сдаться"
                    ],
                    "common_phrases": [
                        "пережить кризис",
                        "пережить горе",
                        "пережить зиму",
                        "пережить стресс",
                        "тяжело пережить"
                    ],
                    "target_lang_definitions": {
                        "en": "To withstand or endure a difficult event, trial, or feeling; to remain alive after danger."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to survive",
                                "target_lang_sense_clarification": "to remain alive or functional through a crisis or hardship"
                            },
                            {
                                "target_lang_word": "to endure",
                                "target_lang_sense_clarification": "to suffer through something painful or difficult patiently"
                            },
                            {
                                "target_lang_word": "to get through",
                                "target_lang_sense_clarification": "to successfully reach the end of a difficult period or situation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a8538c6-7f5f-345b-b11a-d91b01727a8b",
                    "sense_definition": "Прожить дольше, чем кто-либо другой или что-либо.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "outlive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она <w>пережила</w> своего мужа на двадцать лет.",
                            "target_lang_translations": {
                                "en": "She <w>outlived</w> her husband by twenty years."
                            }
                        },
                        {
                            "text": "Этот старинный каменный дом <w>пережил</w> уже несколько поколений владельцев.",
                            "target_lang_translations": {
                                "en": "This old stone house has already <w>outlasted</w> several generations of owners."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пережить мужа",
                        "пережить жену",
                        "пережить всех",
                        "пережить век"
                    ],
                    "target_lang_definitions": {
                        "en": "To live longer than someone or something else."
                    },
                    "translations": {

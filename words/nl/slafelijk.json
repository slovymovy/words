{
    "word_family": [
        "slaaf",
        "slavernij",
        "slaafs",
        "verslaven"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5d4b779e-2ace-3050-b1b7-22bf49df7caf",
                    "sense_definition": "Gedragend op een manier die kenmerkend is voor een slaaf, met name overdreven gehoorzaam of onderdanig.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten toonden een <w>slafelijke</w> gehoorzaamheid aan hun generaal.",
                            "target_lang_translations": {
                                "en": "The soldiers showed a <w>slavish</w> obedience to their general.",
                                "ru": "Солдаты проявляли <w>рабскую</w> покорность своему генералу."
                            }
                        },
                        {
                            "text": "Zij weigerde zich <w>slafelijk</w> op te stellen en kwam voor zichzelf op.",
                            "target_lang_translations": {
                                "en": "She refused to behave <w>servilely</w> and stood up for herself.",
                                "ru": "Она отказалась занимать <w>рабскую</w> позицию и постояла за себя."
                            }
                        }
                    ],
                    "synonyms": [
                        "slaafs",
                        "onderdanig"
                    ],
                    "common_phrases": [
                        "slafelijke gehoorzaamheid",
                        "slafelijke navolging",
                        "slafelijke onderwerping"
                    ],
                    "target_lang_definitions": {
                        "en": "Behaving in a manner characteristic of a slave, particularly overly obedient or submissive.",
                        "ru": "Ведущий себя характерным для раба образом, в частности, чрезмерно послушный или покорный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slavish",
                                "target_lang_sense_clarification": "resembling a slave in unquestioning obedience"
                            },
                            {
                                "target_lang_word": "servile",
                                "target_lang_sense_clarification": "showing an excessive willingness to serve or please others"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рабский",
                                "target_lang_sense_clarification": "свойственный рабу, покорный"
                            },
                            {
                                "target_lang_word": "подобострастный",
                                "target_lang_sense_clarification": "чрезмерно угодливый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bcdf56a-74e7-36f1-af52-8c4d85eee34a",
                    "sense_definition": "Met betrekking tot arbeid die zeer zwaar, uitputtend en vaak eentonig is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "labor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arbeiders verrichtten dagelijks zware en <w>slafelijke</w> arbeid in de mijnen.",
                            "target_lang_translations": {
                                "en": "The workers performed heavy and <w>backbreaking</w> labor in the mines daily.",
                                "ru": "Рабочие ежедневно выполняли тяжелый и <w>каторжный</w> труд в шахтах."
                            }
                        },
                        {
                            "text": "Het was een <w>slafelijk</w> karwei dat veel uithoudingsvermogen vereiste.",
                            "target_lang_translations": {
                                "en": "It was a <w>grueling</w> chore that required a lot of stamina.",
                                "ru": "Это была <w>рабская</w> работа, которая требовала большой выносливости."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitputtend",
                        "vermoeiend"
                    ],
                    "common_phrases": [
                        "slafelijke arbeid",
                        "slafelijk werk"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to labor that is very heavy, exhausting, and often monotonous.",
                        "ru": "Относящийся к труду, который является очень тяжелым, изнурительным и часто однообразным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "backbreaking",
                                "target_lang_sense_clarification": "physically very hard and tiring"
                            },
                            {
                                "target_lang_word": "grueling",
                                "target_lang_sense_clarification": "extremely tiring and demanding"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "каторжный",
                                "target_lang_sense_clarification": "невыносимо тяжелый (о работе)"
                            },
                            {
                                "target_lang_word": "рабский",
                                "target_lang_sense_clarification": "тяжелый и подневольный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    ],
                    "common_phrases": [
                        "cell migration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещение клеток в определённые места внутри организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миграция",

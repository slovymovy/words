                    "sense_definition": "Meervoud van <w>stok</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hond kwam vrolijk terug met twee grote <stokken> in zijn bek.",
                            "target_lang_translations": {
                                "ru": "Собака радостно вернулась с двумя большими <w>палками</w> в зубах.",
                                "en": "The dog came back happily with two big <w>sticks</w> in its mouth."
                            }
                        },
                        {
                            "text": "We verzamelden droge <stokken> in het bos om een kampvuur te maken.",
                            "target_lang_translations": {
                                "ru": "Мы собирали в лесу сухие <w>палки</w>, чтобы развести костер.",
                                "en": "We gathered dry <w>sticks</w> in the forest to make a campfire."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова «stok» (палка).",
                        "en": "Plural of <w>stok</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "палки",
                                "target_lang_sense_clarification": "обломанные ветки, куски дерева"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "sticks",
                                "target_lang_sense_clarification": "pieces of wood, broken branches"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c31ca651-f853-3a40-abda-4cb17f324456",
                    "sense_definition": "Tot stilstand komen, niet verdergaan of tijdelijk onderbroken worden (vaak gezegd van stem, adem, verkeer of processen).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Haar adem <stokte> even toen ze het onverwachte nieuws hoorde.",
                            "target_lang_translations": {
                                "ru": "Ее дыхание на мгновение <w>замерло</w>, когда она услышала неожиданную новость.",
                                "en": "Her breath <w>caught</w> for a moment when she heard the unexpected news."
                            }
                        },
                        {
                            "text": "Het gesprek <stokte> plotseling toen de directeur de kamer binnenkwam.",
                            "target_lang_translations": {
                                "ru": "Разговор внезапно <w>прервался</w>, когда директор вошел в комнату.",
                                "en": "The conversation suddenly <w>stopped</w> when the director entered the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "haperen"
                    ],
                    "antonyms": [
                        "doorgaan"
                    ],
                    "common_phrases": [
                        "de adem stokte",
                        "het gesprek stokte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Останавливаться, прерываться или замирать (часто о голосе, дыхании, движении или процессах).",
                        "en": "To come to a standstill, stop, or be temporarily interrupted (often said of voice, breath, traffic, or processes)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "останавливаться",
                                "target_lang_sense_clarification": "прекращаться (о движении или процессе)"
                            },
                            {
                                "target_lang_word": "прерываться",
                                "target_lang_sense_clarification": "обрываться (о разговоре или голосе)"
                            },
                            {
                                "target_lang_word": "замирать",
                                "target_lang_sense_clarification": "перехватывать (о дыхании)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "to stop",
                                "target_lang_sense_clarification": "to come to a halt"
                            },
                            {
                                "target_lang_word": "to falter",
                                "target_lang_sense_clarification": "to hesitate or be interrupted (voice, conversation)"
                            },
                            {
                                "target_lang_word": "to stall",
                                "target_lang_sense_clarification": "to stop making progress (process, traffic)"
                            },
                            {
                                "target_lang_word": "to catch",
                                "target_lang_sense_clarification": "to stop momentarily (specifically of breath)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a10a71a8-ea90-32ec-89ee-14092f2e10df",
                    "sense_definition": "Letnia przerwa od zajęć szkolnych i akademickich, trwająca zazwyczaj od końca czerwca do początku września.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "vacation_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sierpień jest ostatnim miesiącem <w>wakacji</w>.",
                            "target_lang_translations": {
                                "ru": "Август — последний месяц <w>каникул</w>."
                            }
                        },
                        {
                            "text": "Wszyscy uczniowie z radością czekają na letnie <w>wakacje</w>.",
                            "target_lang_translations": {
                                "ru": "Все ученики с радостью ждут летних <w>каникул</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ferie letnie"
                    ],
                    "antonyms": [
                        "rok szkolny"
                    ],
                    "common_phrases": [
                        "letnie wakacje",
                        "koniec wakacji",
                        "zaczynać wakacje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Летний перерыв в школьных и академических занятиях, обычно длящийся с конца июня до начала сентября."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каникулы",
                                "target_lang_sense_clarification": "перерыв в учебе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a09ee422-0bc1-3163-ac64-b6bfa10ff626",
                    "sense_definition": "Czas wolny od pracy lub codziennych obowiązków, często przeznaczony na wyjazd wypoczynkowy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "vacation_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym roku planujemy spędzić <w>wakacje</w> we Włoszech.",
                            "target_lang_translations": {
                                "ru": "В этом году мы планируем провести <w>отпуск</w> в Италии."
                            }
                        },
                        {
                            "text": "Jutro robię sobie <w>wakacje</w> i nie myślę o pracy.",
                            "target_lang_translations": {
                                "ru": "Завтра я устраиваю себе <w>отпуск</w> и не думаю о работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "urlop",
                        "wczasy",
                        "wypoczynek"
                    ],
                    "antonyms": [
                        "praca"
                    ],
                    "common_phrases": [
                        "jechać na wakacje",
                        "spędzać wakacje",
                        "wymarzone wakacje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободное от работы или повседневных обязанностей время, часто предназначенное для поездки на отдых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отпуск",
                                "target_lang_sense_clarification": "освобождение от работы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
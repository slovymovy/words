    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c7fb9d5-a026-3559-ad82-bdb7cd2955a1",
                    "sense_definition": "A cord or ribbon passed through eyelets in a shoe or garment, pulled tight and tied to fasten the shoe or garment firmly.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fastener",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He stopped walking to tie his shoe <w>lace</w>.",
                            "target_lang_translations": {
                                "ru": "Он остановился, чтобы завязать <w>шнурок</w>."
                            }
                        },
                        {
                            "text": "One of the <w>laces</w> on my boot snapped when I pulled it too hard.",
                            "target_lang_translations": {
                                "ru": "Один из <w>шнурков</w> на моем ботинке порвался, когда я слишком сильно его потянул."
                            }
                        }
                    ],
                    "synonyms": [
                        "shoelace"
                    ],
                    "common_phrases": [
                        "shoe lace",
                        "tie laces",
                        "loose lace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шнурок или лента, продетые через отверстия в обуви или одежде для затягивания и фиксации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шнурок",
                                "target_lang_sense_clarification": "для обуви"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02cc42e9-d616-3e58-9e54-a2d7003562e2",
                    "sense_definition": "A light fabric containing patterns of holes, usually built up from a single thread.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bride wore a beautiful dress made of white <w>lace</w>.",
                            "target_lang_translations": {
                                "ru": "Невеста была в красивом платье из белого <w>кружева</w>."
                            }
                        },
                        {
                            "text": "She hung delicate <w>lace</w> curtains in the living room window.",
                            "target_lang_translations": {
                                "ru": "Она повесила изящные <w>кружевные</w> занавески на окно в гостиной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lace curtains",
                        "lace dress",
                        "black lace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкая ажурная ткань с узорами из дырочек, обычно создаваемая из одной нити."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружево",
                                "target_lang_sense_clarification": "ткань"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ccb5b34-784d-3431-9100-053503ec37c3",
                    "sense_definition": "Spirits added to coffee or another beverage.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He enjoyed his morning coffee with a little <w>lace</w> of brandy.",
                            "target_lang_translations": {
                                "ru": "Он наслаждался утренним кофе с небольшой <w>добавкой</w> бренди."
                            }
                        },
                        {
                            "text": "The bartender added a <w>lace</w> of rum to the hot chocolate.",
                            "target_lang_translations": {
                                "ru": "Бармен добавил <w>каплю</w> рома в горячий шоколад."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "usage as a noun for the added spirit itself is less common in modern English than the verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спиртное, добавленное в кофе или другой напиток."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добавка",
                                "target_lang_sense_clarification": "спиртного"
                            },
                            {
                                "target_lang_word": "капля",
                                "target_lang_sense_clarification": "малое количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "194c893f-3415-3b69-a1b5-f55257505686",
                    "sense_definition": "A snare or gin, especially one made of interwoven cords; a net.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hunter set a hidden <w>lace</w> in the forest to catch small game.",
                            "target_lang_translations": {
                                "ru": "Охотник установил скрытый <w>силок</w> в лесу, чтобы поймать мелкую дичь."
                            }
                        },
                        {
                            "text": "The bird was caught in the <w>lace</w> spread across the grass.",
                            "target_lang_translations": {
                                "ru": "Птица попалась в <w>сеть</w>, расстеленную на траве."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical usage found in older texts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ловушка или силок, особенно сделанные из переплетенных шнуров; сеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "силок",
                                "target_lang_sense_clarification": "ловушка"
                            },
                            {
                                "target_lang_word": "петля",
                                "target_lang_sense_clarification": "ловушка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6650f7c8-323e-3e1d-86de-34efdf15f5bb",
                    "sense_definition": "To fasten something, such as shoes or a garment, with laces.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fasten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please sit down and <w>lace</w> your boots tightly before we start hiking.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, сядь и туго <w>зашнуруй</w> ботинки, прежде чем мы начнем поход."
                            }
                        },
                        {
                            "text": "It took her a few minutes to <w>lace</w> up the back of her corset.",
                            "target_lang_translations": {
                                "ru": "У нее ушло несколько минут, чтобы <w>зашнуровать</w> корсет сзади."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lace up",
                        "lace shoes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Завязывать что-либо, например обувь или одежду, с помощью шнурков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шнуровать",
                                "target_lang_sense_clarification": "завязывать шнурки"
                            },
                            {
                                "target_lang_word": "зашнуровывать",
                                "target_lang_sense_clarification": "затягивать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "968212fa-8149-39bd-bbda-f5ab8e7e0a69",
                    "sense_definition": "To add alcohol, poison, a drug or anything else potentially harmful to food or drink.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mix_in",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The detective discovered that the victim's drink had been <w>laced</w> with poison.",
                            "target_lang_translations": {
                                "ru": "Детектив обнаружил, что в напиток жертвы был <w>подмешан</w> яд."
                            }
                        },
                        {
                            "text": "Someone secretly <w>laced</w> the punch with strong alcohol at the party.",
                            "target_lang_translations": {
                                "ru": "Кто-то тайно <w>подмешал</w> крепкий алкоголь в пунш на вечеринке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "laced with poison",
                        "drink was laced"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавлять алкоголь, яд или что-либо вредное в еду или напиток."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подмешивать",
                                "target_lang_sense_clarification": "добавлять тайно"
                            },
                            {
                                "target_lang_word": "сдабривать",
                                "target_lang_sense_clarification": "добавлять для вкуса или эффекта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73cd74fa-5e69-37bb-a42d-2daed03e7af4",
                    "sense_definition": "To intersperse or diversify with something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mix_in",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her speech was <w>laced</w> with humor and interesting anecdotes.",
                            "target_lang_translations": {
                                "ru": "Ее речь была <w>пронизана</w> юмором и интересными историями."
                            }
                        },
                        {
                            "text": "The author wrote a thriller <w>laced</w> with historical facts.",
                            "target_lang_translations": {
                                "ru": "Автор написал триллер, <w>перемежающийся</w> историческими фактами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "laced with humor",
                        "laced with irony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемежать или разнообразить чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пронизывать",
                                "target_lang_sense_clarification": "наполнять"
                            },
                            {
                                "target_lang_word": "перемежать",
                                "target_lang_sense_clarification": "чередовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5861514d-940f-303d-9e4a-b5748887ee55",
                    "sense_definition": "To interweave items.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "interweave",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lovers walked through the park and <w>laced</w> their fingers together.",
                            "target_lang_translations": {
                                "ru": "Влюбленные шли по парку и <w>сплели</w> пальцы рук."
                            }
                        },
                        {
                            "text": "Tree branches <w>laced</w> together overhead to form a natural tunnel.",
                            "target_lang_translations": {
                                "ru": "Ветви деревьев <w>переплелись</w> над головой, образовав естественный туннель."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lace fingers",
                        "fingers laced"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переплетать предметы между собой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переплетать",
                                "target_lang_sense_clarification": "соединять"
                            },
                            {
                                "target_lang_word": "сплетать",
                                "target_lang_sense_clarification": "пальцы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef97fdc0-be42-386d-957e-2331e9ccd9fe",
                    "sense_definition": "To adorn with narrow strips or braids of some decorative material.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "decorate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The royal gown was <w>laced</w> with silver thread and pearls.",
                            "target_lang_translations": {
                                "ru": "Королевское платье было <w>отделано</w> серебряной нитью и жемчугом."
                            }
                        },
                        {
                            "text": "She wore a blue velvet jacket <w>laced</w> with gold.",
                            "target_lang_translations": {
                                "ru": "На ней был синий бархатный жакет, <w>расшитый</w> золотом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "laced with gold",
                        "laced with silver"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшать узкими полосками или тесьмой из декоративного материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отделывать",
                                "target_lang_sense_clarification": "украшать"
                            },
                            {
                                "target_lang_word": "расшивать",
                                "target_lang_sense_clarification": "украшать шитьем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "724f9509-abb7-375f-bf93-e8875af7af3b",
                    "sense_definition": "To interweave the spokes of a bicycle wheel.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "interweave",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It takes a lot of patience to <w>lace</w> a bicycle wheel correctly.",
                            "target_lang_translations": {
                                "ru": "Требуется много терпения, чтобы правильно <w>заспицевать</w> велосипедное колесо."
                            }
                        },
                        {
                            "text": "The mechanic taught him how to <w>lace</w> new spokes into the rim.",
                            "target_lang_translations": {
                                "ru": "Механик научил его, как <w>вставлять</w> новые спицы в обод."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Переплетать спицы велосипедного колеса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спицевать",
                                "target_lang_sense_clarification": "колесо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3a6267f-9eff-34ac-8060-8c51a7771c3e",
                    "sense_definition": "To beat; to lash; to make stripes on.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The angry man threatened to <w>lace</w> his coat if he did not leave.",
                            "target_lang_translations": {
                                "ru": "Рассерженный мужчина пригрозил <w>отлупить</w> его, если он не уйдет."
                            }
                        },
                        {
                            "text": "He was soundly <w>laced</w> for his disobedience.",
                            "target_lang_translations": {
                                "ru": "Его изрядно <w>выпороли</w> за непослушание."
                            }

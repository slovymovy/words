                },
                {
                    "sense_id": "7dfb5c96-3f83-370a-8771-16998d921725",
                    "sense_definition": "A surname.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Lucky</w> signed the official documents at the bank this morning.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Лаки</w> подписал официальные документы в банке сегодня утром."
                            }
                        },
                        {
                            "text": "We are going to visit the <w>Lucky</w> family for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Мы собираемся навестить семью <w>Лаки</w> сегодня за ужином."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "used as a last name"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ла́ки",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {

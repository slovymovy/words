            "senses": [
                {
                    "sense_id": "edcbe5ca-3a69-316c-b03c-dc6a0771aaf8",
                    "sense_definition": "W mitologii rzymskiej: jedna z trzech bogiń przeznaczenia, decydująca o długości ludzkiego życia.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mythology",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Rzymska <w>Parka</w> przędła nić życia każdego śmiertelnika.",
                            "target_lang_translations": {
                                "ru": "Римская <w>Парка</w> пряла нить жизни каждого смертного."
                            }
                        },
                        {
                            "text": "Artyści często przedstawiali trzy <w>Parki</w> jako kobiety przy kądzieli.",
                            "target_lang_translations": {
                                "ru": "Художники часто изображали трех <w>Парок</w> как женщин за прялкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trzy Parki"
                    ],
                    "target_lang_definitions": {
                        "ru": "В римской мифологии: одна из трех богинь судьбы, определяющая длину человеческой жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Парка",
                                "target_lang_sense_clarification": "богиня"
                            }

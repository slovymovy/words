                    "sense_id": "6452de8b-c4d1-3a33-a309-82738ea72be9",
                    "sense_definition": "A worldwide online payment system used to make transactions and transfer funds.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "Many online stores accept credit cards and <w>PayPal</w> for payment.",
                            "target_lang_translations": {
                                "ru": "Многие интернет-магазины принимают к оплате кредитные карты и <w>PayPal</w>."
                            }
                        },
                        {
                            "text": "You can link your bank account to <w>PayPal</w> to shop securely.",
                            "target_lang_translations": {
                                "ru": "Вы можете привязать свой банковский счет к <w>PayPal</w> для безопасных покупок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pay via PayPal",
                        "PayPal account",
                        "accept PayPal"
                    ],
                    "target_lang_definitions": {

                        {
                            "trait_type": "regional",
                            "comment": "Turkey and Persia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная мера веса, использовавшаяся в Турции и Персии, масса которой варьировалась в зависимости от региона и времени."
                    },
                    "translations": {
                        "ru": [

                    ],
                    "common_phrases": [
                        "run off",
                        "set off",
                        "drive off",
                        "go off"
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении от говорящего, места или ориентира."
                    },
                    "translations": {
                        "ru": [

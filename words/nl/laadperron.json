    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3f722026-94f2-3357-ae39-a0d59ba054e2",
                    "sense_definition": "Een verhoogd platform aan een gebouw waar vrachtwagens hun lading kunnen lossen en laden",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De chauffeur reed de vrachtwagen voorzichtig achteruit naar het <w>laadperron</w>.",
                            "target_lang_translations": {
                                "ru": "Водитель осторожно подал грузовик задним ходом к <w>погрузочной рампе</w>."
                            }
                        },
                        {
                            "text": "Alle pallets staan al klaar op het <w>laadperron</w> voor transport.",
                            "target_lang_translations": {
                                "ru": "Все поддоны уже стоят готовые к отправке на <w>погрузочной платформе</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "laadkaai",
                        "laad-en-losdok"
                    ],
                    "common_phrases": [
                        "aan het laadperron",
                        "op het laadperron"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приподнятая платформа у здания, предназначенная для погрузки и разгрузки грузовых автомобилей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погрузочная рампа",
                                "target_lang_sense_clarification": "площадка для грузовиков"
                            },
                            {
                                "target_lang_word": "погрузочная платформа",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a19f3afc-d882-3c3f-b337-1fbb9e29ce49",
                    "sense_definition": "Een verhoging langs een spoorweg voor het in- en uitladen van goederenwagons",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De goederentrein werd langs het oude <w>laadperron</w> gerangeerd.",
                            "target_lang_translations": {
                                "ru": "Товарный поезд маневрировал вдоль старой <w>грузовой платформы</w>."
                            }
                        },
                        {
                            "text": "Vanaf het <w>laadperron</w> werden de zware kisten direct de trein in geschoven.",
                            "target_lang_translations": {
                                "ru": "С <w>грузовой платформы</w> тяжелые ящики задвигали прямо в поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "laadkaai"
                    ],
                    "common_phrases": [
                        "langs het laadperron"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвышение вдоль железнодорожных путей для погрузки и разгрузки товарных вагонов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грузовая платформа",
                                "target_lang_sense_clarification": "железнодорожная"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "34f2d303-7240-36a5-9ec6-7b4a23565d27",
                    "sense_definition": "Iets volledig kapotmaken of laten verdwijnen zodat het niet meer bestaat.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De grote brand heeft het historische gebouw volledig <w>vernietigd</w>.",
                            "target_lang_translations": {
                                "en": "The large fire completely <w>destroyed</w> the historic building.",
                                "ru": "Сильный пожар полностью <w>уничтожил</w> историческое здание."
                            }
                        },
                        {
                            "text": "Hij probeerde al het bewijs te <w>vernietigen</w> voordat de politie arriveerde.",
                            "target_lang_translations": {
                                "en": "He tried to <w>destroy</w> all the evidence before the police arrived.",
                                "ru": "Он пытался <w>уничтожить</w> все улики до прибытия полиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "vernielen",
                        "vernielen",
                        "vernielen"
                    ],
                    "antonyms": [
                        "creëren",
                        "bouwen",
                        "scheppen"
                    ],
                    "common_phrases": [
                        "bewijs vernietigen",
                        "volledig vernietigen",
                        "alles vernietigen"
                    ],
                    "target_lang_definitions": {
                        "en": "To completely break something or make it disappear so that it no longer exists.",
                        "ru": "Полностью разрушить что-либо или заставить исчезнуть, чтобы оно перестало существовать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "destroy",
                                "target_lang_sense_clarification": "general meaning"
                            },
                            {
                                "target_lang_word": "annihilate",
                                "target_lang_sense_clarification": "to wipe out completely"
                            },
                            {
                                "target_lang_word": "demolish",
                                "target_lang_sense_clarification": "to tear down buildings or structures"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уничтожать",
                                "target_lang_sense_clarification": "сделать так, чтобы чего-то больше не существовало"
                            },
                            {
                                "target_lang_word": "разрушать",
                                "target_lang_sense_clarification": "ломать или портить структуру чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "031cef28-b610-3416-b736-03da9056908a",
                    "sense_definition": "Officieel verklaren dat een uitspraak, besluit of contract niet meer geldig is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter besloot het eerdere vonnis onmiddellijk te <w>vernietigen</w>.",
                            "target_lang_translations": {
                                "en": "The judge decided to immediately <w>quash</w> the earlier verdict.",
                                "ru": "Судья решил немедленно <w>отменить</w> предыдущий приговор."
                            }
                        },
                        {
                            "text": "Het contract werd <w>vernietigd</w> omdat er sprake was van fraude.",
                            "target_lang_translations": {
                                "en": "The contract was <w>voided</w> because there was evidence of fraud.",
                                "ru": "Контракт был <w>аннулирован</w>, так как имело место мошенничество."
                            }
                        }
                    ],
                    "synonyms": [
                        "vernielen",
                        "vernielen",
                        "vernielen"
                    ],
                    "antonyms": [
                        "bekrachtigen",
                        "bevestigen"
                    ],
                    "common_phrases": [
                        "een vonnis vernietigen",
                        "een besluit vernietigen"
                    ],
                    "target_lang_definitions": {
                        "en": "To officially declare that a ruling, decision, or contract is no longer valid.",
                        "ru": "Официально объявить, что решение, постановление или договор больше не действительны."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "annul",
                                "target_lang_sense_clarification": "to declare invalid"
                            },
                            {
                                "target_lang_word": "quash",
                                "target_lang_sense_clarification": "to reject a legal decision or verdict"
                            },
                            {
                                "target_lang_word": "void",
                                "target_lang_sense_clarification": "to invalidate a contract"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "аннулировать",
                                "target_lang_sense_clarification": "объявлять недействительным (договор, соглашение)"
                            },
                            {
                                "target_lang_word": "отменять",
                                "target_lang_sense_clarification": "признавать недействительным (судебное решение, приговор)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "sweeper",
        "sweeping",
        "sweepings",
        "sweepstake"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c7dbba20-ff78-3984-943e-c82d26bfb113",
                    "sense_definition": "To clean a surface, such as a floor, by brushing away dirt or litter with a broom or brush.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <sweep> the kitchen floor before the guests arrive.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <подмети> пол на кухне до прихода гостей."
                            }
                        },
                        {
                            "text": "He <swept> the broken glass into a dustpan.",
                            "target_lang_translations": {
                                "ru": "Он <смел> разбитое стекло в совок."
                            }
                        }
                    ],
                    "synonyms": [
                        "swing"
                    ],
                    "common_phrases": [
                        "sweep the floor",
                        "sweep up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очищать поверхность (например, пол) от грязи или мусора с помощью веника или щетки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подметать",
                                "target_lang_sense_clarification": "действие в процессе или общее занятие"
                            },
                            {
                                "target_lang_word": "мести",
                                "target_lang_sense_clarification": "процесс уборки метлой"
                            },
                            {
                                "target_lang_word": "подмести",
                                "target_lang_sense_clarification": "завершенное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fd10952-6a64-39f4-92ac-0dbf30800745",
                    "sense_definition": "To move, push, or remove someone or something quickly and with great force, often like a wave or wind.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "forceful_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The massive floodwaters <swept> away the old wooden bridge.",
                            "target_lang_translations": {
                                "ru": "Мощные потоки воды <снесли> старый деревянный мост."
                            }
                        },
                        {
                            "text": "She was angry and <swept> the papers off the desk.",
                            "target_lang_translations": {
                                "ru": "Она разозлилась и <смела> бумаги со стола."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sweep away",
                        "sweep aside"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро и с силой перемещать, толкать или удалять кого-либо или что-либо, часто подобно волне или ветру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сметать",
                                "target_lang_sense_clarification": "удалять с поверхности резким движением"
                            },
                            {
                                "target_lang_word": "сносить",
                                "target_lang_sense_clarification": "уносить прочь силой (о ветре или воде)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9f6abb4-ae5e-3869-bfb4-38cfae43d46a",
                    "sense_definition": "To move smoothly and quickly, often in a curve, arc, or extending over a large area.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "smooth_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A sudden feeling of relief <swept> over him when he heard the news.",
                            "target_lang_translations": {
                                "ru": "Внезапное чувство облегчения <охватило> его, когда он услышал новости."
                            }
                        },
                        {
                            "text": "The large black car <swept> around the corner and stopped.",
                            "target_lang_translations": {
                                "ru": "Большая черная машина <пронеслась> за поворот и остановилась."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sweep across",
                        "sweep through",
                        "sweep over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться плавно и быстро, часто по дуге или охватывая большую территорию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проноситься",
                                "target_lang_sense_clarification": "быстро двигаться мимо или сквозь"
                            },
                            {
                                "target_lang_word": "охватывать",
                                "target_lang_sense_clarification": "распространяться на кого-то (о чувствах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af2dea5d-ddb8-310c-8a75-d8433d437a07",
                    "sense_definition": "To search a place thoroughly and methodically, often looking for illegal items or people.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "search_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Police officers <swept> the building looking for the suspect.",
                            "target_lang_translations": {
                                "ru": "Полицейские <прочесали> здание в поисках подозреваемого."
                            }
                        },
                        {
                            "text": "Security teams <swept> the room for listening devices before the meeting.",
                            "target_lang_translations": {
                                "ru": "Служба безопасности <обследовала> комнату на предмет подслушивающих устройств перед встречей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sweep the area",
                        "sweep the room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщательно и методично обыскивать место, часто в поисках незаконных предметов или людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прочёсывать",
                                "target_lang_sense_clarification": "тщательно обследовать местность"
                            },
                            {
                                "target_lang_word": "обыскивать",
                                "target_lang_sense_clarification": "искать что-либо в помещении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c858b48-1742-3647-9c0d-130b2dbe4d69",
                    "sense_definition": "To win all the games or contests in a series against the same opponent.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports_victory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The home team <swept> the series by winning all four games.",
                            "target_lang_translations": {
                                "ru": "Хозяева поля <выиграли всухую> серию, победив во всех четырех играх."
                            }
                        },
                        {
                            "text": "They hope to <sweep> the finals and take the championship trophy.",
                            "target_lang_translations": {
                                "ru": "Они надеются <выиграть всухую> в финале и забрать чемпионский кубок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sweep the series",
                        "sweep the board"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выиграть все игры или состязания в серии против одного и того же противника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выигрывать всухую",
                                "target_lang_sense_clarification": "одержать полную победу во всех матчах серии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2492b80f-e4fc-3259-8ade-302da0660d50",
                    "sense_definition": "To look over a wide area quickly or methodically.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "visual_scan",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <swept> the horizon with his binoculars but saw no ships.",
                            "target_lang_translations": {
                                "ru": "Он <окинул взглядом> горизонт в бинокль, но не увидел кораблей."
                            }
                        },
                        {
                            "text": "Her eyes <swept> the room to see if anyone was watching.",
                            "target_lang_translations": {
                                "ru": "Она <обвела взглядом> комнату, чтобы увидеть, смотрит ли кто-нибудь."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро или методично осматривать широкое пространство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окидывать взглядом",
                                "target_lang_sense_clarification": "быстро посмотреть на широкое пространство"
                            },
                            {
                                "target_lang_word": "обводить взглядом",
                                "target_lang_sense_clarification": "осматривать, перемещая взгляд"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "512e08b7-e240-31c6-8922-ae9e2313b5ad",
                    "sense_definition": "An act of cleaning a surface using a broom or brush.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cleaning_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Give the front steps a quick <sweep> to remove the leaves.",
                            "target_lang_translations": {
                                "ru": "Быстро <подмети> ступеньки крыльца, чтобы убрать листья."
                            }
                        },
                        {
                            "text": "The room needs a good <sweep> before we can paint it.",
                            "target_lang_translations": {
                                "ru": "Комнату нужно хорошенько <подмести>, прежде чем мы сможем ее покрасить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clean sweep",
                        "give it a sweep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по уборке поверхности с помощью веника или щетки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подметание",
                                "target_lang_sense_clarification": "процесс уборки"
                            },
                            {
                                "target_lang_word": "уборка",
                                "target_lang_sense_clarification": "общее название действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47e1c714-75fd-3648-81bb-335f93a13a6f",
                    "sense_definition": "A thorough and methodical search of an area.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "search_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers conducted a <sweep> of the village at dawn.",
                            "target_lang_translations": {
                                "ru": "Солдаты провели <зачистку> деревни на рассвете."
                            }
                        },
                        {
                            "text": "A security <sweep> was performed before the president arrived.",
                            "target_lang_translations": {
                                "ru": "Перед приездом президента был проведен <осмотр> службой безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "security sweep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщательный и методичный обыск местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачистка",
                                "target_lang_sense_clarification": "военный или полицейский термин"
                            },
                            {
                                "target_lang_word": "прочёсывание",
                                "target_lang_sense_clarification": "тщательный поиск на местности"
                            },
                            {
                                "target_lang_word": "осмотр",
                                "target_lang_sense_clarification": "проверка безопасности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa18798f-f196-386d-b432-3e258a4c918c",
                    "sense_definition": "A long, smooth, curving movement, line, or shape.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "smooth_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With a grand <sweep> of his arm, he welcomed us to the hall.",
                            "target_lang_translations": {
                                "ru": "Широким <взмахом> руки он пригласил нас в зал."
                            }
                        },
                        {
                            "text": "We admired the broad <sweep> of the bay from the top of the hill.",
                            "target_lang_translations": {
                                "ru": "Мы любовались широким <изгибом> залива с вершины холма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "broad sweep",
                        "wide sweep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинное, плавное, изогнутое движение, линия или форма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взмах",
                                "target_lang_sense_clarification": "широкое движение рукой"
                            },
                            {
                                "target_lang_word": "изгиб",
                                "target_lang_sense_clarification": "плавная кривая линия ландшафта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "650fc720-7174-3ea5-8086-50e7a4e43eab",
                    "sense_definition": "A person whose job is to clean soot from inside chimneys.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <sweep> arrived early in the morning with his brushes and rods.",
                            "target_lang_translations": {
                                "ru": "<Трубочист> приехал рано утром со своими щетками и прутьями."
                            }
                        },
                        {
                            "text": "His face was black with soot, looking just like a chimney <sweep>.",
                            "target_lang_translations": {
                                "ru": "Его лицо было черным от сажи, совсем как у <трубочиста>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chimney sweep"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "modern usage typically refers to the full term 'chimney sweep'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, чья работа заключается в очистке дымоходов от сажи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трубочист",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
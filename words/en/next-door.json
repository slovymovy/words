    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7c279c89-ffe2-3d08-80a4-c68a5043120d",
                    "sense_definition": "Located in the nearest building, apartment, or room, especially immediately to the left or right.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "proximity_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>next-door</w> neighbor is very friendly and helpful.",
                            "target_lang_translations": {
                                "ru": "Мой сосед <w>из соседней квартиры</w> очень дружелюбный и отзывчивый."
                            }
                        },
                        {
                            "text": "We can hear the television playing in the <w>next-door</w> apartment.",
                            "target_lang_translations": {
                                "ru": "Мы слышим, как работает телевизор в <w>соседней</w> квартире."
                            }
                        }
                    ],
                    "synonyms": [
                        "neighboring",
                        "adjacent"
                    ],
                    "antonyms": [
                        "distant"
                    ],
                    "common_phrases": [
                        "next-door neighbor",
                        "the girl next-door"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в ближайшем здании, квартире или комнате, особенно непосредственно слева или справа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соседний",
                                "target_lang_sense_clarification": "находящийся рядом (о помещении, здании)"
                            },
                            {
                                "target_lang_word": "живущий по соседству",
                                "target_lang_sense_clarification": "о человеке (например, сосед)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c3a7c275-a1c7-3585-8b5e-1265275f09cf",
                    "sense_definition": "In or into the adjacent building or room.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "proximity_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She lives <w>next-door</w> to my parents.",
                            "target_lang_translations": {
                                "ru": "Она живет <w>по соседству</w> с моими родителями."
                            }
                        },
                        {
                            "text": "They went <w>next-door</w> to borrow some sugar.",
                            "target_lang_translations": {
                                "ru": "Они пошли <w>к соседям</w>, чтобы одолжить сахара."
                            }
                        }
                    ],
                    "synonyms": [
                        "nearby"
                    ],
                    "common_phrases": [
                        "live next-door",
                        "go next-door"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often spelled as 'next door' without a hyphen when used as an adverb"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В соседнем здании или комнате; по соседству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по соседству",
                                "target_lang_sense_clarification": "находящийся или живущий рядом"
                            },
                            {
                                "target_lang_word": "рядом",
                                "target_lang_sense_clarification": "в непосредственной близости"
                            },
                            {
                                "target_lang_word": "к соседям",
                                "target_lang_sense_clarification": "направление движения (пойти к соседям)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "target_lang_definitions": {
                        "ru": "Прозвище, часто для имени Чарльз или Кристофер, или для сына, названного в честь отца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Чип",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "355cab57-9a4e-3d3b-9dcf-de9719fb2d28",
                    "sense_definition": "Present participle and gerund of <w>speak</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Who is that <w>speaking</w> on the phone right now?",
                            "target_lang_translations": {
                                "ru": "Кто это там сейчас <w>говорит</w> по телефону?"
                            }
                        },
                        {
                            "text": "Generally <w>speaking</w>, the weather in this region is quite mild.",
                            "target_lang_translations": {
                                "ru": "Вообще <w>говоря</w>, погода в этом регионе довольно мягкая."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола speak."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говорящий",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "говоря",
                                "target_lang_sense_clarification": "деепричастие/герундий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c2d5155-8af5-398e-bfaa-132863af28bd",
                    "sense_definition": "The skill or ability to communicate vocally in a particular language.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I can read French well, but my <w>speaking</w> needs a lot of practice.",
                            "target_lang_translations": {
                                "ru": "Я хорошо читаю по-французски, но моей <w>устной речи</w> требуется много практики."
                            }
                        },
                        {
                            "text": "The course focuses on listening and <w>speaking</w> skills rather than grammar.",
                            "target_lang_translations": {
                                "ru": "Курс сосредоточен на навыках аудирования и <w>говорения</w>, а не на грамматике."
                            }
                        }
                    ],
                    "common_phrases": [
                        "speaking skills",
                        "listening and speaking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Навык или способность общаться устно на определенном языке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говорение",
                                "target_lang_sense_clarification": "навык"
                            },
                            {
                                "target_lang_word": "устная речь",
                                "target_lang_sense_clarification": "способность говорить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4fd41de-6c8b-3689-8b88-fe109e4f5460",
                    "sense_definition": "The act of delivering a speech or talking to an audience.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Public <w>speaking</w> makes many people feel nervous and uncomfortable.",
                            "target_lang_translations": {
                                "ru": "<w>Публичные выступления</w> заставляют многих людей нервничать и чувствовать себя некомфортно."
                            }
                        },
                        {
                            "text": "His <w>speaking</w> at the event was interrupted by loud applause.",
                            "target_lang_translations": {
                                "ru": "Его <w>выступление</w> на мероприятии было прервано громкими аплодисментами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "public speaking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс выступления с речью или обращения к аудитории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выступление",
                                "target_lang_sense_clarification": "публичное"
                            },
                            {
                                "target_lang_word": "речь",
                                "target_lang_sense_clarification": "процесс говорения перед публикой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "22b1339c-1f19-3e46-8085-60659ac23c28",
                    "sense_definition": "Able to speak a specific language or having the power of speech.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "competence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to hire a native English-<w>speaking</w> guide for the tour.",
                            "target_lang_translations": {
                                "ru": "Нам нужно нанять гида, <w>говорящего</w> на родном английском, для этой экскурсии."
                            }
                        },
                        {
                            "text": "The toy store sells a <w>speaking</w> doll that repeats what you say.",
                            "target_lang_translations": {
                                "ru": "В магазине игрушек продается <w>говорящая</w> кукла, которая повторяет то, что вы говорите."
                            }
                        }
                    ],
                    "common_phrases": [
                        "English-speaking",
                        "native speaking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный говорить на определенном языке или обладающий даром речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говорящий",
                                "target_lang_sense_clarification": "владеющий языком или способный говорить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33a5c88d-72e1-30bf-9640-b5d70d378667",
                    "sense_definition": "Relating to or used in ordinary speech rather than singing.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "voice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a very low and pleasant <w>speaking</w> voice.",
                            "target_lang_translations": {
                                "ru": "У нее очень низкий и приятный <w>разговорный</w> голос."
                            }
                        },
                        {
                            "text": "The actor changed his normal <w>speaking</w> style to fit the character.",
                            "target_lang_translations": {
                                "ru": "Актер изменил свою обычную <w>разговорную</w> манеру, чтобы соответствовать персонажу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "speaking voice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к обычной речи или используемый в ней, в отличие от пения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разговорный",
                                "target_lang_sense_clarification": "о голосе или стиле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa509714-bd99-345f-8eea-611f3644dc53",
                    "sense_definition": "Involving the use of spoken words, especially in a performance.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After years as an extra, he finally got a <w>speaking</w> part in a movie.",
                            "target_lang_translations": {
                                "ru": "После многих лет работы в массовке он наконец получил роль <w>со словами</w> в кино."
                            }
                        },
                        {
                            "text": "The president has a <w>speaking</w> engagement at the university next week.",
                            "target_lang_translations": {
                                "ru": "У президента запланировано <w>выступление</w> в университете на следующей неделе."
                            }
                        }
                    ],
                    "antonyms": [
                        "nonspeaking"
                    ],
                    "common_phrases": [
                        "speaking role",
                        "speaking part"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предполагающий использование устной речи, особенно в актерской игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "со словами",
                                "target_lang_sense_clarification": "о роли"
                            },
                            {
                                "target_lang_word": "с речью",
                                "target_lang_sense_clarification": "о выступлении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dea42338-b155-32eb-ba44-4c21dc374f54",
                    "sense_definition": "Highly expressive or eloquent; conveying meaning clearly without words.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "expressive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She gave him a <w>speaking</w> look that told him exactly how she felt.",
                            "target_lang_translations": {
                                "ru": "Она бросила на него <w>говорящий</w> взгляд, который точно сказал ему, что она чувствует."
                            }
                        },
                        {
                            "text": "The portrait was so <w>speaking</w> that it felt like the subject was in the room.",
                            "target_lang_translations": {
                                "ru": "Портрет был настолько <w>выразительным</w>, что казалось, будто изображенный находится в комнате."
                            }
                        }
                    ],
                    "antonyms": [
                        "unspeaking"
                    ],
                    "common_phrases": [
                        "speaking glance",
                        "speaking look"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень выразительный или красноречивый; передающий смысл без слов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говорящий",
                                "target_lang_sense_clarification": "выразительный"
                            },
                            {
                                "target_lang_word": "выразительный",
                                "target_lang_sense_clarification": "красноречивый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "678cbb15-1986-3908-8356-a48060998b4e",
                    "sense_definition": "Used on the telephone to confirm that the person requested is the one talking.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is this Mr. Jones? — Yes, <w>speaking</w>.",
                            "target_lang_translations": {
                                "ru": "Это мистер Джонс? — Да, <w>слушаю</w>."
                            }
                        },
                        {
                            "text": "Hello, this is the manager <w>speaking</w>; how can I help you?",
                            "target_lang_translations": {
                                "ru": "Алло, <w>говорит</w> менеджер; чем я могу вам помочь?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "this is X speaking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется по телефону для подтверждения того, что говорит именно тот, кого спрашивали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слушаю",
                                "target_lang_sense_clarification": "ответ по телефону"
                            },
                            {
                                "target_lang_word": "у аппарата",
                                "target_lang_sense_clarification": "ответ по телефону"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
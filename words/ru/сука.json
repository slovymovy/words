                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "грубое, бранное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A vile, worthless person; a scoundrel (used for any gender)."

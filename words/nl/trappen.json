        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b89f06d6-2759-3374-8bb4-10523ec2d483",
                    "sense_definition": "Met de voet een harde stoot tegen iets of iemand geven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action_kick",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De voetballer <trapte> de bal met kracht in het doel.",
                            "target_lang_translations": {
                                "en": "The football player <kicked> the ball forcefully into the goal.",
                                "ru": "Футболист с силой <пнул> мяч в ворота."
                            }
                        },
                        {
                            "text": "Uit frustratie <trapte> hij hard tegen de dichte deur.",
                            "target_lang_translations": {
                                "en": "Out of frustration, he <kicked> hard against the closed door.",
                                "ru": "От разочарования он сильно <ударил ногой> в закрытую дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "schoppen"
                    ],
                    "common_phrases": [
                        "een balletje trappen",
                        "de deur intrappen",
                        "tegen iets trappen"
                    ],
                    "target_lang_definitions": {
                        "en": "To give a hard blow with the foot against something or someone.",
                        "ru": "Ударять ногой по чему-либо или кому-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kick",
                                "target_lang_sense_clarification": "strike with the foot"

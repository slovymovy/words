                        "binnen de zestien"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "voetbaltaal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Штрафная площадь на футбольном поле, простирающаяся примерно на 16 метров от ворот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штрафная",
                                "target_lang_sense_clarification": "футбольный термин"

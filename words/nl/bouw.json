    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aa92339f-f44e-3767-b31d-e29faf718d8c",
                    "sense_definition": "Het proces van het maken of oprichten van huizen, constructies of andere werken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "construction_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>bouw</w> van het nieuwe ziekenhuis duurde langer dan verwacht.",
                            "target_lang_translations": {
                                "ru": "<w>Строительство</w> новой больницы длилось дольше, чем ожидалось."
                            }
                        },
                        {
                            "text": "Dit huis is nog in <w>bouw</w> en wordt volgend jaar opgeleverd.",
                            "target_lang_translations": {
                                "ru": "Этот дом еще находится в стадии <w>строительства</w> и будет сдан в следующем году."
                            }
                        }
                    ],
                    "synonyms": [
                        "constructie",
                        "opbouw"
                    ],
                    "antonyms": [
                        "sloop",
                        "afbraak"
                    ],
                    "common_phrases": [
                        "in aanbouw",
                        "tijdens de bouw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс создания или возведения домов, сооружений или других объектов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строительство",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "стройка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bd329ae-846d-35cf-be16-f6c312dc7c42",
                    "sense_definition": "De economische sector die zich bezighoudt met het bouwen van gebouwen en infrastructuur.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "construction_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buurman werkt al jaren in de <w>bouw</w> als timmerman.",
                            "target_lang_translations": {
                                "ru": "Мой сосед уже много лет работает на <w>стройке</w> плотником."
                            }
                        },
                        {
                            "text": "Door de economische crisis gaat het slecht met de <w>bouw</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за экономического кризиса дела в <w>строительстве</w> идут плохо."
                            }
                        }
                    ],
                    "synonyms": [
                        "bouwnijverheid",
                        "bouwsector"
                    ],
                    "common_phrases": [
                        "in de bouw werken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Экономический сектор, занимающийся возведением зданий и инфраструктуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строительство",
                                "target_lang_sense_clarification": "отрасль"
                            },
                            {
                                "target_lang_word": "строительная сфера",
                                "target_lang_sense_clarification": "индустрия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95190947-883d-3171-9bcf-6c6cb51eeb7c",
                    "sense_definition": "De manier waarop iets of iemand gevormd of samengesteld is; de structuur of lichaamsvorm.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De atleet heeft een stevige <w>bouw</w> door jarenlange training.",
                            "target_lang_translations": {
                                "ru": "У атлета крепкое <w>телосложение</w> благодаря многолетним тренировкам."
                            }
                        },
                        {
                            "text": "De <w>bouw</w> van deze oude kerk is uniek voor de regio.",
                            "target_lang_translations": {
                                "ru": "<w>Архитектура</w> этой старой церкви уникальна для региона."
                            }
                        }
                    ],
                    "synonyms": [
                        "structuur",
                        "lichaamsbouw",
                        "gestalte",
                        "constructie"
                    ],
                    "common_phrases": [
                        "van stevige bouw"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, как что-то или кто-то сформирован или составлен; структура или форма тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телосложение",
                                "target_lang_sense_clarification": "человека"
                            },
                            {
                                "target_lang_word": "строение",
                                "target_lang_sense_clarification": "структура"
                            },
                            {
                                "target_lang_word": "конструкция",
                                "target_lang_sense_clarification": "устройство"

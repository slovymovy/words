                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжать кого-либо деньгами для начала бизнеса или участия в каком-либо деле."
                    },
                    "translations": {

            "senses": [
                {
                    "sense_id": "20472663-d3cb-33b5-9af6-b6379aba7f68",
                    "sense_definition": "Comparative form of <w>buff</w>: more muscular or physically fit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He looks much <w>buffer</w> now that he has been going to the gym every day.",
                            "target_lang_translations": {
                                "ru": "Он выглядит гораздо <w>более накачанным</w> теперь, когда ходит в спортзал каждый день."
                            }
                        },
                        {
                            "text": "I want to get <w>buffer</w> before the summer starts.",
                            "target_lang_translations": {
                                "ru": "Я хочу стать <w>более мускулистым</w> до начала лета."
                            }
                        }
                    ],
                    "synonyms": [

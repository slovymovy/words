                    "frequency": "High",
                    "semantic_group_id": "logistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The large ship arrived at the port to unload its <w>cargo</w>.",
                            "target_lang_translations": {
                                "ru": "Большой корабль прибыл в порт, чтобы выгрузить свой <w>груз</w>."
                            }
                        },
                        {
                            "text": "Security officers inspected the <w>cargo</w> before it was loaded onto the plane.",
                            "target_lang_translations": {
                                "ru": "Сотрудники службы безопасности досмотрели <w>груз</w> перед его погрузкой в самолет."
                            }
                        }
                    ],
                    "synonyms": [
                        "freight",
                        "load",
                        "shipment",
                        "haul",
                        "goods"
                    ],
                    "common_phrases": [
                        "cargo ship",
                        "cargo plane",
                        "cargo hold",
                        "precious cargo",
                        "handle cargo"
                    ],
                    "target_lang_definitions": {

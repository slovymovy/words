{
    "word_family": [
        "initiation",
        "initiator",
        "initiative",
        "initial",
        "initially",
        "uninitiated"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c366a662-e76d-3646-868e-b34e9ef6d4e2",
                    "sense_definition": "To cause a process, action, or event to begin.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government plans to <w>initiate</w> a new program to support small businesses.",
                            "target_lang_translations": {
                                "ru": "Правительство планирует <w>инициировать</w> новую программу поддержки малого бизнеса."
                            }
                        },
                        {
                            "text": "They decided to <w>initiate</w> legal proceedings against the company for breach of contract.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>возбудить</w> судебное дело против компании за нарушение контракта."
                            }
                        }
                    ],
                    "synonyms": [
                        "commence",
                        "institute"
                    ],
                    "antonyms": [
                        "stop",
                        "end",
                        "cease",
                        "finish",
                        "conclude",
                        "halt",
                        "suspend",
                        "discontinue",
                        "complete",
                        "pause"
                    ],
                    "common_phrases": [
                        "initiate a discussion",
                        "initiate a process",
                        "initiate change",
                        "initiate an investigation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызвать начало процесса, действия или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инициировать",
                                "target_lang_sense_clarification": "официально или формально"
                            },
                            {
                                "target_lang_word": "начать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "положить начало",
                                "target_lang_sense_clarification": "фразеологизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10a8352a-e2a8-30ad-a229-835fce014dc0",
                    "sense_definition": "To admit someone into a group, organization, or secret society, often through a special ceremony.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_membership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fraternity <w>initiates</w> new members at the beginning of every semester.",
                            "target_lang_translations": {
                                "ru": "Братство <w>посвящает</w> новых членов в начале каждого семестра."
                            }
                        },
                        {
                            "text": "He was <w>initiated</w> into the secret order after passing a series of difficult tests.",
                            "target_lang_translations": {
                                "ru": "Он был <w>посвящён</w> в тайный орден после прохождения серии сложных испытаний."
                            }
                        }
                    ],
                    "common_phrases": [
                        "initiate into a cult",
                        "initiate a new member",
                        "initiate into the tribe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять кого-либо в группу, организацию или тайное общество, часто посредством специальной церемонии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посвящать",
                                "target_lang_sense_clarification": "в тайну или общество"
                            },
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "в состав группы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41d8db02-a173-3d25-ab29-69b5a491d1b5",
                    "sense_definition": "To introduce someone to a new skill, subject, or area of knowledge.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor <w>initiated</w> his students in the complex theories of quantum physics.",
                            "target_lang_translations": {
                                "ru": "Профессор <w>познакомил</w> своих студентов со сложными теориями квантовой физики."
                            }
                        },
                        {
                            "text": "She <w>initiated</w> him into the world of classical music during their travels.",
                            "target_lang_translations": {
                                "ru": "Она <w>приобщила</w> его к миру классической музыки во время их путешествий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "initiate in",
                        "initiate into the mysteries"
                    ],
                    "target_lang_definitions": {
                        "ru": "Познакомить кого-либо с новым навыком, предметом или областью знаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приобщать",
                                "target_lang_sense_clarification": "к культуре или знаниям"
                            },
                            {
                                "target_lang_word": "знакомить",
                                "target_lang_sense_clarification": "с чем-либо новым"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f675bdd7-8ceb-395a-adce-6e7c40d17458",
                    "sense_definition": "A person who has recently been admitted to a group or organization, often having completed a ceremony.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_membership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>initiates</w> stood silently in the hall, waiting for the ceremony to begin.",
                            "target_lang_translations": {
                                "ru": "<w>Неофиты</w> молча стояли в зале, ожидая начала церемонии."
                            }
                        },
                        {
                            "text": "As a new <w>initiate</w>, she was expected to learn the history of the society.",
                            "target_lang_translations": {
                                "ru": "Как <w>новопосвящённая</w>, она должна была изучить историю общества."
                            }
                        }
                    ],
                    "antonyms": [
                        "noninitiate#Noun"
                    ],
                    "common_phrases": [
                        "new initiate",
                        "cult initiate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, недавно принятый в группу или организацию, часто прошедший специальную церемонию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неофит",
                                "target_lang_sense_clarification": "новый приверженец"
                            },
                            {
                                "target_lang_word": "новопосвящённый",
                                "target_lang_sense_clarification": "недавно принятый член"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1e78579-8dd5-38a9-9625-bae256727a46",
                    "sense_definition": "A person who has special knowledge or expertise in a specific subject.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This technical manual is written for <w>initiates</w>, not for the general public.",
                            "target_lang_translations": {
                                "ru": "Это техническое руководство написано для <w>посвящённых</w>, а не для широкой публики."
                            }
                        },
                        {
                            "text": "Only <w>initiates</w> of the art form can truly appreciate the subtle details of the performance.",
                            "target_lang_translations": {
                                "ru": "Только <w>посвящённые</w> в этот вид искусства могут по-настоящему оценить тонкие детали представления."
                            }
                        }
                    ],
                    "antonyms": [
                        "novice#Noun"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обладающий особыми знаниями или опытом в определенной области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посвящённый",
                                "target_lang_sense_clarification": "обладающий тайным знанием"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7ef1c504-c0c8-3b31-92e5-d4b18026882d",
                    "sense_definition": "Having been introduced to or instructed in a particular subject, secret, or mystery.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>initiate</w> in the ancient traditions of his people.",
                            "target_lang_translations": {
                                "ru": "Он <w>посвящён</w> в древние традиции своего народа."
                            }
                        },
                        {
                            "text": "Those who are <w>initiate</w> know that the symbol has a deeper meaning.",
                            "target_lang_translations": {
                                "ru": "Те, кто <w>посвящён</w>, знают, что этот символ имеет более глубокое значение."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly found in literary or older contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ознакомленный с определенным предметом, тайной или учением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посвящённый",
                                "target_lang_sense_clarification": "знающий тайны"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
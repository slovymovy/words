    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "51b7f2fb-ba4f-34f5-93e3-a4d7bfa915e4",
                    "sense_definition": "Carrying or equipped with weapons, especially guns.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "weapons",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police warned the public that the suspect was <w>armed</w> and dangerous.",
                            "target_lang_translations": {
                                "ru": "Полиция предупредила общественность, что подозреваемый <w>вооружён</w> и опасен."
                            }
                        },
                        {
                            "text": "Several <w>armed</w> guards were stationed at the entrance to the bank.",
                            "target_lang_translations": {
                                "ru": "Несколько <w>вооружённых</w> охранников стояли у входа в банк."
                            }
                        }
                    ],
                    "synonyms": [
                        "equipped"
                    ],
                    "common_phrases": [
                        "armed forces",
                        "armed conflict",
                        "armed robbery",
                        "heavily armed",
                        "armed guard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий при себе оружие или оснащённый им."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вооружённый",
                                "target_lang_sense_clarification": "имеющий оружие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0ffe406-1258-32ca-a159-e07c57434274",
                    "sense_definition": "Having physical arms (limbs), often of a specified number or type.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "body_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The octopus is a famous eight-<w>armed</w> sea creature.",
                            "target_lang_translations": {
                                "ru": "Осьминог — известное восьми<w>рукое</w> морское существо."
                            }
                        },
                        {
                            "text": "A one-<w>armed</w> man sat quietly in the corner of the room.",
                            "target_lang_translations": {
                                "ru": "Одно<w>рукий</w> мужчина тихо сидел в углу комнаты."
                            }
                        }
                    ],
                    "antonyms": [
                        "armless"
                    ],
                    "common_phrases": [
                        "one-armed",
                        "long-armed",
                        "strong-armed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий руки (конечности), часто определённого количества или типа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "-рукий",
                                "target_lang_sense_clarification": "используется в сложных словах (напр., однорукий)"
                            },
                            {
                                "target_lang_word": "имеющий руки",
                                "target_lang_sense_clarification": "о наличии конечностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdd7cced-9ae0-32f6-a4dd-119e7273c7d0",
                    "sense_definition": "Activated and ready to be triggered, such as an alarm system or an explosive device.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure the security system is <w>armed</w> before you leave the house.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что система безопасности <w>включена</w>, прежде чем выходить из дома."
                            }
                        },
                        {
                            "text": "The bomb squad carefully approached the <w>armed</w> device.",
                            "target_lang_translations": {
                                "ru": "Сапёры осторожно приблизились к <w>взведённому</w> устройству."
                            }
                        }
                    ],
                    "synonyms": [
                        "active",
                        "live"
                    ],
                    "common_phrases": [
                        "armed system",
                        "fully armed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Активированный и готовый к срабатыванию (о сигнализации или взрывном устройстве)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взведённый",
                                "target_lang_sense_clarification": "о механизме или бомбе"
                            },
                            {
                                "target_lang_word": "включённый",
                                "target_lang_sense_clarification": "о сигнализации"
                            },
                            {
                                "target_lang_word": "снаряжённый",
                                "target_lang_sense_clarification": "подготовленный к использованию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "defb423b-71a5-38de-89f7-0918a0fb5dc3",
                    "sense_definition": "Provided with something that gives strength, protection, or an advantage.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She arrived at the meeting <w>armed</w> with all the necessary data to prove her point.",
                            "target_lang_translations": {
                                "ru": "Она пришла на встречу, <w>вооружённая</w> всеми необходимыми данными, чтобы доказать свою точку зрения."
                            }
                        },
                        {
                            "text": "The lawyer came <w>armed</w> with evidence to defend his client.",
                            "target_lang_translations": {
                                "ru": "Адвокат пришёл, <w>вооружённый</w> уликами для защиты своего клиента."
                            }
                        }
                    ],
                    "synonyms": [
                        "furnished",
                        "fortified"
                    ],
                    "common_phrases": [
                        "armed with facts",
                        "armed with knowledge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжённый чем-либо, дающим силу, защиту или преимущество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вооружённый",
                                "target_lang_sense_clarification": "в переносном смысле (знаниями, фактами)"
                            },
                            {
                                "target_lang_word": "оснащённый",
                                "target_lang_sense_clarification": "снабжённый необходимым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5e9aff7-823f-3f1d-9370-c78c68818fdf",
                    "sense_definition": "Having thorns, prickles, or sharp protective parts.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>armed</w> stems of the rose bush discourage animals from eating it.",
                            "target_lang_translations": {
                                "ru": "<w>Колючие</w> стебли розового куста отбивают у животных желание его есть."
                            }
                        },
                        {
                            "text": "Some plants have <w>armed</w> leaves that can cause injury if touched.",
                            "target_lang_translations": {
                                "ru": "У некоторых растений есть <w>шиповатые</w> листья, которые могут поранить при прикосновении."
                            }
                        }
                    ],
                    "synonyms": [
                        "thorny",
                        "prickly"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "botany"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий шипы, колючки или острые защитные части (в биологии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колючий",
                                "target_lang_sense_clarification": "имеющий колючки"
                            },
                            {
                                "target_lang_word": "шиповатый",
                                "target_lang_sense_clarification": "покрытый шипами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "952210e6-9873-30d8-a989-e18a9a149f54",
                    "sense_definition": "Having claws, horns, teeth, or a beak depicted in a different color than the body.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "heraldry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family crest features a lion <w>armed</w> and langued in gold.",
                            "target_lang_translations": {
                                "ru": "На фамильном гербе изображён лев, <w>вооружённый</w> и с языком золотого цвета."
                            }
                        },
                        {
                            "text": "The shield displays an eagle <w>armed</w> gules on a silver background.",
                            "target_lang_translations": {
                                "ru": "На щите изображён орёл, <w>вооружённый</w> червленью, на серебряном фоне."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "heraldry"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий когти, рога, зубы или клюв другого цвета, чем тело (в геральдике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вооружённый",
                                "target_lang_sense_clarification": "геральдический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ed55b0ab-bedb-38c9-b6fa-0f923bc90bc8",
                    "sense_definition": "Simple past and past participle of <w>arm</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government <w>armed</w> the soldiers with new rifles before the mission.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>вооружило</w> солдат новыми винтовками перед заданием."
                            }
                        },
                        {
                            "text": "She <w>armed</w> herself with patience before entering the noisy classroom.",
                            "target_lang_translations": {
                                "ru": "Она <w>вооружилась</w> терпением перед тем, как войти в шумный класс."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола arm (вооружать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вооружил",
                                "target_lang_sense_clarification": "снабдил оружием"
                            },
                            {
                                "target_lang_word": "вооружился",
                                "target_lang_sense_clarification": "взял оружие или подготовился"
                            }
                        ]
                    }
                }
            ]
        }
    ]

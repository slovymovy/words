            "pos": "verb",
            "senses": [
                {
                    "sense_id": "05665ea7-1221-3a05-9eca-cfa6fa987a68",
                    "sense_definition": "Zwiększać swoje rozmiary, ilość, wartość lub intensywność.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "change_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ceny paliwa na stacjach benzynowych znowu <w>rosną</w>.",
                            "target_lang_translations": {
                                "ru": "Цены на топливо на заправках снова <w>растут</w>."
                            }
                        },
                        {
                            "text": "Zainteresowanie nauką języka polskiego ciągle <w>rośnie</w>.",
                            "target_lang_translations": {
                                "ru": "Интерес к изучению польского языка постоянно <w>растет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzrastać",
                        "narastać",
                        "urastać"
                    ],
                    "antonyms": [
                        "zmniejszać się"
                    ],
                    "common_phrases": [
                        "ceny rosną",
                        "napięcie rośnie",
                        "rosnąć w siłę",
                        "szybko rosnąć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличиваться в размерах, количестве, стоимости или интенсивности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расти",
                                "target_lang_sense_clarification": "увеличиваться (о ценах, показателях)"
                            },
                            {
                                "target_lang_word": "возрастать",
                                "target_lang_sense_clarification": "усиливаться (об интересе, напряжении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cb31297-b8fa-371b-87fc-152d20dd95f9",
                    "sense_definition": "Stawać się większym i starszym w procesie rozwoju (o ludziach, zwierzętach i roślinach).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "biological_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój syn <w>rośnie</w> tak szybko, że niedługo będzie wyższy od ciebie.",
                            "target_lang_translations": {
                                "ru": "Твой сын <w>растет</w> так быстро, что скоро будет выше тебя."
                            }
                        },
                        {
                            "text": "Martwię się, bo moje kwiaty na balkonie wcale nie chcą <w>rosnąć</w>.",
                            "target_lang_translations": {
                                "ru": "Я волнуюсь, потому что мои цветы на балконе совсем не хотят <w>расти</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dorastać",
                        "podrastać"
                    ],
                    "common_phrases": [
                        "rosnąć jak na drożdżach",
                        "dzieci rosną",
                        "zdrowo rosnąć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться больше и взрослее в процессе развития (о людях, животных и растениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расти",
                                "target_lang_sense_clarification": "развиваться физически"
                            },
                            {
                                "target_lang_word": "подрастать",
                                "target_lang_sense_clarification": "становиться немного старше или выше"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85c40396-2fcd-3ecf-8703-e2d4248b2f9e",
                    "sense_definition": "Występować naturalnie lub być uprawianym w określonym miejscu (o roślinach).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym starym lesie <w>rosną</w> głównie wysokie sosny i dęby.",
                            "target_lang_translations": {
                                "ru": "В этом старом лесу <w>растут</w> в основном высокие сосны и дубы."
                            }
                        },
                        {
                            "text": "W naszym ogrodzie <w>rosną</w> pyszne pomidory i ogórki.",
                            "target_lang_translations": {
                                "ru": "В нашем саду <w>растут</w> вкусные помидоры и огурцы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rosnąć w lesie",
                        "rosnąć w ogrodzie",
                        "dziko rosnąć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встречаться в природе или выращиваться в определенном месте (о растениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расти",
                                "target_lang_sense_clarification": "произрастать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bb83340-c13b-364b-83d7-77af1403a192",
                    "sense_definition": "Zwiększać swoją objętość pod wpływem fermentacji (o cieście).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciasto na pizzę musi <w>rosnąć</w> w ciepłym miejscu przez godzinę.",
                            "target_lang_translations": {
                                "ru": "Тесто для пиццы должно <w>подниматься</w> в теплом месте в течение часа."
                            }
                        },
                        {
                            "text": "Przykryj miskę ścierką, żeby chleb spokojnie <w>rósł</w>.",
                            "target_lang_translations": {
                                "ru": "Накрой миску полотенцем, чтобы хлеб спокойно <w>поднимался</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ciasto rośnie",
                        "rosnąć pod przykryciem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличиваться в объеме в результате брожения (о тесте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подниматься",
                                "target_lang_sense_clarification": "о тесте"
                            },
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "кулинарный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "88df8484-d490-3f28-8262-655b8fbc378c",
                    "sense_definition": "Snelheid verminderen of tot stilstand komen, meestal door gebruik te maken van een remmechanisme.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De automobilist moest plotseling hard <w>remmen</w> voor het rode stoplicht.",
                            "target_lang_translations": {
                                "ru": "Водителю пришлось внезапно резко <w>затормозить</w> перед красным светофором.",
                                "en": "The driver had to suddenly <w>brake</w> hard for the red traffic light."
                            }
                        },
                        {
                            "text": "Je moet op tijd <w>remmen</w> als je een scherpe bocht nadert.",
                            "target_lang_translations": {
                                "ru": "Нужно вовремя <w>тормозить</w>, когда приближаешься к крутому повороту.",
                                "en": "You must <w>brake</w> in time when approaching a sharp turn."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertragen",
                        "afremmen",
                        "stoppen"
                    ],
                    "antonyms": [
                        "versnellen",
                        "optrekken"
                    ],
                    "common_phrases": [
                        "hard remmen",
                        "plotseling remmen",
                        "tijdig remmen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снижать скорость или останавливаться, обычно используя тормозной механизм.",
                        "en": "To reduce speed or come to a stop, usually by using a braking mechanism."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тормозить",
                                "target_lang_sense_clarification": "снижать скорость"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "brake",
                                "target_lang_sense_clarification": "reduce speed"
                            }
                        ]
                    }
                },
                {

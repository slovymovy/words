    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f5e1d606-8414-3042-9cd3-7b4b4e2016e4",
                    "sense_definition": "A spherical model of Earth or other planet.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "model",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher used a <w>globe</w> to show the students where the country is located.",
                            "target_lang_translations": {
                                "ru": "Учитель использовал <w>глобус</w>, чтобы показать ученикам, где находится эта страна."
                            }
                        },
                        {
                            "text": "I like to spin the <w>globe</w> and see where my finger lands.",
                            "target_lang_translations": {
                                "ru": "Мне нравится крутить <w>глобус</w> и смотреть, куда попадет мой палец."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spin the globe",
                        "look at the globe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сферическая модель Земли или другой планеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гло́бус",
                                "target_lang_sense_clarification": "модель Земли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a79cc95e-5e91-31c5-8408-df1e0c47cdb2",
                    "sense_definition": "The planet Earth.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "planet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "News of the event spread quickly across the <w>globe</w>.",
                            "target_lang_translations": {
                                "ru": "Новости об этом событии быстро разлетелись по всему <w>земному шару</w>."
                            }
                        },
                        {
                            "text": "People from all around the <w>globe</w> gathered for the international conference.",
                            "target_lang_translations": {
                                "ru": "Люди со всего <w>земного шара</w> собрались на международную конференцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "Earth",
                        "world"
                    ],
                    "common_phrases": [
                        "around the globe",
                        "across the globe",
                        "corners of the globe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Планета Земля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "земно́й шар",
                                "target_lang_sense_clarification": "планета"
                            },
                            {
                                "target_lang_word": "Земля́",
                                "target_lang_sense_clarification": "планета"
                            },
                            {
                                "target_lang_word": "мир",
                                "target_lang_sense_clarification": "вселенная, свет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9889d9ff-e166-3bc9-aea7-af829b44c470",
                    "sense_definition": "Any spherical (or nearly spherical) object.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The streetlights were large glass <w>globes</w> that illuminated the park.",
                            "target_lang_translations": {
                                "ru": "Уличные фонари представляли собой большие стеклянные <w>шары</w>, освещавшие парк."
                            }
                        },
                        {
                            "text": "She shook the snow <w>globe</w> and watched the white flakes fall on the tiny house.",
                            "target_lang_translations": {
                                "ru": "Она встряхнула снежный <w>шар</w> и смотрела, как белые хлопья падают на крошечный домик."
                            }
                        }
                    ],
                    "common_phrases": [
                        "glass globe",
                        "snow globe",
                        "golden globe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой сферический (или почти сферический) предмет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шар",
                                "target_lang_sense_clarification": "предмет"
                            },
                            {
                                "target_lang_word": "сфе́ра",
                                "target_lang_sense_clarification": "форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84b6fbe8-0beb-3f3f-a866-e5b91af9f426",
                    "sense_definition": "A light bulb.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to buy a new <w>globe</w> because the lamp in the hallway has gone dark.",
                            "target_lang_translations": {
                                "ru": "Нам нужно купить новую <w>лампочку</w>, потому что лампа в коридоре перегорела."
                            }
                        },
                        {
                            "text": "He carefully unscrewed the hot <w>globe</w> from the ceiling fixture.",
                            "target_lang_translations": {
                                "ru": "Он осторожно выкрутил горячую <w>лампочку</w> из потолочного светильника."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Электрическая лампочка или плафон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ла́мпочка",
                                "target_lang_sense_clarification": "источник света"
                            },
                            {
                                "target_lang_word": "плафо́н",
                                "target_lang_sense_clarification": "абажур"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d291df9-41e8-304a-ad11-b69f893a3ea7",
                    "sense_definition": "A woman's breast.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The article was criticized for making rude comments about her <w>globes</w>.",
                            "target_lang_translations": {
                                "ru": "Статью раскритиковали за грубые комментарии о её <w>шарах</w>."
                            }
                        },
                        {
                            "text": "He made an inappropriate joke about the actress's <w>globes</w>.",
                            "target_lang_translations": {
                                "ru": "Он отпустил неуместную шутку о <w>шарах</w> актрисы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская грудь (вульгарно)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шары́",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "буфера́",
                                "target_lang_sense_clarification": "грубый сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "70c2ac36-a75e-3888-bd3c-2a445b49e175",
                    "sense_definition": "To form into a spherical shape.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "shape_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The water droplets <w>globed</w> on the surface of the freshly waxed car.",
                            "target_lang_translations": {
                                "ru": "Капли воды <w>собирались в шарики</w> на поверхности свеженавощенной машины."
                            }
                        },
                        {
                            "text": "Molten glass <w>globed</w> at the end of the pipe as the artist blew into it.",
                            "target_lang_translations": {
                                "ru": "Расплавленное стекло <w>собиралось в шар</w> на конце трубки, когда художник дул в неё."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать сферическую форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собира́ться в шар",
                                "target_lang_sense_clarification": "о жидкости или мягком материале"
                            },
                            {
                                "target_lang_word": "округля́ться",
                                "target_lang_sense_clarification": "становиться круглым"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "3665b958-ecfd-3107-9141-a4fe80abe407",
                    "sense_definition": "A city in Arizona or a community in Wisconsin.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We stopped for lunch in <w>Globe</w> while driving through Arizona.",
                            "target_lang_translations": {
                                "ru": "Мы остановились пообедать в <w>Глобе</w>, когда ехали через Аризону."
                            }
                        },
                        {
                            "text": "He was born in a small town called <w>Globe</w>.",
                            "target_lang_translations": {
                                "ru": "Он родился в маленьком городке под названием <w>Глоб</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в Аризоне или поселение в Висконсине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Глоб",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "Romein",
        "Romeins"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fac5ba1d-fbeb-3175-b5f7-24ad737a6c9b",
                    "sense_definition": "Meervoud van <w>Romein</w>: inwoners van de stad Rome.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "inhabitants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <Romeinen> zijn erg trots op de geschiedenis van hun stad.",
                            "target_lang_translations": {
                                "ru": "<Римляне> очень гордятся историей своего города."
                            }
                        },
                        {
                            "text": "Veel <Romeinen> verlaten de stad in augustus vanwege de hitte.",
                            "target_lang_translations": {
                                "ru": "Многие <римляне> покидают город в августе из-за жары."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жители города Рим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "римляне",
                                "target_lang_sense_clarification": "жители города"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0bceb9a-a4d2-3b7d-9708-1bcaae8380da",
                    "sense_definition": "Het volk uit de oudheid dat een groot rijk rond de Middellandse Zee bestuurde.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "historical_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <Romeinen> bouwden stevige wegen die eeuwenlang meegingen.",
                            "target_lang_translations": {
                                "ru": "<Римляне> строили прочные дороги, которые служили веками."
                            }
                        },
                        {
                            "text": "Op school leren kinderen veel over de Grieken en de <Romeinen>.",
                            "target_lang_translations": {
                                "ru": "В школе дети много узнают о греках и <римлянах>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de oude Romeinen",
                        "Grieken en Romeinen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древний народ, управлявший огромной империей вокруг Средиземного моря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "римляне",
                                "target_lang_sense_clarification": "древний народ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ffcd059-c727-3665-a1ee-556d81601f2c",
                    "sense_definition": "Meervoud van <w>romein</w>: een lettertype met rechtopstaande tekens.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "typography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze tekst worden cursieven en <romeinen> door elkaar gebruikt.",
                            "target_lang_translations": {
                                "ru": "В этом тексте курсив и <антиква> используются вперемешку."
                            }
                        },
                        {
                            "text": "De zetter koos voor duidelijke <romeinen> voor de hoofdtekst.",
                            "target_lang_translations": {
                                "ru": "Наборщик выбрал четкую <антикву> для основного текста."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шрифт с прямым начертанием (в отличие от курсива)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "антиква",
                                "target_lang_sense_clarification": "типографский шрифт"
                            },
                            {
                                "target_lang_word": "прямой шрифт",
                                "target_lang_sense_clarification": "начертание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "51e585e2-ed0f-376f-a293-025cb8e34cf9",
                    "sense_definition": "Een boek in het Nieuwe Testament van de Bijbel, geschreven als brief van Paulus.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "document_or_program_name",
                    "examples": [
                        {
                            "text": "De dominee preekte afgelopen zondag uit de brief aan de <Romeinen>.",
                            "target_lang_translations": {
                                "ru": "В прошлое воскресенье пастор проповедовал из Послания к <Римлянам>."
                            }
                        },
                        {
                            "text": "In <Romeinen> wordt de christelijke leer uitgebreid uitgelegd.",
                            "target_lang_translations": {
                                "ru": "В <Послании к Римлянам> подробно объясняется христианское учение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brief aan de Romeinen"
                    ],
                    "target_lang_definitions": {

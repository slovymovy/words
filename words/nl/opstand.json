                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boswachter inspecteerde de jonge <w>opstand</w> van eikenbomen.",
                            "target_lang_translations": {
                                "en": "The ranger inspected the young <w>stand</w> of oak trees."
                            }
                        },
                        {
                            "text": "Door de zware storm is een groot deel van de <w>opstand</w> beschadigd geraakt.",
                            "target_lang_translations": {

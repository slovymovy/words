    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "018ff333-1650-3a8f-8e46-774fe81d1cb2",
                    "sense_definition": "Forma mianownika i biernika liczby mnogiej zaimków <w>ten</w>, <w>ta</w> oraz <w>to</w>, używana w odniesieniu do rzeczowników rodzaju niemęskoosobowego",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdzie położyłeś <w>te</w> nowe książki?",
                            "target_lang_translations": {
                                "ru": "Куда ты положил <w>эти</w> новые книги?"
                            }
                        },
                        {
                            "text": "Wszystkie <w>te</w> sprawy musimy załatwić dzisiaj.",
                            "target_lang_translations": {
                                "ru": "Все <w>эти</w> дела мы должны уладить сегодня."
                            }
                        },
                        {
                            "text": "Patrz na <w>te</w> same drzewa co wczoraj.",
                            "target_lang_translations": {
                                "ru": "Смотри на <w>те</w> же деревья, что и вчера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "te same",
                        "te dni",
                        "te rzeczy",
                        "wszystkie te"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rodzaj niemęskoosobowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма именительного и винительного падежа множественного числа указательных местоимений, используемая с существительными нелично-мужского рода"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эти",
                                "target_lang_sense_clarification": "указание на близкие предметы или упоминаемые объекты"
                            },
                            {
                                "target_lang_word": "те",
                                "target_lang_sense_clarification": "в сочетании «te same» (те же) или при противопоставлении"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea764d73-6376-3956-a307-671e1acd5beb",
                    "sense_definition": "Nazwa litery t",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

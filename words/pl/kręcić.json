            "senses": [
                {
                    "sense_id": "d6702bdf-45e4-3d0d-9356-5577b8f8fef0",
                    "sense_definition": "Wprawiać przedmiot w ruch obrotowy wokół jego osi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz mocno <kręcić> korbą, żeby uruchomić ten stary silnik.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно сильно <крутить> ручку, чтобы запустить этот старый двигатель."
                            }
                        },
                        {
                            "text": "Dziecko siedziało na podłodze i lubiło <kręcić> kolorowym bączkiem.",
                            "target_lang_translations": {
                                "ru": "Ребенок сидел на полу и любил <крутить> цветной волчок."
                            }
                        }
                    ],
                    "synonyms": [
                        "obracać"
                    ],
                    "common_phrases": [
                        "kręcić głową",
                        "kręcić kierownicą",
                        "kręcić nosem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить предмет во вращательное движение вокруг его оси."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крутить",
                                "target_lang_sense_clarification": "вращать что-либо (ручку, колесо)"
                            },
                            {
                                "target_lang_word": "вращать",
                                "target_lang_sense_clarification": "более формальный синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89e90e25-24cb-325c-b2bd-89de886b00c7",
                    "sense_definition": "Obracać się wokół własnej osi (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ziemia <kręci> się wokół własnej osi oraz wokół Słońca.",
                            "target_lang_translations": {
                                "ru": "Земля <вращается> вокруг собственной оси, а также вокруг Солнца."
                            }
                        },
                        {
                            "text": "Tancerka potrafi szybko <kręcić> się na jednej nodze.",
                            "target_lang_translations": {
                                "ru": "Танцовщица умеет быстро <крутиться> на одной ноге."
                            }

{
    "word_family": [
        "sloppenwijk"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dd615913-ca67-3d94-8202-5b4330cd6042",
                    "sense_definition": "Een toestand waarin geen vooruitgang meer wordt geboekt; een impasse.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De onderhandelingen over het nieuwe contract zijn in het <w>slop</w> geraakt.",
                            "target_lang_translations": {
                                "en": "The negotiations about the new contract have reached an <w>impasse</w>."
                            }
                        },
                        {
                            "text": "Door de economische crisis zit de huizenmarkt al maanden in het <w>slop</w>.",
                            "target_lang_translations": {
                                "en": "Due to the economic crisis, the housing market has been in a <w>slump</w> for months."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in het slop raken",
                        "in het slop zitten",
                        "uit het slop trekken"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of stagnation where no progress is made; an impasse."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "impasse",
                                "target_lang_sense_clarification": "a situation where no progress is possible"
                            },
                            {
                                "target_lang_word": "rut",
                                "target_lang_sense_clarification": "a fixed or established mode of procedure or course of life"
                            },
                            {
                                "target_lang_word": "slump",
                                "target_lang_sense_clarification": "a period of substantial failure or decline"
                            },
                            {
                                "target_lang_word": "doldrums",
                                "target_lang_sense_clarification": "a state of inactivity or stagnation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9432634-d57e-305e-9439-8259b36843f6",
                    "sense_definition": "Een smalle doorgang of steeg tussen gebouwen, vaak doodlopend.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is maar een nauw <w>slopje</w> tussen ons huis en dat van de buren.",
                            "target_lang_translations": {
                                "en": "There is only a narrow <w>alleyway</w> between our house and the neighbors'."
                            }
                        },
                        {
                            "text": "De kinderen speelden verstoppertje in het donkere <w>slop</w> achter de winkel.",
                            "target_lang_translations": {
                                "en": "The children played hide-and-seek in the dark <w>alley</w> behind the shop."
                            }
                        }
                    ],
                    "synonyms": [
                        "krot"
                    ],
                    "target_lang_definitions": {
                        "en": "A narrow passage or alley between buildings, often a dead end."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "alley",
                                "target_lang_sense_clarification": "a narrow passageway between or behind buildings"
                            },
                            {
                                "target_lang_word": "alleyway",
                                "target_lang_sense_clarification": "a narrow street or passage"
                            },
                            {
                                "target_lang_word": "lane",
                                "target_lang_sense_clarification": "a narrow road or path"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8a7fbd5-fac5-3001-94c4-da7539c21080",
                    "sense_definition": "Een verwaarloosde, armoedige woning.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het gezin woonde jarenlang in een tochtig <w>slop</w> aan de rand van de stad.",
                            "target_lang_translations": {
                                "en": "The family lived for years in a drafty <w>hovel</w> on the edge of the city."
                            }
                        },
                        {
                            "text": "De gemeente besloot het oude <w>slop</w> eindelijk te renoveren.",
                            "target_lang_translations": {
                                "en": "The municipality decided to finally renovate the old <w>shack</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "krot"
                    ],
                    "target_lang_definitions": {
                        "en": "A neglected, shabby dwelling."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hovel",
                                "target_lang_sense_clarification": "a small, squalid, unpleasant, or simply constructed dwelling"
                            },
                            {
                                "target_lang_word": "shack",
                                "target_lang_sense_clarification": "a roughly built hut or cabin"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
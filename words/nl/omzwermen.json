            "senses": [
                {
                    "sense_id": "2a4316ab-adc2-3d6a-8349-a5865a326706",
                    "sense_definition": "Met een grote groep mensen of dieren voortdurend dicht om iemand of iets heen bewegen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De paparazzi <w>omzwermden</w> de beroemde acteur zodra hij het hotel verliet.",
                            "target_lang_translations": {
                                "en": "The paparazzi <w>swarmed</w> the famous actor as soon as he left the hotel.",
                                "ru": "Папарацци <w>окружили</w> известного актера, как только он вышел из отеля."
                            }
                        },
                        {
                            "text": "Vervelende vliegen <w>omzwermen</w> het paard dat rustig in de wei staat.",
                            "target_lang_translations": {
                                "en": "Annoying flies <w>swarm around</w> the horse that is standing quietly in the meadow.",
                                "ru": "Надоедливые мухи <w>вьются вокруг</w> лошади, которая спокойно стоит на лугу."
                            }
                        }
                    ],
                    "synonyms": [
                        "omringen",
                        "insluiten"
                    ],
                    "common_phrases": [
                        "iemand omzwermen",
                        "de prooi omzwermen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inseparable"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To move constantly close around someone or something with a large group of people or animals.",
                        "ru": "Постоянно двигаться большой группой вокруг кого-либо или чего-либо; окружать толпой или роем."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swarm around",
                                "target_lang_sense_clarification": "to move in a large group around a target"
                            },
                            {
                                "target_lang_word": "throng",
                                "target_lang_sense_clarification": "to crowd around someone (specifically for people)"
                            },
                            {
                                "target_lang_word": "swarm",
                                "target_lang_sense_clarification": "to surround and move about in large numbers"

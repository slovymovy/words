                {
                    "sense_id": "c9da124d-5392-3bfb-891a-91d14a2419d4",
                    "sense_definition": "Een specifiek onderdeel van een bedrijf, organisatie of instelling dat verantwoordelijk is voor een bepaalde taak of functie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn collega werkt al tien jaar op de financiële <w>afdeling</w>.",
                            "target_lang_translations": {
                                "en": "My colleague has been working in the finance <w>department</w> for ten years.",
                                "ru": "Мой коллега уже десять лет работает в финансовом <w>отделе</w>."
                            }
                        },
                        {
                            "text": "De <w>afdeling</w> klantenservice is telefonisch bereikbaar tot vijf uur.",
                            "target_lang_translations": {
                                "en": "The customer service <w>department</w> is reachable by phone until five o'clock.",
                                "ru": "<w>Отдел</w> обслуживания клиентов доступен по телефону до пяти часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "departement",
                        "dienst",
                        "sectie"
                    ],
                    "common_phrases": [
                        "hoofd van de afdeling",
                        "op de afdeling",
                        "administratieve afdeling"
                    ],
                    "target_lang_definitions": {
                        "en": "A specific part of a company, organization, or institution responsible for a particular task or function.",
                        "ru": "Определенная часть компании, организации или учреждения, отвечающая за конкретную задачу или функцию."
                    },
                    "translations": {

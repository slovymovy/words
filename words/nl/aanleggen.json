{
    "word_family": [
        "aanleg",
        "aanlegplaats",
        "aanlegsteiger"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "41e53c37-420d-3d7d-b92d-c647c3db0e57",
                    "sense_definition": "Iets nieuws bouwen, ontwerpen of installeren, zoals een weg, een tuin of een netwerk.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente heeft besloten een nieuw fietspad <w>aan te leggen</w>.",
                            "target_lang_translations": {
                                "en": "The municipality has decided to <w>construct</w> a new bicycle path.",
                                "ru": "Муниципалитет решил <w>проложить</w> новую велосипедную дорожку."
                            }
                        },
                        {
                            "text": "Wij hebben vorig jaar een prachtige tuin met vijver laten <w>aanleggen</w>.",
                            "target_lang_translations": {
                                "en": "We had a beautiful garden with a pond <w>laid out</w> last year.",
                                "ru": "В прошлом году мы поручили <w>разбить</w> прекрасный сад с прудом."
                            }
                        }
                    ],
                    "synonyms": [
                        "bouwen",
                        "construeren",
                        "installeren"
                    ],
                    "antonyms": [
                        "slopen",
                        "afbreken"
                    ],
                    "common_phrases": [
                        "een weg aanleggen",
                        "een tuin aanleggen",
                        "elektriciteit aanleggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To build, design, or install something new, such as a road, a garden, or a network.",
                        "ru": "Строить, проектировать или устанавливать что-то новое, например, дорогу, сад или сеть."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "construct",
                                "target_lang_sense_clarification": "general construction (roads, buildings)"
                            },
                            {
                                "target_lang_word": "lay",
                                "target_lang_sense_clarification": "for roads, cables, or gardens"
                            },
                            {
                                "target_lang_word": "install",
                                "target_lang_sense_clarification": "for systems, electricity, or networks"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прокладывать",
                                "target_lang_sense_clarification": "дорогу, путь, кабель"
                            },
                            {
                                "target_lang_word": "разбивать",
                                "target_lang_sense_clarification": "сад, парк"
                            },
                            {
                                "target_lang_word": "проводить",
                                "target_lang_sense_clarification": "электричество, коммуникации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81558306-bdde-39b5-939d-182e72c9e985",
                    "sense_definition": "Met een boot of schip vastmaken aan de kade of oever.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de dichte mist kon het schip nergens veilig <w>aanleggen</w>.",
                            "target_lang_translations": {
                                "en": "Due to the dense fog, the ship could not <w>dock</w> safely anywhere.",
                                "ru": "Из-за густого тумана корабль нигде не мог безопасно <w>причалить</w>."
                            }
                        },
                        {
                            "text": "De kapitein gaf het bevel dat de bemanning moest <w>aanleggen</w> in de haven.",
                            "target_lang_translations": {
                                "en": "The captain gave the order that the crew had to <w>moor</w> in the harbor.",
                                "ru": "Капитан отдал приказ команде <w>пришвартоваться</w> в порту."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanmeren",
                        "afmeren"
                    ],
                    "antonyms": [
                        "afvaren",
                        "vertrekken"
                    ],
                    "common_phrases": [
                        "aan de kade aanleggen",
                        "met de boot aanleggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To tie a boat or ship to the quay or bank.",
                        "ru": "Привязывать лодку или корабль к причалу или берегу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "moor",
                                "target_lang_sense_clarification": "to secure a ship with ropes"
                            },
                            {
                                "target_lang_word": "dock",
                                "target_lang_sense_clarification": "to come into a dock or harbor"
                            },
                            {
                                "target_lang_word": "berth",
                                "target_lang_sense_clarification": "to park a ship in a designated spot"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "причаливать",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "швартоваться",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "773e9676-202f-38d0-9a4a-9391c2da1d42",
                    "sense_definition": "Een wapen op een doel richten voordat je schiet.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_hunting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jager zag het hert en besloot voorzichtig <w>aan te leggen</w>.",
                            "target_lang_translations": {
                                "en": "The hunter saw the deer and decided to carefully <w>take aim</w>.",
                                "ru": "Охотник увидел оленя и решил осторожно <w>прицелиться</w>."
                            }
                        },
                        {
                            "text": "Hij wachtte rustig tot hij zijn geweer kon <w>aanleggen</w> om te vuren.",
                            "target_lang_translations": {
                                "en": "He waited quietly until he could <w>aim</w> his rifle to fire.",
                                "ru": "Он спокойно ждал, пока сможет <w>вскинуть</w> ружье, чтобы выстрелить."
                            }
                        }
                    ],
                    "synonyms": [
                        "richten",
                        "mikken"
                    ],
                    "common_phrases": [
                        "het geweer aanleggen",
                        "op iemand aanleggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To point a weapon at a target before shooting.",
                        "ru": "Направлять оружие на цель перед выстрелом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take aim",
                                "target_lang_sense_clarification": "deliberate action of aiming"
                            },
                            {
                                "target_lang_word": "aim",
                                "target_lang_sense_clarification": "general term"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прицеливаться",
                                "target_lang_sense_clarification": "наводить оружие"
                            },
                            {
                                "target_lang_word": "вскидывать",
                                "target_lang_sense_clarification": "ружье к плечу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10cf7dae-39df-36c9-b815-465a622f1e19",
                    "sense_definition": "Een baby aan de borst leggen om borstvoeding te geven.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kraamhulp leerde de jonge moeder hoe ze de baby goed moest <w>aanleggen</w>.",
                            "target_lang_translations": {
                                "en": "The maternity nurse taught the young mother how to properly <w>latch</w> the baby <w>on</w>.",
                                "ru": "Акушерка учила молодую мать, как правильно <w>прикладывать</w> ребенка."
                            }
                        },
                        {
                            "text": "Als de baby honger heeft, kun je hem het beste direct <w>aanleggen</w>.",
                            "target_lang_translations": {
                                "en": "If the baby is hungry, it is best to <w>put</w> him <w>to the breast</w> immediately.",
                                "ru": "Если ребенок голоден, лучше всего сразу <w>приложить</w> его к груди."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de baby aanleggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To put a baby to the breast to breastfeed.",
                        "ru": "Прикладывать младенца к груди для кормления."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "latch on",
                                "target_lang_sense_clarification": "technical term for breastfeeding attachment"
                            },
                            {
                                "target_lang_word": "put to the breast",
                                "target_lang_sense_clarification": "descriptive action"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прикладывать",
                                "target_lang_sense_clarification": "к груди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "827f0325-66b7-3c05-a6b0-c8ab6612e7f9",
                    "sense_definition": "Bewust proberen iets te bereiken of te veroorzaken, vaak met een negatieve of uitdagende bedoeling.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het lijkt wel alsof hij het op ruzie <w>aanlegt</w> met die opmerkingen.",
                            "target_lang_translations": {
                                "en": "It seems as if he is <w>looking for</w> a fight with those comments.",
                                "ru": "Кажется, будто он этими замечаниями <w>напрашивается</w> на ссору."
                            }
                        },
                        {
                            "text": "Zij heeft het er duidelijk op <w>aangelegd</w> om promotie te maken.",
                            "target_lang_translations": {
                                "en": "She is clearly <w>aiming</w> to get a promotion.",
                                "ru": "Она явно <w>добивалась</w> повышения."
                            }
                        }
                    ],
                    "synonyms": [
                        "aansturen op",
                        "uitlokken"
                    ],
                    "common_phrases": [
                        "het erop aanleggen",
                        "het op ruzie aanleggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To consciously try to achieve or cause something, often with a negative or provocative intent.",
                        "ru": "Намеренно добиваться чего-либо или провоцировать ситуацию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be out for",
                                "target_lang_sense_clarification": "intent to cause something (e.g., trouble)"
                            },
                            {
                                "target_lang_word": "aim for",
                                "target_lang_sense_clarification": "to have as a goal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "напрашиваться",
                                "target_lang_sense_clarification": "на конфликт, ссору"
                            },
                            {
                                "target_lang_word": "добиваться",
                                "target_lang_sense_clarification": "цели"
                            }
                        ]
                    }
                }
            ]
        }
    ]

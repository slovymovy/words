                            },
                            {
                                "target_lang_word": "беспорядочно",
                                "target_lang_sense_clarification": "без порядка или организации"
                            },
                            {
                                "target_lang_word": "наугад",
                                "target_lang_sense_clarification": "без точного знания или расчета"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb22eee4-0636-302d-93d0-d682caac5ca0",
                    "sense_definition": "A public place, often outdoors, where people meet to buy and sell goods, usually at temporary stalls.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_trade_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went to the local <w>market</w> to buy fresh vegetables and fruit.",
                            "target_lang_translations": {
                                "ru": "Мы пошли на местный <w>рынок</w>, чтобы купить свежие овощи и фрукты."
                            }
                        },
                        {
                            "text": "Every Saturday, the town square turns into a busy <w>market</w> with many vendors.",
                            "target_lang_translations": {
                                "ru": "Каждую субботу городская площадь превращается в оживленный <w>рынок</w> с множеством торговцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "marketplace",
                        "bazaar",
                        "fair"
                    ],
                    "common_phrases": [
                        "farmers market",
                        "flea market",
                        "street market",
                        "fish market"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общественное место, часто под открытым небом, где люди собираются для покупки и продажи товаров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рынок",
                                "target_lang_sense_clarification": "место торговли"
                            },
                            {
                                "target_lang_word": "базар",
                                "target_lang_sense_clarification": "торговая площадь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c645aba8-c3ca-30f3-b9b8-52d7dd8c12fb",
                    "sense_definition": "A store that sells food and household items; a grocery store.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_trade_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you stop by the <w>market</w> on your way home to get some milk?",
                            "target_lang_translations": {
                                "ru": "Можешь зайти в <w>магазин</w> по дороге домой и купить молока?"
                            }
                        },
                        {
                            "text": "The small corner <w>market</w> is convenient for quick shopping trips.",
                            "target_lang_translations": {
                                "ru": "Небольшой угловой <w>магазин</w> удобен для быстрых покупок."
                            }
                        }
                    ],
                    "synonyms": [
                        "mart#Noun"
                    ],
                    "common_phrases": [
                        "super market",
                        "meat market"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in North American English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Магазин, в котором продаются продукты питания и хозяйственные товары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магазин",
                                "target_lang_sense_clarification": "торговое предприятие"
                            },
                            {
                                "target_lang_word": "продуктовый магазин",
                                "target_lang_sense_clarification": "магазин продуктов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9e851f0-fa08-34f5-b257-f03a202ebced",
                    "sense_definition": "The general system of buying and selling goods, services, or financial shares.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Investors are worried because the stock <w>market</w> has fallen significantly this week.",
                            "target_lang_translations": {
                                "ru": "Инвесторы обеспокоены, так как фондовый <w>рынок</w> значительно упал на этой неделе."
                            }
                        },
                        {
                            "text": "The housing <w>market</w> is becoming very expensive for first-time buyers.",
                            "target_lang_translations": {
                                "ru": "<w>Рынок</w> жилья становится очень дорогим для тех, кто покупает впервые."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stock market",
                        "job market",
                        "black market",
                        "free market",
                        "on the market"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общая система купли-продажи товаров, услуг или ценных бумаг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рынок",
                                "target_lang_sense_clarification": "экономическая система"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f664e05b-f5dc-3a62-a62c-54fed5497005",
                    "sense_definition": "A specific group of people who are interested in buying a particular product or service.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is trying to expand its <w>market</w> to include younger teenagers.",
                            "target_lang_translations": {
                                "ru": "Компания пытается расширить свой <w>рынок сбыта</w>, включив в него подростков."
                            }
                        },
                        {
                            "text": "There is a growing <w>market</w> for electric vehicles in urban areas.",
                            "target_lang_translations": {
                                "ru": "В городских районах растет <w>рынок</w> электромобилей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "target market",
                        "niche market",
                        "mass market",
                        "gap in the market"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа потребителей, заинтересованных в определенном товаре или услуге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рынок сбыта",
                                "target_lang_sense_clarification": "потенциальные покупатели"
                            },
                            {
                                "target_lang_word": "рынок",
                                "target_lang_sense_clarification": "сфера спроса"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "821baa51-cb4e-39c5-b05e-294cb06d2044",
                    "sense_definition": "To advertise and organize the sale of a product to make it attractive to customers.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "business_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They plan to <w>market</w> the new smartphone as a device for professional photographers.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>продвигать</w> новый смартфон как устройство для профессиональных фотографов."
                            }
                        },
                        {
                            "text": "It can be difficult to <w>market</w> a product that has so much competition.",
                            "target_lang_translations": {
                                "ru": "Бывает трудно <w>продвигать</w> продукт, у которого так много конкурентов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "market a product",
                        "market aggressively",
                        "market services"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рекламировать и организовывать продажу товара, чтобы сделать его привлекательным для покупателей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продвигать",
                                "target_lang_sense_clarification": "на рынке"
                            },
                            {
                                "target_lang_word": "рекламировать",
                                "target_lang_sense_clarification": "товар"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c348447d-5e46-3abe-9b6a-a155aff86b86",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Market</w> was known for his contributions to the local library.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Маркет</w> был известен своим вкладом в местную библиотеку."
                            }
                        },
                        {
                            "text": "We looked up the name <w>Market</w> in the telephone directory.",
                            "target_lang_translations": {
                                "ru": "Мы искали фамилию <w>Маркет</w> в телефонном справочнике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Маркет",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "86928035-5968-3abb-a616-2810557b61ea",
                    "sense_definition": "Poważanie, autorytet i szacunek, jakimi ktoś cieszy się w swoim otoczeniu ze względu na swoje zasługi lub cechy charakteru.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "respect_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzięki swojej sprawiedliwości i mądrości, stary sędzia miał ogromny <w>mir</w> wśród lokalnej społeczności."
                        },
                        {
                            "text": "Nowy kierownik musi ciężko pracować, aby zdobyć <w>mir</w> u swoich podwładnych i zyskać ich zaufanie."
                        }
                    ],
                    "synonyms": [
                        "poważanie",
                        "szacunek"
                    ],
                    "antonyms": [
                        "hańba",
                        "infamia"
                    ],
                    "common_phrases": [
                        "mieć mir",
                        "zdobyć mir",
                        "cieszyć się mirem"
                    ]
                },
                {
                    "sense_id": "c9cbb4b1-f3cb-34a0-b502-d9a5478c8a26",
                    "sense_definition": "Stan spokoju, brak wojny lub konfliktów; zgoda panująca między ludźmi.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "peace_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu latach krwawych walk, zwaśnione narody w końcu zawarły trwały <w>mir</w>."
                        },
                        {
                            "text": "Policja interweniowała w sprawie zgłoszenia o zakłócanie <w>miru</w> domowego przez głośnych sąsiadów."
                        }
                    ],
                    "synonyms": [
                        "pokój",
                        "zgoda"
                    ],
                    "antonyms": [
                        "wojna"
                    ],
                    "common_phrases": [
                        "mir domowy",
                        "zakłócanie miru",
                        "zawrzeć mir"

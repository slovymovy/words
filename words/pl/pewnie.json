    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "fc010935-7f22-3c40-a8e5-1dcc160054f0",
                    "sense_definition": "Słowo wyrażające przypuszczenie, że coś jest bardzo prawdopodobne lub prawie pewne.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "probability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek nie odbiera telefonu, <w>pewnie</w> jest bardzo zajęty.",
                            "target_lang_translations": {
                                "ru": "Марек не отвечает на звонок, <w>наверное</w>, он очень занят."
                            }
                        },
                        {
                            "text": "Jutro <w>pewnie</w> będzie padał deszcz, więc weź parasol.",
                            "target_lang_translations": {
                                "ru": "Завтра, <w>вероятно</w>, пойдет дождь, так что возьми зонт."
                            }
                        }
                    ],
                    "synonyms": [
                        "prawdopodobnie",
                        "zapewne",
                        "chyba"
                    ],
                    "common_phrases": [
                        "pewnie tak",
                        "pewnie nie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used commonly in spoken language instead of 'prawdopodobnie'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, выражающее предположение, что что-то весьма вероятно или почти несомненно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наверное",
                                "target_lang_sense_clarification": "предположение"
                            },
                            {
                                "target_lang_word": "вероятно",
                                "target_lang_sense_clarification": "высокая вероятность"
                            },
                            {
                                "target_lang_word": "наверняка",
                                "target_lang_sense_clarification": "почти точно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e6cab1b-b58b-3885-985b-b4a485abfe92",
                    "sense_definition": "W sposób zdecydowany, bez wahania i z wiarą we własne siły.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "manner_confidence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca prowadził samochód bardzo <w>pewnie</w> mimo trudnych warunków.",
                            "target_lang_translations": {
                                "ru": "Водитель вел машину очень <w>уверенно</w>, несмотря на сложные условия."
                            }
                        },
                        {
                            "text": "Musisz <w>pewnie</w> odpowiadać na pytania podczas rozmowy kwalifikacyjnej.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>уверенно</w> отвечать на вопросы во время собеседования."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdecydowanie",
                        "śmiało"
                    ],
                    "antonyms": [
                        "niepewnie"
                    ],
                    "common_phrases": [
                        "czuć się pewnie",
                        "kroczyć pewnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решительно, без колебаний и с верой в собственные силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уверенно",
                                "target_lang_sense_clarification": "без колебаний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7e34981-7207-3363-87c5-104ab731d18c",
                    "sense_definition": "W sposób stabilny, mocny i bezpieczny, nie budzący obaw o upadek lub błąd.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "manner_stability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko zaczęło już <w>pewnie</w> stać na własnych nogach.",
                            "target_lang_translations": {
                                "ru": "Ребенок начал уже <w>твёрдо</w> стоять на ногах."
                            }
                        },
                        {
                            "text": "Drabina nie stoi <w>pewnie</w>, uważaj, żeby nie spaść.",
                            "target_lang_translations": {
                                "ru": "Лестница стоит не <w>устойчиво</w>, смотри не упади."
                            }
                        }
                    ],
                    "synonyms": [
                        "stabilnie",
                        "bezpiecznie"
                    ],
                    "antonyms": [
                        "niepewnie"
                    ],
                    "common_phrases": [
                        "stać pewnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устойчиво, крепко и безопасно, не вызывая опасений падения или ошибки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устойчиво",
                                "target_lang_sense_clarification": "стабильно"
                            },
                            {
                                "target_lang_word": "твёрдо",
                                "target_lang_sense_clarification": "крепко"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "69672350-4526-32f3-ba7d-10b19e1332e4",
                    "sense_definition": "Wykrzyknik wyrażający zdecydowaną zgodę, potwierdzenie lub brak wątpliwości.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "– Pomożesz mi z zakupami? – <w>Pewnie</w>, nie ma problemu!",
                            "target_lang_translations": {
                                "ru": "– Поможешь мне с покупками? – <w>Конечно</w>, без проблем!"
                            }
                        },
                        {
                            "text": "– Czy masz ochotę na lody? – No <w>pewnie</w>!",
                            "target_lang_translations": {
                                "ru": "– Хочешь мороженого? – Ну <w>конечно</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "oczywiście",
                        "jasne",
                        "naturalnie",
                        "zgoda"
                    ],
                    "common_phrases": [
                        "no pewnie",
                        "a pewnie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "very common in informal dialogue"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, выражающее решительное согласие, подтверждение или отсутствие сомнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конечно",
                                "target_lang_sense_clarification": "согласие"
                            },
                            {
                                "target_lang_word": "разумеется",
                                "target_lang_sense_clarification": "подтверждение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

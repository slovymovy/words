        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e0caba7a-4986-3bdd-b420-e3c0bfb12bdc",
                    "sense_definition": "Decided or arranged in advance and not subject to change.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She receives a <w>fixed</w> salary every month regardless of how many hours she works.",

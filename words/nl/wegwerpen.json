    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "68eddc45-12c3-39cd-98d9-7678a4902435",
                    "sense_definition": "Zich ontdoen van iets dat niet meer bruikbaar of gewenst is, vaak door het in de vuilnisbak te gooien.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "dispose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je mag geen batterijen in de gewone vuilnisbak <w>wegwerpen</w>.",
                            "target_lang_translations": {
                                "en": "You are not allowed to <w>throw away</w> batteries in the regular trash bin.",
                                "ru": "Нельзя <w>выбрасывать</w> батарейки в обычное мусорное ведро."
                            }
                        },
                        {
                            "text": "Hij heeft zijn oude schoenen gisteren <w>weggeworpen</w>.",
                            "target_lang_translations": {
                                "en": "He <w>threw away</w> his old shoes yesterday.",
                                "ru": "Вчера он <w>выбросил</w> свои старые ботинки."
                            }
                        }
                    ],
                    "synonyms": [
                        "weggooien"
                    ],
                    "antonyms": [
                        "bewaren",
                        "houden"
                    ],
                    "common_phrases": [
                        "afval wegwerpen",
                        "rommel wegwerpen"
                    ],
                    "target_lang_definitions": {
                        "en": "To get rid of something that is no longer useful or wanted, often by throwing it in the trash.",
                        "ru": "Избавляться от чего-то, что больше не нужно или не желательно, часто выбрасывая в мусорное ведро."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throw away",
                                "target_lang_sense_clarification": "most common; to put in the trash"
                            },
                            {
                                "target_lang_word": "discard",
                                "target_lang_sense_clarification": "slightly more formal; to get rid of"
                            },
                            {
                                "target_lang_word": "dispose of",
                                "target_lang_sense_clarification": "formal; to deal with waste"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выбрасывать",
                                "target_lang_sense_clarification": "избавляться от мусора или ненужных вещей"
                            },
                            {
                                "target_lang_word": "выкидывать",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d9c939d-411a-34f8-9cf7-12932987a405",
                    "sense_definition": "Iets met een krachtige zwaaiende beweging van zich af gooien.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_throw",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De atleet zal de zware kogel zo ver mogelijk <w>wegwerpen</w>.",
                            "target_lang_translations": {
                                "en": "The athlete will <w>throw</w> the heavy shot as far <w>away</w> as possible.",
                                "ru": "Атлет <w>метнет</w> тяжелое ядро как можно дальше."
                            }
                        },
                        {
                            "text": "Zij zag hoe de jongen de steen in het water <w>wegwierp</w>.",
                            "target_lang_translations": {
                                "en": "She saw how the boy <w>threw</w> the stone <w>away</w> into the water.",
                                "ru": "Она видела, как мальчик <w>выбросил</w> камень в воду."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegsmijten",
                        "gooien"
                    ],
                    "common_phrases": [
                        "ver wegwerpen"
                    ],
                    "target_lang_definitions": {
                        "en": "To throw something away from oneself with a forceful swinging motion.",
                        "ru": "Бросать что-то от себя сильным размашистым движением."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throw away",
                                "target_lang_sense_clarification": "physical action"
                            },
                            {
                                "target_lang_word": "cast away",
                                "target_lang_sense_clarification": "literary or forceful throwing"
                            },
                            {
                                "target_lang_word": "fling away",
                                "target_lang_sense_clarification": "to throw with force or anger"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отбрасывать",
                                "target_lang_sense_clarification": "бросать в сторону от себя"
                            },
                            {
                                "target_lang_word": "швырять",
                                "target_lang_sense_clarification": "бросать с силой или злостью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e51b15bc-fda2-3ba6-8aeb-3756334a0020",
                    "sense_definition": "Een idee, gevoel of belemmering van zich afzetten of niet accepteren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reject",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij besloot al zijn twijfels van zich <w>weg</w> te <w>werpen</w>.",
                            "target_lang_translations": {
                                "en": "He decided to <w>cast aside</w> all his doubts.",
                                "ru": "Он решил <w>отбросить</w> все свои сомнения."
                            }
                        },
                        {
                            "text": "De commissie zou het slechte voorstel direct <w>wegwerpen</w>.",
                            "target_lang_translations": {
                                "en": "The committee would immediately <w>reject</w> the bad proposal.",
                                "ru": "Комитет немедленно <w>отвергнет</w> это плохое предложение."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwerpen",
                        "afwijzen"
                    ],
                    "antonyms": [
                        "aannemen",
                        "accepteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To set aside or refuse to accept an idea, feeling, or obstacle.",
                        "ru": "Отвергать идею, чувство или препятствие; не принимать что-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cast aside",
                                "target_lang_sense_clarification": "figurative; to dismiss doubts or feelings"
                            },
                            {
                                "target_lang_word": "reject",
                                "target_lang_sense_clarification": "to refuse to accept a proposal"
                            },
                            {
                                "target_lang_word": "discard",
                                "target_lang_sense_clarification": "to abandon an idea"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отбрасывать",
                                "target_lang_sense_clarification": "избавляться от мыслей или сомнений"
                            },
                            {
                                "target_lang_word": "отвергать",
                                "target_lang_sense_clarification": "не принимать предложение или идею"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
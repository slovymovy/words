                    ],
                    "common_phrases": [
                        "gaan verhuizen",
                        "verhuizen naar"
                    ],
                    "target_lang_definitions": {
                        "en": "To change one's place of residence or business location.",
                        "ru": "Менять место жительства или местонахождение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "change residence"
                            },
                            {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8572166c-a62f-349c-96af-275c9a26e0cf",
                    "sense_definition": "Het vermogen om zich in te leven in de gevoelens of situatie van anderen; inlevingsvermogen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "empathy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar toonde veel <w>begrip</w> voor de moeilijke thuissituatie van de leerling.",
                            "target_lang_translations": {
                                "ru": "Учитель проявил большое <w>понимание</w> к сложной семейной ситуации ученика."
                            }
                        },
                        {
                            "text": "Ik heb er alle <w>begrip</w> voor dat je vandaag niet op het feest kunt komen.",
                            "target_lang_translations": {
                                "ru": "Я с полным <w>пониманием</w> отношусь к тому, что ты не сможешь прийти сегодня на вечеринку."
                            }
                        }
                    ],
                    "antonyms": [
                        "onbegrip"
                    ],
                    "common_phrases": [
                        "begrip tonen",
                        "begrip hebben voor",
                        "vol begrip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность войти в положение других людей, сопереживать их чувствам или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понимание",
                                "target_lang_sense_clarification": "сочувственное отношение, отзывчивость"
                            },
                            {
                                "target_lang_word": "сочувствие",
                                "target_lang_sense_clarification": "эмоциональная поддержка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "866f8424-f693-38c1-bc1a-500e708378a3",
                    "sense_definition": "Het vermogen om iets met het verstand te doorgronden of te snappen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intellect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ingewikkelde uitleg van de professor ging mijn <w>begrip</w> helaas volledig te boven.",
                            "target_lang_translations": {
                                "ru": "Сложное объяснение профессора, к сожалению, было выше моего <w>понимания</w>."
                            }
                        },
                        {
                            "text": "Voor een goed <w>begrip</w> van de tekst is het belangrijk om rustig te lezen.",
                            "target_lang_translations": {
                                "ru": "Для хорошего <w>понимания</w> текста важно читать спокойно."
                            }
                        }
                    ],
                    "antonyms": [
                        "onbegrip"
                    ],
                    "common_phrases": [
                        "boven mijn begrip",
                        "een goed begrip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность осмыслить или понять что-либо разумом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понимание",
                                "target_lang_sense_clarification": "постижение смысла, осознание"
                            },
                            {
                                "target_lang_word": "разумение",
                                "target_lang_sense_clarification": "интеллектуальная способность (часто в идиомах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28dae77b-218c-3243-b1d6-6d294ac0157b",
                    "sense_definition": "Een abstract idee, term of woord dat een bepaalde betekenis vertegenwoordigt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vrijheid is voor veel mensen een heel belangrijk <w>begrip</w> in de samenleving.",
                            "target_lang_translations": {
                                "ru": "Свобода для многих людей — очень важное <w>понятие</w> в обществе."
                            }

{
    "word_family": [
        "keyboarder",
        "keyboardist"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c72dc9ac-a531-3f34-a3b7-70a5936678df",
                    "sense_definition": "A set of keys laid out in a grid-like pattern, used for typing on a computer or typewriter.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please type your password using the <w>keyboard</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, введите пароль, используя <w>клавиатуру</w>."
                            }
                        },
                        {
                            "text": "I spilled coffee on my computer <w>keyboard</w> yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера я пролил кофе на <w>клавиатуру</w> своего компьютера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "computer keyboard",
                        "on the keyboard",
                        "wireless keyboard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство с набором клавиш для ввода данных в компьютер или пишущую машинку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клавиатура",
                                "target_lang_sense_clarification": "устройство ввода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b5bcc0d-31b5-3115-aa40-44d893ad84c8",
                    "sense_definition": "An electronic musical instrument that produces sounds when its keys are pressed.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wants to learn how to play the <w>keyboard</w>.",
                            "target_lang_translations": {
                                "ru": "Она хочет научиться играть на <w>клавишных</w>."
                            }
                        },
                        {
                            "text": "Our band needs someone who owns a good <w>keyboard</w>.",
                            "target_lang_translations": {
                                "ru": "Нашей группе нужен кто-то, у кого есть хороший <w>синтезатор</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "electronic keyboard",
                        "synthesizer"
                    ],
                    "common_phrases": [
                        "play the keyboard",
                        "electric keyboard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Электронный музыкальный инструмент, издающий звуки при нажатии клавиш."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синтезатор",
                                "target_lang_sense_clarification": "электронный инструмент"
                            },
                            {
                                "target_lang_word": "клавишные",
                                "target_lang_sense_clarification": "тип музыкальных инструментов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d84cb503-b711-3267-a409-64e2d34858b1",
                    "sense_definition": "The row of keys on a musical instrument such as a piano or organ.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pianist ran her fingers quickly along the <w>keyboard</w>.",
                            "target_lang_translations": {
                                "ru": "Пианистка быстро пробежала пальцами по <w>клавиатуре</w>."
                            }
                        },
                        {
                            "text": "He cleaned the dust from the piano <w>keyboard</w>.",
                            "target_lang_translations": {
                                "ru": "Он стер пыль с <w>клавиатуры</w> пианино."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piano keyboard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ряд клавиш на музыкальном инструменте, таком как пианино или орган."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клавиатура",
                                "target_lang_sense_clarification": "часть инструмента"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b6316a28-8afe-36b7-9a6c-c00ef4188945",
                    "sense_definition": "To type text on a computer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The clerk <w>keyboarded</w> the data into the system all afternoon.",
                            "target_lang_translations": {
                                "ru": "Клерк весь день <w>набирал</w> данные в систему."
                            }
                        },
                        {
                            "text": "She is responsible for <w>keyboarding</w> the manuscript.",
                            "target_lang_translations": {
                                "ru": "Она отвечает за <w>набор</w> рукописи."
                            }
                        }
                    ],
                    "synonyms": [
                        "electronic keyboard",
                        "synthesizer"
                    ],
                    "common_phrases": [
                        "keyboarding skills"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набирать текст на компьютере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набирать",
                                "target_lang_sense_clarification": "текст на клавиатуре"
                            },
                            {
                                "target_lang_word": "печатать",
                                "target_lang_sense_clarification": "вводить текст"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
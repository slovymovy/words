{
    "word_family": [
        "druggist",
        "drugstore"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cfa52867-92b7-3958-afe8-4ec0cd85c7ea",
                    "sense_definition": "A substance used to treat an illness, relieve a symptom, or modify a chemical process in the body for a specific purpose.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor prescribed a new <w>drug</w> to help lower her high blood pressure.",
                            "target_lang_translations": {
                                "ru": "Врач выписал новое <w>лекарство</w>, чтобы помочь снизить её высокое кровяное давление."
                            }
                        },
                        {
                            "text": "Aspirin is a common <w>drug</w> that reduces pain and inflammation.",
                            "target_lang_translations": {
                                "ru": "Аспирин — это распространенное <w>лекарство</w>, которое уменьшает боль и воспаление."
                            }
                        }
                    ],
                    "synonyms": [
                        "medicine",
                        "medication",
                        "pharmaceutical",
                        "medicament"
                    ],
                    "common_phrases": [
                        "prescription drug",
                        "generic drug",
                        "wonder drug",
                        "drug interaction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество, используемое для лечения болезни, облегчения симптомов или изменения химических процессов в организме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лекарство",
                                "target_lang_sense_clarification": "общее название лечебного средства"
                            },
                            {
                                "target_lang_word": "препарат",
                                "target_lang_sense_clarification": "медицинское средство, медикамент"
                            },
                            {
                                "target_lang_word": "медикамент",
                                "target_lang_sense_clarification": "формальное название лекарственного средства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96fff67d-bf84-3f51-ae56-720fd0231c11",
                    "sense_definition": "A psychoactive substance, especially one which is illegal and addictive, ingested for recreational use.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "illegal_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police arrested the dealer for selling illegal <w>drugs</w> to teenagers.",
                            "target_lang_translations": {
                                "ru": "Полиция арестовала дилера за продажу запрещенных <w>наркотиков</w> подросткам."
                            }
                        },
                        {
                            "text": "He struggled with <w>drug</w> addiction for many years before getting clean.",
                            "target_lang_translations": {
                                "ru": "Он много лет боролся с зависимостью от <w>наркотиков</w>, прежде чем завязал."
                            }
                        }
                    ],
                    "synonyms": [
                        "dope",
                        "substance"
                    ],
                    "common_phrases": [
                        "illegal drugs",
                        "recreational drug",
                        "drug abuse",
                        "hard drugs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психоактивное вещество, особенно незаконное и вызывающее привыкание, принимаемое для развлечения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наркотик",
                                "target_lang_sense_clarification": "общее название психоактивных веществ"
                            },
                            {
                                "target_lang_word": "наркота",
                                "target_lang_sense_clarification": "сленговое, собирательное название наркотиков"
                            },
                            {
                                "target_lang_word": "дурь",
                                "target_lang_sense_clarification": "жаргонное название наркотиков (обычно марихуаны)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99051595-dfc5-3ebd-bf5c-61393eef8527",
                    "sense_definition": "Anything, such as a substance, emotion, or action, to which one is addicted or obsessed.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_addiction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "For him, adrenaline is a <w>drug</w> that he cannot live without.",
                            "target_lang_translations": {
                                "ru": "Для него адреналин — это <w>наркотик</w>, без которого он не может жить."
                            }
                        },
                        {
                            "text": "Work became her <w>drug</w> of choice to distract herself from personal problems.",
                            "target_lang_translations": {
                                "ru": "Работа стала её любимым <w>наркотиком</w>, чтобы отвлечься от личных проблем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drug of choice",
                        "love is the drug"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-либо (вещество, эмоция или действие), к чему человек испытывает зависимость или одержимость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наркотик",
                                "target_lang_sense_clarification": "в переносном смысле: источник сильной зависимости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f44ffe1-ee3d-3c8d-a425-5c6b8a45c977",
                    "sense_definition": "A store where medicines and other miscellaneous articles are sold; a drugstore.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to stop by the <w>drug</w> to pick up some shampoo and toothpaste.",
                            "target_lang_translations": {
                                "ru": "Мне нужно зайти в <w>аптеку</w>, чтобы купить шампунь и зубную пасту."
                            }
                        },
                        {
                            "text": "We met at the soda fountain inside the local <w>drug</w> after school.",
                            "target_lang_translations": {
                                "ru": "Мы встретились у автомата с газировкой в местной <w>аптеке</w> после школы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for drugstore"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Магазин, где продаются лекарства и другие товары; аптека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аптека",
                                "target_lang_sense_clarification": "учреждение для продажи лекарств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec492ae9-90cf-31eb-8275-82f78603b3cf",
                    "sense_definition": "Any commodity that lies on hand, or is not salable; an article of slow sale, or in no demand.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "unsalable_item",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Without proper advertising, the new product became a <w>drug</w> on the market.",
                            "target_lang_translations": {
                                "ru": "Без надлежащей рекламы новый продукт стал <w>неходовым товаром</w> на рынке."
                            }
                        },
                        {
                            "text": "These old books are a <w>drug</w> in the store; nobody wants to buy them.",
                            "target_lang_translations": {
                                "ru": "Эти старые книги — <w>балласт</w> в магазине; никто не хочет их покупать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drug on the market"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой товар, который не продается или на который нет спроса; неходовой товар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неходовой товар",
                                "target_lang_sense_clarification": "товар, не пользующийся спросом"
                            },
                            {
                                "target_lang_word": "балласт",
                                "target_lang_sense_clarification": "перен. что-то лишнее, обременяющее"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "84165857-9a43-3e3f-aa0b-14a4f5170ffe",
                    "sense_definition": "To administer a substance to a person or animal to make them unconscious or stuporous, or to add such a substance to food or drink.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "administer_drug",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She suspected that someone had <w>drugged</w> her drink at the party.",
                            "target_lang_translations": {
                                "ru": "Она подозревала, что кто-то <w>подсыпал наркотик</w> ей в напиток на вечеринке."
                            }
                        },
                        {
                            "text": "The spies <w>drugged</w> the guards so they could enter the building unnoticed.",
                            "target_lang_translations": {
                                "ru": "Шпионы <w>одурманили</w> охранников, чтобы проникнуть в здание незамеченными."
                            }
                        }
                    ],
                    "synonyms": [
                        "dope"
                    ],
                    "common_phrases": [
                        "drug a drink",
                        "get drugged"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать кому-либо вещество, чтобы лишить сознания или одурманить, либо добавить такое вещество в еду или питье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одурманивать",
                                "target_lang_sense_clarification": "лишать ясности сознания с помощью препаратов"
                            },
                            {
                                "target_lang_word": "накачивать",
                                "target_lang_sense_clarification": "разг. давать большое количество лекарств или наркотиков"
                            },
                            {
                                "target_lang_word": "подсыпать наркотики",
                                "target_lang_sense_clarification": "тайно добавлять вещество в еду или питье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5329a8c5-7204-363b-93e0-e28b66229b88",
                    "sense_definition": "To consume illegal or recreational drugs habitually.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "consume_drug",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent years drinking and <w>drugging</w> before he finally sought help.",
                            "target_lang_translations": {
                                "ru": "Он провел годы, пьянствуя и <w>принимая наркотики</w>, прежде чем наконец обратился за помощью."
                            }
                        },
                        {
                            "text": "They partied and <w>drugged</w> all weekend without sleeping.",
                            "target_lang_translations": {
                                "ru": "Они тусовались и <w>наркоманили</w> все выходные без сна."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drinking and drugging"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулярно употреблять незаконные или рекреационные наркотики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать наркотики",
                                "target_lang_sense_clarification": "употреблять запрещенные вещества"
                            },
                            {
                                "target_lang_word": "наркоманить",
                                "target_lang_sense_clarification": "разг. вести образ жизни наркомана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85c758d2-b76f-3b58-95ac-534176352aa6",
                    "sense_definition": "Nonstandard or dialectal past tense and past participle of <w>drag</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "form_of_drag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>drug</w> the heavy box across the floor because he could not lift it.",
                            "target_lang_translations": {
                                "ru": "Он <w>тащил</w> тяжелую коробку по полу, потому что не мог её поднять."
                            }
                        },
                        {
                            "text": "Look what the cat <w>drug</w> in from the rain!",
                            "target_lang_translations": {
                                "ru": "Смотри, что кошка <w>притащила</w> с дождя!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "look what the cat drug in"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "past of drag"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нестандартная или диалектная форма прошедшего времени глагола \"drag\" (тащить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тащил",
                                "target_lang_sense_clarification": "прошедшее время от глагола тащить"
                            },
                            {
                                "target_lang_word": "притащил",
                                "target_lang_sense_clarification": "совершенный вид глагола тащить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca1ba081-0cfd-34fd-8ace-4d3c6e7fb2a3",
                    "sense_definition": "To prescribe or administer medicines to someone.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "prescribe_drug",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The physician <w>drugged</w> the patient with a heavy dose of sedatives.",
                            "target_lang_translations": {
                                "ru": "Врач <w>накачал</w> пациента большой дозой седативных средств."
                            }
                        },
                        {
                            "text": "In the old days, doctors <w>drugged</w> patients with substances we now consider dangerous.",
                            "target_lang_translations": {
                                "ru": "В старые времена врачи <w>лечили</w> пациентов веществами, которые мы сейчас считаем опасными."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прописывать или давать лекарства кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лечить лекарствами",
                                "target_lang_sense_clarification": "применять медикаментозное лечение"
                            },
                            {
                                "target_lang_word": "пичкать лекарствами",
                                "target_lang_sense_clarification": "разг. давать слишком много лекарств"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "fbc2fc6f-5640-39ff-b1c5-d2eb86f10ff9",
                    "sense_definition": "Устройство для запирания дверей, ящиков и других предметов, которое обычно открывается ключом или набором кода.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "security_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Уходя из дома, обязательно закрывайте входную дверь на <w>замок</w>.",
                            "target_lang_translations": {
                                "en": "When leaving the house, be sure to <w>lock</w> the front door."
                            }
                        },
                        {
                            "text": "Старый навесной <w>замок</w> заржавел и никак не хотел открываться.",
                            "target_lang_translations": {
                                "en": "The old <w>padlock</w> had rusted and simply wouldn't open."
                            }
                        }
                    ],
                    "synonyms": [
                        "запор"
                    ],
                    "common_phrases": [
                        "дверной замок",
                        "навесной замок",
                        "закрыть на замок",
                        "взломать замок"
                    ],
                    "target_lang_definitions": {
                        "en": "A mechanical device for securing a door, drawer, or container, typically operated by a key or combination."

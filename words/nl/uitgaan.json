                },
                {
                    "sense_id": "c8257754-b71c-3bab-bbdd-0fdd3fc2c6f3",
                    "sense_definition": "Afgelopen zijn (van een bijeenkomst, school of kerkdienst) waardoor men weg mag.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen rennen naar huis zodra de school <uitgaat>.",
                            "target_lang_translations": {
                                "en": "The children run home as soon as school <gets out>.",
                                "ru": "Дети бегут домой, как только школа <заканчивается>."
                            }
                        },
                        {
                            "text": "We dronken koffie nadat de kerkdienst <uitging>.",
                            "target_lang_translations": {
                                "en": "We drank coffee after the church service <ended>.",
                                "ru": "Мы пили кофе после того, как церковная служба <закончилась>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de school gaat uit",
                        "de kerk gaat uit"
                    ],
                    "target_lang_definitions": {

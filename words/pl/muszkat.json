    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e689401-60cd-3e1e-99f4-b310ec4b3d37",
                    "sense_definition": "Słodkie, aromatyczne wino deserowe wytwarzane z winogron o tej samej nazwie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "wine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do deseru podano kieliszek schłodzonego <w>muszkatu</w>.",
                            "target_lang_translations": {
                                "ru": "К десерту подали бокал охлажденного <w>муската</w>."
                            }
                        },
                        {
                            "text": "Ten bułgarski <w>muszkat</w> charakteryzuje się wyjątkowym bukietem zapachowym.",
                            "target_lang_translations": {
                                "ru": "Этот болгарский <w>мускат</w> отличается исключительным ароматическим букетом."
                            }
                        }
                    ],
                    "synonyms": [
                        "muszkatel"
                    ],
                    "common_phrases": [
                        "słodki muszkat",
                        "kieliszek muszkatu",
                        "butelka muszkatu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкое ароматное десертное вино, производимое из одноименного сорта винограда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мускат",
                                "target_lang_sense_clarification": "вино"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "455547fb-a670-3da5-a988-7efb4830f18e",
                    "sense_definition": "Odmiana winogron o intensywnym zapachu, używana do produkcji win deserowych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej winnicy uprawia się głównie <w>muszkat</w> ze względu na sprzyjający klimat.",
                            "target_lang_translations": {
                                "ru": "На этом винограднике выращивают в основном <w>мускат</w> из-за благоприятного климата."
                            }
                        },
                        {
                            "text": "Owoce <w>muszkatu</w> są bardzo słodkie i mają charakterystyczny aromat.",
                            "target_lang_translations": {
                                "ru": "Ягоды <w>муската</w> очень сладкие и имеют характерный аромат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szczep muszkat",
                        "uprawa muszkatu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сорт винограда с интенсивным ароматом, используемый для производства десертных вин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мускат",
                                "target_lang_sense_clarification": "сорт винограда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94498cd4-bb47-3506-8ce8-4fa4a782ae01",
                    "sense_definition": "Nasiono muszkatołowca używane jako aromatyczna przyprawa kuchenna.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "spice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kucharz dodał szczyptę <w>muszkatu</w> do sosu beszamelowego.",
                            "target_lang_translations": {
                                "ru": "Повар добавил щепотку <w>мускатного ореха</w> в соус бешамель."
                            }
                        },
                        {
                            "text": "Świeżo starty <w>muszkat</w> ma znacznie intensywniejszy zapach niż ten z torebki.",
                            "target_lang_translations": {
                                "ru": "Свеженатертый <w>мускатный орех</w> имеет гораздо более интенсивный запах, чем тот, что из пакетика."
                            }
                        }
                    ],
                    "synonyms": [
                        "gałka muszkatołowa"
                    ],
                    "common_phrases": [
                        "szczypta muszkatu",
                        "starty muszkat",
                        "zapach muszkatu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Семя мускатного дерева, используемое в качестве ароматной кухонной приправы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мускатный орех",
                                "target_lang_sense_clarification": "пряность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "317a79b5-cb42-3317-b3c9-d6a041cd5e51",
                    "sense_definition": "Wiecznie zielone drzewo tropikalne, z którego nasion pozyskuje się przyprawę korzenną.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na plantacji rośnie stary <w>muszkat</w>, który wciąż owocuje.",
                            "target_lang_translations": {
                                "ru": "На плантации растет старый <w>мускатник</w>, который все еще плодоносит."
                            }
                        },
                        {
                            "text": "Liście <w>muszkatu</w> są ciemnozielone i błyszczące.",
                            "target_lang_translations": {
                                "ru": "Листья <w>мускатника</w> темно-зеленые и блестящие."
                            }
                        }
                    ],
                    "synonyms": [
                        "muszkatołowiec",
                        "muszkatowiec",
                        "muszkatołowiec korzenny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вечнозеленое тропическое дерево, из семян которого получают пряность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мускатник",
                                "target_lang_sense_clarification": "дерево"
                            },
                            {
                                "target_lang_word": "мускатное дерево",
                                "target_lang_sense_clarification": "ботаника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ceb793c-31be-3b04-bdaa-e438192e6d6e",
                    "sense_definition": "Regionalne określenie popularnej rośliny ozdobnej, znanej jako pelargonia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na parapecie u babci zawsze kwitł czerwony <w>muszkat</w>.",
                            "target_lang_translations": {
                                "ru": "На подоконнике у бабушки всегда цвела красная <w>герань</w>."
                            }
                        },
                        {
                            "text": "Muszę podlać <w>muszkaty</w>, bo ziemia w doniczkach jest już sucha.",
                            "target_lang_translations": {
                                "ru": "Мне нужно полить <w>герани</w>, так как земля в горшках уже сухая."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czerwony muszkat",
                        "kwitnący muszkat"
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5edffda2-7011-3cb4-85e6-ccc03bdc1c6c",
                    "sense_definition": "Stopień rozwoju, zaawansowania, umiejętności lub jakości czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "degree_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>poziom</w> znajomości języka angielskiego znacznie się poprawił w ostatnim roku.",
                            "target_lang_translations": {
                                "ru": "Мой <w>уровень</w> владения английским языком значительно улучшился за последний год."
                            }
                        },
                        {
                            "text": "Ten mecz piłkarski stał na bardzo wysokim <w>poziomie</w> sportowym.",
                            "target_lang_translations": {
                                "ru": "Этот футбольный матч был на очень высоком спортивном <w>уровне</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stopień",
                        "jakość"
                    ],
                    "common_phrases": [
                        "wysoki poziom",
                        "niski poziom",
                        "poziom życia",
                        "poziom trudności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень развития, продвинутости, навыков или качества чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "степень развития"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38bbf08b-8dc7-3405-8c7a-33c3970a3052",
                    "sense_definition": "Kondygnacja w budynku lub innej konstrukcji; piętro.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "structure_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Parking podziemny w tym centrum handlowym ma dwa <w>poziomy</w>.",
                            "target_lang_translations": {
                                "ru": "У подземной парковки в этом торговом центре два <w>уровня</w>."
                            }
                        },
                        {
                            "text": "Winda zatrzymała się na <w>poziomie</w> zero, gdzie znajduje się wyjście.",
                            "target_lang_translations": {
                                "ru": "Лифт остановился на нулевом <w>уровне</w>, где находится выход."
                            }
                        }
                    ],
                    "synonyms": [
                        "piętro",
                        "kondygnacja"
                    ],
                    "common_phrases": [
                        "na poziomie zero",
                        "wielopoziomowy parking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Этаж в здании или другой конструкции; ярус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "этаж, ярус"
                            },
                            {
                                "target_lang_word": "этаж",
                                "target_lang_sense_clarification": "в здании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8595a45-ca85-30a3-87a0-5b8dfe9912ae",
                    "sense_definition": "Jeden z etapów w grze komputerowej, który gracz musi ukończyć.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "game_stage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mogę przejść tego <w>poziomu</w>, ponieważ jest zbyt trudny.",
                            "target_lang_translations": {
                                "ru": "Я не могу пройти этот <w>уровень</w>, так как он слишком сложный."
                            }
                        },
                        {
                            "text": "Gracz musi zebrać wszystkie monety, aby odblokować następny <w>poziom</w>.",
                            "target_lang_translations": {
                                "ru": "Игрок должен собрать все монеты, чтобы разблокировать следующий <w>уровень</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "etap"
                    ],
                    "common_phrases": [
                        "przejść poziom",
                        "kolejny poziom",
                        "ostatni poziom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из этапов в компьютерной игре, который игрок должен пройти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "этап игры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88ddec1e-a33b-3301-aced-7fe511af3787",
                    "sense_definition": "Wartość mierzalnej wielkości, np. cieczy, dźwięku lub substancji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "degree_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ostatnich ulewach <w>poziom</w> wody w rzece niebezpiecznie wzrósł.",
                            "target_lang_translations": {
                                "ru": "После последних ливней <w>уровень</w> воды в реке опасно поднялся."
                            }
                        },
                        {
                            "text": "Musisz regularnie kontrolować <w>poziom</w> cukru we krwi dla swojego zdrowia.",
                            "target_lang_translations": {
                                "ru": "Ты должен регулярно контролировать <w>уровень</w> сахара в крови для своего здоровья."
                            }
                        }
                    ],
                    "synonyms": [
                        "wysokość"
                    ],
                    "common_phrases": [
                        "poziom wody",
                        "poziom morza",
                        "poziom hałasu",
                        "poziom cukru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Значение измеряемой величины, например, жидкости, звука или вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "показатель величины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc51bc5f-2c20-398f-807d-8cf687a180d2",
                    "sense_definition": "Ustawienie równoległe do podłoża lub powierzchni wody; przeciwieństwo pionu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "orientation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy sprawdzić <w>poziom</w> tej półki, żeby książki się nie zsuwały.",
                            "target_lang_translations": {
                                "ru": "Мы должны проверить <w>уровень</w> этой полки, чтобы книги не соскальзывали."
                            }
                        },
                        {
                            "text": "Budowlaniec użył specjalnego narzędzia, aby wyznaczyć idealny <w>poziom</w> podłogi.",
                            "target_lang_translations": {
                                "ru": "Строитель использовал специальный инструмент, чтобы определить идеальный <w>уровень</w> пола."
                            }
                        }
                    ],
                    "antonyms": [
                        "pion"
                    ],
                    "common_phrases": [
                        "trzymać poziom",
                        "wyznaczyć poziom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положение, параллельное земле или поверхности воды; противоположность вертикали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "горизонтальное положение"
                            },
                            {
                                "target_lang_word": "горизонталь",
                                "target_lang_sense_clarification": "линия уровня"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4bf7f81-56fc-3628-aa6d-6172bf4b5674",
                    "sense_definition": "Wysoki standard kultury osobistej, wykształcenia lub moralności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "personal_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To była bardzo ciekawa dyskusja na <w>poziomie</w>, pełna merytorycznych argumentów.",
                            "target_lang_translations": {
                                "ru": "Это была очень интересная дискуссия на <w>уровне</w>, полная содержательных аргументов."
                            }
                        },
                        {
                            "text": "Mimo trudnej sytuacji zachował się jak człowiek na <w>poziomie</w> i nie podniósł głosu.",
                            "target_lang_translations": {
                                "ru": "Несмотря на сложную ситуацию, он повел себя как человек на <w>уровне</w> и не повысил голос."
                            }
                        }
                    ],
                    "common_phrases": [
                        "na poziomie",
                        "człowiek na poziomie",
                        "rozrywka na poziomie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокий стандарт личной культуры, образования или морали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "высокий стандарт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e53aafe7-89b5-323f-8953-451702aaa657",
                    "sense_definition": "Warstwa skał osadowych o jednolitym składzie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "structure_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geolodzy badają ten <w>poziom</w> skalny w poszukiwaniu skamieniałości.",
                            "target_lang_translations": {
                                "ru": "Геологи изучают этот скальный <w>слой</w> в поисках окаменелостей."
                            }
                        },
                        {
                            "text": "W kopalni odkryto nowy <w>poziom</w> zawierający cenne surowce mineralne.",
                            "target_lang_translations": {
                                "ru": "В шахте открыли новый <w>горизонт</w>, содержащий ценные минеральные ресурсы."
                            }
                        }
                    ],
                    "synonyms": [
                        "warstwa"
                    ],
                    "common_phrases": [
                        "poziom wodonośny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой осадочных пород с однородным составом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горизонт",
                                "target_lang_sense_clarification": "геологический слой"
                            },
                            {
                                "target_lang_word": "слой",
                                "target_lang_sense_clarification": "пласт породы"
                            }
                        ]
                    }
                }
            ]
        }
    ]

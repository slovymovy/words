            "senses": [
                {
                    "sense_id": "769e5a7d-12ae-3a7e-92e9-116d9ae8ab1c",
                    "sense_definition": "Iemand die aan het doodgaan is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>stervende</w> kreeg het sacrament der zieken.",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ddf8920f-99dc-34cc-8dea-bd4c09a7b7c9",
                    "sense_definition": "Заставить работать, привести в действие (механизм, двигатель, компьютерную программу, процесс).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "activation_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель попытался <w>запустить</w> двигатель, но машина не заводилась.",
                            "target_lang_translations": {
                                "en": "The driver tried to <w>start</w> the engine, but the car wouldn't start."
                            }
                        },
                        {
                            "text": "Нам нужно <w>запустить</w> новую программу на компьютере.",
                            "target_lang_translations": {
                                "en": "We need to <w>run</w> a new program on the computer."
                            }
                        }
                    ],
                    "common_phrases": [
                        "запустить двигатель",
                        "запустить программу",
                        "запустить производство",
                        "запустить процесс"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something work, put into action (a mechanism, engine, computer program, or process)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "launch",
                                "target_lang_sense_clarification": "programs, projects, processes"
                            },
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "engines, mechanisms"
                            },
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "computer programs"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15767e62-1a08-3aae-a36c-5c72eae88b51",
                    "sense_definition": "Отправить в полёт (ракету, космический аппарат, воздушного змея).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "activation_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учёные готовятся <w>запустить</w> спутник на орбиту Земли.",
                            "target_lang_translations": {
                                "en": "Scientists are preparing to <w>launch</w> a satellite into Earth's orbit."
                            }
                        },
                        {
                            "text": "В выходные мы с детьми ходили <w>запустить</w> воздушного змея.",
                            "target_lang_translations": {
                                "en": "On the weekend, the children and I went to <w>fly</w> a kite."
                            }
                        }
                    ],
                    "common_phrases": [
                        "запустить ракету",
                        "запустить спутник",
                        "запустить змея",
                        "запустить фейерверк"
                    ],
                    "target_lang_definitions": {
                        "en": "To send into flight (a rocket, spacecraft, or kite)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "launch",
                                "target_lang_sense_clarification": "rockets, satellites"
                            },
                            {
                                "target_lang_word": "fly",
                                "target_lang_sense_clarification": "kites"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecd56142-2a56-3245-b77f-8973a5f7eb9f",
                    "sense_definition": "Перестать заботиться о чём-либо, оставить без внимания; позволить болезни развиться из-за отсутствия лечения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "neglect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если <w>запустить</w> простуду, могут возникнуть серьёзные осложнения.",
                            "target_lang_translations": {
                                "en": "If you <w>neglect</w> a cold, serious complications may arise."
                            }
                        },
                        {
                            "text": "Хозяева уехали и совсем решили <w>запустить</w> свой сад.",
                            "target_lang_translations": {
                                "en": "The owners left and decided to completely <w>neglect</w> their garden."
                            }
                        }
                    ],
                    "antonyms": [
                        "обиходить"
                    ],
                    "common_phrases": [
                        "запустить болезнь",
                        "запустить учёбу",
                        "запустить дела",
                        "всё запустить"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop caring about something, leave without attention; to allow a disease to develop due to lack of treatment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "neglect",
                                "target_lang_sense_clarification": "health, duties, appearance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58a02268-c67b-35d0-9368-e00b67970dd0",
                    "sense_definition": "Глубоко сунуть, поместить внутрь чего-либо (обычно руку, пальцы или когти).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_insertion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он любил <w>запустить</w> руку в мешок с конфетами и выбрать самую большую.",
                            "target_lang_translations": {
                                "en": "He loved to <w>thrust</w> his hand into the bag of candy and pick the biggest one."
                            }
                        },
                        {
                            "text": "Кот умудрился <w>запустить</w> когти в мягкую обивку дивана.",
                            "target_lang_translations": {
                                "en": "The cat managed to <w>sink</w> its claws into the soft upholstery of the sofa."
                            }
                        }
                    ],
                    "common_phrases": [
                        "запустить руку в карман",
                        "запустить пальцы в волосы",
                        "запустить когти",
                        "запустить ложку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in expressive contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To push or place deep inside something (usually a hand, fingers, or claws)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thrust",
                                "target_lang_sense_clarification": "hand or fingers"
                            },
                            {
                                "target_lang_word": "stick",
                                "target_lang_sense_clarification": "informal; hand or fingers"
                            },
                            {
                                "target_lang_word": "sink",
                                "target_lang_sense_clarification": "claws"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c3d9a8c-6683-38cd-b187-f6961dda035b",
                    "sense_definition": "Разрешить войти, впустить кого-либо внутрь помещения.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "admittance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охранник согласился <w>запустить</w> нас в здание только после проверки документов.",
                            "target_lang_translations": {
                                "en": "The guard agreed to <w>let</w> us <w>in</w> the building only after checking our documents."
                            }
                        },
                        {
                            "text": "Нужно скорее <w>запустить</w> кошку домой, на улице холодно.",
                            "target_lang_translations": {
                                "en": "We need to <w>let</w> the cat <w>in</w> the house quickly; it's cold outside."
                            }
                        }
                    ],
                    "common_phrases": [
                        "запустить гостей",
                        "запустить в дом",
                        "запустить зрителей"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "standard word is впустить"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To allow to enter, to let someone inside a premise."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let in",
                                "target_lang_sense_clarification": "people, animals"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dca5c57b-6a8b-3711-bfb3-0523ab8fb3ca",
                    "sense_definition": "Бросить, кинуть что-либо с силой (обычно в кого-то или во что-то).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "throwing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рассерженный мальчик хотел <w>запустить</w> камнем в забор.",
                            "target_lang_translations": {
                                "en": "The angry boy wanted to <w>throw</w> a stone at the fence."
                            }
                        },
                        {
                            "text": "В него могли <w>запустить</w> подушкой во время дружеской драки.",
                            "target_lang_translations": {
                                "en": "They might <w>throw</w> a pillow at him during a friendly fight."
                            }
                        }
                    ],
                    "synonyms": [
                        "швырнуть",
                        "зашвырнуть",
                        "запульнуть"
                    ],
                    "common_phrases": [
                        "запустить камнем",
                        "запустить снежком",
                        "запустить тапком"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To throw or cast something with force (usually at someone or something)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throw",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "hurl",
                                "target_lang_sense_clarification": "with great force"
                            },
                            {
                                "target_lang_word": "fling",
                                "target_lang_sense_clarification": "carelessly or violently"
                            }
                        ]
                    }
                }
            ]
        }
    ]

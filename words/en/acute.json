                },
                {
                    "sense_id": "0d5c6247-d577-3840-8841-a12d3e406c44",
                    "sense_definition": "Describing a disease or condition that develops quickly and is severe, but lasts for a short time.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient was hospitalized with an <w>acute</w> infection that required immediate antibiotics.",
                            "target_lang_translations": {
                                "ru": "Пациента госпитализировали с <w>острой</w> инфекцией, требующей немедленного приема антибиотиков."
                            }
                        },
                        {
                            "text": "Unlike chronic back pain, <w>acute</w> pain usually goes away once the injury heals.",
                            "target_lang_translations": {
                                "ru": "В отличие от хронической боли в спине, <w>острая</w> боль обычно проходит, как только травма заживает."
                            }
                        }
                    ],
                    "synonyms": [
                        "sudden",
                        "emergent"
                    ],
                    "antonyms": [
                        "chronic"
                    ],
                    "common_phrases": [
                        "acute illness",
                        "acute infection",
                        "acute respiratory syndrome"
                    ],

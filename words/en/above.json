{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "f656cd44-c7d6-3ab2-9bef-fdfaa7dda041",
                    "sense_definition": "In a higher position than something else.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The plane flew high <w>above</w> the clouds.",
                            "target_lang_translations": {
                                "ru": "Самолёт летел высоко <w>над</w> облаками."
                            }
                        },
                        {
                            "text": "Please sign your name on the line <w>above</w> the date.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, распишитесь на строке <w>над</w> датой."
                            }
                        }
                    ],
                    "antonyms": [
                        "below",
                        "beneath"
                    ],
                    "common_phrases": [
                        "above sea level",
                        "above ground",
                        "above the clouds"
                    ],
                    "target_lang_definitions": {
                        "ru": "В более высоком положении, чем что-то другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "над",
                                "target_lang_sense_clarification": "предлог; указывает на положение поверх или выше чего-либо"
                            },
                            {
                                "target_lang_word": "выше",
                                "target_lang_sense_clarification": "на более высоком уровне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01ef8d00-6fc1-3c08-9073-1cbb6f301365",
                    "sense_definition": "More than a particular amount, number, or level.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "measure_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The temperature is expected to be <w>above</w> freezing today.",
                            "target_lang_translations": {
                                "ru": "Ожидается, что температура сегодня будет <w>выше</w> нуля."
                            }
                        },
                        {
                            "text": "Her grades in science are well <w>above</w> average.",
                            "target_lang_translations": {
                                "ru": "Её оценки по естественным наукам значительно <w>выше</w> среднего."
                            }
                        }
                    ],
                    "antonyms": [
                        "below"
                    ],
                    "common_phrases": [
                        "above average",
                        "above expectations",
                        "over and above"
                    ],
                    "target_lang_definitions": {
                        "ru": "Больше определенного количества, числа или уровня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выше",
                                "target_lang_sense_clarification": "больше чем; превышая уровень"
                            },
                            {
                                "target_lang_word": "свыше",
                                "target_lang_sense_clarification": "более; часто используется с числами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b1dc00c-00cb-3839-9ffd-3329d22ff316",
                    "sense_definition": "Higher in rank, authority, or importance than someone.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "rank_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A general ranks <w>above</w> a colonel in the army.",
                            "target_lang_translations": {
                                "ru": "Генерал по званию <w>выше</w> полковника."
                            }
                        },
                        {
                            "text": "We must follow the instructions sent from <w>above</w> us.",
                            "target_lang_translations": {
                                "ru": "Мы должны следовать инструкциям, спущенным <w>сверху</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "below"
                    ],
                    "common_phrases": [
                        "head and shoulders above",
                        "above the law"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выше по званию, авторитету или важности, чем кто-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выше",
                                "target_lang_sense_clarification": "по положению или рангу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "128c5e84-8775-338e-80cf-8e92c180161c",
                    "sense_definition": "More important than or in preference to something else.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "measure_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She values honesty <w>above</w> all other qualities.",
                            "target_lang_translations": {
                                "ru": "Она ценит честность <w>превыше</w> всех остальных качеств."
                            }
                        },
                        {
                            "text": "We need to prioritize safety <w>above</w> speed.",
                            "target_lang_translations": {
                                "ru": "Нам нужно ставить безопасность <w>выше</w> скорости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "above all",
                        "above everything else"
                    ],
                    "target_lang_definitions": {
                        "ru": "Важнее или предпочтительнее чего-то другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "превыше",
                                "target_lang_sense_clarification": "важнее всего; превосходя всё остальное"
                            },
                            {
                                "target_lang_word": "выше",
                                "target_lang_sense_clarification": "более важно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e744740-74c7-3276-b05a-830103c5f589",
                    "sense_definition": "Too good or honest to do something; not affected by something negative.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "moral_exemption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>above</w> lying to his friends to get what he wants.",
                            "target_lang_translations": {
                                "ru": "Он <w>выше</w> того, чтобы лгать друзьям ради получения желаемого."
                            }
                        },
                        {
                            "text": "The politician claimed to be <w>above</w> suspicion regarding the scandal.",
                            "target_lang_translations": {
                                "ru": "Политик заявил, что он <w>выше</w> подозрений в связи со скандалом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "above suspicion",
                        "above reproach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слишком хороший или честный, чтобы делать что-то; не подверженный чему-то негативному."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выше",
                                "target_lang_sense_clarification": "морально выше чего-либо; вне досягаемости"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "92a0f93b-91ad-3b41-abe6-6bd263b4688f",
                    "sense_definition": "At or to a higher place; overhead.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The birds circled high <w>above</w> in the sky.",
                            "target_lang_translations": {
                                "ru": "Птицы кружили высоко <w>вверху</w> в небе."
                            }
                        },
                        {
                            "text": "We heard loud noises coming from the apartment <w>above</w>.",
                            "target_lang_translations": {
                                "ru": "Мы слышали громкий шум из квартиры <w>сверху</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "below"
                    ],
                    "common_phrases": [
                        "from above",
                        "high above"
                    ],
                    "target_lang_definitions": {
                        "ru": "В более высоком месте или по направлению вверх; над головой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наверху",
                                "target_lang_sense_clarification": "в верхней части; этажом выше"
                            },
                            {
                                "target_lang_word": "вверху",
                                "target_lang_sense_clarification": "на высоте; над головой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b923b220-859e-35f6-bf82-449964a77f6e",
                    "sense_definition": "Mentioned earlier in a piece of writing.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "textual_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As stated <w>above</w>, the meeting has been cancelled.",
                            "target_lang_translations": {
                                "ru": "Как сказано <w>выше</w>, встреча отменена."
                            }
                        },
                        {
                            "text": "See the diagram <w>above</w> for more details on the structure.",
                            "target_lang_translations": {
                                "ru": "См. диаграмму <w>выше</w> для получения подробностей о структуре."
                            }
                        }
                    ],
                    "antonyms": [
                        "below"
                    ],
                    "common_phrases": [
                        "as mentioned above",
                        "see above"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упомянуто ранее в тексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выше",
                                "target_lang_sense_clarification": "ранее по тексту"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7690babb-027b-3ed9-aafb-6f779fba7c3c",
                    "sense_definition": "Mentioned or appearing earlier in the same text.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "textual_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please read the <w>above</w> statement carefully before signing.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, внимательно прочитайте <w>вышеупомянутое</w> заявление перед подписанием."
                            }
                        },
                        {
                            "text": "The <w>above</w> address is incorrect and needs to be updated.",
                            "target_lang_translations": {
                                "ru": "<w>Вышеуказанный</w> адрес неверен и требует обновления."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the above example",
                        "the above statement"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Упомянутый или появляющийся ранее в том же тексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вышеупомянутый",
                                "target_lang_sense_clarification": "упомянутый ранее"
                            },
                            {
                                "target_lang_word": "вышеуказанный",
                                "target_lang_sense_clarification": "указанный ранее"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "86dec2cc-7e43-3160-b9dc-863545f9b1a6",
                    "sense_definition": "The person or thing mentioned previously.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "textual_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If none of the <w>above</w> applies to you, leave the box blank.",
                            "target_lang_translations": {
                                "ru": "Если ничего из <w>вышеперечисленного</w> к вам не относится, оставьте поле пустым."
                            }
                        },
                        {
                            "text": "Please select all of the <w>above</w> if you agree with every point.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, выберите всё <w>вышеперечисленное</w>, если вы согласны с каждым пунктом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "none of the above",
                        "all of the above"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или предмет, упомянутые ранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вышеперечисленное",
                                "target_lang_sense_clarification": "то, что перечислено выше (для списков)"
                            },
                            {
                                "target_lang_word": "вышесказанное",
                                "target_lang_sense_clarification": "то, что сказано выше (для утверждений)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
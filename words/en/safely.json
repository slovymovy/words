            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c89ecdc9-b9b0-3325-b15e-c6bf17937a1d",
                    "sense_definition": "In a careful way to avoid danger or risk.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please drive <w>safely</w> on the icy roads tonight.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, ведите машину <w>осторожно</w> сегодня вечером на обледенелых дорогах."
                            }
                        },
                        {
                            "text": "The workers learned how to handle the chemicals <w>safely</w>.",
                            "target_lang_translations": {
                                "ru": "Рабочие учились обращаться с химикатами <w>безопасно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cautiously",
                        "carefully"
                    ],
                    "antonyms": [
                        "dangerously",
                        "recklessly"
                    ],
                    "common_phrases": [
                        "drive safely",
                        "play it safely",
                        "act safely"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осторожным образом, чтобы избежать опасности или риска."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безопасно",
                                "target_lang_sense_clarification": "без риска"
                            },
                            {
                                "target_lang_word": "осторожно",
                                "target_lang_sense_clarification": "внимательно, с предосторожностью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59dd3444-b42c-396d-aefd-88db6fd92aad",
                    "sense_definition": "Without being harmed, damaged, or lost; in a secure position or state.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We were relieved to hear that the plane had landed <w>safely</w>.",
                            "target_lang_translations": {
                                "ru": "Мы с облегчением узнали, что самолет приземлился <w>благополучно</w>."
                            }
                        },
                        {
                            "text": "Make sure the important documents are filed <w>safely</w> away.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что важные документы <w>надёжно</w> убраны на хранение."
                            }
                        }
                    ],
                    "synonyms": [
                        "securely",
                        "unharmed"
                    ],
                    "antonyms": [
                        "dangerously"
                    ],
                    "common_phrases": [
                        "arrive safely",
                        "return safely",
                        "safely home",
                        "safely tucked away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без вреда, повреждений или потерь; в надежном положении или состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благополучно",
                                "target_lang_sense_clarification": "без происшествий, успешно"
                            },
                            {
                                "target_lang_word": "надёжно",
                                "target_lang_sense_clarification": "прочно, в сохранности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

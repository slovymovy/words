{
    "word_family": [
        "lemieszowy",
        "lemieszek"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b5976a08-0c1e-3608-b332-6410b3ca3a94",
                    "sense_definition": "Dolna, ostra część korpusu pługa, która odcina skibę ziemi od spodu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plough_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik naostrzył stary <w>lemiesz</w> przed rozpoczęciem wiosennej orki.",
                            "target_lang_translations": {
                                "ru": "Фермер наточил старый <w>лемех</w> перед началом весенней пахоты."
                            }
                        },
                        {
                            "text": "Pług z nowym <w>lemieszem</w> głęboko wchodził w twardą glebę.",
                            "target_lang_translations": {
                                "ru": "Плуг с новым <w>лемехом</w> глубоко входил в твердую почву."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ostry lemiesz",
                        "stalowy lemiesz",
                        "wymiana lemiesza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижняя, острая часть корпуса плуга, которая подрезает пласт земли снизу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лемех",
                                "target_lang_sense_clarification": "часть плуга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8d0e17a-bea8-3460-ae7a-11aed1e555f5",
                    "sense_definition": "Element roboczy maszyn budowlanych lub drogowych (np. spycharek, pługów śnieżnych), służący do przesuwania i wyrównywania materiału.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "machine_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Operator spycharki opuścił <w>lemiesz</w>, aby usunąć zwałowisko ziemi.",
                            "target_lang_translations": {
                                "ru": "Оператор бульдозера опустил <w>отвал</w>, чтобы убрать насыпь земли."
                            }
                        },
                        {
                            "text": "Zimą drogowcy montują gumowy <w>lemiesz</w> do bezpiecznego odśnieżania ulic.",
                            "target_lang_translations": {
                                "ru": "Зимой дорожники устанавливают резиновый <w>нож</w> для безопасной очистки улиц от снега."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lemiesz spycharki",
                        "lemiesz pługa śnieżnego",
                        "gumowy lemiesz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рабочий элемент строительных или дорожных машин (например, бульдозеров, снегоочистителей), служащий для перемещения и выравнивания материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвал",
                                "target_lang_sense_clarification": "рабочий орган бульдозера или снегоочистителя"
                            },
                            {
                                "target_lang_word": "нож",
                                "target_lang_sense_clarification": "режущая кромка отвала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c8322b7-f8ab-37a4-beff-e419af623e5b",
                    "sense_definition": "Nieparzysta, płaska kość twarzoczaszki, która buduje tylną i dolną część przegrody nosowej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas badania tomograficznego lekarz ocenił stan <w>lemiesza</w> pacjenta.",
                            "target_lang_translations": {
                                "ru": "Во время томографии врач оценил состояние <w>сошника</w> пациента."
                            }
                        },
                        {
                            "text": "<w>Lemiesz</w> łączy się z kością sitową oraz szczęką.",
                            "target_lang_translations": {
                                "ru": "<w>Сошник</w> соединяется с решетчатой костью и верхней челюстью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kość lemiesza",
                        "złamanie lemiesza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непарная плоская кость лицевого черепа, образующая заднюю и нижнюю часть носовой перегородки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сошник",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbbc7cd7-7c02-3eba-844a-54c8f25d1bb0",
                    "sense_definition": "Element ogonowej części łoża działa, wbijany w ziemię w celu stabilizacji i hamowania odrzutu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze musieli mocno wbić <w>lemiesze</w> w grunt, aby działo stało stabilnie.",
                            "target_lang_translations": {
                                "ru": "Солдаты должны были глубоко забить <w>сошники</w> в грунт, чтобы орудие стояло устойчиво."
                            }
                        },
                        {
                            "text": "Po oddaniu salwy sprawdzono, czy <w>lemiesz</w> nie uległ uszkodzeniu.",
                            "target_lang_translations": {
                                "ru": "После залпа проверили, не поврежден ли <w>сошник</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lemiesz ogonowy",
                        "wbić lemiesz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент хвостовой части лафета орудия, заглубляемый в землю для стабилизации и торможения отката."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сошник",
                                "target_lang_sense_clarification": "артиллерия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "6e402a28-f470-32c7-8b35-c6354f16c3b8",
                    "sense_definition": "Polskie nazwisko męskie lub żeńskie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Wczoraj spotkałem pana <w>Lemiesza</w> w sklepie ogrodniczym.",
                            "target_lang_translations": {
                                "ru": "Вчера я встретил господина <w>Лемеша</w> в садовом магазине."
                            }
                        },
                        {
                            "text": "To odpowiedzialne zadanie zostało przydzielone dyrektorowi <w>Lemieszowi</w>.",
                            "target_lang_translations": {
                                "ru": "Эта ответственная задача была поручена директору <w>Лемешу</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия (мужская или женская)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лемеш",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
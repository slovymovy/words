            "pos": "noun",
            "senses": [
                {
                    "sense_id": "674b053a-e0fd-3e88-8522-5b4f14a28e96",
                    "sense_definition": "Орудие труда, механизм или техническое приспособление, используемое для выполнения какой-либо работы.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужен подходящий <инструмент>, чтобы починить этот стул.",
                            "target_lang_translations": {
                                "en": "I need a suitable <tool> to fix this chair."
                            }
                        },
                        {
                            "text": "Бензиновый триммер — это очень удобный <инструмент> для ухода за садом.",
                            "target_lang_translations": {
                                "en": "A gas trimmer is a very convenient <tool> for garden care."
                            }
                        },
                        {
                            "text": "Её удивила небрежность, с которой были разбросаны рабочие <инструменты>.",
                            "target_lang_translations": {
                                "en": "She was surprised by the carelessness with which the work <tools> were scattered."
                            }
                        }
                    ],
                    "synonyms": [
                        "орудие",
                        "приспособле́ние"
                    ],
                    "common_phrases": [
                        "рабочий инструмент",
                        "набор инструментов",
                        "садовый инструмент",
                        "взять инструмент"
                    ],
                    "target_lang_definitions": {
                        "en": "A physical device, implement, or machine used to carry out a specific task or work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tool",
                                "target_lang_sense_clarification": "general purpose"
                            },
                            {
                                "target_lang_word": "instrument",
                                "target_lang_sense_clarification": "precision or scientific context"
                            },
                            {
                                "target_lang_word": "implement",
                                "target_lang_sense_clarification": "agricultural or formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdb59904-1cac-3b15-a8cc-9be9c85f3373",
                    "sense_definition": "Предмет, специально созданный или приспособленный для извлечения музыкальных звуков и исполнения музыки.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Скрипка — это очень сложный и красивый музыкальный <инструмент>.",
                            "target_lang_translations": {
                                "en": "The violin is a very complex and beautiful musical <instrument>."
                            }
                        },
                        {
                            "text": "В оркестре зазвучали новые <инструменты>, и мелодия стала громче.",
                            "target_lang_translations": {
                                "en": "New <instruments> began to play in the orchestra, and the melody became louder."
                            }
                        }
                    ],
                    "synonyms": [
                        "музыкальный инструмент"
                    ],
                    "common_phrases": [
                        "музыкальный инструмент",
                        "играть на инструменте",
                        "духовой инструмент",
                        "струнный инструмент",
                        "настроить инструмент"
                    ],
                    "target_lang_definitions": {

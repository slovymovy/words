                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "to understand"
                            },
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "to notice or hear"
                            },
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "to realize"

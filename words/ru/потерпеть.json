{
    "word_family": [
        "терпение",
        "терпеливый",
        "терпимый",
        "терпеть",
        "вытерпеть",
        "перетерпеть",
        "стерпеть"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "664acc6e-4c3d-383a-ba30-8b0355237694",
                    "sense_definition": "Проявить терпение в течение некоторого времени; подождать, мирясь с неудобствами или болью.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "wait_endure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тебе нужно <потерпеть> ещё пять минут, и мы приедем.",
                            "target_lang_translations": {
                                "en": "You need to <be patient> for five more minutes, and we will arrive."
                            }
                        },
                        {
                            "text": "Врач сказал <потерпеть>, пока подействует лекарство.",
                            "target_lang_translations": {
                                "en": "The doctor said to <endure> until the medicine takes effect."
                            }
                        }
                    ],
                    "synonyms": [
                        "снести"
                    ],
                    "common_phrases": [
                        "немного потерпеть",
                        "потерпеть боль",
                        "прошу потерпеть"
                    ],
                    "target_lang_definitions": {
                        "en": "To show patience for a certain time; to wait while accepting inconvenience or pain."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be patient",
                                "target_lang_sense_clarification": "to wait calmly for a short time"
                            },
                            {
                                "target_lang_word": "endure",
                                "target_lang_sense_clarification": "to bear pain or discomfort temporarily"
                            },
                            {
                                "target_lang_word": "hold on",
                                "target_lang_sense_clarification": "informal, to wait or persist"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19de8485-fd64-39d2-899a-8bf9f05335dc",
                    "sense_definition": "Испытать или пережить что-либо неприятное, тяжелое (обычно о неудачах, бедствиях или убытках).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "suffer_undergo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша команда <потерпела> поражение в финальном матче.",
                            "target_lang_translations": {
                                "en": "Our team <suffered> a defeat in the final match."
                            }
                        },
                        {
                            "text": "Во время шторма судно <потерпело> крушение.",
                            "target_lang_translations": {
                                "en": "During the storm, the ship <suffered> a shipwreck."
                            }
                        }
                    ],
                    "common_phrases": [
                        "потерпеть поражение",
                        "потерпеть неудачу",
                        "потерпеть крах",
                        "потерпеть убытки",
                        "потерпеть аварию"
                    ],
                    "target_lang_definitions": {
                        "en": "To experience or undergo something unpleasant or heavy (usually regarding failures, disasters, or losses)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suffer",
                                "target_lang_sense_clarification": "defeat, failure, or disaster"
                            },
                            {
                                "target_lang_word": "sustain",
                                "target_lang_sense_clarification": "losses or damage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30ea5e01-455e-3edd-8d13-168fbeb873ac",
                    "sense_definition": "Допустить что-либо, смириться с чем-либо неприятным; не воспротивиться чему-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tolerate_allow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не намерен <потерпеть> такое неуважительное отношение.",
                            "target_lang_translations": {
                                "en": "I do not intend to <tolerate> such a disrespectful attitude."
                            }
                        },
                        {
                            "text": "Мы больше не можем <потерпеть> этот беспорядок в доме.",
                            "target_lang_translations": {
                                "en": "We can no longer <put up with> this mess in the house."
                            }
                        }
                    ],
                    "synonyms": [
                        "стерпеть",
                        "снести"
                    ],
                    "common_phrases": [
                        "этого не потерплю",
                        "потерпеть обиду"
                    ],
                    "target_lang_definitions": {
                        "en": "To allow something, to reconcile oneself with something unpleasant; not to oppose something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tolerate",
                                "target_lang_sense_clarification": "to allow or accept behavior"
                            },
                            {
                                "target_lang_word": "stand for",
                                "target_lang_sense_clarification": "usually negative: to accept or permit"
                            },
                            {
                                "target_lang_word": "put up with",
                                "target_lang_sense_clarification": "to accept an annoying situation"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3f877993-d78c-3a7d-ad49-937a9e275f55",
                    "sense_definition": "Zmieniać swoje miejsce pobytu, przemieszczać się z jednego miejsca na drugie lub wykonywać ruchy ciałem (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek <porusza> się powoli o lasce.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка медленно <передвигается> с тростью."
                            }
                        },
                        {
                            "text": "W tym dużym mieście najlepiej <poruszać> się metrem.",
                            "target_lang_translations": {
                                "ru": "В этом большом городе лучше всего <передвигаться> на метро."
                            }
                        }
                    ],
                    "synonyms": [
                        "przemieszczać się",
                        "przemieszczać się",
                        "wzruszać"
                    ],
                    "antonyms": [
                        "stać",
                        "leżeć",
                        "siedzieć"
                    ],
                    "common_phrases": [
                        "poruszać się pieszo",
                        "poruszać się samochodem",
                        "szybko się poruszać",
                        "poruszać się z trudem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Менять свое местоположение, перемещаться с одного места на другое или совершать движения телом (используется с возвратным местоимением \"się\")."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передвигаться",
                                "target_lang_sense_clarification": "перемещаться в пространстве"
                            },
                            {
                                "target_lang_word": "двигаться",
                                "target_lang_sense_clarification": "совершать движения, быть в движении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41ebf37f-8129-39cf-a7ee-2fb6650e1250",
                    "sense_definition": "Sprawiać, że ktoś lub coś zmienia swoje położenie albo zaczyna wykonywać ruchy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiatr zaczął mocno <poruszać> gałęziami drzew.",
                            "target_lang_translations": {
                                "ru": "Ветер начал сильно <шевелить> ветками деревьев."
                            }
                        },
                        {
                            "text": "Musisz delikatnie <poruszać> anteną, żeby złapać sygnał.",
                            "target_lang_translations": {
                                "ru": "Нужно осторожно <подвигать> антенну, чтобы поймать сигнал."
                            }
                        }
                    ],
                    "synonyms": [
                        "przemieszczać się",
                        "wzruszać"
                    ],
                    "antonyms": [
                        "stać",
                        "leżeć",
                        "siedzieć"
                    ],
                    "common_phrases": [
                        "poruszać głową",
                        "poruszać rękami",
                        "poruszać ustami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить в движение, заставлять кого-то или что-то менять положение или начинать двигаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двигать",
                                "target_lang_sense_clarification": "перемещать предмет"
                            },
                            {
                                "target_lang_word": "шевелить",
                                "target_lang_sense_clarification": "слегка двигать чем-либо (напр. пальцами, губами)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "535bf45a-fd46-38e3-8931-72a09a1bbe55",
                    "sense_definition": "Rozpoczynać rozmowę na jakiś temat lub wspominać o czymś w trakcie dyskusji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "discussion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na dzisiejszym zebraniu musimy <poruszyć> kwestię budżetu.",
                            "target_lang_translations": {
                                "ru": "На сегодняшнем собрании мы должны <поднять> вопрос бюджета."
                            }
                        },
                        {
                            "text": "Nie chciałem <poruszać> tego trudnego tematu przy dzieciach.",
                            "target_lang_translations": {
                                "ru": "Я не хотел <затрагивать> эту трудную тему при детях."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzruszać"
                    ],
                    "common_phrases": [
                        "poruszać temat",
                        "poruszać kwestię",
                        "poruszać problem",
                        "poruszać wątek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать разговор на какую-либо тему или упоминать о чем-то в ходе дискуссии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затрагивать",
                                "target_lang_sense_clarification": "касаться темы в разговоре"
                            },
                            {
                                "target_lang_word": "поднимать",
                                "target_lang_sense_clarification": "выносить вопрос на обсуждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee4ca1b6-0cf3-3cee-8983-4af43c244933",
                    "sense_definition": "Wywoływać u kogoś silne emocje, uczucia lub wzruszenie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego smutna historia bardzo mnie <poruszyła>.",
                            "target_lang_translations": {
                                "ru": "Его грустная история меня очень <тронула>."
                            }
                        },
                        {
                            "text": "Ten film dokumentalny <porusza> serca widzów na całym świecie.",
                            "target_lang_translations": {
                                "ru": "Этот документальный фильм <трогает> сердца зрителей во всем мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzruszać"
                    ],
                    "common_phrases": [
                        "głęboko poruszać",
                        "historia porusza",
                        "poruszać do łez",
                        "poruszać wyobraźnię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать у кого-то сильные эмоции, чувства или волнение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трогать",
                                "target_lang_sense_clarification": "вызывать умиление или сочувствие"
                            },
                            {
                                "target_lang_word": "волновать",
                                "target_lang_sense_clarification": "вызывать переживания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
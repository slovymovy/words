{
    "word_family": [
        "beschrijving",
        "beschrijfbaar"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ca304f40-df4f-34b7-a0c0-d5e5b3b7c416",
                    "sense_definition": "Vertellen of opschrijven hoe iets of iemand eruitziet, is of wat er gebeurd is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je de man <w>beschrijven</w> die je gisteren bij de winkel hebt gezien?",
                            "target_lang_translations": {
                                "en": "Can you <w>describe</w> the man you saw at the store yesterday?"
                            }
                        },
                        {
                            "text": "In haar dagboek <w>beschrijft</w> ze precies hoe ze zich die dag voelde.",
                            "target_lang_translations": {
                                "en": "In her diary, she <w>describes</w> exactly how she felt that day."
                            }
                        },
                        {
                            "text": "Het is moeilijk om de schoonheid van het uitzicht met woorden te <w>beschrijven</w>.",
                            "target_lang_translations": {
                                "en": "It is difficult to <w>describe</w> the beauty of the view with words."
                            }
                        }
                    ],
                    "synonyms": [
                        "schetsen",
                        "afschilderen",
                        "typeren",
                        "verwoorden"
                    ],
                    "common_phrases": [
                        "in detail beschrijven",
                        "de situatie beschrijven",
                        "kort beschrijven",
                        "een gevoel beschrijven"
                    ],
                    "target_lang_definitions": {
                        "en": "To tell or write down what someone or something looks like, is, or what happened."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "describe",
                                "target_lang_sense_clarification": "to represent in words"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fc4104f-ca6e-35fc-bc0b-16a9d11ec386",
                    "sense_definition": "Tekst schrijven op een oppervlak, zoals papier.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "writing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De monniken hebben in de middeleeuwen vele vellen perkament <w>beschreven</w>.",
                            "target_lang_translations": {
                                "en": "The monks <w>wrote on</w> many sheets of parchment in the Middle Ages."
                            }
                        },
                        {
                            "text": "Hij pakte een pen en begon de lege bladzijde te <w>beschrijven</w>.",
                            "target_lang_translations": {
                                "en": "He took a pen and started to <w>write on</w> the empty page."
                            }
                        }
                    ],
                    "synonyms": [
                        "volschrijven"
                    ],
                    "common_phrases": [
                        "een beschreven blad"
                    ],
                    "target_lang_definitions": {
                        "en": "To write text onto a surface, such as paper."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "write on",
                                "target_lang_sense_clarification": "to add text to a surface"
                            },
                            {
                                "target_lang_word": "cover with writing",
                                "target_lang_sense_clarification": "to fill a surface with text"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "103f2509-5b9b-33d3-8ab2-381d5b9a012a",
                    "sense_definition": "Een beweging maken in de vorm van een bepaalde lijn of figuur.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "motion_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aarde <w>beschrijft</w> in een jaar tijd een baan rond de zon.",
                            "target_lang_translations": {
                                "en": "The earth <w>describes</w> an orbit around the sun in a year's time."
                            }
                        },
                        {
                            "text": "Met een groots gebaar <w>beschreef</w> de dirigent een boog in de lucht.",
                            "target_lang_translations": {
                                "en": "With a grand gesture, the conductor <w>described</w> an arc in the air."
                            }
                        }
                    ],
                    "synonyms": [
                        "vormen"
                    ],
                    "common_phrases": [
                        "een baan beschrijven",
                        "een cirkel beschrijven",
                        "een boog beschrijven"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a movement that follows the shape of a specific line or figure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "describe",
                                "target_lang_sense_clarification": "to trace a shape or path"
                            },
                            {
                                "target_lang_word": "trace",
                                "target_lang_sense_clarification": "to follow a path or line"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1893df87-230f-3c67-b218-f92a3a717a61",
                    "sense_definition": "Czynność polegająca na połykaniu płynu w celu zaspokojenia pragnienia; rzeczownik odczasownikowy od słowa <w>pić</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regularne <picie> dużej ilości wody jest kluczowe dla zdrowia.",
                            "target_lang_translations": {
                                "ru": "Регулярное <питьё> большого количества воды является ключевым для здоровья."
                            }
                        },
                        {
                            "text": "Lekarz zalecił pacjentowi powolne <picie> ciepłych naparów ziołowych.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал пациенту медленное <питьё> тёплых травяных настоев."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypijanie"
                    ],
                    "antonyms": [
                        "niepicie"
                    ],
                    "common_phrases": [
                        "picie wody",
                        "picie herbaty",
                        "picie kawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в глотании жидкости для утоления жажды; отглагольное существительное от слова <w>pić</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "питьё",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "питие",
                                "target_lang_sense_clarification": "устар./книжн."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c1c26d5-9922-3d48-aaf5-f3a98d3c82d1",
                    "sense_definition": "Potoczne określenie dowolnego płynu przeznaczonego do spożycia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy wziąłeś ze sobą jakieś <picie> na naszą wycieczkę?",
                            "target_lang_translations": {
                                "ru": "Ты взял с собой какое-нибудь <питьё> на нашу экскурсию?"
                            }
                        },
                        {
                            "text": "W lodówce znajdziesz zimne <picie> dla wszystkich gości.",
                            "target_lang_translations": {
                                "ru": "В холодильнике ты найдёшь холодное <питьё> для всех гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "napój"
                    ],
                    "antonyms": [
                        "jedzenie"
                    ],
                    "common_phrases": [
                        "coś do picia",
                        "kupić picie",
                        "zimne picie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название любой жидкости, предназначенной для питья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напиток",
                                "target_lang_sense_clarification": "жидкость"
                            },
                            {
                                "target_lang_word": "питьё",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b2ff38e-341c-3e59-89d3-9a873bc395f4",
                    "sense_definition": "Nałogowe lub nadmierne spożywanie alkoholu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego ciągłe <picie> doprowadziło w końcu do rozpadu rodziny.",
                            "target_lang_translations": {
                                "ru": "Его постоянное <пьянство> привело в конце концов к распаду семьи."
                            }
                        },
                        {
                            "text": "Musisz natychmiast rzucić <picie>, jeśli chcesz odzyskać zdrowie.",
                            "target_lang_translations": {
                                "ru": "Ты должен немедленно бросить <пить>, если хочешь вернуть здоровье."
                            }
                        }
                    ],
                    "antonyms": [
                        "abstynencja"
                    ],
                    "common_phrases": [
                        "rzucić picie",
                        "problem z piciem",
                        "picie do lustra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пристрастие к алкоголю или его чрезмерное употребление."
                    },
                    "translations": {

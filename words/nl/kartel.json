    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "380b6d80-4beb-379d-91df-703426028a9e",
                    "sense_definition": "Een verbond van producenten of bedrijven om de markt te beheersen en onderlinge concurrentie te beperken.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "alliance_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De toezichthouder gaf de bedrijven een boete voor het vormen van een illegaal <w>kartel</w>.",
                            "target_lang_translations": {
                                "ru": "Регулятор оштрафовал компании за создание незаконного <w>картеля</w>.",
                                "en": "The regulator fined the companies for forming an illegal <w>cartel</w>."
                            }
                        },
                        {
                            "text": "Door het <w>kartel</w> bleven de prijzen van de producten kunstmatig hoog.",
                            "target_lang_translations": {
                                "ru": "Из-за <w>картеля</w> цены на продукты оставались искусственно высокими.",
                                "en": "Due to the <w>cartel</w>, product prices remained artificially high."
                            }
                        }
                    ],
                    "synonyms": [
                        "syndicaat",
                        "trust",
                        "monopolie"
                    ],
                    "common_phrases": [
                        "een kartel vormen",
                        "verboden kartel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединение производителей или компаний для контроля над рынком и ограничения конкуренции.",
                        "en": "An association of producers or companies to control the market and limit mutual competition."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "картель",
                                "target_lang_sense_clarification": "экономическое объединение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "cartel",
                                "target_lang_sense_clarification": "economic group"
                            },
                            {
                                "target_lang_word": "syndicate",
                                "target_lang_sense_clarification": "business group"
                            },
                            {
                                "target_lang_word": "trust",
                                "target_lang_sense_clarification": "business association"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1b959ce-60c3-3f5d-b2aa-1a28eba3753c",
                    "sense_definition": "Een samenwerkingsverband tussen politieke partijen, vaak om verkiezingen te winnen of macht te verdelen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "alliance_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oppositiepartijen vormden een <w>kartel</w> om de regeringspartij te verslaan.",
                            "target_lang_translations": {
                                "ru": "Оппозиционные партии сформировали <w>коалицию</w>, чтобы победить правящую партию.",
                                "en": "The opposition parties formed a <w>coalition</w> to defeat the ruling party."
                            }
                        },
                        {
                            "text": "In de gemeenteraad is een politiek <w>kartel</w> ontstaan tussen twee fracties.",
                            "target_lang_translations": {
                                "ru": "В муниципальном совете возник политический <w>картель</w> между двумя фракциями.",
                                "en": "A political <w>alliance</w> has emerged between two factions in the city council."
                            }
                        }
                    ],
                    "synonyms": [
                        "coalitie",
                        "verbond",
                        "alliantie"
                    ],
                    "common_phrases": [
                        "politiek kartel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз политических партий, часто для победы на выборах или распределения власти.",
                        "en": "A partnership between political parties, often to win elections or distribute power."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коалиция",
                                "target_lang_sense_clarification": "политический союз"
                            },
                            {
                                "target_lang_word": "блок",
                                "target_lang_sense_clarification": "объединение партий"
                            },
                            {
                                "target_lang_word": "картель",
                                "target_lang_sense_clarification": "политический термин"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "coalition",
                                "target_lang_sense_clarification": "political union"
                            },
                            {
                                "target_lang_word": "electoral alliance",
                                "target_lang_sense_clarification": "cooperation between parties"
                            },
                            {
                                "target_lang_word": "bloc",
                                "target_lang_sense_clarification": "group of parties"
                            }
                        ]
                    }
                }
            ]
        },
        {

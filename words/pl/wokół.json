            "senses": [
                {
                    "sense_id": "150a80ea-b0ac-3674-9bcc-bb7a01ef34f8",
                    "sense_definition": "W miejscu otaczającym kogoś lub coś; ze wszystkich stron.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_around",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pszczoły latały <w>wokół</w> kolorowych kwiatów na łące.",
                            "target_lang_translations": {
                                "ru": "Пчелы летали <w>вокруг</w> разноцветных цветов на лугу."
                            }
                        },
                        {
                            "text": "Ziemia nieustannie krąży <w>wokół</w> Słońca po swojej orbicie.",
                            "target_lang_translations": {
                                "ru": "Земля непрерывно вращается <w>вокруг</w> Солнца по своей орбите."
                            }
                        }
                    ],
                    "synonyms": [
                        "dookoła",
                        "naokoło",
                        "wokoło",
                        "dokoła",
                        "wkoło"
                    ],
                    "common_phrases": [
                        "krążyć wokół",
                        "wokół domu",
                        "wokół nas",
                        "siedzieć wokół"
                    ],
                    "target_lang_definitions": {
                        "ru": "В месте, окружающем кого-либо или что-либо; со всех сторон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вокруг",
                                "target_lang_sense_clarification": "предлог места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea9b4311-a3a7-3d18-855e-79317f851928",
                    "sense_definition": "W odniesieniu do określonego tematu, sprawy lub wydarzenia; w związku z czymś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_topic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W mediach toczyła się zażarta dyskusja <w>wokół</w> nowego projektu ustawy.",
                            "target_lang_translations": {
                                "ru": "В СМИ шла ожесточенная дискуссия <w>вокруг</w> нового законопроекта."
                            }
                        },
                        {
                            "text": "Zrobiło się dużo niepotrzebnego hałasu <w>wokół</w> tej prywatnej sprawy.",
                            "target_lang_translations": {
                                "ru": "Поднялось много ненужного шума <w>вокруг</w> этого частного дела."
                            }
                        }
                    ],
                    "synonyms": [
                        "wokoło",
                        "dookoła",
                        "wkoło"
                    ],
                    "common_phrases": [
                        "dyskusja wokół",
                        "hałas wokół",
                        "spór wokół",
                        "zamieszanie wokół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относительно определенной темы, вопроса или события; в связи с чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вокруг",
                                "target_lang_sense_clarification": "касательно темы"
                            },
                            {
                                "target_lang_word": "по поводу",
                                "target_lang_sense_clarification": "относительно чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "bc393b02-6c70-35af-a656-b4f5afacf870",
                    "sense_definition": "W każdym kierunku od miejsca, w którym się znajduje; ze wszystkich stron.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_around",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopiec rozejrzał się <w>wokół</w>, szukając swojej zagubionej piłki.",
                            "target_lang_translations": {
                                "ru": "Мальчик огляделся <w>вокруг</w> в поисках своего потерянного мяча."
                            }
                        },
                        {
                            "text": "Wszystko <w>wokół</w> było pokryte grubą warstwą białego śniegu.",
                            "target_lang_translations": {
                                "ru": "Всё <w>вокруг</w> было покрыто толстым слоем белого снега."
                            }
                        }
                    ],
                    "synonyms": [

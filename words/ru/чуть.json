    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c9528030-bcc7-30dd-8daf-193096929b38",
                    "sense_definition": "Немного, в незначительной степени; слегка.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кинозвёзды на фотографии улыбались, <w>чуть</w> разомкнув губы.",
                            "target_lang_translations": {
                                "en": "The movie stars in the photo were smiling, <w>slightly</w> parting their lips."
                            }
                        },
                        {
                            "text": "Суп показался мне <w>чуть</w> недосоленным, но всё равно вкусным.",
                            "target_lang_translations": {
                                "en": "The soup seemed <w>a little</w> under-salted to me, but still tasty."
                            }
                        }
                    ],
                    "synonyms": [
                        "немного",
                        "слегка́",
                        "чу́точку",
                        "ма́ленько",
                        "чуто́к"
                    ],
                    "antonyms": [
                        "намного"
                    ],
                    "common_phrases": [
                        "чуть больше",
                        "чуть меньше",
                        "чуть позже",
                        "чуть лучше"
                    ],
                    "target_lang_definitions": {
                        "en": "A small amount, to an insignificant degree; slightly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "a little",
                                "target_lang_sense_clarification": "degree"
                            },
                            {
                                "target_lang_word": "slightly",
                                "target_lang_sense_clarification": "degree"
                            },
                            {
                                "target_lang_word": "a bit",
                                "target_lang_sense_clarification": "degree"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a9840b0-b775-3aa8-a643-9088f0f6cbc7",
                    "sense_definition": "Почти; указывает на то, что действие едва не совершилось (обычно с частицей «не»).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <w>чуть</w> не умер от страха, когда увидел в лесу медведя.",
                            "target_lang_translations": {
                                "en": "He <w>almost</w> died of fright when he saw a bear in the forest."
                            }
                        },
                        {
                            "text": "Мы так спешили на вокзал, что <w>чуть</w> не забыли билеты дома.",
                            "target_lang_translations": {
                                "en": "We were rushing to the station so much that we <w>nearly</w> forgot the tickets at home."
                            }
                        }
                    ],
                    "common_phrases": [
                        "чуть не",
                        "чуть ли не",
                        "чуть было не"
                    ],
                    "target_lang_definitions": {
                        "en": "Almost; indicates that an action nearly happened (usually with the particle 'не')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "almost",
                                "target_lang_sense_clarification": "proximity to action"
                            },
                            {
                                "target_lang_word": "nearly",
                                "target_lang_sense_clarification": "proximity to action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5013199f-b39b-3b79-b250-cdf9b0d4e24b",
                    "sense_definition": "С трудом, едва; очень слабо или еле заметно.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "difficulty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В полной тишине был <w>чуть</w> слышен шум дождя за окном.",
                            "target_lang_translations": {
                                "en": "In the complete silence, the sound of rain outside the window was <w>barely</w> audible."
                            }
                        },
                        {
                            "text": "Бабушка шла очень медленно и <w>чуть</w> переставляла больные ноги.",
                            "target_lang_translations": {
                                "en": "Grandma was walking very slowly and <w>hardly</w> moving her sore legs."
                            }
                        }
                    ],
                    "synonyms": [
                        "едва́",
                        "е́ле",
                        "с трудом",
                        "насилу",
                        "е́ле-е́ле",
                        "едва́-едва́"
                    ],
                    "common_phrases": [
                        "чуть слышно",
                        "чуть живой",
                        "чуть дыша"
                    ],
                    "target_lang_definitions": {
                        "en": "With difficulty, barely; very weakly or scarcely noticeably."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "barely",
                                "target_lang_sense_clarification": "with difficulty or faintly"
                            },
                            {
                                "target_lang_word": "hardly",
                                "target_lang_sense_clarification": "scarcely"
                            },
                            {
                                "target_lang_word": "faintly",
                                "target_lang_sense_clarification": "perceptibility"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "9583b20c-c723-30d3-9ba8-efc06c1343d8",
                    "sense_definition": "Как только; в тот самый момент, когда.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Чуть</w> солнце взошло, рыбаки уже сидели на берегу реки.",
                            "target_lang_translations": {
                                "en": "<w>As soon as</w> the sun rose, the fishermen were already sitting on the riverbank."
                            }
                        },
                        {
                            "text": "<w>Чуть</w> коснувшись подушки, уставший путник сразу же крепко уснул.",
                            "target_lang_translations": {
                                "en": "<w>The moment</w> he touched the pillow, the tired traveler immediately fell fast asleep."
                            }
                        }
                    ],
                    "synonyms": [
                        "как только",
                        "лишь только",
                        "едва́",
                        "едва только"
                    ],
                    "common_phrases": [
                        "чуть свет",
                        "чуть что"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in spoken or expressive language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "As soon as; at the very moment when."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "as soon as",
                                "target_lang_sense_clarification": "time conjunction"
                            },
                            {
                                "target_lang_word": "the moment",
                                "target_lang_sense_clarification": "time conjunction"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серьёзный",
                                "target_lang_sense_clarification": "не пустяковый"
                            },
                            {
                                "target_lang_word": "важный",
                                "target_lang_sense_clarification": "имеющий большое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f4077fd-97eb-38dc-9352-4c976476764c",
                    "sense_definition": "Stanowiący zagrożenie dla zdrowia, życia lub bezpieczeństwa; trudny do wyleczenia lub rozwiązania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "danger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent jest w <w>poważnym</w> stanie i musi zostać w szpitalu na obserwacji.",
                            "target_lang_translations": {
                                "ru": "Пациент находится в <w>тяжёлом</w> состоянии и должен остаться в больнице под наблюдением."
                            }
                        },
                        {
                            "text": "Jego dziadek przeszedł niedawno <w>poważną</w> operację serca.",
                            "target_lang_translations": {
                                "ru": "Его дедушка недавно перенёс <w>серьёзную</w> операцию на сердце."
                            }

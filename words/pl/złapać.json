    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7e56982e-40b1-3b5d-beb9-d2dbf31c5009",
                    "sense_definition": "Chwycić w ręce przedmiot, który leci w powietrzu lub się porusza.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_catch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <złapać> piłkę, zanim spadnie na ziemię.",
                            "target_lang_translations": {
                                "ru": "Ты должен <поймать> мяч, пока он не упал на землю."
                            }
                        },
                        {
                            "text": "Bramkarzowi udało się <złapać> piłkę w ostatniej chwili.",
                            "target_lang_translations": {
                                "ru": "Вратарю удалось <поймать> мяч в последний момент."
                            }
                        }
                    ],
                    "synonyms": [
                        "chwycić",
                        "złowić"
                    ],
                    "antonyms": [
                        "upuścić"
                    ],
                    "common_phrases": [
                        "złapać piłkę",
                        "złapać w locie",
                        "złapać coś w ręce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Схватить руками предмет, который летит по воздуху или движется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поймать",
                                "target_lang_sense_clarification": "схватить что-то движущееся"
                            },
                            {
                                "target_lang_word": "схватить",
                                "target_lang_sense_clarification": "быстро взять в руки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "273371d8-4214-3223-8202-36c5f202344e",
                    "sense_definition": "Zdążyć wsiąść do środka transportu przed jego odjazdem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy biec, żeby <złapać> ostatni autobus do domu.",
                            "target_lang_translations": {
                                "ru": "Мы должны бежать, чтобы <успеть на> последний автобус домой."
                            }
                        },
                        {
                            "text": "Czy uda nam się <złapać> taksówkę o tej porze?",
                            "target_lang_translations": {
                                "ru": "Удастся ли нам <поймать> такси в это время?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zabrać się",
                        "załapać się"
                    ],
                    "antonyms": [
                        "spóźnić się"
                    ],
                    "common_phrases": [
                        "złapać autobus",
                        "złapać pociąg",
                        "złapać taksówkę",
                        "złapać okazję"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Успеть сесть на транспорт до его отправления или остановить его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "успеть на",
                                "target_lang_sense_clarification": "на автобус, поезд (по расписанию)"
                            },
                            {
                                "target_lang_word": "поймать",
                                "target_lang_sense_clarification": "такси, попутку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ad3298b-7298-351e-92b5-f698dc7a62b3",
                    "sense_definition": "Zarazić się chorobą lub infekcją.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "disease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę <złapać> grypy, więc biorę witaminy.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <подхватить> грипп, поэтому принимаю витамины."
                            }
                        },
                        {
                            "text": "Marek musiał <złapać> przeziębienie w szkole.",
                            "target_lang_translations": {
                                "ru": "Марек, должно быть, <подхватил> простуду в школе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabrać się"
                    ],
                    "common_phrases": [
                        "złapać katar",
                        "złapać grypę",
                        "złapać wirusa",
                        "złapać infekcję"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заразиться болезнью или инфекцией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подхватить",
                                "target_lang_sense_clarification": "разг. заразиться"
                            },
                            {
                                "target_lang_word": "заразиться",
                                "target_lang_sense_clarification": "медицинский/общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11e964cf-16a0-3f57-a04a-8d23e15b1876",
                    "sense_definition": "Szybkim ruchem ująć coś lub kogoś (np. za rękę, za ubranie), często aby przytrzymać.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_grab",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko chciało <złapać> matkę za rękę na pasach.",
                            "target_lang_translations": {
                                "ru": "Ребенок хотел <схватить> мать за руку на переходе."
                            }
                        },
                        {
                            "text": "Próbował <złapać> spadający wazon, ale nie zdążył.",
                            "target_lang_translations": {
                                "ru": "Он пытался <поймать> падающую вазу, но не успел."
                            }
                        }
                    ],
                    "synonyms": [
                        "chwycić",
                        "ująć"
                    ],
                    "antonyms": [
                        "puścić"
                    ],
                    "common_phrases": [
                        "złapać za rękę",
                        "złapać za klamkę",
                        "złapać za ramię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстрым движением взять кого-то или что-то (напр. за руку), часто чтобы удержать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "схватить",
                                "target_lang_sense_clarification": "резко взять"
                            },
                            {
                                "target_lang_word": "ухватить",
                                "target_lang_sense_clarification": "крепко взяться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88d28e97-a31c-31c6-854a-1ce449cfa6d4",
                    "sense_definition": "Pozbawić wolności uciekającego człowieka lub zwierzę; schwytać kogoś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "capture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policji udało się w końcu <złapać> groźnego przestępcę.",
                            "target_lang_translations": {
                                "ru": "Полиции наконец удалось <поймать> опасного преступника."
                            }
                        },
                        {
                            "text": "Kot próbował <złapać> mysz w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Кот пытался <поймать> мышь в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopaść",
                        "dorwać",
                        "złowić"
                    ],
                    "antonyms": [
                        "wypuścić",
                        "uwolnić"
                    ],
                    "common_phrases": [
                        "złapać złodzieja",
                        "złapać mysz",
                        "złapać przestępcę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишить свободы убегающего человека или животное; задержать кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поймать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "задержать",
                                "target_lang_sense_clarification": "о преступнике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72ff63b6-5b06-3b4c-bbfd-f8c4845b7f7c",
                    "sense_definition": "Wykorzystać sprzyjającą chwilę lub okazję.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "opportunity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <złapać> okazję, póki jest jeszcze dostępna.",
                            "target_lang_translations": {
                                "ru": "Ты должен <воспользоваться> возможностью, пока она еще есть."
                            }
                        },
                        {
                            "text": "Usiadł na ławce, żeby <złapać> oddech po biegu.",
                            "target_lang_translations": {
                                "ru": "Он сел на скамейку, чтобы <перевести> дух после бега."
                            }
                        }
                    ],
                    "antonyms": [
                        "stracić",
                        "zmarnować"
                    ],
                    "common_phrases": [
                        "złapać okazję",
                        "złapać oddech",
                        "złapać chwilę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воспользоваться благоприятным моментом или возможностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспользоваться",
                                "target_lang_sense_clarification": "случаем, возможностью"
                            },
                            {
                                "target_lang_word": "уловить",
                                "target_lang_sense_clarification": "момент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31b2b193-8ad8-3284-95e9-ea26b99c7aed",
                    "sense_definition": "Zrozumieć sens czegoś, pojąć coś (zwykle nagle).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "understand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dopiero po chwili udało mi się <złapać>, o czym on mówi.",
                            "target_lang_translations": {
                                "ru": "Только через минуту мне удалось <уловить>, о чем он говорит."
                            }
                        },
                        {
                            "text": "Nie mogę <złapać> sensu tego trudnego zadania.",
                            "target_lang_translations": {
                                "ru": "Я не могу <уловить> смысл этого трудного задания."
                            }
                        }
                    ],
                    "synonyms": [
                        "pojąć",
                        "załapać"
                    ],
                    "common_phrases": [
                        "złapać sens",
                        "złapać o co chodzi",
                        "złapać żart"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Понять смысл чего-либо, уяснить (обычно внезапно)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уловить",
                                "target_lang_sense_clarification": "смысл, суть"
                            },
                            {
                                "target_lang_word": "понять",
                                "target_lang_sense_clarification": "осознать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23d396fa-1e39-3816-8e84-f44db34a8b89",
                    "sense_definition": "Zastać kogoś w trakcie wykonywania jakiejś czynności, zazwyczaj niedozwolonej.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "red_handed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel zdołał <złapać> ucznia na ściąganiu podczas testu.",
                            "target_lang_translations": {
                                "ru": "Учитель смог <поймать> ученика на списывании во время теста."
                            }
                        },
                        {
                            "text": "Nie daj się <złapać> na kłamstwie, bo stracisz zaufanie.",
                            "target_lang_translations": {
                                "ru": "Не дай <поймать> себя на лжи, иначе потеряешь доверие."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyłapać się"
                    ],
                    "common_phrases": [
                        "złapać na kłamstwie",
                        "złapać na gorącym uczynku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Застать кого-то во время выполнения какого-либо действия, обычно запрещенного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поймать",
                                "target_lang_sense_clarification": "на месте преступления/лжи"
                            },
                            {
                                "target_lang_word": "застукать",
                                "target_lang_sense_clarification": "разг. застать врасплох"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bac775e-2b0b-3b99-85b3-4559351e1a7f",
                    "sense_definition": "Uzyskać połączenie lub odbiór fal (np. radiowych, zasięgu).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W górach bardzo trudno jest <złapać> zasięg w telefonie.",
                            "target_lang_translations": {
                                "ru": "В горах очень трудно <поймать> сеть на телефоне."
                            }
                        },
                        {
                            "text": "Próbuję <złapać> jakąś stację radiową, ale słychać tylko szum.",
                            "target_lang_translations": {
                                "ru": "Я пытаюсь <поймать> какую-нибудь радиостанцию, но слышен только шум."
                            }
                        }
                    ],
                    "antonyms": [
                        "zgubić"
                    ],
                    "common_phrases": [
                        "złapać zasięg",
                        "złapać sygnał",
                        "złapać stację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установить соединение или прием волн (напр. радио, сети)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поймать",
                                "target_lang_sense_clarification": "сигнал, волну"
                            }
                        ]
                    }
                }
            ]
        }
    ]

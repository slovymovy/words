    "entries": [
        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "ea71330b-401f-3bac-92eb-60dde3f2ac2d",
                    "sense_definition": "Небольшое, но неопределённое количество кого-либо или чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На столе в большой вазе лежало <w>несколько</w> красных яблок.",
                            "target_lang_translations": {
                                "en": "There were <w>several</w> red apples lying in a large vase on the table."
                            }
                        },
                        {
                            "text": "Я уже <w>несколько</w> раз звонил ему, но он не отвечает.",
                            "target_lang_translations": {
                                "en": "I have already called him <w>several</w> times, but he is not answering."
                            }
                        }
                    ],
                    "synonyms": [
                        "немного",
                        "сколько-то"
                    ],
                    "antonyms": [
                        "нисколько"
                    ],
                    "common_phrases": [
                        "несколько раз",
                        "несколько дней",
                        "несколько человек",
                        "несколько лет",
                        "несколько минут"
                    ],
                    "target_lang_definitions": {
                        "en": "A small but indefinite number of people or things."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "several",
                                "target_lang_sense_clarification": "more than two but not many"
                            },
                            {
                                "target_lang_word": "a few",
                                "target_lang_sense_clarification": "a small number of"
                            },
                            {
                                "target_lang_word": "some",
                                "target_lang_sense_clarification": "an indefinite number"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "93d09875-295c-35c8-9b29-f8464abbbf05",
                    "sense_definition": "В некоторой степени, немного, слегка.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его голос звучал <w>несколько</w> странно и неуверенно.",
                            "target_lang_translations": {
                                "en": "His voice sounded <w>somewhat</w> strange and uncertain."
                            }
                        },
                        {
                            "text": "Эта задача оказалась <w>несколько</w> сложнее, чем мы предполагали вначале.",
                            "target_lang_translations": {
                                "en": "This task turned out to be <w>a little</w> more difficult than we initially assumed."
                            }
                        }
                    ],
                    "synonyms": [
                        "немного",
                        "слегка"
                    ],
                    "common_phrases": [
                        "несколько иначе",
                        "несколько странный",
                        "несколько больше",
                        "несколько позже"
                    ],
                    "target_lang_definitions": {
                        "en": "To a certain extent or degree; slightly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "somewhat",
                                "target_lang_sense_clarification": "to a limited extent or degree"
                            },
                            {
                                "target_lang_word": "a little",
                                "target_lang_sense_clarification": "slightly; to a small degree"
                            },
                            {
                                "target_lang_word": "a bit",
                                "target_lang_sense_clarification": "informal; slightly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c97c2a9-9fcd-3fc6-a7a5-b609f759fdcb",
                    "sense_definition": "В течение некоторого, непродолжительного времени; недолго.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Путники отдохнули <w>несколько</w> у ручья и продолжили свой путь.",
                            "target_lang_translations": {
                                "en": "The travelers rested <w>a little</w> by the stream and continued their journey."
                            }
                        },
                        {
                            "text": "Он помолчал <w>несколько</w>, собираясь с мыслями перед ответом.",
                            "target_lang_translations": {
                                "en": "He remained silent <w>for a while</w>, gathering his thoughts before answering."
                            }
                        }
                    ],
                    "synonyms": [
                        "недолго"
                    ],
                    "antonyms": [
                        "долго"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "For a short period of time; for a while."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "a little",
                                "target_lang_sense_clarification": "for a short time"
                            },
                            {
                                "target_lang_word": "a bit",
                                "target_lang_sense_clarification": "for a short time"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
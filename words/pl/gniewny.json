    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "527cc4dc-eb69-3239-93dd-cb196662df10",
                    "sense_definition": "Taki, który odczuwa złość, jest na kogoś zły lub ma skłonność do częstego denerwowania się.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad to <w>gniewny</w> człowiek, który ciągle na wszystkich krzyczy.",
                            "target_lang_translations": {
                                "ru": "Мой сосед — <w>вспыльчивый</w> человек, который постоянно на всех кричит."
                            }
                        },
                        {
                            "text": "Dyrektor był bardzo <w>gniewny</w> z powodu dużych opóźnień w projekcie.",
                            "target_lang_translations": {
                                "ru": "Директор был очень <w>разгневан</w> из-за больших задержек в проекте."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozgniewany",
                        "rozzłoszczony",
                        "wściekły",
                        "oburzony",
                        "wzburzony",
                        "zły"
                    ],
                    "antonyms": [
                        "spokojny",
                        "łagodny",
                        "opanowany",
                        "zrównoważony"
                    ],
                    "common_phrases": [
                        "młodzi gniewni",
                        "gniewny tłum",
                        "gniewny na cały świat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий гнев, злой на кого-то или склонный часто раздражаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сердитый",
                                "target_lang_sense_clarification": "испытывающий злость"
                            },
                            {
                                "target_lang_word": "разгневанный",
                                "target_lang_sense_clarification": "находящийся в состоянии сильного гнева"
                            },
                            {
                                "target_lang_word": "вспыльчивый",
                                "target_lang_sense_clarification": "склонный к гневу (о характере)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b13011ac-222b-3d91-860e-52a4ae12922e",
                    "sense_definition": "Taki, który wyraża złość, surowość lub niezadowolenie (używane w odniesieniu do głosu, wyglądu, gestów).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzuciła mu <w>gniewne</w> spojrzenie, ale nic nie powiedziała.",
                            "target_lang_translations": {
                                "ru": "Она бросила на него <w>гневный</w> взгляд, но ничего не сказала."
                            }
                        },
                        {
                            "text": "Przez zamknięte drzwi słychać było tylko <w>gniewny</w> ton głosu mojego brata.",
                            "target_lang_translations": {
                                "ru": "Сквозь закрытую дверь был слышен только <w>гневный</w> тон голоса моего брата."
                            }
                        }
                    ],
                    "synonyms": [
                        "groźny",
                        "srogi",
                        "surowy",
                        "wrogi"
                    ],
                    "antonyms": [
                        "przyjazny",
                        "ciepły",
                        "łagodny"
                    ],
                    "common_phrases": [
                        "gniewne spojrzenie",
                        "gniewny wzrok",
                        "gniewny pomruk",
                        "gniewny ton"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражающий гнев, суровость или недовольство (о голосе, взгляде, жестах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гневный",
                                "target_lang_sense_clarification": "выражающий сильное негодование"
                            },
                            {
                                "target_lang_word": "сердитый",
                                "target_lang_sense_clarification": "выражающий недовольство"
                            },
                            {
                                "target_lang_word": "грозный",
                                "target_lang_sense_clarification": "суровый, угрожающий"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e6cb88e0-98c5-3638-a0a5-cb24e13ce068",
                    "sense_definition": "Iemand iets mededelen of zich met gesproken woorden uiten.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <zegt> dat hij morgen op bezoek komt.",
                            "target_lang_translations": {
                                "en": "He <says> that he is coming to visit tomorrow.",
                                "ru": "Он <говорит>, что придет в гости завтра."
                            }
                        },
                        {
                            "text": "Naar eigen <zeggen> heeft hij niets fout gedaan.",
                            "target_lang_translations": {
                                "en": "According to what he <says>, he has done nothing wrong.",
                                "ru": "По его собственным <словам>, он ничего плохого не сделал."
                            }
                        },
                        {
                            "text": "Ik heb hem duidelijk de waarheid <gezegd>.",
                            "target_lang_translations": {
                                "en": "I clearly <told> him the truth.",
                                "ru": "Я ясно <сказал> ему правду."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertellen",
                        "mededelen",
                        "spreken",
                        "uiten"
                    ],
                    "antonyms": [
                        "zwijgen"
                    ],
                    "common_phrases": [
                        "eerlijk gezegd",
                        "naar eigen zeggen",
                        "zogezegd",
                        "ik zeg maar zo",
                        "u tegen zeggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To communicate something to someone or express oneself with spoken words.",
                        "ru": "Сообщать что-либо кому-либо или выражать свои мысли устно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "say",
                                "target_lang_sense_clarification": "to utter words or express a thought"
                            },
                            {
                                "target_lang_word": "tell",
                                "target_lang_sense_clarification": "to communicate information to someone"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "говорить",
                                "target_lang_sense_clarification": "произносить слова или выражать мысли"
                            },
                            {
                                "target_lang_word": "сказать",
                                "target_lang_sense_clarification": "сообщить информацию (совершенный вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89863dac-fbfb-3954-8b9d-8b8a2db805e6",
                    "sense_definition": "Een bepaalde betekenis hebben of ergens op duiden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat wil dit vreemde woord precies <zeggen>?",
                            "target_lang_translations": {
                                "en": "What exactly does this strange word <mean>?",
                                "ru": "Что именно <означает> это странное слово?"
                            }
                        },
                        {
                            "text": "Dat wil niet <zeggen> dat we het ermee eens zijn.",
                            "target_lang_translations": {
                                "en": "That does not <mean> that we agree with it.",
                                "ru": "Это не <значит>, что мы с этим согласны."
                            }
                        }
                    ],
                    "synonyms": [
                        "betekenen",
                        "inhouden"
                    ],
                    "common_phrases": [
                        "dat wil zeggen"
                    ],

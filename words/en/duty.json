{
    "word_family": [
        "dutiful",
        "dutiable",
        "dutifully"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "65413b6f-b72e-38ac-9943-1ec57741c4c6",
                    "sense_definition": "That which one is morally or legally obligated to do.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "obligation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is the <w>duty</w> of every citizen to follow the law.",
                            "target_lang_translations": {
                                "ru": "Соблюдать закон — это <w>долг</w> каждого гражданина."
                            }
                        },
                        {
                            "text": "He felt a strong sense of <w>duty</w> to help his family.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал сильное чувство <w>долга</w> помогать своей семье."
                            }
                        }
                    ],
                    "synonyms": [
                        "obligation"
                    ],
                    "antonyms": [
                        "right"
                    ],
                    "common_phrases": [
                        "do one's duty",
                        "sense of duty",
                        "civic duty",
                        "moral duty"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что человек обязан делать в силу моральных или юридических норм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "долг",
                                "target_lang_sense_clarification": "моральное или гражданское обязательство"
                            },
                            {
                                "target_lang_word": "обязанность",
                                "target_lang_sense_clarification": "задача или функция, которую необходимо выполнять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "777ae7fc-675e-33e7-b8e8-79e17d7be152",
                    "sense_definition": "The state of being at work and responsible for or doing a particular task.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "work_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The security guard is on <w>duty</w> all night.",
                            "target_lang_translations": {
                                "ru": "Охранник находится на <w>дежурстве</w> всю ночь."
                            }
                        },
                        {
                            "text": "She reports for <w>duty</w> at the hospital at seven o'clock.",
                            "target_lang_translations": {
                                "ru": "Она заступает на <w>дежурство</w> в больнице в семь часов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on duty",
                        "off duty",
                        "report for duty",
                        "jury duty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние нахождения на работе и выполнения служебных обязанностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дежурство",
                                "target_lang_sense_clarification": "выполнение рабочих обязанностей по графику"
                            },
                            {
                                "target_lang_word": "служба",
                                "target_lang_sense_clarification": "исполнение служебных обязанностей, работа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "303fa74e-db52-3109-8195-3a98c0b45162",
                    "sense_definition": "A tax placed on imports or exports; a tariff.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tax_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must pay a <w>duty</w> on goods brought from other countries.",
                            "target_lang_translations": {
                                "ru": "Вы должны заплатить <w>пошлину</w> за товары, ввезенные из других стран."
                            }
                        },
                        {
                            "text": "The government raised the import <w>duty</w> on luxury cars.",
                            "target_lang_translations": {
                                "ru": "Правительство повысило импортную <w>пошлину</w> на роскошные автомобили."
                            }
                        }
                    ],
                    "synonyms": [
                        "obligation",
                        "obligation"
                    ],
                    "common_phrases": [
                        "customs duty",
                        "import duty",
                        "excise duty",
                        "stamp duty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Налог, налагаемый на импорт или экспорт; тариф."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пошлина",
                                "target_lang_sense_clarification": "государственный денежный сбор, особенно таможенный"
                            },
                            {
                                "target_lang_word": "сбор",
                                "target_lang_sense_clarification": "обязательный платеж или взнос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e9226f6-4344-36f0-a1cc-3b41f7c06c87",
                    "sense_definition": "The act of urination or defecation, especially for a dog.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "euphemism_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We took the puppy outside so he could do his <w>duty</w>.",
                            "target_lang_translations": {
                                "ru": "Мы вывели щенка на улицу, чтобы он мог сделать свои <w>дела</w>."
                            }
                        },
                        {
                            "text": "The dog did his <w>duty</w> on the grass in the park.",
                            "target_lang_translations": {
                                "ru": "Собака справила свою <w>нужду</w> на траве в парке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do one's duty"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "euphemistic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт мочеиспускания или дефекации, особенно у собак."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нужда",
                                "target_lang_sense_clarification": "физиологическая потребность (эвфемизм)"
                            },
                            {
                                "target_lang_word": "дела",
                                "target_lang_sense_clarification": "разг. эвфемизм для туалета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1da11481-4e3c-39f9-9133-32759868d47a",
                    "sense_definition": "Respect; reverence; regard; act of respect; homage.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "respect_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He paid his <w>duty</w> to the king with a deep bow.",
                            "target_lang_translations": {
                                "ru": "Он засвидетельствовал свое <w>почтение</w> королю глубоким поклоном."
                            }
                        },
                        {
                            "text": "Please send my humble <w>duty</w> to your parents.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, передайте мое скромное <w>почтение</w> вашим родителям."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уважение; почтение; проявление уважения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почтение",
                                "target_lang_sense_clarification": "глубокое уважение (устар.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7545fa2-9c21-30c5-967b-ae93fc0d134f",
                    "sense_definition": "One's due, something one is owed; a debt or fee.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "debt_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The landlord came to collect his <w>duty</w> from the tenants.",
                            "target_lang_translations": {
                                "ru": "Домовладелец пришел собрать причитающуюся ему <w>плату</w> с жильцов."
                            }
                        },
                        {
                            "text": "He felt he had paid his <w>duty</w> to society.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, что отдал свой <w>долг</w> обществу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что причитается; долг или плата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плата",
                                "target_lang_sense_clarification": "денежное вознаграждение или сбор (устар.)"
                            },
                            {
                                "target_lang_word": "долг",
                                "target_lang_sense_clarification": "то, что взято взаймы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c21fa5f-ca55-3c83-bac1-7f3499a63ddb",
                    "sense_definition": "The efficiency of an engine in work done per unit of fuel.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineer measured the steam engine's <w>duty</w> carefully.",
                            "target_lang_translations": {
                                "ru": "Инженер тщательно измерил <w>производительность</w> парового двигателя."
                            }
                        },
                        {
                            "text": "High <w>duty</w> was essential for the industrial pumps of that era.",
                            "target_lang_translations": {
                                "ru": "Высокая <w>производительность</w> была необходима для промышленных насосов той эпохи."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical engineering context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "КПД двигателя, выраженный в работе, совершенной на единицу топлива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "производительность",
                                "target_lang_sense_clarification": "эффективность работы механизма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "008c2755-ab05-323b-b803-7cd643b404e0",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Duty</w> lives in the house on the corner.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Дьюти</w> живет в доме на углу."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <w>Duty</w> at the conference.",
                            "target_lang_translations": {
                                "ru": "Я встретил женщину по имени Сара <w>Дьюти</w> на конференции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дьюти",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
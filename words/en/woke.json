    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d9d65174-bd17-35ec-a6c5-4bad81425963",
                    "sense_definition": "simple past of <w>wake</w>",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>woke</w> up early this morning to catch the first train.",
                            "target_lang_translations": {
                                "ru": "Я <w>проснулся</w> сегодня рано утром, чтобы успеть на первый поезд."
                            }
                        },
                        {
                            "text": "The loud noise from the street <w>woke</w> the baby.",
                            "target_lang_translations": {
                                "ru": "Громкий шум с улицы <w>разбудил</w> ребенка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "woke up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время от глагола wake (просыпаться или будить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проснулся",
                                "target_lang_sense_clarification": "о человеке, который перестал спать"
                            },
                            {
                                "target_lang_word": "разбудил",
                                "target_lang_sense_clarification": "действие, направленное на кого-то другого"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "928245e9-cd7d-3a1a-8c73-fdfb4e44d449",
                    "sense_definition": "Holding progressive views or attitudes, principally with regard to social justice.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "political_stance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company tried to appear <w>woke</w> by launching a new diversity campaign.",
                            "target_lang_translations": {
                                "ru": "Компания пыталась казаться <w>прогрессивной</w>, запустив новую кампанию за разнообразие."
                            }
                        },
                        {
                            "text": "He considers himself <w>woke</w> and actively supports various civil rights movements.",
                            "target_lang_translations": {
                                "ru": "Он считает себя <w>социально сознательным</w> и активно поддерживает различные движения за гражданские права."
                            }
                        }
                    ],
                    "synonyms": [
                        "politically correct",
                        "right-on"
                    ],
                    "antonyms": [
                        "unwoke",
                        "politically incorrect"
                    ],
                    "common_phrases": [
                        "stay woke"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Придерживающийся прогрессивных взглядов, особенно в вопросах социальной справедливости и равенства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прогрессивный",
                                "target_lang_sense_clarification": "современный, поддерживающий реформы"
                            },
                            {
                                "target_lang_word": "социально сознательный",
                                "target_lang_sense_clarification": "осознающий социальные проблемы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9255d292-6f24-3974-a1fe-d0748f34a8c1",
                    "sense_definition": "Promoting cancel culture, identitarianism, victim mentality, or virtue signalling.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "political_stance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Critics argued that the movie was too <w>woke</w> and prioritized politics over storytelling.",
                            "target_lang_translations": {
                                "ru": "Критики утверждали, что фильм слишком <w>повесточный</w> и ставит политику выше сюжета."
                            }
                        },
                        {
                            "text": "She felt that the university had become a <w>woke</w> environment where dissenting opinions were not tolerated.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала, что университет стал <w>ультралиберальной</w> средой, где не терпят инакомыслия."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Продвигающий навязчивую политкорректность или культуру отмены (часто используется в негативном или ироничном ключе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повесточный",
                                "target_lang_sense_clarification": "сленг: следующий модной политической повестке (о фильмах, играх)"
                            },
                            {
                                "target_lang_word": "помешанный на политкорректности",
                                "target_lang_sense_clarification": "негативная характеристика"
                            },
                            {
                                "target_lang_word": "ультралиберальный",
                                "target_lang_sense_clarification": "радикально настроенный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52373bc5-6dec-3416-a00d-08185d9869cd",
                    "sense_definition": "Alert, aware of what is going on, or well-informed, especially of racial and other social justice issues.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "awareness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to stay <w>woke</w> to the systemic issues affecting our community.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно оставаться <w>бдительным</w> к системным проблемам, затрагивающим наше сообщество."
                            }
                        },
                        {
                            "text": "The song encourages listeners to stay <w>woke</w> and not ignore the truth.",
                            "target_lang_translations": {
                                "ru": "Песня призывает слушателей быть <w>осознанными</w> и не игнорировать правду."
                            }
                        }
                    ],
                    "synonyms": [
                        "alert",
                        "aware",
                        "conscious",
                        "vigilant"
                    ],
                    "common_phrases": [
                        "stay woke"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "originally African-American Vernacular English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бдительный, осведомленный о происходящем, не дающий себя обмануть (изначальное значение в афроамериканском сленге)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бдительный",
                                "target_lang_sense_clarification": "внимательный к опасности или обману"
                            },
                            {
                                "target_lang_word": "осознанный",
                                "target_lang_sense_clarification": "понимающий суть вещей"
                            }
                        ]
                    }
                }
            ]
        },

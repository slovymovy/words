{
    "word_family": [
        "stacker",
        "restack",
        "unstack",
        "stackable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "98f32a8f-a0d8-301e-a729-201bb40307d4",
                    "sense_definition": "A pile of objects arranged one on top of another.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_pile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please bring me a chair from that <w>stack</w> in the corner.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, принеси мне стул из той <w>стопки</w> в углу."
                            }
                        },
                        {
                            "text": "He placed the books in a neat <w>stack</w> on the desk.",
                            "target_lang_translations": {
                                "ru": "Он сложил книги в аккуратную <w>стопку</w> на столе."
                            }
                        }
                    ],
                    "synonyms": [
                        "pile",
                        "heap",
                        "mound"
                    ],
                    "common_phrases": [
                        "stack of books",
                        "stack of papers",
                        "neat stack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Куча предметов, аккуратно сложенных друг на друга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стопка",
                                "target_lang_sense_clarification": "аккуратная вертикальная стопа (книг, тарелок)"
                            },
                            {
                                "target_lang_word": "кипа",
                                "target_lang_sense_clarification": "пачка или связка (бумаг, газет)"
                            },
                            {
                                "target_lang_word": "куча",
                                "target_lang_sense_clarification": "общее название для нагромождения предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b605a23-f831-371d-9bdb-8ee4bd511cb4",
                    "sense_definition": "A large quantity or collection of something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "large_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They paid him a <w>stack</w> of money to keep quiet about the incident.",
                            "target_lang_translations": {
                                "ru": "Они заплатили ему <w>пачку</w> денег, чтобы он молчал об инциденте."
                            }
                        },
                        {
                            "text": "We have a whole <w>stack</w> of work to do before the deadline.",
                            "target_lang_translations": {
                                "ru": "У нас целая <w>кипа</w> работы, которую нужно сделать до дедлайна."
                            }
                        }
                    ],
                    "synonyms": [
                        "heap",
                        "pile",
                        "mound"
                    ],
                    "common_phrases": [
                        "stack of money",
                        "whole stack of"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to refer to money or work"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество или собрание чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куча",
                                "target_lang_sense_clarification": "разг. большое количество"
                            },
                            {
                                "target_lang_word": "кипа",
                                "target_lang_sense_clarification": "о бумагах или делах"
                            },
                            {
                                "target_lang_word": "пачка",
                                "target_lang_sense_clarification": "о деньгах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "960ca9b4-47f0-30a5-a8a3-19b526f6ca46",
                    "sense_definition": "A linear data structure in computing where items are added and removed in reverse order (last in, first out).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The program pushes the current value onto the <w>stack</w> for later retrieval.",
                            "target_lang_translations": {
                                "ru": "Программа помещает текущее значение в <w>стек</w> для последующего извлечения."
                            }
                        },
                        {
                            "text": "A <w>stack</w> overflow error occurred when the memory limit was exceeded.",
                            "target_lang_translations": {
                                "ru": "Ошибка переполнения <w>стека</w> произошла при превышении лимита памяти."
                            }
                        }
                    ],
                    "common_phrases": [
                        "push onto stack",
                        "pop from stack",
                        "call stack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Линейная структура данных в информатике, работающая по принципу «последним пришёл — первым ушёл» (LIFO)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стек",
                                "target_lang_sense_clarification": "структура данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdd3375c-bac7-35dd-adc6-4f1be047e508",
                    "sense_definition": "A set of software components or technologies used together to run an application.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A LAMP <w>stack</w> is a common configuration for hosting websites.",
                            "target_lang_translations": {
                                "ru": "<w>Стек</w> LAMP — это распространенная конфигурация для хостинга веб-сайтов."
                            }
                        },
                        {
                            "text": "Developers need to be familiar with the entire technology <w>stack</w> to build modern apps.",
                            "target_lang_translations": {
                                "ru": "Разработчикам необходимо быть знакомыми со всем технологическим <w>стеком</w> для создания современных приложений."
                            }
                        }
                    ],
                    "synonyms": [
                        "technology stack",
                        "protocol stack"
                    ],
                    "common_phrases": [
                        "full stack",
                        "tech stack",
                        "software stack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор программных компонентов или технологий, используемых совместно для работы приложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стек",
                                "target_lang_sense_clarification": "набор технологий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23b2e7b9-7bd8-367c-a973-7db820f3ef9d",
                    "sense_definition": "A tall chimney on a factory, ship, or train for carrying smoke away.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chimney",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Black smoke poured from the <w>stack</w> of the steamship as it left the dock.",
                            "target_lang_translations": {
                                "ru": "Черный дым валил из <w>трубы</w> парохода, когда он отходил от причала."
                            }
                        },
                        {
                            "text": "The factory <w>stack</w> could be seen from miles away across the valley.",
                            "target_lang_translations": {
                                "ru": "Заводскую <w>трубу</w> было видно за много миль через долину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "smoke stack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокая дымовая труба на заводе, судне или поезде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "труба",
                                "target_lang_sense_clarification": "дымовая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f7a06b7-0e38-3555-92e1-f7f62999c09c",
                    "sense_definition": "The area in a library where the main collection of books is stored on shelves.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "library",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You took me to your library and kissed me in the <w>stacks</w>.",
                            "target_lang_translations": {
                                "ru": "Ты привел меня в свою библиотеку и поцеловал между <w>стеллажами</w>."
                            }
                        },
                        {
                            "text": "Students often study quietly near the book <w>stacks</w>.",
                            "target_lang_translations": {
                                "ru": "Студенты часто тихо занимаются возле книжных <w>стеллажей</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in the stacks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зона в библиотеке, где на стеллажах хранится основной фонд книг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "книгохранилище",
                                "target_lang_sense_clarification": "помещение для хранения книг"
                            },
                            {
                                "target_lang_word": "стеллажи",
                                "target_lang_sense_clarification": "полки с книгами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9aba925-1c04-3d4a-af4e-477acceea58e",
                    "sense_definition": "A large vertical column of rock in the sea near a coast, formed by erosion.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Old Man of Hoy is a famous sea <w>stack</w> in Scotland.",
                            "target_lang_translations": {
                                "ru": "Олд-Ман-оф-Хой — известный морской <w>кекур</w> в Шотландии."
                            }
                        },
                        {
                            "text": "Waves crashed against the base of the rock <w>stack</w> during the storm.",
                            "target_lang_translations": {
                                "ru": "Во время шторма волны разбивались об основание скалистого <w>останца</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sea stack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокая вертикальная скала в море у побережья, образованная эрозией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кекур",
                                "target_lang_sense_clarification": "столбовидная скала"
                            },
                            {
                                "target_lang_word": "скала",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98ac42c7-03e0-30e9-b1f9-dd1e64c77769",
                    "sense_definition": "A crash or fall, often involving a vehicle.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "crash_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had a nasty <w>stack</w> on his bike yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера он сильно <w>упал</w> с велосипеда."
                            }
                        },
                        {
                            "text": "Going all out in a race means you might have a few <w>stacks</w>.",
                            "target_lang_translations": {
                                "ru": "Выкладываться в гонке по полной означает, что можно совершить пару <w>падений</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "smash",
                        "wreck"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in Australian/NZ English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Падение или авария, часто с участием транспортного средства (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падение",
                                "target_lang_sense_clarification": "с велосипеда, скейта и т.д."
                            },
                            {
                                "target_lang_word": "авария",
                                "target_lang_sense_clarification": "ДТП"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "15c6ff89-b4f8-3f32-9154-7c7267dcdef5",
                    "sense_definition": "To arrange objects in a pile, one on top of another.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_pile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>stack</w> those chairs in the corner after the meeting.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>сложите</w> эти стулья в углу после собрания."
                            }
                        },
                        {
                            "text": "He used to <w>stack</w> shelves in a supermarket before becoming a footballer.",
                            "target_lang_translations": {
                                "ru": "Раньше он <w>раскладывал</w> товар по полкам в супермаркете, прежде чем стать футболистом."
                            }
                        }
                    ],
                    "synonyms": [
                        "pile",
                        "pile up",
                        "heap",
                        "stack up"
                    ],
                    "common_phrases": [
                        "stack shelves",
                        "stack chairs",
                        "stack up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Складывать предметы в стопку, один на другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "складывать",
                                "target_lang_sense_clarification": "в стопку"
                            },
                            {
                                "target_lang_word": "нагромождать",
                                "target_lang_sense_clarification": "класть кучей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "739858ad-4db7-3703-a8b1-61a479d998ac",
                    "sense_definition": "To arrange or manipulate something dishonestly to gain an unfair advantage.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "rig_cheat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many citizens feel the political system is <w>stacked</w> against them.",
                            "target_lang_translations": {
                                "ru": "Многие граждане чувствуют, что политическая система <w>настроена</w> против них."
                            }
                        },
                        {
                            "text": "Someone is <w>stacking</w> the deck to ensure they win every time.",
                            "target_lang_translations": {
                                "ru": "Кто-то <w>подтасовывает</w> колоду, чтобы гарантировать себе победу каждый раз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stack the deck",
                        "stack against"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечестно подстраивать что-либо для получения преимущества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подтасовывать",
                                "target_lang_sense_clarification": "факты, карты"
                            },
                            {
                                "target_lang_word": "настраивать",
                                "target_lang_sense_clarification": "против кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fde94040-2dcc-3204-8ac2-dfe5df9e1984",
                    "sense_definition": "To crash a vehicle or fall over.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "crash_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>stacked</w> the car on the way to work this morning.",
                            "target_lang_translations": {
                                "ru": "Я <w>разбил</w> машину по дороге на работу сегодня утром."
                            }
                        },
                        {
                            "text": "He <w>stacked</w> his bike while trying a new trick.",
                            "target_lang_translations": {
                                "ru": "Он <w>грохнулся</w> с велосипеда, пытаясь выполнить новый трюк."
                            }
                        }
                    ],
                    "synonyms": [
                        "smash",
                        "wreck"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in Australian/NZ English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разбить транспортное средство или упасть (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбить",
                                "target_lang_sense_clarification": "машину"
                            },
                            {
                                "target_lang_word": "грохнуться",
                                "target_lang_sense_clarification": "упасть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38542f85-d6f1-3a4e-b315-2c065ee2c0e4",
                    "sense_definition": "To operate cumulatively; for effects or bonuses to add up when combined.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "gaming_cumulative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "These two strength potions do not <w>stack</w>, so use only one.",
                            "target_lang_translations": {
                                "ru": "Эти два зелья силы не <w>суммируются</w>, поэтому используй только одно."
                            }
                        },
                        {
                            "text": "If you get two magical widgets, their effects will <w>stack</w> to quadruple your power.",
                            "target_lang_translations": {
                                "ru": "Если ты получишь два магических виджета, их эффекты <w>суммируются</w> и увеличат твою силу вчетверо."
                            }
                        }
                    ],
                    "synonyms": [
                        "accumulate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суммироваться (об эффектах или бонусах в играх)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суммироваться",
                                "target_lang_sense_clarification": "накладываться друг на друга"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "b4c2063f-bc3f-312e-ae34-4be9f8a42a68",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Stack</w> will see you now.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Стэк</w> примет вас сейчас."
                            }
                        },
                        {
                            "text": "The <w>Stack</w> family has lived here for generations.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Стэк</w> живет здесь уже несколько поколений."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Стэк",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
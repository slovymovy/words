{
    "word_family": [
        "присоединить",
        "присоединение",
        "соединить",
        "соединение",
        "единый"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "00cdc551-9b09-3d02-b1d4-5ba031fcb103",
                    "sense_definition": "Вступить в какую-либо группу, стать участником какого-либо действия, сообщества или разговора.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "social_join",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы идём обедать, вы хотите <w>присоединиться</w> к нам?",
                            "target_lang_translations": {
                                "en": "We are going to lunch, do you want to <w>join</w> us?"
                            }
                        },
                        {
                            "text": "Позже к нашей компании решил <w>присоединиться</w> мой друг.",
                            "target_lang_translations": {
                                "en": "Later my friend decided to <w>join</w> our group."
                            }
                        }
                    ],
                    "synonyms": [
                        "подключиться"
                    ],
                    "antonyms": [
                        "отстать"
                    ],
                    "common_phrases": [
                        "присоединиться к нам",
                        "присоединиться к группе",
                        "присоединиться к разговору"
                    ],
                    "target_lang_definitions": {
                        "en": "To enter a group, become a participant in an activity, community, or conversation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "join",
                                "target_lang_sense_clarification": "become a member or come into the company of"
                            },
                            {
                                "target_lang_word": "join in",
                                "target_lang_sense_clarification": "participate in an activity or conversation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "082f8f8f-d5e4-3a19-aeef-0dd22bc9e4a4",
                    "sense_definition": "Установить связь, контакт или стать физической частью чего-либо целого.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_connect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой компьютер не может <w>присоединиться</w> к беспроводной сети.",
                            "target_lang_translations": {
                                "en": "My computer cannot <w>connect</w> to the wireless network."
                            }
                        },
                        {
                            "text": "Новый вагон должен <w>присоединиться</w> к поезду на следующей станции.",
                            "target_lang_translations": {
                                "en": "The new carriage must be <w>joined</w> to the train at the next station."
                            }
                        }
                    ],
                    "synonyms": [
                        "соединиться",
                        "подключиться"
                    ],
                    "antonyms": [
                        "отсоединиться",
                        "отключиться"
                    ],
                    "common_phrases": [
                        "присоединиться к сети",
                        "присоединиться к системе"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish a connection, contact, or become a physical part of a whole."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "connect",
                                "target_lang_sense_clarification": "establish a link, especially with networks or devices"
                            },
                            {
                                "target_lang_word": "join",
                                "target_lang_sense_clarification": "physically attach or link together"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5bc55ee-90d0-3935-8340-e0f8b7f7ad07",
                    "sense_definition": "Выразить согласие с чьим-либо мнением, поддержать чьи-то слова или действия.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <w>присоединиться</w> к поздравлениям и пожелать вам счастья.",
                            "target_lang_translations": {
                                "en": "I want to <w>join</w> in the congratulations and wish you happiness."
                            }
                        },
                        {
                            "text": "Коллеги решили <w>присоединиться</w> к мнению директора по этому вопросу.",
                            "target_lang_translations": {
                                "en": "The colleagues decided to <w>agree</w> with the director's opinion on this matter."
                            }
                        }
                    ],
                    "common_phrases": [
                        "присоединиться к мнению",
                        "присоединиться к словам",
                        "присоединиться к поздравлениям"
                    ],
                    "target_lang_definitions": {
                        "en": "To express agreement with someone's opinion, support someone's words or actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "join",
                                "target_lang_sense_clarification": "associate oneself with a sentiment or action"
                            },
                            {
                                "target_lang_word": "agree with",
                                "target_lang_sense_clarification": "support an opinion or view"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
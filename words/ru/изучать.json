                            },
                            {
                                "target_lang_word": "study",
                                "target_lang_sense_clarification": "to look at carefully and minutely"
                            },
                            {
                                "target_lang_word": "scrutinize",
                                "target_lang_sense_clarification": "to examine very carefully and critically"
                            }
                        ]
                    }
                }
            ]

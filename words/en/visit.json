{
    "word_family": [
        "visitor",
        "visitation"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dd444e0a-7e18-311b-8cd4-a9dad35289c5",
                    "sense_definition": "To go to see and spend time with a person socially.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to <w>visit</w> her grandparents for Christmas.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>навестить</w> своих бабушку и дедушку на Рождество."
                            }
                        },
                        {
                            "text": "We plan to <w>visit</w> friends in London next week.",
                            "target_lang_translations": {
                                "ru": "Мы планируем <w>навестить</w> друзей в Лондоне на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "call on"
                    ],
                    "common_phrases": [
                        "visit friends",
                        "visit family",
                        "visit relatives"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить или приезжать к кому-либо, чтобы провести время вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навещать",
                                "target_lang_sense_clarification": "несовершенный вид; регулярно приходить к кому-то"
                            },
                            {
                                "target_lang_word": "навестить",
                                "target_lang_sense_clarification": "совершенный вид; прийти к кому-то один раз"
                            },
                            {
                                "target_lang_word": "посещать",
                                "target_lang_sense_clarification": "более формальный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d96d232-087d-38f8-8a96-670cd0453fb4",
                    "sense_definition": "To go to a specific place to look around, for pleasure, or for a specific purpose.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Millions of tourists <w>visit</w> the famous tower every year.",
                            "target_lang_translations": {
                                "ru": "Миллионы туристов <w>посещают</w> знаменитую башню каждый год."
                            }
                        },
                        {
                            "text": "You should <w>visit</w> a museum while you are in the city.",
                            "target_lang_translations": {
                                "ru": "Тебе стоит <w>посетить</w> музей, пока ты в городе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "visit a museum",
                        "visit a website",
                        "visit a country"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить в определенное место для осмотра, развлечения или с конкретной целью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посещать",
                                "target_lang_sense_clarification": "несовершенный вид; ходить в музеи, страны, места"
                            },
                            {
                                "target_lang_word": "посетить",
                                "target_lang_sense_clarification": "совершенный вид; сходить в место"
                            },
                            {
                                "target_lang_word": "побывать",
                                "target_lang_sense_clarification": "провести время в каком-то месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c701b40b-05ac-312f-a21a-36ff7194e09d",
                    "sense_definition": "To cause something negative, such as punishment or suffering, to affect someone.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sins of the past were <w>visited</w> upon the new generation.",
                            "target_lang_translations": {
                                "ru": "Грехи прошлого <w>обрушились</w> на новое поколение."
                            }
                        },
                        {
                            "text": "A terrible plague was <w>visited</w> on the village.",
                            "target_lang_translations": {
                                "ru": "Страшная чума <w>обрушилась</w> на деревню."
                            }
                        }
                    ],
                    "common_phrases": [
                        "visit upon",
                        "visit with"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергать кого-либо чему-то негативному, например, наказанию или страданиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обрушивать",
                                "target_lang_sense_clarification": "насылать беду или наказание"
                            },
                            {
                                "target_lang_word": "ниспосылать",
                                "target_lang_sense_clarification": "обычно о божественном наказании или испытании"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b4e9d575-705d-3952-9b8f-72920aedde06",
                    "sense_definition": "A short stay with someone or at a place.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We paid a quick <w>visit</w> to James on our way home.",
                            "target_lang_translations": {
                                "ru": "Мы нанесли Джеймсу короткий <w>визит</w> по дороге домой."
                            }
                        },
                        {
                            "text": "Next time you are in town, give me a <w>visit</w>.",
                            "target_lang_translations": {
                                "ru": "В следующий раз, когда будешь в городе, нанеси мне <w>визит</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pay a visit",
                        "flying visit",
                        "worth a visit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткое пребывание у кого-либо или в каком-либо месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "визит",
                                "target_lang_sense_clarification": "официальное или вежливое посещение"
                            },
                            {
                                "target_lang_word": "посещение",
                                "target_lang_sense_clarification": "факт прихода куда-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42648d3b-ad38-3f51-86a1-9ab4faadffbc",
                    "sense_definition": "A professional appointment or consultation, especially with a doctor.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor is available for a home <w>visit</w> if you are too sick to travel.",
                            "target_lang_translations": {
                                "ru": "Врач доступен для <w>визита</w> на дом, если вы слишком больны, чтобы ехать."
                            }
                        },
                        {
                            "text": "My dental <w>visit</w> is scheduled for tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Мой <w>визит</w> к стоматологу запланирован на завтрашнее утро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "doctor's visit",
                        "home visit",
                        "site visit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Профессиональная встреча или консультация, особенно с врачом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "визит",
                                "target_lang_sense_clarification": "приход врача или к врачу"
                            },
                            {
                                "target_lang_word": "прием",
                                "target_lang_sense_clarification": "консультация у специалиста"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
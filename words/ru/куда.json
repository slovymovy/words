    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9159d387-5ade-3af6-a518-afee49cc4f1f",
                    "sense_definition": "В какое место, в каком направлении (вопросительное или относительное слово).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Скажите, пожалуйста, <w>куда</w> идёт этот автобус?",
                            "target_lang_translations": {
                                "en": "Could you please tell me <w>where</w> this bus goes?"
                            }
                        },
                        {
                            "text": "Я совершенно не понимаю, <w>куда</w> мы сейчас едем.",
                            "target_lang_translations": {
                                "en": "I absolutely do not understand <w>where</w> we are going right now."
                            }
                        }
                    ],
                    "antonyms": [
                        "откуда"
                    ],
                    "common_phrases": [
                        "куда глаза глядят",
                        "не знаю куда",
                        "куда угодно",
                        "бог знает куда"
                    ],
                    "target_lang_definitions": {
                        "en": "To what place; in what direction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "where",
                                "target_lang_sense_clarification": "to what place"
                            },
                            {
                                "target_lang_word": "whither",
                                "target_lang_sense_clarification": "archaic or formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b61cafd-cf10-3c11-b36a-a486ef1a3ab4",
                    "sense_definition": "Значительно, намного (используется для усиления сравнительной степени прилагательных или наречий).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сегодняшний экзамен оказался <w>куда</w> сложнее, чем я ожидал.",
                            "target_lang_translations": {
                                "en": "Today's exam turned out to be <w>much</w> harder than I expected."
                            }
                        },
                        {
                            "text": "Ему стало <w>куда</w> лучше после того, как он выпил горячего чая.",
                            "target_lang_translations": {
                                "en": "He felt <w>much</w> better after he drank some hot tea."
                            }
                        }
                    ],
                    "synonyms": [
                        "гора́здо",
                        "намно́го"
                    ],
                    "common_phrases": [
                        "куда лучше",
                        "куда больше",
                        "куда важнее",
                        "куда интереснее"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Significantly; much (used to intensify comparative adjectives or adverbs)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "much",
                                "target_lang_sense_clarification": "with comparative adjectives"
                            },
                            {
                                "target_lang_word": "far",
                                "target_lang_sense_clarification": "intensifier for comparisons"
                            },
                            {
                                "target_lang_word": "way",
                                "target_lang_sense_clarification": "colloquial intensifier"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5fbacdd-8d2b-3770-88d1-95b0e9f770bd",
                    "sense_definition": "Зачем, для чего, с какой целью (часто с оттенком неодобрения или недоумения).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Куда</w> тебе столько дров, если зима уже заканчивается?",
                            "target_lang_translations": {
                                "en": "<w>What</w> do you need so much firewood <w>for</w>, if winter is already ending?"
                            }
                        },
                        {
                            "text": "Не понимаю, <w>куда</w> нам так спешить в выходной день.",
                            "target_lang_translations": {
                                "en": "I don't understand <w>why</w> we need to rush so much on a day off."
                            }
                        }
                    ],
                    "synonyms": [
                        "зачем",
                        "к чему",
                        "для чего"
                    ],
                    "common_phrases": [
                        "куда тебе",
                        "куда нам",
                        "куда спешить"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "What for; for what purpose (often with a shade of disapproval or bewilderment)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "what ... for",
                                "target_lang_sense_clarification": "asking about purpose"
                            },
                            {
                                "target_lang_word": "why",
                                "target_lang_sense_clarification": "asking for reason"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75c78a0b-4cfe-3f79-9884-a7bfff264a0f",
                    "sense_definition": "В какое-нибудь место (употребляется вместо неопределённых наречий «куда-нибудь», «куда-либо»).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "indefinite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если вы <w>куда</w> отойдёте, обязательно предупредите дежурного.",
                            "target_lang_translations": {
                                "en": "If you go <w>anywhere</w>, be sure to warn the attendant."
                            }
                        },
                        {
                            "text": "Давай сходим <w>куда</w> вечером, чтобы не сидеть дома.",
                            "target_lang_translations": {
                                "en": "Let's go <w>somewhere</w> in the evening so we don't stay at home."
                            }
                        }
                    ],
                    "synonyms": [
                        "куда-нибудь",
                        "куда-либо",
                        "куда-то"
                    ],
                    "common_phrases": [
                        "сходить куда",
                        "поехать куда",
                        "выбраться куда"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To some place; somewhere (used instead of indefinite adverbs)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "somewhere",
                                "target_lang_sense_clarification": "to some place"
                            },
                            {
                                "target_lang_word": "anywhere",
                                "target_lang_sense_clarification": "to any place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f96e7fd4-5dcb-3ec1-abe2-002dcfa98d9e",
                    "sense_definition": "Выражает сомнение, скептицизм или невозможность сравниться с кем-либо или чем-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "skepticism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Куда</w> мне до него, он настоящий профессионал своего дела!",
                            "target_lang_translations": {
                                "en": "<w>How</w> can I compare to him, he is a real professional in his field!"
                            }
                        },
                        {
                            "text": "Да <w>куда</w> тебе понять такие сложные научные теории.",
                            "target_lang_translations": {
                                "en": "Oh, <w>how</w> could you understand such complex scientific theories."
                            }
                        }
                    ],
                    "common_phrases": [
                        "куда там",
                        "куда уж мне",
                        "куда тебе до"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Expresses doubt, skepticism, or impossibility of comparison."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "how",
                                "target_lang_sense_clarification": "rhetorical, implying impossibility"
                            },
                            {
                                "target_lang_word": "nowhere near",
                                "target_lang_sense_clarification": "idiomatic comparison"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77d939de-9fec-3580-b3ac-921e05a94763",
                    "sense_definition": "Как, насколько, в какой степени (обычно в восклицательных предложениях).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Куда</w> на выдумки природа таровата!",
                            "target_lang_translations": {
                                "en": "<w>How</w> inventive nature is!"
                            }
                        },
                        {
                            "text": "<w>Куда</w> как весело мы провели время в деревне.",
                            "target_lang_translations": {
                                "en": "We had <w>ever so</w> much fun in the village."
                            }
                        }
                    ],
                    "common_phrases": [
                        "куда как"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "How; to what extent (usually in exclamatory sentences)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "how",
                                "target_lang_sense_clarification": "exclamatory intensifier"
                            },
                            {
                                "target_lang_word": "ever so",
                                "target_lang_sense_clarification": "archaic or literary intensifier"
                            }
                        ]
                    }
                }
            ]

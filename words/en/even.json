    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "50e5c1ce-215c-31a1-826b-e2066e86bd75",
                    "sense_definition": "In reality; implying an extreme example in the case mentioned, often used to express surprise or emphasis.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I don't <w>even</w> know his name.",
                            "target_lang_translations": {
                                "ru": "Я <w>даже</w> не знаю его имени."
                            }
                        },
                        {
                            "text": "She didn't <w>even</w> say goodbye before leaving.",
                            "target_lang_translations": {
                                "ru": "Она <w>даже</w> не попрощалась перед уходом."
                            }
                        },
                        {
                            "text": "Even a child could understand this simple rule.",
                            "target_lang_translations": {
                                "ru": "<w>Даже</w> ребёнок мог бы понять это простое правило."
                            }
                        }
                    ],
                    "synonyms": [
                        "so much as"
                    ],
                    "common_phrases": [
                        "even if",
                        "even though",
                        "not even"
                    ],
                    "target_lang_definitions": {
                        "ru": "На самом деле; подразумевает крайний пример в упомянутом случае, часто используется для выражения удивления или усиления."

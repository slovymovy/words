    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17b46f8e-eaf7-3490-8f43-8a3f49df4681",
                    "sense_definition": "Собрать какое-либо количество чего-либо (грибов, ягод, предметов), взять много чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы пошли в лес и <набрали> полную корзину грибов.",
                            "target_lang_translations": {
                                "en": "We went to the forest and <picked> a full basket of mushrooms."
                            }
                        },
                        {
                            "text": "Перед поездкой он <набрал> много еды в дорогу.",
                            "target_lang_translations": {
                                "en": "Before the trip, he <gathered> a lot of food for the road."
                            }
                        }
                    ],
                    "common_phrases": [
                        "набрать грибов",
                        "набрать ягод",
                        "набрать полную корзину",
                        "набрать кучу вещей"
                    ],
                    "target_lang_definitions": {
                        "en": "To gather or collect a quantity of something (mushrooms, berries, objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gather",
                                "target_lang_sense_clarification": "general collection of items"
                            },
                            {
                                "target_lang_word": "collect",
                                "target_lang_sense_clarification": "bringing things together"
                            },
                            {
                                "target_lang_word": "pick",
                                "target_lang_sense_clarification": "specifically for mushrooms, berries, or flowers"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "258875e4-7c5e-34e6-a53f-62a49124d5b9",
                    "sense_definition": "Позвонить кому-либо по телефону, нажимая цифры номера.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если тебе станет скучно, просто <набери> мне.",
                            "target_lang_translations": {
                                "en": "If you get bored, just <call> me."
                            }
                        },
                        {
                            "text": "Я пытался <набрать> твой номер, но было занято.",
                            "target_lang_translations": {
                                "en": "I tried to <dial> your number, but it was busy."
                            }
                        }
                    ],
                    "common_phrases": [
                        "набрать номер",
                        "набрать другу",
                        "набрать меня"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used simply as 'to call'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To call someone by phone or press the digits of a number."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dial",
                                "target_lang_sense_clarification": "to press numbers on a phone"
                            },
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "colloquial usage for phoning someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0523209-5500-38e0-8c2b-00eee2449449",
                    "sense_definition": "Увеличить какой-либо количественный показатель (скорость, вес, высоту, очки).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "increase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автомобиль выехал на трассу и быстро <набрал> скорость.",
                            "target_lang_translations": {
                                "en": "The car drove onto the highway and quickly <picked up> speed."
                            }
                        },
                        {
                            "text": "За время отпуска я <набрал> несколько лишних килограммов.",
                            "target_lang_translations": {
                                "en": "During the vacation, I <gained> a few extra kilograms."
                            }
                        }
                    ],
                    "synonyms": [
                        "увеличить"
                    ],
                    "antonyms": [
                        "сбросить",
                        "уменьшить",
                        "сократить"
                    ],
                    "common_phrases": [
                        "набрать вес",
                        "набрать скорость",
                        "набрать высоту",
                        "набрать очки"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase a quantitative indicator (speed, weight, altitude, points)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gain",
                                "target_lang_sense_clarification": "weight, points, or altitude"
                            },
                            {
                                "target_lang_word": "pick up",
                                "target_lang_sense_clarification": "specifically for speed"
                            },
                            {
                                "target_lang_word": "put on",
                                "target_lang_sense_clarification": "specifically for weight"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0070e00-edb3-3e7e-a56e-94ffa563d3f6",
                    "sense_definition": "Написать текст или символы с помощью клавиатуры компьютера или телефона.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "typing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Секретарь успела <набрать> важный документ за десять минут.",
                            "target_lang_translations": {
                                "en": "The secretary managed to <type> the important document in ten minutes."
                            }
                        },
                        {
                            "text": "Чтобы войти в подъезд, нужно <набрать> код на двери.",
                            "target_lang_translations": {
                                "en": "To enter the building, you need to <enter> the code on the door."
                            }
                        }
                    ],
                    "common_phrases": [
                        "набрать текст",
                        "набрать сообщение",
                        "набрать пароль",
                        "набрать код"
                    ],
                    "target_lang_definitions": {
                        "en": "To write text or symbols using a keyboard."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "type",
                                "target_lang_sense_clarification": "text on a computer or phone"
                            },
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "a code or password"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57969ab6-73c0-3d90-a979-9613f433b89f",
                    "sense_definition": "Вобрать внутрь себя (обычно воздух в легкие или жидкость в рот).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "absorbing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пловец глубоко вздохнул и <набрал> полные легкие воздуха.",
                            "target_lang_translations": {
                                "en": "The swimmer took a deep breath and <filled> his lungs with air."
                            }
                        },
                        {
                            "text": "Он <набрал> в рот воды и не мог ничего сказать.",
                            "target_lang_translations": {
                                "en": "He <took> a mouthful of water and couldn't say anything."
                            }
                        }
                    ],
                    "common_phrases": [
                        "набрать воздуха",
                        "набрать воды в рот",
                        "набрать полные легкие"
                    ],
                    "target_lang_definitions": {
                        "en": "To take inside oneself (usually air into lungs or liquid into mouth)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take in",
                                "target_lang_sense_clarification": "air or liquid"
                            },
                            {
                                "target_lang_word": "fill",
                                "target_lang_sense_clarification": "lungs or mouth with something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc25d76b-c84e-3ffa-afcf-f96222714d48",
                    "sense_definition": "Принять людей в группу, на работу, на учебу или на службу.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "recruiting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Завод планирует <набрать> новых рабочих в следующем месяце.",
                            "target_lang_translations": {
                                "en": "The factory plans to <hire> new workers next month."
                            }
                        },
                        {
                            "text": "Тренер <набрал> в команду самых сильных и быстрых игроков.",
                            "target_lang_translations": {
                                "en": "The coach <recruited> the strongest and fastest players for the team."
                            }
                        }
                    ],
                    "synonyms": [
                        "нанять",
                        "завербовать",
                        "принять"
                    ],
                    "antonyms": [
                        "распустить",
                        "выгнать",
                        "сократить"
                    ],
                    "common_phrases": [
                        "набрать сотрудников",
                        "набрать команду",
                        "набрать группу",
                        "набрать персонал"
                    ],
                    "target_lang_definitions": {
                        "en": "To accept people into a group, job, or service."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recruit",
                                "target_lang_sense_clarification": "for a team or group"
                            },
                            {
                                "target_lang_word": "hire",
                                "target_lang_sense_clarification": "for employment"
                            }
                        ]
                    }
                }
            ]
        }

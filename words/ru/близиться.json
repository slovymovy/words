    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0cdc4c69-264e-3d87-bacf-ff398f4a2a56",
                    "sense_definition": "Наступать или происходить в скором времени (о времени, событиях или состояниях).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "approach_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лето <близилось> к концу, и дни становились заметно короче.",
                            "target_lang_translations": {
                                "en": "Summer was <drawing> to a close, and the days were becoming noticeably shorter."
                            }
                        },
                        {
                            "text": "Время обеда <близится>, поэтому нам пора заканчивать работу.",
                            "target_lang_translations": {
                                "en": "Lunchtime is <approaching>, so it is time for us to finish work."
                            }
                        }
                    ],
                    "synonyms": [
                        "приближаться",
                        "быть на носу"
                    ],
                    "common_phrases": [
                        "близиться к концу",
                        "близиться к завершению",
                        "близиться к развязке",
                        "близится вечер"
                    ],
                    "target_lang_definitions": {
                        "en": "To happen or occur soon (regarding time, events, or states)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "approach",
                                "target_lang_sense_clarification": "time or event"
                            },
                            {
                                "target_lang_word": "draw near",
                                "target_lang_sense_clarification": "time"
                            },
                            {
                                "target_lang_word": "near",
                                "target_lang_sense_clarification": "to get close to a state or time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6d5ffff-cfde-327c-b7ab-786a9b763d59",
                    "sense_definition": "Перемещаясь в пространстве, сокращать расстояние до кого-либо или чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "approach_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Одинокий путник медленно <близился> к городским воротам.",
                            "target_lang_translations": {
                                "en": "The lonely traveler was slowly <approaching> the city gates."
                            }
                        },
                        {
                            "text": "Лодка <близилась> к берегу, покачиваясь на высоких волнах.",
                            "target_lang_translations": {
                                "en": "The boat was <nearing> the shore, bobbing on the high waves."
                            }
                        }
                    ],
                    "synonyms": [
                        "приближаться"
                    ],
                    "antonyms": [
                        "удаляться"
                    ],
                    "target_lang_definitions": {
                        "en": "To reduce the distance to someone or something while moving in space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "approach",
                                "target_lang_sense_clarification": "physical movement"
                            },
                            {
                                "target_lang_word": "draw near",
                                "target_lang_sense_clarification": "physical movement"
                            }
                        ]
                    }
                }
            ]
        }
    ]

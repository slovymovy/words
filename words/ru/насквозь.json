                            },
                            {
                                "target_lang_word": "right through",
                                "target_lang_sense_clarification": "perceiving the truth or essence of someone"
                            },
                            {
                                "target_lang_word": "to the bone",
                                "target_lang_sense_clarification": "physically affecting the body (cold, wetness)"
                            },
                            {
                                "target_lang_word": "through",
                                "target_lang_sense_clarification": "completely (used with soaked/wet)"
                            }

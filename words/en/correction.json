    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "94a5edfd-2f56-357b-ba36-551188dacd4b",
                    "sense_definition": "A change made to something in order to fix a mistake or improve it.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_correction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please make the necessary <w>corrections</w> to the document before printing it.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, внесите необходимые <w>исправления</w> в документ перед печатью."
                            }
                        },
                        {
                            "text": "I noticed a small spelling <w>correction</w> in the latest version of your report.",
                            "target_lang_translations": {
                                "ru": "Я заметил небольшое <w>исправление</w> в последней версии вашего отчета."
                            }
                        }
                    ],
                    "synonyms": [
                        "amendment",
                        "revision",
                        "alteration",
                        "modification"
                    ],
                    "antonyms": [
                        "error",
                        "mistake"
                    ],
                    "common_phrases": [
                        "make a correction",
                        "spelling correction",
                        "minor correction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменение, внесенное во что-либо с целью исправления ошибки или улучшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исправление",
                                "target_lang_sense_clarification": "устранение ошибки"
                            },
                            {
                                "target_lang_word": "поправка",
                                "target_lang_sense_clarification": "небольшое изменение"
                            },
                            {
                                "target_lang_word": "правка",
                                "target_lang_sense_clarification": "редактирование текста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbdb4db5-32ce-3fc4-86d6-4883f49b1118",
                    "sense_definition": "The act or process of setting something right or making it accurate.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_correction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>correction</w> of these errors will take more time than we expected.",
                            "target_lang_translations": {
                                "ru": "<w>Исправление</w> этих ошибок займет больше времени, чем мы ожидали."
                            }
                        },
                        {
                            "text": "He is responsible for the <w>correction</w> of all exam papers in the department.",
                            "target_lang_translations": {
                                "ru": "Он отвечает за <w>проверку</w> всех экзаменационных работ на кафедре."
                            }
                        }
                    ],
                    "synonyms": [
                        "rectification",
                        "fixing",
                        "repair"
                    ],
                    "common_phrases": [
                        "under correction",
                        "stand for correction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс исправления чего-либо или приведения в точное соответствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исправление",
                                "target_lang_sense_clarification": "процесс улучшения"
                            },
                            {
                                "target_lang_word": "коррекция",
                                "target_lang_sense_clarification": "техническое или формальное исправление"
                            },
                            {
                                "target_lang_word": "правка",
                                "target_lang_sense_clarification": "процесс редактирования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5146710e-d431-3395-bf56-c26d9c2684a5",
                    "sense_definition": "Punishment intended to improve behavior or rehabilitate an offender, often referring to the prison system.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The state is investing more money in its <w>corrections</w> system to reduce crime.",
                            "target_lang_translations": {
                                "ru": "Государство вкладывает больше средств в систему <w>исполнения наказаний</w>, чтобы снизить уровень преступности."
                            }
                        },
                        {
                            "text": "She works in the department of <w>corrections</w> as a counselor for inmates.",
                            "target_lang_translations": {
                                "ru": "Она работает в департаменте <w>исправительных учреждений</w> консультантом для заключенных."
                            }
                        }
                    ],
                    "synonyms": [
                        "discipline",
                        "rehabilitation"
                    ],
                    "common_phrases": [
                        "department of corrections",
                        "corrections officer",
                        "house of correction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наказание или система мер, направленная на исправление поведения правонарушителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исправительные меры",
                                "target_lang_sense_clarification": "меры воздействия"
                            },
                            {
                                "target_lang_word": "наказание",
                                "target_lang_sense_clarification": "кара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bc94ba9-0aac-3b98-8865-98b809d95c26",
                    "sense_definition": "A decline in stock market prices, typically of at least 10%, after a period of rising values.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many investors are worried about a possible market <w>correction</w> in the coming weeks.",
                            "target_lang_translations": {
                                "ru": "Многие инвесторы обеспокоены возможной <w>коррекцией</w> рынка в ближайшие недели."
                            }
                        },
                        {
                            "text": "The stock market underwent a sharp <w>correction</w> after the unexpected news.",
                            "target_lang_translations": {
                                "ru": "Фондовый рынок претерпел резкую <w>коррекцию</w> после неожиданных новостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "downturn",
                        "dip",
                        "drop"
                    ],
                    "antonyms": [
                        "rally",
                        "boom"
                    ],
                    "common_phrases": [
                        "market correction",
                        "price correction"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "finance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Снижение рыночных цен (обычно не менее чем на 10%) после периода роста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коррекция",
                                "target_lang_sense_clarification": "финансовый термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b287e0e0-8dcc-3c64-8690-ccd69ba4cfff",
                    "sense_definition": "An amount or quantity added to or subtracted from a value to adjust it for accuracy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot made a course <w>correction</w> to account for the strong wind.",
                            "target_lang_translations": {
                                "ru": "Пилот выполнил <w>коррекцию</w> курса, чтобы учесть сильный ветер."
                            }
                        },
                        {
                            "text": "We applied a mathematical <w>correction</w> to the data to ensure the results were precise.",
                            "target_lang_translations": {
                                "ru": "Мы применили математическую <w>поправку</w> к данным, чтобы обеспечить точность результатов."
                            }
                        }
                    ],
                    "synonyms": [
                        "adjustment",
                        "offset",
                        "compensation"
                    ],
                    "common_phrases": [
                        "course correction",
                        "color correction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Величина, добавляемая или вычитаемая для уточнения значения или курса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поправка",
                                "target_lang_sense_clarification": "величина корректировки"
                            },
                            {
                                "target_lang_word": "коррекция",
                                "target_lang_sense_clarification": "изменение курса или параметров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19af3c5a-de40-3a20-a7c5-ed97e2a3b1ac",
                    "sense_definition": "A word used in radio communication to indicate that a mistake was made and the correct information follows.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Target is at grid four-five, <w>correction</w>, grid four-six.",
                            "target_lang_translations": {
                                "ru": "Цель в квадрате четыре-пять, <w>поправка</w>, квадрат четыре-шесть."
                            }
                        },
                        {
                            "text": "The time is fourteen hundred hours, <w>correction</w>, fifteen hundred hours.",
                            "target_lang_translations": {
                                "ru": "Время четырнадцать ноль-ноль, <w>поправка</w>, пятнадцать ноль-ноль."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "radio communication"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в радиосвязи для обозначения ошибки и последующего сообщения правильной информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поправка",
                                "target_lang_sense_clarification": "команда в радиообмене"
                            }
                        ]
                    }
                }
            ]
        }
    ]

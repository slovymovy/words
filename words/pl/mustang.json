                {
                    "sense_id": "fb56ca2a-295c-3071-acf0-66b2c894d8bf",
                    "sense_definition": "Zdziczały koń z równin Ameryki Północnej, potomek koni przywiezionych przez hiszpańskich konkwistadorów.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "horse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na rozległej prerii pasło się wielkie stado dzikich <w>mustangów</w>.",
                            "target_lang_translations": {
                                "ru": "На обширной прерии паслось большое стадо диких <w>мустангов</w>."
                            }
                        },
                        {
                            "text": "Ten narowisty <w>mustang</w> nie pozwolił nikomu na siebie wsiąść.",
                            "target_lang_translations": {
                                "ru": "Этот норовистый <w>мустанг</w> никому не позволял сесть на себя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dziki mustang",
                        "stado mustangów",
                        "ujeżdżać mustanga"
                    ],
                    "target_lang_definitions": {

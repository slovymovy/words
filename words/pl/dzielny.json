                    "sense_id": "da1b38f8-2dfb-3fa1-8eb7-df8d36db39fe",
                    "sense_definition": "Odważny, niepoddający się przeciwnościom, potrafiący zachować zimną krew w trudnych sytuacjach.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_bravery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój syn był bardzo <w>dzielny</w> podczas wizyty u dentysty i wcale nie płakał.",
                            "target_lang_translations": {
                                "ru": "Мой сын был очень <w>храбрым</w> во время визита к стоматологу и совсем не плакал."
                            }
                        },
                        {
                            "text": "<w>Dzielni</w> strażacy z narażeniem życia ratowali mieszkańców płonącego budynku.",
                            "target_lang_translations": {
                                "ru": "<w>Отважные</w> пожарные, рискуя жизнью, спасали жильцов горящего здания."
                            }
                        }
                    ],
                    "synonyms": [
                        "odważny",
                        "śmiały",
                        "nieustraszony"
                    ],
                    "antonyms": [
                        "tchórzliwy"
                    ],
                    "common_phrases": [
                        "dzielny żołnierz",
                        "dzielny pacjent",
                        "bądź dzielny",
                        "dzielna postawa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Храбрый, не сдающийся перед лицом невзгод, умеющий сохранять хладнокровие в трудных ситуациях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "храбрый",
                                "target_lang_sense_clarification": "общее понятие; тот, кто не боится опасности"
                            },
                            {
                                "target_lang_word": "смелый",
                                "target_lang_sense_clarification": "решительный, не боящийся риска"
                            },
                            {
                                "target_lang_word": "отважный",
                                "target_lang_sense_clarification": "бесстрашный, готовый к подвигу"
                            },
                            {

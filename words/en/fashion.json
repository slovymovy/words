                },
                {
                    "sense_id": "bc5d56f7-73b4-37d2-9c1b-1ee944b4a14f",
                    "sense_definition": "A current trend or custom that is popular at a specific time.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "style_trend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeans never seem to go out of <w>fashion</w>.",
                            "target_lang_translations": {
                                "ru": "Кажется, джинсы никогда не выходят из <w>моды</w>."
                            }
                        },
                        {
                            "text": "That style of music is back in <w>fashion</w> this year.",
                            "target_lang_translations": {
                                "ru": "Этот стиль музыки снова в <w>моде</w> в этом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "trend",
                        "craze",
                        "fad",
                        "vogue"

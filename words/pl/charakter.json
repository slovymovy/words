            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7da96ceb-a276-3df7-8dd8-d4612e961bc7",
                    "sense_definition": "Zespół cech psychicznych właściwych danej osobie, określający jej zachowanie i sposób bycia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "personality_traits",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat ma bardzo spokojny i ugodowy <w>charakter</w>.",
                            "target_lang_translations": {
                                "ru": "У моего брата очень спокойный и покладистый <w>характер</w>."
                            }
                        },
                        {
                            "text": "Ona ma trudny <w>charakter</w>, dlatego często kłóci się z innymi ludźmi.",
                            "target_lang_translations": {
                                "ru": "У нее тяжелый <w>характер</w>, поэтому она часто ссорится с другими людьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "usposobienie",
                        "temperament",
                        "natura"
                    ],
                    "common_phrases": [
                        "trudny charakter",
                        "cechy charakteru",
                        "zły charakter",
                        "łagodny charakter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность психических качеств, свойственных данному человеку, определяющих его поведение и образ действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "характер",
                                "target_lang_sense_clarification": "личность, нрав"
                            },
                            {
                                "target_lang_word": "нрав",
                                "target_lang_sense_clarification": "темперамент, уклад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27cb7a30-1daa-32c9-b1f6-c2705c08df72",
                    "sense_definition": "Zespół cech wyróżniających dany przedmiot, zjawisko lub proces; specyfika lub rodzaj czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "nature_of_thing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To spotkanie ma <w>charakter</w> wyłącznie prywatny, a nie służbowy.",
                            "target_lang_translations": {
                                "ru": "Эта встреча носит исключительно частный <w>характер</w>, а не служебный."
                            }
                        },
                        {
                            "text": "Działania te mają <w>charakter</w> zapobiegawczy i chronią przed rozwojem choroby.",
                            "target_lang_translations": {
                                "ru": "Эти действия носят профилактический <w>характер</w> и защищают от развития болезни."
                            }
                        }
                    ],
                    "synonyms": [
                        "natura",
                        "właściwość",
                        "zabarwienie"
                    ],
                    "common_phrases": [
                        "charakter oficjalny",
                        "charakter prawny",
                        "mieć charakter",
                        "o charakterze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность отличительных черт предмета, явления или процесса; специфика или вид чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "характер",
                                "target_lang_sense_clarification": "свойство, особенность"
                            },
                            {
                                "target_lang_word": "природа",
                                "target_lang_sense_clarification": "сущность, происхождение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a56cb01-2da7-32d7-90c1-3bcfadeb60f7",
                    "sense_definition": "Siła woli, nieustępliwość, stałość przekonań i odwaga cywilna.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "personality_traits",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trudnych chwilach pokazał swój prawdziwy <w>charakter</w> i nie poddał się.",
                            "target_lang_translations": {
                                "ru": "В трудные моменты он показал свой настоящий <w>характер</w> и не сдался."
                            }
                        },
                        {
                            "text": "Trzeba mieć <w>charakter</w>, żeby odmówić w takiej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Нужно иметь <w>характер</w>, чтобы отказать в такой ситуации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "człowiek z charakterem",
                        "mieć charakter",
                        "pokazać charakter",
                        "wyrabiać charakter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сила воли, упорство, твердость убеждений и смелость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "характер",
                                "target_lang_sense_clarification": "сила воли, стержень"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70818339-e2b3-3d9e-8cdc-31bf853275a6",
                    "sense_definition": "Rola lub funkcja, w jakiej ktoś występuje w danej sytuacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "role_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek został zatrudniony w tej firmie w <w>charakterze</w> doradcy.",
                            "target_lang_translations": {
                                "ru": "Мой дядя был нанят в эту фирму в <w>качестве</w> консультанта."
                            }
                        },
                        {
                            "text": "Zostałem wezwany do sądu, aby zeznawać w <w>charakterze</w> świadka.",
                            "target_lang_translations": {
                                "ru": "Меня вызвали в суд давать показания в <w>качестве</w> свидетеля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "w charakterze świadka",
                        "w charakterze gościa",
                        "w charakterze eksperta"
                    ],
                    "traits": [
                        {

                    ],
                    "synonyms": [
                        "подле"
                    ],
                    "common_phrases": [
                        "под Москвой",
                        "под городом"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates location close to or not far from a place (usually a city)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "near",
                                "target_lang_sense_clarification": "in the vicinity of"
                            },
                            {
                                "target_lang_word": "just outside",
                                "target_lang_sense_clarification": "located immediately outside a city or town"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7431409b-f3ef-32bf-9853-4239be6aa528",
                    "sense_definition": "Указывает на предмет или место, для которого что-либо предназначено или используется.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эту пустую банку можно использовать <под> варенье.",
                            "target_lang_translations": {
                                "en": "This empty jar can be used <w>for</w> jam."
                            }
                        },
                        {
                            "text": "Мы расчистили площадку <под> строительство дома.",
                            "target_lang_translations": {
                                "en": "We cleared the site <w>for</w> the construction of the house."
                            }
                        },
                        {
                            "text": "В новой квартире выделили комнату <под> детскую.",
                            "target_lang_translations": {
                                "en": "In the new apartment, a room was allocated <w>for</w> the nursery."
                            }
                        }

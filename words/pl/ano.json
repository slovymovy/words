                    "frequency": "Middle",
                    "semantic_group_id": "introduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pytasz, co tam robiłem? <w>Ano</w> siedziałem i czekałem na ciebie."
                        },
                        {
                            "text": "Gdzie się podziały te pieniądze? <w>Ano</w> wydaliśmy wszystko na remont."
                        }
                    ],
                    "synonyms": [
                        "otóż",
                        "więc",
                        "a więc"
                    ],
                    "common_phrases": [
                        "ano właśnie",
                        "ano widzisz"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""

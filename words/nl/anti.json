{
    "word_family": [
        "antipathie",
        "antigif",
        "antistof",
        "antithese"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "82e21231-f124-3d6f-8159-fd00489bea21",
                    "sense_definition": "Tegen iets zijn; een afwijzende houding hebben.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben fel <w>anti</w> als het gaat om oneerlijkheid.",
                            "target_lang_translations": {
                                "ru": "Я категорически <w>против</w>, когда речь идет о нечестности.",
                                "en": "I am fiercely <w>anti</w> when it comes to dishonesty."
                            }
                        },
                        {
                            "text": "Waarom ben je vandaag zo <w>anti</w> tegenover mijn plannen?",
                            "target_lang_translations": {
                                "ru": "Почему ты сегодня так <w>настроен против</w> моих планов?",
                                "en": "Why are you so <w>anti</w> towards my plans today?"
                            }
                        }
                    ],
                    "synonyms": [
                        "tegen"
                    ],
                    "antonyms": [
                        "pro"
                    ],
                    "common_phrases": [
                        "ergens anti zijn",
                        "anti reageren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть против чего-либо; иметь отрицательное отношение.",
                        "en": "To be against something; to have a dismissive attitude."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "против",
                                "target_lang_sense_clarification": "несогласный, имеющий возражения"
                            },
                            {
                                "target_lang_word": "настроен против",
                                "target_lang_sense_clarification": "имеющий враждебный или отрицательный настрой"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "anti",
                                "target_lang_sense_clarification": "opposed to a specific idea or practice, often colloquial"
                            },
                            {
                                "target_lang_word": "opposed",
                                "target_lang_sense_clarification": "disapproving of or disagreeing with something"
                            },
                            {
                                "target_lang_word": "against",
                                "target_lang_sense_clarification": "in opposition to"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "1361d8ce-d792-3896-ade2-ede5bab46721",
                    "sense_definition": "Gericht tegen; in strijd met.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn gedrag is <w>anti</w> alle regels van de school.",
                            "target_lang_translations": {
                                "ru": "Его поведение <w>против</w> всех школьных правил.",
                                "en": "His behavior is <w>against</w> all the rules of the school."
                            }
                        },
                        {
                            "text": "Deze maatregel is duidelijk <w>anti</w> de belangen van de werknemers.",
                            "target_lang_translations": {
                                "ru": "Эта мера явно <w>против</w> интересов работников.",
                                "en": "This measure is clearly <w>against</w> the interests of the employees."
                            }
                        }
                    ],
                    "synonyms": [
                        "tegen"
                    ],
                    "antonyms": [
                        "pro"
                    ],
                    "common_phrases": [
                        "anti alles",
                        "anti de gevestigde orde"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направленный против; противоречащий.",
                        "en": "Directed against; in conflict with."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "против",
                                "target_lang_sense_clarification": "указывает на направленность действия наперекор кому-либо или чему-либо"
                            },
                            {
                                "target_lang_word": "вопреки",
                                "target_lang_sense_clarification": "вразрез с, несмотря на"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "against",
                                "target_lang_sense_clarification": "indicating opposition or conflict"
                            },
                            {
                                "target_lang_word": "contrary to",
                                "target_lang_sense_clarification": "in conflict with rules or expectations"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00c44122-e0a7-3de6-a0cd-366f1dc7f6b7",
                    "sense_definition": "Iemand die ergens op tegen is; een tegenstander.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>anti's</w> lieten luid van zich horen tijdens de vergadering.",
                            "target_lang_translations": {
                                "ru": "<w>Противники</w> громко заявили о себе во время собрания.",
                                "en": "The <w>antis</w> made themselves heard loudly during the meeting."
                            }
                        },
                        {
                            "text": "Er was een felle discussie tussen de voorstanders en de <w>anti's</w>.",
                            "target_lang_translations": {
                                "ru": "Разгорелась ожесточенная дискуссия между сторонниками и <w>противниками</w>.",
                                "en": "There was a fierce discussion between the proponents and the <w>antis</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de anti's"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто выступает против чего-либо; оппонент.",
                        "en": "Someone who is against something; an opponent."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противник",
                                "target_lang_sense_clarification": "человек, который противодействует или возражает"
                            },
                            {
                                "target_lang_word": "оппозиционер",
                                "target_lang_sense_clarification": "участник сопротивления или оппозиции"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "opponent",
                                "target_lang_sense_clarification": "a person who disagrees with or resists a proposal or practice"
                            },
                            {
                                "target_lang_word": "anti",
                                "target_lang_sense_clarification": "a person opposed to a particular concept or principle"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
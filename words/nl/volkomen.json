                        "totaal",
                        "volledig"
                    ],
                    "antonyms": [
                        "onvolkomen"
                    ],
                    "common_phrases": [
                        "volkomen stilte",
                        "volkomen vreemde",
                        "volkomen onzin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без каких-либо недостатков; целый и полностью завершенный.",
                        "en": "Without anything missing; entirely complete."
                    },

                },
                {
                    "sense_id": "81e8372a-8934-3f89-a356-62dd5f465a42",
                    "sense_definition": "Paid for but not consumed by a customer, so that it can be given to a less fortunate person.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "charity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He left money for a <w>suspended</w> coffee to help someone in need.",
                            "target_lang_translations": {
                                "ru": "Он оставил деньги на <w>подвешенный</w> кофе, чтобы помочь нуждающемуся."
                            }
                        },
                        {
                            "text": "The cafe has a board listing every <w>suspended</w> meal available for the homeless.",
                            "target_lang_translations": {
                                "ru": "В кафе есть доска со списком всех <w>подвешенных</w> блюд, доступных для бездомных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "suspended coffee"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оплаченный заранее для передачи нуждающимся (обычно о еде или напитках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвешенный",
                                "target_lang_sense_clarification": "благотворительность (кофе, обед)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91bff15c-1c7b-3ea4-b6ab-01b017230a87",
                    "sense_definition": "Existing as solid particles dispersed throughout a fluid but not dissolved.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fine dust remained <w>suspended</w> in the air for hours after the explosion.",
                            "target_lang_translations": {
                                "ru": "Мелкая пыль оставалась <w>взвешенной</w> в воздухе в течение нескольких часов после взрыва."
                            }
                        },
                        {
                            "text": "Shake the bottle well to ensure the <w>suspended</w> particles are evenly distributed.",
                            "target_lang_translations": {
                                "ru": "Хорошо встряхните бутылку, чтобы <w>взвешенные</w> частицы распределились равномерно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "suspended solids",
                        "suspended matter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся во взвешенном состоянии (о частицах в жидкости или газе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взвешенный",
                                "target_lang_sense_clarification": "химия/физика"
                            }

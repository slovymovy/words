                    "target_lang_definitions": {
                        "en": "To possess; to be the owner of something.",
                        "ru": "Владеть чем-либо; быть собственником чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "to possess or own"
                            },
                            {
                                "target_lang_word": "own",
                                "target_lang_sense_clarification": "to have legal possession of"
                            },
                            {
                                "target_lang_word": "possess",
                                "target_lang_sense_clarification": "formal: to have or own"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "иметь",
                                "target_lang_sense_clarification": "владеть или обладать"
                            },
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "быть в наличии (в конструкции 'у меня есть')"
                            },
                            {
                                "target_lang_word": "владеть",
                                "target_lang_sense_clarification": "быть собственником"
                            }
                        ]

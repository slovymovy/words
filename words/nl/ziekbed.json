                {
                    "sense_id": "a726d441-7b34-34bf-884f-cd2b7e2b82ea",
                    "sense_definition": "Een periode waarin iemand door ziekte in bed moet blijven liggen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een langdurig <w>ziekbed</w> is hij gelukkig weer helemaal hersteld.",
                            "target_lang_translations": {
                                "en": "After a long <w>illness</w>, he has fortunately fully recovered.",
                                "ru": "К счастью, после продолжительной <w>болезни</w> он полностью выздоровел."
                            }
                        },
                        {
                            "text": "Zij overleed vrijdag in haar woonplaats na een kort <w>ziekbed</w>.",
                            "target_lang_translations": {
                                "en": "She passed away on Friday in her hometown after a short <w>illness</w>.",
                                "ru": "Она скончалась в пятницу в своем родном городе после скоротечной <w>болезни</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ziekteperiode"
                    ],
                    "common_phrases": [
                        "langdurig ziekbed",
                        "kort ziekbed",
                        "zwaar ziekbed"
                    ],
                    "target_lang_definitions": {
                        "en": "A period during which someone must stay in bed due to illness.",
                        "ru": "Период, в течение которого кто-то вынужден оставаться в постели из-за болезни."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "illness",
                                "target_lang_sense_clarification": "period of sickness"
                            },
                            {
                                "target_lang_word": "sickbed",
                                "target_lang_sense_clarification": "period of confinement to bed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "болезнь",
                                "target_lang_sense_clarification": "период заболевания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "229a7ed6-65fa-3440-bcaa-1c8bd6b4c6a5",
                    "sense_definition": "Het bed waarin een zieke ligt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dokter zat aan het <w>ziekbed</w> om de patiënt te onderzoeken.",
                            "target_lang_translations": {
                                "en": "The doctor sat at the <w>bedside</w> to examine the patient.",
                                "ru": "Врач сидел у <w>постели</w> больного, чтобы осмотреть пациента."
                            }
                        },
                        {
                            "text": "De hele familie verzamelde zich rond het <w>ziekbed</w> van grootvader.",
                            "target_lang_translations": {
                                "en": "The whole family gathered around grandfather's <w>sickbed</w>.",
                                "ru": "Вся семья собралась у <w>постели</w> дедушки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan het ziekbed",
                        "rond het ziekbed"
                    ],
                    "target_lang_definitions": {
                        "en": "The bed in which a sick person lies.",
                        "ru": "Кровать, в которой лежит больной."
                    },
                    "translations": {

                {
                    "sense_id": "01c5e381-d850-350f-a194-8c7d9ca15040",
                    "sense_definition": "Widziana przestrzeń, krajobraz; to, co można zobaczyć z określonego miejsca.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze szczytu góry roztaczał się wspaniały <w>widok</w> na całą okolicę.",
                            "target_lang_translations": {
                                "ru": "С вершины горы открывался великолепный <w>вид</w> на всю окрестность."
                            }
                        },
                        {
                            "text": "Z okna naszego hotelu mamy piękny <w>widok</w> na morze.",
                            "target_lang_translations": {
                                "ru": "Из окна нашего отеля открывается прекрасный <w>вид</w> на море."
                            }
                        }
                    ],
                    "synonyms": [
                        "krajobraz",
                        "panorama",
                        "pejzaż"
                    ],
                    "common_phrases": [
                        "widok z okna",
                        "piękny widok",
                        "widok na morze",
                        "rozległy widok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пространство, которое можно увидеть с определенного места; пейзаж."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "пейзаж, панорама"
                            },
                            {
                                "target_lang_word": "пейзаж",
                                "target_lang_sense_clarification": "ландшафт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba3ff335-d84f-3479-b11f-58bf1754ac54",
                    "sense_definition": "Obraz lub wygląd kogoś lub czegoś; fakt zobaczenia kogoś lub czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "visual_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na <w>widok</w> policjanta złodziej zaczął szybko uciekać.",
                            "target_lang_translations": {
                                "ru": "При <w>виде</w> полицейского вор начал быстро убегать."
                            }
                        },
                        {
                            "text": "Tęsknię za <w>widokiem</w> mojego rodzinnego domu.",
                            "target_lang_translations": {
                                "ru": "Я скучаю по <w>виду</w> моего родного дома."
                            }
                        }
                    ],
                    "synonyms": [

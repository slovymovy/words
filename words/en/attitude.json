                    ],
                    "common_phrases": [
                        "give attitude",
                        "have an attitude",
                        "bad attitude"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to mean arrogant or uncooperative behavior"
                        }

            "senses": [
                {
                    "sense_id": "17888853-93b5-326c-99cb-2fcfc4dda399",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>mommen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>mom</w> mijn gezicht zodat niemand mij op straat herkent.",
                            "target_lang_translations": {
                                "ru": "Я <w>маскирую</w> свое лицо, чтобы никто не узнал меня на улице."
                            }
                        },
                        {
                            "text": "Als ik me voor het toneelstuk kleed, <w>mom</w> ik me als een oude man.",
                            "target_lang_translations": {
                                "ru": "Когда я одеваюсь для спектакля, я <w>ряжусь</w> под старика."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени от глагола 'mommen' (маскировать, рядить)."
                    },
                    "translations": {
                        "ru": [

        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e6bd6d7-1832-3398-aba4-e8ab46de03e0",
                    "sense_definition": "Het besef of de aandacht voor iets; bijna uitsluitend gebruikt in vaste uitdrukkingen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "awareness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij had er geen <w>erg</w> in dat hij zijn sleutels was vergeten.",
                            "target_lang_translations": {
                                "en": "He had no <w>notion</w> that he had forgotten his keys.",
                                "ru": "Он не <w>заметил</w>, что забыл свои ключи."
                            }
                        },
                        {
                            "text": "Zonder dat ze er <w>erg</w> in had, liep ze de verkeerde kant op.",
                            "target_lang_translations": {
                                "en": "Without <w>realizing</w> it, she walked in the wrong direction.",
                                "ru": "Сама того не <w>замечая</w>, она пошла не в ту сторону."

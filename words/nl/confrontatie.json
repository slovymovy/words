            "senses": [
                {
                    "sense_id": "aaba9c90-2a18-36bc-8e6c-0f30bc2c4d18",
                    "sense_definition": "Een vijandige ontmoeting, ruzie of conflict tussen personen of groepen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie probeerde een <w>confrontatie</w> tussen de twee groepen demonstranten te voorkomen.",
                            "target_lang_translations": {
                                "ru": "Полиция пыталась предотвратить <w>столкновение</w> между двумя группами демонстрантов."
                            }
                        },
                        {
                            "text": "Hij durfde de <w>confrontatie</w> met zijn boze baas niet aan te gaan.",
                            "target_lang_translations": {
                                "ru": "Он не осмелился пойти на <w>конфронтацию</w> со своим злым начальником."
                            }
                        }
                    ],
                    "synonyms": [
                        "botsing",
                        "conflict",
                        "aanvaring",
                        "strijd"
                    ],
                    "antonyms": [
                        "verzoening",
                        "overeenstemming",
                        "harmonie"
                    ],
                    "common_phrases": [
                        "de confrontatie aangaan",
                        "een confrontatie vermijden",
                        "een harde confrontatie",
                        "een directe confrontatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Враждебная встреча, ссора или конфликт между людьми или группами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столкновение",
                                "target_lang_sense_clarification": "физический или открытый конфликт"
                            },
                            {
                                "target_lang_word": "конфронтация",
                                "target_lang_sense_clarification": "противоборство, часто политическое или формальное"
                            },
                            {
                                "target_lang_word": "противостояние",
                                "target_lang_sense_clarification": "сопротивление, борьба интересов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "076ee10f-eb0c-393f-9039-c6985bf254be",
                    "sense_definition": "Het moment waarop men direct geconfronteerd wordt met (vaak onaangename) feiten, problemen of situaties.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>confrontatie</w> met de harde werkelijkheid kwam voor hem als een schok.",
                            "target_lang_translations": {
                                "ru": "<w>Столкновение</w> с суровой реальностью стало для него шоком."
                            }
                        },
                        {
                            "text": "Na jaren van uitstel volgde eindelijk de <w>confrontatie</w> met zijn verleden.",
                            "target_lang_translations": {
                                "ru": "После многих лет откладывания наконец произошло <w>столкновение</w> с его прошлым."
                            }
                        }
                    ],
                    "synonyms": [
                        "blootstelling"
                    ],
                    "antonyms": [
                        "vermijding",
                        "ontwijking"
                    ],
                    "common_phrases": [
                        "confrontatie met de werkelijkheid",
                        "confrontatie met zichzelf",
                        "confrontatie met het verleden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Момент непосредственного столкновения с (часто неприятными) фактами, проблемами или ситуациями."
                    },
                    "translations": {
                        "ru": [
                            {

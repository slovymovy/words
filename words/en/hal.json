    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "01fa1132-95a9-37c1-a02b-36a11522cbf7",
                    "sense_definition": "A diminutive of the male given names <w>Henry</w>, <w>Harold</w> and <w>Harry</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My uncle <w>Hal</w> served in the navy for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Хэл</w> служил на флоте двадцать лет."
                            }
                        },
                        {
                            "text": "Everyone was surprised when <w>Hal</w> arrived at the party early.",
                            "target_lang_translations": {
                                "ru": "Все удивились, когда <w>Хэл</w> пришел на вечеринку пораньше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Prince Hal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма мужских имен Генри, Гарольд и Гарри."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хэл",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65b0758e-8556-3a9f-8e78-d9370bd409e5",
                    "sense_definition": "Initialism of Hindustan Aeronautics Limited.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "The engineer submitted her report to the <w>HAL</w> management team.",
                            "target_lang_translations": {
                                "ru": "Инженер представила свой отчет руководству <w>ХАЛ</w>."
                            }
                        },
                        {
                            "text": "Several new aircraft prototypes are being developed by <w>HAL</w>.",
                            "target_lang_translations": {
                                "ru": "Несколько новых прототипов самолетов разрабатываются компанией <w>ХАЛ</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура компании Hindustan Aeronautics Limited."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ХАЛ",
                                "target_lang_sense_clarification": "авиастроительная компания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e8fadca6-85cf-3c3b-808a-bf64ddc58ce2",
                    "sense_definition": "Acronym of hardware abstraction layer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>HAL</w> allows the operating system to interact with different hardware architectures.",
                            "target_lang_translations": {
                                "ru": "<w>HAL</w> позволяет операционной системе взаимодействовать с различными аппаратными архитектурами."
                            }
                        },
                        {
                            "text": "Programmers rely on the <w>HAL</w> to write code that is portable across devices.",
                            "target_lang_translations": {
                                "ru": "Программисты полагаются на <w>HAL</w> для написания кода, переносимого между устройствами."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от \"hardware abstraction layer\" (уровень аппаратных абстракций)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень аппаратных абстракций",
                                "target_lang_sense_clarification": "компьютерный термин"
                            },
                            {
                                "target_lang_word": "HAL",
                                "target_lang_sense_clarification": "аббревиатура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4518abdf-824f-39c1-b133-4027aa32d30d",
                    "sense_definition": "A homicidal computer, an artificial intelligence that acts similarly to the HAL 9000 featured in 2001: A Space Odyssey.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am afraid the new smart home system is behaving like a <w>HAL</w>.",
                            "target_lang_translations": {
                                "ru": "Боюсь, новая система умного дома ведет себя как <w>ХЭЛ</w>."
                            }
                        },
                        {
                            "text": "The sci-fi movie featured a rogue AI that became a <w>HAL</w> and locked the crew out.",
                            "target_lang_translations": {
                                "ru": "В научно-фантастическом фильме был показан вышедший из-под контроля ИИ, который превратился в <w>ХЭЛа</w> и запер экипаж снаружи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "HAL 9000"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютер-убийца; искусственный интеллект, действующий подобно HAL 9000 из фильма \"2001 год: Космическая одиссея\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ХЭЛ",
                                "target_lang_sense_clarification": "персонаж фильма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6acc81fa-da6e-3011-bbae-84ef1dc11824",
                    "sense_definition": "A special temporary state of consciousness, generally understood to be the product of a Sufi's spiritual practices while on his way toward God.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mystic entered a deep <w>hal</w> during his evening meditation.",
                            "target_lang_translations": {
                                "ru": "Мистик вошел в глубокий <w>хал</w> во время вечерней медитации."
                            }
                        },
                        {
                            "text": "Scholars of Sufism discuss the difference between a permanent station and a fleeting <w>hal</w>.",
                            "target_lang_translations": {
                                "ru": "Исследователи суфизма обсуждают различие между постоянной стоянкой и мимолетным <w>халом</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Особое временное состояние сознания в суфизме, возникающее в ходе духовных практик."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хал",
                                "target_lang_sense_clarification": "суфизм"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
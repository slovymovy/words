                    "sense_definition": "jedyny naturalny satelita Ziemi, drugi po Słońcu najjaśniejszy obiekt na ziemskim niebie",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "astronomy_proper_name",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Amerykańscy astronauci jako pierwsi ludzie wylądowali na <Księżycu>.",
                            "target_lang_translations": {
                                "ru": "Американские астронавты первыми высадились на <Луне>."
                            }
                        },
                        {
                            "text": "Ziemia i obiegający ją <Księżyc> wzajemnie się przyciągają.",
                            "target_lang_translations": {
                                "ru": "Земля и вращающаяся вокруг неё <Луна> притягивают друг друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "Srebrny Glob",
                        "Luna"
                    ],
                    "antonyms": [
                        "Sol",
                        "Sol"
                    ],
                    "common_phrases": [
                        "lot na Księżyc",
                        "zaćmienie Księżyca",
                        "fazy Księżyca",
                        "ciemna strona Księżyca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единственный естественный спутник Земли, второй по яркости объект на земном небе после Солнца"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Луна",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        }

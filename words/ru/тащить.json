    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e929c02d-1303-321a-baea-d657df7623dc",
                    "sense_definition": "Перемещать кого-либо или что-либо с усилием, волоча по земле или неся на себе",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие медленно <тащили> тяжелый диван на пятый этаж.",
                            "target_lang_translations": {
                                "en": "The workers were slowly <hauling> the heavy sofa up to the fifth floor."
                            }
                        },
                        {
                            "text": "Лошадь с большим трудом <тащила> телегу, нагруженную дровами.",
                            "target_lang_translations": {
                                "en": "The horse was <pulling> the cart loaded with firewood with great difficulty."
                            }
                        },
                        {
                            "text": "Вон там, на лужку, за деревней, трое <тащат> что-то.",
                            "target_lang_translations": {
                                "en": "Over there, on the meadow behind the village, three people are <dragging> something."
                            }
                        }
                    ],
                    "synonyms": [
                        "тянуть",
                        "волочить"
                    ],
                    "antonyms": [
                        "толкать"
                    ],
                    "common_phrases": [
                        "еле тащить ноги",
                        "тащить на себе",
                        "тащить чемодан",
                        "тащить в гору"
                    ],
                    "target_lang_definitions": {
                        "en": "To move someone or something with effort, either by dragging it along the ground or carrying it"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drag",
                                "target_lang_sense_clarification": "to pull along a surface with effort"
                            },
                            {
                                "target_lang_word": "haul",
                                "target_lang_sense_clarification": "to transport or carry something heavy"
                            },
                            {
                                "target_lang_word": "lug",
                                "target_lang_sense_clarification": "to carry or drag something heavy with exertion"
                            },
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "to hold and transport"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bda149db-5d40-30e0-8108-9643699a9593",
                    "sense_definition": "Тайком уносить и присваивать чужое; совершать кражу",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "theft",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сторож заметил, что кто-то <тащит> с завода детали, и вызвал полицию.",
                            "target_lang_translations": {
                                "en": "The guard noticed that someone was <stealing> parts from the factory and called the police."
                            }
                        },
                        {
                            "text": "Наш кот постоянно <тащит> со стола колбасу, если мы отворачиваемся.",
                            "target_lang_translations": {
                                "en": "Our cat constantly <swipes> sausage from the table if we turn away."
                            }
                        }
                    ],
                    "synonyms": [
                        "воровать",
                        "красть"
                    ],
                    "common_phrases": [
                        "тащить всё подряд",
                        "тащить с работы",
                        "тащить деньги"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To take something that belongs to someone else without permission; to steal (colloquial)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steal",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "pinch",
                                "target_lang_sense_clarification": "informal/slang"
                            },
                            {
                                "target_lang_word": "swipe",
                                "target_lang_sense_clarification": "informal, to steal quickly"
                            },
                            {
                                "target_lang_word": "filch",
                                "target_lang_sense_clarification": "to steal something of small value"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20e0e4b2-466e-36a7-8016-09ed1e80a733",
                    "sense_definition": "Вызывать сильный восторг, очень нравиться, доставлять удовольствие",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот новый музыкальный альбом меня просто <тащит>.",
                            "target_lang_translations": {
                                "en": "I just really <dig> this new music album."
                            }
                        },
                        {
                            "text": "Меня не <тащит> такая скучная работа, я хочу заниматься творчеством.",
                            "target_lang_translations": {
                                "en": "I don't <get a kick out of> such boring work; I want to do something creative."
                            }
                        }
                    ],
                    "common_phrases": [
                        "меня тащит",
                        "музыка тащит"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cause strong delight or pleasure; to be very appealing (slang)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dig",
                                "target_lang_sense_clarification": "slang, to like or understand"
                            },
                            {
                                "target_lang_word": "get a kick out of",
                                "target_lang_sense_clarification": "to find enjoyable or amusing"
                            },
                            {
                                "target_lang_word": "be crazy about",
                                "target_lang_sense_clarification": "to like very much"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d770b14-9d70-38ef-af9b-94bb55481f39",
                    "sense_definition": "Играть лучше других в команде, обеспечивая победу за счет личного мастерства (в компьютерных играх)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "gaming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вчера наш снайпер в одиночку <тащил> всю команду в финальном раунде.",
                            "target_lang_translations": {
                                "en": "Yesterday our sniper single-handedly <carried> the whole team in the final round."
                            }
                        },
                        {
                            "text": "Трудно <тащить> игру, когда союзники постоянно совершают ошибки.",
                            "target_lang_translations": {
                                "en": "It's hard to <carry> the game when allies constantly make mistakes."
                            }
                        }
                    ],
                    "common_phrases": [
                        "тащить катку",
                        "тащить команду",
                        "соло тащить"

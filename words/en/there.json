    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "7b2cf9dd-ef04-3947-bb6f-c4a471035aed",
                    "sense_definition": "Used as the grammatical subject of the verb 'be' to indicate that something exists or happens.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "existential_subject",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>There</w> are two apples on the table.",
                            "target_lang_translations": {
                                "ru": "На столе <w>лежат</w> два яблока."
                            }
                        },
                        {
                            "text": "Is <w>there</w> a bank near here?",
                            "target_lang_translations": {
                                "ru": "<w>Есть</w> ли поблизости банк?"
                            }
                        },
                        {
                            "text": "No, <w>there</w> isn't any milk left.",
                            "target_lang_translations": {
                                "ru": "Нет, молока не <w>осталось</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "there is",
                        "there are",
                        "is there",
                        "are there",
                        "there was",
                        "there were"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как грамматическое подлежащее с глаголом \"быть\" для указания на существование или наличие чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "указывает на наличие"
                            },
                            {
                                "target_lang_word": "имеется",
                                "target_lang_sense_clarification": "формальное указание на наличие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c513259d-4de1-35b6-ac51-f49765f72a83",
                    "sense_definition": "Used after a greeting to address someone in a friendly or informal way.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hi <w>there</w>, how are you doing today?",
                            "target_lang_translations": {
                                "ru": "<w>Привет</w>, как твои дела сегодня?"
                            }
                        },
                        {
                            "text": "Hello <w>there</w>! It is good to see you.",
                            "target_lang_translations": {
                                "ru": "<w>Привет</w>! Рад тебя видеть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hi there",
                        "hello there"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется после приветствия при дружеском или неформальном обращении."
                    },
                    "translations": {
                        "ru": []
                    }
                },
                {
                    "sense_id": "022ba241-26e4-3924-a129-10d79d53f0ed",
                    "sense_definition": "Used as a dummy subject with verbs like 'seem', 'appear', or 'happen' followed by 'to be'.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "existential_subject",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>There</w> seems to be a mistake in the report.",
                            "target_lang_translations": {
                                "ru": "<w>Кажется</w>, в отчете ошибка."
                            }
                        },
                        {
                            "text": "I expected <w>there</w> to be more people at the party.",
                            "target_lang_translations": {
                                "ru": "Я ожидал, что на вечеринке <w>будет</w> больше людей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "there seems to be",
                        "there appears to be"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как формальное подлежащее с глаголами \"казаться\", \"оказаться\" и т.д."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кажется",
                                "target_lang_sense_clarification": "вводное слово, выражающее предположение"
                            },
                            {
                                "target_lang_word": "похоже",
                                "target_lang_sense_clarification": "указывает на вероятность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "127f01c0-2ba1-34a4-9409-e56f498f4cb0",
                    "sense_definition": "Used as a subject with intransitive verbs other than 'be' to indicate existence or occurrence.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "existential_subject",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>There</w> remain several problems with this approach.",
                            "target_lang_translations": {
                                "ru": "С этим подходом <w>остается</w> несколько проблем."
                            }
                        },
                        {
                            "text": "Once upon a time, <w>there</w> lived a woodsman in the forest.",
                            "target_lang_translations": {
                                "ru": "Давным-давно в лесу <w>жил</w> дровосек."
                            }
                        }
                    ],
                    "common_phrases": [
                        "there remains",
                        "there exists"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как подлежащее с непереходными глаголами (кроме \"быть\") для указания на существование или происшествие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "существует",
                                "target_lang_sense_clarification": "быть в наличии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4670da2e-cbdd-35c2-813a-2883be955f85",
                    "sense_definition": "In or at a specific place or location that is away from the speaker.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_static",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I used to live <w>there</w> when I was a child.",
                            "target_lang_translations": {
                                "ru": "Я жил <w>там</w> в детстве."
                            }
                        },
                        {
                            "text": "Your keys are right <w>there</w> on the table.",
                            "target_lang_translations": {
                                "ru": "Твои ключи прямо <w>там</w>, на столе."
                            }
                        }
                    ],
                    "synonyms": [
                        "over there",
                        "yonder"
                    ],
                    "antonyms": [
                        "here"
                    ],
                    "common_phrases": [
                        "over there",
                        "right there",
                        "stay there"
                    ],
                    "target_lang_definitions": {
                        "ru": "В определенном месте или точке, находящейся вдали от говорящего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "там",
                                "target_lang_sense_clarification": "в том месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c42b49c-bec3-32a2-ab02-e77c2638e7e4",
                    "sense_definition": "To or into a specific place or location.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are going <w>there</w> for our vacation next week.",
                            "target_lang_translations": {
                                "ru": "Мы едем <w>туда</w> в отпуск на следующей неделе."
                            }
                        },
                        {
                            "text": "I have never been <w>there</w> before.",
                            "target_lang_translations": {
                                "ru": "Я никогда не был <w>там</w> раньше."
                            }
                        }
                    ],
                    "synonyms": [
                        "thither"
                    ],
                    "antonyms": [
                        "here"
                    ],
                    "common_phrases": [
                        "go there",
                        "get there"
                    ],
                    "target_lang_definitions": {
                        "ru": "В определенное место или направление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туда",
                                "target_lang_sense_clarification": "в то место (направление)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a69f739-47f2-3d43-aeca-e3a7fa0f1b91",
                    "sense_definition": "At that specific point, stage, or moment in a process, speech, or activity.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He did not stop <w>there</w> but continued his story.",
                            "target_lang_translations": {
                                "ru": "Он не остановился <w>на этом</w>, а продолжил свой рассказ."
                            }
                        },
                        {
                            "text": "Let's leave the discussion <w>there</w> for today.",
                            "target_lang_translations": {
                                "ru": "Давайте оставим обсуждение <w>на этом</w> сегодня."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stop there",
                        "leave it there"
                    ],
                    "target_lang_definitions": {
                        "ru": "В этот конкретный момент, на этой стадии или этапе процесса, речи или деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на этом",
                                "target_lang_sense_clarification": "в этом пункте или моменте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55a3c163-9652-3ee6-9393-4555238a7c03",
                    "sense_definition": "Available to provide support, help, or comfort to someone.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You know that I will always be <w>there</w> for you.",
                            "target_lang_translations": {
                                "ru": "Ты знаешь, что я всегда буду <w>рядом</w>."
                            }
                        },
                        {
                            "text": "She was <w>there</w> when I needed a friend.",
                            "target_lang_translations": {
                                "ru": "Она была <w>рядом</w>, когда мне был нужен друг."
                            }
                        }
                    ],
                    "common_phrases": [
                        "be there for someone",
                        "always there"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовый оказать поддержку, помощь или утешение кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рядом",
                                "target_lang_sense_clarification": "готовый помочь (быть рядом)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "749491e8-3542-3929-a035-f20434799420",
                    "sense_definition": "In that specific matter, respect, or relation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I certainly agree with you <w>there</w>.",
                            "target_lang_translations": {
                                "ru": "Я, безусловно, согласен с вами <w>в этом</w>."
                            }
                        },
                        {
                            "text": "You are absolutely right <w>there</w>.",
                            "target_lang_translations": {
                                "ru": "Вы абсолютно правы <w>в этом</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В этом конкретном вопросе, отношении или аспекте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в этом",
                                "target_lang_sense_clarification": "в данном вопросе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12546f7a-1e24-3edd-8382-5065d4169265",
                    "sense_definition": "Into a specific topic or subject, usually one that is sensitive, controversial, or undesirable.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "idiomatic_topic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please don't go <w>there</w>; I do not want to talk about it.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не начинай <w>об этом</w>; я не хочу говорить на эту тему."
                            }
                        },
                        {
                            "text": "He started to argue, but I told him not to go <w>there</w>.",
                            "target_lang_translations": {
                                "ru": "Он начал спорить, но я сказал ему не касаться <w>этого</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "don't go there"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Касаться определенной темы, обычно деликатной, спорной или нежелательной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "об этом",
                                "target_lang_sense_clarification": "касательно этой темы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c0711d83-ba1d-3a5d-93de-d43e8d3c0bac",
                    "sense_definition": "That specific place or position.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need someone to drive us from here to <w>there</w>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно, чтобы кто-то отвез нас отсюда <w>туда</w>."
                            }
                        },
                        {
                            "text": "I will handle the project from <w>there</w>.",
                            "target_lang_translations": {
                                "ru": "Я буду вести проект <w>оттуда</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "from here to there"
                    ],
                    "target_lang_definitions": {
                        "ru": "Это конкретное место или положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туда",
                                "target_lang_sense_clarification": "в то место"
                            },
                            {
                                "target_lang_word": "оттуда",
                                "target_lang_sense_clarification": "из того места"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d88c24d2-66c8-3d6a-8a73-bdd1c164378b",
                    "sense_definition": "Used to offer encouragement or sympathy to someone.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>There</w>, <w>there</w>, everything will be alright.",
                            "target_lang_translations": {
                                "ru": "<w>Ну-ну</w>, все будет хорошо."
                            }
                        },
                        {
                            "text": "<w>There</w>, <w>there</w>, stop crying now.",
                            "target_lang_translations": {
                                "ru": "<w>Тише</w>, <w>тише</w>, перестань плакать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "there there"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для утешения или успокоения кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ну-ну",
                                "target_lang_sense_clarification": "утешение"
                            },
                            {
                                "target_lang_word": "тише",
                                "target_lang_sense_clarification": "призыв успокоиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c4f5012-d7cb-328b-ac43-35e80eb7b17f",
                    "sense_definition": "Used to express satisfaction, victory, or the completion of a task.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>There</w>! I have finally fixed the lock.",
                            "target_lang_translations": {
                                "ru": "<w>Вот</w>! Я наконец починил замок."
                            }
                        },
                        {
                            "text": "<w>There</w>! That is exactly what I wanted.",
                            "target_lang_translations": {
                                "ru": "<w>Вот</w>! Это именно то, что я хотел."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения удовлетворения, победы или завершения дела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вот",
                                "target_lang_sense_clarification": "указание на результат"
                            },
                            {
                                "target_lang_word": "готово",
                                "target_lang_sense_clarification": "завершение действия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
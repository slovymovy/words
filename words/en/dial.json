{
    "word_family": [
        "dialer",
        "redial",
        "sundial"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "94e5f2ee-cdad-3214-8df1-6f6341230605",
                    "sense_definition": "To enter a telephone number to make a call, either by pressing buttons or turning a rotary disk.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "telephony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In case of an emergency, you should <w>dial</w> the police immediately.",
                            "target_lang_translations": {
                                "ru": "В случае чрезвычайной ситуации вам следует немедленно <w>набрать</w> полицию."
                            }
                        },
                        {
                            "text": "She picked up the receiver to <w>dial</w> her friend's number.",
                            "target_lang_translations": {
                                "ru": "Она сняла трубку, чтобы <w>набрать</w> номер подруги."
                            }
                        }
                    ],
                    "synonyms": [
                        "call",
                        "phone",
                        "ring"
                    ],
                    "common_phrases": [
                        "dial a number",
                        "dial direct"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вводить номер телефона для совершения звонка, нажимая кнопки или вращая диск."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набира́ть",
                                "target_lang_sense_clarification": "номер телефона"
                            },
                            {
                                "target_lang_word": "звони́ть",
                                "target_lang_sense_clarification": "по телефону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5ffd7b1-3d40-381d-9093-88c6484da6b0",
                    "sense_definition": "To adjust, control, or select a setting or level, often used figuratively to mean increasing or decreasing intensity.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The candidate had to <w>dial</w> down his aggressive language during the debate.",
                            "target_lang_translations": {
                                "ru": "Кандидату пришлось <w>сбавить</w> агрессивный тон во время дебатов."
                            }
                        },
                        {
                            "text": "He <w>dialed</w> the volume up to hear the news report more clearly.",
                            "target_lang_translations": {
                                "ru": "Он <w>прибавил</w> громкость, чтобы лучше слышать новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "adjust",
                        "regulate",
                        "tune"
                    ],
                    "common_phrases": [
                        "dial down",
                        "dial up",
                        "dial in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулировать, управлять или выбирать настройку; часто используется в переносном смысле для обозначения изменения интенсивности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регули́ровать",
                                "target_lang_sense_clarification": "настройки"
                            },
                            {
                                "target_lang_word": "изменя́ть",
                                "target_lang_sense_clarification": "уровень"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b527ebb3-4965-3a55-a912-0419b04cbf05",
                    "sense_definition": "A device with a circular face and a needle or numbers that shows a measurement, such as speed, pressure, or time.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>dial</w> on the dashboard indicated that the car was running out of fuel.",
                            "target_lang_translations": {
                                "ru": "<w>Шкала</w> на приборной панели показывала, что топливо заканчивается."
                            }
                        },
                        {
                            "text": "Check the pressure <w>dial</w> to ensure the machine is safe to operate.",
                            "target_lang_translations": {
                                "ru": "Проверьте <w>датчик</w> давления, чтобы убедиться в безопасности работы машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "gauge",
                        "meter",
                        "indicator"
                    ],
                    "common_phrases": [
                        "pressure dial",
                        "speed dial"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибор или часть прибора с круглой шкалой и стрелкой, показывающий измерения (скорость, давление и т.д.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цифербла́т",
                                "target_lang_sense_clarification": "прибора"
                            },
                            {
                                "target_lang_word": "шкала́",
                                "target_lang_sense_clarification": "измерительная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "608325eb-72ed-3993-8841-9f906f762b1f",
                    "sense_definition": "A rotatable knob or button on a machine used to change settings like volume, frequency, or temperature.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She turned the <w>dial</w> to find a radio station with better reception.",
                            "target_lang_translations": {
                                "ru": "Она повернула <w>ручку</w> настройки, чтобы найти радиостанцию с лучшим приемом."
                            }
                        },
                        {
                            "text": "Use the temperature <w>dial</w> to set the oven to the correct heat.",
                            "target_lang_translations": {
                                "ru": "Используйте <w>регулятор</w> температуры, чтобы установить духовку на нужный режим."
                            }
                        }
                    ],
                    "synonyms": [
                        "knob",
                        "control",
                        "regulator"
                    ],
                    "common_phrases": [
                        "turn the dial",
                        "volume dial"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вращающаяся ручка или кнопка на устройстве для изменения настроек (громкости, температуры)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регуля́тор",
                                "target_lang_sense_clarification": "ручка управления"
                            },
                            {
                                "target_lang_word": "ру́чка",
                                "target_lang_sense_clarification": "настройки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47f1d030-fe2c-3943-8b2a-613fa8daf8e5",
                    "sense_definition": "A rotating disk with finger holes on older telephones used to input a phone number.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "telephony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The antique telephone had a heavy rotary <w>dial</w> that made a clicking sound.",
                            "target_lang_translations": {
                                "ru": "У старинного телефона был тяжелый вращающийся <w>диск</w>, издававший щелкающий звук."
                            }
                        },
                        {
                            "text": "It takes much longer to use a <w>dial</w> than to press buttons on a keypad.",
                            "target_lang_translations": {
                                "ru": "Пользоваться <w>диском</w> гораздо дольше, чем нажимать кнопки на клавиатуре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rotary dial"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to the mechanical component of older phones"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вращающийся диск с отверстиями для пальцев на старых телефонах для набора номера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диск",
                                "target_lang_sense_clarification": "телефонный"
                            },
                            {
                                "target_lang_word": "набо́рный диск",
                                "target_lang_sense_clarification": "телефона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d9365e1-e980-3199-b589-67c3176baba0",
                    "sense_definition": "A person's face.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had a huge smile on his <w>dial</w> when he heard the good news.",
                            "target_lang_translations": {
                                "ru": "Когда он услышал хорошие новости, на его <w>физиономии</w> засияла широкая улыбка."
                            }
                        },
                        {
                            "text": "Wipe that grumpy look off your <w>dial</w> and come join the party.",
                            "target_lang_translations": {
                                "ru": "Сотри это недовольное выражение со своей <w>физиономии</w> и присоединяйся к вечеринке."
                            }
                        }
                    ],
                    "synonyms": [
                        "face",
                        "mug"
                    ],
                    "common_phrases": [
                        "smile on one's dial"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often used in British or Australian English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "физионо́мия",
                                "target_lang_sense_clarification": "разг."
                            },
                            {
                                "target_lang_word": "табло́",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c9c8abd3-fcc2-311b-ae1d-746bc510d37e",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Dial</w> has been working at the company for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Дайал</w> работает в компании уже двадцать лет."
                            }
                        },
                        {
                            "text": "We invited the <w>Dial</w> family to the neighborhood gathering.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили семью <w>Дайал</w> на соседскую вечеринку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Да́йал",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0ec0590f-23d1-3a4c-8cd1-d7fa04b17cdd",
                    "sense_definition": "Вызывать интерес или симпатию, манить, заставлять обращать на себя внимание.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attraction_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот старинный город продолжает <притягивать> туристов своей уникальной архитектурой.",
                            "target_lang_translations": {
                                "en": "This ancient city continues to <attract> tourists with its unique architecture."
                            }
                        },
                        {
                            "text": "Её загадочная улыбка умела <притягивать> взгляды всех присутствующих в зале.",
                            "target_lang_translations": {
                                "en": "Her mysterious smile knew how to <draw> the gazes of everyone present in the hall."
                            }
                        }
                    ],
                    "synonyms": [
                        "привлекать",
                        "манить",
                        "увлекать"
                    ],
                    "antonyms": [
                        "отталкивать"
                    ],
                    "common_phrases": [
                        "притягивать взгляд",
                        "притягивать внимание",
                        "притягивать удачу",
                        "притягивать людей"
                    ],
                    "target_lang_definitions": {
                        "en": "To arouse interest or sympathy, to beckon, or to compel attention."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attract",
                                "target_lang_sense_clarification": "to cause someone to have a liking or interest"
                            },
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "to attract attention or crowds"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59fbc0df-6ac3-30ec-b72a-caa06a0809c1",
                    "sense_definition": "Тянуть, приближая к себе или к чему-либо физическим усилием.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "attraction_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рыбак начал медленно <притягивать> тяжёлую сеть к борту лодки.",
                            "target_lang_translations": {
                                "en": "The fisherman began to slowly <pull> the heavy net toward the side of the boat."
                            }
                        },
                        {
                            "text": "Чтобы открыть дверь, нужно <притягивать> ручку на себя, а не толкать.",
                            "target_lang_translations": {
                                "en": "To open the door, you need to <pull> the handle toward you, not push."
                            }
                        }
                    ],
                    "synonyms": [
                        "подтягивать",
                        "тянуть"
                    ],
                    "antonyms": [
                        "отталкивать",
                        "оттягивать"
                    ],
                    "common_phrases": [
                        "притягивать к себе",
                        "притягивать канат",
                        "притягивать колени"
                    ],
                    "target_lang_definitions": {
                        "en": "To pull something closer to oneself using physical effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pull",
                                "target_lang_sense_clarification": "to use force to move something towards oneself"
                            },
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "to pull or drag something closer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b82a8304-067e-3e06-b696-b52dab40fdb0",
                    "sense_definition": "Приближать к себе воздействием физического поля (магнита или гравитации).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "attraction_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мощный магнит способен <притягивать> даже крупные металлические предметы.",
                            "target_lang_translations": {
                                "en": "A powerful magnet is capable of <attracting> even large metal objects."
                            }
                        },
                        {
                            "text": "Земля <притягивает> Луну, удерживая её на постоянной орбите.",
                            "target_lang_translations": {
                                "en": "The Earth <attracts> the Moon, keeping it in a constant orbit."
                            }
                        }
                    ],
                    "antonyms": [
                        "отталкивать"
                    ],
                    "common_phrases": [
                        "магнит притягивает",
                        "земля притягивает",
                        "притягивать железо",
                        "притягивать металл"
                    ],
                    "target_lang_definitions": {
                        "en": "To pull towards oneself through a physical force like magnetism or gravity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attract",
                                "target_lang_sense_clarification": "to pull towards without physical contact"
                            },
                            {
                                "target_lang_word": "pull",
                                "target_lang_sense_clarification": "to exert a force of attraction"
                            }
                        ]
                    }
                }
            ]
        }
    ]

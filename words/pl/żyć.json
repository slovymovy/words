                    "synonyms": [
                        "przebywać",
                        "bytować",
                        "zachowywać się"
                    ],
                    "common_phrases": [
                        "żyć na wsi",
                        "żyć w mieście",
                        "żyć za granicą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проживать или находиться постоянно в определенном месте."

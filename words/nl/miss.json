                },
                {
                    "sense_id": "364d1ba7-77f5-3dd6-a9fa-71f427523fe1",
                    "sense_definition": "Aanspreektitel voor een juffrouw of lerares, vaak gebruikt in een internationale of tweetalige context.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vraag maar aan <w>Miss</w> Jansen of het mag.",
                            "target_lang_translations": {
                                "en": "Just ask <w>Miss</w> Jansen if it's allowed.",
                                "ru": "Спроси у <w>мисс</w> Янсен, можно ли это."
                            }
                        },
                        {
                            "text": "Goedemorgen <w>Miss</w>, heeft u even tijd?",
                            "target_lang_translations": {
                                "en": "Good morning <w>Miss</w>, do you have a moment?",
                                "ru": "Доброе утро, <w>мисс</w>, у вас есть минутка?"
                            }
                        }
                    ],
                    "synonyms": [
                        "juffrouw"
                    ],
                    "target_lang_definitions": {
                        "en": "Title of address for a young woman or teacher.",
                        "ru": "Обращение к незамужней женщине или учительнице (часто в международном контексте)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Miss",
                                "target_lang_sense_clarification": "title of address"

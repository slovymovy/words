    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "f5fddf46-23ba-3865-8898-7194b9f5d922",
                    "sense_definition": "Used to express affirmation, agreement, or consent; a casual equivalent of yes.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "—Do you want to come with us? —<w>Yeah</w>, I would love to.",
                            "target_lang_translations": {
                                "ru": "— Ты хочешь пойти с нами? — <w>Да</w>, с удовольствием."
                            }
                        },
                        {
                            "text": "<w>Yeah</w>, that sounds like a great plan for the weekend.",
                            "target_lang_translations": {
                                "ru": "<w>Ага</w>, звучит как отличный план на выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "yes",
                        "yep",
                        "yup",
                        "aye"
                    ],
                    "antonyms": [
                        "no",
                        "nah",
                        "nope"
                    ],
                    "common_phrases": [
                        "oh yeah",
                        "yeah right",
                        "hell yeah"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения утверждения, согласия или разрешения; разговорный эквивалент слова \"да\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "общеупотребительное выражение согласия"
                            },
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "разговорное, неформальное согласие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3dd0188f-3b85-3f12-a408-a1299ee65816",
                    "sense_definition": "Used to express joy, celebration, or excitement.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Yeah</w>! We finally won the championship!",
                            "target_lang_translations": {
                                "ru": "<w>Да</w>! Мы наконец-то выиграли чемпионат!"
                            }
                        },
                        {
                            "text": "I got the job! <w>Yeah</w>!",
                            "target_lang_translations": {
                                "ru": "Я получил работу! <w>Ура</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "yay",
                        "woohoo",
                        "hurray",
                        "yippee"
                    ],
                    "common_phrases": [
                        "oh yeah"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения радости, торжества или восторга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "эмоциональный возглас при успехе"
                            },
                            {
                                "target_lang_word": "ура",
                                "target_lang_sense_clarification": "восклицание радости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adb2ef6d-dc1a-3df2-bbe4-d90038c5ae9f",
                    "sense_definition": "Used to show that one is listening to or acknowledging what has been said without necessarily agreeing.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "—I think we should leave early. —<w>Yeah</w>, I see your point.",
                            "target_lang_translations": {
                                "ru": "— Думаю, нам стоит уйти пораньше. — <w>Угу</w>, я понимаю, о чём ты."
                            }
                        },
                        {
                            "text": "It is a difficult situation, <w>Yeah</w>, but we will manage.",
                            "target_lang_translations": {
                                "ru": "Ситуация сложная, <w>да</w>, но мы справимся."
                            }
                        }
                    ],
                    "synonyms": [
                        "uh-huh",
                        "mhm"
                    ],
                    "common_phrases": [
                        "yeah yeah"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы показать, что собеседник слушает или принимает сказанное к сведению, не обязательно соглашаясь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угу",
                                "target_lang_sense_clarification": "звук, подтверждающий внимание"
                            },
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "подтверждение того, что информация принята"
                            },
                            {
                                "target_lang_word": "ну да",
                                "target_lang_sense_clarification": "частичное или формальное согласие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c866150e-9fe7-3b4b-a391-d268463b3c9a",
                    "sense_definition": "Used within a sentence to maintain the listener's attention or at the end to check for understanding.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "discourse_marker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was walking down the street, <w>yeah</w>, and suddenly it started to rain.",
                            "target_lang_translations": {
                                "ru": "Я шел по улице, <w>да</w>, и вдруг пошел дождь."
                            }
                        },
                        {
                            "text": "You know how to fix this, <w>yeah</w>?",
                            "target_lang_translations": {
                                "ru": "Ты знаешь, как это исправить, <w>да</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "right"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется внутри предложения для удержания внимания слушателя или в конце для проверки понимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "вопросительная частица в конце фразы"
                            },
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "вводное слово для связки речи"
                            }
                        ]
                    }
                }
            ]
        },

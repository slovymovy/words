    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7488abac-c03b-36a9-9b78-0947734957db",
                    "sense_definition": "Состояние атмосферы в определённом месте в определённое время (температура, влажность, осадки, ветер).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "weather_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Завтра ожидается тёплая и солнечная <w>погода</w>.",
                            "target_lang_translations": {
                                "en": "Warm and sunny <w>weather</w> is expected tomorrow."
                            }
                        },
                        {
                            "text": "Мы всегда слушаем прогноз <w>погоды</w> перед поездкой.",
                            "target_lang_translations": {
                                "en": "We always listen to the <w>weather</w> forecast before a trip."
                            }
                        }
                    ],
                    "synonyms": [
                        "погодка"
                    ],
                    "common_phrases": [
                        "прогноз погоды",
                        "хорошая погода",
                        "плохая погода",
                        "солнечная погода"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of the atmosphere at a specific place and time (temperature, humidity, precipitation, wind)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weather",
                                "target_lang_sense_clarification": "general atmospheric conditions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f378c3d-9a96-3f89-a020-634213e445c0",
                    "sense_definition": "Обстановка, климат или настроение в какой-либо группе людей или области деятельности (переносное значение).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_atmosphere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Именно эти люди делают <w>погоду</w> в современной науке.",
                            "target_lang_translations": {
                                "en": "It is these people who set the <w>tone</w> in modern science."
                            }
                        },
                        {
                            "text": "Политическая <w>погода</w> в стране начала меняться к лучшему.",
                            "target_lang_translations": {
                                "en": "The political <w>climate</w> in the country began to change for the better."
                            }
                        }
                    ],
                    "synonyms": [
                        "атмосфера"
                    ],
                    "common_phrases": [
                        "делать погоду"
                    ],
                    "target_lang_definitions": {
                        "en": "The situation, climate, or mood within a group of people or field of activity (figurative meaning)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "climate",
                                "target_lang_sense_clarification": "figurative political or social environment"
                            },
                            {
                                "target_lang_word": "atmosphere",
                                "target_lang_sense_clarification": "mood or tone of a situation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a7ba576-0fdc-32ab-ba0b-280eafb872ae",
                    "sense_definition": "Ненастье, плохие метеорологические условия (дождь, снег, сильный ветер).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weather_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В такую <w>погоду</w> хороший хозяин собаку на улицу не выгонит.",
                            "target_lang_translations": {
                                "en": "A good owner wouldn't turn a dog out in such <w>weather</w>."
                            }
                        },
                        {
                            "text": "На море разыгралась настоящая <w>погода</w>, и корабли остались в порту.",
                            "target_lang_translations": {
                                "en": "A real <w>storm</w> broke out at sea, and the ships remained in port."
                            }
                        }
                    ],
                    "synonyms": [
                        "ненастье",
                        "непогода"
                    ],
                    "antonyms": [
                        "вёдро"
                    ],
                    "common_phrases": [
                        "ждать у моря погоды"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "народно-разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Bad weather, poor meteorological conditions (rain, snow, strong wind)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bad weather",
                                "target_lang_sense_clarification": "unpleasant conditions"
                            },
                            {
                                "target_lang_word": "storm",
                                "target_lang_sense_clarification": "severe weather conditions"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "70d1ca25-ff31-3ed7-8665-066b212db64c",
                    "sense_definition": "Obszar, na którym coś działa, jest dostępne (np. sygnał telefoniczny) lub wywiera wpływ.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety, w piwnicy nie mam <w>zasięgu</w> i nie mogę do ciebie zadzwonić.",
                            "target_lang_translations": {
                                "ru": "К сожалению, в подвале у меня нет <w>связи</w>, и я не могу тебе позвонить."
                            }
                        },
                        {
                            "text": "Pozostań w <w>zasięgu</w> mojego głosu, abym mógł cię zawołać.",
                            "target_lang_translations": {
                                "ru": "Оставайся в <w>пределах</w> слышимости, чтобы я мог тебя позвать."
                            }
                        },
                        {
                            "text": "Ceny tych mieszkań są poza <w>zasięgiem</w> przeciętnego obywatela.",
                            "target_lang_translations": {
                                "ru": "Цены на эти квартиры находятся за <w>пределами</w> возможностей среднего гражданина."
                            }
                        }
                    ],
                    "synonyms": [
                        "obszar występowania",
                        "obszar występowania"
                    ],
                    "common_phrases": [
                        "w zasięgu ręki",
                        "poza zasięgiem",
                        "brak zasięgu",
                        "w zasięgu wzroku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зона действия, область, в которой что-либо доступно (например, телефонный сигнал) или оказывает влияние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зона действия",
                                "target_lang_sense_clarification": "область, где что-то функционирует"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "наличие сигнала сети (разг.)"
                            },
                            {
                                "target_lang_word": "пределы",
                                "target_lang_sense_clarification": "границы досягаемости или возможностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f0f7043-5cfd-3188-857a-a83f831f543c",
                    "sense_definition": "Maksymalna odległość, jaką może przebyć pojazd bez tankowania lub pocisk po wystrzeleniu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mały <w>zasięg</w> samochodów elektrycznych zniechęca wielu ewentualnych nabywców.",
                            "target_lang_translations": {
                                "ru": "Малый <w>запас хода</w> электромобилей отпугивает многих потенциальных покупателей."
                            }
                        },
                        {
                            "text": "Ten nowy samolot pasażerski ma imponujący <w>zasięg</w> lotu.",
                            "target_lang_translations": {
                                "ru": "У этого нового пассажирского самолета внушительная <w>дальность</w> полета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zasięg lotu",
                        "maksymalny zasięg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Максимальное расстояние, которое может преодолеть транспортное средство без дозаправки или снаряд после выстрела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дальность",
                                "target_lang_sense_clarification": "расстояние полета или действия оружия"
                            },
                            {
                                "target_lang_word": "запас хода",
                                "target_lang_sense_clarification": "расстояние, которое может проехать транспорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1946fbb-79c8-3bea-b822-b44f8ef6b5a4",
                    "sense_definition": "Geograficzny obszar, na którym naturalnie występuje dany gatunek roślin lub zwierząt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zasięg</w> występowania niedźwiedzia polarnego kurczy się z powodu zmian klimatu.",
                            "target_lang_translations": {
                                "ru": "<w>Ареал</w> обитания белого медведя сокращается из-за изменения климата."
                            }
                        },
                        {
                            "text": "Ta rzadka roślina ma bardzo ograniczony <w>zasięg</w>.",
                            "target_lang_translations": {
                                "ru": "У этого редкого растения очень ограниченный <w>ареал</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "areał",
                        "obszar występowania"
                    ],
                    "common_phrases": [
                        "zasięg występowania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Географическая область, в которой естественно обитает данный вид растений или животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ареал",
                                "target_lang_sense_clarification": "область распространения вида"
                            }
                        ]
                    }
                }
            ]
        }
    ]

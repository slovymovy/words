{
    "word_family": [
        "отрывать",
        "отрыв",
        "отрывочный"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00aaec78-109a-316e-a18c-50d1a4037725",
                    "sense_definition": "Небольшая часть литературного, музыкального или иного художественного произведения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель попросил нас прочитать и пересказать <w>отрывок</w> из новой книги.",
                            "target_lang_translations": {
                                "en": "The teacher asked us to read and retell a <w>passage</w> from the new book."
                            }
                        },
                        {
                            "text": "Этот музыкальный <w>отрывок</w> звучит очень грустно и мелодично.",
                            "target_lang_translations": {
                                "en": "This musical <w>excerpt</w> sounds very sad and melodic."
                            }
                        }
                    ],
                    "synonyms": [
                        "фрагмент",
                        "пассаж"
                    ],
                    "common_phrases": [
                        "отрывок из книги",
                        "музыкальный отрывок",
                        "читать отрывок",
                        "небольшой отрывок"
                    ],
                    "target_lang_definitions": {
                        "en": "A short part of a literary, musical, or other artistic work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "passage",
                                "target_lang_sense_clarification": "a section of text or music"
                            },
                            {
                                "target_lang_word": "excerpt",
                                "target_lang_sense_clarification": "a short extract from a film, broadcast, or piece of writing"
                            },
                            {
                                "target_lang_word": "extract",
                                "target_lang_sense_clarification": "a portion selected from a larger work"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b715f41-7f54-3492-89f5-4051b57b7335",
                    "sense_definition": "То, что представляет собой часть чего-либо целого, неполный остаток или фрагмент (разговора, мыслей, воспоминаний).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я случайно услышал <w>отрывок</w> их разговора в коридоре.",
                            "target_lang_translations": {
                                "en": "I accidentally overheard a <w>snatch</w> of their conversation in the hallway."
                            }
                        },
                        {
                            "text": "В памяти всплывали лишь смутные <w>отрывки</w> вчерашнего вечера.",
                            "target_lang_translations": {
                                "en": "Only vague <w>fragments</w> of yesterday evening surfaced in my memory."
                            }
                        }
                    ],
                    "synonyms": [
                        "кусок",
                        "обрывок",
                        "фрагмент"
                    ],
                    "common_phrases": [
                        "отрывки фраз",
                        "отрывки воспоминаний",
                        "отрывки сведений"
                    ],
                    "target_lang_definitions": {
                        "en": "An incomplete part or isolated piece of a whole (e.g. of a conversation or memory)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fragment",
                                "target_lang_sense_clarification": "a small, incomplete part"
                            },
                            {
                                "target_lang_word": "snatch",
                                "target_lang_sense_clarification": "a brief, often interrupted part of a conversation or song"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
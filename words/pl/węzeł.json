                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sytuacja w firmie stała się tak skomplikowana, że przypominała <w>węzeł</w> gordyjski.",
                            "target_lang_translations": {
                                "ru": "Ситуация в фирме стала такой сложной, что напоминала гордиев <w>узел</w>."
                            }

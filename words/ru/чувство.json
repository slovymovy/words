    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d588d2e6-0b12-3446-9888-deb1747d7e2a",
                    "sense_definition": "Внутреннее психическое состояние человека, эмоция или душевное переживание (радость, страх, гордость и т.д.).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня есть странное <чувство>, что мы здесь уже были.",
                            "target_lang_translations": {
                                "en": "I have a strange <feeling> that we have been here before."
                            }
                        },
                        {
                            "text": "Глубокое <чувство> вины не давало ему спокойно спать.",
                            "target_lang_translations": {
                                "en": "A deep <sense> of guilt did not let him sleep peacefully."
                            }
                        },
                        {
                            "text": "Она испытывала <чувство> гордости за успехи своего сына.",
                            "target_lang_translations": {
                                "en": "She felt a <sense> of pride in her son's success."
                            }
                        }
                    ],
                    "synonyms": [
                        "эмоция"
                    ],
                    "common_phrases": [
                        "чувство юмора",
                        "чувство вины",
                        "чувство долга",
                        "чувство собственного достоинства",
                        "смешанные чувства"
                    ],
                    "target_lang_definitions": {
                        "en": "An internal mental state, emotion, or spiritual experience (joy, fear, pride, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "feeling",
                                "target_lang_sense_clarification": "general emotional state"
                            },
                            {
                                "target_lang_word": "sense",
                                "target_lang_sense_clarification": "specific awareness or distinct emotion (e.g., sense of guilt)"
                            },
                            {
                                "target_lang_word": "emotion",
                                "target_lang_sense_clarification": "strong feeling"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0999a13-e2a8-316e-8ac2-a60b3cbcf58d",
                    "sense_definition": "Физическое ощущение, которое испытывает организм (боль, голод, холод и т.п.).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгой прогулки у нас появилось сильное <чувство> голода.",
                            "target_lang_translations": {
                                "en": "After a long walk, we had a strong <feeling> of hunger."
                            }
                        },
                        {
                            "text": "Лекарство помогло быстро убрать неприятное <чувство> боли.",
                            "target_lang_translations": {
                                "en": "The medicine helped quickly remove the unpleasant <sensation> of pain."
                            }
                        }
                    ],
                    "synonyms": [
                        "ощущение"
                    ],
                    "common_phrases": [
                        "чувство голода",
                        "чувство холода",
                        "чувство боли",
                        "чувство сытости"
                    ],
                    "target_lang_definitions": {
                        "en": "A physical sensation experienced by the body (pain, hunger, cold, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sensation",
                                "target_lang_sense_clarification": "physical perception"
                            },
                            {
                                "target_lang_word": "feeling",
                                "target_lang_sense_clarification": "physical awareness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d650bce2-6194-3504-a26c-07c407c0e255",
                    "sense_definition": "Способность организма воспринимать окружающий мир (зрение, слух, обоняние, осязание, вкус).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Собаки имеют очень острое <чувство> обоняния.",
                            "target_lang_translations": {
                                "en": "Dogs have a very acute <sense> of smell."
                            }
                        },
                        {
                            "text": "У человека есть пять основных <чувств>, которые помогают ему жить.",
                            "target_lang_translations": {
                                "en": "Humans have five basic <senses> that help them live."
                            }
                        }
                    ],
                    "common_phrases": [
                        "органы чувств",
                        "шестое чувство",
                        "пять чувств"
                    ],
                    "target_lang_definitions": {
                        "en": "The ability of the organism to perceive the surrounding world (sight, hearing, smell, touch, taste)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sense",
                                "target_lang_sense_clarification": "faculty of perception"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40f8b18d-d024-35c9-8a45-06163cbe8b43",
                    "sense_definition": "Состояние сознания, способность контролировать себя и реагировать на окружающее (обычно используется в контексте потери сознания).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От сильной жары и духоты девушка упала без <чувств>.",
                            "target_lang_translations": {
                                "en": "From the intense heat and stuffiness, the girl fell <unconscious>."
                            }
                        },
                        {
                            "text": "Врачи долго пытались привести пациента в <чувство> после аварии.",
                            "target_lang_translations": {
                                "en": "Doctors tried for a long time to bring the patient to his <senses> after the accident."
                            }
                        }
                    ],
                    "synonyms": [
                        "сознание"
                    ],
                    "common_phrases": [
                        "привести в чувство",
                        "упасть без чувств",
                        "лишиться чувств",
                        "прийти в чувство"
                    ],
                    "target_lang_definitions": {
                        "en": "State of consciousness, ability to control oneself (usually used in the context of losing consciousness)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "senses",
                                "target_lang_sense_clarification": "state of awareness (plural)"
                            },
                            {
                                "target_lang_word": "consciousness",
                                "target_lang_sense_clarification": "state of being awake"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9e01958-2cd2-3c21-8bc8-0db45c80f7e4",
                    "sense_definition": "Любовь, сердечное влечение к другому человеку.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он долго скрывал своё нежное <чувство> к соседке.",
                            "target_lang_translations": {
                                "en": "He hid his tender <feeling> for his neighbor for a long time."
                            }
                        },
                        {
                            "text": "Между ними вспыхнуло настоящее, глубокое <чувство>.",
                            "target_lang_translations": {
                                "en": "A real, deep <feeling> sparked between them."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ответное чувство",
                        "первое чувство",
                        "большое чувство"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Love, romantic attraction to another person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "feeling",
                                "target_lang_sense_clarification": "romantic emotion"
                            },
                            {
                                "target_lang_word": "feelings",
                                "target_lang_sense_clarification": "romantic attraction (plural)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

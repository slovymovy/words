                    ]
                },
                {
                    "sense_id": "c07ca204-58d1-3c30-8035-84f5a972d802",
                    "sense_definition": "Wykrzyknienie wyrażające powątpiewanie, rezygnację, zadumę lub wskazujące na to, że coś jest łatwo powiedzieć, a trudniej zrobić.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ba</w>, żeby to było takie proste, jak ci się wydaje."
                        },
                        {
                            "text": "Chciałbym wyjechać na długie wakacje. <w>Ba</w>! Gdyby tylko było mnie na to stać."
                        }
                    ],
                    "synonyms": [
                        "ech"
                    ]
                }
            ]
        },
        {

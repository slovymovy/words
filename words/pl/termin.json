            "senses": [
                {
                    "sense_id": "687785e6-3ba7-37b8-9517-c3c04e578c71",
                    "sense_definition": "Data lub przedział czasu wyznaczony na wykonanie jakiejś czynności albo moment, w którym coś ma nastąpić.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_constraint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy ustalić dogodny <w>termin</w> naszego spotkania.",
                            "target_lang_translations": {
                                "ru": "Мы должны назначить удобную <w>дату</w> нашей встречи."
                            }
                        },
                        {
                            "text": "Niestety nie udało mi się dotrzymać <w>terminu</w> oddania projektu.",
                            "target_lang_translations": {
                                "ru": "К сожалению, мне не удалось соблюсти <w>срок</w> сдачи проекта."
                            }
                        },
                        {
                            "text": "Sprawdź <w>termin</w> ważności na opakowaniu tego produktu.",
                            "target_lang_translations": {
                                "ru": "Проверь <w>срок</w> годности на упаковке этого продукта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "termin ważności",
                        "dotrzymać terminu",
                        "w terminie",
                        "termin płatności",
                        "przekroczyć termin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дата или период времени, назначенные для выполнения какого-либо действия, или момент, когда что-то должно произойти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срок",
                                "target_lang_sense_clarification": "период времени или дедлайн"
                            },
                            {
                                "target_lang_word": "дата",
                                "target_lang_sense_clarification": "календарный день"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5209c1b8-804f-3556-bed2-4c22bb28d898",
                    "sense_definition": "Wyraz lub wyrażenie o ściśle zdefiniowanym znaczeniu w określonej dziedzinie wiedzy, nauki lub techniki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "vocabulary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>termin</w> medyczny jest trudny do zrozumienia dla pacjenta.",
                            "target_lang_translations": {
                                "ru": "Этот медицинский <w>термин</w> труден для понимания пациентом."
                            }
                        },
                        {
                            "text": "W umowie użyto wielu skomplikowanych <w>terminów</w> prawniczych.",
                            "target_lang_translations": {
                                "ru": "В договоре использовано много сложных юридических <w>терминов</w>."
                            }

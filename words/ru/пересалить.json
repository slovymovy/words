    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "88273ef4-60d5-3f1b-a656-8e01d996a08b",
                    "sense_definition": "Положить в еду слишком много сала или жира, сделав её чрезмерно жирной.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Повар случайно <пересалил> кашу, добавив в неё слишком много шкварок.",
                            "target_lang_translations": {
                                "en": "The cook accidentally <w>put too much fat in</w> the porridge by adding too many cracklings."
                            }
                        },
                        {
                            "text": "Старайся не <пересаливать> суп, иначе он получится слишком тяжёлым для желудка.",
                            "target_lang_translations": {
                                "en": "Try not to <w>add too much fat to</w> the soup, otherwise it will turn out too heavy for the stomach."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пересалить кашу",
                        "пересалить суп",
                        "пересалить блюдо"
                    ],
                    "target_lang_definitions": {
                        "en": "To put too much lard or fat into food, making it excessively greasy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put too much fat in",
                                "target_lang_sense_clarification": "cooking context"
                            },
                            {
                                "target_lang_word": "make too greasy",
                                "target_lang_sense_clarification": "regarding food"
                            },
                            {
                                "target_lang_word": "add too much lard to",
                                "target_lang_sense_clarification": "specific to lard"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0dca2fa-f2c8-3279-91ae-edf402d32dcc",
                    "sense_definition": "Испачкать, покрыть жиром или грязью множество предметов один за другим.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Во время ремонта механик умудрился <пересалить> мазутом всю свою одежду.",
                            "target_lang_translations": {
                                "en": "During the repairs, the mechanic managed to <w>grease up</w> all his clothes with fuel oil."
                            }
                        },
                        {
                            "text": "Дети хватали еду руками и успели <пересалить> скатерть и салфетки.",
                            "target_lang_translations": {
                                "en": "The children grabbed food with their hands and managed to <w>grease up</w> the tablecloth and napkins."
                            }
                        }
                    ],
                    "synonyms": [
                        "перепачкать",
                        "перемарать"
                    ],
                    "common_phrases": [
                        "пересалить одежду",
                        "пересалить руки",
                        "всё пересалить"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stain or cover multiple items with grease or dirt one after another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grease up",
                                "target_lang_sense_clarification": "to make dirty with grease"
                            },
                            {
                                "target_lang_word": "stain with grease",
                                "target_lang_sense_clarification": "to soil multiple things"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8401b3d-9e11-3afb-a21a-2131da81810b",
                    "sense_definition": "В детской игре (салочки): коснуться рукой многих или всех участников, передавая им роль водящего или выводя из игры.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Саша бегал быстрее всех и за пять минут успел <пересалить> половину игроков.",
                            "target_lang_translations": {
                                "en": "Sasha ran faster than everyone else and in five minutes managed to <w>tag</w> half the players."
                            }
                        },
                        {
                            "text": "К концу перемены водящий <пересалил> почти весь класс, и игра закончилась.",
                            "target_lang_translations": {
                                "en": "By the end of the break, the person who was \"it\" had <w>tagged</w> almost the whole class, and the game ended."
                            }
                        }
                    ],
                    "synonyms": [
                        "переловить"
                    ],
                    "common_phrases": [
                        "пересалить игроков",
                        "пересалить всех"
                    ],
                    "target_lang_definitions": {
                        "en": "In the children's game of tag: to touch many or all participants."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tag",
                                "target_lang_sense_clarification": "in a game"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
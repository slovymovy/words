                },
                {
                    "sense_id": "d7b7fd31-a817-3040-9c4f-b111d4692963",
                    "sense_definition": "Przeznaczony do spożycia przez ludzi; nadający się do jedzenia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

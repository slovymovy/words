                {
                    "sense_id": "4d258206-6c37-3c03-b3b2-5902856141e8",
                    "sense_definition": "De mogelijkheid om over iets of iemand te beslissen of er gebruik van te maken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als gast heeft u de hele dag de <w>beschikking</w> over het zwembad.",
                            "target_lang_translations": {
                                "ru": "Как гость, вы имеете в своем <w>распоряжении</w> бассейн в течение всего дня.",
                                "en": "As a guest, you have the <w>use</w> of the pool all day."
                            }
                        },
                        {
                            "text": "Ik sta volledig tot uw <w>beschikking</w> als u nog vragen heeft.",
                            "target_lang_translations": {
                                "ru": "Я полностью в вашем <w>распоряжении</w>, если у вас есть еще вопросы.",
                                "en": "I am entirely at your <w>disposal</w> if you have any further questions."
                            }
                        },
                        {
                            "text": "Het bedrijf stelde hem een auto ter <w>beschikking</w> voor zijn werkbezoeken.",
                            "target_lang_translations": {
                                "ru": "Компания предоставила ему автомобиль в <w>распоряжение</w> для рабочих поездок.",
                                "en": "The company placed a car at his <w>disposal</w> for his work visits."
                            }
                        }
                    ],
                    "synonyms": [
                        "toegang",
                        "gebruik",
                        "macht"
                    ],
                    "common_phrases": [
                        "ter beschikking stellen",
                        "tot uw beschikking",
                        "de beschikking hebben over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность распоряжаться чем-либо или использовать что-либо по своему усмотрению.",
                        "en": "The ability to decide on or make use of something or someone."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распоряжение",
                                "target_lang_sense_clarification": "право решать или использовать"

{
    "word_family": [
        "przeciwnik",
        "przeciwność",
        "przeciwnie",
        "sprzeciw",
        "sprzeciwiać się"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b6b710c0-744c-37e5-9697-52fcf8bf85f1",
                    "sense_definition": "Całkowicie różny od czegoś innego; będący odwrotnością.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_difference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam zupełnie <w>przeciwne</w> zdanie na ten temat niż mój brat.",
                            "target_lang_translations": {
                                "ru": "У меня совершенно <w>противоположное</w> мнение на этот счёт, чем у моего брата."
                            }
                        },
                        {
                            "text": "Lekarstwo wywołało skutek <w>przeciwny</w> do zamierzonego i pacjent poczuł się gorzej.",
                            "target_lang_translations": {
                                "ru": "Лекарство вызвало эффект, <w>противоположный</w> ожидаемому, и пациент почувствовал себя хуже."
                            }
                        }
                    ],
                    "synonyms": [
                        "odwrotny",
                        "przeciwstawny",
                        "odmienny",
                        "inny"
                    ],
                    "antonyms": [
                        "tożsamy",
                        "zgodny"
                    ],
                    "common_phrases": [
                        "w przeciwnym razie",
                        "skutek przeciwny do zamierzonego",
                        "przeciwne zdanie",
                        "biegun przeciwny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершенно отличный от чего-либо другого; являющийся противоположностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противоположный",
                                "target_lang_sense_clarification": "совершенно другой, обратный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b816c02f-1b38-3e99-a08f-61fe9b09e7af",
                    "sense_definition": "Niezgadzający się na coś, nastawiony niechętnie lub wrogo do kogoś lub czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice są <w>przeciwni</w> temu pomysłowi, ponieważ uważają go za ryzykowny.",
                            "target_lang_translations": {
                                "ru": "Мои родители <w>против</w> этой идеи, так как считают её рискованной."
                            }
                        },
                        {
                            "text": "Większość mieszkańców była <w>przeciwna</w> budowie nowej drogi przez las.",
                            "target_lang_translations": {
                                "ru": "Большинство жителей было <w>против</w> строительства новой дороги через лес."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieprzychylny"
                    ],
                    "antonyms": [
                        "przychylny"
                    ],
                    "common_phrases": [
                        "być przeciwnym czemuś",
                        "głosy przeciwne",
                        "jestem temu przeciwny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не соглашающийся с чем-либо, настроенный враждебно или отрицательно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "против",
                                "target_lang_sense_clarification": "не согласный (в значении сказуемого)"
                            },
                            {
                                "target_lang_word": "несогласный",
                                "target_lang_sense_clarification": "выражающий несогласие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d07e318-ee4f-3f3a-b255-5f8544f1794f",
                    "sense_definition": "Znajdujący się po drugiej stronie czegoś lub skierowany w odwrotnym kierunku.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód ciężarowy nadjechał z <w>przeciwnego</w> kierunku i zablokował drogę.",
                            "target_lang_translations": {
                                "ru": "Грузовик выехал со <w>встречного</w> направления и заблокировал дорогу."
                            }
                        },
                        {
                            "text": "Sklep spożywczy znajduje się po <w>przeciwnej</w> stronie ulicy.",
                            "target_lang_translations": {
                                "ru": "Продуктовый магазин находится на <w>противоположной</w> стороне улицы."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeciwległy"
                    ],
                    "common_phrases": [
                        "przeciwny kierunek",
                        "przeciwna strona ulicy",
                        "pas przeciwny",
                        "w przeciwną stronę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся на другой стороне или направленный в обратную сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противоположный",
                                "target_lang_sense_clarification": "находящийся напротив"
                            },
                            {
                                "target_lang_word": "встречный",
                                "target_lang_sense_clarification": "движущийся навстречу (о направлении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba42269b-dd55-328b-a040-3a0b04294d70",
                    "sense_definition": "Należący do rywalizującej grupy, walczący po drugiej stronie (np. w sporcie lub konflikcie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rivalry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bramkarz <w>przeciwnej</w> drużyny obronił rzut karny w ostatniej minucie meczu.",
                            "target_lang_translations": {
                                "ru": "Вратарь <w>противоположной</w> команды отбил пенальти на последней минуте матча."
                            }
                        },
                        {
                            "text": "Adwokat strony <w>przeciwnej</w> przedstawił nowe dowody w sprawie.",
                            "target_lang_translations": {
                                "ru": "Адвокат <w>противной</w> стороны представил новые доказательства по делу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przeciwna drużyna",
                        "strona przeciwna",
                        "obóz przeciwny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий к соперничающей группе, сражающийся на другой стороне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противоположный",
                                "target_lang_sense_clarification": "соперничающий"
                            },
                            {
                                "target_lang_word": "противный",
                                "target_lang_sense_clarification": "враждебный (обычно в выражении 'противная сторона')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3486826-1921-33de-852e-61b8b36a0251",
                    "sense_definition": "O liczbie, która różni się od innej liczby tylko znakiem (plus lub minus).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Liczbą <w>przeciwną</w> do pięciu jest minus pięć.",
                            "target_lang_translations": {
                                "ru": "Числом, <w>противоположным</w> пяти, является минус пять."
                            }
                        },
                        {
                            "text": "Suma dwóch liczb <w>przeciwnych</w> zawsze wynosi zero.",
                            "target_lang_translations": {
                                "ru": "Сумма двух <w>противоположных</w> чисел всегда равна нулю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "liczby przeciwne",
                        "znak przeciwny"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "matematyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О числе, которое отличается от другого только знаком (плюс или минус)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противоположный",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
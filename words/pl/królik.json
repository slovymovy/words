    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc3a8b8f-04e5-3d6b-b617-ea98ffc594c1",
                    "sense_definition": "Ssak z rodziny zającowatych o długich uszach, krótkim ogonie i miękkim futrze, często hodowany jako zwierzę domowe.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój mały <królik> uwielbia jeść świeżą marchewkę i sałatę.",
                            "target_lang_translations": {
                                "ru": "Мой маленький <кролик> обожает есть свежую морковку и салат."
                            }
                        },
                        {
                            "text": "W sklepie zoologicznym widziałem białego <królika> z czerwonymi oczami.",
                            "target_lang_translations": {
                                "ru": "В зоомагазине я видел белого <кролика> с красными глазами."
                            }
                        }
                    ],
                    "synonyms": [
                        "trusia"
                    ],
                    "common_phrases": [
                        "królik doświadczalny",
                        "biały królik",
                        "hodować króliki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Млекопитающее из семейства зайцевых с длинными ушами, коротким хвостом и мягким мехом, часто содержащееся как домашнее животное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кролик",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2278839-fc8b-3c3c-80fc-3ff7e82d1e7b",
                    "sense_definition": "Mięso z królika przeznaczone do spożycia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na niedzielny obiad mama przygotowała pieczonego <królika> w śmietanie.",
                            "target_lang_translations": {
                                "ru": "На воскресный обед мама приготовила запеченного <кролика> в сметане."
                            }
                        },
                        {
                            "text": "Pasztet z <królika> jest znacznie delikatniejszy niż wieprzowy.",
                            "target_lang_translations": {
                                "ru": "Паштет из <кролика> гораздо нежнее, чем свиной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pasztet z królika",
                        "pieczony królik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясо кролика, употребляемое в пищу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крольчатина",
                                "target_lang_sense_clarification": "мясо"
                            },
                            {
                                "target_lang_word": "кролик",
                                "target_lang_sense_clarification": "кулинарное блюдо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3592e6f-d8f0-3bfc-8d2b-6c51303e5dd1",
                    "sense_definition": "Futro pozyskane z królika, wykorzystywane w przemyśle odzieżowym.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej zimowa czapka jest wykonana z naturalnego <królika>.",
                            "target_lang_translations": {
                                "ru": "Её зимняя шапка сделана из натурального <кролика>."
                            }
                        },
                        {
                            "text": "Ten płaszcz ma miękki kołnierz z farbowanego <królika>.",
                            "target_lang_translations": {
                                "ru": "У этого пальто мягкий воротник из крашеного <кролика>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czapka z królika",
                        "futro z królika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мех кролика, используемый для изготовления одежды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кролик",
                                "target_lang_sense_clarification": "мех"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd69acc0-e193-3f96-a034-f73ccad46772",
                    "sense_definition": "Zdrobnienie od <w>król</w>; władca małego lub mało znaczącego państwa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ruler",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lokalny <królik> próbował narzucić swoje prawa wszystkim okolicznym mieszkańcom.",
                            "target_lang_translations": {
                                "ru": "Местный <королёк> пытался навязать свои законы всем окрестным жителям."
                            }
                        },
                        {
                            "text": "Historia zapomniała o wielu takich <królikach>, którzy rządzili tylko przez chwilę.",
                            "target_lang_translations": {
                                "ru": "История забыла о многих таких <корольках>, которые правили лишь мгновение."
                            }
                        }
                    ],
                    "synonyms": [
                        "kacyk"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительное от слова «король»; правитель небольшого или маловлиятельного государства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "королёк",
                                "target_lang_sense_clarification": "пренебрежительно о правителе"
                            },
                            {
                                "target_lang_word": "царёк",
                                "target_lang_sense_clarification": "мелкий правитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "282578b8-019a-3aa0-9964-998ae9c86700",
                    "sense_definition": "Mężczyzna o bardzo dużym temperamencie seksualnym.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten chłopak to prawdziwy <królik>, ciągle szuka nowych partnerek.",
                            "target_lang_translations": {
                                "ru": "Этот парень — настоящий <кролик>, он постоянно ищет новых партнерш."
                            }
                        },
                        {
                            "text": "Zachowują się jak <króliki>, nie zważając na opinię innych ludzi.",
                            "target_lang_translations": {
                                "ru": "Они ведут себя как <кролики>, не обращая внимания на мнение других людей."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина с очень сильным половым влечением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кролик",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                }
            ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "10849f89-97ac-33fb-bf37-f22dcdd0cbf5",
                    "sense_definition": "Een zachte, fijngeweven stof met rechtopstaande haartjes, vergelijkbaar met fluweel.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "textile_fabric",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koningin droeg een mantel van zwaar, donker <w>floer</w>.",
                            "target_lang_translations": {
                                "en": "The queen wore a cloak of heavy, dark <w>velvet</w>.",
                                "ru": "Королева носила мантию из тяжелого темного <w>бархата</w>."
                            }
                        },
                        {
                            "text": "In de oude kist vonden we lappen van rood <w>floer</w>.",
                            "target_lang_translations": {
                                "en": "In the old chest, we found pieces of red <w>velvet</w>.",
                                "ru": "В старом сундуке мы нашли лоскуты красного <w>бархата</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "fluweel",
                        "velours"
                    ],
                    "common_phrases": [
                        "van floer",
                        "zwart floer"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "wordt tegenwoordig meestal 'fluweel' genoemd"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A soft, finely woven fabric with upright hairs, similar to velvet.",
                        "ru": "Мягкая, тонкая ткань с вертикальным ворсом, похожая на бархат."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "velvet",
                                "target_lang_sense_clarification": "dated term for this fabric"
                            },
                            {
                                "target_lang_word": "plush",
                                "target_lang_sense_clarification": "fabric with a cut pile"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бархат",
                                "target_lang_sense_clarification": "ткань"
                            },
                            {
                                "target_lang_word": "плюш",
                                "target_lang_sense_clarification": "ткань с длинным ворсом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c65fc5ba-d757-3af7-9acf-8db0cbfb5754",
                    "sense_definition": "De rechtopstaande haartjes of draden op een fluwelen stof.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "textile_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je tegen het <w>floer</w> in strijkt, voelt de stof ruwer aan.",
                            "target_lang_translations": {
                                "en": "If you stroke against the <w>nap</w>, the fabric feels rougher.",
                                "ru": "Если гладить против <w>ворса</w>, ткань кажется более грубой."
                            }
                        },
                        {
                            "text": "Het <w>floer</w> van het tapijt is door het vele lopen platgedrukt.",
                            "target_lang_translations": {
                                "en": "The <w>pile</w> of the carpet has been flattened by frequent walking.",
                                "ru": "<w>Ворс</w> ковра примялся от частой ходьбы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The upright hairs or threads on a velvet fabric.",
                        "ru": "Вертикальные волоски или нити на поверхности бархатной ткани."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pile",
                                "target_lang_sense_clarification": "raised surface of a fabric"
                            },
                            {
                                "target_lang_word": "nap",
                                "target_lang_sense_clarification": "raised fuzzy surface"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ворс",
                                "target_lang_sense_clarification": "поверхность ткани"
                            }
                        ]
                    }
                }
            ]
        }
    ]

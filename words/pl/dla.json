    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "f9a1c277-8298-37a6-bb47-2569ecd69084",
                    "sense_definition": "Wskazuje na osobę lub rzecz, która jest odbiorcą czegoś lub dla której coś jest przeznaczone.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "recipient",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem ten piękny bukiet kwiatów specjalnie <w>dla</w> mojej mamy.",
                            "target_lang_translations": {
                                "ru": "Я купил этот красивый букет цветов специально <w>для</w> моей мамы."
                            }
                        },
                        {
                            "text": "To jest bardzo ważna wiadomość <w>dla</w> wszystkich pracowników firmy.",
                            "target_lang_translations": {
                                "ru": "Это очень важное сообщение <w>для</w> всех сотрудников компании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prezent <w>dla</w> ciebie",
                        "miejsce <w>dla</w> inwalidów",
                        "wszystko <w>dla</w> rodziny",
                        "czas <w>dla</w> siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на лицо или предмет, которому что-либо предназначается или адресуется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для",
                                "target_lang_sense_clarification": "предназначение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f597b87-4a35-31cf-99f1-aa025245417d",
                    "sense_definition": "Wskazuje na obiekt, na który coś oddziałuje, często w kontekście korzyści, szkody lub bezpieczeństwa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regularne uprawianie sportu jest bardzo dobre <w>dla</w> twojego zdrowia.",
                            "target_lang_translations": {
                                "ru": "Регулярные занятия спортом очень полезны <w>для</w> твоего здоровья."
                            }
                        },
                        {
                            "text": "Ten nowy lek jest całkowicie bezpieczny <w>dla</w> małych dzieci.",
                            "target_lang_translations": {
                                "ru": "Это новое лекарство полностью безопасно <w>для</w> маленьких детей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szkodliwe <w>dla</w> zdrowia",
                        "niebezpieczne <w>dla</w> otoczenia",
                        "dobre <w>dla</w> ciebie",
                        "przyjazny <w>dla</w> środowiska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на объект, на который оказывается воздействие, часто в контексте пользы, вреда или безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для",
                                "target_lang_sense_clarification": "влияние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bb1e2ee-143f-30bd-82c1-0f9718bf3d19",
                    "sense_definition": "Wskazuje na cel, przyczynę lub motywację działania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zrobiliśmy to tylko <w>dla</w> zabawy, nie chcieliśmy nikogo urazić.",
                            "target_lang_translations": {
                                "ru": "Мы сделали это только <w>ради</w> забавы, мы не хотели никого обидеть."
                            }
                        },
                        {
                            "text": "Musisz to zrobić <w>dla</w> własnego dobra i spokoju ducha.",
                            "target_lang_translations": {
                                "ru": "Ты должен сделать это <w>ради</w> собственного блага и душевного спокойствия."
                            }
                        }
                    ],
                    "synonyms": [
                        "celem",
                        "w celu"
                    ],
                    "common_phrases": [
                        "<w>dla</w> zabawy",
                        "<w>dla</w> przykładu",
                        "<w>dla</w> bezpieczeństwa",
                        "<w>dla</w> pewności",
                        "sztuka <w>dla</w> sztuki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на цель, причину или мотивацию действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ради",
                                "target_lang_sense_clarification": "мотивация"
                            },
                            {
                                "target_lang_word": "для",
                                "target_lang_sense_clarification": "цель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8797a5b-d44d-371d-8e68-7ff013f30057",
                    "sense_definition": "Wskazuje na osobę, z której punktu widzenia coś jest oceniane lub względem której przejawiana jest jakaś postawa.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "perspective",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten temat jest <w>dla</w> mnie bardzo trudny do zrozumienia.",
                            "target_lang_translations": {
                                "ru": "Эта тема <w>для</w> меня очень сложна для понимания."
                            }
                        },
                        {
                            "text": "Nauczyciel był bardzo wyrozumiały <w>dla</w> spóźnionych uczniów.",
                            "target_lang_translations": {
                                "ru": "Учитель был очень снисходителен <w>к</w> опоздавшим ученикам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trudne <w>dla</w> mnie",
                        "ważne <w>dla</w> nas",
                        "miły <w>dla</w> gości",
                        "surowy <w>dla</w> podwładnych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на лицо, чья точка зрения выражается или по отношению к которому проявляется определенное отношение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для",
                                "target_lang_sense_clarification": "мнение"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "отношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df6e3611-3a72-3ca5-8ee2-4599ce62320f",
                    "sense_definition": "Wskazuje stronę, która zdobyła punkty lub prowadzi w wyniku rywalizacji sportowej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "score",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mecz zakończył się wynikiem trzy do jednego <w>dla</w> gospodarzy.",
                            "target_lang_translations": {
                                "ru": "Матч закончился со счетом три один <w>в пользу</w> хозяев."
                            }
                        },
                        {
                            "text": "W pierwszej połowie wynik był korzystny <w>dla</w> drużyny gości.",
                            "target_lang_translations": {
                                "ru": "В первом тайме результат был <w>в пользу</w> команды гостей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dwa do zera <w>dla</w> nas",
                        "punkt <w>dla</w> przeciwnika",
                        "jeden zero <w>dla</w> Polski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на сторону, которая выигрывает или ведет в счете в спортивном состязании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в пользу",
                                "target_lang_sense_clarification": "счёт"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e85b79b-7c34-3c02-9fca-92a927950a37",
                    "sense_definition": "A feeling of sadness or depression resulting from being alone or having no companions.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She felt a deep sense of <w>loneliness</w> after her best friend moved to another city.",
                            "target_lang_translations": {
                                "ru": "Она ощутила глубокое чувство <w>одиночества</w> после того, как её лучшая подруга переехала в другой город."
                            }
                        },
                        {
                            "text": "Many elderly people struggle to overcome <w>loneliness</w> during the holiday season.",
                            "target_lang_translations": {
                                "ru": "Многие пожилые люди с трудом пытаются преодолеть <w>одиночество</w> во время праздников."
                            }
                        }
                    ],
                    "synonyms": [
                        "lonesomeness",
                        "forlornness",
                        "friendlessness"
                    ],
                    "antonyms": [
                        "companionship"
                    ],
                    "common_phrases": [
                        "sense of loneliness",
                        "overcome loneliness",
                        "feelings of loneliness",
                        "suffer from loneliness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство грусти или подавленности, вызванное отсутствием общения или компании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одиночество",
                                "target_lang_sense_clarification": "эмоциональное состояние"
                            },
                            {
                                "target_lang_word": "тоска",
                                "target_lang_sense_clarification": "чувство покинутости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16a014c2-cdaf-3ce7-94fe-cb5d40b53544",
                    "sense_definition": "The condition or state of being alone or isolated from others.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The explorer had to endure the extreme <w>loneliness</w> of the Antarctic winter.",
                            "target_lang_translations": {
                                "ru": "Исследователю пришлось пережить крайнее <w>одиночество</w> антарктической зимы."
                            }
                        },
                        {
                            "text": "He accepted the <w>loneliness</w> of his position as the necessary cost of leadership.",
                            "target_lang_translations": {
                                "ru": "Он принял <w>одиночество</w> своего положения как необходимую плату за лидерство."
                            }
                        }
                    ],
                    "synonyms": [
                        "aloneness",
                        "solitariness",
                        "solitude",
                        "isolation"
                    ],
                    "antonyms": [
                        "fellowship",
                        "sociableness"
                    ],
                    "common_phrases": [
                        "extreme loneliness",
                        "bear the loneliness",
                        "moments of loneliness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Условие или состояние пребывания в одиночестве или изоляции от других людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одиночество",
                                "target_lang_sense_clarification": "фактическое состояние"
                            },
                            {
                                "target_lang_word": "изоляция",
                                "target_lang_sense_clarification": "отрыв от общества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d948c34-71ef-36a8-94aa-9b9054338b8b",
                    "sense_definition": "The quality of a place being unfrequented, empty, or devoid of human activity.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "place_atmosphere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The vast <w>loneliness</w> of the desert landscape was both frightening and beautiful.",
                            "target_lang_translations": {
                                "ru": "Бескрайняя <w>пустынность</w> пустынного пейзажа была одновременно пугающей и прекрасной."
                            }
                        },
                        {
                            "text": "We were struck by the sheer <w>loneliness</w> of the abandoned farmhouse on the hill.",
                            "target_lang_translations": {
                                "ru": "Нас поразило явное <w>одиночество</w> заброшенного хутора на холме."
                            }
                        }
                    ],
                    "synonyms": [
                        "seclusion",
                        "unfrequentedness",
                        "isolation"
                    ],
                    "common_phrases": [
                        "loneliness of the place",
                        "vast loneliness"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in literary or descriptive contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство места быть безлюдным, пустым или лишенным человеческой деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пустынность",
                                "target_lang_sense_clarification": "отсутствие людей"
                            },
                            {
                                "target_lang_word": "безлюдность",
                                "target_lang_sense_clarification": "свойство места"
                            },
                            {
                                "target_lang_word": "уединённость",
                                "target_lang_sense_clarification": "отдалённость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3118cc9a-18e5-309f-a1cb-fee10a0fdcd7",
                    "sense_definition": "A desire to be alone or a disposition towards solitude.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "disposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He cherished his <w>loneliness</w> and refused to attend the crowded gathering.",
                            "target_lang_translations": {
                                "ru": "Он дорожил своим <w>уединением</w> и отказался идти на многолюдное собрание."
                            }
                        },
                        {
                            "text": "Her <w>loneliness</w> was not out of sadness, but a preference for quiet reflection.",
                            "target_lang_translations": {
                                "ru": "Её <w>уединение</w> было вызвано не грустью, а предпочтением тихих размышлений."
                            }
                        }
                    ],
                    "synonyms": [
                        "solitude"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage typically implies sadness"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Желание быть одному или склонность к уединению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уединение",
                                "target_lang_sense_clarification": "желание быть одному"
                            }
                        ]
                    }
                }
            ]
        }
    ]

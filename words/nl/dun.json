                {
                    "sense_id": "e0595c17-468a-3685-b2a6-f3dbbec31987",
                    "sense_definition": "Van geringe dikte; met een kleine afstand tussen de twee tegenovergestelde oppervlakken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_dimension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er ligt vanochtend een <w>dun</w> laagje ijs op de sloten.",
                            "target_lang_translations": {
                                "en": "There is a <w>thin</w> layer of ice on the ditches this morning.",
                                "ru": "Сегодня утром на канавах лежит <w>тонкий</w> слой льда."
                            }
                        },
                        {
                            "text": "Schrijf je naam op dit <w>dunne</w> strookje papier.",
                            "target_lang_translations": {
                                "en": "Write your name on this <w>thin</w> strip of paper.",
                                "ru": "Напиши свое имя на этой <w>тонкой</w> полоске бумаги."
                            }
                        }
                    ],
                    "antonyms": [
                        "dik"
                    ],
                    "common_phrases": [
                        "een dunne laag",
                        "dun papier",
                        "dun ijs"
                    ],
                    "target_lang_definitions": {
                        "en": "Of little thickness; with a small distance between the two opposite surfaces.",
                        "ru": "Имеющий малую толщину; с небольшим расстоянием между двумя противоположными поверхностями."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thin",
                                "target_lang_sense_clarification": "having little thickness"

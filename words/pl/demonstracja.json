            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c8b42a63-25a5-3919-9aa8-379f36220a09",
                    "sense_definition": "Zbiorowe wystąpienie publiczne mające na celu wyrażenie protestu lub poparcia dla jakiejś sprawy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj w centrum miasta odbyła się głośna <w>demonstracja</w> przeciwko nowym podatkom.",
                            "target_lang_translations": {
                                "ru": "Вчера в центре города прошла громкая <w>демонстрация</w> против новых налогов."
                            }
                        },
                        {
                            "text": "Policja pilnowała porządku podczas pokojowej <w>demonstracji</w> studentów.",
                            "target_lang_translations": {
                                "ru": "Полиция следила за порядком во время мирной <w>демонстрации</w> студентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "manifestacja",
                        "protest"
                    ],
                    "antonyms": [
                        "kontrdemonstracja"
                    ],
                    "common_phrases": [
                        "pokojowa demonstracja",
                        "udział w demonstracji",
                        "zorganizować demonstrację",
                        "tłumić demonstrację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Массовое публичное выступление с целью выражения протеста или поддержки какого-либо дела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демонстрация",
                                "target_lang_sense_clarification": "публичное шествие"
                            },
                            {
                                "target_lang_word": "митинг",
                                "target_lang_sense_clarification": "собрание людей"
                            },
                            {
                                "target_lang_word": "манифестация",
                                "target_lang_sense_clarification": "массовое выступление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb47258a-a108-3f8c-b1cd-193809fdeaaf",
                    "sense_definition": "Czynność pokazywania, jak coś działa, wygląda lub jak należy coś wykonać; prezentacja.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel przygotował dla uczniów ciekawą <w>demonstrację</w> reakcji chemicznej.",
                            "target_lang_translations": {
                                "ru": "Учитель подготовил для учеников интересную <w>демонстрацию</w> химической реакции."
                            }
                        },
                        {
                            "text": "Sprzedawca przeprowadził <w>demonstrację</w> nowego modelu telefonu dla klientów.",
                            "target_lang_translations": {
                                "ru": "Продавец провел <w>демонстрацию</w> новой модели телефона для клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "protest"
                    ],
                    "common_phrases": [
                        "demonstracja produktu",
                        "praktyczna demonstracja",
                        "demonstracja oprogramowania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, показывающее, как что-то работает, выглядит или как нужно что-то делать; презентация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демонстрация",
                                "target_lang_sense_clarification": "наглядный показ"
                            },
                            {
                                "target_lang_word": "показ",
                                "target_lang_sense_clarification": "представление"
                            },
                            {
                                "target_lang_word": "презентация",
                                "target_lang_sense_clarification": "представление продукта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4096e89c-a4f4-3522-86ef-527659e511af",
                    "sense_definition": "Ostentacyjne i wyraźne okazywanie swoich uczuć lub postawy, często obliczone na wywołanie efektu lub zwrócenie uwagi.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego spóźnienie było celową <w>demonstracją</w> braku szacunku dla szefa.",
                            "target_lang_translations": {
                                "ru": "Его опоздание было намеренной <w>демонстрацией</w> неуважения к начальнику."
                            }
                        },
                        {
                            "text": "To była tylko <w>demonstracja</w> siły, aby przestraszyć przeciwnika.",
                            "target_lang_translations": {
                                "ru": "Это была лишь <w>демонстрация</w> силы, чтобы напугать противника."
                            }
                        }
                    ],
                    "common_phrases": [
                        "demonstracja siły",
                        "demonstracja niezadowolenia",
                        "demonstracja władzy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарочитое и явное проявление своих чувств или отношения, часто рассчитанное на произведение эффекта или привлечение внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демонстрация",
                                "target_lang_sense_clarification": "проявление чувств"
                            },
                            {
                                "target_lang_word": "показное поведение",
                                "target_lang_sense_clarification": "нарочитость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce4d6cc3-f198-3bc3-970a-359ba25ca77b",
                    "sense_definition": "Manewr taktyczny wojsk mający na celu odwrócenie uwagi przeciwnika lub wywarcie na nim presji.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Generał zarządził <w>demonstrację</w> floty przy granicy państwa.",
                            "target_lang_translations": {
                                "ru": "Генерал приказал провести <w>демонстрацию</w> флота у границы государства."
                            }
                        },
                        {
                            "text": "Ruchy wojsk były jedynie <w>demonstracją</w>, a nie rzeczywistym atakiem.",
                            "target_lang_translations": {
                                "ru": "Передвижения войск были лишь <w>демонстрацией</w>, а не реальной атакой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "demonstracja zbrojna",
                        "demonstracja morska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тактический маневр войск, направленный на отвлечение внимания противника или оказание на него давления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демонстрация",
                                "target_lang_sense_clarification": "военный маневр"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "34ed4379-745a-3e1f-92a6-3d24c4aacc9d",
                    "sense_definition": "Męskie imię pochodzenia słowiańskiego, oznaczające kogoś, kto włada pokojem lub światem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "person_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mój wujek <w>Włodzimierz</w> mieszka w Krakowie i jest lekarzem.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Владимир</w> живет в Кракове и работает врачом."
                            }
                        },
                        {
                            "text": "Kniaź <w>Włodzimierz</w> odegrał kluczową rolę w historii Rusi.",
                            "target_lang_translations": {
                                "ru": "Князь <w>Владимир</w> сыграл ключевую роль в истории Руси."
                            }
                        }
                    ],
                    "synonyms": [
                        "Włodek",
                        "Włodzimir"
                    ],
                    "common_phrases": [
                        "pan Włodzimierz",
                        "święty Włodzimierz",
                        "wujek Włodzimierz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя славянского происхождения, означающее того, кто владеет миром."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Владимир",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47e1eb20-aa19-3f57-abea-4b491f9dcf2c",
                    "sense_definition": "Miasto w zachodniej Rosji, stolica obwodu, położone nad rzeką Klaźmą; historycznie ważne centrum polityczne i religijne.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geo_location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Jutro wieczorem jadę pociągiem do <w>Włodzimierza</w>.",
                            "target_lang_translations": {
                                "ru": "Завтра вечером я еду на поезде во <w>Владимир</w>."
                            }
                        },
                        {
                            "text": "Zabytki we <w>Włodzimierzu</w> są wpisane na listę UNESCO.",
                            "target_lang_translations": {
                                "ru": "Памятники во <w>Владимире</w> внесены в список всемирного наследия ЮНЕСКО."
                            }
                        }
                    ],
                    "common_phrases": [
                        "miasto Włodzimierz",
                        "zwiedzanie Włodzimierza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в западной части России, административный центр области, расположенный на реке Клязьме; исторически важный политический и религиозный центр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Владимир",
                                "target_lang_sense_clarification": "город в России"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a74cfc1b-947c-3d24-b24c-97d31d03c5c7",
                    "sense_definition": "Miasto na zachodniej Ukrainie, w obwodzie wołyńskim, historyczna stolica Wołynia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geo_location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Autobus rejsowy do <w>Włodzimierza</w> odjeżdża z dworca głównego.",
                            "target_lang_translations": {
                                "ru": "Рейсовый автобус во <w>Владимир</w> отправляется с главного вокзала."
                            }
                        },
                        {
                            "text": "Jego rodzina pochodzi z okolic <w>Włodzimierza</w> na Wołyniu.",
                            "target_lang_translations": {
                                "ru": "Его семья родом из окрестностей <w>Владимира</w> на Волыни."
                            }
                        }
                    ],
                    "synonyms": [
                        "Włodzimierz Wołyński"
                    ],
                    "common_phrases": [
                        "historia Włodzimierza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Город на западной Украине, в Волынской области, историческая столица Волыни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Владимир",
                                "target_lang_sense_clarification": "город на Украине"
                            },
                            {
                                "target_lang_word": "Владимир-Волынский",
                                "target_lang_sense_clarification": "историческое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d66543b8-4b72-3349-a0aa-98c222c3edb5",
                    "sense_definition": "Nazwa kilku wsi w Polsce, położonych między innymi w województwie łódzkim i wielkopolskim.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geo_location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mała wieś <w>Włodzimierz</w> leży niedaleko Łasku.",
                            "target_lang_translations": {
                                "ru": "Маленькая деревня <w>Влодзимеж</w> находится недалеко от Ласка."
                            }
                        },
                        {
                            "text": "Przejeżdżaliśmy przez <w>Włodzimierz</w> w drodze na wakacje.",
                            "target_lang_translations": {
                                "ru": "Мы проезжали через <w>Влодзимеж</w> по дороге в отпуск."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название нескольких деревень в Польше, расположенных, в частности, в Лодзинском и Великопольском воеводствах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Влодзимеж",
                                "target_lang_sense_clarification": "польский топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]

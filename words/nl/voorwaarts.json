    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "5fa526c9-0453-35dd-8fc4-5696ecf99745",
                    "sense_definition": "In een richting die naar voren wijst.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De auto rolde langzaam <w>voorwaarts</w> toen hij de rem losliet.",
                            "target_lang_translations": {
                                "en": "The car rolled slowly <w>forward</w> when he released the brake."
                            }
                        },
                        {
                            "text": "We moeten <w>voorwaarts</w> blijven kijken en niet in het verleden blijven hangen.",
                            "target_lang_translations": {
                                "en": "We must keep looking <w>forward</w> and not dwell on the past."
                            }
                        }
                    ],
                    "synonyms": [
                        "vooruit"
                    ],
                    "antonyms": [
                        "achterwaarts",
                        "terugwaarts"
                    ],
                    "common_phrases": [
                        "stap voorwaarts",
                        "blik voorwaarts"
                    ],
                    "target_lang_definitions": {
                        "en": "In a direction pointing forward."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forward",
                                "target_lang_sense_clarification": "direction of movement"
                            },
                            {
                                "target_lang_word": "forwards",
                                "target_lang_sense_clarification": "direction of movement"
                            },
                            {
                                "target_lang_word": "ahead",
                                "target_lang_sense_clarification": "in front"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f1f0b02f-162a-35db-8295-a3a78bf93d5e",
                    "sense_definition": "Gericht naar de voorkant of bewegend in die richting.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>voorwaartse</w> beweging van het schip werd plotseling gestopt.",
                            "target_lang_translations": {
                                "en": "The <w>forward</w> movement of the ship was suddenly stopped."
                            }
                        },
                        {
                            "text": "De turner maakte een perfecte <w>voorwaartse</w> salto.",
                            "target_lang_translations": {
                                "en": "The gymnast performed a perfect <w>forward</w> somersault."
                            }
                        }
                    ],
                    "antonyms": [
                        "achterwaarts",
                        "terugwaarts"
                    ],
                    "common_phrases": [
                        "voorwaartse beweging",
                        "voorwaartse salto",
                        "voorwaartse kracht"
                    ],
                    "target_lang_definitions": {
                        "en": "Directed towards the front or moving in that direction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forward",
                                "target_lang_sense_clarification": "adjective describing movement or position"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "3959cd1a-87de-3aef-abd7-2093fff6a88c",
                    "sense_definition": "Een bevel of aansporing om te vertrekken of door te lopen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De commandant riep luid naar zijn troepen: \"<w>Voorwaarts</w>!\"",
                            "target_lang_translations": {
                                "en": "The commander shouted loudly to his troops: \"<w>Forward</w>!\""
                            }
                        },
                        {
                            "text": "Kom op iedereen, <w>voorwaarts</w>, we hebben geen tijd te verliezen.",
                            "target_lang_translations": {
                                "en": "Come on everyone, <w>onward</w>, we have no time to lose."
                            }
                        }
                    ],
                    "antonyms": [
                        "achterwaarts"
                    ],
                    "common_phrases": [
                        "voorwaarts mars"
                    ],
                    "target_lang_definitions": {
                        "en": "A command or encouragement to depart or keep walking."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forward",
                                "target_lang_sense_clarification": "military command"
                            },
                            {
                                "target_lang_word": "onward",
                                "target_lang_sense_clarification": "encouragement to proceed"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
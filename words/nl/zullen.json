{
    "word_family": [],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f6d2460d-708d-30e4-9829-f12cce2388a9",
                    "sense_definition": "Hulpwerkwoord om aan te geven dat iets in de toekomst gaat gebeuren.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>zal</w> je morgen even bellen.",
                            "target_lang_translations": {
                                "en": "I <w>will</w> give you a call tomorrow.",
                                "ru": "Я тебе завтра <w>позвоню</w>."
                            }
                        },
                        {
                            "text": "Het <w>zal</w> waarschijnlijk de hele dag regenen.",
                            "target_lang_translations": {
                                "en": "It <w>will</w> probably rain all day.",
                                "ru": "Вероятно, весь день <w>будет</w> идти дождь."
                            }
                        },
                        {
                            "text": "Dat <w>zal</w> niet gebeuren.",
                            "target_lang_translations": {
                                "en": "That <w>will</w> not happen.",
                                "ru": "Этого не <w>произойдет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gaan"
                    ],
                    "common_phrases": [
                        "het zal wel",
                        "ik zal proberen",
                        "dat zal wel"
                    ],
                    "target_lang_definitions": {
                        "en": "Auxiliary verb indicating that something will happen in the future.",
                        "ru": "Вспомогательный глагол для указания на то, что действие произойдет в будущем."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "will",
                                "target_lang_sense_clarification": "future tense marker"
                            },
                            {
                                "target_lang_word": "shall",
                                "target_lang_sense_clarification": "future tense (formal or 1st person)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "вспомогательный глагол для образования будущего времени"
                            },
                            {
                                "target_lang_word": "стать",
                                "target_lang_sense_clarification": "вспомогательный глагол, обозначающий начало действия или будущее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6ae0d61-37dc-3055-ac6e-143ee167f6d4",
                    "sense_definition": "Gebruikt om een voorstel te doen, iets aan te bieden of een intentie uit te spreken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zullen</w> we vanavond samen naar de film gaan?",
                            "target_lang_translations": {
                                "en": "<w>Shall</w> we go to the movies together tonight?",
                                "ru": "<w>Давай</w> сходим сегодня вечером в кино вместе?"
                            }
                        },
                        {
                            "text": "<w>Zal</w> ik de deur voor je openhouden?",
                            "target_lang_translations": {
                                "en": "<w>Shall</w> I hold the door open for you?",
                                "ru": "<w>Давай</w> я подержу дверь для тебя?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "zullen we",
                        "zal ik"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to make a suggestion, an offer, or express an intention.",
                        "ru": "Используется для предложения совместного действия, помощи или выражения намерения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shall",
                                "target_lang_sense_clarification": "in questions/suggestions (e.g., Shall we?)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "давай",
                                "target_lang_sense_clarification": "используется для предложений (zullen we...)"
                            },
                            {
                                "target_lang_word": "может",
                                "target_lang_sense_clarification": "вводное слово для предложений или вопросов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1d7e208-9f2e-3b33-bd72-6f26cfaf4922",
                    "sense_definition": "Hulpwerkwoord om een voorwaarde, wens of mogelijkheid uit te drukken (in de vorm 'zou' of 'zouden').",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als ik meer tijd had, <w>zou</w> ik vaker sporten.",
                            "target_lang_translations": {
                                "en": "If I had more time, I <w>would</w> exercise more often.",
                                "ru": "Если бы у меня было больше времени, я <w>бы</w> чаще занимался спортом."
                            }
                        },
                        {
                            "text": "Dat <w>zou</w> ik nooit doen als ik jou was.",
                            "target_lang_translations": {
                                "en": "I <w>would</w> never do that if I were you.",
                                "ru": "Я <w>бы</w> никогда этого не сделал на твоем месте."
                            }
                        },
                        {
                            "text": "Ik <w>zou</w> graag een kopje koffie willen.",
                            "target_lang_translations": {
                                "en": "I <w>would</w> like a cup of coffee.",
                                "ru": "Я <w>бы</w> хотел чашечку кофе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik zou graag",
                        "het zou kunnen",
                        "zou moeten"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verleden tijd van zullen, gebruikt als conditionalis"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Auxiliary verb expressing a condition, wish, or possibility (past form 'would').",
                        "ru": "Вспомогательный глагол для выражения условия, желания или возможности (в форме 'zou' или 'zouden')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "would",
                                "target_lang_sense_clarification": "conditional form"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бы",
                                "target_lang_sense_clarification": "частица для образования условного наклонения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ead96bb6-2d5d-3b81-b440-1b1793b3bbc8",
                    "sense_definition": "Drukt een streng bevel, verplichting of noodzakelijkheid uit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gij <w>zult</w> niet stelen.",
                            "target_lang_translations": {
                                "en": "Thou <w>shalt</w> not steal.",
                                "ru": "Не <w>укради</w>."
                            }
                        },
                        {
                            "text": "Je <w>zult</w> luisteren naar wat ik zeg.",
                            "target_lang_translations": {
                                "en": "You <w>shall</w> listen to what I say.",
                                "ru": "Ты <w>будешь</w> слушать то, что я говорю."
                            }
                        }
                    ],
                    "synonyms": [
                        "moeten"
                    ],
                    "common_phrases": [
                        "gij zult"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "vooral in bijbelse of zeer formele context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Expresses a strict command, obligation, or necessity.",
                        "ru": "Выражает строгий приказ, обязательство или необходимость."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shall",
                                "target_lang_sense_clarification": "formal command or obligation"
                            },
                            {
                                "target_lang_word": "must",
                                "target_lang_sense_clarification": "obligation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "должен",
                                "target_lang_sense_clarification": "выражает обязанность"
                            },
                            {
                                "target_lang_word": "обязан",
                                "target_lang_sense_clarification": "выражает строгую необходимость"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
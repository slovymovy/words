    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a443f088-a900-3208-91b9-9359270cd3f3",
                    "sense_definition": "Stawać się silniejszym, bardziej intensywnym lub gwałtownym (zwykle o zjawiskach atmosferycznych, uczuciach lub dźwiękach); używane z zaimkiem zwrotnym „się”.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensity_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczorem wiatr zaczął się gwałtownie <w>wzmagać</w>, zapowiadając nadejście silnej burzy.",
                            "target_lang_translations": {
                                "ru": "Вечером ветер начал резко <w>усиливаться</w>, предвещая приближение сильной бури."
                            }
                        },
                        {
                            "text": "Z każdym dniem jego tęsknota za rodzinnym domem <w>wzmagała</w> się coraz bardziej.",
                            "target_lang_translations": {
                                "ru": "С каждым днём его тоска по родному дому всё больше <w>нарастала</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nasilać się",
                        "narastać",
                        "potęgować się",
                        "rosnąć"
                    ],
                    "antonyms": [
                        "słabnąć",
                        "ustępować",
                        "cichnąć",
                        "maleć"
                    ],
                    "common_phrases": [
                        "wiatr się wzmaga",
                        "ból się wzmaga",
                        "niepokój się wzmaga",
                        "hałas się wzmaga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться сильнее, интенсивнее или резче (обычно об атмосферных явлениях, чувствах или звуках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усиливаться",
                                "target_lang_sense_clarification": "общий смысл; становиться более сильным"
                            },
                            {
                                "target_lang_word": "нарастать",
                                "target_lang_sense_clarification": "постепенно увеличиваться (о звуке, чувствах, темпе)"
                            },
                            {
                                "target_lang_word": "крепчать",
                                "target_lang_sense_clarification": "становиться сильнее (о ветре или морозе)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed481230-ce2a-34eb-8615-1a23bab911b2",
                    "sense_definition": "Powodować, że coś staje się większe, silniejsze lub bardziej intensywne.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intensity_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zapach pieczonego ciasta <w>wzmagał</w> mój apetyt, mimo że byłem już po obiedzie.",
                            "target_lang_translations": {
                                "ru": "Запах печёного пирога <w>усиливал</w> мой аппетит, хотя я уже пообедал."
                            }
                        },
                        {
                            "text": "Trudna sytuacja gospodarcza <w>wzmaga</w> niezadowolenie społeczne w wielu regionach kraju.",
                            "target_lang_translations": {
                                "ru": "Трудная экономическая ситуация <w>усиливает</w> социальное недовольство во многих регионах страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "nasilać",
                        "potęgować",
                        "zwiększać",
                        "intensyfikować",
                        "podsycać"
                    ],
                    "antonyms": [
                        "osłabiać",
                        "łagodzić",
                        "zmniejszać",
                        "uśmierzać"
                    ],
                    "common_phrases": [
                        "wzmagać czujność",
                        "wzmagać apetyt",
                        "wzmagać wysiłki",
                        "wzmagać presję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо более сильным, интенсивным или значительным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усиливать",
                                "target_lang_sense_clarification": "общий смысл; делать более сильным"
                            },
                            {
                                "target_lang_word": "обострять",
                                "target_lang_sense_clarification": "делать более острым или напряженным (о чувствах, внимании, конфликтах)"
                            },
                            {
                                "target_lang_word": "повышать",
                                "target_lang_sense_clarification": "увеличивать уровень или степень (о голосе, требованиях)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
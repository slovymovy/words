                    "sense_id": "45497a9c-9915-39eb-9f19-6573331f28a3",
                    "sense_definition": "przyjść na świat, rozpocząć życie (używane z zaimkiem zwrotnym \"się\")",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "birth_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W którym roku <w>urodziłeś</w> się ty i twoi rodzice?",
                            "target_lang_translations": {
                                "ru": "В каком году <w>родились</w> ты и твои родители?"
                            }
                        },
                        {
                            "text": "Moja babcia <w>urodziła</w> się w małej wiosce pod Krakowem.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка <w>родилась</w> в маленькой деревне под Краковом."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyjść na świat"
                    ],
                    "antonyms": [
                        "umrzeć",
                        "zginąć"
                    ],
                    "common_phrases": [
                        "urodzić się w Polsce",
                        "urodzić się w maju",
                        "urodzić się zdrowym",
                        "nowo urodzony"
                    ],
                    "target_lang_definitions": {
                        "ru": "появиться на свет, начать жить (используется с возвратным местоимением «się»)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родиться",
                                "target_lang_sense_clarification": "появиться на свет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9faeaa00-8e20-3a1c-aa0f-0bb503f773bc",
                    "sense_definition": "wydać potomstwo na świat (o matce)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "birth_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Anna <w>urodziła</w> wczoraj zdrowego i silnego chłopca.",
                            "target_lang_translations": {
                                "ru": "Анна вчера <w>родила</w> здорового и сильного мальчика."
                            }
                        },
                        {
                            "text": "Lekarz powiedział, że pacjentka wkrótce <w>urodzi</w> bliźniaki.",
                            "target_lang_translations": {
                                "ru": "Врач сказал, что пациентка скоро <w>родит</w> близнецов."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydać na świat"
                    ],
                    "common_phrases": [
                        "urodzić dziecko",
                        "urodzić syna",
                        "urodzić córkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "произвести потомство на свет (о матери)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родить",
                                "target_lang_sense_clarification": "произвести на свет ребенка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "082ae158-847d-39df-8a9d-dad5cef5e1f3",
                    "sense_definition": "wydać plony lub owoce (o ziemi lub roślinach)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tego lata nasza stara jabłoń <w>urodziła</w> mnóstwo owoców.",
                            "target_lang_translations": {
                                "ru": "Этим летом наша старая яблоня <w>родила</w> много фруктов."
                            }
                        },
                        {
                            "text": "Żyzna gleba <w>urodzi</w> zboże nawet w trudniejszych warunkach pogodowych.",
                            "target_lang_translations": {
                                "ru": "Плодородная почва <w>родит</w> зерно даже в более трудных погодных условиях."
                            }
                        }
                    ],
                    "synonyms": [
                        "obrodzić",
                        "wydać"
                    ],
                    "common_phrases": [
                        "ziemia urodziła",
                        "urodzić plony"
                    ],
                    "target_lang_definitions": {

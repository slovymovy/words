                {
                    "sense_id": "23d00f40-a1b6-3a19-acf4-0003b80d558e",
                    "sense_definition": "Ogół Polaków i osób pochodzenia polskiego mieszkających na stałe poza granicami Polski.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "Prezydent spotkał się w Atenach z miejscową <w>Polonią</w>.",

                        "vroeger"
                    ],
                    "common_phrases": [
                        "tot later",
                        "later op de dag",
                        "voor later"
                    ],
                    "target_lang_definitions": {
                        "en": "At a time in the future; after some time has passed.",
                        "ru": "В какой-то момент в будущем; по прошествии времени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "later",

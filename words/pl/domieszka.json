                {
                    "sense_id": "85aea6e8-c803-3cd6-b94e-3b02f103e21b",
                    "sense_definition": "Niewielka ilość substancji stanowiąca dodatek do głównego składnika całości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To paliwo zawiera szkodliwą <w>domieszkę</w> siarki.",
                            "target_lang_translations": {
                                "ru": "Это топливо содержит вредную <w>примесь</w> серы."
                            }
                        },
                        {
                            "text": "Kupiłem wełniany sweter z niewielką <w>domieszką</w> akrylu.",
                            "target_lang_translations": {
                                "ru": "Я купил шерстяной свитер с небольшой <w>добавкой</w> акрила."
                            }
                        }
                    ],
                    "synonyms": [
                        "dodatek",
                        "odrobina",
                        "drobina"
                    ],
                    "common_phrases": [

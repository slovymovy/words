    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7a801a69-c50f-3830-ac6d-a522dca92e94",
                    "sense_definition": "Strongly or sublimely beautiful, enjoyable, or pleasurable.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_pleasure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After a long week of work, the massage felt absolutely <w>heavenly</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой рабочей недели массаж был просто <w>боже́ственным</w>."
                            }
                        },
                        {
                            "text": "This chocolate cake tastes <w>heavenly</w> and is the best I have ever had.",
                            "target_lang_translations": {
                                "ru": "У этого шоколадного торта <w>боже́ственный</w> вкус, и это лучшее, что я когда-либо пробовал."
                            }
                        }
                    ],
                    "synonyms": [
                        "wonderful",
                        "delightful",
                        "blissful",
                        "divine"
                    ],
                    "common_phrases": [
                        "simply heavenly",
                        "heavenly smell",
                        "heavenly taste"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень красивый, приятный или доставляющий наслаждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "боже́ственный",
                                "target_lang_sense_clarification": "прекрасный, великолепный"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d1913d4-f110-3558-9d5c-9b406fa4641b",
                    "sense_definition": "Simple past and past participle of <w>bind</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_bind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hostages were <w>bound</w> with thick ropes.",
                            "target_lang_translations": {
                                "ru": "Заложники были <w>связаны</w> толстыми веревками."
                            }
                        },
                        {
                            "text": "We are <w>bound</w> together by our shared history.",
                            "target_lang_translations": {
                                "ru": "Мы <w>связаны</w> вместе нашей общей историей."
                            }
                        }
                    ],
                    "synonyms": [
                        "tied",
                        "fastened",
                        "secured"
                    ],
                    "antonyms": [
                        "free"
                    ],
                    "common_phrases": [
                        "bound hand and foot",
                        "bound by law"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола bind (связывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свя́занный",
                                "target_lang_sense_clarification": "скрепленный или соединенный чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec64f4f2-c4cd-386e-b90d-980e488d9d8c",
                    "sense_definition": "To run with long, jumping strides or to leap energetically.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_move",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The large dog <w>bounded</w> across the grass to greet us.",
                            "target_lang_translations": {
                                "ru": "Большая собака <w>проскакала</w> по траве, чтобы поприветствовать нас."
                            }
                        },
                        {
                            "text": "She <w>bounded</w> up the stairs to answer the phone.",
                            "target_lang_translations": {
                                "ru": "Она <w>взбежала</w> по лестнице, чтобы ответить на звонок."
                            }
                        }
                    ],
                    "synonyms": [
                        "leap",
                        "spring",
                        "jump",
                        "bounce"
                    ],
                    "common_phrases": [
                        "bound away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бежать большими шагами или энергично прыгать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скака́ть",
                                "target_lang_sense_clarification": "перемещаться прыжками"
                            },
                            {
                                "target_lang_word": "пры́гать",
                                "target_lang_sense_clarification": "совершать прыжки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a31d3eb-6c1c-3710-9abb-603b94ac535c",
                    "sense_definition": "To form the boundary or limit of a specific area.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The property is <w>bounded</w> by a river on the south side.",
                            "target_lang_translations": {
                                "ru": "Владение <w>ограничено</w> рекой с южной стороны."
                            }
                        },
                        {
                            "text": "A high stone wall <w>bounded</w> the ancient garden.",
                            "target_lang_translations": {
                                "ru": "Высокая каменная стена <w>ограничивала</w> старый сад."
                            }
                        }
                    ],
                    "synonyms": [
                        "border",
                        "enclose",
                        "surround",
                        "limit"
                    ],
                    "common_phrases": [
                        "bounded by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служить границей или пределом определенной территории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ограни́чивать",
                                "target_lang_sense_clarification": "являться границей чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1f467e7f-4ef3-357f-8e20-24e3c83fa1ee",
                    "sense_definition": "Certain or extremely likely to happen or be true.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "adj_certain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>bound</w> to rain with all those dark clouds in the sky.",
                            "target_lang_translations": {
                                "ru": "С такими темными тучами на небе <w>непременно</w> пойдет дождь."
                            }
                        },
                        {
                            "text": "They are <w>bound</w> to find out the truth eventually.",
                            "target_lang_translations": {
                                "ru": "В конце концов они <w>непременно</w> узнают правду."
                            }
                        }
                    ],
                    "synonyms": [
                        "certain",
                        "sure",
                        "destined",
                        "inevitable"
                    ],
                    "common_phrases": [
                        "bound to happen",
                        "bound to be"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несомненный или крайне вероятный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непреме́нный",
                                "target_lang_sense_clarification": "обязательный, неизбежный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88c7be90-ac7a-30ea-9cb1-26c9c377383a",
                    "sense_definition": "Moving in a specific direction or ready to go to a destination.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train standing at platform two is <w>bound</w> for Paris.",
                            "target_lang_translations": {
                                "ru": "Поезд, стоящий на второй платформе, <w>направляется</w> в Париж."
                            }
                        },
                        {
                            "text": "We are finally homeward <w>bound</w> after a long trip.",
                            "target_lang_translations": {
                                "ru": "После долгого путешествия мы наконец <w>направляемся</w> домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "headed",
                        "going"
                    ],
                    "common_phrases": [
                        "homeward bound",
                        "outward bound",
                        "north bound"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движущийся в определенном направлении или готовый отправиться к месту назначения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "направля́ющийся",
                                "target_lang_sense_clarification": "движущийся куда-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "881cb6e8-d90b-3d0b-9e09-2454d0a084e0",
                    "sense_definition": "Forced to do something by law, duty, or a promise.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_obliged",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are not legally <w>bound</w> to sign this contract.",
                            "target_lang_translations": {
                                "ru": "Вы юридически не <w>обязаны</w> подписывать этот контракт."
                            }
                        },
                        {
                            "text": "I felt <w>bound</w> to help him after he had done so much for me.",
                            "target_lang_translations": {
                                "ru": "Я чувствовал себя <w>обязанным</w> помочь ему после того, как он так много для меня сделал."
                            }
                        }
                    ],
                    "synonyms": [
                        "obliged",
                        "obligated",
                        "required",
                        "compelled"
                    ],
                    "antonyms": [
                        "free"
                    ],
                    "common_phrases": [
                        "legally bound",
                        "duty bound",
                        "honor bound"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вынужденный сделать что-то по закону, долгу или обещанию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обяза́нный",
                                "target_lang_sense_clarification": "имеющий обязательство сделать что-то"
                            },
                            {
                                "target_lang_word": "до́лжный",
                                "target_lang_sense_clarification": "вынужденный долгом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2ba4cbfa-db83-3be5-9771-04a92cb3827a",
                    "sense_definition": "A limit or border that defines an area or acceptable behavior.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ball went out of <w>bounds</w> and stopped the game.",

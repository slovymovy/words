    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9c2b3ec5-de5a-326a-9980-c8cc03acee88",
                    "sense_definition": "Związany z wodą, dotyczący wody lub zawierający wodę.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest duży zbiornik <w>wodny</w>, który zaopatruje miasto.",
                            "target_lang_translations": {
                                "ru": "Это большой <w>водный</w> резервуар, который снабжает город."
                            }
                        },
                        {
                            "text": "Para <w>wodna</w> unosiła się nad gorącą herbatą.",
                            "target_lang_translations": {
                                "ru": "<w>Водяной</w> пар поднимался над горячим чаем."
                            }
                        }
                    ],
                    "antonyms": [
                        "lądowy",
                        "bezwodny"
                    ],
                    "common_phrases": [
                        "zbiornik wodny",
                        "para wodna",
                        "zasoby wodne",
                        "sporty wodne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с водой, касающийся воды или содержащий воду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водный",
                                "target_lang_sense_clarification": "относящийся к воде (напр. транспорт, спорт, ресурсы)"
                            },
                            {
                                "target_lang_word": "водяной",
                                "target_lang_sense_clarification": "состоящий из воды (напр. пар, капля)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e17ed34-ec1a-3d1e-80e9-d911c26c0109",
                    "sense_definition": "Żyjący w wodzie lub rosnący w środowisku wodnym.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "bio_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ptactwo <w>wodne</w> ma tutaj raj pośród trzcin.",
                            "target_lang_translations": {
                                "ru": "<w>Водные</w> птицы находят здесь рай среди тростника."
                            }
                        },
                        {
                            "text": "Wąż <w>wodny</w> gonił drobną rybkę w stawie.",
                            "target_lang_translations": {
                                "ru": "<w>Водяной</w> уж гнался за мелкой рыбкой в пруду."
                            }
                        }
                    ],
                    "antonyms": [
                        "lądowy"
                    ],
                    "common_phrases": [
                        "ptactwo wodne",
                        "rośliny wodne",
                        "żółw wodny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живущий в воде или растущий в водной среде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водный",
                                "target_lang_sense_clarification": "обитающий в воде (общий термин)"
                            },
                            {
                                "target_lang_word": "водяной",
                                "target_lang_sense_clarification": "в названиях конкретных животных (напр. водяная крыса)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ab319fb-ef6c-39ab-8c9f-1b67c6965649",
                    "sense_definition": "Przeznaczony do poruszania się po wodzie lub transportu na wodzie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Popłynęliśmy na wyspę rowerem <w>wodnym</w>.",
                            "target_lang_translations": {
                                "ru": "Мы поплыли на остров на <w>водном</w> велосипеде."
                            }
                        },
                        {
                            "text": "Tramwaj <w>wodny</w> jest popularną atrakcją turystyczną w lecie.",
                            "target_lang_translations": {
                                "ru": "<w>Водный</w> трамвай — популярный туристический аттракцион летом."
                            }
                        }
                    ],
                    "antonyms": [
                        "lądowy",
                        "powietrzny"
                    ],
                    "common_phrases": [
                        "rower wodny",
                        "tramwaj wodny",
                        "szlak wodny",
                        "transport wodny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для передвижения по воде или транспортировки по воде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водный",
                                "target_lang_sense_clarification": "о транспорте и путях сообщения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae241f28-37e5-3950-b030-d3d46e6f3053",
                    "sense_definition": "Napędzany siłą wody lub działający dzięki wodzie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "powered_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten zegar <w>wodny</w> to cenna pamiątka po moim dziadku.",
                            "target_lang_translations": {
                                "ru": "Эти <w>водяные</w> часы — ценная память о моем дедушке."
                            }
                        },
                        {
                            "text": "Stary młyn <w>wodny</w> wciąż miele zboże na mąkę.",
                            "target_lang_translations": {
                                "ru": "Старая <w>водяная</w> мельница всё ещё мелет зерно на муку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "młyn wodny",
                        "zegar wodny",
                        "turbina wodna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводимый в движение силой воды или работающий благодаря воде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водяной",
                                "target_lang_sense_clarification": "работающий на воде (напр. мельница, часы)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60db2ff4-406d-38e0-beb2-28545b23205f",
                    "sense_definition": "O roztworze: powstały przez rozpuszczenie substancji w wodzie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chem_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do eksperymentu potrzebny jest nasycony roztwór <w>wodny</w> soli.",
                            "target_lang_translations": {
                                "ru": "Для эксперимента нужен насыщенный <w>водный</w> раствор соли."
                            }
                        },
                        {
                            "text": "Farby <w>wodne</w> są łatwe do zmycia z rąk.",
                            "target_lang_translations": {
                                "ru": "<w>Водные</w> краски легко смываются с рук."
                            }
                        }
                    ],
                    "common_phrases": [
                        "roztwór wodny"
                    ],
                    "target_lang_definitions": {
                        "ru": "О растворе: образованный путем растворения вещества в воде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водный",
                                "target_lang_sense_clarification": "о химических растворах и смесях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d794857b-03fe-3a30-ac0f-59133d46a07b",
                    "sense_definition": "Mający rzadką konsystencję, zawierający dużo wody.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "dialect_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta zupa jest dzisiaj wyjątkowo <w>wodna</w> i bez smaku.",
                            "target_lang_translations": {
                                "ru": "Этот суп сегодня исключительно <w>водянистый</w> и безвкусный."
                            }
                        },
                        {
                            "text": "Nie lubię, gdy sos jest zbyt <w>wodny</w>.",
                            "target_lang_translations": {
                                "ru": "Я не люблю, когда соус слишком <w>водянистый</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wodniak"
                    ],
                    "antonyms": [
                        "gęsty"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Bukowina"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий жидкую консистенцию, содержащий много воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водянистый",
                                "target_lang_sense_clarification": "разбавленный, жидкий, безвкусный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "23d0868a-0bb1-3a7f-8ce4-04e159f75c3d",
                    "sense_definition": "Pomocnik szypra na statku rzecznym (zwykle na Wiśle).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody <w>wodny</w> uczył się fachu od doświadczonego kapitana.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>водник</w> учился ремеслу у опытного капитана."
                            }
                        },
                        {
                            "text": "Każdy <w>wodny</w> musiał doskonale znać nurt rzeki.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>водник</w> должен был отлично знать течение реки."
                            }
                        }
                    ],
                    "synonyms": [
                        "wodniak"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "związane z żeglugą na Wiśle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помощник шкипера на речном судне (обычно на Висле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водник",
                                "target_lang_sense_clarification": "работник водного транспорта (приближенный эквивалент)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
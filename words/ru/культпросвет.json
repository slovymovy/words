                        },
                        {
                            "trait_type": "archaic",
                            "comment": "советский термин"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short for \"cultural-educational department\"; a Soviet institution or unit responsible for culture and education."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cultural education department",

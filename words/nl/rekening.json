                {
                    "sense_id": "3bacc7f2-66fc-3b62-a524-9159e55b9dca",
                    "sense_definition": "Een schriftelijk overzicht van de kosten die betaald moeten worden voor geleverde goederen of diensten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "payment_request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ober, mag ik de <w>rekening</w> van u?",
                            "target_lang_translations": {
                                "en": "Waiter, may I have the <w>bill</w>, please?",
                                "ru": "Официант, можно мне <w>счёт</w>?"
                            }
                        },
                        {
                            "text": "Hij betaalde de <w>rekening</w> voor het diner direct met zijn pinpas.",
                            "target_lang_translations": {
                                "en": "He paid the <w>bill</w> for the dinner immediately with his debit card.",
                                "ru": "Он сразу оплатил <w>счёт</w> за ужин своей дебетовой картой."
                            }
                        }
                    ],
                    "synonyms": [
                        "factuur",
                        "nota",
                        "afrekening"
                    ],
                    "common_phrases": [
                        "de rekening vragen",
                        "de rekening betalen",
                        "de rekening delen"
                    ],
                    "target_lang_definitions": {
                        "en": "A written statement of the costs that must be paid for goods or services provided.",
                        "ru": "Письменный отчет о расходах, которые необходимо оплатить за предоставленные товары или услуги."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8ee3231a-09a8-397c-8dc5-860dbbea0179",
                    "sense_definition": "Расписание, план выполнения последовательности действий с указанием точных сроков.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автобус прибыл на остановку точно по <w>графику</w>.",
                            "target_lang_translations": {
                                "en": "The bus arrived at the stop exactly on <w>schedule</w>."
                            }
                        },
                        {
                            "text": "У нашего директора очень плотный рабочий <w>график</w>.",
                            "target_lang_translations": {
                                "en": "Our director has a very tight work <w>schedule</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "план",
                        "программа",
                        "план"
                    ],
                    "common_phrases": [
                        "по графику",
                        "рабочий график",
                        "выбиться из графика",
                        "плотный график"
                    ],
                    "target_lang_definitions": {
                        "en": "A plan for carrying out a process or procedure, giving lists of intended events and times."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "schedule",
                                "target_lang_sense_clarification": "plan of events with times"
                            },
                            {
                                "target_lang_word": "timetable",
                                "target_lang_sense_clarification": "transport or class schedule"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aab413cd-7f0f-3c2c-82e3-6e1490e26380",
                    "sense_definition": "Чертеж, изображающий с помощью линий или кривых зависимость одной величины от другой.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ученик аккуратно начертил <w>график</w> функции в тетради.",
                            "target_lang_translations": {
                                "en": "The student neatly drew a <w>graph</w> of the function in the notebook."
                            }
                        },
                        {
                            "text": "<w>График</w> продаж показывает значительный рост прибыли в этом месяце.",
                            "target_lang_translations": {
                                "en": "The sales <w>chart</w> shows significant profit growth this month."
                            }
                        }
                    ],
                    "synonyms": [
                        "диаграмма"
                    ],
                    "common_phrases": [
                        "график функции",
                        "построить график",
                        "график температуры"
                    ],
                    "target_lang_definitions": {
                        "en": "A diagram showing the relation between variable quantities, typically of two variables, each measured along one of a pair of axes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "graph",
                                "target_lang_sense_clarification": "diagram of relation between quantities"
                            },
                            {

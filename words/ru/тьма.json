                            },
                            {
                                "target_lang_word": "ignorance",
                                "target_lang_sense_clarification": "lack of knowledge"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a3eb574-e3b6-390e-93c5-7dfab75496ba",

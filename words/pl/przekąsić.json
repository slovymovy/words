            "senses": [
                {
                    "sense_id": "7c959ff4-2264-3851-91a1-5723abca12b3",
                    "sense_definition": "Zjeść niewielką ilość jedzenia, zazwyczaj szybko lub między głównymi posiłkami, aby zaspokoić pierwszy głód.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "eating_small_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę coś <w>przekąsić</w> przed wyjściem do pracy, bo jestem głodny.",
                            "target_lang_translations": {
                                "ru": "Мне нужно что-нибудь <w>перекусить</w> перед выходом на работу, потому что я голоден."
                            }
                        },
                        {
                            "text": "Zatrzymaliśmy się na stacji benzynowej, żeby <w>przekąsić</w> kanapkę i wypić kawę.",
                            "target_lang_translations": {
                                "ru": "Мы остановились на заправке, чтобы <w>перекусить</w> бутербродом и выпить кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "podjeść",
                        "przegryźć",
                        "zjeść"
                    ],
                    "common_phrases": [
                        "przekąsić coś na szybko",
                        "przekąsić małe co nieco",
                        "wpaść coś przekąsić"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Съесть небольшое количество еды, обычно быстро или между основными приемами пищи, чтобы утолить легкий голод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перекусить",
                                "target_lang_sense_clarification": "съесть немного еды"
                            },
                            {
                                "target_lang_word": "подкрепиться",
                                "target_lang_sense_clarification": "восстановить силы едой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "136d5825-e053-3d1c-b603-7e013e46c03b",
                    "sense_definition": "Zjeść coś bezpośrednio po wypiciu mocnego alkoholu, aby złagodzić jego ostry smak.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "eating_small_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wujek wypił kieliszek wódki i od razu <w>przekąsił</w> go kiszonym ogórkiem.",
                            "target_lang_translations": {
                                "ru": "Дядя выпил рюмку водки и сразу <w>закусил</w> её соленым огурцом."
                            }
                        },
                        {
                            "text": "Na stole stały śledzie, którymi można było <w>przekąsić</w> po toaście.",
                            "target_lang_translations": {
                                "ru": "На столе стояла селедка, которой можно было <w>закусить</w> после тоста."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakąsić",
                        "przegryźć"
                    ],
                    "common_phrases": [
                        "przekąsić ogórkiem",
                        "przekąsić po kielichu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Съесть что-либо сразу после употребления крепкого алкоголя, чтобы смягчить его резкий вкус."
                    },
                    "translations": {
                        "ru": [
                            {

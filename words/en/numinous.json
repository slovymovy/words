            "senses": [
                {
                    "sense_id": "f59add5d-218c-37d9-a15c-85d4b0263828",
                    "sense_definition": "Evoking a strong sense of the mystical, holy, or spiritual; awe-inspiring.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The choir's performance had a <w>numinous</w> quality that moved the entire audience to tears.",
                            "target_lang_translations": {
                                "ru": "Выступление хора обладало <w>мистическим</w> свойством, которое тронуло всю аудиторию до слёз."
                            }
                        },
                        {
                            "text": "He described the sunset over the mountains as a <w>numinous</w> experience that changed his perspective on life.",
                            "target_lang_translations": {
                                "ru": "Он описал закат над горами как <w>нуминозный</w> опыт, который изменил его взгляд на жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "spiritual",
                        "mystical",
                        "transcendent",
                        "awe-inspiring",
                        "sublime"
                    ],
                    "antonyms": [
                        "mundane",
                        "profane"
                    ],
                    "common_phrases": [
                        "numinous experience",
                        "numinous quality",
                        "numinous beauty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий сильное чувство мистического, святого или духовного; внушающий благоговение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нуминозный",
                                "target_lang_sense_clarification": "философский/религиозный термин"
                            },
                            {
                                "target_lang_word": "мистический",
                                "target_lang_sense_clarification": "таинственный, духовный"
                            },
                            {
                                "target_lang_word": "внушающий благоговение",
                                "target_lang_sense_clarification": "вызывающий трепет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae54ebe2-25bf-3495-8b8c-d06feb93ac7c",
                    "sense_definition": "Relating to a deity or spirit; indicating the presence of a divinity.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient temple was considered a <w>numinous</w> place where gods communicated directly with humans.",
                            "target_lang_translations": {
                                "ru": "Древний храм считался <w>священным</w> местом, где боги общались напрямую с людьми."
                            }
                        },
                        {
                            "text": "She felt a <w>numinous</w> presence in the silent forest, as if ancient spirits were watching her.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала <w>божественное</w> присутствие в тихом лесу, словно древние духи наблюдали за ней."
                            }
                        }
                    ],
                    "synonyms": [
                        "divine",
                        "holy",
                        "sacred"
                    ],
                    "antonyms": [
                        "secular"
                    ],
                    "common_phrases": [
                        "numinous power",
                        "numinous presence",
                        "numinous object"
                    ],
                    "target_lang_definitions": {

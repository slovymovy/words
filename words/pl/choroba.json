                    ],
                    "common_phrases": [
                        "choroba zakaźna",
                        "choroba przewlekła",
                        "objawy choroby",
                        "ciężka choroba",
                        "choroba cywilizacyjna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние организма, при котором нарушается нормальное функционирование органов или систем; противоположность здоровью."
                    },
                    "translations": {

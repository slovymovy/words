                        "zalesiony",
                        "lesisty"
                    ],
                    "common_phrases": [
                        "krajobraz bezdrzewny",
                        "teren bezdrzewny",
                        "strefa bezdrzewna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, на котором не растут деревья; лишенный древесной растительности."
                    },
                    "translations": {

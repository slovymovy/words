    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "53776325-a47b-34f6-aa4b-5fdb6a1b0f0e",
                    "sense_definition": "Przezroczysta, delikatna tkanina upinana na głowie, stanowiąca tradycyjny element stroju panny młodej lub zakonnic.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Panna młoda miała na głowie długi, koronkowy <w>welon</w>.",
                            "target_lang_translations": {
                                "ru": "На голове у невесты была длинная кружевная <w>фата</w>."
                            }
                        },
                        {
                            "text": "Wiatr delikatnie unosił jej biały <w>welon</w> podczas ceremonii ślubnej.",
                            "target_lang_translations": {
                                "ru": "Ветер нежно приподнимал её белую <w>фату</w> во время свадебной церемонии."
                            }
                        }
                    ],
                    "synonyms": [
                        "woal"
                    ],
                    "common_phrases": [
                        "welon ślubny",
                        "biały welon",
                        "długi welon",
                        "podnieść welon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прозрачная, тонкая ткань, прикрепляемая к голове, являющаяся традиционным элементом наряда невесты или монахини."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фата",
                                "target_lang_sense_clarification": "свадебный головной убор"
                            },
                            {
                                "target_lang_word": "вуаль",
                                "target_lang_sense_clarification": "тонкая сетка, закрывающая лицо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f9fbdea-6d55-3c20-89fd-4343016cfefd",
                    "sense_definition": "Odmiana hodowlana złotej rybki, charakteryzująca się wydłużonymi, zwisającymi płetwami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W moim akwarium pływa duży, czarny <w>welon</w>.",
                            "target_lang_translations": {
                                "ru": "В моем аквариуме плавает большой черный <w>вуалехвост</w>."
                            }
                        },
                        {
                            "text": "Ten <w>welon</w> ma wyjątkowo piękne i długie płetwy ogonowe.",
                            "target_lang_translations": {
                                "ru": "У этого <w>вуалехвоста</w> исключительно красивые и длинные хвостовые плавники."
                            }
                        }
                    ],
                    "synonyms": [
                        "welonka"
                    ],
                    "common_phrases": [
                        "złoty welon",
                        "hodowla welonów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность золотой рыбки, характеризующаяся удлиненными, свисающими плавниками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вуалехвост",
                                "target_lang_sense_clarification": "разновидность аквариумной рыбки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5775466e-8828-34e2-b22a-0b2d8dbf4f75",
                    "sense_definition": "Warstwa mgły, dymu lub chmur, która coś otacza, przesłania lub unosi się nad czymś.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gęsty <w>welon</w> mgły spowił całą dolinę o poranku.",
                            "target_lang_translations": {
                                "ru": "Густая <w>пелена</w> тумана окутала всю долину на рассвете."
                            }
                        },
                        {
                            "text": "Nad miastem unosił się szary <w>welon</w> smogu i spalin.",
                            "target_lang_translations": {
                                "ru": "Над городом висела серая <w>завеса</w> смога и выхлопных газов."
                            }
                        }
                    ],
                    "synonyms": [
                        "całun",
                        "zasłona"
                    ],
                    "common_phrases": [
                        "welon mgły",
                        "welon dymu",
                        "welon chmur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой тумана, дыма или облаков, который что-то окружает, заслоняет или парит над чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пелена",
                                "target_lang_sense_clarification": "сплошной покров (тумана, слез)"
                            },
                            {
                                "target_lang_word": "завеса",
                                "target_lang_sense_clarification": "преграда из дыма или тумана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c723889-a78f-336e-9d90-69b79f786f59",
                    "sense_definition": "Ozdobna tkanina liturgiczna używana do przykrywania naczyń świętych lub jako szata naramienna kapłana.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ksiądz nałożył na ramiona ozdobny <w>welon</w> przed błogosławieństwem.",
                            "target_lang_translations": {
                                "ru": "Священник надел на плечи украшенный <w>велум</w> перед благословением."
                            }
                        },
                        {
                            "text": "Ministrant przyniósł <w>welon</w> do przykrycia kielicha mszalnego.",
                            "target_lang_translations": {
                                "ru": "Министрант принес <w>велум</w>, чтобы накрыть чашу для мессы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "welon kielichowy",
                        "welon naramienny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративная литургическая ткань, используемая для покрытия священных сосудов или как наплечная накидка священника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велум",
                                "target_lang_sense_clarification": "литургическое облачение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
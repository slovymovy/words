    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c3ec0b23-0173-3710-90ea-17f729f2fed9",
                    "sense_definition": "Перестать издавать шум, звуки.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летний ливень быстро <отшумел>, и снова засияло солнце.",
                            "target_lang_translations": {
                                "en": "The summer downpour quickly <w>died down</w>, and the sun shone again."
                            }
                        },
                        {
                            "text": "Когда ветер <отшумел> в кронах деревьев, в лесу наступила тишина.",
                            "target_lang_translations": {
                                "en": "When the wind <w>ceased</w> in the treetops, silence fell over the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "затихнуть"
                    ],
                    "common_phrases": [
                        "дождь отшумел",
                        "ветер отшумел",
                        "лес отшумел"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop making noise or sounds."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "die down",
                                "target_lang_sense_clarification": "regarding wind, rain, or storms"
                            },
                            {
                                "target_lang_word": "subside",
                                "target_lang_sense_clarification": "regarding noise or commotion"
                            },
                            {
                                "target_lang_word": "cease",
                                "target_lang_sense_clarification": "formal, regarding sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56772e18-a0c3-3cb0-8eac-6665a5116c86",
                    "sense_definition": "Пройти, закончиться (о чём-либо бурном, активном или насыщенном событиями).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его бурная молодость давно <отшумела>, оставив лишь воспоминания.",
                            "target_lang_translations": {
                                "en": "His turbulent youth had long since <w>passed</w>, leaving only memories."
                            }
                        },
                        {
                            "text": "В городе <отшумели> праздничные гулянья, и улицы опустели.",
                            "target_lang_translations": {
                                "en": "The festive celebrations in the city <w>were over</w>, and the streets emptied."
                            }
                        }
                    ],
                    "synonyms": [
                        "отгреметь"
                    ],
                    "common_phrases": [
                        "молодость отшумела",
                        "праздник отшумел",
                        "слава отшумела"
                    ],
                    "target_lang_definitions": {
                        "en": "To pass or come to an end (referring to something turbulent, active, or eventful)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "regarding time periods like youth or glory"
                            },
                            {
                                "target_lang_word": "be over",
                                "target_lang_sense_clarification": "regarding events or celebrations"
                            },
                            {
                                "target_lang_word": "fade away",
                                "target_lang_sense_clarification": "regarding memories or past times"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
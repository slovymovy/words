            "pos": "verb",
            "senses": [
                {
                    "sense_id": "08ae1e4f-2467-3649-91eb-cb6ca749b957",
                    "sense_definition": "Przekazywać coś komuś (np. list, paczkę, wiadomość) za pośrednictwem poczty, internetu, kuriera lub innej osoby.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transfer_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie <w>wysyłam</w> wiele e-maili do klientów w pracy.",
                            "target_lang_translations": {
                                "ru": "Каждый день я <w>отправляю</w> много электронных писем клиентам по работе."
                            }
                        },
                        {
                            "text": "Moja babcia zawsze <w>wysyła</w> mi kartkę na urodziny.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка всегда <w>посылает</w> мне открытку на день рождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "słać",
                        "posyłać",
                        "nadawać"
                    ],
                    "antonyms": [
                        "odbierać"
                    ],
                    "common_phrases": [
                        "wysyłać list",
                        "wysyłać wiadomość",
                        "wysyłać paczkę",
                        "wysyłać e-mail",
                        "wysyłać pieniądze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправлять или передавать что-либо (письмо, посылку, сообщение) по почте, через интернет или курьером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправлять",
                                "target_lang_sense_clarification": "наиболее универсальное слово для писем, сообщений и грузов"
                            },
                            {
                                "target_lang_word": "посылать",
                                "target_lang_sense_clarification": "общеупотребительный синоним, часто используется в разговорной речи"
                            },
                            {
                                "target_lang_word": "высылать",
                                "target_lang_sense_clarification": "отправлять что-либо (часто документы или товары) по запросу или назначению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8724e16-df12-3921-85ad-79b2482cb410",
                    "sense_definition": "Polecać komuś udanie się w określone miejsce w jakimś celu.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "transfer_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama często <w>wysyła</w> mnie do sklepu po świeże pieczywo.",
                            "target_lang_translations": {
                                "ru": "Мама часто <w>посылает</w> меня в магазин за свежим хлебом."
                            }
                        },
                        {
                            "text": "Firma <w>wysyła</w> swoich pracowników na zagraniczne szkolenia.",
                            "target_lang_translations": {
                                "ru": "Компания <w>отправляет</w> своих сотрудников на зарубежные тренинги."
                            }
                        }
                    ],
                    "synonyms": [
                        "posyłać"
                    ],
                    "common_phrases": [
                        "wysyłać dzieci do szkoły",
                        "wysyłać kogoś po zakupy",
                        "wysyłać na delegację",
                        "wysyłać na śmierć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять кого-либо в определенное место с поручением или целью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посылать",
                                "target_lang_sense_clarification": "отправлять кого-то с поручением (например, в магазин)"
                            },
                            {
                                "target_lang_word": "отправлять",
                                "target_lang_sense_clarification": "направлять куда-либо (в командировку, на учебу)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efdf75b0-f4ee-38a7-9d16-7dfda7fde1b7",
                    "sense_definition": "Emitować fale, sygnały, światło lub dźwięki w przestrzeń.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emit_signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta antena <w>wysyła</w> bardzo silny sygnał radiowy.",
                            "target_lang_translations": {
                                "ru": "Эта антенна <w>посылает</w> очень сильный радиосигнал."
                            }
                        },
                        {
                            "text": "Słońce <w>wysyła</w> promienie, które ogrzewają naszą planetę.",
                            "target_lang_translations": {
                                "ru": "Солнце <w>посылает</w> лучи, которые согревают нашу планету."
                            }
                        }
                    ],
                    "synonyms": [
                        "nadawać"
                    ],
                    "antonyms": [
                        "odbierać"
                    ],
                    "common_phrases": [
                        "wysyłać sygnał",
                        "wysyłać fale",
                        "wysyłać impulsy",
                        "wysyłać światło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испускать или излучать волны, сигналы, свет или звуки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посылать",
                                "target_lang_sense_clarification": "направлять сигналы, импульсы или лучи"
                            },
                            {
                                "target_lang_word": "излучать",
                                "target_lang_sense_clarification": "испускать свет, тепло или волны"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "транслировать сигнал"
                            }
                        ]
                    }
                }
            ]
        }
    ]

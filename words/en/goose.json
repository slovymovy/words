                        {
                            "text": "They tried to <w>goose</w> the ratings with a celebrity guest.",
                            "target_lang_translations": {
                                "ru": "Они пытались <w>поднять</w> рейтинги с помощью приглашенной звезды."
                            }
                        }
                    ],
                    "common_phrases": [

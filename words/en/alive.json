    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8e6e052b-8927-398d-aff2-902f67544093",
                    "sense_definition": "Having life; living; not dead.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "life_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctors worked hard to keep the patient <w>alive</w>.",
                            "target_lang_translations": {
                                "ru": "Врачи упорно трудились, чтобы оставить пациента <w>в живых</w>."
                            }
                        },
                        {
                            "text": "Is the spider in the jar dead or <w>alive</w>?",
                            "target_lang_translations": {
                                "ru": "Паук в банке мёртв или <w>жив</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "living",
                        "animate",
                        "alive and kicking",
                        "alive and well"
                    ],
                    "antonyms": [
                        "dead"
                    ],
                    "common_phrases": [
                        "stay alive",
                        "come alive",
                        "bury alive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий жизнью; живущий; не мёртвый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живо́й",
                                "target_lang_sense_clarification": "о человеке, животном или растении"
                            },
                            {
                                "target_lang_word": "в живы́х",
                                "target_lang_sense_clarification": "не умерший, уцелевший"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "457ab050-2f81-3fae-aa6b-9c613d745120",
                    "sense_definition": "Used after a superlative or 'all' to emphasize that someone or something is the most extreme example among all living things.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is the luckiest man <w>alive</w> to have survived the crash.",
                            "target_lang_translations": {
                                "ru": "Он самый удачливый человек <w>на свете</w>, раз выжил в этой аварии."
                            }
                        },
                        {
                            "text": "She is considered the greatest writer <w>alive</w> today.",
                            "target_lang_translations": {
                                "ru": "Её считают величайшим <w>ныне живущим</w> писателем."
                            }
                        }
                    ],
                    "synonyms": [
                        "in the world",
                        "existing"
                    ],
                    "common_phrases": [
                        "man alive",
                        "luckiest man alive"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "postpositive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется после превосходной степени для усиления, означая «среди всех живущих»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на свете",
                                "target_lang_sense_clarification": "усиление превосходной степени"
                            },
                            {
                                "target_lang_word": "ныне живущий",
                                "target_lang_sense_clarification": "существующий в настоящее время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f4fc800-76b3-3b32-a321-1e41c2ce5a11",
                    "sense_definition": "Continuing to exist, function, or be in operation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "active_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We try to keep the old traditions <w>alive</w> for the next generation.",
                            "target_lang_translations": {
                                "ru": "Мы стараемся сохранить старые традиции <w>живыми</w> для следующего поколения."
                            }
                        },
                        {
                            "text": "The team kept their hopes <w>alive</w> with a late goal.",
                            "target_lang_translations": {
                                "ru": "Команда сохранила свои надежды <w>живыми</w> благодаря позднему голу."
                            }
                        }
                    ],
                    "synonyms": [
                        "active",
                        "existing",
                        "surviving"
                    ],
                    "common_phrases": [
                        "keep alive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжающий существовать, действовать или быть в силе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живо́й",
                                "target_lang_sense_clarification": "актуальный, действующий"
                            },
                            {
                                "target_lang_word": "существующий",
                                "target_lang_sense_clarification": "продолжающий быть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ba000fc-589f-3fe8-9b46-cf505d1da10f",
                    "sense_definition": "Full of energy, excitement, or activity; swarming with people or things.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "energetic_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The market square was <w>alive</w> with tourists and locals.",
                            "target_lang_translations": {
                                "ru": "Рыночная площадь была <w>оживлена</w> туристами и местными жителями."
                            }
                        },
                        {
                            "text": "The city comes <w>alive</w> at night with music and lights.",
                            "target_lang_translations": {
                                "ru": "Город <w>оживает</w> ночью, наполняясь музыкой и огнями."
                            }
                        }
                    ],
                    "synonyms": [
                        "active",
                        "lively",
                        "animated",
                        "zestful"
                    ],
                    "common_phrases": [
                        "alive with",
                        "come alive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный энергии, движения или активности; оживлённый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оживлённый",
                                "target_lang_sense_clarification": "наполненный движением"
                            },
                            {
                                "target_lang_word": "полный жизни",
                                "target_lang_sense_clarification": "энергичный, активный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0eee9b3d-0bbb-33b1-8f61-68f81ba84b5e",
                    "sense_definition": "Feeling full of energy, happiness, and vitality.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "energetic_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fresh mountain air made him feel truly <w>alive</w>.",
                            "target_lang_translations": {
                                "ru": "Свежий горный воздух заставил его почувствовать себя по-настоящему <w>живым</w>."
                            }
                        },
                        {
                            "text": "She finally felt <w>alive</w> after quitting her boring job.",
                            "target_lang_translations": {
                                "ru": "Она наконец почувствовала себя <w>живой</w> после увольнения со скучной работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "lively",
                        "vital",
                        "peppy",
                        "frisky"
                    ],
                    "common_phrases": [
                        "feel alive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий полноту жизни, энергию и радость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живо́й",
                                "target_lang_sense_clarification": "полный сил и энергии"
                            },
                            {
                                "target_lang_word": "оживший",
                                "target_lang_sense_clarification": "воспрянувший духом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c332de17-0a20-3788-8ef8-d80739daf168",
                    "sense_definition": "Fully aware of or sensitive to a fact, problem, or situation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police are <w>alive</w> to the danger of the situation.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>осознаёт</w> опасность ситуации."
                            }
                        },
                        {
                            "text": "She was fully <w>alive</w> to the fact that she might fail.",
                            "target_lang_translations": {
                                "ru": "Она полностью <w>осознавала</w> тот факт, что может потерпеть неудачу."
                            }
                        }
                    ],
                    "synonyms": [
                        "active"
                    ],
                    "common_phrases": [
                        "alive to"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "predicative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ясно осознающий или понимающий важность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осознающий",
                                "target_lang_sense_clarification": "понимающий ситуацию"
                            },
                            {
                                "target_lang_word": "осведомлённый",
                                "target_lang_sense_clarification": "знающий о факте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "021363c4-fdf2-380b-9301-627c71107837",
                    "sense_definition": "Carrying an electric current; connected to a source of power.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do not touch that wire because it is <w>alive</w>.",
                            "target_lang_translations": {
                                "ru": "Не трогай этот провод, потому что он <w>под напряжением</w>."
                            }
                        },
                        {
                            "text": "The technician checked if the circuit was still <w>alive</w>.",
                            "target_lang_translations": {
                                "ru": "Техник проверил, находится ли цепь всё ещё <w>под напряжением</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "live",
                        "energized",
                        "hot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся под электрическим напряжением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "под напряжением",
                                "target_lang_sense_clarification": "об электрической цепи"

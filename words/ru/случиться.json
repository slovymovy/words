{
    "word_family": [
        "случай",
        "случайный",
        "случайно",
        "случаться",
        "случайность"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "90a4a435-1e88-326e-a7fc-366b6fbb02c7",
                    "sense_definition": "Произойти, совершиться (о событии, происшествии или случае).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вчера с нами <случилось> небольшое приключение.",
                            "target_lang_translations": {
                                "en": "A small adventure <happened> to us yesterday."
                            }
                        },
                        {
                            "text": "Расскажи мне, что <случилось> в школе.",
                            "target_lang_translations": {
                                "en": "Tell me what <happened> at school."
                            }
                        }
                    ],
                    "synonyms": [
                        "произойти",
                        "совершиться",
                        "проистечь"
                    ],
                    "common_phrases": [
                        "что случилось",
                        "ничего не случилось",
                        "как это случилось"
                    ],
                    "target_lang_definitions": {
                        "en": "To take place or occur (about an event, incident, or case)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "happen",
                                "target_lang_sense_clarification": "general occurrence"
                            },
                            {
                                "target_lang_word": "occur",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "come about",
                                "target_lang_sense_clarification": "to happen or develop"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "205ee415-ce74-3a10-a6d5-3f05a6533761",
                    "sense_definition": "Выпасть на чью-либо долю, представиться (о возможности или случае).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне <случилось> побывать в этом городе много лет назад.",
                            "target_lang_translations": {
                                "en": "I <happened> to visit this city many years ago."
                            }
                        },
                        {
                            "text": "Ему <случилось> встретить старого друга на вокзале.",
                            "target_lang_translations": {
                                "en": "He <happened> to meet an old friend at the station."
                            }
                        }
                    ],
                    "synonyms": [
                        "довестись",
                        "довестись"
                    ],
                    "common_phrases": [
                        "мне случилось",
                        "ему случилось"
                    ],
                    "target_lang_definitions": {
                        "en": "To have the chance or opportunity to do something; to fall to someone's lot."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "happen",
                                "target_lang_sense_clarification": "used with infinitive (e.g., happened to meet)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41a3d072-53f4-3f92-8a81-0afad72746e2",
                    "sense_definition": "Оказаться в наличии, присутствовать где-либо (обычно случайно).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К сожалению, у меня с собой не <случилось> денег.",
                            "target_lang_translations": {
                                "en": "Unfortunately, I didn't <happen> to have any money with me."
                            }
                        },
                        {
                            "text": "Его не <случилось> дома в нужный момент.",
                            "target_lang_translations": {
                                "en": "He didn't <happen> to be home at the right moment."
                            }
                        }
                    ],
                    "synonyms": [
                        "довестись",
                        "совершиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To be present, available, or on hand (usually by chance)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "happen",
                                "target_lang_sense_clarification": "context of availability (e.g., happen to have)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b1908de-8001-3846-841e-43287b596b04",
                    "sense_definition": "Спариться, совершить половой акт для получения потомства (о животных).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Собаки <случились> во дворе без присмотра хозяев.",
                            "target_lang_translations": {
                                "en": "The dogs <mated> in the yard without the owners' supervision."
                            }
                        },
                        {
                            "text": "Для разведения породы животные успешно <случились>.",
                            "target_lang_translations": {
                                "en": "For breeding purposes, the animals successfully <mated>."
                            }
                        }
                    ],
                    "synonyms": [
                        "совокупиться",
                        "сойтись"
                    ],
                    "target_lang_definitions": {
                        "en": "To mate or perform the sexual act for reproduction (of animals)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mate",
                                "target_lang_sense_clarification": "animals"
                            },
                            {
                                "target_lang_word": "couple",
                                "target_lang_sense_clarification": "formal/biological"
                            },
                            {
                                "target_lang_word": "pair",
                                "target_lang_sense_clarification": "to come together for breeding"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
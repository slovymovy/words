            "senses": [
                {
                    "sense_id": "06f29b2b-c8ef-3d8b-ac29-e5e19baa1996",
                    "sense_definition": "De officiële toestemming om iets te doen of ergens binnen te gaan.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "admission_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fotograaf kreeg <w>toelating</w> om backstage foto's te maken.",
                            "target_lang_translations": {
                                "en": "The photographer received <w>permission</w> to take photos backstage.",
                                "ru": "Фотограф получил <w>разрешение</w> фотографировать за кулисами."
                            }
                        },
                        {
                            "text": "Zonder schriftelijke <w>toelating</w> van de eigenaar mag je het terrein niet op.",
                            "target_lang_translations": {
                                "en": "Without written <w>permission</w> from the owner, you may not enter the property.",
                                "ru": "Без письменного <w>разрешения</w> владельца вам нельзя заходить на территорию."
                            }
                        }
                    ],
                    "synonyms": [
                        "permissie"
                    ],
                    "common_phrases": [
                        "toelating vragen",
                        "toelating geven",
                        "schriftelijke toelating"
                    ],
                    "target_lang_definitions": {
                        "en": "The official permission to do something or enter somewhere.",
                        "ru": "Официальное разрешение сделать что-либо или войти куда-либо."
                    },
                    "translations": {

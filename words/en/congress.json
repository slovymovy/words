        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "15b16f58-3117-3f7d-a1a9-575e792d0f47",
                    "sense_definition": "A formal gathering or assembly; a conference held to discuss or decide on a specific question.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Delegates from thirty countries attended the international <w>congress</w> on climate change.",
                            "target_lang_translations": {
                                "ru": "Делегаты из тридцати стран посетили международный <w>конгресс</w> по изменению климата."
                            }
                        },
                        {
                            "text": "The medical <w>congress</w> is held annually to present new research findings.",
                            "target_lang_translations": {
                                "ru": "Медицинский <w>конгресс</w> проводится ежегодно для представления новых результатов исследований."
                            }
                        }
                    ],
                    "synonyms": [
                        "assembly"
                    ],
                    "common_phrases": [
                        "annual congress",
                        "international congress",
                        "party congress",
                        "hold a congress"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное собрание или съезд; конференция, проводимая для обсуждения или решения конкретного вопроса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конгресс",
                                "target_lang_sense_clarification": "крупное совещание"
                            },
                            {
                                "target_lang_word": "съезд",
                                "target_lang_sense_clarification": "собрание представителей"
                            },
                            {
                                "target_lang_word": "конференция",
                                "target_lang_sense_clarification": "научное или деловое собрание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bce22a0f-4e2c-3520-8bf4-d1c717c65175",
                    "sense_definition": "A legislative body of a state, originally the bicameral legislature of the United States of America.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legislature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new law was debated intensely in the national <w>congress</w> before being passed.",
                            "target_lang_translations": {
                                "ru": "Новый закон бурно обсуждался в национальном <w>конгрессе</w> перед принятием."
                            }
                        },
                        {
                            "text": "She decided to run for a seat in the <w>congress</w> to represent her local district.",
                            "target_lang_translations": {
                                "ru": "Она решила баллотироваться в <w>конгресс</w>, чтобы представлять свой местный округ."
                            }
                        }
                    ],
                    "synonyms": [
                        "legislature",
                        "parliament"
                    ],
                    "common_phrases": [
                        "national congress"
                    ],
                    "target_lang_definitions": {
                        "ru": "Законодательный орган государства, первоначально двухпалатный законодательный орган США."
                    },

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4141cb6c-2c6b-3e63-81cd-a262f13cb31c",
                    "sense_definition": "Прекращение жизнедеятельности организма, конец жизни.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cessation_of_life",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Внезапная <смерть> известного писателя стала большой потерей для литературы.",
                            "target_lang_translations": {
                                "en": "The sudden <death> of the famous writer became a great loss for literature."
                            }
                        },
                        {
                            "text": "Врачи делали всё возможное, но <смерть> наступила мгновенно.",
                            "target_lang_translations": {
                                "en": "Doctors did everything possible, but <death> occurred instantly."
                            }
                        }
                    ],
                    "synonyms": [
                        "кончина",
                        "уход из жизни",
                        "конец",
                        "агония"
                    ],
                    "antonyms": [
                        "жизнь",
                        "рождение",
                        "воскрешение"
                    ],
                    "common_phrases": [
                        "вопрос жизни и смерти",
                        "на волосок от смерти",
                        "верная смерть",
                        "клиническая смерть"
                    ],
                    "target_lang_definitions": {
                        "en": "Cessation of the organism's vital activity, end of life."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "death",
                                "target_lang_sense_clarification": "cessation of life"
                            },
                            {
                                "target_lang_word": "passing",
                                "target_lang_sense_clarification": "polite/euphemism"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e79495e6-5fda-3e93-ac3d-6c8e22f9f13f",
                    "sense_definition": "Полное прекращение существования или деятельности чего-либо; конец.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cessation_of_life",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для профессионального спортсмена серьёзная травма — это <смерть> карьеры.",
                            "target_lang_translations": {
                                "en": "For a professional athlete, a serious injury is the <death> of a career."
                            }
                        },
                        {
                            "text": "Отсутствие новых идей означает медленную <смерть> для любого искусства.",
                            "target_lang_translations": {
                                "en": "The absence of new ideas means a slow <death> for any art."
                            }
                        }
                    ],
                    "synonyms": [
                        "конец",
                        "финал",
                        "преставление"
                    ],
                    "antonyms": [
                        "расцвет",
                        "оживление",
                        "жизнь"
                    ],
                    "common_phrases": [
                        "смерть надежды",
                        "подобно смерти"
                    ],
                    "target_lang_definitions": {
                        "en": "Complete cessation of existence or activity of something; the end."
                    },
                    "translations": {

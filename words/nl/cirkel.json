    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "85976d3b-8497-3532-90e1-9759919b9361",
                    "sense_definition": "Een ronde, gesloten lijn waarbij alle punten op de rand even ver van het middelpunt liggen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape_geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar tekent een perfecte <w>cirkel</w> op het bord.",
                            "target_lang_translations": {
                                "en": "The teacher draws a perfect <w>circle</w> on the board.",
                                "ru": "Учитель рисует на доске идеальный <w>круг</w>."
                            }
                        },
                        {
                            "text": "Alle kinderen gaan in een grote <w>cirkel</w> op de grond zitten.",
                            "target_lang_translations": {
                                "en": "All the children sit down in a large <w>circle</w> on the ground.",
                                "ru": "Все дети садятся в большой <w>круг</w> на полу."
                            }
                        }
                    ],
                    "synonyms": [
                        "kring",
                        "rondje"
                    ],
                    "common_phrases": [
                        "in een cirkel",
                        "de cirkel is rond"
                    ],
                    "target_lang_definitions": {
                        "en": "A round, closed line where every point on the edge is the same distance from the center.",
                        "ru": "Круглая замкнутая линия или плоская фигура, все точки границы которой находятся на одинаковом расстоянии от центра."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "circle",
                                "target_lang_sense_clarification": "geometric shape"
                            },
                            {
                                "target_lang_word": "ring",
                                "target_lang_sense_clarification": "shape"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "геометрическая фигура"
                            },
                            {
                                "target_lang_word": "окружность",
                                "target_lang_sense_clarification": "линия, ограничивающая круг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa3edb59-f933-32d0-b911-ed4adc360439",
                    "sense_definition": "Een groep mensen die met elkaar omgaan, bij elkaar horen of dezelfde interesses hebben.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij behoort tot de intieme <w>cirkel</w> van de president.",
                            "target_lang_translations": {
                                "en": "He belongs to the president's inner <w>circle</w>.",
                                "ru": "Он входит в близкий <w>круг</w> президента."
                            }
                        },
                        {
                            "text": "Binnen onze <w>cirkel</w> van vrienden is dat heel normaal.",
                            "target_lang_translations": {
                                "en": "Within our <w>circle</w> of friends, that is quite normal.",
                                "ru": "В нашем <w>кругу</w> друзей это вполне нормально."
                            }
                        }
                    ],
                    "synonyms": [
                        "kring",
                        "groep",
                        "gezelschap"
                    ],
                    "common_phrases": [
                        "in kleine cirkel"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people who associate with each other, belong together, or share the same interests.",
                        "ru": "Группа людей, связанных общими интересами, отношениями или деятельностью."
                    },
                    "translations": {

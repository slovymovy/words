                {
                    "sense_id": "80a4817d-4274-312b-a988-004b6d44a7ad",
                    "sense_definition": "A place name, such as a river or village in England.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The village of <w>Burn</w> is located in North Yorkshire.",
                            "target_lang_translations": {
                                "ru": "Деревня <w>Бёрн</w> расположена в Северном Йоркшире."
                            }
                        },
                        {
                            "text": "They visited the River <w>Burn</w> during their trip to Devon.",
                            "target_lang_translations": {
                                "ru": "Они посетили реку <w>Бёрн</w> во время поездки в Девон."
                            }

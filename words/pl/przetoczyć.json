    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9cbaf9e3-ec43-301e-9e2d-06020aa7904e",
                    "sense_definition": "Wprowadzić krew lub preparat krwiopochodny do żyły pacjenta w celu ratowania zdrowia lub życia",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medical_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarze zdecydowali, że trzeba natychmiast <w>przetoczyć</w> rannemu krew.",
                            "target_lang_translations": {
                                "ru": "Врачи решили, что нужно немедленно <w>перелить</w> раненому кровь."
                            }
                        },
                        {
                            "text": "Podczas operacji konieczne było, aby <w>przetoczyć</w> pacjentowi dwie jednostki osocza.",
                            "target_lang_translations": {
                                "ru": "Во время операции было необходимо <w>перелить</w> пациенту две единицы плазмы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przetoczyć krew",
                        "przetoczyć osocze",
                        "przetoczyć płytki krwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ввести кровь или препарат крови в вену пациента для спасения здоровья или жизни"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перелить",
                                "target_lang_sense_clarification": "кровь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87c3df7b-c57e-34ad-8d05-89d9366cbea8",
                    "sense_definition": "Gwałtownie przeminąć lub odbyć się na dużym obszarze (o zjawiskach atmosferycznych, wojnach, dyskusjach); używane z zaimkiem zwrotnym „się”",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "events_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez miasto <w>przetoczyła</w> się potężna burza z piorunami.",
                            "target_lang_translations": {
                                "ru": "Через город <w>пронеслась</w> мощная гроза."
                            }
                        },
                        {
                            "text": "Przez media <w>przetoczyła</w> się fala krytyki dotycząca nowej ustawy.",
                            "target_lang_translations": {
                                "ru": "В СМИ <w>прокатилась</w> волна критики по поводу нового закона."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przetoczyć się przez kraj",
                        "przetoczyć się przez media",
                        "przetoczyć się jak burza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стремительно пройти или охватить большую территорию (о погодных явлениях, войнах, дискуссиях); используется с возвратным местоимением"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокатиться",
                                "target_lang_sense_clarification": "о волне, эхе, событиях"
                            },
                            {
                                "target_lang_word": "пронестись",
                                "target_lang_sense_clarification": "о буре, урагане"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99ae8d29-ee0d-35ca-9c23-17e5f1774746",
                    "sense_definition": "Przemieścić obiekt o obłym kształcie lub obiekt na kołach, popychając go tak, aby obracał się wokół własnej osi",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_rolling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy wspólnie <w>przetoczyć</w> ten ciężki głaz na drugą stronę ogrodu.",
                            "target_lang_translations": {
                                "ru": "Мы должны вместе <w>перекатить</w> этот тяжелый валун на другую сторону сада."
                            }
                        },
                        {
                            "text": "Mechanik pomógł mi <w>przetoczyć</w> zepsuty samochód do warsztatu.",
                            "target_lang_translations": {
                                "ru": "Механик помог мне <w>перекатить</w> сломанную машину в мастерскую."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeturlać"
                    ],
                    "common_phrases": [
                        "przetoczyć beczkę",
                        "przetoczyć kamień",
                        "przetoczyć oponę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместить круглый предмет или объект на колесах, толкая его так, чтобы он вращался вокруг своей оси"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перекатить",
                                "target_lang_sense_clarification": "переместить качением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca71dc99-a750-3f05-8b50-0dc3f546269c",
                    "sense_definition": "Przelać ciecz z jednego naczynia do drugiego, często przy użyciu rurki lub wężyka",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "liquid_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Winiarz musi ostrożnie <w>przetoczyć</w> wino z beczki do butelek.",
                            "target_lang_translations": {
                                "ru": "Винодел должен осторожно <w>перелить</w> вино из бочки в бутылки."
                            }
                        },
                        {
                            "text": "Udało nam się <w>przetoczyć</w> trochę paliwa z kanistra do baku samochodu.",
                            "target_lang_translations": {
                                "ru": "Нам удалось <w>перелить</w> немного топлива из канистры в бак автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "przelać",
                        "przepompować"
                    ],
                    "common_phrases": [
                        "przetoczyć wino",
                        "przetoczyć paliwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перелить жидкость из одного сосуда в другой, часто с использованием трубки или шланга"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перелить",
                                "target_lang_sense_clarification": "жидкость"
                            },
                            {
                                "target_lang_word": "сцедить",
                                "target_lang_sense_clarification": "вино, осадок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "baa60abc-c75d-378d-9ac3-a4109ca9186f",
                    "sense_definition": "Przestawić pojazd szynowy, taki jak wagon lub lokomotywa, na inny tor",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "railway",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lokomotywa manewrowa ma za zadanie <w>przetoczyć</w> wagony towarowe na bocznicę.",
                            "target_lang_translations": {
                                "ru": "Задача маневрового локомотива — <w>перегнать</w> товарные вагоны на запасной путь."
                            }
                        },
                        {
                            "text": "Dyspozytor wydał polecenie, aby <w>przetoczyć</w> uszkodzony skład do hangaru naprawczego.",
                            "target_lang_translations": {
                                "ru": "Диспетчер дал команду <w>перегнать</w> поврежденный состав в ремонтный ангар."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przetoczyć wagon",
                        "przetoczyć pociąg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместить железнодорожное транспортное средство, например вагон или локомотив, на другой путь"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перегнать",
                                "target_lang_sense_clarification": "вагоны, поезд"
                            },
                            {
                                "target_lang_word": "переставить",
                                "target_lang_sense_clarification": "на другой путь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
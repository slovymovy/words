            "senses": [
                {
                    "sense_id": "f293027a-b1b6-3dbf-8fd4-b5d093f82385",
                    "sense_definition": "Относящийся к текущему году, сезону или сегодняшнему дню.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В <нынешнем> году зима наступила неожиданно рано.",
                            "target_lang_translations": {
                                "en": "Winter arrived unexpectedly early <this> year."
                            }
                        },
                        {
                            "text": "<Нынешнее> лето обещает быть очень жарким и сухим.",
                            "target_lang_translations": {
                                "en": "<This> summer promises to be very hot and dry."
                            }
                        }
                    ],
                    "synonyms": [
                        "сегодняшний",
                        "текущий"
                    ],
                    "antonyms": [
                        "прошлый",
                        "прошлогодний"
                    ],
                    "common_phrases": [
                        "нынешний год",
                        "нынешняя зима",
                        "нынешнее лето"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the current year, season, or day."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "this",
                                "target_lang_sense_clarification": "referring to the specific time period now occurring"
                            },
                            {
                                "target_lang_word": "current",
                                "target_lang_sense_clarification": "belonging to the present time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c1d0f3d-7fbf-36f7-85a0-73dfaa05cca5",
                    "sense_definition": "Существующий или действующий в настоящее время; современный.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "current_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Нынешняя> молодёжь часто увлекается компьютерными играми.",
                            "target_lang_translations": {
                                "en": "<Today's> youth is often into computer games."
                            }
                        },
                        {
                            "text": "Нам нужно обсудить <нынешнюю> ситуацию в экономике.",
                            "target_lang_translations": {
                                "en": "We need to discuss the <current> situation in the economy."
                            }
                        }
                    ],
                    "synonyms": [

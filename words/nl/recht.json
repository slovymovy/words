                },
                {
                    "sense_id": "774e1c6e-98f3-384f-a6d6-a9fcd576cbb4",
                    "sense_definition": "Direct en eerlijk; zonder omwegen of bedrog.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is altijd <w>recht</w> door zee en zegt wat ze denkt.",
                            "target_lang_translations": {
                                "ru": "Она всегда <w>прямолинейна</w> и говорит то, что думает.",
                                "en": "She is always <w>straightforward</w> and says what she thinks."
                            }
                        },
                        {
                            "text": "Ik waardeer een <w>recht</w> antwoord meer dan een smoesje.",
                            "target_lang_translations": {
                                "ru": "Я ценю <w>прямой</w> ответ больше, чем отговорки.",
                                "en": "I appreciate a <w>straight</w> answer more than an excuse."
                            }
                        }
                    ],
                    "common_phrases": [
                        "recht door zee",
                        "recht voor zijn raap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямой и честный; без обходных путей или обмана.",
                        "en": "Direct and honest; without detours or deceit."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямой",
                                "target_lang_sense_clarification": "откровенный"
                            },
                            {
                                "target_lang_word": "прямолинейный",
                                "target_lang_sense_clarification": "о характере"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "straight",
                                "target_lang_sense_clarification": "honest or direct"
                            },
                            {
                                "target_lang_word": "straightforward",
                                "target_lang_sense_clarification": "of character"
                            },
                            {
                                "target_lang_word": "direct",
                                "target_lang_sense_clarification": "without evasion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d6d355e-ba2b-3f87-94bc-6b3f1b434af8",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c0bf4474-7e4f-3de4-a16f-6d6210dd29fe",
                    "sense_definition": "Wielkie, międzynarodowe zawody sportowe organizowane cyklicznie, zazwyczaj co cztery lata.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "events",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy kibice czekają na letnie <w>igrzyska</w> olimpijskie.",
                            "target_lang_translations": {
                                "ru": "Все болельщики ждут летних Олимпийских <w>игр</w>."
                            }
                        },
                        {
                            "text": "Polscy siatkarze zdobyli złoto na ostatnich <w>igrzyskach</w>.",
                            "target_lang_translations": {
                                "ru": "Польские волейболисты завоевали золото на последних <w>играх</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawody"
                    ],
                    "common_phrases": [
                        "igrzyska olimpijskie",
                        "letnie igrzyska",
                        "zimowe igrzyska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупные международные спортивные соревнования, проводимые циклически, обычно каждые четыре года."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игры",
                                "target_lang_sense_clarification": "спортивные соревнования (обычно Олимпийские)"
                            },
                            {
                                "target_lang_word": "олимпиада",
                                "target_lang_sense_clarification": "синоним Олимпийских игр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b488cf66-51a6-3746-a168-ea39f1b79e98",
                    "sense_definition": "Publiczne widowisko lub uroczystość o charakterze rozrywkowym, organizowane zwłaszcza w starożytności.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "events",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzymski tłum domagał się od cesarza chleba i <w>igrzysk</w>.",
                            "target_lang_translations": {
                                "ru": "Римская толпа требовала от императора хлеба и <w>зрелищ</w>."
                            }
                        },
                        {
                            "text": "Podczas starożytnych <w>igrzysk</w> odbywały się wyścigi rydwanów.",
                            "target_lang_translations": {
                                "ru": "Во время древних <w>игр</w> проходили гонки на колесницах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chleba i igrzysk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичное зрелище или торжество развлекательного характера, устраиваемое, в частности, в древности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зрелища",
                                "target_lang_sense_clarification": "публичные развлечения (в идиоме \"хлеба и зрелищ\")"
                            },
                            {
                                "target_lang_word": "игры",
                                "target_lang_sense_clarification": "исторические состязания"
                            }
                        ]
                    }
                }
            ]
        }
    ]

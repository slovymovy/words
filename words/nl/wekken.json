{
    "word_family": [
        "wekker",
        "opwekken",
        "verwekken",
        "ontwaken"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c14567d6-9486-304f-9046-cdd30a8ee633",
                    "sense_definition": "Iemand wakker maken; uit de slaap halen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "wake_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je me morgenochtend om zeven uur <wekken>?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <разбудить> меня завтра утром в семь часов?",
                                "en": "Can you <w>wake</w> me <w>up</w> tomorrow morning at seven o'clock?"
                            }
                        },
                        {
                            "text": "De harde muziek van de buren <wekte> ons midden in de nacht.",
                            "target_lang_translations": {
                                "ru": "Громкая музыка соседей <разбудила> нас посреди ночи.",
                                "en": "The neighbors' loud music <w>woke</w> us <w>up</w> in the middle of the night."
                            }
                        }
                    ],
                    "synonyms": [
                        "wakker maken"
                    ],
                    "antonyms": [
                        "in slaap sussen"
                    ],
                    "common_phrases": [
                        "iemand wekken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Будить кого-то; выводить из состояния сна.",
                        "en": "To cause someone to stop sleeping; to wake up."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбудить",
                                "target_lang_sense_clarification": "прервать сон (совершенный вид)"
                            },
                            {
                                "target_lang_word": "будить",
                                "target_lang_sense_clarification": "прерывать сон (несовершенный вид)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "wake",
                                "target_lang_sense_clarification": "to rouse from sleep"
                            },
                            {
                                "target_lang_word": "wake up",
                                "target_lang_sense_clarification": "to rouse from sleep"
                            },
                            {
                                "target_lang_word": "awaken",
                                "target_lang_sense_clarification": "to rouse from sleep (formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4741be46-0ac4-3294-a65f-40143df9b17c",
                    "sense_definition": "Een bepaald gevoel, idee of een reactie doen ontstaan.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "cause_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn vreemde gedrag <wekte> argwaan bij de politie.",
                            "target_lang_translations": {
                                "ru": "Его странное поведение <вызвало> подозрение у полиции.",
                                "en": "His strange behavior <w>aroused</w> suspicion with the police."
                            }
                        },
                        {
                            "text": "De nieuwe plannen van de regering <wekken> de indruk dat er bezuinigd gaat worden.",
                            "target_lang_translations": {
                                "ru": "Новые планы правительства <производят> впечатление, что грядут сокращения.",
                                "en": "The government's new plans <w>create</w> the impression that cuts are coming."
                            }
                        }
                    ],
                    "synonyms": [
                        "veroorzaken",
                        "oproepen",
                        "teweegbrengen"
                    ],
                    "antonyms": [
                        "wegnemen"
                    ],
                    "common_phrases": [
                        "de indruk wekken",
                        "argwaan wekken",
                        "interesse wekken",
                        "vertrouwen wekken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать определенное чувство, идею или реакцию.",
                        "en": "To cause a certain feeling, idea, or reaction to arise."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызвать",
                                "target_lang_sense_clarification": "стать причиной (чувства, реакции)"
                            },
                            {
                                "target_lang_word": "произвести",
                                "target_lang_sense_clarification": "создать (впечатление)"
                            },
                            {
                                "target_lang_word": "пробудить",
                                "target_lang_sense_clarification": "возбудить (интерес, чувства)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "arouse",
                                "target_lang_sense_clarification": "to evoke (interest, suspicion)"
                            },
                            {
                                "target_lang_word": "raise",
                                "target_lang_sense_clarification": "to cause (suspicion, questions)"
                            },
                            {
                                "target_lang_word": "create",
                                "target_lang_sense_clarification": "to produce (an impression)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
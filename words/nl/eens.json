{
    "word_family": [
        "oneens",
        "eensgezind"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7f68c88d-606b-3587-9c9b-b18e7f913dbc",
                    "sense_definition": "Een woordje dat een zin vriendelijker, beleefder, minder dwingend of juist nonchalanter maakt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "modal_particle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk <w>eens</w> wat ik voor je heb meegenomen.",
                            "target_lang_translations": {
                                "ru": "Посмотри<w>-ка</w>, что я тебе принес."
                            }
                        },
                        {
                            "text": "Kom <w>eens</w> hier als je tijd hebt.",
                            "target_lang_translations": {
                                "ru": "Подойди<w>-ка</w> сюда, если есть время."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kijk eens",
                        "kom eens",
                        "zeg eens",
                        "even eens"
                    ],
                    "target_lang_definitions": {
                        "ru": "Частица, которая делает предложение более дружелюбным, вежливым или непринужденным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "-ка",
                                "target_lang_sense_clarification": "смягчающая частица (суффикс)"
                            },
                            {
                                "target_lang_word": "же",
                                "target_lang_sense_clarification": "усилительная частица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e0da27b-e376-3879-b018-af5e42f18a78",
                    "sense_definition": "Eén enkele keer; eenmaal.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_count",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb hem maar <w>eens</w> in mijn leven gezien.",
                            "target_lang_translations": {
                                "ru": "Я видел его всего <w>один раз</w> в жизни."
                            }
                        },
                        {
                            "text": "Het is nu <w>eens</w> niet anders, we moeten doorgaan.",
                            "target_lang_translations": {
                                "ru": "Так уж <w>сложилось</w>, ничего не поделаешь, мы должны продолжать."
                            }
                        }
                    ],
                    "synonyms": [
                        "eenmaal",
                        "een keer"
                    ],
                    "common_phrases": [
                        "eens zo groot",
                        "voor eens en altijd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один единственный раз; единожды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "один раз",
                                "target_lang_sense_clarification": "количество"
                            },
                            {
                                "target_lang_word": "однажды",
                                "target_lang_sense_clarification": "как-то раз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7edfa646-3e44-30c1-add8-e3d8c1528e04",
                    "sense_definition": "Op een onbepaald moment in het verleden of de toekomst.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_indefinite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er was <w>eens</w> een koning die in een groot kasteel woonde.",
                            "target_lang_translations": {
                                "ru": "<w>Однажды</w> жил король, который обитал в большом замке."
                            }
                        },
                        {
                            "text": "<w>Eens</w> zullen we begrijpen waarom dit moest gebeuren.",
                            "target_lang_translations": {
                                "ru": "<w>Когда-нибудь</w> мы поймем, почему это должно было случиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "ooit"
                    ],
                    "common_phrases": [
                        "er was eens",
                        "eens te meer"
                    ],
                    "target_lang_definitions": {
                        "ru": "В неопределенный момент в прошлом или будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "однажды",
                                "target_lang_sense_clarification": "в прошлом"
                            },
                            {
                                "target_lang_word": "когда-нибудь",
                                "target_lang_sense_clarification": "в будущем"
                            },
                            {
                                "target_lang_word": "когда-то",
                                "target_lang_sense_clarification": "в прошлом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bfae32b-349d-318a-a05a-ab63fa39a071",
                    "sense_definition": "Zelfs; gebruikt om nadruk te leggen, meestal in ontkennende zinnen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft me niet <w>eens</w> gedag gezegd toen hij wegging.",
                            "target_lang_translations": {
                                "ru": "Он <w>даже</w> не попрощался со мной, когда уходил."
                            }
                        },
                        {
                            "text": "Ze wisten niet <w>eens</w> dat we vandaag zouden komen.",
                            "target_lang_translations": {
                                "ru": "Они <w>даже</w> не знали, что мы сегодня придем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niet eens",
                        "geen eens"
                    ],
                    "target_lang_definitions": {
                        "ru": "Даже; используется для усиления, обычно в отрицательных предложениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "даже",
                                "target_lang_sense_clarification": "усиление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d8a479bf-2e14-3e0a-8a41-98509530e360",
                    "sense_definition": "Dezelfde mening hebbend; in overeenstemming zijn met iemand.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben het volledig met je <w>eens</w> over dat onderwerp.",
                            "target_lang_translations": {
                                "ru": "Я полностью с тобой <w>согласен</w> в этом вопросе."
                            }
                        },
                        {
                            "text": "Ze konden het maar niet <w>eens</w> worden over de bestemming.",
                            "target_lang_translations": {
                                "ru": "Они никак не могли <w>договориться</w> о месте назначения."
                            }
                        }
                    ],
                    "antonyms": [
                        "oneens"
                    ],
                    "common_phrases": [
                        "het eens zijn",
                        "het eens worden"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "alleen predicatief"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий то же мнение; быть в согласии с кем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласен",
                                "target_lang_sense_clarification": "имеющий то же мнение"
                            },
                            {
                                "target_lang_word": "заодно",
                                "target_lang_sense_clarification": "единодушный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "2edc0f43-97e8-3648-a5ee-ddaaa61a922d",
                    "sense_definition": "Op het moment dat; vanaf het tijdstip dat.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conjunction_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Eens</w> we het eens zijn, kunnen we eindelijk verder.",
                            "target_lang_translations": {
                                "ru": "<w>Как только</w> мы договоримся, мы сможем наконец двигаться дальше."
                            }
                        },
                        {
                            "text": "<w>Eens</w> de zon onder is, koelt het hier snel af.",
                            "target_lang_translations": {
                                "ru": "<w>Как только</w> солнце садится, здесь быстро холодает."
                            }
                        }
                    ],
                    "synonyms": [
                        "zodra"
                    ],
                    "target_lang_definitions": {
                        "ru": "В тот момент, когда; начиная с того времени, как."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как только",
                                "target_lang_sense_clarification": "союз времени"
                            },
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "союз"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "09dcec7e-a54e-3768-8880-5dd63dbdcf0d",
                    "sense_definition": "Oude naamvalsvorm (genitief) van het onbepaalde lidwoord 'een'.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "article_genitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het leven <w>eens</w> mensen is kort en kwetsbaar.",
                            "target_lang_translations": {
                                "ru": "Жизнь <w>одного</w> человека коротка и уязвима."
                            }
                        },
                        {
                            "text": "Hij sprak in de naam <w>eens</w> anderen.",
                            "target_lang_translations": {
                                "ru": "Он говорил от имени <w>другого</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшая форма родительного падежа неопределенного артикля 'een'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одного",
                                "target_lang_sense_clarification": "родительный падеж"
                            },
                            {
                                "target_lang_word": "некоего",
                                "target_lang_sense_clarification": "неопределенный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
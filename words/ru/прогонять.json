{
    "word_family": [
        "прогнать",
        "гнать",
        "выгонять",
        "изгонять",
        "разгонять",
        "нагонять",
        "отгонять",
        "подгонять",
        "перегонять",
        "угонять",
        "прогон"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07db496d-4c1a-3a3e-b9f1-5b648575379a",
                    "sense_definition": "Заставлять уйти, покинуть место; удалять силой или приказом.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "force_leave",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хозяин магазина каждый день <прогоняет> шумных подростков от входа.",
                            "target_lang_translations": {
                                "en": "The shop owner <chases away> noisy teenagers from the entrance every day."
                            }
                        },
                        {
                            "text": "Не нужно <прогонять> собаку, она просто хочет поиграть.",
                            "target_lang_translations": {
                                "en": "You don't need to <shoo away> the dog, it just wants to play."
                            }
                        }
                    ],
                    "synonyms": [
                        "выгонять",
                        "изгонять",
                        "выдворять"
                    ],
                    "antonyms": [
                        "звать",
                        "приглашать",
                        "подзывать"
                    ],
                    "common_phrases": [
                        "прогонять из дома",
                        "прогонять прочь",
                        "прогонять со двора"
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone or an animal to leave a place; to remove by force or order."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive away",
                                "target_lang_sense_clarification": "general term for forcing to leave"
                            },
                            {
                                "target_lang_word": "chase away",
                                "target_lang_sense_clarification": "implies active pursuit or aggression"
                            },
                            {
                                "target_lang_word": "shoo away",
                                "target_lang_sense_clarification": "specifically for animals or children, using gestures"
                            },
                            {
                                "target_lang_word": "kick out",
                                "target_lang_sense_clarification": "informal, usually from a building or room"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e20d00ed-78f6-3894-81fd-1121179b71e6",
                    "sense_definition": "Заставлять исчезнуть (о мыслях, чувствах, состояниях); избавляться от чего-либо неприятного.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dispel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я стараюсь <прогонять> мрачные мысли и думать о хорошем.",
                            "target_lang_translations": {
                                "en": "I try to <banish> gloomy thoughts and think about good things."
                            }
                        },
                        {
                            "text": "Утренняя зарядка отлично <прогоняет> сон и дарит бодрость.",
                            "target_lang_translations": {
                                "en": "Morning exercise excellently <dispels> sleep and gives energy."
                            }
                        }
                    ],
                    "synonyms": [
                        "развеивать",
                        "отгонять"
                    ],
                    "antonyms": [
                        "нагонять"
                    ],
                    "common_phrases": [
                        "прогонять тоску",
                        "прогонять сон",
                        "прогонять страх",
                        "прогонять мысли"
                    ],
                    "target_lang_definitions": {
                        "en": "To force thoughts, feelings, or states to disappear; to get rid of something unpleasant."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dispel",
                                "target_lang_sense_clarification": "formal, often used for gloom, myths, or sleep"
                            },
                            {
                                "target_lang_word": "banish",
                                "target_lang_sense_clarification": "to deliberately push away thoughts or memories"
                            },
                            {
                                "target_lang_word": "drive away",
                                "target_lang_sense_clarification": "general term for removing feelings or states"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8902f7c2-0477-3de0-b56d-c8e05b736ab9",
                    "sense_definition": "Увольнять с работы, лишать должности (обычно резко или с негативным оттенком).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fire_job",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор грозится <прогонять> всех, кто опаздывает на совещания.",
                            "target_lang_translations": {
                                "en": "The director threatens to <fire> everyone who is late for meetings."
                            }
                        },
                        {
                            "text": "Его никто не <прогоняет> со службы, он уходит по собственному желанию.",
                            "target_lang_translations": {
                                "en": "No one is <kicking> him <out> of the service; he is leaving of his own accord."
                            }
                        }
                    ],
                    "synonyms": [
                        "увольнять",
                        "выгонять"
                    ],
                    "antonyms": [
                        "нанимать",
                        "принимать"
                    ],
                    "common_phrases": [
                        "прогонять с работы",
                        "прогонять со службы"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To dismiss someone from a job or position, often abruptly or with a negative connotation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fire",
                                "target_lang_sense_clarification": "standard term for dismissal"
                            },
                            {
                                "target_lang_word": "kick out",
                                "target_lang_sense_clarification": "informal, implies harsh removal"
                            },
                            {
                                "target_lang_word": "sack",
                                "target_lang_sense_clarification": "informal, common in British English"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fea57da3-2284-3ad7-bac6-5129cf2016a9",
                    "sense_definition": "Пропускать что-либо через механизм, устройство или систему для обработки или проверки.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "process_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Программист <прогоняет> код через систему тестов перед релизом.",
                            "target_lang_translations": {
                                "en": "The programmer <runs> the code through a testing system before release."
                            }
                        },
                        {
                            "text": "Мы <прогоняем> воду через фильтр, чтобы очистить её от примесей.",
                            "target_lang_translations": {
                                "en": "We <pass> water through a filter to purify it from impurities."
                            }
                        }
                    ],
                    "synonyms": [
                        "пропускать"
                    ],
                    "common_phrases": [
                        "прогонять через компьютер",
                        "прогонять через базу",
                        "прогонять через фильтр"
                    ],
                    "target_lang_definitions": {
                        "en": "To pass something through a mechanism, device, or system for processing or verification."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "common for software, code, or tests"
                            },
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "used for liquids or physical objects through a filter"
                            },
                            {
                                "target_lang_word": "run through",
                                "target_lang_sense_clarification": "to process through a system"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c33edb5-fbec-3b9a-aa46-9b19e6b9c95c",
                    "sense_definition": "Репетировать спектакль, пьесу или номер целиком, без остановок.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "rehearse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед премьерой актёры <прогоняют> весь спектакль в костюмах.",
                            "target_lang_translations": {
                                "en": "Before the premiere, the actors <run through> the entire play in costumes."
                            }
                        },
                        {
                            "text": "Режиссёр решил ещё раз <прогонять> финальную сцену для закрепления.",
                            "target_lang_translations": {
                                "en": "The director decided to <run through> the final scene one more time to consolidate it."
                            }
                        }
                    ],
                    "synonyms": [
                        "репетировать"
                    ],
                    "common_phrases": [
                        "прогонять спектакль",
                        "прогонять сцену",
                        "прогонять номер"
                    ],
                    "target_lang_definitions": {
                        "en": "To rehearse a play, performance, or act entirely from start to finish without stopping."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run through",
                                "target_lang_sense_clarification": "specific theater/performance term"
                            },
                            {
                                "target_lang_word": "rehearse",
                                "target_lang_sense_clarification": "general term"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "reencounter"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "02ae7c0e-ce0c-3aa0-ab26-1272db91bfde",
                    "sense_definition": "A meeting, especially one that is unplanned or unexpected.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "meeting_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Their <w>encounter</w> was a matter of chance.",
                            "target_lang_translations": {
                                "ru": "Их <w>встреча</w> была делом случая."
                            }
                        },
                        {
                            "text": "I had a brief <w>encounter</w> with a famous actor at the airport.",
                            "target_lang_translations": {
                                "ru": "У меня была короткая <w>встреча</w> с известным актером в аэропорту."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chance encounter",
                        "brief encounter",
                        "unexpected encounter",
                        "close encounter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встреча, особенно незапланированная или неожиданная."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встреча",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "случайная встреча",
                                "target_lang_sense_clarification": "незапланированная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e272e365-8787-3186-8240-6d02c4f0e8e0",
                    "sense_definition": "A hostile, often violent meeting or confrontation between opposing parties.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "meeting_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers survived a violent <w>encounter</w> with the enemy forces.",
                            "target_lang_translations": {
                                "ru": "Солдаты пережили жестокое <w>столкновение</w> с силами противника."
                            }
                        },
                        {
                            "text": "Police reported a tense <w>encounter</w> with the suspects.",
                            "target_lang_translations": {
                                "ru": "Полиция сообщила о напряженном <w>столкновении</w> с подозреваемыми."
                            }
                        }
                    ],
                    "synonyms": [
                        "clash",
                        "confrontation",
                        "skirmish",
                        "brush"
                    ],
                    "common_phrases": [
                        "violent encounter",
                        "bloody encounter",
                        "military encounter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Враждебное, часто жестокое столкновение или противостояние между противоборствующими сторонами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столкновение",
                                "target_lang_sense_clarification": "конфликт, противостояние"
                            },
                            {
                                "target_lang_word": "стычка",
                                "target_lang_sense_clarification": "небольшой бой, ссора"
                            },
                            {
                                "target_lang_word": "схватка",
                                "target_lang_sense_clarification": "борьба, бой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87e0ab2e-9da8-3e06-a39f-32666fd94ffb",
                    "sense_definition": "A match or game between two opposing sides in sports.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "meeting_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fans enjoyed an enthralling <w>encounter</w> between the two rival teams.",
                            "target_lang_translations": {
                                "ru": "Болельщики наслаждались захватывающей <w>встречей</w> двух команд-соперниц."
                            }
                        },
                        {
                            "text": "This final <w>encounter</w> will decide who wins the championship.",
                            "target_lang_translations": {
                                "ru": "Эта финальная <w>встреча</w> решит, кто выиграет чемпионат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "thrilling encounter",
                        "final encounter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Матч или игра между двумя противоборствующими сторонами в спорте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встреча",
                                "target_lang_sense_clarification": "спортивное состязание"
                            },
                            {
                                "target_lang_word": "поединок",
                                "target_lang_sense_clarification": "соревнование, матч"
                            },
                            {
                                "target_lang_word": "матч",
                                "target_lang_sense_clarification": "спортивная игра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9cfe351-851e-335c-be20-54ecae243a44",
                    "sense_definition": "A sexual meeting or activity between two people.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "meeting_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He described the sexual <w>encounter</w> as consensual.",
                            "target_lang_translations": {
                                "ru": "Он описал этот сексуальный <w>контакт</w> как добровольный."
                            }
                        },
                        {
                            "text": "She wanted to forget her brief <w>encounter</w> with the stranger.",
                            "target_lang_translations": {
                                "ru": "Она хотела забыть свою мимолетную <w>связь</w> с незнакомцем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sexual encounter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сексуальная встреча или контакт между двумя людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "интимные отношения"
                            },
                            {
                                "target_lang_word": "интимная встреча",
                                "target_lang_sense_clarification": "свидание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0535809-09fd-313b-8ddf-a0caaf5a4e0c",
                    "sense_definition": "An extrajudicial killing by police or security forces.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "meeting_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gangster was killed in an alleged police <w>encounter</w>.",
                            "target_lang_translations": {
                                "ru": "Гангстер был убит в ходе предполагаемой полицейской <w>перестрелки</w>."
                            }
                        },
                        {
                            "text": "Human rights groups investigated the circumstances of the <w>encounter</w>.",
                            "target_lang_translations": {
                                "ru": "Правозащитные группы расследовали обстоятельства этого <w>столкновения</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "police encounter",
                        "fake encounter"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "South Asia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внесудебная расправа со стороны полиции или сил безопасности (термин, характерный для Южной Азии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перестрелка",
                                "target_lang_sense_clarification": "часто инсценированная"
                            },
                            {
                                "target_lang_word": "столкновение",
                                "target_lang_sense_clarification": "инцидент с полицией"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d75f7a0c-6c00-3b37-a72a-d9d5050e66e2",
                    "sense_definition": "The period when a spacecraft is close enough to a planet or object to gather data.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "meeting_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The probe sent back images during its <w>encounter</w> with Jupiter.",
                            "target_lang_translations": {
                                "ru": "Зонд передал изображения во время <w>сближения</w> с Юпитером."
                            }
                        },
                        {
                            "text": "Mission control prepared for the spacecraft's closest <w>encounter</w>.",
                            "target_lang_translations": {
                                "ru": "Центр управления полетами готовился к максимальному <w>сближению</w> космического аппарата."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Период сближения космического аппарата с небесным телом для сбора данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сближение",
                                "target_lang_sense_clarification": "космического аппарата с объектом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0489cf1e-c0d6-3994-8318-42747550b819",
                    "sense_definition": "To meet someone or experience something, especially unexpectedly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action_meet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We did not expect to <w>encounter</w> any problems during the trip.",
                            "target_lang_translations": {
                                "ru": "Мы не ожидали <w>столкнуться</w> с какими-либо проблемами во время поездки."
                            }
                        },
                        {
                            "text": "Hikers may <w>encounter</w> wild animals in this area.",
                            "target_lang_translations": {
                                "ru": "Туристы могут <w>встретить</w> диких животных в этом районе."
                            }
                        }
                    ],
                    "synonyms": [
                        "cross paths"
                    ],
                    "common_phrases": [
                        "encounter problems",
                        "encounter difficulties",
                        "encounter resistance",
                        "encounter a situation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встретить кого-то или испытать что-то, особенно неожиданно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столкнуться",
                                "target_lang_sense_clarification": "с трудностями, проблемами"
                            },
                            {
                                "target_lang_word": "встретить",
                                "target_lang_sense_clarification": "кого-либо или что-либо"
                            },
                            {
                                "target_lang_word": "наткнуться",
                                "target_lang_sense_clarification": "случайно обнаружить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8da9fb3-9e68-31db-819c-00b6963bbe8b",
                    "sense_definition": "To engage in conflict with an enemy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_meet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The army prepared to <w>encounter</w> the invading forces at dawn.",
                            "target_lang_translations": {
                                "ru": "Армия готовилась <w>встретить</w> вторгшиеся войска на рассвете."
                            }
                        },
                        {
                            "text": "They <w>encountered</w> heavy fire near the border.",
                            "target_lang_translations": {
                                "ru": "Они <w>столкнулись</w> с сильным огнем у границы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "encounter the enemy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вступать в конфликт с врагом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столкнуться",
                                "target_lang_sense_clarification": "вступить в бой"
                            },
                            {
                                "target_lang_word": "встретить",
                                "target_lang_sense_clarification": "противника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "107f5e87-33b9-3888-bbe7-38b69736ee61",
                    "sense_definition": "To kill or execute someone extrajudicially.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_meet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The suspect was <w>encountered</w> by the special task force.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый был <w>ликвидирован</w> спецназом."
                            }
                        },
                        {
                            "text": "Officers threatened to <w>encounter</w> the criminal if he did not surrender.",
                            "target_lang_translations": {
                                "ru": "Офицеры угрожали <w>ликвидировать</w> преступника, если он не сдастся."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "South Asia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Убить или казнить кого-либо во внесудебном порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ликвидировать",
                                "target_lang_sense_clarification": "убить при задержании"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
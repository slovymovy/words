{
    "word_family": [
        "antique",
        "antiquity",
        "antiquarian",
        "antiquated"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ff5aa9fd-fe5c-3db7-9975-df1de388621d",
                    "sense_definition": "A person who studies, collects, or is knowledgeable about antiquities, ancient artifacts, and relics of the past.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_collector",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old <w>antiquary</w> spent his entire life researching the history of the local cathedral.",
                            "target_lang_translations": {
                                "ru": "Старый <w>антиквар</w> провел всю свою жизнь, исследуя историю местного собора."
                            }
                        },
                        {
                            "text": "As a passionate <w>antiquary</w>, she was thrilled to find a Roman coin in the muddy field.",
                            "target_lang_translations": {
                                "ru": "Будучи страстным <w>любителем древностей</w>, она была в восторге, найдя римскую монету на грязном поле."
                            }
                        }
                    ],
                    "synonyms": [
                        "antiquarian",
                        "collector"
                    ],
                    "common_phrases": [
                        "local antiquary",
                        "learned antiquary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который изучает, собирает или хорошо разбирается в древностях, древних артефактах и реликвиях прошлого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "антиквар",
                                "target_lang_sense_clarification": "знаток или собиратель древностей"
                            },
                            {
                                "target_lang_word": "любитель древностей",
                                "target_lang_sense_clarification": "исследователь старины"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "80824445-8ae9-3835-b8d2-453e35ab4914",
                    "sense_definition": "Relating to the ancient past or the study of antiquities.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>antiquary</w> interests led him to visit every museum in the city.",
                            "target_lang_translations": {
                                "ru": "Его <w>антикварные</w> интересы побудили его посетить каждый музей в городе."
                            }
                        },
                        {
                            "text": "The library contains many volumes of <w>antiquary</w> lore that are rarely read today.",
                            "target_lang_translations": {
                                "ru": "В библиотеке хранится множество томов <w>антикварных</w> знаний, которые сегодня редко читают."
                            }
                        }
                    ],
                    "synonyms": [
                        "antiquarian",
                        "ancient"
                    ],
                    "antonyms": [
                        "modern",
                        "contemporary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к древнему прошлому или изучению древностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "антикварный",
                                "target_lang_sense_clarification": "относящийся к старине"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
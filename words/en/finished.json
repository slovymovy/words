                    "synonyms": [
                        "doomed",
                        "wrecked",
                        "sunk",
                        "done for",
                        "ruined"
                    ],
                    "common_phrases": [
                        "completely finished",
                        "finished with"
                    ],
                    "target_lang_definitions": {

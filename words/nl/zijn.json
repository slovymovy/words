{
    "word_family": [
        "welzijn",
        "samenzijn",
        "bewustzijn",
        "aanwezig",
        "afwezig",
        "bijzijn"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5c2aeace-b7f2-37e9-a580-59fdfe4c89a7",
                    "sense_definition": "Koppelwerkwoord dat het onderwerp verbindt met een eigenschap, toestand, beroep of identiteit.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "copula",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De lucht <w>is</w> vandaag heel helder en blauw.",
                            "target_lang_translations": {
                                "en": "The sky <w>is</w> very clear and blue today.",
                                "ru": "Небо сегодня <w>—</w> очень чистое и голубое."
                            }
                        },
                        {
                            "text": "Mijn buurman <w>is</w> leraar op een basisschool.",
                            "target_lang_translations": {
                                "en": "My neighbor <w>is</w> a teacher at a primary school.",
                                "ru": "Мой сосед <w>—</w> учитель в начальной школе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan de beurt zijn",
                        "van plan zijn",
                        "bezig zijn",
                        "klaar zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Linking verb connecting the subject to a property, state, profession, or identity.",
                        "ru": "Глагол-связка, соединяющий подлежащее со свойством, состоянием, профессией или идентичностью."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "am, is, are, was, were"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "глагол-связка; в настоящем времени часто опускается или заменяется тире"
                            },
                            {
                                "target_lang_word": "являться",
                                "target_lang_sense_clarification": "более формальный синоним глагола 'быть'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e212d0a5-73a3-3875-964f-b62dd449748b",
                    "sense_definition": "Zich op een bepaalde plaats bevinden of aanwezig zijn.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>ben</w> nu in de keuken koffie aan het zetten.",
                            "target_lang_translations": {
                                "en": "I <w>am</w> in the kitchen making coffee now.",
                                "ru": "Я сейчас <w>нахожусь</w> на кухне, варю кофе."
                            }
                        },
                        {
                            "text": "Waar <w>was</w> je gisteren tijdens het feest?",
                            "target_lang_translations": {
                                "en": "Where <w>were</w> you yesterday during the party?",
                                "ru": "Где ты <w>был</w> вчера во время вечеринки?"
                            }
                        }
                    ],
                    "synonyms": [
                        "bestaan",
                        "bennen"
                    ],
                    "common_phrases": [
                        "er zijn",
                        "weg zijn",
                        "thuis zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "To be located or present at a specific place.",
                        "ru": "Находиться или присутствовать в определенном месте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "to exist in a location"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "быть расположенным где-либо"
                            },
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "присутствовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9dad495-0ca5-3a87-a637-ecf2e300488e",
                    "sense_definition": "Onpersoonlijk werkwoord gebruikt om het weer, de tijd of een algemene toestand aan te geven.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "impersonal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>is</w> vandaag erg koud en regenachtig.",
                            "target_lang_translations": {
                                "en": "It <w>is</w> very cold and rainy today.",
                                "ru": "Сегодня очень <w>холодно</w> и дождливо."
                            }
                        },
                        {
                            "text": "Het <w>is</w> al laat, we moeten naar huis.",
                            "target_lang_translations": {
                                "en": "It <w>is</w> already late, we have to go home.",
                                "ru": "Уже <w>поздно</w>, нам пора домой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het is tijd",
                        "het is mooi weer"
                    ],
                    "target_lang_definitions": {
                        "en": "Impersonal verb used to indicate weather, time, or a general condition.",
                        "ru": "Безличный глагол, используемый для обозначения погоды, времени или общего состояния."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "used in 'it is' constructions"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "в прошедшем и будущем времени; в настоящем обычно опускается в пользу наречия (холодно, поздно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e62bfdc-2f3a-3ab7-a5bb-55746bc45623",
                    "sense_definition": "Gelijk zijn aan; als uitkomst hebben (bij rekenen).",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drie keer vijf <w>is</w> vijftien.",
                            "target_lang_translations": {
                                "en": "Three times five <w>is</w> fifteen.",
                                "ru": "Трижды пять <w>будет</w> пятнадцать."
                            }
                        },
                        {
                            "text": "Twee plus twee <w>is</w> vier.",
                            "target_lang_translations": {
                                "en": "Two plus two <w>is</w> four.",
                                "ru": "Два плюс два <w>равно</w> четырем."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To equal; to result in (in calculations).",
                        "ru": "Равняться; давать в результате (при вычислениях)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "is, equals"
                            },
                            {
                                "target_lang_word": "equal",
                                "target_lang_sense_clarification": "mathematical equality"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "равняться",
                                "target_lang_sense_clarification": "быть равным"
                            },
                            {
                                "target_lang_word": "получаться",
                                "target_lang_sense_clarification": "разг. 'будет' (дважды два будет четыре)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21f30e63-7807-3f64-bd24-03f64839f4d4",
                    "sense_definition": "Hulpwerkwoord dat samen met een voltooid deelwoord de voltooide tijd vormt (vooral bij werkwoorden van beweging of verandering).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "auxiliary_perfect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>is</w> gisteren naar de dokter geweest.",
                            "target_lang_translations": {
                                "en": "He <w>went</w> to the doctor yesterday.",
                                "ru": "Он вчера <w>ходил</w> к врачу."
                            }
                        },
                        {
                            "text": "Wij <w>zijn</w> vroeg opgestaan om de trein te halen.",
                            "target_lang_translations": {
                                "en": "We <w>got</w> up early to catch the train.",
                                "ru": "Мы рано <w>встали</w>, чтобы успеть на поезд."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Auxiliary verb used with a past participle to form the perfect tense (especially for verbs of motion or change).",
                        "ru": "Вспомогательный глагол, используемый с причастием прошедшего времени для образования перфекта (особенно для глаголов движения или изменения)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "English uses 'have' for the perfect tense where Dutch may use 'zijn'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "вспомогательный глагол (не переводится отдельно, образует прошедшее время)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e4f4de4-56b6-3f15-9a86-372289dea4d2",
                    "sense_definition": "Even ergens naartoe gaan en weer terugkomen.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "go_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>ben</w> even naar de bakker om brood te halen.",
                            "target_lang_translations": {
                                "en": "I <w>am</w> just off to the bakery to get bread.",
                                "ru": "Я <w>схожу</w> ненадолго в булочную за хлебом."
                            }
                        },
                        {
                            "text": "Zij <w>is</w> vandaag naar het strand geweest.",
                            "target_lang_translations": {
                                "en": "She <w>has</w> been to the beach today.",
                                "ru": "Она сегодня <w>была</w> на пляже."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To go somewhere briefly and return; to have visited.",
                        "ru": "Сходить куда-то ненадолго и вернуться; побывать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have been",
                                "target_lang_sense_clarification": "indicates visiting a place and returning"
                            },
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "to go to a place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сходить",
                                "target_lang_sense_clarification": "пойти и вернуться"
                            },
                            {
                                "target_lang_word": "побывать",
                                "target_lang_sense_clarification": "посетить место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bdbe819-139a-379c-a3dd-747b957a60fd",
                    "sense_definition": "Hulpwerkwoord dat samen met een voltooid deelwoord de voltooide tijd van de lijdende vorm uitdrukt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "auxiliary_passive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De muur <w>is</w> gisteren door de schilder geschilderd.",
                            "target_lang_translations": {
                                "en": "The wall <w>was</w> painted by the painter yesterday.",
                                "ru": "Стена <w>была</w> покрашена маляром вчера."
                            }
                        },
                        {
                            "text": "Alle documenten <w>zijn</w> veilig opgeslagen in het archief.",
                            "target_lang_translations": {
                                "en": "All documents <w>are</w> safely stored in the archive.",
                                "ru": "Все документы надежно <w>сохранены</w> в архиве."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Auxiliary verb used with a past participle to express the passive voice.",
                        "ru": "Вспомогательный глагол, используемый с причастием прошедшего времени для выражения страдательного залога."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "forms the passive voice (is/was/been)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "образует пассивную конструкцию (был сделан)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fd1ce28-c8a7-35dd-bb44-c655d4a3e570",
                    "sense_definition": "In combinatie met 'te' en een werkwoord: uitdrukken dat iets mogelijk of noodzakelijk is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "possibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat handschrift <w>is</w> bijna niet te lezen.",
                            "target_lang_translations": {
                                "en": "That handwriting <w>is</w> almost impossible to read.",
                                "ru": "Этот почерк почти <w>невозможно</w> разобрать."
                            }
                        },
                        {
                            "text": "Dit probleem <w>is</w> eenvoudig op te lossen.",
                            "target_lang_translations": {
                                "en": "This problem <w>is</w> easy to solve.",
                                "ru": "Эту проблему <w>легко</w> решить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niet te doen",
                        "af te raden"
                    ],
                    "target_lang_definitions": {
                        "en": "Combined with 'te' (to) and a verb: to express that something is possible or necessary.",
                        "ru": "В сочетании с 'te' и глаголом: выражает возможность или необходимость."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "in constructions like 'is to be seen'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "можно",
                                "target_lang_sense_clarification": "в конструкциях 'можно сделать', 'поддается'"
                            },
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "выражает необходимость"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "fe5f7897-e916-3db9-82ae-4bdbe1e63854",
                    "sense_definition": "Bezittelijk voornaamwoord van de derde persoon enkelvoud; geeft aan dat iets van hem (mannelijk) of van het (onzijdig) is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possessive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De man liet <w>zijn</w> hond uit in het park.",
                            "target_lang_translations": {
                                "en": "The man walked <w>his</w> dog in the park.",
                                "ru": "Мужчина выгуливал <w>свою</w> собаку в парке."
                            }
                        },
                        {
                            "text": "Het boek heeft <w>zijn</w> kaft verloren door het vele gebruik.",
                            "target_lang_translations": {
                                "en": "The book has lost <w>its</w> cover through much use.",
                                "ru": "Книга потеряла <w>свою</w> обложку из-за частого использования."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op zijn gemak",
                        "op zijn kop",
                        "te zijner tijd"
                    ],
                    "target_lang_definitions": {
                        "en": "Possessive pronoun of the third person singular; indicates belonging to him (masculine) or it (neuter).",
                        "ru": "Притяжательное местоимение третьего лица единственного числа; указывает на принадлежность ему (мужской род) или этому (средний род)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "his",
                                "target_lang_sense_clarification": "belonging to a male"
                            },
                            {
                                "target_lang_word": "its",
                                "target_lang_sense_clarification": "belonging to an object or animal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "его",
                                "target_lang_sense_clarification": "принадлежащий ему"
                            },
                            {
                                "target_lang_word": "свой",
                                "target_lang_sense_clarification": "принадлежащий подлежащему (он взял свою книгу)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6b841143-4177-3bb8-9cc5-392b113c2440",
                    "sense_definition": "Het bestaan; de realiteit dat iets of iemand is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "existence_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het menselijk <w>zijn</w> is een complex filosofisch onderwerp.",
                            "target_lang_translations": {
                                "en": "Human <w>existence</w> is a complex philosophical subject.",
                                "ru": "Человеческое <w>бытие</w> — сложная философская тема."
                            }
                        },
                        {
                            "text": "Muziek is een essentieel onderdeel van mijn <w>zijn</w>.",
                            "target_lang_translations": {
                                "en": "Music is an essential part of my <w>being</w>.",
                                "ru": "Музыка — неотъемлемая часть моего <w>существа</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestaan"
                    ],
                    "target_lang_definitions": {
                        "en": "Existence; the reality that something or someone is.",
                        "ru": "Существование; реальность того, что кто-то или что-то есть."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "being",
                                "target_lang_sense_clarification": "nature or essence of a person"
                            },
                            {
                                "target_lang_word": "existence",
                                "target_lang_sense_clarification": "state of existing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бытие",
                                "target_lang_sense_clarification": "философское понятие существования"
                            },
                            {
                                "target_lang_word": "существование",
                                "target_lang_sense_clarification": "факт жизни или наличия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab28d9a6-547a-36ba-a4eb-f5ffa2793272",
                    "sense_definition": "Отделение в кузове автомобиля, предназначенное для размещения и перевозки грузов",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы загрузили все сумки в <w>багажник</w> и поехали на дачу.",
                            "target_lang_translations": {
                                "en": "We loaded all the bags into the <w>trunk</w> and drove to the country house."
                            }
                        },
                        {
                            "text": "Пожалуйста, открой <w>багажник</w>, мне нужно достать куртку.",
                            "target_lang_translations": {
                                "en": "Please open the <w>trunk</w>, I need to get my jacket."
                            }
                        }
                    ],
                    "common_phrases": [
                        "открыть багажник",
                        "положить в багажник",
                        "объём багажника",
                        "багажник машины"
                    ],
                    "target_lang_definitions": {
                        "en": "A compartment in a car, usually at the rear, designed for holding luggage and other cargo"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trunk",
                                "target_lang_sense_clarification": "American English"
                            },
                            {
                                "target_lang_word": "boot",
                                "target_lang_sense_clarification": "British English"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7c90f92-4737-3570-a27e-e142473c4562",
                    "sense_definition": "Металлическая конструкция над колесом велосипеда или мотоцикла для закрепления и перевозки груза",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мальчик вёз своего друга на <w>багажнике</w> велосипеда.",
                            "target_lang_translations": {
                                "en": "The boy was carrying his friend on the bicycle <w>rack</w>."
                            }
                        },
                        {
                            "text": "К <w>багажнику</w> можно прикрепить специальную корзину для продуктов.",
                            "target_lang_translations": {
                                "en": "A special grocery basket can be attached to the <w>rack</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "багажник велосипеда",
                        "везти на багажнике",
                        "закрепить на багажнике"
                    ],
                    "target_lang_definitions": {
                        "en": "A metal frame over the wheel of a bicycle or motorcycle for carrying loads"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rack",
                                "target_lang_sense_clarification": "bicycle or motorcycle"
                            },
                            {
                                "target_lang_word": "carrier",
                                "target_lang_sense_clarification": "luggage carrier"
                            }
                        ]
                    }
                }
            ]
        }

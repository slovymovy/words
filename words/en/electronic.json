                    ],
                    "synonyms": [
                        "computerized",
                        "automated"
                    ],
                    "antonyms": [
                        "mechanical",
                        "manual"
                    ],
                    "common_phrases": [
                        "electronic device",
                        "electronic equipment",
                        "electronic components"
                    ],
                    "target_lang_definitions": {

            "senses": [
                {
                    "sense_id": "39ca854f-71d3-3cad-a141-d2c0c1c0f73a",
                    "sense_definition": "The capital and largest city of Sweden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geo_location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We are planning a weekend trip to <w>Stockholm</w> to visit the museums.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку на выходные в <w>Стокгольм</w>, чтобы посетить музеи."
                            }
                        },
                        {
                            "text": "Winters in <w>Stockholm</w> are known for being very dark and cold.",
                            "target_lang_translations": {
                                "ru": "Зимы в <w>Стокгольме</w> известны тем, что они очень темные и холодные."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Stockholm syndrome",
                        "Stockholm archipelago",
                        "city of Stockholm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Столица и крупнейший город Швеции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Стокгольм",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ad7632c-87b4-397a-b90c-ad4b2e830183",
                    "sense_definition": "A county in Sweden that includes the capital city.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geo_location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The healthcare system in <w>Stockholm</w> county serves over two million people.",
                            "target_lang_translations": {
                                "ru": "Система здравоохранения в лене <w>Стокгольм</w> обслуживает более двух миллионов человек."
                            }
                        },
                        {
                            "text": "She lives in a small town in the northern part of <w>Stockholm</w>.",
                            "target_lang_translations": {
                                "ru": "Она живет в маленьком городке в северной части <w>Стокгольма</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лен (административный округ) в Швеции, включающий столицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Стокгольм",
                                "target_lang_sense_clarification": "лен / регион"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43bf9f70-a173-3d33-9bb5-538e81819399",
                    "sense_definition": "The government of Sweden, referred to by the name of its capital.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "<w>Stockholm</w> has announced new environmental regulations for the coming year.",
                            "target_lang_translations": {
                                "ru": "<w>Стокгольм</w> объявил о новых экологических нормах на предстоящий год."
                            }
                        },
                        {
                            "text": "Negotiations between Brussels and <w>Stockholm</w> continued late into the night.",
                            "target_lang_translations": {
                                "ru": "Переговоры между Брюсселем и <w>Стокгольмом</w> продолжались до глубокой ночи."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Правительство Швеции (употребляется как метонимия)."

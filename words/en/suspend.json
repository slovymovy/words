{
    "word_family": [
        "suspension",
        "suspense",
        "suspender",
        "suspensive",
        "suspensory"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "020491d7-6933-3c22-8a9d-f1b0f0d1ab66",
                    "sense_definition": "To stop something from happening or being active for a short time.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The construction work was <w>suspended</w> because of the heavy rain.",
                            "target_lang_translations": {
                                "ru": "Строительные работы были <w>приостановлены</w> из-за сильного дождя."
                            }
                        },
                        {
                            "text": "They agreed to <w>suspend</w> operations until the safety check was complete.",
                            "target_lang_translations": {
                                "ru": "Они договорились <w>приостановить</w> деятельность до завершения проверки безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "pause",
                        "interrupt",
                        "postpone",
                        "delay"
                    ],
                    "antonyms": [
                        "resume"
                    ],
                    "common_phrases": [
                        "suspend operations",
                        "suspend a meeting",
                        "suspend services"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остановить что-либо или прекратить активность на короткое время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приостанавливать",
                                "target_lang_sense_clarification": "временно прекращать действие"
                            },
                            {
                                "target_lang_word": "приостановить",
                                "target_lang_sense_clarification": "совершенный вид, сделать паузу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5df83b27-eab4-3cce-ab81-d6c282634eb8",
                    "sense_definition": "To officially prevent someone from going to work or school for a period of time, usually as a punishment.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school had to <w>suspend</w> the student for breaking the rules.",
                            "target_lang_translations": {
                                "ru": "Школе пришлось временно <w>отстранить</w> ученика от занятий за нарушение правил."
                            }
                        },
                        {
                            "text": "The police officer was <w>suspended</w> from duty pending an investigation.",
                            "target_lang_translations": {
                                "ru": "Полицейский был <w>отстранён</w> от исполнения служебных обязанностей на время расследования."
                            }
                        }
                    ],
                    "synonyms": [
                        "bar",
                        "exclude",
                        "remove"
                    ],
                    "common_phrases": [
                        "suspend a student",
                        "suspend from duty",
                        "suspend a license"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально запретить кому-либо посещать работу или школу на определенный срок, обычно в качестве наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отстранять",
                                "target_lang_sense_clarification": "от работы или должности"
                            },
                            {
                                "target_lang_word": "временно исключать",
                                "target_lang_sense_clarification": "из школы или организации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59670a5a-c1f7-3681-856a-2ad040fa4168",
                    "sense_definition": "To hang something from a high position so that it is free on all sides except at the point of support.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A beautiful lamp was <w>suspended</w> from the ceiling of the dining room.",
                            "target_lang_translations": {
                                "ru": "Красивая лампа была <w>подвешена</w> к потолку столовой."
                            }
                        },
                        {
                            "text": "The bridge is <w>suspended</w> by thick steel cables.",
                            "target_lang_translations": {
                                "ru": "Мост <w>подвешен</w> на толстых стальных тросах."
                            }
                        }
                    ],
                    "synonyms": [
                        "hang",
                        "dangle"
                    ],
                    "common_phrases": [
                        "suspend from the ceiling",
                        "suspend in the air"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вешать что-либо с высокой позиции так, чтобы оно свободно свисало."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвешивать",
                                "target_lang_sense_clarification": "крепить в висячем положении"
                            },
                            {
                                "target_lang_word": "повесить",
                                "target_lang_sense_clarification": "закрепить наверху"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15d31841-dbac-3aca-8fcc-3f4dea42b9df",
                    "sense_definition": "To delay forming an opinion or making a decision until more information is available.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>suspend</w> judgment until you have heard all the facts.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>воздержитесь</w> от суждений, пока не услышите все факты."
                            }
                        },
                        {
                            "text": "To enjoy the movie, you have to <w>suspend</w> disbelief and accept the magic.",
                            "target_lang_translations": {
                                "ru": "Чтобы получить удовольствие от фильма, нужно <w>отбросить</w> недоверие и принять магию."
                            }
                        }
                    ],
                    "synonyms": [
                        "defer",
                        "withhold"
                    ],
                    "common_phrases": [
                        "suspend judgment",
                        "suspend disbelief"
                    ],
                    "target_lang_definitions": {
                        "ru": "Откладывать принятие решения или формирование мнения до появления новой информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "переносить на более поздний срок"
                            },
                            {
                                "target_lang_word": "воздерживаться",
                                "target_lang_sense_clarification": "от суждений или выводов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3d977a0-da7f-3940-bd4c-ab25026631e2",
                    "sense_definition": "To make solid particles float in a liquid or gas without dissolving.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Shake the mixture well to <w>suspend</w> the powder in the liquid.",
                            "target_lang_translations": {
                                "ru": "Хорошо взболтайте смесь, чтобы <w>распределить</w> порошок в жидкости."
                            }
                        },
                        {
                            "text": "Dust can remain <w>suspended</w> in the air for a long time.",
                            "target_lang_translations": {
                                "ru": "Пыль может долго оставаться <w>взвешенной</w> в воздухе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "suspend in water",
                        "suspend particles"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять твердые частицы распределяться в жидкости или газе, не растворяясь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распределять",
                                "target_lang_sense_clarification": "равномерно смешивать частицы в среде"
                            },
                            {
                                "target_lang_word": "приводить во взвешенное состояние",
                                "target_lang_sense_clarification": "химический/физический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
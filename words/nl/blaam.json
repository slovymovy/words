                {
                    "sense_id": "dd9f6f26-a59d-3efa-b252-8fda0b4c2c28",
                    "sense_definition": "De verantwoordelijkheid of schuld voor een fout of verkeerde handeling; een uiting van afkeuring.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De minister treft geen enkele <w>blaam</w> voor de gemaakte fouten.",
                            "target_lang_translations": {
                                "ru": "Министр не несет никакой <w>вины</w> за допущенные ошибки.",
                                "en": "The minister bears no <w>blame</w> whatsoever for the mistakes made."
                            }
                        },
                        {
                            "text": "Zij probeerden de <w>blaam</w> op hun collega's te schuiven.",
                            "target_lang_translations": {
                                "ru": "Они пытались переложить <w>вину</w> на своих коллег.",
                                "en": "They tried to shift the <w>blame</w> onto their colleagues."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwijt",
                        "schuld",
                        "afkeuring"
                    ],
                    "antonyms": [
                        "lof"
                    ],
                    "common_phrases": [
                        "blaam treffen",
                        "vrij van blaam",
                        "blaam werpen op"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ответственность за проступок или выражение неодобрения.",
                        "en": "Responsibility or guilt for a mistake or wrong action; an expression of disapproval."
                    },
                    "translations": {
                        "ru": [
                            {

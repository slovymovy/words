        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4fa1f8e2-a0b8-38a2-b3ac-ed418282f597",
                    "sense_definition": "Silent; not speaking.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "silence_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to keep <w>mum</w> about the surprise party.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>помалкивать</w> о вечеринке-сюрпризе."
                            }
                        },
                        {
                            "text": "The witness remained <w>mum</w> when asked about the incident.",
                            "target_lang_translations": {
                                "ru": "Свидетель оставался <w>безмолвным</w>, когда его спросили об инциденте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "keep mum",
                        "stay mum"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "idiomatic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Молчаливый; не говорящий ни слова."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молчаливый",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "безмолвный",
                                "target_lang_sense_clarification": "книжное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "70a00b49-52a0-3de9-b3e1-e40e5d7358d9",
                    "sense_definition": "To act in a pantomime or disguise oneself for entertainment.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villagers went out to <w>mum</w> during the winter festival.",
                            "target_lang_translations": {
                                "ru": "Деревенские жители вышли <w>рядиться</w> во время зимнего праздника."
                            }
                        },
                        {
                            "text": "They <w>mummed</w> in colorful costumes and masks.",
                            "target_lang_translations": {
                                "ru": "Они <w>разыгрывали пантомиму</w> в красочных костюмах и масках."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Играть в пантомиме или переодеваться для развлечения (рядиться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рядиться",
                                "target_lang_sense_clarification": "надевать маскарадные костюмы"
                            },
                            {
                                "target_lang_word": "играть в пантомиме",
                                "target_lang_sense_clarification": "выступать без слов"
                            }
                        ]
                    }
                }
            ]
        },
        {

                {
                    "sense_id": "5ed12601-17a8-317c-be6c-e0e9b5d643a4",
                    "sense_definition": "A river in southwestern France, which flows into the Dordogne.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rivers",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "We went kayaking on the <w>Isle</w> while visiting France.",
                            "target_lang_translations": {

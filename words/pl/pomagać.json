    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c1a09f77-b74f-3b18-844e-281d655a4908",
                    "sense_definition": "Ułatwiać komuś wykonanie jakiejś czynności lub pracy poprzez osobisty udział w niej lub dostarczenie potrzebnych środków.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "wsparcie_działanie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój starszy brat często <w>pomaga</w> mi w odrabianiu trudnych lekcji.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат часто <w>помогает</w> мне делать трудные уроки."
                            }
                        },
                        {
                            "text": "Rodzice chętnie <w>pomagają</w> swoim dzieciom w nauce czytania i pisania.",
                            "target_lang_translations": {
                                "ru": "Родители охотно <w>помогают</w> своим детям учиться читать и писать."
                            }
                        }
                    ],
                    "synonyms": [
                        "ułatwiać",
                        "wspierać",
                        "asystować"
                    ],
                    "antonyms": [
                        "przeszkadzać",
                        "utrudniać"
                    ],
                    "common_phrases": [
                        "pomagać w domu",
                        "pomagać w nauce",
                        "chętnie pomagać",
                        "wzajemnie sobie pomagać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содействовать кому-либо в выполнении работы или действий, принимая в них личное участие или предоставляя средства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помогать",
                                "target_lang_sense_clarification": "содействовать в делах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "108eced2-8eff-38ca-b7e4-f833d48fb03b",
                    "sense_definition": "Wspierać kogoś w trudnej sytuacji życiowej lub materialnej, ratować z opresji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "wsparcie_działanie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu wolontariuszy <w>pomaga</w> bezdomnym zwierzętom w lokalnym schronisku.",
                            "target_lang_translations": {
                                "ru": "Многие волонтёры <w>помогают</w> бездомным животным в местном приюте."
                            }
                        },
                        {
                            "text": "Należy <w>pomagać</w> ludziom, którzy nagle znaleźli się w potrzebie.",
                            "target_lang_translations": {
                                "ru": "Нужно <w>помогать</w> людям, которые внезапно оказались в беде."
                            }
                        }
                    ],
                    "synonyms": [
                        "ratować",
                        "nieść pomoc",
                        "wspierać"
                    ],
                    "antonyms": [
                        "zostawiać samemu sobie"
                    ],
                    "common_phrases": [
                        "pomagać biednym",
                        "pomagać innym",
                        "pomagać finansowo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддерживать кого-либо в трудной жизненной или материальной ситуации, спасать из беды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помогать",
                                "target_lang_sense_clarification": "оказывать поддержку"
                            },
                            {
                                "target_lang_word": "поддерживать",
                                "target_lang_sense_clarification": "морально или материально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f72d743-d688-31b2-8a9b-cea03a5c9266",
                    "sense_definition": "Przynosić ulgę w chorobie, usuwać ból lub być skutecznym środkiem na jakąś dolegliwość.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "lecznictwo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta maść ziołowa świetnie <w>pomaga</w> na bóle mięśni i stawów.",
                            "target_lang_translations": {
                                "ru": "Эта травяная мазь отлично <w>помогает</w> от боли в мышцах и суставах."
                            }
                        },
                        {
                            "text": "Gorąca herbata z miodem i cytryną zwykle <w>pomaga</w> na przeziębienie.",
                            "target_lang_translations": {
                                "ru": "Горячий чай с мёдом и лимоном обычно <w>помогает</w> от простуды."
                            }
                        }
                    ],
                    "synonyms": [
                        "działać",
                        "być skutecznym"
                    ],
                    "antonyms": [
                        "szkodzić",
                        "pogarszać stan"
                    ],
                    "common_phrases": [
                        "pomagać na ból",
                        "pomagać na stres",
                        "to nie pomaga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приносить облегчение при болезни, устранять боль или быть эффективным средством от недуга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помогать",
                                "target_lang_sense_clarification": "приносить облегчение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
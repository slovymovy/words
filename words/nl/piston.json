    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0dc75eab-2e99-36e5-99ff-b8ce49c6455b",
                    "sense_definition": "Een koperen blaasinstrument met ventielen, dat lijkt op een kleine trompet.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "music_instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij speelt al jaren <w>piston</w> in de lokale harmonie.",
                            "target_lang_translations": {
                                "en": "He has been playing the <w>cornet</w> in the local wind band for years."
                            }
                        },
                        {
                            "text": "De klank van een <w>piston</w> is iets zachter dan die van een trompet.",
                            "target_lang_translations": {
                                "en": "The sound of a <w>cornet</w> is slightly softer than that of a trumpet."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op de piston spelen",
                        "een solo voor piston"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A brass instrument with valves, resembling a small trumpet."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cornet",
                                "target_lang_sense_clarification": "musical instrument"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88632f1e-bbf4-39f8-8791-8b5b6d5c923f",
                    "sense_definition": "Een cilindrisch onderdeel dat heen en weer beweegt in een motor of pomp om druk op te bouwen of kracht over te brengen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mechanics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De monteur moest de versleten <w>piston</w> in de motor vervangen.",
                            "target_lang_translations": {
                                "en": "The mechanic had to replace the worn <w>piston</w> in the engine."
                            }
                        },
                        {
                            "text": "Als er geen olie meer is, kan de <w>piston</w> vastlopen in de cilinder.",
                            "target_lang_translations": {
                                "en": "If there is no oil left, the <w>piston</w> can seize up in the cylinder."
                            }
                        }
                    ],
                    "synonyms": [
                        "zuiger"
                    ],
                    "common_phrases": [
                        "de piston vervangen",
                        "een vastgelopen piston"
                    ],
                    "target_lang_definitions": {
                        "en": "A cylindrical part moving back and forth in an engine or pump to build pressure or transfer force."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "piston",
                                "target_lang_sense_clarification": "mechanical part"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5145244-89e1-39ce-bc5a-9729228b091b",
                    "sense_definition": "Het indrukbare ventiel op een koperen blaasinstrument waarmee de toonhoogte wordt gewijzigd.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de <w>pistons</w> van je instrument regelmatig oliën zodat ze soepel blijven bewegen.",
                            "target_lang_translations": {
                                "en": "You must oil the <w>valves</w> of your instrument regularly so they keep moving smoothly."
                            }
                        },
                        {
                            "text": "Het middelste <w>piston</w> bleef hangen tijdens het concert.",
                            "target_lang_translations": {
                                "en": "The middle <w>valve</w> got stuck during the concert."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de pistons oliën",
                        "drie pistons"
                    ],
                    "target_lang_definitions": {
                        "en": "The pressable valve on a brass instrument that changes the pitch."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "valve",
                                "target_lang_sense_clarification": "instrument part"
                            },
                            {
                                "target_lang_word": "piston valve",
                                "target_lang_sense_clarification": "technical term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0689d14e-3524-307d-8878-f06bbb69c7e9",
                    "sense_definition": "Iemand die zijn invloed of relaties gebruikt om een ander aan een voordeel of positie te helpen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zonder een stevige <w>piston</w> is het moeilijk om die baan te krijgen.",
                            "target_lang_translations": {
                                "en": "Without a strong <w>connection</w>, it is difficult to get that job."
                            }
                        },
                        {
                            "text": "Hij had een goede <w>piston</w> bij de directie en werd snel gepromoveerd.",
                            "target_lang_translations": {
                                "en": "He had a good <w>connection</w> with the management and was promoted quickly."
                            }
                        }
                    ],
                    "synonyms": [
                        "zuiger"
                    ],
                    "common_phrases": [
                        "een piston hebben",
                        "werken met pistons"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who uses their influence or connections to help another gain an advantage or position."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "connection",
                                "target_lang_sense_clarification": "influential contact"
                            },
                            {
                                "target_lang_word": "string-puller",
                                "target_lang_sense_clarification": "person using influence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0989c80f-b122-36f7-a986-36ef4e508236",

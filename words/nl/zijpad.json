            "senses": [
                {
                    "sense_id": "f6818d44-359b-387f-82bf-0d1164dd2804",
                    "sense_definition": "Een kleiner pad dat aftakt van een groter pad of een hoofdweg.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens onze wandeling kozen we een smal <w>zijpad</w> door het bos.",
                            "target_lang_translations": {
                                "en": "During our walk, we chose a narrow <w>side path</w> through the forest.",
                                "ru": "Во время прогулки мы свернули на узкую <w>боковую тропинку</w> в лесу."
                            }
                        },
                        {
                            "text": "Dit <w>zijpad</w> leidt direct naar het strand.",
                            "target_lang_translations": {
                                "en": "This <w>side path</w> leads directly to the beach.",
                                "ru": "Эта <w>боковая дорожка</w> ведет прямо к пляжу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zijweg",
                        "zijweggetje"
                    ],
                    "antonyms": [
                        "hoofdpad"
                    ],
                    "common_phrases": [
                        "een zijpad inslaan",
                        "het zijpad nemen"
                    ],
                    "target_lang_definitions": {
                        "en": "A smaller path that branches off from a larger path or main road.",
                        "ru": "Более мелкая тропа, ответвляющаяся от основной дороги или пути."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "side path",
                                "target_lang_sense_clarification": "literal path"
                            },
                            {
                                "target_lang_word": "byway",
                                "target_lang_sense_clarification": "secondary path or road"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "боковая тропинка",
                                "target_lang_sense_clarification": "узкая дорожка для пешеходов"
                            },
                            {
                                "target_lang_word": "боковая дорожка",
                                "target_lang_sense_clarification": "ответвление от основного пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e36db35-343b-3001-9892-b53c7e9a36cc",
                    "sense_definition": "Een uitweiding die afdwaalt van de hoofdgedachte of het hoofdonderwerp.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De spreker week af van zijn tekst en nam een onverwacht <w>zijpad</w>.",
                            "target_lang_translations": {
                                "en": "The speaker deviated from his text and went off on an unexpected <w>tangent</w>.",
                                "ru": "Оратор отклонился от текста и сделал неожиданное <w>отступление</w>."
                            }
                        },
                        {
                            "text": "Laten we bij de kern blijven en geen onnodig <w>zijpad</w> bewandelen.",
                            "target_lang_translations": {
                                "en": "Let's stick to the point and not go down an unnecessary <w>sidetrack</w>.",
                                "ru": "Давайте придерживаться сути и не делать ненужных <w>отступлений</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zijsprong",
                        "zijweg"
                    ],
                    "common_phrases": [
                        "een zijpad bewandelen",
                        "op een zijpad geraken"
                    ],
                    "target_lang_definitions": {
                        "en": "A digression that strays from the main idea or subject.",
                        "ru": "Отклонение от основной мысли или предмета обсуждения."
                    },
                    "translations": {

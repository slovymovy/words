            "senses": [
                {
                    "sense_id": "e9474a68-528c-3ff6-96af-b28df6c31d93",
                    "sense_definition": "Заботливым уходом добиваться выздоровления больного или раненого",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама долго <w>выхаживала</w> меня, когда я заболел тяжёлым гриппом.",
                            "target_lang_translations": {
                                "en": "Mom <w>nursed</w> me <w>back to health</w> for a long time when I came down with a bad flu."
                            }
                        },
                        {
                            "text": "Врачи сутками <w>выхаживали</w> пострадавших после аварии.",
                            "target_lang_translations": {
                                "en": "Doctors spent days <w>nursing</w> the victims after the accident."
                            }
                        }
                    ],
                    "synonyms": [
                        "вылечивать",
                        "излечивать",
                        "исцелять"
                    ],
                    "antonyms": [
                        "гробить"
                    ],
                    "common_phrases": [
                        "выхаживать больного",
                        "выхаживать раненого",
                        "долго выхаживать"
                    ],
                    "target_lang_definitions": {
                        "en": "To restore a sick or wounded person to health through careful care and nursing"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nurse",
                                "target_lang_sense_clarification": "restore to health"
                            },
                            {
                                "target_lang_word": "care for",
                                "target_lang_sense_clarification": "look after someone sick"
                            },
                            {
                                "target_lang_word": "tend",
                                "target_lang_sense_clarification": "attend to a patient"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c94bbe8c-a00c-3eb1-8bca-a268662f9d8a",
                    "sense_definition": "Заботливо выращивать, воспитывать (обычно о детёнышах животных, птенцах или растениях)",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Птицы вместе строят гнездо и <w>выхаживают</w> своих птенцов.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a68633f6-3793-3f73-b145-7767c14c01b9",
                    "sense_definition": "Помещать тело умершего в землю или в гробницу, совершать обряд погребения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родственники решили <хоронить> дедушку на старом деревенском кладбище.",
                            "target_lang_translations": {
                                "en": "The relatives decided to <bury> grandfather in the old village cemetery."
                            }
                        },
                        {
                            "text": "В древние времена воинов было принято <хоронить> вместе с их оружием.",
                            "target_lang_translations": {
                                "en": "In ancient times, it was customary to <bury> warriors together with their weapons."
                            }
                        }
                    ],
                    "synonyms": [
                        "погребать",
                        "предавать земле"
                    ],
                    "antonyms": [
                        "выкапывать",
                        "эксгумировать"
                    ],
                    "common_phrases": [
                        "хоронить заживо",
                        "хоронить с почестями",
                        "хоронить на кладбище"
                    ],
                    "target_lang_definitions": {
                        "en": "To place the body of a deceased person into the ground or a tomb; to perform a burial rite."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bury",
                                "target_lang_sense_clarification": "to place a dead body in the ground"
                            },
                            {
                                "target_lang_word": "inter",
                                "target_lang_sense_clarification": "formal; to place in a grave or tomb"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a627889-43d0-3e64-b343-e92369a850fd",
                    "sense_definition": "Считать что-либо утратившим значение, исчезнувшим, несостоятельным или навсегда ушедшим в прошлое.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не стоит раньше времени <хоронить> этот проект, у нас ещё есть шанс на успех.",
                            "target_lang_translations": {
                                "en": "We shouldn't <bury> this project prematurely; we still have a chance of success."
                            }
                        },
                        {
                            "text": "После первой неудачи он начал <хоронить> свою мечту стать музыкантом.",
                            "target_lang_translations": {
                                "en": "After the first failure, he began to <bury> his dream of becoming a musician."
                            }
                        }
                    ],
                    "synonyms": [
                        "списывать"
                    ],
                    "antonyms": [
                        "воскрешать",
                        "оживлять"
                    ],
                    "common_phrases": [
                        "хоронить надежду",
                        "хоронить талант",
                        "хоронить идею",
                        "рано хоронить"
                    ],
                    "target_lang_definitions": {
                        "en": "To consider something to have lost its value, failed, or to be gone forever; to give up on something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bury",
                                "target_lang_sense_clarification": "figurative; to put an end to or abandon an idea or hope"
                            },
                            {
                                "target_lang_word": "write off",
                                "target_lang_sense_clarification": "to decide that something is a failure or lost cause"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "054d3df0-d241-3a42-93a2-dbe65101ec6f",
                    "sense_definition": "Прятать, скрывать что-либо в надёжном месте, убирать с глаз.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старуха любила <хоронить> свои сбережения в старом сундуке под замком.",
                            "target_lang_translations": {
                                "en": "The old woman loved to <hide> her savings in an old chest under lock and key."
                            }
                        },
                        {
                            "text": "Густой туман начал <хоронить> очертания далёких гор.",
                            "target_lang_translations": {
                                "en": "Thick fog began to <bury> the outlines of the distant mountains."
                            }
                        }
                    ],
                    "synonyms": [
                        "погребать",
                        "предавать земле"
                    ],
                    "common_phrases": [
                        "хоронить взгляд",
                        "хоронить в памяти"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "poetic"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To hide or conceal something in a secure place; to remove from sight."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hide",
                                "target_lang_sense_clarification": "to put something where it cannot be seen"
                            },
                            {
                                "target_lang_word": "bury",
                                "target_lang_sense_clarification": "to cover or conceal deeply"
                            },
                            {
                                "target_lang_word": "conceal",
                                "target_lang_sense_clarification": "formal; to keep from sight"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2d7aa01c-675a-3a8e-b467-934ab012e7cf",
                    "sense_definition": "A category of things or people that share common features or characteristics.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "noun_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "What <w>sort</w> of music do you like to listen to?",
                            "target_lang_translations": {
                                "ru": "Какой <w>тип</w> музыки вам нравится слушать?"
                            }
                        },
                        {
                            "text": "This shop sells every <w>sort</w> of cheese imaginable.",
                            "target_lang_translations": {
                                "ru": "Этот магазин продает любой <w>сорт</w> сыра, какой только можно вообразить."
                            }
                        },
                        {
                            "text": "He is the <w>sort</w> of person who always helps others.",
                            "target_lang_translations": {
                                "ru": "Он из того <w>сорта</w> людей, которые всегда помогают другим."
                            }
                        }
                    ],
                    "synonyms": [
                        "kind",
                        "type",
                        "variety",
                        "genre"
                    ],
                    "common_phrases": [
                        "sort of",
                        "all sorts of",
                        "of a sort",
                        "nothing of the sort"
                    ],
                    "target_lang_definitions": {
                        "ru": "Категория вещей или людей, которые имеют общие черты или характеристики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "общая разновидность"
                            },
                            {
                                "target_lang_word": "сорт",
                                "target_lang_sense_clarification": "качество товара или продукта"
                            },
                            {
                                "target_lang_word": "тип",
                                "target_lang_sense_clarification": "класс или категория"
                            },
                            {
                                "target_lang_word": "разновидность",
                                "target_lang_sense_clarification": "вариация чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29db45a6-2425-3793-8d50-085826af7c91",
                    "sense_definition": "An act of organizing or arranging things.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to have a good <w>sort</w> of my clothes this weekend.",
                            "target_lang_translations": {
                                "ru": "Мне нужно устроить хороший <w>разбор</w> своей одежды в эти выходные."
                            }
                        },
                        {
                            "text": "Give the files a quick <w>sort</w> before you leave.",
                            "target_lang_translations": {
                                "ru": "Сделай быструю <w>сортировку</w> файлов перед уходом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по организации или приведению вещей в порядок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сортировка",
                                "target_lang_sense_clarification": "процесс распределения"
                            },
                            {
                                "target_lang_word": "разбор",
                                "target_lang_sense_clarification": "наведение порядка в вещах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adabe85a-e36a-3799-a29e-e65daec4591f",
                    "sense_definition": "A person with a specific character or nature.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a decent <w>sort</w> who will always lend a hand.",
                            "target_lang_translations": {
                                "ru": "Он порядочный <w>человек</w>, который всегда поможет."
                            }
                        },
                        {
                            "text": "My neighbor is a good <w>sort</w>, despite being a bit noisy.",
                            "target_lang_translations": {
                                "ru": "Мой сосед — хороший <w>человек</w>, несмотря на то, что немного шумный."
                            }
                        }
                    ],
                    "synonyms": [
                        "character",
                        "individual"
                    ],
                    "common_phrases": [
                        "good sort",
                        "bad sort",
                        "decent sort"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек с определенным характером или натурой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "человек",
                                "target_lang_sense_clarification": "личность (в контексте характера)"
                            },
                            {
                                "target_lang_word": "тип",
                                "target_lang_sense_clarification": "разг., субъект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bb1d5f7-9327-36a7-a20c-916c1d02066f",
                    "sense_definition": "An algorithm used to arrange data in a specific order.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bubble <w>sort</w> is simple but often inefficient for large lists.",
                            "target_lang_translations": {
                                "ru": "Пузырьковая <w>сортировка</w> проста, но часто неэффективна для больших списков."
                            }
                        },
                        {
                            "text": "We implemented a merge <w>sort</w> to organize the database records.",
                            "target_lang_translations": {
                                "ru": "Мы внедрили <w>сортировку</w> слиянием для организации записей в базе данных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bubble sort",
                        "merge sort",
                        "quick sort"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Алгоритм, используемый для упорядочивания данных в определенном порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сортировка",
                                "target_lang_sense_clarification": "алгоритм"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0f0fad7e-2765-3b39-becc-e5659aceb17b",
                    "sense_definition": "To separate items into different groups based on shared characteristics.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_categorize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>sort</w> the recycling into glass, paper, and plastic.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>рассортируйте</w> вторсырье на стекло, бумагу и пластик."
                            }
                        },
                        {
                            "text": "She <w>sorted</w> the laundry into whites and colors before washing.",
                            "target_lang_translations": {
                                "ru": "Она <w>рассортировала</w> белье на белое и цветное перед стиркой."
                            }
                        },
                        {
                            "text": "We need to <w>sort</w> through these old boxes in the attic.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>разобрать</w> эти старые коробки на чердаке."
                            }
                        }
                    ],
                    "synonyms": [
                        "categorize",
                        "classify",
                        "group",
                        "class"
                    ],
                    "antonyms": [
                        "mix",
                        "jumble"
                    ],
                    "common_phrases": [
                        "sort through",
                        "sort into",
                        "sort out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разделять предметы на разные группы на основе общих характеристик."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сортировать",
                                "target_lang_sense_clarification": "распределять по группам"
                            },
                            {
                                "target_lang_word": "разбирать",
                                "target_lang_sense_clarification": "раскладывать вещи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e7ed4b2-5ea0-378c-af91-aa23c8344e0a",
                    "sense_definition": "To arrange things in a specific sequence, such as alphabetically or numerically.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The computer program <w>sorts</w> the files by date modified.",
                            "target_lang_translations": {
                                "ru": "Компьютерная программа <w>сортирует</w> файлы по дате изменения."
                            }
                        },
                        {
                            "text": "You should <w>sort</w> the index cards alphabetically by author name.",
                            "target_lang_translations": {
                                "ru": "Тебе следует <w>отсортировать</w> карточки в алфавитном порядке по имени автора."
                            }
                        }
                    ],
                    "synonyms": [
                        "order",
                        "rank"
                    ],
                    "antonyms": [
                        "scramble"
                    ],
                    "common_phrases": [
                        "sort by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Располагать вещи в определенной последовательности, например, по алфавиту или по номерам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сортировать",
                                "target_lang_sense_clarification": "упорядочивать данные"
                            },
                            {
                                "target_lang_word": "упорядочивать",
                                "target_lang_sense_clarification": "выстраивать в порядок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bf00d0e-cc0a-31c3-8262-40884edf92bc",
                    "sense_definition": "To fix a problem, handle a task, or organize something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_fix",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>sort</w> my travel arrangements for the conference.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>уладить</w> организацию поездки на конференцию."
                            }
                        },
                        {
                            "text": "Don't worry about the broken lock, I will <w>sort</w> it tomorrow.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся о сломанном замке, я <w>разберусь</w> с этим завтра."
                            }
                        }
                    ],
                    "synonyms": [
                        "sort out"
                    ],
                    "common_phrases": [
                        "get sorted",
                        "sort it"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "especially British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Решить проблему, выполнить задачу или организовать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разобраться",
                                "target_lang_sense_clarification": "решить проблему"
                            },
                            {
                                "target_lang_word": "уладить",
                                "target_lang_sense_clarification": "организовать или исправить"
                            }
                        ]
                    }

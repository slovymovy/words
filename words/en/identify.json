{
    "word_family": [
        "identification",
        "identity",
        "identifiable",
        "identical",
        "identifier",
        "unidentified"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "eb7738e3-c244-33f8-8a0e-99dd4195c8eb",
                    "sense_definition": "To recognize someone or something and establish who or what they are.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "recognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The witness was able to <w>identify</w> the thief from a photograph.",
                            "target_lang_translations": {
                                "ru": "Свидетель смог <w>опознать</w> вора по фотографии."
                            }
                        },
                        {
                            "text": "We need to <w>identify</w> the problem before we can fix it.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>определить</w> проблему, прежде чем мы сможем её решить."
                            }
                        }
                    ],
                    "synonyms": [
                        "recognize",
                        "ID"
                    ],
                    "common_phrases": [
                        "identify the cause",
                        "identify the problem",
                        "positively identify"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узнавать кого-то или что-то и устанавливать, кто или что это."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опознать",
                                "target_lang_sense_clarification": "узнать личность (преступника, подозреваемого)"
                            },
                            {
                                "target_lang_word": "определить",
                                "target_lang_sense_clarification": "выявить или установить (проблему, причину)"
                            },
                            {
                                "target_lang_word": "идентифицировать",
                                "target_lang_sense_clarification": "установить тождество (официально/формально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2918b819-f172-351f-a864-6cdd9d131ebc",
                    "sense_definition": "To feel a strong emotional connection or understanding with someone or something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many young readers <w>identify</w> with the main character in the novel.",
                            "target_lang_translations": {
                                "ru": "Многие юные читатели <w>отождествляют себя</w> с главным героем романа."
                            }
                        },
                        {
                            "text": "I find it difficult to <w>identify</w> with his aggressive ambition.",
                            "target_lang_translations": {
                                "ru": "Мне трудно <w>солидаризироваться</w> с его агрессивными амбициями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "identify with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствовать сильную эмоциональную связь или понимание по отношению к кому-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отождествлять себя",
                                "target_lang_sense_clarification": "чувствовать сходство или единство с кем-то"
                            },
                            {
                                "target_lang_word": "солидаризироваться",
                                "target_lang_sense_clarification": "выражать согласие или поддержку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49f7c0cf-b3cf-3924-8f23-443bdcc67809",
                    "sense_definition": "To describe oneself as belonging to a particular group or having a specific characteristic.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The candidate <w>identifies</w> as a progressive on most social issues.",
                            "target_lang_translations": {
                                "ru": "Кандидат <w>идентифицирует себя</w> как прогрессист по большинству социальных вопросов."
                            }
                        },
                        {
                            "text": "More people in the survey <w>identify</w> as vegetarian than ever before.",
                            "target_lang_translations": {
                                "ru": "В опросе больше людей, чем когда-либо, <w>определяют себя</w> как вегетарианцы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "identify as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывать себя как принадлежащего к определенной группе или обладающего определенной характеристикой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идентифицировать себя",
                                "target_lang_sense_clarification": "определять свою принадлежность к группе"
                            },
                            {
                                "target_lang_word": "определять себя",
                                "target_lang_sense_clarification": "давать себе характеристику"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68c72c76-3c90-32e7-909e-0823cf0bce99",
                    "sense_definition": "To consider two different things to be the same or closely connected.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is a mistake to <w>identify</w> wealth with happiness.",
                            "target_lang_translations": {
                                "ru": "Ошибочно <w>отождествлять</w> богатство со счастьем."
                            }
                        },
                        {
                            "text": "He tends to <w>identify</w> the company's success with his own personal achievement.",
                            "target_lang_translations": {
                                "ru": "Он склонен <w>отождествлять</w> успех компании со своими личными достижениями."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать две разные вещи одинаковыми или тесно связанными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отождествлять",
                                "target_lang_sense_clarification": "признавать тождественным, ставить знак равенства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b0a2116-085d-386c-b014-b69f26ea2ac4",
                    "sense_definition": "To establish the taxonomic classification or scientific name of an organism.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "recognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The botanist helped <w>identify</w> the rare plant found in the forest.",
                            "target_lang_translations": {
                                "ru": "Ботаник помог <w>определить</w> редкое растение, найденное в лесу."
                            }
                        },
                        {
                            "text": "Students learned how to <w>identify</w> different species of birds by their calls.",
                            "target_lang_translations": {
                                "ru": "Студенты учились <w>распознавать</w> разные виды птиц по их голосам."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical (biology)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устанавливать таксономическую классификацию или научное название организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "определить",
                                "target_lang_sense_clarification": "установить вид или название (в биологии)"
                            },
                            {
                                "target_lang_word": "идентифицировать",
                                "target_lang_sense_clarification": "распознать объект (научный контекст)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
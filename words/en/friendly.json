        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fc6cc7b3-76e9-3661-a48a-cd14ced9f5db",
                    "sense_definition": "Generally warm, approachable, and easy to relate with in character.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your cat seems very <w>friendly</w> to strangers.",
                            "target_lang_translations": {
                                "ru": "Твоя кошка кажется очень <w>дружелюбной</w> к незнакомцам."
                            }
                        },
                        {
                            "text": "He gave a <w>friendly</w> smile to the new student in class.",
                            "target_lang_translations": {
                                "ru": "Он одарил нового ученика в классе <w>приветливой</w> улыбкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "affectionate",
                        "amiable",
                        "congenial",
                        "cordial",
                        "genial",
                        "hospitable",
                        "welcoming"
                    ],
                    "antonyms": [
                        "unfriendly",
                        "hostile",
                        "standoffish"
                    ],
                    "common_phrases": [
                        "friendly smile",
                        "friendly face",
                        "friendly gesture",
                        "friendly manner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявляющий теплоту, открытость и легкость в общении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дружелюбный",
                                "target_lang_sense_clarification": "расположенный к людям"

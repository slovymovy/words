        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9f193aca-9498-3ea7-81ef-e4cf076cdb89",
                    "sense_definition": "Taki, który minął, odbył się już i należy do przeszłości; poprzedni, odnoszący się do czasu bezpośrednio przed obecnym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <w>zeszłym</w> roku pojechaliśmy na długie wakacje nad morze.",
                            "target_lang_translations": {
                                "ru": "В <w>прошлом</w> году мы поехали в долгий отпуск на море."
                            }
                        },
                        {
                            "text": "Nie pamiętam dokładnie, co robiłem w <w>zeszłą</w> sobotę.",
                            "target_lang_translations": {
                                "ru": "Я не помню точно, что делал в <w>прошлую</w> субботу."
                            }
                        },
                        {
                            "text": "W <w>zeszłym</w> tygodniu w naszej firmie było bardzo dużo pracy.",
                            "target_lang_translations": {
                                "ru": "На <w>прошлой</w> неделе в нашей фирме было очень много работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "miniony",
                        "ubiegły",
                        "poprzedni"
                    ],
                    "antonyms": [
                        "przyszły",
                        "następny"
                    ],
                    "common_phrases": [
                        "zeszły rok",
                        "zeszły tydzień",
                        "zeszły miesiąc",
                        "w zeszłym tygodniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, который прошел, уже состоялся и относится к прошлому; предыдущий, относящийся к периоду времени непосредственно перед настоящим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прошлый",
                                "target_lang_sense_clarification": "предыдущий (год, месяц, неделя)"
                            },
                            {
                                "target_lang_word": "минувший",
                                "target_lang_sense_clarification": "прошедший (более книжное)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b5d28ea2-dcd1-3da7-a1d9-65fdb15ae0a2",
                    "sense_definition": "Trzecia osoba liczby mnogiej rodzaju niemęskoosobowego czasu przeszłego od <w>zejść</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyny <w>zeszły</w> na dół, żeby zjeść wspólne śniadanie.",
                            "target_lang_translations": {
                                "ru": "Девушки <w>спустились</w> вниз, чтобы вместе позавтракать."
                            }
                        },
                        {
                            "text": "Te trudne plamy w końcu <w>zeszły</w> z mojej ulubionej koszuli.",
                            "target_lang_translations": {
                                "ru": "Эти сложные пятна наконец <w>сошли</w> с моей любимой рубашки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czas przeszły"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма третьего лица множественного числа нелично-мужского рода прошедшего времени от глагола <w>zejść</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спустились",
                                "target_lang_sense_clarification": "движение вниз"
                            },
                            {
                                "target_lang_word": "сошли",
                                "target_lang_sense_clarification": "исчезновение (о пятнах) или движение вниз"
                            }
                        ]
                    }
                }
            ]
        }
    ]

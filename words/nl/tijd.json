                {
                    "sense_id": "950bc8b6-586d-317f-87d2-dbc0110453e4",
                    "sense_definition": "De beschikbare hoeveelheid uren of minuten die men heeft om iets te doen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb je even <w>tijd</w> om mij te helpen?",

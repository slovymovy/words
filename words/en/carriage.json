{
    "word_family": [
        "carry",
        "carrier",
        "miscarriage"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "39e5de10-2d56-305a-b808-020f50717ea8",
                    "sense_definition": "A separate section of a train where passengers sit.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vehicle_conveyance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We walked through every <w>carriage</w> looking for a free seat.",
                            "target_lang_translations": {
                                "ru": "Мы прошли через каждый <w>вагон</w> в поисках свободного места."
                            }
                        },
                        {
                            "text": "The railway <w>carriage</w> was quiet as the train moved through the countryside.",
                            "target_lang_translations": {
                                "ru": "В железнодорожном <w>вагоне</w> было тихо, пока поезд ехал по сельской местности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "railway carriage",
                        "first-class carriage",
                        "train carriage"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная секция поезда, предназначенная для размещения пассажиров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вагон",
                                "target_lang_sense_clarification": "часть поезда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02096f53-01cb-3a42-9f7d-d78780f42a3f",
                    "sense_definition": "A vehicle with four wheels, usually pulled by horses, used for transporting people.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vehicle_conveyance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The queen waved to the crowd from her open <w>carriage</w>.",
                            "target_lang_translations": {
                                "ru": "Королева помахала толпе из своей открытой <w>кареты</w>."
                            }
                        },
                        {
                            "text": "Tourists often enjoy a romantic horse-drawn <w>carriage</w> ride through the park.",
                            "target_lang_translations": {
                                "ru": "Туристы часто наслаждаются романтической поездкой в конном <w>экипаже</w> по парку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "horse-drawn carriage",
                        "carriage ride",
                        "ride in a carriage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Четырехколесное транспортное средство, обычно запряженное лошадьми, используемое для перевозки людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карета",
                                "target_lang_sense_clarification": "закрытый пассажирский экипаж"
                            },
                            {
                                "target_lang_word": "экипаж",
                                "target_lang_sense_clarification": "конная повозка"
                            },
                            {
                                "target_lang_word": "повозка",
                                "target_lang_sense_clarification": "общее название транспортного средства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe7f4eab-deae-33ae-8bb4-f72bb10fbf45",
                    "sense_definition": "The money charged for transporting goods.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The total bill includes the cost of <w>carriage</w> and delivery.",
                            "target_lang_translations": {
                                "ru": "Общий счет включает стоимость <w>перевозки</w> и доставки."
                            }
                        },
                        {
                            "text": "Please confirm if <w>carriage</w> is free for this order.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подтвердите, является ли <w>доставка</w> бесплатной для этого заказа."
                            }
                        }
                    ],
                    "synonyms": [
                        "freight",
                        "charge",
                        "cartage"
                    ],
                    "common_phrases": [
                        "carriage paid",
                        "carriage forward",
                        "cost of carriage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная плата, взимаемая за транспортировку товаров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевозка",
                                "target_lang_sense_clarification": "оплата транспортировки"
                            },
                            {
                                "target_lang_word": "доставка",
                                "target_lang_sense_clarification": "услуга по транспортировке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11111ed0-392a-34be-b946-90b9e4ceefa2",
                    "sense_definition": "The way a person stands, moves, or holds their body.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her proud <w>carriage</w> made her stand out in the room.",
                            "target_lang_translations": {
                                "ru": "Ее гордая <w>осанка</w> выделяла ее среди присутствующих в комнате."
                            }
                        },
                        {
                            "text": "He maintained an upright <w>carriage</w> despite his old age.",
                            "target_lang_translations": {
                                "ru": "Он сохранял прямую <w>осанку</w>, несмотря на преклонный возраст."
                            }
                        }
                    ],
                    "common_phrases": [
                        "erect carriage",
                        "upright carriage",
                        "graceful carriage"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Манера человека стоять, двигаться или держать свое тело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осанка",
                                "target_lang_sense_clarification": "манера держать тело"
                            },
                            {
                                "target_lang_word": "выправка",
                                "target_lang_sense_clarification": "подтянутый вид"
                            },
                            {
                                "target_lang_word": "манера держаться",
                                "target_lang_sense_clarification": "общий внешний вид и поведение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "5bb95b35-a809-3db4-8755-e74ff85c75bf",
                    "sense_definition": "An informal term for <w>father</w>, dad, or papa.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "family_members",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>pa</w> taught me how to ride a bike when I was small.",
                            "target_lang_translations": {
                                "ru": "Мой <w>папа</w> научил меня кататься на велосипеде, когда я был маленьким."
                            }
                        },
                        {
                            "text": "I need to ask <w>pa</w> if I can borrow the car tonight.",
                            "target_lang_translations": {
                                "ru": "Мне нужно спросить у <w>папы</w>, можно ли мне взять машину сегодня вечером."
                            }
                        }
                    ],
                    "synonyms": [

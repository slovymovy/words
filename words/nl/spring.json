            "senses": [
                {
                    "sense_id": "4a61d576-2a6a-34c8-8a67-548b39193a84",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>springen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>spring</w> over de grote plas water.",
                            "target_lang_translations": {
                                "ru": "Я <w>перепрыгиваю</w> через большую лужу."
                            }
                        },
                        {
                            "text": "Elke ochtend <w>spring</w> ik vrolijk uit mijn bed.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я радостно <w>вскакиваю</w> с постели."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik spring"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени от глагола *springen*."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прыгаю",
                                "target_lang_sense_clarification": "форма глагола 'прыгать' (я)"
                            },
                            {
                                "target_lang_word": "вскакиваю",
                                "target_lang_sense_clarification": "резкое движение (например, с кровати)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c8e9398-ffb4-351e-a8da-37de4232a5c6",
                    "sense_definition": "Gebiedende wijs van <w>springen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Spring</w> maar achterop bij mij op de fiets.",
                            "target_lang_translations": {
                                "ru": "<w>Запрыгивай</w> ко мне на багажник велосипеда."
                            }
                        },
                        {
                            "text": "<w>Spring</w> niet in het diepe gedeelte van het zwembad.",
                            "target_lang_translations": {
                                "ru": "Не <w>прыгай</w> в глубокую часть бассейна."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spring maar",
                        "spring niet"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола *springen*."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прыгай",
                                "target_lang_sense_clarification": "приказ или просьба (ты)"
                            },
                            {
                                "target_lang_word": "запрыгивай",
                                "target_lang_sense_clarification": "приглашение сесть (на велосипед и т.д.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bb51e14-77c7-314e-8821-e40d0d23f95d",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>springen</w> (bij inversie).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waarom <w>spring</w> je niet over het hek?",
                            "target_lang_translations": {
                                "ru": "Почему ты не <w>перепрыгиваешь</w> через забор?"
                            }
                        },
                        {
                            "text": "<w>Spring</w> jij als eerste of zal ik gaan?",
                            "target_lang_translations": {
                                "ru": "Ты <w>прыгаешь</w> первым или мне пойти?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "spring je",
                        "spring jij"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Второе лицо единственного числа настоящего времени от глагола *springen* (при инверсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прыгаешь",
                                "target_lang_sense_clarification": "форма глагола 'прыгать' (ты)"
                            },
                            {
                                "target_lang_word": "перепрыгиваешь",
                                "target_lang_sense_clarification": "преодоление препятствия"
                            }

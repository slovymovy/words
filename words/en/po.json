                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"purchase order\" (заказ на поставку), коммерческого документа, выдаваемого покупателем продавцу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заказ на поставку",
                                "target_lang_sense_clarification": "документ"
                            },
                            {
                                "target_lang_word": "ордер на закупку",
                                "target_lang_sense_clarification": "документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d76c45f5-d1d6-35be-b0ea-40a5b0bfeef3",
                    "sense_definition": "An abbreviation for probation officer, an official who supervises offenders on probation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abbreviations",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has a mandatory meeting with his <w>PO</w> every Tuesday afternoon.",

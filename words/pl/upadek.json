    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0fa96ee-4ae2-3373-98a1-65cc3dfd9ecb",
                    "sense_definition": "Przewrócenie się na ziemię lub podłogę, zwykle na skutek utraty równowagi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_fall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe dziecko płakało, ponieważ jego <w>upadek</w> na twardy chodnik był bolesny.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок плакал, так как его <w>падение</w> на твердый тротуар было болезненным."
                            }
                        },
                        {
                            "text": "Podczas nauki jazdy na łyżwach zaliczyłem niejeden <w>upadek</w>.",
                            "target_lang_translations": {
                                "ru": "Учась кататься на коньках, я пережил не одно <w>падение</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bolesny upadek",
                        "zaliczyć upadek",
                        "chronić przed upadkiem",
                        "przypadkowy upadek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Падение на землю или пол, обычно в результате потери равновесия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падение",
                                "target_lang_sense_clarification": "потеря равновесия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "783502f2-3873-3e53-bfcc-c06c16bd1691",
                    "sense_definition": "Przemieszczenie się obiektu lub osoby z góry na dół pod wpływem grawitacji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_fall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Grawitacja powoduje swobodny <w>upadek</w> wszystkich ciał w próżni.",
                            "target_lang_translations": {
                                "ru": "Гравитация вызывает свободное <w>падение</w> всех тел в вакууме."
                            }
                        },
                        {
                            "text": "Niefortunny <w>upadek</w> ze schodów skończył się dla niego złamaniem nogi.",
                            "target_lang_translations": {
                                "ru": "Неудачное <w>падение</w> с лестницы закончилось для него переломом ноги."
                            }
                        }
                    ],
                    "antonyms": [
                        "wznoszenie"
                    ],
                    "common_phrases": [
                        "upadek z wysokości",
                        "swobodny upadek",
                        "upadek meteorytu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещение объекта или человека сверху вниз под воздействием гравитации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падение",
                                "target_lang_sense_clarification": "с высоты, физическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0c5731a-0572-3209-8abd-afc734771667",
                    "sense_definition": "Utrata dotychczasowej pozycji, majątku lub znaczenia; pogorszenie się stanu czegoś.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_decline",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złe zarządzanie finansami spowodowało szybki <w>upadek</w> jego firmy.",
                            "target_lang_translations": {
                                "ru": "Плохое управление финансами привело к быстрому <w>краху</w> его фирмы."
                            }
                        },
                        {
                            "text": "Wiele czynników złożyło się na powolny <w>upadek</w> rolnictwa w tym regionie.",
                            "target_lang_translations": {
                                "ru": "Многие факторы способствовали медленному <w>упадку</w> сельского хозяйства в этом регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruina",
                        "niepowodzenie",
                        "fiasko",
                        "porażka"
                    ],
                    "antonyms": [
                        "sukces",
                        "wzlot",
                        "zwycięstwo"
                    ],
                    "common_phrases": [
                        "upadek gospodarczy",
                        "chylić się ku upadkowi",
                        "całkowity upadek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утрата прежнего положения, состояния или значения; ухудшение состояния чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упадок",
                                "target_lang_sense_clarification": "ухудшение состояния, регресс"
                            },
                            {
                                "target_lang_word": "крах",
                                "target_lang_sense_clarification": "разорение, провал"
                            },
                            {
                                "target_lang_word": "спад",
                                "target_lang_sense_clarification": "снижение показателей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5459f27c-c614-31cb-afc7-009e329e2c20",
                    "sense_definition": "Ostateczny koniec istnienia państwa, ustroju politycznego, miasta lub organizacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_decline",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Historycy do dziś spierają się o główne przyczyny, które spowodowały <w>upadek</w> Cesarstwa Rzymskiego.",
                            "target_lang_translations": {
                                "ru": "Историки до сих пор спорят о главных причинах, вызвавших <w>падение</w> Римской империи."
                            }
                        },
                        {
                            "text": "<w>Upadek</w> muru berlińskiego stał się symbolem końca zimnej wojny.",
                            "target_lang_translations": {
                                "ru": "<w>Падение</w> Берлинской стены стало символом окончания холодной войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpad",
                        "krach",
                        "klęska",
                        "katastrofa",
                        "zmierzch"
                    ],
                    "antonyms": [
                        "odrodzenie"
                    ],
                    "common_phrases": [
                        "upadek rządu",
                        "upadek komunizmu",
                        "upadek cywilizacji",
                        "upadek Rzymu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окончательное прекращение существования государства, политического строя, города или организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падение",
                                "target_lang_sense_clarification": "города, режима"
                            },
                            {
                                "target_lang_word": "крушение",
                                "target_lang_sense_clarification": "империи, надежд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0268758b-afed-3f95-a5d7-0612f48a271e",
                    "sense_definition": "Sprzeniewierzenie się zasadom moralnym, utrata godności lub cnoty.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "moral_decline",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powieść opisuje stopniowy <w>upadek</w> moralny głównego bohatera, który dla pieniędzy zrobi wszystko.",
                            "target_lang_translations": {
                                "ru": "Роман описывает постепенное моральное <w>падение</w> главного героя, который ради денег готов на всё."
                            }
                        },
                        {
                            "text": "Obserwujemy dzisiaj <w>upadek</w> obyczajów i brak szacunku dla starszych.",
                            "target_lang_translations": {
                                "ru": "Сегодня мы наблюдаем <w>упадок</w> нравов и отсутствие уважения к старшим."
                            }
                        }
                    ],
                    "synonyms": [
                        "zepsucie",
                        "degeneracja",
                        "degrengolada",
                        "stoczenie się",
                        "bagno"
                    ],
                    "antonyms": [
                        "odrodzenie moralne"
                    ],
                    "common_phrases": [
                        "upadek moralny",
                        "upadek obyczajów",
                        "dno i upadek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отступление от моральных принципов, потеря достоинства или добродетели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падение",
                                "target_lang_sense_clarification": "нравственное"
                            },
                            {
                                "target_lang_word": "упадок",
                                "target_lang_sense_clarification": "нравов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

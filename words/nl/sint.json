    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "57385a8a-d433-3763-8838-afd0a7939a5b",
                    "sense_definition": "Een veelgebruikte benaming voor <w>Sinterklaas</w>, de kindervriend die jaarlijks in december wordt gevierd.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sinterklaas_figure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen zingen liedjes omdat de <w>Sint</w> weer in het land is.",
                            "target_lang_translations": {
                                "ru": "Дети поют песни, потому что <w>Синтерклаас</w> снова в стране."
                            }
                        },
                        {
                            "text": "Ieder jaar brengt de <w>Sint</w> cadeautjes voor de kinderen.",
                            "target_lang_translations": {
                                "ru": "Каждый год <w>Синтерклаас</w> приносит детям подарки."
                            }
                        }
                    ],
                    "synonyms": [
                        "Sinterklaas",
                        "goedheiligman"
                    ],
                    "common_phrases": [
                        "de Sint komt",
                        "wachten op de Sint",
                        "dag Sinterklaasje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенное название Синтерклааса, друга детей, праздник которого отмечается ежегодно в декабре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Синтерклаас",
                                "target_lang_sense_clarification": "фольклорный персонаж"
                            },
                            {
                                "target_lang_word": "Святой Николай",
                                "target_lang_sense_clarification": "прообраз Синтерклааса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "253cae8b-d615-395b-a474-19b6ec44bd71",
                    "sense_definition": "Een titel voor een persoon die door de kerk heilig is verklaard, of de persoon zelf.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_saint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel kerken en scholen zijn vernoemd naar een <w>sint</w>.",
                            "target_lang_translations": {
                                "ru": "Многие церкви и школы названы в честь какого-либо <w>святого</w>."
                            }
                        },
                        {
                            "text": "<w>Sint</w> Jozef is een bekende heilige in de katholieke kerk.",
                            "target_lang_translations": {
                                "ru": "<w>Святой</w> Иосиф — известный святой в католической церкви."
                            }
                        }
                    ],
                    "synonyms": [
                        "heilige"
                    ],
                    "common_phrases": [
                        "Sint-Maarten",
                        "Sint-Pieter",
                        "een ware sint"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул человека, причисленного церковью к лику святых, или сам этот человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "святой",
                                "target_lang_sense_clarification": "религиозный титул"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
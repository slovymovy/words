    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f3ded323-8bcd-34da-aeae-b917799b47ee",
                    "sense_definition": "Wydawać ciche, piskliwe i żałosne dźwięki (zazwyczaj o psie), wyrażające ból, strach, zniecierpliwienie lub uległość.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dźwięki_zwierząt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój pies zaczął cicho <skomleć>, gdy zobaczył, że wyciągam smycz na spacer.",
                            "target_lang_translations": {
                                "ru": "Мой пес начал тихо <скулить>, когда увидел, что я достаю поводок для прогулки."
                            }
                        },
                        {
                            "text": "Zranione zwierzę leżało w rowie i żałośnie <skomlało> z bólu.",
                            "target_lang_translations": {
                                "ru": "Раненое животное лежало в канаве и жалобно <скулило> от боли."
                            }
                        }
                    ],
                    "synonyms": [
                        "skowyczeć",
                        "skowytać",
                        "skamleć",
                        "skomlić"
                    ],
                    "common_phrases": [
                        "skomleć z bólu",
                        "skomleć pod drzwiami",
                        "żałośnie skomleć",
                        "pies skomle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать тихие, визгливые и жалобные звуки (обычно о собаке), выражающие боль, страх или нетерпение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скулить",
                                "target_lang_sense_clarification": "издавать жалобные звуки (о животном)"
                            },
                            {
                                "target_lang_word": "визжать",
                                "target_lang_sense_clarification": "издавать резкий, высокий звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4624e709-01cd-3eab-8d37-8a9c6467bde9",
                    "sense_definition": "Prosić o coś w sposób natrętny, płaczliwy i pozbawiony godności; błagać.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "proszenie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie zamierzam <skomleć> o podwyżkę, na którą uczciwie zapracowałem.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь <выпрашивать> повышение, которое честно заработал."
                            }
                        },
                        {
                            "text": "Więzień <skomlał> o litość, ale strażnicy pozostali niewzruszeni na jego błagania.",
                            "target_lang_translations": {
                                "ru": "Заключенный <молил> о пощаде, но охранники остались равнодушны к его мольбам."
                            }
                        }
                    ],
                    "synonyms": [
                        "błagać",
                        "skamleć"
                    ],
                    "common_phrases": [
                        "skomleć o litość",
                        "skomleć o pomoc",
                        "skomleć o wybaczenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Назойливо и плаксиво просить о чем-либо, теряя достоинство; умолять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клянчить",
                                "target_lang_sense_clarification": "назойливо выпрашивать"
                            },
                            {
                                "target_lang_word": "умолять",
                                "target_lang_sense_clarification": "страстно просить, молить"
                            },
                            {
                                "target_lang_word": "скулить",
                                "target_lang_sense_clarification": "перен. жаловаться или униженно просить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
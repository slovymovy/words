                {
                    "sense_id": "c2c956c6-e0c7-3b76-8d16-fff81b27d58d",
                    "sense_definition": "Sąd lub kolegium sędziowskie posiadające szczególne uprawnienia, często rozstrzygające sprawy wagi państwowej lub międzynarodowej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "court_institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj <w>trybunał</w> wydał wyrok w sprawie zgodności nowej ustawy z konstytucją.",
                            "target_lang_translations": {
                                "ru": "Вчера <w>трибунал</w> вынес решение о соответствии нового закона конституции."
                            }
                        },
                        {
                            "text": "Międzynarodowy <w>trybunał</w> w Hadze zajmuje się ściganiem zbrodni wojennych.",
                            "target_lang_translations": {
                                "ru": "Международный <w>трибунал</w> в Гааге занимается преследованием военных преступлений."
                            }
                        }
                    ],
                    "synonyms": [
                        "tryb.",
                        "tryb."
                    ],
                    "common_phrases": [
                        "Trybunał Konstytucyjny",
                        "Trybunał Stanu",
                        "Trybunał Sprawiedliwości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суд или судейская коллегия с особыми полномочиями, часто рассматривающая дела государственной или международной важности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трибунал",
                                "target_lang_sense_clarification": "судебный орган с особыми полномочиями"
                            },
                            {
                                "target_lang_word": "суд",
                                "target_lang_sense_clarification": "в названиях высших государственных судов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "208dbbbf-9fce-3d6f-9a71-17ff49bfc368",
                    "sense_definition": "Sąd apelacyjny dla szlachty funkcjonujący w dawnej Polsce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "court_institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szlachta z całego regionu zjeżdżała się do miasta, gdzie obradował <w>trybunał</w>.",
                            "target_lang_translations": {
                                "ru": "Шляхта со всего региона съезжалась в город, где заседал <w>трибунал</w>."
                            }
                        },
                        {
                            "text": "Historyczny <w>trybunał</w> koronny był najwyższą instancją sądową dla stanu szlacheckiego.",
                            "target_lang_translations": {
                                "ru": "Исторический коронный <w>трибунал</w> был высшей судебной инстанцией для дворянского сословия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Trybunał Koronny",
                        "Trybunał Litewski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Апелляционный суд для шляхты, действовавший в старой Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трибунал",

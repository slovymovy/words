    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "96480dd6-0cd7-3f95-8c98-dc67e049c5e6",
                    "sense_definition": "Sprawić, że ktoś lub coś znajdzie się wewnątrz czegoś, używając siły fizycznej do ciągnięcia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz mocno <w>wciągnąć</w> brzuch, żeby dopiąć te spodnie.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно сильно <w>втянуть</w> живот, чтобы застегнуть эти брюки."
                            }
                        },
                        {
                            "text": "Pomóż mi <w>wciągnąć</w> ten ciężki fotel do pokoju przez wąskie drzwi.",
                            "target_lang_translations": {
                                "ru": "Помоги мне <w>втащить</w> это тяжелое кресло в комнату через узкую дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "wprowadzić",
                        "wtoczyć"
                    ],
                    "antonyms": [
                        "wypchnąć",
                        "wyciągnąć"
                    ],
                    "common_phrases": [
                        "wciągnąć brzuch",
                        "wciągnąć do środka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то или что-то оказаться внутри чего-то, используя физическую силу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "втащить",
                                "target_lang_sense_clarification": "переместить внутрь с усилием (мебель, тяжести)"
                            },
                            {
                                "target_lang_word": "втянуть",
                                "target_lang_sense_clarification": "втянуть живот или переместить внутрь тягой"
                            },
                            {
                                "target_lang_word": "затащить",
                                "target_lang_sense_clarification": "переместить внутрь (разговорное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f4bab47-6410-3c4b-a06f-17825f3a8fac",
                    "sense_definition": "Wdychać powietrze, zapach lub inną substancję do płuc lub nosa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "breathing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pływak głęboko <w>wciągnął</w> powietrze przed skokiem do wody.",
                            "target_lang_translations": {
                                "ru": "Пловец глубоко <w>вдохнул</w> воздух перед прыжком в воду."
                            }
                        },
                        {
                            "text": "Pies głośno <w>wciągnął</w> zapach pieczonego mięsa.",
                            "target_lang_translations": {
                                "ru": "Пес громко <w>втянул</w> запах жареного мяса."
                            }
                        }
                    ],
                    "synonyms": [
                        "wdychać",
                        "zaciągnąć się"
                    ],
                    "antonyms": [
                        "wydychać",
                        "wypuścić"
                    ],
                    "common_phrases": [
                        "wciągnąć powietrze",
                        "wciągnąć nosem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вдыхать воздух, запах или другую субстанцию в легкие или нос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдохнуть",
                                "target_lang_sense_clarification": "воздух или запах"
                            },
                            {
                                "target_lang_word": "втянуть",
                                "target_lang_sense_clarification": "с шумом или усилием (носом)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ece30d9-aa1c-3993-a01a-e3158903dadc",
                    "sense_definition": "Sprawić, że ktoś zaangażuje się w jakąś czynność, sytuację lub nabierze zainteresowania czymś (często wbrew woli lub bardzo intensywnie).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_involvement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta książka tak mnie <w>wciągnęła</w>, że przeczytałem ją w jedną noc.",
                            "target_lang_translations": {
                                "ru": "Эта книга так меня <w>затянула</w>, что я прочитал ее за одну ночь."
                            }
                        },
                        {
                            "text": "Nie daj się <w>wciągnąć</w> w ich podejrzane interesy.",
                            "target_lang_translations": {
                                "ru": "Не дай <w>втянуть</w> себя в их подозрительные дела."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaangażować",
                        "zainteresować",
                        "uwikłać"
                    ],
                    "antonyms": [
                        "zniechęcić",
                        "odrzucić"
                    ],
                    "common_phrases": [
                        "wciągnąć się w rozmowę",
                        "wciągnąć w kłopoty",
                        "wciągnąć w wir"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вовлечь кого-то в действие, ситуацию или сильно заинтересовать чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затянуть",
                                "target_lang_sense_clarification": "сильно заинтересовать (о книге, фильме)"
                            },
                            {
                                "target_lang_word": "вовлечь",
                                "target_lang_sense_clarification": "заставить участвовать"
                            },
                            {
                                "target_lang_word": "втянуть",
                                "target_lang_sense_clarification": "впутать в неприятности или разговор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40caaa76-2a9e-3967-bcfa-e5685b535a97",
                    "sense_definition": "Podnieść coś do góry, zazwyczaj ciągnąc za linę, łańcuch lub używając mechanizmu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hoisting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze uroczyście <w>wciągnęli</w> flagę państwową na maszt.",
                            "target_lang_translations": {
                                "ru": "Солдаты торжественно <w>подняли</w> государственный флаг на мачту."
                            }
                        },
                        {
                            "text": "Musimy <w>wciągnąć</w> żagle, aby statek mógł płynąć szybciej.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>поднять</w> паруса, чтобы корабль мог плыть быстрее."
                            }
                        }
                    ],
                    "synonyms": [
                        "podnieść",
                        "wywindować"
                    ],
                    "antonyms": [
                        "opuścić"
                    ],
                    "common_phrases": [
                        "wciągnąć flagę",
                        "wciągnąć żagle",
                        "wciągnąć na maszt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поднять что-то вверх, обычно с помощью веревки или механизма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поднять",
                                "target_lang_sense_clarification": "флаг, паруса, груз"
                            },
                            {
                                "target_lang_word": "втащить",
                                "target_lang_sense_clarification": "поднять наверх с усилием"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    ],
                    "synonyms": [
                        "типичный",
                        "характерный"
                    ],
                    "common_phrases": [
                        "классический пример",
                        "классическая ошибка",
                        "классический случай",
                        "классический образец"
                    ],
                    "target_lang_definitions": {
                        "en": "Typical or characteristic; happening as is usual in such cases."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "classic",
                                "target_lang_sense_clarification": "typical, characteristic, or a perfect example of something"

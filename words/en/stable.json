                        ]
                    }
                },
                {
                    "sense_id": "9da487f9-b0bd-397f-a6c9-854c916ed54e",
                    "sense_definition": "A group of people, animals (especially racehorses), or products managed or trained by the same person or organization.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The trainer has a large <w>stable</w> of champion racehorses.",
                            "target_lang_translations": {
                                "ru": "У тренера большая <w>конюшня</w> чемпионских скаковых лошадей."
                            }
                        },
                        {
                            "text": "The modeling agency manages a <w>stable</w> of talented young faces.",
                            "target_lang_translations": {
                                "ru": "Модельное агентство управляет <w>группой</w> талантливых молодых лиц."
                            }
                        }
                    ],
                    "synonyms": [
                        "string"
                    ],
                    "common_phrases": [
                        "racing stable",
                        "stable of artists"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей или животных (особенно скаковых лошадей), управляемых одной организацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конюшня",
                                "target_lang_sense_clarification": "спортивная команда или группа лошадей"
                            },
                            {
                                "target_lang_word": "группа",
                                "target_lang_sense_clarification": "коллектив людей"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "016c2f4b-faf2-3c48-9e55-2807907f1037",
                    "sense_definition": "To put or keep a horse in a stable.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_keep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>stabled</w> the horses before the heavy storm began.",
                            "target_lang_translations": {
                                "ru": "Они <w>поставили в конюшню</w> лошадей до того, как началась сильная буря."
                            }
                        },
                        {
                            "text": "The farmer <w>stables</w> his animals during the cold winter months.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>содержит в конюшне</w> своих животных в холодные зимние месяцы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать или содержать лошадь в конюшне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставить в конюшню",
                                "target_lang_sense_clarification": "размещать животное"
                            },
                            {
                                "target_lang_word": "содержать в конюшне",
                                "target_lang_sense_clarification": "обеспечивать уход"
                            }
                        ]

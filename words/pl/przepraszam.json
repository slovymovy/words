                },
                {
                    "sense_id": "9172cadc-2f54-3a9a-9fa2-194ec1524de6",
                    "sense_definition": "Zwrot używany do uprzejmego zwrócenia na siebie uwagi, na przykład przed zadaniem pytania lub prośbą o przejście.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attention_getter",
                    "name_type": "no",
                    "examples": [

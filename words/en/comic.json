    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c0514744-dc10-3be0-8e63-d3c01c3769cc",
                    "sense_definition": "Pertaining to or characteristic of comedy as a literary or dramatic genre.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "humor_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie provides some much-needed <w>comic</w> relief after the sad scenes.",
                            "target_lang_translations": {
                                "ru": "Фильм дает столь необходимую <w>комическую</w> разрядку после грустных сцен."
                            }
                        },
                        {
                            "text": "He is a famous <w>comic</w> actor known for his funny expressions.",
                            "target_lang_translations": {
                                "ru": "Он известный <w>комедийный</w> актер, знаменитый своей смешной мимикой."
                            }
                        }
                    ],
                    "synonyms": [
                        "comedic"
                    ],
                    "antonyms": [
                        "tragic"
                    ],
                    "common_phrases": [
                        "comic relief",
                        "comic opera",
                        "comic actor",
                        "comic timing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к комедии как к литературному или драматическому жанру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комеди́йный",
                                "target_lang_sense_clarification": "относящийся к жанру комедии (фильм, актер)"
                            },
                            {
                                "target_lang_word": "коми́ческий",
                                "target_lang_sense_clarification": "свойственный комедии, используемый в комедии"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c7656997-810d-3fba-9230-db5aab8e42c1",
                    "sense_definition": "Глубокая убеждённость в истинности чего-либо, уверенность в успехе или будущем, не требующая доказательств.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "conviction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его непоколебимая <вера> в победу вдохновляла всю команду.",
                            "target_lang_translations": {
                                "en": "His unshakable <faith> in victory inspired the whole team."
                            }
                        },
                        {
                            "text": "У меня есть твёрдая <вера>, что всё закончится хорошо.",
                            "target_lang_translations": {
                                "en": "I have a firm <belief> that everything will end well."
                            }
                        }
                    ],
                    "synonyms": [
                        "убеждённость",
                        "уверенность"
                    ],
                    "antonyms": [
                        "сомнение",
                        "скепсис",
                        "неверие"
                    ],
                    "common_phrases": [
                        "вера в себя",
                        "вера в лучшее",
                        "вера в успех",
                        "потерять веру"
                    ],
                    "target_lang_definitions": {
                        "en": "Deep conviction in the truth of something, or confidence in success or the future without requiring proof."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "faith",
                                "target_lang_sense_clarification": "strong conviction without proof"
                            },
                            {
                                "target_lang_word": "belief",
                                "target_lang_sense_clarification": "mental acceptance or opinion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06d051dd-b44d-3709-9862-9bf0330ddff3",
                    "sense_definition": "Признание существования Бога, высших сил; система религиозных убеждений.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Истинная <вера> помогает людям пережить самые трудные времена.",
                            "target_lang_translations": {
                                "en": "True <faith> helps people survive the most difficult times."
                            }
                        },
                        {
                            "text": "В этом старом храме люди обретают <веру> и душевный покой.",
                            "target_lang_translations": {
                                "en": "In this old temple, people find <faith> and peace of mind."
                            }
                        }
                    ],
                    "synonyms": [
                        "религия",
                        "вероисповедание"
                    ],
                    "antonyms": [
                        "атеизм",
                        "безверие"
                    ],
                    "common_phrases": [
                        "вера в Бога",
                        "православная вера",
                        "символ веры",
                        "обратиться в веру"
                    ],
                    "target_lang_definitions": {
                        "en": "Belief in the existence of God or higher powers; a system of religious beliefs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "faith",
                                "target_lang_sense_clarification": "spiritual trust or specific religion"
                            },
                            {
                                "target_lang_word": "religion",
                                "target_lang_sense_clarification": "system of worship"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "080da20b-47da-384e-bbea-5d00bae0ac58",
                    "sense_definition": "Чувство доверия к кому-либо, признание чьей-либо честности и надежности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "trust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После того случая у меня больше нет <веры> этому человеку.",
                            "target_lang_translations": {
                                "en": "After that incident, I no longer have <faith> in this person."
                            }
                        },
                        {
                            "text": "Мы приняли его слова на <веру>, не проверяя факты.",
                            "target_lang_translations": {
                                "en": "We took his words on <trust>, without checking the facts."
                            }
                        }
                    ],
                    "synonyms": [
                        "доверие"
                    ],
                    "antonyms": [
                        "недоверие",
                        "подозрение"
                    ],
                    "common_phrases": [
                        "принять на веру",
                        "нет веры кому-то"
                    ],
                    "target_lang_definitions": {
                        "en": "A feeling of trust towards someone; recognition of someone's honesty and reliability."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trust",
                                "target_lang_sense_clarification": "reliance on someone's character"
                            },
                            {
                                "target_lang_word": "faith",
                                "target_lang_sense_clarification": "complete confidence in someone"
                            },
                            {
                                "target_lang_word": "credence",
                                "target_lang_sense_clarification": "acceptance of a claim as true"
                            }
                        ]
                    }
                }
            ]
        },
        {

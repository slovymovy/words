                        {
                            "text": "Use the brass <w>pull</w> to open the drawer.",
                            "target_lang_translations": {
                                "ru": "Используйте латунную <w>ручку</w>, чтобы открыть ящик."
                            }
                        }
                    ],
                    "synonyms": [
                        "handle",
                        "knob",
                        "lever"
                    ],
                    "antonyms": [
                        "button",
                        "push-button"
                    ],
                    "common_phrases": [
                        "zipper pull",
                        "drawer pull",
                        "door pull"

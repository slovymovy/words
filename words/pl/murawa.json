{
    "word_family": [
        "murawowy",
        "murawka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0f13bee8-d9df-3529-9d07-53a347ad6d96",
                    "sense_definition": "Trawiasta nawierzchnia boiska sportowego, na której odbywa się gra.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Piłkarze obu drużyn wybiegli na <w>murawę</w> tuż przed pierwszym gwizdkiem sędziego.",
                            "target_lang_translations": {
                                "ru": "Футболисты обеих команд выбежали на <w>газон</w> прямо перед первым свистком судьи."
                            }
                        },
                        {
                            "text": "Mecz został przerwany, ponieważ nasiąknięta wodą <w>murawa</w> nie nadawała się do dalszej gry.",
                            "target_lang_translations": {
                                "ru": "Матч был прерван, так как пропитанный водой <w>газон</w> был непригоден для дальнейшей игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "boisko",
                        "płyta boiska"
                    ],
                    "common_phrases": [
                        "wybiec na murawę",
                        "stan murawy",
                        "podgrzewana murawa",
                        "śliska murawa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Травяное покрытие спортивного поля, на котором проходит игра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газон",
                                "target_lang_sense_clarification": "спортивный"
                            },
                            {
                                "target_lang_word": "поле",
                                "target_lang_sense_clarification": "игровая площадка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3834041-64cf-38d1-99b6-b4f0dd1b48b2",
                    "sense_definition": "Gęsta, niska trawa pokrywająca ziemię, tworząca zwartą warstwę roślinności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W parku rozłożyliśmy koc na miękkiej <w>murawie</w> i rozpoczęliśmy piknik.",
                            "target_lang_translations": {
                                "ru": "В парке мы расстелили одеяло на мягкой <w>траве</w> и начали пикник."
                            }
                        },
                        {
                            "text": "Owce pasły się spokojnie na soczystej, zielonej <w>murawie</w> górskiego zbocza.",
                            "target_lang_translations": {
                                "ru": "Овцы мирно паслись на сочной, зеленой <w>траве</w> горного склона."
                            }
                        }
                    ],
                    "synonyms": [
                        "trawa",
                        "trawnik",
                        "darń"
                    ],
                    "antonyms": [
                        "klepisko"
                    ],
                    "common_phrases": [
                        "zielona murawa",
                        "miękka murawa",
                        "soczysta murawa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Густая, низкая трава, покрывающая землю и образующая плотный слой растительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трава",
                                "target_lang_sense_clarification": "растение"
                            },
                            {
                                "target_lang_word": "мурава",
                                "target_lang_sense_clarification": "густая молодая трава"
                            },
                            {
                                "target_lang_word": "дёрн",
                                "target_lang_sense_clarification": "верхний слой почвы с травой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fea5af1-22c0-3dbf-8287-792dd46086ad",
                    "sense_definition": "Warstwa roślinności zarastająca powierzchnię wód stojących, bagien lub torfowisk.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podróżnik ostrożnie stąpał po zdradliwej <w>murawie</w>, bojąc się wpaść do bagna.",
                            "target_lang_translations": {
                                "ru": "Путешественник осторожно ступал по предательской <w>сплавине</w>, боясь провалиться в болото."
                            }
                        },
                        {
                            "text": "Gęsta <w>murawa</w> mchów i traw pokrywała całe torfowisko, tworząc iluzję stałego lądu.",
                            "target_lang_translations": {
                                "ru": "Густая <w>дернина</w> из мхов и трав покрывала весь торфяник, создавая иллюзию твердой суши."
                            }
                        }
                    ],
                    "synonyms": [
                        "kożuch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой растительности, зарастающий поверхность стоячих вод, болот или торфяников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сплавина",
                                "target_lang_sense_clarification": "на болоте"
                            },
                            {
                                "target_lang_word": "дернина",
                                "target_lang_sense_clarification": "слой растительности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "72472336-a0f1-321d-9c52-06a813c44a54",
                    "sense_definition": "Udać się gdzieś drogą powietrzną (np. samolotem) lub przemieścić się w powietrzu (o ptakach, owadach).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym roku <polecimy> całą rodziną na wakacje do Hiszpanii.",
                            "target_lang_translations": {
                                "ru": "В следующем году мы всей семьей <полетим> в отпуск в Испанию."
                            }
                        },
                        {
                            "text": "Ptaki <poleciały> do ciepłych krajów przed nadejściem zimy.",
                            "target_lang_translations": {
                                "ru": "Птицы <полетели> в теплые края перед наступлением зимы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "polecieć samolotem",
                        "polecieć na wakacje",
                        "polecieć w kosmos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправиться куда-либо по воздуху (на самолете) или переместиться в воздухе (о птицах, насекомых)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полететь",
                                "target_lang_sense_clarification": "переместиться по воздуху"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22473ac4-68e8-3f3d-b49c-50114f7ab23b",
                    "sense_definition": "Bardzo szybko udać się w jakieś miejsce; pobiec.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_run",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama poprosiła mnie, żebym szybko <poleciał> do sklepu po świeży chleb.",
                            "target_lang_translations": {
                                "ru": "Мама попросила меня быстро <сбегать> в магазин за свежим хлебом."
                            }
                        },
                        {
                            "text": "Jak tylko zadzwonił dzwonek, dzieci <poleciały> na plac zabaw.",
                            "target_lang_translations": {
                                "ru": "Как только прозвенел звонок, дети <побежали> на детскую площадку."
                            }
                        }
                    ],
                    "synonyms": [
                        "pobiec",
                        "pognać",
                        "popędzić",
                        "puścić się biegiem"
                    ],
                    "common_phrases": [
                        "polecieć do sklepu",
                        "polecieć po zakupy",
                        "polecieć sprawdzić"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used frequently in spoken language instead of 'pobiec'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень быстро отправиться куда-либо; побежать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побежать",
                                "target_lang_sense_clarification": "быстро отправиться бегом"
                            },
                            {
                                "target_lang_word": "сбегать",
                                "target_lang_sense_clarification": "быстро сходить/съездить куда-то и вернуться"
                            },
                            {
                                "target_lang_word": "помчаться",
                                "target_lang_sense_clarification": "очень быстро устремиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bb1f3cb-36b9-34ae-a707-1a96e5bd1e8f",
                    "sense_definition": "Spaść, przewrócić się lub zostać gwałtownie rzuconym w powietrze.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wazon zachwiał się na półce i <poleciał> prosto na podłogę.",
                            "target_lang_translations": {
                                "ru": "Ваза покачнулась на полке и <полетела> прямо на пол."
                            }
                        },
                        {
                            "text": "Piłka kopnięta przez zawodnika <poleciała> wysoko ponad bramką.",
                            "target_lang_translations": {
                                "ru": "Мяч, ударенный игроком, <полетел> высоко над воротами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "polecieć na ziemię",
                        "polecieć w dół",
                        "polecieć do tyłu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упасть, опрокинуться или стремительно переместиться в воздухе (о предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полететь",
                                "target_lang_sense_clarification": "упасть или быть брошенным"
                            },
                            {
                                "target_lang_word": "упасть",
                                "target_lang_sense_clarification": "свалиться вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbf0df08-b324-3adc-b9e4-a9501282d487",
                    "sense_definition": "Bardzo się czymś lub kimś zainteresować; dać się skusić (często w kontekście romantycznym lub materialnym).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_attraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy wiedzieli, że on <poleciał> tylko na jej pieniądze.",
                            "target_lang_translations": {
                                "ru": "Все знали, что он <повелся> только на ее деньги."
                            }
                        },
                        {
                            "text": "Wielu klientów <poleciało> na tę kolorową reklamę i kupiło produkt.",
                            "target_lang_translations": {
                                "ru": "Многие клиенты <клюнули> на эту красочную рекламу и купили продукт."
                            }
                        }
                    ],
                    "common_phrases": [
                        "polecieć na kasę",
                        "polecieć na wygląd",
                        "polecieć na kogoś"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно заинтересоваться кем-то или чем-то; соблазниться (часто в романтическом или материальном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повестись",
                                "target_lang_sense_clarification": "разг. дать себя уговорить или обмануть"
                            },
                            {
                                "target_lang_word": "клюнуть",
                                "target_lang_sense_clarification": "разг. соблазниться чем-либо"
                            },
                            {
                                "target_lang_word": "запасть",
                                "target_lang_sense_clarification": "разг. сильно увлечься кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "124032c1-2ae1-322f-82f5-6effb2d8fc74",
                    "sense_definition": "Gwałtownie spaść lub stracić na wartości (o cenach, wskaźnikach, akcjach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_drop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ogłoszeniu kryzysu ceny mieszkań w centrum <poleciały> w dół.",
                            "target_lang_translations": {
                                "ru": "После объявления кризиса цены на квартиры в центре <полетели> вниз."
                            }
                        },
                        {
                            "text": "Wartość waluty <poleciała> na łeb na szyję w ciągu jednego dnia.",
                            "target_lang_translations": {
                                "ru": "Стоимость валюты стремительно <рухнула> за один день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ceny poleciały",
                        "akcje poleciały",
                        "polecieć na łeb na szyję"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко упасть или снизиться (о ценах, показателях, акциях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рухнуть",
                                "target_lang_sense_clarification": "стремительно упасть (о ценах)"
                            },
                            {
                                "target_lang_word": "полететь",
                                "target_lang_sense_clarification": "разг. резко пойти вниз"
                            },
                            {
                                "target_lang_word": "обвалиться",
                                "target_lang_sense_clarification": "резко снизиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b63f296-367a-386c-9129-3ae2d5e72d9f",
                    "sense_definition": "Zostać zwolnionym z pracy lub usuniętym z zajmowanego stanowiska.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_fired",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wykryciu afery finansowej dyrektor <poleciał> ze swojego stanowiska.",
                            "target_lang_translations": {
                                "ru": "После раскрытия финансовой аферы директор <слетел> со своей должности."
                            }
                        },
                        {
                            "text": "Jeśli nie poprawisz wyników sprzedaży, możesz <polecieć> z pracy.",
                            "target_lang_translations": {
                                "ru": "Если не улучшишь результаты продаж, можешь <вылететь> с работы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "polecieć ze stanowiska",
                        "polecieć z pracy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть уволенным с работы или снятым с занимаемой должности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вылететь",
                                "target_lang_sense_clarification": "разг. быть уволенным"
                            },
                            {
                                "target_lang_word": "слететь",
                                "target_lang_sense_clarification": "разг. потерять должность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
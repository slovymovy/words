            "senses": [
                {
                    "sense_id": "dedaae93-a65d-3d94-845c-0dd9dec6c43b",
                    "sense_definition": "According to statistical evidence or from a statistical point of view.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The results of the medical trial were found to be <w>statistically</w> significant.",
                            "target_lang_translations": {
                                "ru": "Результаты медицинских испытаний оказались <w>статистически</w> значимыми."
                            }
                        },
                        {
                            "text": "<w>Statistically</w>, flying is one of the safest modes of transportation available today.",
                            "target_lang_translations": {
                                "ru": "<w>Статистически</w>, авиаперелеты — один из самых безопасных видов транспорта, доступных сегодня."
                            }
                        }
                    ],
                    "synonyms": [
                        "numerically",
                        "mathematically"
                    ],
                    "common_phrases": [
                        "statistically significant",
                        "statistically speaking",
                        "statistically impossible",
                        "statistically improbable"
                    ],
                    "target_lang_definitions": {
                        "ru": "Согласно статистическим данным или с точки зрения статистики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "статистически",
                                "target_lang_sense_clarification": "с точки зрения статистики"

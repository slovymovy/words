            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d8121c5e-2b56-3d50-9b6f-9c3059a1cff7",
                    "sense_definition": "Полюбить друг друга; проникнуться взаимным чувством любви.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mutual_love",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Говорят, что в браке по расчёту стерпится — <w>слюбится</w>.",
                            "target_lang_translations": {
                                "en": "They say that in an arranged marriage, you get used to it and <w>fall in love</w>."
                            }
                        },
                        {
                            "text": "Молодые люди быстро <w>слюбились</w> и решили сыграть свадьбу.",
                            "target_lang_translations": {
                                "en": "The young people quickly <w>fell in love</w> and decided to have a wedding."
                            }
                        }
                    ],
                    "synonyms": [
                        "влюбиться",
                        "сойтись"
                    ],
                    "antonyms": [
                        "разлюбить"
                    ],
                    "common_phrases": [
                        "стерпится — слюбится"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в народной речи или пословицах"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To fall in love with each other; to develop mutual feelings of love."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall in love",
                                "target_lang_sense_clarification": "mutual process, often over time"
                            },
                            {
                                "target_lang_word": "grow to love each other",
                                "target_lang_sense_clarification": "developing affection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4121cea6-ed9e-30ed-90dd-b73dac4acc1c",
                    "sense_definition": "Стать любимым, понравиться кому-либо; то же, что <w>полюбиться</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "be_liked",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чем тебе так <w>слюбилась</w> эта далёкая страна?",
                            "target_lang_translations": {
                                "en": "Why have you <w>grown so fond of</w> this distant country?"
                            }
                        },
                        {
                            "text": "Ему сразу <w>слюбилась</w> местная природа и тишина.",
                            "target_lang_translations": {
                                "en": "He immediately <w>took a fancy to</w> the local nature and silence."
                            }
                        }
                    ],
                    "synonyms": [
                        "полюбиться",
                        "приглянуться",
                        "понравиться"
                    ],
                    "antonyms": [
                        "разонравиться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "устаревающий или просторечный оттенок"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become loved or liked by someone; to appeal to someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take a fancy to",
                                "target_lang_sense_clarification": "to start liking something or someone"
                            },
                            {
                                "target_lang_word": "grow fond of",
                                "target_lang_sense_clarification": "to develop affection for"
                            },
                            {
                                "target_lang_word": "appeal to",
                                "target_lang_sense_clarification": "to be attractive or interesting to"
                            }
                        ]
                    }
                }
            ]
        }

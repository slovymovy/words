            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a03e9057-b536-3f12-956d-52fa0cd38bbf",
                    "sense_definition": "Zichtbaar worden of ergens aankomen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Plotseling <w>verscheen</w> er een grote zwarte hond om de hoek van de straat.",
                            "target_lang_translations": {
                                "en": "Suddenly a big black dog <w>appeared</w> around the corner of the street."
                            }
                        },
                        {
                            "text": "Na een lange regenbui <w>verscheen</w> er eindelijk een prachtige regenboog aan de hemel.",
                            "target_lang_translations": {
                                "en": "After a long rain shower, a beautiful rainbow finally <w>appeared</w> in the sky."
                            }
                        },
                        {
                            "text": "De acteur wilde niet in het openbaar <w>verschijnen</w> na het grote schandaal.",
                            "target_lang_translations": {
                                "en": "The actor did not want to <w>appear</w> in public after the big scandal."
                            }
                        }
                    ],
                    "synonyms": [
                        "opduiken",
                        "tevoorschijn komen",
                        "zich vertonen"
                    ],
                    "antonyms": [
                        "verdwijnen"
                    ],
                    "common_phrases": [
                        "ten tonele verschijnen",
                        "in het openbaar verschijnen"
                    ],
                    "target_lang_definitions": {
                        "en": "To become visible or arrive somewhere."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "to become visible"
                            },
                            {
                                "target_lang_word": "show up",
                                "target_lang_sense_clarification": "to arrive, often unexpectedly"
                            },
                            {
                                "target_lang_word": "emerge",
                                "target_lang_sense_clarification": "to come into view"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdefd2c1-6a6c-3562-a24a-9f6eff7b561a",
                    "sense_definition": "Gepubliceerd en beschikbaar worden voor het publiek (van boeken, kranten of media).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn nieuwe roman zal volgende maand in de boekhandel <w>verschijnen</w>.",
                            "target_lang_translations": {
                                "en": "His new novel will <w>appear</w> in bookstores next month."
                            }
                        },
                        {
                            "text": "Het artikel is gisteren in de lokale krant <w>verschenen</w> en trok veel aandacht.",
                            "target_lang_translations": {
                                "en": "The article <w>appeared</w> in the local newspaper yesterday and attracted a lot of attention."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitkomen",
                        "gepubliceerd worden"
                    ],
                    "common_phrases": [
                        "in druk verschijnen",
                        "binnenkort verschijnen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be published and become available to the public (of books, newspapers, or media)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be published",
                                "target_lang_sense_clarification": "to be released in print or media"
                            },
                            {
                                "target_lang_word": "come out",
                                "target_lang_sense_clarification": "common phrasal verb for release"
                            },
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "formal usage for publication"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76e4db97-175e-3e9f-ba53-2f8eba8bcf5f",
                    "sense_definition": "Zich officieel melden bij een rechtbank of autoriteit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte moet morgenochtend vroeg voor de rechter <w>verschijnen</w> om zijn verklaring af te leggen.",
                            "target_lang_translations": {
                                "en": "The suspect must <w>appear</w> before the judge early tomorrow morning to make his statement."
                            }
                        },
                        {
                            "text": "Indien u niet kunt <w>verschijnen</w>, dient u dit tijdig aan de griffie door te geven.",
                            "target_lang_translations": {
                                "en": "If you cannot <w>appear</w>, you must report this to the registry in time."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorkomen"
                    ],
                    "antonyms": [

            "senses": [
                {
                    "sense_id": "cccf04dc-965c-33c3-9bbc-515f81d974b7",
                    "sense_definition": "Wejść do środka transportu (np. samochodu, pociągu, autobusu), aby nim podróżować.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_entry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy szybko <w>wsiąść</w> do pociągu, bo zaraz odjeżdża.",
                            "target_lang_translations": {
                                "ru": "Мы должны быстро <w>сесть</w> в поезд, потому что он скоро отправляется."
                            }
                        },
                        {
                            "text": "Marek <w>wsiadł</w> do taksówki i pojechał na lotnisko.",
                            "target_lang_translations": {
                                "ru": "Марек <w>сел</w> в такси и поехал в аэропорт."
                            }
                        },
                        {
                            "text": "Czemu kolega nie <w>wsiadł</w> do autobusu, tylko został na przystanku?",
                            "target_lang_translations": {
                                "ru": "Почему коллега не <w>сел</w> в автобус, а остался на остановке?"
                            }
                        }
                    ],
                    "antonyms": [
                        "wysiąść"
                    ],
                    "common_phrases": [
                        "wsiąść do pociągu",
                        "wsiąść do autobusu",
                        "wsiąść do samochodu",
                        "wsiąść do samolotu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Войти в транспортное средство (например, в машину, поезд, автобус), чтобы поехать на нем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сесть",
                                "target_lang_sense_clarification": "в транспорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "248e759f-52f1-354e-9535-7379804b9dc2",
                    "sense_definition": "Usiąść na czymś okrakiem, zajmując miejsce na grzbiecie zwierzęcia lub na pojeździe takim jak rower czy motocykl.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mounting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chcę <w>wsiąść</w> na rower i pojechać na wycieczkę do lasu.",
                            "target_lang_translations": {
                                "ru": "Я хочу <w>сесть</w> на велосипед и поехать на прогулку в лес."
                            }
                        },
                        {
                            "text": "Jeździec z łatwością <w>wsiadł</w> na swojego konia.",
                            "target_lang_translations": {
                                "ru": "Всадник с легкостью <w>сел</w> на свою лошадь."
                            }
                        }
                    ],
                    "antonyms": [
                        "zsiąść"
                    ],
                    "common_phrases": [
                        "wsiąść na rower",
                        "wsiąść na konia",
                        "wsiąść na motocykl"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сесть верхом на что-либо, занимая место на спине животного или на транспортном средстве, таком как велосипед или мотоцикл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сесть",
                                "target_lang_sense_clarification": "верхом / на велосипед"
                            }
                        ]
                    }
                }
            ]
        }

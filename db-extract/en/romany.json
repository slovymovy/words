                                "target_word": "Zigeuner",
                                "target_lang_code": "de",
                                "target_senses": [
                                    {
                                        "sense_id": "6ffeeb38-5e10-3f00-9291-43474ad6189e",
                                        "entry_id": "f702ad4f-94b1-39b2-8364-f2818eeeea61",
                                        "glosses": [
                                            "gegebenenfalls Fremdbezeichnung, die auf Angehörige der Roma und auf Jenische angewendet wird"
                                        ],
                                        "tags": [
                                            "derogatory"
                                        ],
                                        "examples": [
                                            {

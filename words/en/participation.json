    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "29bf84c5-d67c-3485-9628-5c2239aa255c",
                    "sense_definition": "The act of taking part or becoming involved in an activity or event.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher was pleased with the student's active <w>participation</w> in class.",
                            "target_lang_translations": {
                                "ru": "Учитель был доволен активным <w>участием</w> ученика на уроке."
                            }
                        },
                        {
                            "text": "Successful events rely on the <w>participation</w> of the local community.",
                            "target_lang_translations": {
                                "ru": "Успешные мероприятия зависят от <w>участия</w> местного сообщества."
                            }
                        }
                    ],
                    "synonyms": [
                        "involvement",
                        "engagement",
                        "contribution"
                    ],
                    "antonyms": [
                        "nonparticipation",
                        "abstention"
                    ],
                    "common_phrases": [
                        "active participation",
                        "full participation",
                        "participation rate",
                        "encourage participation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в принятии участия или вовлечении в какую-либо деятельность или событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участие",
                                "target_lang_sense_clarification": "вовлеченность в деятельность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "204eb929-71db-3aaa-935a-003e676f1bf1",
                    "sense_definition": "An ownership interest or a right to share in profits.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The deal offered him a significant <w>participation</w> in the company's future profits.",
                            "target_lang_translations": {
                                "ru": "Сделка предлагала ему значительное <w>участие</w> в будущих прибылях компании."
                            }
                        },
                        {
                            "text": "Banks often require equity <w>participation</w> for large loans.",
                            "target_lang_translations": {
                                "ru": "Банки часто требуют долевого <w>участия</w> для выдачи крупных кредитов."
                            }
                        }
                    ],
                    "synonyms": [
                        "stake",
                        "interest",
                        "share"
                    ],
                    "common_phrases": [
                        "equity participation",
                        "profit participation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доля собственности или право на получение части прибыли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участие",
                                "target_lang_sense_clarification": "доля в капитале или прибыли"
                            },
                            {
                                "target_lang_word": "доля",
                                "target_lang_sense_clarification": "часть чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92967f5a-b3d6-37bb-b3a3-e4b79648911a",
                    "sense_definition": "The state of being related to or forming part of a larger whole.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The philosopher explored the <w>participation</w> of the mind in the physical world.",
                            "target_lang_translations": {
                                "ru": "Философ исследовал <w>причастность</w> разума к физическому миру."
                            }
                        },
                        {
                            "text": "His theory describes the <w>participation</w> of individual events in a cosmic order.",
                            "target_lang_translations": {
                                "ru": "Его теория описывает <w>причастность</w> отдельных событий к космическому порядку."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние связи с чем-либо или принадлежности к большему целому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "причастность",
                                "target_lang_sense_clarification": "принадлежность к целому"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
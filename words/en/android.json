    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cf9fe7c4-b473-3a47-a8e3-51b9d64ed4fd",
                    "sense_definition": "A mobile operating system developed by Google, widely used on smartphones and tablets.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "technology_or_software_name",
                    "examples": [
                        {
                            "text": "Many developers prefer creating apps for <w>Android</w> because of its large user base.",
                            "target_lang_translations": {
                                "ru": "Многие разработчики предпочитают создавать приложения для <w>Android</w> из-за его большой базы пользователей."
                            }
                        },
                        {
                            "text": "You can download the game from the store if your phone runs on <w>Android</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете скачать игру из магазина, если ваш телефон работает на <w>Android</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Android phone",
                        "Android app",
                        "on Android",
                        "Android version"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мобильная операционная система, разработанная Google, широко используемая на смартфонах и планшетах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Андро́ид",
                                "target_lang_sense_clarification": "операционная система"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a4a6a029-32b2-3746-89ba-73d78b28aa4e",
                    "sense_definition": "A smartphone or mobile device that runs the Android operating system.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to switch to an <w>Android</w> after using other phones for years.",
                            "target_lang_translations": {
                                "ru": "Она решила перейти на <w>андроид</w> после многих лет использования других телефонов."
                            }
                        },
                        {
                            "text": "Most <w>Androids</w> use a standard charging cable that is easy to find.",
                            "target_lang_translations": {
                                "ru": "Большинство <w>андроидов</w> используют стандартный зарядный кабель, который легко найти."
                            }
                        }
                    ],
                    "common_phrases": [
                        "buy an Android",
                        "new Android",
                        "switch to Android"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Смартфон или мобильное устройство, работающее на операционной системе Android."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "андро́ид",
                                "target_lang_sense_clarification": "смартфон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c7d1f12-919d-384c-98c6-548e821a5eae",
                    "sense_definition": "A robot designed to resemble a human in appearance and behavior.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sci-fi movie featured an <w>android</w> that could feel human emotions.",
                            "target_lang_translations": {
                                "ru": "В научно-фантастическом фильме был показан <w>андроид</w>, способный испытывать человеческие эмоции."
                            }
                        },
                        {
                            "text": "Scientists are working to build an <w>android</w> capable of assisting with elderly care.",
                            "target_lang_translations": {
                                "ru": "Ученые работают над созданием <w>андроида</w>, способного помогать в уходе за пожилыми людьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "humanoid"
                    ],
                    "common_phrases": [
                        "rogue android",
                        "advanced android",
                        "human-like android"
                    ],
                    "target_lang_definitions": {
                        "ru": "Робот, внешне и по поведению напоминающий человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "андро́ид",
                                "target_lang_sense_clarification": "робот"
                            },
                            {
                                "target_lang_word": "человекоподо́бный ро́бот",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1103a021-cfc5-30ff-a2a8-765bf542a506",
                    "sense_definition": "Relating to or characteristic of a male, often used in anatomy to describe body shape or pelvis structure.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor explained that an <w>android</w> fat distribution is concentrated around the abdomen.",
                            "target_lang_translations": {
                                "ru": "Врач объяснил, что <w>андроидное</w> распределение жира сосредоточено вокруг живота."
                            }
                        },
                        {
                            "text": "Forensic analysis of the skeleton revealed an <w>android</w> pelvis, suggesting the remains were male.",
                            "target_lang_translations": {
                                "ru": "Судебно-медицинский анализ скелета выявил <w>андроидный</w> таз, что указывало на принадлежность останков мужчине."
                            }
                        }
                    ],
                    "antonyms": [
                        "gynoid"
                    ],
                    "common_phrases": [
                        "android pelvis",
                        "android obesity",
                        "android shape"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к мужскому типу; часто используется в анатомии для описания телосложения или строения таза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "андро́идный",
                                "target_lang_sense_clarification": "анатомический термин"
                            },
                            {
                                "target_lang_word": "по мужско́му ти́пу",
                                "target_lang_sense_clarification": "характеристика"
                            }
                        ]
                    }
                }
            ]
        }

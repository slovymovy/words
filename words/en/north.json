{
    "word_family": [
        "northern",
        "northerner",
        "northerly",
        "northward",
        "northbound"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9914bf71-6fc3-3feb-ae1f-dba7d2b3bac7",
                    "sense_definition": "The direction towards the top of a standard map, opposite to the south.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The compass needle always points to the magnetic <w>north</w>.",
                            "target_lang_translations": {
                                "ru": "Стрелка компаса всегда указывает на магнитный <w>север</w>."
                            }
                        },
                        {
                            "text": "Cold winds usually blow from the <w>north</w> in the winter.",
                            "target_lang_translations": {
                                "ru": "Зимой холодные ветры обычно дуют с <w>севера</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "south"
                    ],
                    "common_phrases": [
                        "to the north",
                        "from the north",
                        "true north",
                        "magnetic north"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направление к верхней части стандартной карты, противоположное югу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "север",
                                "target_lang_sense_clarification": "сторона света"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb78dd6b-5d17-3112-bef9-5669bcbed789",
                    "sense_definition": "The positive pole of a magnet.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "magnetism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The red end of the needle represents <w>north</w> on this device.",
                            "target_lang_translations": {
                                "ru": "Красный конец стрелки обозначает <w>север</w> на этом приборе."
                            }
                        },
                        {
                            "text": "Opposite poles attract, so the <w>north</w> of one magnet pulls the south of another.",
                            "target_lang_translations": {
                                "ru": "Противоположные полюса притягиваются, поэтому <w>север</w> одного магнита притягивает юг другого."
                            }
                        }
                    ],
                    "antonyms": [
                        "south"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Положительный полюс магнита."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "север",
                                "target_lang_sense_clarification": "полюс магнита"
                            },
                            {
                                "target_lang_word": "северный полюс",
                                "target_lang_sense_clarification": "магнетизм"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f7ff11f0-cd2e-36f9-a01b-acb72d2bbffc",
                    "sense_definition": "Towards or in the direction of the north.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We drove <w>north</w> for three hours to reach the cabin.",
                            "target_lang_translations": {
                                "ru": "Мы ехали <w>на север</w> три часа, чтобы добраться до хижины."
                            }
                        },
                        {
                            "text": "Many birds fly <w>north</w> when the spring weather arrives.",
                            "target_lang_translations": {
                                "ru": "Многие птицы летят <w>на север</w>, когда наступает весенняя погода."
                            }
                        }
                    ],
                    "antonyms": [
                        "south"
                    ],
                    "common_phrases": [
                        "go north",
                        "travel north",
                        "head north",
                        "look north"
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении севера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на север",
                                "target_lang_sense_clarification": "направление движения"
                            },
                            {
                                "target_lang_word": "к северу",
                                "target_lang_sense_clarification": "местоположение относительно чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b850fcdd-96b7-32cd-bc4b-2fe8509dc09b",
                    "sense_definition": "Located in, directed towards, or relating to the north.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_attribute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They live on the <w>north</w> coast of the island.",
                            "target_lang_translations": {
                                "ru": "Они живут на <w>северном</w> побережье острова."
                            }
                        },
                        {
                            "text": "Please use the <w>north</w> entrance because the main door is locked.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, используйте <w>северный</w> вход, так как главная дверь заперта."
                            }
                        }
                    ],
                    "antonyms": [
                        "south"
                    ],
                    "common_phrases": [
                        "north side",
                        "North Pole",
                        "north star"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный на севере, направленный на север или относящийся к нему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "северный",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d20cb9b2-3b70-3cf1-a174-7c3fe1123d6c",
                    "sense_definition": "Of wind, blowing from the north.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A cold <w>north</w> wind made us shiver despite our coats.",
                            "target_lang_translations": {
                                "ru": "Холодный <w>северный</w> ветер заставил нас дрожать, несмотря на пальто."
                            }
                        },
                        {
                            "text": "The sailors waited for the <w>north</w> breeze to fill the sails.",
                            "target_lang_translations": {
                                "ru": "Моряки ждали, когда <w>северный</w> бриз наполнит паруса."
                            }
                        }
                    ],
                    "antonyms": [
                        "south"
                    ],
                    "common_phrases": [
                        "north wind"
                    ],
                    "target_lang_definitions": {
                        "ru": "О ветре: дующий с севера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "северный",
                                "target_lang_sense_clarification": "о ветре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e492c463-5132-3ea5-870e-fa4c635d4ead",
                    "sense_definition": "Higher than or more than a specific amount.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity_idiom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The final cost of the project was <w>north</w> of one million dollars.",
                            "target_lang_translations": {
                                "ru": "Окончательная стоимость проекта была <w>свыше</w> одного миллиона долларов."
                            }
                        },
                        {
                            "text": "He is currently earning a salary somewhere <w>north</w> of the national average.",
                            "target_lang_translations": {
                                "ru": "В настоящее время он получает зарплату где-то <w>выше</w> средней по стране."
                            }
                        }
                    ],
                    "common_phrases": [
                        "north of"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "business or informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выше или больше определенного количества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свыше",
                                "target_lang_sense_clarification": "более чем"
                            },
                            {
                                "target_lang_word": "более",
                                "target_lang_sense_clarification": "больше чем"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "0aed552d-e7b9-3853-bd00-8b119b59648e",
                    "sense_definition": "The northern part of a specific country or region, often considered as a distinct cultural or political area.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "region_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The industrial revolution had a major impact on the <w>North</w> of England.",
                            "target_lang_translations": {
                                "ru": "Промышленная революция оказала огромное влияние на <w>Север</w> Англии."
                            }
                        },
                        {
                            "text": "During the American Civil War, the <w>North</w> fought against the South.",
                            "target_lang_translations": {
                                "ru": "Во время Гражданской войны в США <w>Север</w> сражался против Юга."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the North"
                    ],
                    "target_lang_definitions": {
                        "ru": "Северная часть конкретной страны или региона, часто рассматриваемая как отдельная культурная или политическая область."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Север",
                                "target_lang_sense_clarification": "регион"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e4d7f2b-9105-394c-b197-752d8cf68e43",
                    "sense_definition": "The group of economically developed countries, mostly located in the northern hemisphere.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geopolitics",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Economists often discuss the wealth gap between the <w>North</w> and the developing South.",
                            "target_lang_translations": {
                                "ru": "Экономисты часто обсуждают разрыв в благосостоянии между <w>Севером</w> и развивающимся Югом."
                            }
                        },
                        {
                            "text": "The Global <w>North</w> consumes a large percentage of the world's resources.",
                            "target_lang_translations": {
                                "ru": "Глобальный <w>Север</w> потребляет значительную часть мировых ресурсов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Global North"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа экономически развитых стран, в основном расположенных в северном полушарии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Север",
                                "target_lang_sense_clarification": "геополитика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5b2480f-6c0f-3f2d-b565-a797231b318b",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>North</w> will see you in his office now.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Норт</w> примет вас в своем кабинете сейчас."
                            }
                        },
                        {
                            "text": "We invited the <w>North</w> family to dinner next week.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили семью <w>Норт</w> на ужин на следующей неделе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Норт",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "af255eb6-737b-3782-8007-048552c0adb8",
                    "sense_definition": "To turn or move in a northern direction.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "movement_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ship <w>northed</w> slowly against the strong current.",
                            "target_lang_translations": {
                                "ru": "Корабль медленно <w>продвигался на север</w> против сильного течения."
                            }
                        },
                        {
                            "text": "After we <w>northed</w> for a few miles, the weather began to clear.",
                            "target_lang_translations": {
                                "ru": "После того как мы <w>прошли на север</w> несколько миль, погода начала проясняться."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поворачивать или двигаться в северном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двигаться на север",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "брать курс на север",
                                "target_lang_sense_clarification": "навигация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
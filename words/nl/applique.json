                },
                {
                    "sense_id": "23e6ca20-f30d-3c92-8044-406bdfc438c1",
                    "sense_definition": "Een lamp of verlichtingselement dat aan de wand is bevestigd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de lange gang hangt aan elke muur een bronzen <w>applique</w>.",
                            "target_lang_translations": {
                                "en": "In the long hallway, a bronze <w>sconce</w> hangs on every wall.",
                                "ru": "В длинном коридоре на каждой стене висит бронзовое <w>бра</w>."
                            }
                        },
                        {
                            "text": "We zoeken een moderne <w>applique</w> voor naast de spiegel in de badkamer.",
                            "target_lang_translations": {
                                "en": "We are looking for a modern <w>wall light</w> for next to the mirror in the bathroom.",
                                "ru": "Мы ищем современное <w>бра</w>, чтобы повесить рядом с зеркалом в ванной."
                            }
                        }

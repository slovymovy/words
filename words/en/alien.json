            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0332ed71-f83e-37c2-8821-e5be1ad12d14",
                    "sense_definition": "A creature or life form that comes from another planet or world.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "extraterrestrial_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the movie, the <w>aliens</w> came to Earth in peace.",
                            "target_lang_translations": {
                                "ru": "В фильме <w>пришельцы</w> прибыли на Землю с миром."
                            }
                        },
                        {
                            "text": "Many people believe that <w>aliens</w> have visited our planet in the past.",
                            "target_lang_translations": {
                                "ru": "Многие верят, что <w>инопланетяне</w> посещали нашу планету в прошлом."
                            }
                        }
                    ],
                    "synonyms": [
                        "extraterrestrial"
                    ],
                    "antonyms": [
                        "earthling",
                        "human"
                    ],
                    "common_phrases": [
                        "space alien",
                        "alien invasion",
                        "alien life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существо или форма жизни, прибывшая с другой планеты или из другого мира."
                    },
                    "translations": {
                        "ru": [
                            {

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "510b42a3-1ed4-34c5-8e1b-c5e9ff865b20",
                    "sense_definition": "Настойчиво попросить или потребовать совершить какое-либо действие; обратиться с призывом.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "urge_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Президент <призвал> всех граждан сохранять спокойствие в сложной ситуации."
                        },
                        {
                            "text": "Учитель строго <призвал> учеников к порядку и тишине."
                        }
                    ],
                    "synonyms": [
                        "побудить",
                        "потребовать"
                    ],
                    "antonyms": [
                        "отговорить"
                    ],
                    "common_phrases": [
                        "призвать к порядку",
                        "призвать к ответу",
                        "призвать к спокойствию",
                        "призвать к совести"
                    ]
                },
                {
                    "sense_id": "4f2776fa-0977-3305-86be-9d16a4c8ed4a",
                    "sense_definition": "Зачислить на военную службу; объявить набор в армию.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_service",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моего старшего брата <призвали> в армию сразу после окончания университета."
                        },
                        {
                            "text": "В случае войны государство может <призвать> на фронт всех мужчин призывного возраста."
                        }
                    ],
                    "synonyms": [
                        "мобилизовать"
                    ],
                    "antonyms": [
                        "демобилизовать"
                    ],
                    "common_phrases": [
                        "призвать в армию",
                        "призвать на службу",
                        "призвать на фронт"
                    ]
                },
                {
                    "sense_id": "5d6bf4a3-af24-3d66-9994-a9b4aa4681aa",
                    "sense_definition": "Официально или торжественно пригласить, попросить прийти или явиться куда-либо (часто с целью помощи или совета).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "summon_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Король <призвал> к себе лучших советников для обсуждения государственных дел."
                        },
                        {
                            "text": "В трудную минуту она мысленно <призвала> на помощь всех святых."
                        }
                    ],
                    "synonyms": [
                        "позвать",
                        "вызвать",
                        "пригласить"
                    ],
                    "antonyms": [
                        "прогнать"
                    ],
                    "common_phrases": [
                        "призвать на помощь",
                        "призвать к себе",
                        "призвать свидетелей"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "high style or bookish"
                        }
                    ]
                },
                {
                    "sense_id": "363f47ca-068e-3aa6-aff0-ce616f2bab43",
                    "sense_definition": "С помощью магического ритуала или заклинания заставить появиться мистическое существо или духа.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "summon_magic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Могущественный маг <призвал> духа огня, чтобы защитить замок от врагов."
                        },
                        {
                            "text": "В древних легендах герои часто пытались <призвать> богов для спасения мира."
                        }
                    ],
                    "synonyms": [
                        "вызвать"
                    ],
                    "antonyms": [
                        "изгнать"
                    ],
                    "common_phrases": [
                        "призвать духа",
                        "призвать демона",
                        "призвать дождь"
                    ]
                }
            ]
        }
    ]
}
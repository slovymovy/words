            "senses": [
                {
                    "sense_id": "6a9a2dce-1c25-3126-a425-27fb4896218d",
                    "sense_definition": "Involving low cost or value; cheap.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager gave us the <w>nickel</w> tour of the new office building.",

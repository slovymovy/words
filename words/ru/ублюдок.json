            "senses": [
                {
                    "sense_id": "9c3c9f76-7648-34a4-93eb-718055f05bfb",
                    "sense_definition": "Грубое оскорбление, используемое для обозначения подлого, никчёмного или неприятного человека.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот <w>ублюдок</w> украл все наши сбережения и сбежал.",
                            "target_lang_translations": {
                                "en": "That <w>bastard</w> stole all our savings and ran away."
                            }
                        },
                        {
                            "text": "Убирайся из моего дома, грязный <w>ублюдок</w>!",
                            "target_lang_translations": {
                                "en": "Get out of my house, you filthy <w>bastard</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "мразь",
                        "вы́блядок"
                    ],
                    "common_phrases": [

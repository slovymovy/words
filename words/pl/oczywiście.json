    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "80f9f231-a037-3e9c-8d5c-1298c220a71d",
                    "sense_definition": "Wykrzyknik wyrażający zdecydowaną zgodę, potwierdzenie lub aprobatę dla czyjejś wypowiedzi lub propozycji.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "– Czy mogę pożyczyć twój telefon? – <w>Oczywiście</w>!",
                            "target_lang_translations": {
                                "ru": "– Могу я одолжить твой телефон? – <w>Конечно</w>!"
                            }
                        },
                        {
                            "text": "– Pójdziesz z nami do kina? – <w>Oczywiście</w>, bardzo chętnie.",
                            "target_lang_translations": {
                                "ru": "– Ты пойдешь с нами в кино? – <w>Конечно</w>, с большим удовольствием."
                            }
                        }
                    ],
                    "synonyms": [
                        "jasne",
                        "pewnie",
                        "naturalnie",
                        "tak",
                        "jak najbardziej",
                        "mowa",
                        "a jak",
                        "ma się rozumieć",
                        "rozumie się"
                    ],
                    "common_phrases": [
                        "no oczywiście",
                        "ależ oczywiście",
                        "oczywiście że tak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, выражающее решительное согласие, подтверждение или одобрение чьего-либо высказывания или предложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конечно",
                                "target_lang_sense_clarification": "выражение согласия"
                            },
                            {
                                "target_lang_word": "разумеется",
                                "target_lang_sense_clarification": "подтверждение"
                            },
                            {
                                "target_lang_word": "естественно",
                                "target_lang_sense_clarification": "само собой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0f092173-df93-340f-b36f-e803025befea",
                    "sense_definition": "Słowo używane w zdaniu do podkreślenia, że to, o czym się mówi, jest pewne, bezsporne lub zrozumiałe samo przez się.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Oczywiście</w> zapomniałem parasola i zmokłem w drodze do domu.",
                            "target_lang_translations": {
                                "ru": "<w>Конечно</w>, я забыл зонт и промок по дороге домой."
                            }
                        },
                        {
                            "text": "To jest <w>oczywiście</w> tylko moja prywatna opinia.",
                            "target_lang_translations": {
                                "ru": "Это, <w>конечно</w>, лишь мое частное мнение."
                            }
                        }
                    ],
                    "synonyms": [
                        "naturalnie",
                        "rzecz jasna",
                        "bez wątpienia",
                        "z pewnością",
                        "niewątpliwie",
                        "na pewno",
                        "rzecz prosta",
                        "rzecz oczywista"
                    ],
                    "common_phrases": [
                        "oczywiście że nie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вводное слово, используемое для подчеркивания того, что сказанное является верным, бесспорным или само собой разумеющимся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конечно",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "естественно",
                                "target_lang_sense_clarification": "по понятным причинам"
                            },
                            {
                                "target_lang_word": "несомненно",
                                "target_lang_sense_clarification": "бесспорно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b2e4765-4b47-3f2c-8e10-fe780f0d3a62",
                    "sense_definition": "W sposób wyraźny, widoczny i łatwy do zauważenia lub zrozumienia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wniosek ten wynika <w>oczywiście</w> z przedstawionych wcześniej danych.",
                            "target_lang_translations": {
                                "ru": "Этот вывод <w>очевидно</w> следует из представленных ранее данных."
                            }
                        },
                        {
                            "text": "Problem nie przedstawia się tak <w>oczywiście</w>, jak mogłoby się wydawać na pierwszy rzut oka.",
                            "target_lang_translations": {
                                "ru": "Проблема представляется не так <w>очевидно</w>, как могло бы показаться на первый взгляд."
                            }
                        }
                    ],
                    "synonyms": [
                        "ewidentnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Явным, заметным образом; так, что это легко понять или увидеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очевидно",
                                "target_lang_sense_clarification": "явно, заметно"
                            },
                            {
                                "target_lang_word": "явно",
                                "target_lang_sense_clarification": "отчетливо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f03e63e3-b053-3e54-912c-40085e8e158d",
                    "sense_definition": "Na własne oczy; będąc obecnym przy czymś osobiście.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_presence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciał przekonać się o tym <w>oczywiście</w>, udając się na miejsce zdarzenia.",
                            "target_lang_translations": {
                                "ru": "Он хотел убедиться в этом <w>воочию</w>, отправившись на место происшествия."
                            }
                        },
                        {
                            "text": "Świadek widział to <w>oczywiście</w> i mógł potwierdzić przebieg wypadków.",
                            "target_lang_translations": {
                                "ru": "Свидетель видел это <w>воочию</w> и мог подтвердить ход событий."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "82613c6a-a5bc-3868-b648-f9c41ebd3cc6",
                    "sense_definition": "Taki, który łatwo podporządkowuje się woli innych i pozwala sobą kierować; nie stawia oporu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój pies jest bardzo <w>uległy</w> i zawsze wykonuje wszystkie polecenia.",
                            "target_lang_translations": {
                                "ru": "Мой пес очень <w>покорный</w> и всегда выполняет все команды."
                            }
                        },
                        {
                            "text": "Nowy pracownik okazał się zbyt <w>uległy</w> wobec nierealnych wymagań klienta.",
                            "target_lang_translations": {
                                "ru": "Новый сотрудник оказался слишком <w>уступчивым</w> по отношению к нереальным требованиям клиента."
                            }
                        }
                    ],
                    "synonyms": [
                        "posłuszny",
                        "pokorny",
                        "potulny",
                        "podporządkowany",
                        "ustępliwy"
                    ],
                    "antonyms": [
                        "nieuległy"
                    ],
                    "common_phrases": [
                        "uległy charakter",
                        "być uległym",
                        "uległy wobec przełożonych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто легко подчиняется воле других и позволяет собой руководить; не оказывает сопротивления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покорный",
                                "target_lang_sense_clarification": "послушный, не сопротивляющийся"
                            },
                            {
                                "target_lang_word": "уступчивый",
                                "target_lang_sense_clarification": "склонный соглашаться, идти на уступки"
                            },
                            {
                                "target_lang_word": "податливый",
                                "target_lang_sense_clarification": "легко поддающийся влиянию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8ea5f2d-5e1f-3bf0-b767-95b29e0b0f89",
                    "sense_definition": "Wyrażający posłuszeństwo, pokorę lub gotowość do podporządkowania się (np. o głosie, gestach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna odpowiedział cichym i <w>uległym</w> głosem, unikając wzroku rozmówcy.",
                            "target_lang_translations": {
                                "ru": "Мужчина ответил тихим и <w>покорным</w> голосом, избегая взгляда собеседника."
                            }
                        },
                        {
                            "text": "Jej <w>uległy</w> uśmiech miał załagodzić napiętą sytuację w biurze.",
                            "target_lang_translations": {
                                "ru": "Её <w>покорная</w> улыбка должна была сгладить напряженную ситуацию в офисе."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokorny",
                        "uniżony"
                    ],
                    "common_phrases": [
                        "uległy ton",
                        "uległy gest",
                        "uległy głos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражающий послушание, смирение или готовность подчиниться (например, о голосе, жестах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покорный",
                                "target_lang_sense_clarification": "выражающий повиновение"
                            },
                            {
                                "target_lang_word": "смиренный",
                                "target_lang_sense_clarification": "кроткий, лишенный гордости"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
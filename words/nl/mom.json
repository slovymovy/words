                            {
                                "target_lang_word": "личина",
                                "target_lang_sense_clarification": "устаревшее или книжное"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "mask",
                                "target_lang_sense_clarification": "facial covering (archaic usage)"
                            }
                        ]

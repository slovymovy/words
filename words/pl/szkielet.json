{
    "word_family": [
        "szkielecik",
        "szkieletowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a5d0cf54-2ccd-3cfd-954b-d7309ced4d13",
                    "sense_definition": "Twarda konstrukcja anatomiczna podtrzymująca ciało organizmu, złożona z kości lub chrząstek.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anatomia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lekcji biologii oglądaliśmy ludzki <w>szkielet</w> stojący w rogu sali.",
                            "target_lang_translations": {
                                "ru": "На уроке биологии мы рассматривали человеческий <w>скелет</w>, стоящий в углу класса."
                            }
                        },
                        {
                            "text": "Archeolodzy odnaleźli kompletny <w>szkielet</w> nieznanego wcześniej gatunku zwierzęcia.",
                            "target_lang_translations": {
                                "ru": "Археологи нашли полный <w>скелет</w> неизвестного ранее вида животного."
                            }
                        }
                    ],
                    "synonyms": [
                        "kościec",
                        "układ kostny",
                        "układ szkieletowy"
                    ],
                    "common_phrases": [
                        "szkielet człowieka",
                        "szkielet dinozaura",
                        "budowa szkieletu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердая анатомическая структура, поддерживающая тело организма, состоящая из костей или хрящей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скелет",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93046cfc-abf1-317c-90ce-ff3d7cf03c27",
                    "sense_definition": "Wewnętrzna konstrukcja nośna budowli, pojazdu lub urządzenia, do której mocowane są pozostałe elementy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "konstrukcja_fizyczna",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy postawili już stalowy <w>szkielet</w> nowego wieżowca w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Рабочие уже возвели стальной <w>каркас</w> нового небоскреба в центре города."
                            }
                        },
                        {
                            "text": "Drewniany <w>szkielet</w> dachu musi zostać odpowiednio zabezpieczony przed wilgocią.",
                            "target_lang_translations": {
                                "ru": "Деревянный <w>каркас</w> крыши должен быть должным образом защищен от влаги."
                            }
                        }
                    ],
                    "synonyms": [
                        "baza",
                        "trzon",
                        "zarys"
                    ],
                    "common_phrases": [
                        "szkielet budynku",
                        "szkielet konstrukcji",
                        "stalowy szkielet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренняя несущая конструкция здания, транспортного средства или устройства, к которой крепятся остальные элементы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каркас",
                                "target_lang_sense_clarification": "здания или конструкции"
                            },
                            {
                                "target_lang_word": "остов",
                                "target_lang_sense_clarification": "основа конструкции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc592052-e86b-3e62-9eae-59eb43416f5e",
                    "sense_definition": "Wstępny plan, zarys lub główna struktura czegoś, co ma zostać rozwinięte lub uzupełnione.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "struktura_abstrakcyjna",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autor stworzył dopiero <w>szkielet</w> swojej nowej książki i musi dopisać dialogi.",
                            "target_lang_translations": {
                                "ru": "Автор создал только <w>скелет</w> своей новой книги и должен дописать диалоги."
                            }
                        },
                        {
                            "text": "Przedstawiony na zebraniu <w>szkielet</w> projektu wymaga jeszcze wielu uzupełnień.",
                            "target_lang_translations": {
                                "ru": "Представленный на собрании <w>скелет</w> проекта требует еще множества дополнений."
                            }
                        }
                    ],
                    "synonyms": [
                        "zarys",
                        "baza"
                    ],
                    "common_phrases": [
                        "szkielet powieści",
                        "szkielet programu",
                        "szkielet projektu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предварительный план, набросок или основная структура чего-либо, что должно быть развито или дополнено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скелет",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "костяк",
                                "target_lang_sense_clarification": "основа сюжета или плана"
                            },
                            {
                                "target_lang_word": "набросок",
                                "target_lang_sense_clarification": "схема"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b587752b-72fe-31d9-99d2-654413590d2f",
                    "sense_definition": "Osoba bardzo chuda, często wyglądająca na wyniszczoną chorobą lub głodem.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "osoba",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długiej i ciężkiej chorobie mój wujek wyglądał jak żywy <w>szkielet</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой и тяжелой болезни мой дядя выглядел как живой <w>скелет</w>."
                            }
                        },
                        {
                            "text": "Musisz zacząć więcej jeść, bo niedługo zamienisz się w <w>szkielet</w>.",
                            "target_lang_translations": {
                                "ru": "Ты должен начать больше есть, иначе скоро превратишься в <w>скелет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "chudeusz",
                        "chudzielec",
                        "kościotrup",
                        "suchar"
                    ],
                    "common_phrases": [
                        "wyglądać jak szkielet",
                        "żywy szkielet"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej, czasem obraźliwie lub ze współczuciem"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень худой человек, часто выглядящий изможденным болезнью или голодом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скелет",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "мощи",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a4f338f-6538-3101-b3da-98eea0428abb",
                    "sense_definition": "Pozostałości zniszczonego obiektu, z którego zachowała się tylko główna konstrukcja.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "konstrukcja_fizyczna",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na poboczu drogi stał wypalony <w>szkielet</w> samochodu osobowego po wypadku.",
                            "target_lang_translations": {
                                "ru": "На обочине дороги стоял выгоревший <w>остов</w> легкового автомобиля после аварии."
                            }
                        },
                        {
                            "text": "Z pięknego pałacu pozostał jedynie smutny <w>szkielet</w> murów porośnięty bluszczem.",
                            "target_lang_translations": {
                                "ru": "От прекрасного дворца остался лишь печальный <w>остов</w> стен, поросший плющом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wypalony szkielet",
                        "szkielet statku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остатки разрушенного объекта, от которого сохранилась только основная конструкция."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остов",
                                "target_lang_sense_clarification": "разрушенного объекта"
                            },
                            {
                                "target_lang_word": "скелет",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7af7456f-6722-366c-b96b-b3ba38ecb750",
                    "sense_definition": "Podstawowa, najważniejsza grupa osób w zespole lub organizacji, stanowiąca jej oparcie.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "struktura_abstrakcyjna",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trener oparł <w>szkielet</w> reprezentacji na doświadczonych zawodnikach z ligi zagranicznej.",
                            "target_lang_translations": {
                                "ru": "Тренер построил <w>костяк</w> сборной на опытных игроках из зарубежной лиги."
                            }
                        },
                        {
                            "text": "Ci pracownicy stanowią <w>szkielet</w> naszej firmy i gwarantują jej stabilność.",
                            "target_lang_translations": {
                                "ru": "Эти сотрудники составляют <w>костяк</w> нашей фирмы и гарантируют её стабильность."
                            }
                        }
                    ],
                    "synonyms": [
                        "trzon"
                    ],
                    "common_phrases": [
                        "szkielet drużyny",
                        "szkielet armii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная, наиболее важная группа людей в команде или организации, составляющая её опору."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "костяк",
                                "target_lang_sense_clarification": "группы или команды"
                            },
                            {
                                "target_lang_word": "ядро",
                                "target_lang_sense_clarification": "основа коллектива"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
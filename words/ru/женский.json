            "senses": [
                {
                    "sense_id": "7a68ee6d-7e9b-3c15-a3aa-17c6b57f1f93",
                    "sense_definition": "Связанный с женщиной, принадлежащий ей или свойственный ей.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В коридоре послышался тихий <w>женский</w> смех, и дверь сразу открылась.",
                            "target_lang_translations": {
                                "en": "Quiet <w>female</w> laughter was heard in the corridor, and the door opened immediately."
                            }
                        },
                        {
                            "text": "Её <w>женская</w> интуиция подсказывала, что нужно быть осторожнее в этом деле.",
                            "target_lang_translations": {
                                "en": "Her <w>feminine</w> intuition told her that she needed to be more careful in this matter."
                            }
                        }
                    ],
                    "antonyms": [
                        "мужской"
                    ],
                    "common_phrases": [
                        "женский голос",
                        "женская рука",
                        "женский характер",
                        "женская красота",
                        "женская логика"
                    ],
                    "target_lang_definitions": {
                        "en": "Related to a woman, belonging to her, or characteristic of her."
                    },
                    "translations": {

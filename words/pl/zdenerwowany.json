                {
                    "sense_id": "4dfa9cc5-0df1-3cdb-9575-7b7d16fb82af",
                    "sense_definition": "Odczuwający niepokój, lęk lub napięcie psychiczne, często przed spodziewanym wydarzeniem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anxiety",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed ważnym egzaminem student był bardzo <w>zdenerwowany</w>.",
                            "target_lang_translations": {
                                "ru": "Перед важным экзаменом студент был очень <w>взволнован</w>."
                            }
                        },
                        {
                            "text": "Mój brat wygląda na <w>zdenerwowanego</w> wizytą u dentysty.",
                            "target_lang_translations": {
                                "ru": "Мой брат выглядит <w>взволнованным</w> из-за визита к стоматологу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zestresowany",
                        "niespokojny",
                        "spięty",
                        "zaniepokojony"
                    ],
                    "antonyms": [
                        "spokojny",
                        "zrelaksowany",
                        "opanowany"
                    ],
                    "common_phrases": [
                        "być zdenerwowanym",
                        "wyglądać na zdenerwowanego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий беспокойство, страх или психическое напряжение, часто перед ожидаемым событием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взволнованный",
                                "target_lang_sense_clarification": "испытывающий волнение или тревогу перед событием"
                            },
                            {
                                "target_lang_word": "нервный",
                                "target_lang_sense_clarification": "находящийся в состоянии напряжения или стресса"
                            },
                            {
                                "target_lang_word": "беспокойный",
                                "target_lang_sense_clarification": "лишенный спокойствия, тревожный"
                            }

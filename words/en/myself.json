                    ],
                    "common_phrases": [
                        "feel like myself",
                        "be myself",
                        "not myself"
                    ],
                    "target_lang_definitions": {
                        "ru": "В своем обычном состоянии (здоровья, духа или характера)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собой",
                                "target_lang_sense_clarification": "в выражении \"быть собой\""
                            },
                            {
                                "target_lang_word": "сам не свой",
                                "target_lang_sense_clarification": "идиома: не в себе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "557eec69-3198-3596-a473-11c2d5508df4",
                    "sense_definition": "I; used as a subject in place of 'I', usually joined with another name.",

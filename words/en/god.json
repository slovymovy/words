    ],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c407551a-0eb9-3ef5-b272-14be52e69ed8",
                    "sense_definition": "The supreme being, creator, and ruler of the universe in monotheistic religions.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Many people pray to <w>God</w> for guidance and strength.",
                            "target_lang_translations": {
                                "ru": "Многие люди молятся <w>Богу</w> о наставлении и силе."
                            }
                        },
                        {
                            "text": "She believes that <w>God</w> created the world and everything in it.",
                            "target_lang_translations": {
                                "ru": "Она верит, что <w>Бог</w> создал мир и всё, что в нём."
                            }
                        }
                    ],
                    "synonyms": [
                        "supreme being",
                        "LORD"
                    ],
                    "antonyms": [
                        "Devil",
                        "Satan"
                    ],
                    "common_phrases": [
                        "believe in God",
                        "thank God",
                        "God bless you",
                        "act of God"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верховное существо, творец и правитель вселенной в монотеистических религиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бог",
                                "target_lang_sense_clarification": "единое верховное существо"
                            },
                            {
                                "target_lang_word": "Господь",
                                "target_lang_sense_clarification": "в христианской традиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0db51f9a-8ecc-3fcf-b8b7-ca51c239edaa",
                    "sense_definition": "The transcendent principle or ultimate cause of the universe, often discussed in philosophy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Philosophers have debated the existence and nature of <w>God</w> for centuries.",
                            "target_lang_translations": {
                                "ru": "Философы веками спорили о существовании и природе <w>Бога</w>."
                            }
                        },
                        {
                            "text": "Aristotle described <w>God</w> as the unmoved mover who set the cosmos in motion.",
                            "target_lang_translations": {
                                "ru": "Аристотель описывал <w>Бога</w> как неподвижного перводвигателя, который привел космос в движение."
                            }
                        }
                    ],
                    "synonyms": [
                        "divinity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трансцендентный принцип или первопричина вселенной, часто обсуждаемая в философии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бог",
                                "target_lang_sense_clarification": "философское понятие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "564a4677-9be6-3245-82e4-1162a5a790c0",
                    "sense_definition": "A supernatural being with special powers who is worshipped, often controlling a specific part of the world or nature.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mars was the Roman <w>god</w> of war and agriculture.",
                            "target_lang_translations": {
                                "ru": "Марс был римским <w>богом</w> войны и земледелия."
                            }
                        },
                        {
                            "text": "The ancient Greeks built many temples to honor every <w>god</w> and goddess.",
                            "target_lang_translations": {
                                "ru": "Древние греки строили множество храмов, чтобы почтить каждого <w>бога</w> и богиню."
                            }
                        }
                    ],
                    "synonyms": [
                        "deity",
                        "divinity"
                    ],
                    "common_phrases": [
                        "gods and goddesses",
                        "sun god",
                        "rain god"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сверхъестественное существо с особыми силами, которому поклоняются (обычно в политеизме)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бог",
                                "target_lang_sense_clarification": "языческое божество"
                            },
                            {
                                "target_lang_word": "божество",
                                "target_lang_sense_clarification": "сверхъестественное существо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a3dd87f-a729-351d-9f67-0f1191c4407a",
                    "sense_definition": "Something or someone that is admired, worshipped, or prioritized excessively.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "For some ambitious people, money becomes a <w>god</w> that controls their decisions.",
                            "target_lang_translations": {
                                "ru": "Для некоторых амбициозных людей деньги становятся <w>богом</w>, который управляет их решениями."
                            }
                        },
                        {
                            "text": "The famous singer was treated like a <w>god</w> by his screaming fans.",
                            "target_lang_translations": {
                                "ru": "Кричащие фанаты относились к известному певцу как к <w>богу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "imp"
                    ],
                    "common_phrases": [
                        "make a god of"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кто-то или что-то, чем чрезмерно восхищаются, кому поклоняются или отдают высший приоритет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идол",
                                "target_lang_sense_clarification": "предмет слепого поклонения"
                            },
                            {
                                "target_lang_word": "бог",
                                "target_lang_sense_clarification": "в переносном смысле"
                            },
                            {
                                "target_lang_word": "кумир",
                                "target_lang_sense_clarification": "объект восхищения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfe4b458-eaa8-38cc-b4f0-fd8342344fc5",
                    "sense_definition": "A person who is exceptionally skilled or talented in a particular activity.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He plays the guitar so well that he is considered a rock <w>god</w>.",
                            "target_lang_translations": {
                                "ru": "Он играет на гитаре так хорошо, что его считают рок-<w>богом</w>."
                            }
                        },
                        {
                            "text": "In the world of coding, she is an absolute <w>god</w> who can fix anything.",
                            "target_lang_translations": {
                                "ru": "В мире программирования она абсолютный <w>бог</w>, который может исправить что угодно."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который исключительно опытен или талантлив в определенной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бог",
                                "target_lang_sense_clarification": "мастер своего дела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39f47837-c907-38fa-b25a-684a2e048db1",
                    "sense_definition": "An exceedingly handsome or physically attractive man.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He looked like a Greek <w>god</w> after spending the entire summer at the beach.",
                            "target_lang_translations": {
                                "ru": "Он выглядел как греческий <w>бог</w>, проведя всё лето на пляже."
                            }
                        },
                        {
                            "text": "All the guests stared at him because he was such a handsome <w>god</w>.",
                            "target_lang_translations": {
                                "ru": "Все гости смотрели на него, потому что он был таким красивым <w>богом</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно красивый или физически привлекательный мужчина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бог",
                                "target_lang_sense_clarification": "о красивом мужчине"
                            },
                            {
                                "target_lang_word": "Аполлон",
                                "target_lang_sense_clarification": "эталон мужской красоты"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "9ac6061a-f3c2-3785-8405-89d765bc50ad",
                    "sense_definition": "Used to express strong emotions such as surprise, fear, annoyance, or frustration.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oh <w>God</w>, I completely forgot that we had a meeting today!",
                            "target_lang_translations": {
                                "ru": "О <w>Боже</w>, я совершенно забыл, что у нас сегодня встреча!"
                            }
                        },
                        {
                            "text": "My <w>God</w>, look at how much the children have grown since last year.",
                            "target_lang_translations": {
                                "ru": "<w>Боже</w> мой, посмотри, как выросли дети с прошлого года."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oh my God",
                        "good God",
                        "oh God"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения сильных эмоций, таких как удивление, страх, раздражение или разочарование."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Боже",
                                "target_lang_sense_clarification": "междометие"
                            },
                            {
                                "target_lang_word": "Господи",
                                "target_lang_sense_clarification": "междометие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "15ff1cde-2d18-3d74-9471-9532f9be8411",
                    "sense_definition": "To treat someone as a deity or to idolize them excessively.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loyal subjects <w>godded</w> their king, believing he could do no wrong.",
                            "target_lang_translations": {
                                "ru": "Верные подданные <w>обожествляли</w> своего короля, полагая, что он не может совершить ошибку."
                            }
                        },
                        {
                            "text": "He was <w>godded</w> by the media after his spectacular victory in the championship.",
                            "target_lang_translations": {
                                "ru": "СМИ <w>обожествляли</w> его после впечатляющей победы в чемпионате."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относиться к кому-либо как к божеству или чрезмерно боготворить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обожествлять",
                                "target_lang_sense_clarification": "делать богом"
                            },
                            {
                                "target_lang_word": "боготворить",
                                "target_lang_sense_clarification": "сильно почитать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
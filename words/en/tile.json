{
    "word_family": [
        "tiler",
        "tiling",
        "retile"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fbc68f4c-0c6f-36dc-88c6-e74ca3872a32",
                    "sense_definition": "A flat piece of baked clay, stone, or other material used for covering roofs, floors, or walls.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "construction_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to replace a broken <w>tile</w> on the roof before it rains.",
                            "target_lang_translations": {
                                "ru": "Нам нужно заменить сломанную <w>черепицу</w> на крыше, пока не пошел дождь."
                            }
                        },
                        {
                            "text": "She chose blue ceramic <w>tiles</w> for the bathroom walls.",
                            "target_lang_translations": {
                                "ru": "Она выбрала голубую керамическую <w>плитку</w> для стен ванной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "floor tiles",
                        "roof tiles",
                        "bathroom tiles",
                        "ceramic tiles",
                        "carpet tiles"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский кусок обожженной глины, камня или другого материала, используемый для покрытия крыш, полов или стен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плитка",
                                "target_lang_sense_clarification": "для стен и пола"
                            },
                            {
                                "target_lang_word": "черепица",
                                "target_lang_sense_clarification": "для крыши"
                            },
                            {
                                "target_lang_word": "кафель",
                                "target_lang_sense_clarification": "керамическая плитка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "350c7778-6ecf-3e02-a49f-9ca1dea76def",
                    "sense_definition": "A small flat piece used in board games like Scrabble, Mahjong, or Dominoes.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "game_piece",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Each player draws seven <w>tiles</w> from the bag at the start of the game.",
                            "target_lang_translations": {
                                "ru": "Каждый игрок тянет семь <w>фишек</w> из мешка в начале игры."
                            }
                        },
                        {
                            "text": "He placed a <w>tile</w> on the board to complete the word.",
                            "target_lang_translations": {
                                "ru": "Он положил <w>фишку</w> на доску, чтобы закончить слово."
                            }
                        }
                    ],
                    "synonyms": [
                        "piece"
                    ],
                    "common_phrases": [
                        "letter tiles",
                        "mahjong tiles",
                        "domino tiles"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая плоская фигура, используемая в настольных играх, таких как Скрэббл, Маджонг или Домино."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "костяшка",
                                "target_lang_sense_clarification": "домино, маджонг"
                            },
                            {
                                "target_lang_word": "фишка",
                                "target_lang_sense_clarification": "скрэббл, общая"
                            },
                            {
                                "target_lang_word": "плитка",
                                "target_lang_sense_clarification": "маджонг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7769abc-4842-3c50-8cbc-6beab998dec0",
                    "sense_definition": "A rectangular graphic image used to build a larger background or map in computer graphics.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The digital map is made up of hundreds of small <w>tiles</w> that load as you scroll.",
                            "target_lang_translations": {
                                "ru": "Цифровая карта состоит из сотен маленьких <w>тайлов</w>, которые загружаются при прокрутке."
                            }
                        },
                        {
                            "text": "Designers use repeating <w>tiles</w> to create seamless background textures.",
                            "target_lang_translations": {
                                "ru": "Дизайнеры используют повторяющиеся <w>плитки</w> для создания бесшовных фоновых текстур."
                            }
                        }
                    ],
                    "common_phrases": [
                        "map tiles",
                        "background tiles"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямоугольное графическое изображение, используемое для создания большого фона или карты в компьютерной графике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тайл",
                                "target_lang_sense_clarification": "термин в разработке"
                            },
                            {
                                "target_lang_word": "фрагмент",
                                "target_lang_sense_clarification": "часть карты"
                            },
                            {
                                "target_lang_word": "плитка",
                                "target_lang_sense_clarification": "элемент интерфейса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2706cd8-b7d4-38e7-9f3c-54bdb8fb4ad2",
                    "sense_definition": "A stiff hat, such as a top hat.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gentleman tipped his <w>tile</w> to the ladies as he walked past.",
                            "target_lang_translations": {
                                "ru": "Джентльмен приподнял свой <w>цилиндр</w> перед дамами, проходя мимо."
                            }
                        },
                        {
                            "text": "He looked quite dashing in his new coat and silk <w>tile</w>.",
                            "target_lang_translations": {
                                "ru": "Он выглядел весьма элегантно в новом пальто и шелковом <w>цилиндре</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жесткая шляпа, например, цилиндр (устаревший сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цилиндр",
                                "target_lang_sense_clarification": "головной убор"
                            },
                            {
                                "target_lang_word": "шляпа",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f37b526b-90e7-30cc-84f8-a6cca28e0775",
                    "sense_definition": "To cover a surface with tiles.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "construction_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to <w>tile</w> the kitchen floor instead of using wood.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>выложить плиткой</w> пол на кухне вместо использования дерева."
                            }
                        },
                        {
                            "text": "The builder <w>tiled</w> the shower area with white marble.",
                            "target_lang_translations": {
                                "ru": "Строитель <w>облицевал</w> душевую зону белым мрамором."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tile a floor",
                        "tile a bathroom",
                        "tile a roof",
                        "tile a wall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать поверхность плиткой или черепицей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облицовывать",
                                "target_lang_sense_clarification": "стены, пол"
                            },
                            {
                                "target_lang_word": "выкладывать плиткой",
                                "target_lang_sense_clarification": "пол, дорожки"
                            },
                            {
                                "target_lang_word": "крыть",
                                "target_lang_sense_clarification": "крышу черепицей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "503d52d4-a967-313d-923c-0a8e16d77cf6",
                    "sense_definition": "To arrange items, such as windows on a computer screen, next to each other without overlapping.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>tile</w> the open windows to view all documents simultaneously.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>расположить</w> открытые окна <w>мозаикой</w>, чтобы видеть все документы одновременно."
                            }
                        },
                        {
                            "text": "The software allows you to <w>tile</w> the image across the desktop background.",
                            "target_lang_translations": {
                                "ru": "Программа позволяет <w>замостить</w> изображением фон рабочего стола."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tile windows",
                        "tile vertically",
                        "tile horizontally"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Располагать элементы, например окна на экране, рядом друг с другом без перекрытия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замостить",
                                "target_lang_sense_clarification": "фон, текстуру"
                            },
                            {
                                "target_lang_word": "располагать мозаикой",
                                "target_lang_sense_clarification": "окна"
                            }
                        ]
                    }
                }
            ]
        }
    ]

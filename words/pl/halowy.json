{
    "word_family": [
        "hala",
        "halówka"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "73012052-8f58-3f6e-b5d8-470e6078f2f2",
                    "sense_definition": "Odnoszący się do sportów lub wydarzeń odbywających się w zamkniętym, zadaszonym obiekcie (hali sportowej), a nie na otwartym powietrzu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sport_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W zimie lekkoatleci rozpoczynają sezon <w>halowy</w>, startując w zawodach pod dachem.",
                            "target_lang_translations": {
                                "ru": "Зимой легкоатлеты начинают сезон <w>в помещении</w>, участвуя в соревнованиях под крышей."
                            }
                        },
                        {
                            "text": "Nasz lokalny klub zorganizował charytatywny turniej <w>halowy</w> w piłkę nożną.",
                            "target_lang_translations": {
                                "ru": "Наш местный клуб организовал благотворительный турнир по футболу <w>в зале</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "plenerowy",
                        "stadionowy"
                    ],
                    "common_phrases": [
                        "mistrzostwa halowe",
                        "turniej halowy",
                        "sezon halowy",
                        "rekord halowy",
                        "lekkoatletyka halowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к спорту или мероприятиям, проходящим в закрытом помещении (спортивном зале), а не на открытом воздухе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в помещении",
                                "target_lang_sense_clarification": "о спортивных сезонах, чемпионатах"
                            },
                            {
                                "target_lang_word": "зальный",
                                "target_lang_sense_clarification": "о видах спорта, проводимых в зале"
                            },
                            {
                                "target_lang_word": "крытый",
                                "target_lang_sense_clarification": "о стадионах, кортах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aad8313e-1f57-3c56-ac00-35034426fc5c",
                    "sense_definition": "Związany z konstrukcją, wyposażeniem lub funkcjonowaniem dużej hali (przemysłowej, targowej lub magazynowej).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "building_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowoczesny system <w>halowy</w> pozwala na szybką budowę dużych powierzchni magazynowych.",
                            "target_lang_translations": {
                                "ru": "Современная <w>зальная</w> система позволяет быстро строить большие складские площади."
                            }
                        },
                        {
                            "text": "Specjalistyczny dźwig <w>halowy</w> służy do przenoszenia ciężkich elementów wewnątrz fabryki.",
                            "target_lang_translations": {
                                "ru": "Специализированный <w>цеховой</w> кран служит для перемещения тяжелых элементов внутри завода."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dźwig halowy",
                        "konstrukcja halowa",
                        "system halowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с конструкцией, оборудованием или функционированием большого зала (промышленного, выставочного или складского)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цеховой",
                                "target_lang_sense_clarification": "относящийся к производственному цеху"
                            },
                            {
                                "target_lang_word": "зальный",
                                "target_lang_sense_clarification": "относящийся к конструкции типа «зал»"
                            },
                            {
                                "target_lang_word": "ангарный",
                                "target_lang_sense_clarification": "относящийся к ангарам или складам"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3c3227e4-f656-3a75-9852-8324f0c1c99d",
                    "sense_definition": "Simple past and past participle of <w>hear</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>heard</w> a strange noise outside my window last night.",
                            "target_lang_translations": {
                                "ru": "Я <w>услышал</w> странный шум за окном прошлой ночью."
                            }
                        },
                        {
                            "text": "Have you <w>heard</w> the news about the upcoming festival?",
                            "target_lang_translations": {
                                "ru": "Ты <w>слышал</w> новости о предстоящем фестивале?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "heard of",
                        "heard about",
                        "make oneself heard",
                        "ever heard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени глагола hear (слышать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слышал",
                                "target_lang_sense_clarification": "прошедшее время, несовершенный вид"
                            },
                            {
                                "target_lang_word": "услышал",
                                "target_lang_sense_clarification": "прошедшее время, совершенный вид"
                            },
                            {
                                "target_lang_word": "услышанный",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        },
        {

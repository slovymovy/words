    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c0eabcc3-b618-340e-b29f-5793041eed46",
                    "sense_definition": "Een zaak, onderwerp of aangelegenheid die besproken wordt of waarover nagedacht wordt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is slechts een <w>kwestie</w> van tijd voordat we de uitslag weten.",
                            "target_lang_translations": {
                                "ru": "Это лишь <w>вопрос</w> времени, когда мы узнаем результат."
                            }
                        },
                        {
                            "text": "Over de persoon in <w>kwestie</w> wil ik liever geen uitspraken doen.",
                            "target_lang_translations": {
                                "ru": "Я бы предпочел не высказываться о человеке, о котором идет <w>речь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaak",
                        "vraag"
                    ],
                    "common_phrases": [
                        "een kwestie van tijd",
                        "in kwestie",
                        "een andere kwestie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дело, предмет или обстоятельство, которое обсуждается или обдумывается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вопрос",
                                "target_lang_sense_clarification": "тема обсуждения"
                            },
                            {
                                "target_lang_word": "дело",
                                "target_lang_sense_clarification": "обстоятельство или случай"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f21d029-f215-36f1-8c8a-c33ce2d6b8ae",
                    "sense_definition": "Een moeilijkheid, probleem of vraagstuk dat om een oplossing vraagt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten deze ingewikkelde <w>kwestie</w> zo snel mogelijk oplossen.",
                            "target_lang_translations": {
                                "ru": "Мы должны решить этот сложный <w>вопрос</w> как можно скорее."
                            }
                        },
                        {
                            "text": "Voor hem is geld nooit een <w>kwestie</w> geweest bij het maken van keuzes.",
                            "target_lang_translations": {
                                "ru": "Для него деньги никогда не были <w>проблемой</w> при принятии решений."
                            }
                        }
                    ],
                    "synonyms": [
                        "vraagstuk",
                        "zaak",
                        "vraag"
                    ],
                    "common_phrases": [
                        "een heikele kwestie",
                        "geen kwestie zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудность, проблема или задача, требующая решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проблема",
                                "target_lang_sense_clarification": "трудность"
                            },
                            {
                                "target_lang_word": "вопрос",
                                "target_lang_sense_clarification": "задача, требующая решения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b195aad-fb19-316d-a3b0-ca381590e773",
                    "sense_definition": "Een geschil, ruzie of onenigheid tussen verschillende partijen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De diplomatieke <w>kwestie</w> zorgde voor veel spanningen tussen de twee landen.",
                            "target_lang_translations": {
                                "ru": "Дипломатический <w>конфликт</w> вызвал большую напряженность между двумя странами."
                            }
                        },
                        {
                            "text": "De buren hebben hun slepende <w>kwestie</w> over de erfgrens eindelijk bijgelegd.",
                            "target_lang_translations": {
                                "ru": "Соседи наконец уладили свой затянувшийся <w>спор</w> о границе участка."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спор, ссора или разногласие между различными сторонами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спор",
                                "target_lang_sense_clarification": "разногласие"
                            },
                            {
                                "target_lang_word": "конфликт",
                                "target_lang_sense_clarification": "столкновение интересов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

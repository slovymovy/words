    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5403a5fa-4a68-3ca8-9a5a-87a811886405",
                    "sense_definition": "Страдательная форма глагола <w>проигрывать</w>; подвергаться воспроизведению (о музыке, записи) или поражению (о сражении, споре).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот видеофайл не <w>проигрывается</w> на старом компьютере.",
                            "target_lang_translations": {
                                "en": "This video file <w>won't play</w> on the old computer."
                            }
                        },
                        {
                            "text": "Иногда сражение <w>проигрывается</w> из-за одной тактической ошибки.",
                            "target_lang_translations": {
                                "en": "Sometimes a battle <w>is lost</w> due to a single tactical error."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be reproduced (of music or recordings) or to be lost (of a battle or dispute)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be played",
                                "target_lang_sense_clarification": "media or recording"
                            },
                            {
                                "target_lang_word": "play",
                                "target_lang_sense_clarification": "intransitive, of media files"
                            },
                            {
                                "target_lang_word": "be lost",
                                "target_lang_sense_clarification": "battle or game"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4cdcfb1-ad0a-365c-9822-7067b034bdbc",
                    "sense_definition": "Терять свои деньги или имущество в результате участия в азартной игре.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он имеет привычку постоянно <w>проигрываться</w> в карты до последней копейки.",
                            "target_lang_translations": {
                                "en": "He has a habit of constantly <w>losing everything</w> at cards down to the last penny."
                            }
                        },
                        {
                            "text": "Не стоит садиться за стол с профессионалами, если боишься <w>проигрываться</w>.",
                            "target_lang_translations": {
                                "en": "You shouldn't sit at the table with professionals if you're afraid of <w>losing your shirt</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "продуваться"
                    ],
                    "common_phrases": [
                        "проигрываться в пух и прах",
                        "проигрываться до копейки",
                        "проигрываться дотла"
                    ],
                    "target_lang_definitions": {
                        "en": "To lose one's money or property as a result of gambling."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lose everything",
                                "target_lang_sense_clarification": "gambling context"
                            },
                            {
                                "target_lang_word": "lose one's shirt",
                                "target_lang_sense_clarification": "idiomatic"
                            }
                        ]
                    }
                }
            ]
        }
    ]

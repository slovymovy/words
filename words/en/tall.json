                    ],
                    "common_phrases": [
                        "tall tale",
                        "tall story",
                        "tall order"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in idioms like 'tall tale' or 'tall order'"

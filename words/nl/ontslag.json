                {
                    "sense_id": "7ab5f203-d3f4-3c50-8ddd-e53216aa2fe9",
                    "sense_definition": "Het beëindigen van een arbeidscontract door de werkgever",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "employment_termination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de slechte economische situatie kreeg hij onverwachts zijn <w>ontslag</w>.",
                            "target_lang_translations": {
                                "en": "Due to the bad economic situation, he unexpectedly received his <w>dismissal</w>.",
                                "ru": "Из-за плохой экономической ситуации он неожиданно получил <w>увольнение</w>."
                            }
                        },
                        {
                            "text": "Bij een ontslag op staande voet moet je het pand direct verlaten.",
                            "target_lang_translations": {
                                "en": "In the event of a summary <w>dismissal</w>, you must leave the premises immediately.",
                                "ru": "При <w>увольнении</w> без предупреждения нужно немедленно покинуть здание."
                            }
                        }
                    ],
                    "synonyms": [
                        "afdanking"
                    ],
                    "antonyms": [
                        "aanstelling",
                        "aanname"
                    ],
                    "common_phrases": [
                        "ontslag op staande voet",
                        "ontslag krijgen",
                        "gedwongen ontslag",
                        "collectief ontslag"
                    ],
                    "target_lang_definitions": {
                        "en": "The termination of an employment contract by the employer",
                        "ru": "Расторжение трудового договора по инициативе работодателя"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dismissal",
                                "target_lang_sense_clarification": "formal termination of employment"
                            },
                            {
                                "target_lang_word": "firing",
                                "target_lang_sense_clarification": "informal, termination of employment"
                            },
                            {
                                "target_lang_word": "sacking",
                                "target_lang_sense_clarification": "informal, often implying fault"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "увольнение",
                                "target_lang_sense_clarification": "расторжение контракта"
                            },
                            {
                                "target_lang_word": "расчёт",
                                "target_lang_sense_clarification": "разг. увольнение с работы"
                            }
                        ]

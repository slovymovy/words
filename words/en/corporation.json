                    ],
                    "synonyms": [
                        "paunch"
                    ],
                    "common_phrases": [
                        "multinational corporation",
                        "major corporation",
                        "global corporation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная компания или группа компаний, уполномоченная действовать как единое юридическое лицо."
                    },
                    "translations": {
                        "ru": [

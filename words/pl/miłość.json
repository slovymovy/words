                {
                    "sense_id": "e5c746e0-6e15-3a6a-b9f1-07e595e2d9ef",
                    "sense_definition": "Potok w Polsce będący lewym dopływem Raby.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Moczyliśmy nogi w <w>Miłości</w>, zajadając lody na patykach.",
                            "target_lang_translations": {
                                "ru": "Мы мочили ноги в <w>Милости</w>, поедая мороженое на палочке."

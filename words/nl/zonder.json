                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vertrok <w>zonder</w> dat iemand het merkte.",
                            "target_lang_translations": {
                                "en": "He left <w>without</w> anyone noticing."
                            }
                        },
                        {
                            "text": "Je kunt dit niet doen <w>zonder</w> dat er gevolgen zijn.",
                            "target_lang_translations": {
                                "en": "You cannot do this <w>without</w> there being consequences."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zonder dat"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to introduce a subordinate clause indicating that a certain circumstance does not take place."

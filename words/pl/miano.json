            "senses": [
                {
                    "sense_id": "74dc4b11-697f-3910-80f8-8f5af97dd0df",
                    "sense_definition": "Określenie, nazwa lub tytuł, często o charakterze wartościującym, nadawane komuś lub czemuś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "designation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten film w pełni zasługuje na <w>miano</w> kultowego dzieła kinematografii.",

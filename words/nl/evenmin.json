                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij houdt niet van voetbal en <w>evenmin</w> van tennissen.",
                            "target_lang_translations": {
                                "en": "He doesn't like football, and <w>neither</w> does he like tennis."
                            }
                        },
                        {
                            "text": "Dat is al <w>evenmin</w> een goed idee.",
                            "target_lang_translations": {
                                "en": "That is <w>just as little</w> a good idea."
                            }
                        }
                    ],

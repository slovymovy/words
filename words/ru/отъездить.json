            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6f96788d-f1af-34ee-9df8-54b273cb1981",
                    "sense_definition": "Провести определённое время в езде или проехать какое-либо расстояние.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы честно <w>отъездили</w> на этом автомобиле пять лет без поломок.",
                            "target_lang_translations": {
                                "en": "We honestly <w>drove</w> this car for five years without breakdowns."
                            }
                        },
                        {
                            "text": "Таксист <w>отъездил</w> свою смену и отправился домой отдыхать.",
                            "target_lang_translations": {
                                "en": "The taxi driver <w>drove</w> his shift and went home to rest."
                            }
                        }
                    ],
                    "synonyms": [
                        "проездить"
                    ],
                    "common_phrases": [
                        "отъездить сезон",
                        "отъездить срок",
                        "отъездить смену"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend a certain amount of time driving or riding, or to cover a specific distance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to drive",
                                "target_lang_sense_clarification": "for a certain period of time"
                            },
                            {
                                "target_lang_word": "to ride",
                                "target_lang_sense_clarification": "for a certain period of time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0af44e3a-59ba-336c-aab8-a9f4ce584e70",
                    "sense_definition": "Привести в негодность, испортить или сильно утомить долгой и небрежной ездой (о животном или транспортном средстве).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За один год небрежной эксплуатации он полностью <w>отъездил</w> новый мотоцикл.",
                            "target_lang_translations": {
                                "en": "In one year of careless operation, he completely <w>wore out</w> the new motorcycle."
                            }
                        },
                        {
                            "text": "Кучер так торопился, что едва не <w>отъездил</w> лошадей до полусмерти.",
                            "target_lang_translations": {
                                "en": "The coachman was in such a hurry that he almost <w>exhausted</w> the horses to death."
                            }
                        }
                    ],
                    "synonyms": [
                        "заездить",
                        "изнурить",
                        "угробить"
                    ],
                    "common_phrases": [
                        "отъездить лошадь",
                        "отъездить машину"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To ruin, damage, or exhaust a vehicle or animal through long or careless use."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to wear out",
                                "target_lang_sense_clarification": "a vehicle or animal through use"
                            },
                            {
                                "target_lang_word": "to run into the ground",
                                "target_lang_sense_clarification": "idiom: to ruin something by overuse"
                            },
                            {
                                "target_lang_word": "to exhaust",
                                "target_lang_sense_clarification": "an animal by riding/driving"
                            }
                        ]
                    }
                }
            ]

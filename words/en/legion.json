    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9dcfc0be-402e-31e4-b1a3-e2d30c9395ee",
                    "sense_definition": "A vast number of people or things; a multitude.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Legions of fans waited outside the stadium to see the famous band.",
                            "target_lang_translations": {
                                "ru": "<w>Легионы</w> фанатов ждали у стадиона, чтобы увидеть знаменитую группу."
                            }
                        },
                        {
                            "text": "He has a <w>legion</w> of followers on social media who support his work.",
                            "target_lang_translations": {
                                "ru": "У него <w>легион</w> подписчиков в социальных сетях, которые поддерживают его работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "multitude",
                        "host",
                        "throng",
                        "mass"
                    ],
                    "common_phrases": [
                        "legions of fans",
                        "a legion of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Огромное количество людей или вещей; множество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "легион",
                                "target_lang_sense_clarification": "перен. огромное множество"
                            },
                            {
                                "target_lang_word": "множество",
                                "target_lang_sense_clarification": "большое количество"
                            },
                            {
                                "target_lang_word": "тьма",
                                "target_lang_sense_clarification": "разг. очень много"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "094eab67-bc37-3e33-8015-a956b5109012",
                    "sense_definition": "The major unit or division of the Roman army, usually comprising 3000 to 6000 infantry soldiers.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "military_unit_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Roman <w>legion</w> was known for its discipline and effective battle formations.",
                            "target_lang_translations": {
                                "ru": "Римский <w>легион</w> был известен своей дисциплиной и эффективными боевыми построениями."
                            }
                        },
                        {
                            "text": "A standard <w>legion</w> included thousands of infantry supported by cavalry troops.",
                            "target_lang_translations": {
                                "ru": "Стандартный <w>легион</w> включал тысячи пехотинцев при поддержке кавалерии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Roman legion",
                        "tenth legion"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context referring to Ancient Rome"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная организационная единица римской армии, обычно состоящая из 3000–6000 пехотинцев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "легион",
                                "target_lang_sense_clarification": "римское воинское подразделение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ed8dffa-7fdb-3d75-97e2-a0a76a4f4989",

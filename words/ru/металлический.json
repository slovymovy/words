            "senses": [
                {
                    "sense_id": "8814b0bd-77ea-3e2d-9fd5-b1cbaffc4590",
                    "sense_definition": "Сделанный из металла, состоящий из металла.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В коробке лежали старые <металлические> инструменты для ремонта.",
                            "target_lang_translations": {
                                "en": "Old <metal> tools for repair lay in the box."
                            }
                        },
                        {
                            "text": "Мы решили установить в квартиру прочную <металлическую> дверь.",
                            "target_lang_translations": {
                                "en": "We decided to install a sturdy <metal> door in the apartment."
                            }
                        },
                        {
                            "text": "В кармане у него громко звенели <металлические> монеты.",
                            "target_lang_translations": {
                                "en": "<Metal> coins jingled loudly in his pocket."
                            }
                        }
                    ],
                    "antonyms": [
                        "неметаллический"
                    ],
                    "common_phrases": [
                        "металлическая дверь",
                        "металлический предмет",
                        "металлическая конструкция",
                        "металлический каркас"
                    ],
                    "target_lang_definitions": {
                        "en": "Made of metal, consisting of metal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "metal",
                                "target_lang_sense_clarification": "made of metal"
                            },
                            {
                                "target_lang_word": "metallic",
                                "target_lang_sense_clarification": "made of or containing metal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "746c0e97-e37c-3c23-9940-e534876505fc",
                    "sense_definition": "Напоминающий металл по цвету или блеску; холодный и блестящий.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этой новой машины красивый <металлический> блеск.",
                            "target_lang_translations": {
                                "en": "This new car has a beautiful <metallic> shine."
                            }
                        },
                        {
                            "text": "Вода в озере имела холодный <металлический> оттенок.",
                            "target_lang_translations": {
                                "en": "The water in the lake had a cold <metallic> tint."
                            }
                        }
                    ],
                    "synonyms": [
                        "стальной",
                        "серебристый"
                    ],
                    "common_phrases": [
                        "металлический блеск",
                        "металлический цвет",
                        "металлический оттенок"
                    ],
                    "target_lang_definitions": {
                        "en": "Resembling metal in color or shine; cold and shiny."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "metallic",
                                "target_lang_sense_clarification": "resembling metal"
                            }

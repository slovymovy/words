    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5d4d7890-0c1a-3ba4-9913-af3b43c2d3df",
                    "sense_definition": "Jednostka monetarna w Polsce, obecnie setna część złotego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "currency_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pudełko pastylek miętowych kosztuje dwa złote i dwadzieścia <w>groszy</w>.",
                            "target_lang_translations": {
                                "ru": "Коробочка мятных пастилок стоит два злотых и двадцать <w>грошей</w>."
                            }
                        },
                        {
                            "text": "Nie mam przy sobie ani <w>grosza</w>, więc nie mogę tego kupić.",
                            "target_lang_translations": {
                                "ru": "У меня с собой нет ни <w>гроша</w>, поэтому я не могу это купить."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrót gr",
                        "gw. warsz. groszak"
                    ],
                    "common_phrases": [
                        "grosz do grosza",
                        "co do grosza",
                        "ani grosza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная единица Польши, равная одной сотой злотого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грош",
                                "target_lang_sense_clarification": "польская монета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "539fabc8-2b07-360a-8ab3-a3e42797f49d",
                    "sense_definition": "Nieznacząca suma pieniędzy; kwota nieprzedstawiająca większej wartości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zostawił mi jakieś <w>grosze</w> na utrzymanie i myślał, że wszystko jest w porządku.",
                            "target_lang_translations": {
                                "ru": "Он оставил мне какие-то <w>гроши</w> на жизнь и думал, что всё в порядке."
                            }
                        },
                        {
                            "text": "Udało mi się kupić ten świetny płaszcz za <w>grosze</w> na wyprzedaży.",
                            "target_lang_translations": {
                                "ru": "Мне удалось купить это отличное пальто за <w>копейки</w> на распродаже."
                            }
                        }
                    ],
                    "synonyms": [
                        "śmieszne pieniądze"
                    ],
                    "common_phrases": [
                        "za grosze",
                        "zarabiać grosze",
                        "kosztować grosze"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane potocznie w odniesieniu do małych kwot"
                        },
                        {
                            "trait_type": "form",
                            "comment": "zwykle w liczbie mnogiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Незначительная сумма денег; ничтожно малая стоимость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копейки",
                                "target_lang_sense_clarification": "перен. малая сумма"
                            },
                            {
                                "target_lang_word": "гроши",
                                "target_lang_sense_clarification": "перен. ничтожная сумма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4848e812-8279-31d5-bff8-5c34e4d053b1",
                    "sense_definition": "Miedziana lub srebrna moneta różnej wartości, bita dawniej w krajach Europy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "currency_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum numizmatycznym oglądaliśmy stary srebrny <w>grosz</w> z czasów średniowiecza.",
                            "target_lang_translations": {
                                "ru": "В нумизматическом музее мы рассматривали старинный серебряный <w>грош</w> времен средневековья."
                            }
                        },
                        {
                            "text": "Kolekcjoner zapłacił dużą sumę za rzadki <w>grosz</w> praski.",
                            "target_lang_translations": {
                                "ru": "Коллекционер заплатил большую сумму за редкий пражский <w>грош</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grosz praski"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy dawnych monet"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная медная или серебряная монета, чеканившаяся в Европе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грош",
                                "target_lang_sense_clarification": "старинная монета"
                            }
                        ]
                    }
                }

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "12ec32af-35ff-3933-ae1b-eff86d5877ca",
                    "sense_definition": "Sprawdzić stan zdrowia osoby lub funkcjonowanie organizmu za pomocą narzędzi medycznych lub zmysłów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "medical_exam",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz musi dokładnie <zbadać> pacjenta, aby postawić właściwą diagnozę.",
                            "target_lang_translations": {
                                "ru": "Врач должен тщательно <обследовать> пациента, чтобы поставить правильный диагноз."
                            }
                        },
                        {
                            "text": "Pielęgniarka przyszła, żeby <zbadać> ciśnienie krwi chorego.",
                            "target_lang_translations": {
                                "ru": "Медсестра пришла, чтобы <измерить> артериальное давление больного."
                            }
                        }
                    ],
                    "synonyms": [
                        "osłuchać",
                        "skontrolować",
                        "obejrzeć"
                    ],
                    "common_phrases": [
                        "zbadać pacjenta",
                        "zbadać krew",
                        "zbadać puls",
                        "zbadać wzrok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверить состояние здоровья человека или функционирование организма с помощью медицинских инструментов или органов чувств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обследовать",
                                "target_lang_sense_clarification": "провести медицинский осмотр"
                            },
                            {
                                "target_lang_word": "осмотреть",
                                "target_lang_sense_clarification": "визуально или физически проверить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76591902-302c-3671-bb4e-b928173b08f0",
                    "sense_definition": "Przeprowadzić analizę lub dochodzenie w celu poznania szczegółów, przyczyn lub właściwości jakiegoś zjawiska.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "investigation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja przyjechała na miejsce, aby <zbadać> przyczyny tego wypadku.",
                            "target_lang_translations": {
                                "ru": "Полиция прибыла на место, чтобы <расследовать> причины этой аварии."
                            }
                        },
                        {
                            "text": "Firma chce <zbadać> rynek przed wprowadzeniem nowego produktu do sprzedaży.",
                            "target_lang_translations": {
                                "ru": "Компания хочет <исследовать> рынок перед запуском нового продукта в продажу."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeanalizować",
                        "sprawdzić",
                        "zweryfikować",
                        "zgłębić",
                        "przestudiować"
                    ],
                    "antonyms": [
                        "zignorować",
                        "pominąć"
                    ],
                    "common_phrases": [
                        "zbadać sprawę",
                        "zbadać przyczyny",
                        "zbadać rynek",
                        "dokładnie zbadać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Провести анализ или расследование с целью выяснения деталей, причин или свойств какого-либо явления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исследовать",
                                "target_lang_sense_clarification": "изучить научным или аналитическим методом"
                            },
                            {
                                "target_lang_word": "изучить",
                                "target_lang_sense_clarification": "внимательно рассмотреть"
                            },
                            {
                                "target_lang_word": "расследовать",
                                "target_lang_sense_clarification": "выяснить обстоятельства происшествия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9ac6ab3-376b-351f-8917-5c1351759161",
                    "sense_definition": "Poddać się badaniu lekarskiemu w celu sprawdzenia własnego stanu zdrowia (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_exam",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy dorosły człowiek powinien regularnie się <zbadać>, aby uniknąć poważnych chorób.",
                            "target_lang_translations": {
                                "ru": "Каждый взрослый человек должен регулярно <обследоваться>, чтобы избежать серьезных заболеваний."
                            }
                        },
                        {
                            "text": "Muszę iść do przychodni i <zbadać> się u specjalisty.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти в поликлинику и <обследоваться> у специалиста."
                            }
                        }
                    ],
                    "synonyms": [
                        "przebadać się",
                        "skontrolować się"
                    ],
                    "common_phrases": [
                        "zbadać się profilaktycznie",
                        "muszę się zbadać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пройти медицинское обследование для проверки собственного здоровья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обследоваться",
                                "target_lang_sense_clarification": "пройти осмотр у врача"
                            },
                            {
                                "target_lang_word": "провериться",
                                "target_lang_sense_clarification": "разг. проверить состояние здоровья"
                            }
                        ]
                    }
                }
            ]
        }
    ]

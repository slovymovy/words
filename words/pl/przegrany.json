    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "11a56ca2-9261-35db-b336-8370c1690733",
                    "sense_definition": "Taki, który poniósł porażkę w walce, grze lub rywalizacji sportowej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "defeat_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz <przegrany> zespół musiał szybko opuścić boisko.",
                            "target_lang_translations": {
                                "ru": "Наша <проигравшая> команда должна была быстро покинуть поле."
                            }
                        },
                        {
                            "text": "Mimo że był <przegrany>, podał rękę swojemu przeciwnikowi.",
                            "target_lang_translations": {
                                "ru": "Хотя он был <проигравшим>, он пожал руку своему противнику."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokonany",
                        "zwyciężony"
                    ],
                    "antonyms": [
                        "wygrany",
                        "zwycięski"
                    ],
                    "common_phrases": [
                        "przegrany mecz",
                        "przegrany zawodnik",
                        "przegrany zespół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто потерпел поражение в борьбе, игре или спортивном соревновании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проигравший",
                                "target_lang_sense_clarification": "о сопернике или команде"
                            },
                            {
                                "target_lang_word": "побеждённый",
                                "target_lang_sense_clarification": "кем-то в борьбе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f40c65c-fa60-3e2e-a152-b199072a3a46",
                    "sense_definition": "Taki, który nie ma szans na powodzenie lub sukces; skazany na niepowodzenie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hopeless_situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiedziałem, że to z góry <przegrana> sprawa, ale próbowałem walczyć.",
                            "target_lang_translations": {
                                "ru": "Я знал, что это заранее <проигранное> дело, но пытался бороться."
                            }
                        },
                        {
                            "text": "Nie stawiaj się od razu na <przegranej> pozycji w tej dyskusji.",
                            "target_lang_translations": {
                                "ru": "Не ставь себя сразу в <проигрышное> положение в этой дискуссии."
                            }
                        }
                    ],
                    "synonyms": [
                        "beznadziejny",
                        "stracony",
                        "skończony"
                    ],
                    "antonyms": [
                        "wygrany"
                    ],
                    "common_phrases": [
                        "przegrana sprawa",
                        "na przegranej pozycji",
                        "z góry przegrany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий шансов на успех; обречённый на неудачу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проигрышный",
                                "target_lang_sense_clarification": "о позиции или ситуации"
                            },
                            {
                                "target_lang_word": "безнадёжный",
                                "target_lang_sense_clarification": "не имеющий шансов"
                            },
                            {
                                "target_lang_word": "обречённый",
                                "target_lang_sense_clarification": "на провал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4f9910f-f933-38e3-9e9b-76abf41c95d3",
                    "sense_definition": "Taki, który czuje się zrezygnowany, nieszczęśliwy lub zniszczony z powodu życiowych niepowodzeń.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "broken_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po utracie pracy i rodziny czuł się całkowicie <przegrany>.",
                            "target_lang_translations": {
                                "ru": "После потери работы и семьи он чувствовал себя полностью <сломленным>."
                            }
                        },
                        {
                            "text": "To smutne patrzeć na tak zdolnego, a jednak życiowo <przegranego> człowieka.",
                            "target_lang_translations": {
                                "ru": "Грустно смотреть на такого способного, но <неудавшегося> в жизни человека."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezradny",
                        "skończony"
                    ],
                    "antonyms": [
                        "spełniony",
                        "szczęśliwy"
                    ],
                    "common_phrases": [
                        "czuć się przegranym",
                        "człowiek przegrany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий себя подавленным, несчастным или сломленным из-за жизненных неудач."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сломленный",
                                "target_lang_sense_clarification": "жизненными обстоятельствами"
                            },
                            {
                                "target_lang_word": "конченый",
                                "target_lang_sense_clarification": "разг., полностью опустившийся"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [
        "терпение",
        "терпеливый",
        "терпимость",
        "терпимый",
        "нестерпимый",
        "вытерпеть",
        "перетерпеть",
        "потерпеть"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "52d597f2-87c5-3159-9a94-b580b360a978",
                    "sense_definition": "Испытывать сильную неприязнь к кому-либо или чему-либо, очень не любить (обычно используется с отрицанием).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "dislike",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>терпеть</w> не могу, когда люди опаздывают на встречи.",
                            "target_lang_translations": {
                                "en": "I can't <w>stand</w> it when people are late for meetings."
                            }
                        },
                        {
                            "text": "Мой брат совершенно не <w>терпит</w> варёный лук в супе.",
                            "target_lang_translations": {
                                "en": "My brother absolutely cannot <w>stand</w> boiled onions in soup."
                            }
                        }
                    ],
                    "synonyms": [
                        "переносить"
                    ],
                    "antonyms": [
                        "любить",
                        "обожать"
                    ],
                    "common_phrases": [
                        "терпеть не могу",
                        "не терпеть лжи"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in the phrase 'терпеть не могу'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To feel strong dislike towards someone or something (usually used with negation)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand",
                                "target_lang_sense_clarification": "usually in negative contexts like 'can't stand'"
                            },
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "usually in negative contexts like 'can't bear'"
                            },
                            {
                                "target_lang_word": "tolerate",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5748111c-6363-3132-8f62-7222a3298544",
                    "sense_definition": "Стойко переносить физическую боль, страдания или тяжёлые жизненные обстоятельства.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "endure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Раненый солдат стиснул зубы и молча <w>терпел</w> сильную боль.",
                            "target_lang_translations": {
                                "en": "The wounded soldier clenched his teeth and silently <w>endured</w> the severe pain."
                            }
                        },
                        {
                            "text": "Нам пришлось <w>терпеть</w> холод и голод во время похода.",
                            "target_lang_translations": {
                                "en": "We had to <w>endure</w> cold and hunger during the hike."
                            }
                        }
                    ],
                    "synonyms": [
                        "переносить",
                        "сносить",
                        "переживать"
                    ],
                    "antonyms": [
                        "сдаваться"
                    ],
                    "common_phrases": [
                        "терпеть боль",
                        "терпеть голод",
                        "терпеть нужду"
                    ],
                    "target_lang_definitions": {
                        "en": "To steadfastly endure physical pain, suffering, or difficult life circumstances."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "endure",
                                "target_lang_sense_clarification": "pain or hardship"
                            },
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "suffering or difficulty"
                            },
                            {
                                "target_lang_word": "suffer",
                                "target_lang_sense_clarification": "pain"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14bdf828-c90b-31d9-a015-8f833dcf74ad",
                    "sense_definition": "Мириться с наличием кого-либо или чего-либо, позволять чему-либо происходить, несмотря на недовольство.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "endure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она больше не хотела <w>терпеть</w> его грубое поведение и ушла.",
                            "target_lang_translations": {
                                "en": "She didn't want to <w>put up with</w> his rude behavior anymore and left."
                            }
                        },
                        {
                            "text": "Учитель сказал, что не будет <w>терпеть</w> шум в классе.",
                            "target_lang_translations": {
                                "en": "The teacher said he would not <w>tolerate</w> noise in the classroom."
                            }
                        }
                    ],
                    "synonyms": [
                        "мириться",
                        "допускать",
                        "сносить"
                    ],
                    "common_phrases": [
                        "терпеть обиды",
                        "терпеть унижения",
                        "с трудом терпеть"
                    ],
                    "target_lang_definitions": {
                        "en": "To reconcile with the presence of someone or something, to allow something to happen despite dissatisfaction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put up with",
                                "target_lang_sense_clarification": "unpleasant behavior or situations"
                            },
                            {
                                "target_lang_word": "tolerate",
                                "target_lang_sense_clarification": "bad behavior"
                            },
                            {
                                "target_lang_word": "stand",
                                "target_lang_sense_clarification": "informal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4244291c-23a9-3027-b24d-3812330fd28d",
                    "sense_definition": "Ожидать чего-либо, сдерживая свои чувства, желания или физические потребности.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "wait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если ты хочешь добиться успеха, нужно уметь ждать и <w>терпеть</w>.",
                            "target_lang_translations": {
                                "en": "If you want to achieve success, you need to know how to wait and <w>be patient</w>."
                            }
                        },
                        {
                            "text": "Время <w>терпит</w>, поэтому мы можем обсудить детали позже.",
                            "target_lang_translations": {
                                "en": "Time <w>can wait</w>, so we can discuss the details later."
                            }
                        }
                    ],
                    "synonyms": [
                        "ждать"
                    ],
                    "antonyms": [
                        "спешить"
                    ],
                    "common_phrases": [
                        "время терпит",
                        "время не терпит",
                        "потерпи немного"
                    ],
                    "target_lang_definitions": {
                        "en": "To wait for something while suppressing one's feelings, desires, or physical needs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be patient",
                                "target_lang_sense_clarification": "state of waiting"
                            },
                            {
                                "target_lang_word": "wait",
                                "target_lang_sense_clarification": "regarding time or needs"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4d83a84-0933-3afd-9348-a2b04163a134",
                    "sense_definition": "Испытывать или переживать что-либо неприятное, тяжёлое (обычно о неудачах, бедствиях или потерях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "undergo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом матче наша команда <w>терпит</w> обидное поражение.",
                            "target_lang_translations": {
                                "en": "In this match, our team is <w>suffering</w> a bitter defeat."
                            }
                        },
                        {
                            "text": "Компания начала <w>терпеть</w> огромные убытки из-за экономического кризиса.",
                            "target_lang_translations": {
                                "en": "The company began to <w>suffer</w> huge losses due to the economic crisis."
                            }
                        }
                    ],
                    "synonyms": [
                        "испытывать",
                        "переживать",
                        "переносить"
                    ],
                    "common_phrases": [
                        "терпеть поражение",
                        "терпеть убытки",
                        "терпеть неудачу",
                        "терпеть бедствие",
                        "терпеть крах"
                    ],
                    "target_lang_definitions": {
                        "en": "To experience or undergo something unpleasant or difficult (usually regarding failures, disasters, or losses)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suffer",
                                "target_lang_sense_clarification": "losses, defeat, or disaster"
                            },
                            {
                                "target_lang_word": "sustain",
                                "target_lang_sense_clarification": "losses or injuries"
                            },
                            {
                                "target_lang_word": "undergo",
                                "target_lang_sense_clarification": "difficulties"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b28c5b1-1b56-3c40-8d16-4a1843380b6c",
                    "sense_definition": "Категорически не допускать возможности чего-либо (обычно о времени, делах или строгих правилах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "allow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это срочное дело не <w>терпит</w> никаких отлагательств.",
                            "target_lang_translations": {
                                "en": "This urgent matter <w>brooks</w> no delay."
                            }
                        },
                        {
                            "text": "Строгий тон генерала не <w>терпел</w> возражений со стороны подчинённых.",
                            "target_lang_translations": {
                                "en": "The general's strict tone <w>brooked</w> no objections from subordinates."
                            }
                        }
                    ],
                    "synonyms": [
                        "допускать"
                    ],
                    "common_phrases": [
                        "не терпеть отлагательств",
                        "не терпеть возражений"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often used in formal or literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To categorically not allow the possibility of something (usually regarding time, business, or strict rules)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brook",
                                "target_lang_sense_clarification": "formal, usually negative (e.g., no delay)"
                            },
                            {
                                "target_lang_word": "admit of",
                                "target_lang_sense_clarification": "formal, allow for"
                            },
                            {
                                "target_lang_word": "tolerate",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "87d243f1-035b-3629-8348-59fe1d5299e7",
                    "sense_definition": "Het gebied binnen de grenzen van het eigen land.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "domestic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel mensen blijven dit jaar in het <w>binnenland</w> voor hun vakantie.",
                            "target_lang_translations": {
                                "en": "Many people are staying within <w>the country</w> for their holidays this year.",
                                "ru": "Многие люди в этом году остаются проводить отпуск <w>внутри страны</w>."
                            }
                        },
                        {
                            "text": "Het nieuws over de verkiezingen was in het hele <w>binnenland</w> te horen.",
                            "target_lang_translations": {
                                "en": "The news about the elections could be heard throughout the entire <w>country</w>.",
                                "ru": "Новости о выборах были слышны по всей <w>стране</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "buitenland"
                    ],
                    "common_phrases": [
                        "in het binnenland",
                        "het hele binnenland"
                    ],
                    "target_lang_definitions": {
                        "en": "The area within the borders of one's own country.",
                        "ru": "Территория в пределах границ собственной страны."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "domestic",
                                "target_lang_sense_clarification": "relating to one's own country, opposite of foreign"
                            },
                            {
                                "target_lang_word": "the country",
                                "target_lang_sense_clarification": "the nation itself, as opposed to abroad"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "внутренняя часть страны",
                                "target_lang_sense_clarification": "территория государства"
                            },
                            {
                                "target_lang_word": "страна",
                                "target_lang_sense_clarification": "в значении 'не за границей'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c450515e-ec4f-3e50-92f2-96cf0d638980",
                    "sense_definition": "Het deel van een land dat ver van de kust of grens ligt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "inland",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We reisden ver het diepe <w>binnenland</w> in om de natuur te zien.",
                            "target_lang_translations": {
                                "en": "We traveled far into the deep <w>interior</w> to see nature.",
                                "ru": "Мы поехали далеко в <w>глубь страны</w>, чтобы увидеть природу."
                            }
                        },
                        {
                            "text": "Het dorp ligt in het <w>binnenland</w>, ver weg van de drukke kust.",
                            "target_lang_translations": {
                                "en": "The village lies <w>inland</w>, far away from the busy coast.",
                                "ru": "Деревня расположена во <w>внутренних районах</w>, далеко от шумного побережья."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het diepe binnenland",
                        "naar het binnenland"
                    ],
                    "target_lang_definitions": {
                        "en": "The part of a country situated far from the coast or border.",
                        "ru": "Часть страны, расположенная далеко от побережья или границы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inland",
                                "target_lang_sense_clarification": "the interior part of a country"
                            },
                            {
                                "target_lang_word": "interior",
                                "target_lang_sense_clarification": "the inner part of a country, away from the coast"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "внутренние районы",
                                "target_lang_sense_clarification": "удаленные от моря"
                            },
                            {
                                "target_lang_word": "глубь страны",
                                "target_lang_sense_clarification": "отдаленная от границ территория"
                            },
                            {
                                "target_lang_word": "глубинка",
                                "target_lang_sense_clarification": "разговорное, отдаленная местность"
                            }
                        ]
                    }
                }
            ]

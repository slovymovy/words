                    ],
                    "common_phrases": [
                        "watervast verlijmd"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that is resistant to the influence of water.",
                        "ru": "Способом, обеспечивающим устойчивость к воздействию воды."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "waterproof",

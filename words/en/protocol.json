    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eb74fcec-6ec3-397e-99e4-4577a767ac78",
                    "sense_definition": "A system of fixed rules and formal behavior used in official business, diplomacy, or social situations.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "rules_of_conduct",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is considered a breach of <w>protocol</w> to wear casual clothes to such a formal ceremony."
                        },
                        {
                            "text": "You must follow the correct <w>protocol</w> when meeting a member of the royal family."
                        }
                    ],
                    "synonyms": [
                        "procedure",
                        "policy",
                        "interface",
                        "policy"
                    ],
                    "common_phrases": [
                        "breach of protocol",
                        "follow protocol",
                        "royal protocol",
                        "strict protocol"
                    ]
                },
                {
                    "sense_id": "b2fcae07-9d83-377b-b01f-542dac2d29a5",
                    "sense_definition": "A set of standard rules that allows electronic devices or computers to communicate with each other.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "computing_rules",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The computer uses a specific <w>protocol</w> to send data securely over the network."
                        },
                        {
                            "text": "Engineers developed a new communication <w>protocol</w> to improve internet speeds."
                        }
                    ],
                    "common_phrases": [
                        "internet protocol",
                        "security protocol",
                        "communication protocol",
                        "transfer protocol"
                    ]
                },
                {
                    "sense_id": "9e03961c-c836-3365-9d1a-f0481b8219cb",
                    "sense_definition": "A detailed plan or set of steps to be followed in a scientific experiment or medical treatment.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "scientific_method",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor followed the treatment <w>protocol</w> exactly to ensure the patient's safety."
                        },
                        {
                            "text": "Scientists must adhere to a strict <w>protocol</w> when conducting clinical trials."
                        }
                    ],
                    "synonyms": [
                        "procedure"
                    ],
                    "common_phrases": [
                        "safety protocol",
                        "testing protocol",
                        "experimental protocol",
                        "treatment protocol"
                    ]
                },
                {
                    "sense_id": "1415a37c-54cd-3f5a-b034-2cf1d1db37b6",
                    "sense_definition": "An official international agreement or a document that records the details of a treaty.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legal_document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Several countries gathered to sign the new environmental <w>protocol</w> regarding climate change."
                        },
                        {
                            "text": "The <w>protocol</w> was added to the existing treaty to clarify the trade regulations."
                        }
                    ],
                    "common_phrases": [
                        "sign the protocol",
                        "Kyoto Protocol",
                        "additional protocol"
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aaa67437-91bf-3bfd-974f-4f8248df79e3",
                    "sense_definition": "To draft or record something in an official document or according to specific rules.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The clerk was asked to <w>protocol</w> the minutes of the meeting immediately."
                        },
                        {
                            "text": "They <w>protocolled</w> the agreement before the negotiations were officially closed."
                        }
                    ]
                }
            ]
        }
    ]

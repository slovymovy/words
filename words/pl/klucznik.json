{
    "word_family": [
        "klucz",
        "klucznica",
        "kluczowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e33bebc-0db0-3d10-8ffa-97d936226369",
                    "sense_definition": "Osoba, która przechowuje klucze i pilnuje dostępu do pomieszczeń w budynku, często w więzieniu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więzienny <w>klucznik</w> otworzył ciężkie, metalowe drzwi celi.",
                            "target_lang_translations": {
                                "ru": "Тюремный <w>ключник</w> открыл тяжелую металлическую дверь камеры."
                            }
                        },
                        {
                            "text": "Każdego wieczora <w>klucznik</w> robił obchód całego zamku, sprawdzając zamki.",
                            "target_lang_translations": {
                                "ru": "Каждый вечер <w>ключник</w> обходил весь замок, проверяя замки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "więzienny klucznik",
                        "pęk kluczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который хранит ключи и охраняет доступ к помещениям, часто в тюрьме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ключник",
                                "target_lang_sense_clarification": "хранитель ключей"
                            },
                            {
                                "target_lang_word": "тюремщик",
                                "target_lang_sense_clarification": "надзиратель в тюрьме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6887026-8623-354b-8a7e-31aeb2c2003d",
                    "sense_definition": "Dawny urzędnik dworski lub służący zarządzający gospodarstwem domowym, spiżarnią i kluczami do nich.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <w>klucznik</w> dbał o to, by w spiżarni niczego nie brakowało na ucztę.",
                            "target_lang_translations": {
                                "ru": "Старый <w>ключник</w> заботился о том, чтобы в кладовой всего хватало для пира."
                            }
                        },
                        {
                            "text": "Hrabia wezwał służbę, aby <w>klucznik</w> wydał wino z piwnicy.",
                            "target_lang_translations": {
                                "ru": "Граф позвал слуг, чтобы <w>ключник</w> выдал вино из погреба."
                            }
                        }
                    ],
                    "synonyms": [
                        "szafarz"
                    ],
                    "common_phrases": [
                        "klucznik dworski",
                        "wierny klucznik"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "odnosi się do dawnych funkcji dworskich"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинный придворный чиновник или слуга, управляющий домашним хозяйством, кладовой и ключами от них."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ключник",
                                "target_lang_sense_clarification": "управляющий хозяйством"
                            },
                            {
                                "target_lang_word": "эконом",
                                "target_lang_sense_clarification": "заведующий хозяйством"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "04c40f75-81e3-3137-9d05-12d931302816",
                    "sense_definition": "Naar beneden bewegen door de zwaartekracht, vaak per ongeluk of onbedoeld.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_gravity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De appel <w>viel</w> van de boom op het gras.",
                            "target_lang_translations": {
                                "ru": "Яблоко <w>упало</w> с дерева на траву.",
                                "en": "The apple <w>fell</w> from the tree onto the grass."
                            }
                        },
                        {
                            "text": "Pas op dat je niet <w>valt</w> over die losse tegels.",
                            "target_lang_translations": {
                                "ru": "Осторожно, не <w>упади</w> на этой шаткой плитке.",
                                "en": "Watch out that you don't <w>fall</w> over those loose tiles."
                            }
                        }
                    ],
                    "synonyms": [
                        "neervallen"
                    ],
                    "antonyms": [
                        "opstaan"
                    ],
                    "common_phrases": [
                        "hard vallen",
                        "op de grond vallen",
                        "van de trap vallen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться вниз под действием гравитации, часто случайно или непреднамеренно.",
                        "en": "To move downwards from a higher to a lower position due to gravity, often accidentally."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "движение вниз"
                            },
                            {
                                "target_lang_word": "упасть",
                                "target_lang_sense_clarification": "завершенное действие"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "general movement downwards"
                            },
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "to fall suddenly or vertically"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5d7ac6b-6c8c-3043-860f-ab7b6cc47282",
                    "sense_definition": "Plaatsvinden op een bepaalde datum of dag.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn verjaardag <w>valt</w> dit jaar op een zaterdag.",
                            "target_lang_translations": {
                                "ru": "Мой день рождения в этом году <w>выпадает</w> на субботу.",
                                "en": "My birthday <w>falls</w> on a Saturday this year."
                            }
                        },
                        {
                            "text": "Kerstmis <w>valt</w> altijd op 25 december.",
                            "target_lang_translations": {
                                "ru": "Рождество всегда <w>приходится</w> на 25 декабря.",
                                "en": "Christmas always <w>falls</w> on December 25th."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in het weekend vallen",
                        "op een dag vallen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить в определенную дату или день.",
                        "en": "To happen or take place on a particular day or date."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпадать",
                                "target_lang_sense_clarification": "о датах"
                            },
                            {
                                "target_lang_word": "приходиться",
                                "target_lang_sense_clarification": "о событиях"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "occur on a date"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ea60de9-9e90-3ebf-a05e-7a96a36cfda6",
                    "sense_definition": "Beginnen of intreden (gezegd van een dagdeel of seizoen).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De avond <w>valt</w> al vroeg in de wintermaanden.",
                            "target_lang_translations": {
                                "ru": "В зимние месяцы вечер <w>наступает</w> рано.",
                                "en": "Evening <w>falls</w> early during the winter months."
                            }
                        },
                        {
                            "text": "Toen de nacht <w>viel</w>, werd het stil op straat.",
                            "target_lang_translations": {
                                "ru": "Когда <w>опустилась</w> ночь, на улице стало тихо.",
                                "en": "When night <w>fell</w>, it became quiet on the street."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de avond valt",
                        "de nacht valt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинаться или наступать (о времени суток или времени года).",
                        "en": "To begin or arrive, specifically regarding darkness, night, or a season."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наступать",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "опускаться",
                                "target_lang_sense_clarification": "о темноте или ночи"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "of night or darkness"
                            },
                            {
                                "target_lang_word": "set in",
                                "target_lang_sense_clarification": "of a season or period"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdaf1f28-435d-3eee-b1d0-6566bc8c933f",
                    "sense_definition": "Mogelijk zijn (altijd in combinatie met 'te').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "possibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er <w>valt</w> niets meer aan deze situatie te veranderen.",
                            "target_lang_translations": {
                                "ru": "В этой ситуации больше ничего не <w>удастся</w> изменить.",
                                "en": "There is nothing left that <w>can be</w> changed about this situation."
                            }
                        },
                        {
                            "text": "Het <w>valt</w> te bezien of hij gelijk heeft.",
                            "target_lang_translations": {
                                "ru": "Еще <w>предстоит</w> увидеть, прав ли он.",
                                "en": "It <w>remains</w> to be seen whether he is right."
                            }
                        }
                    ],
                    "common_phrases": [
                        "valt te bezien",
                        "niet te vallen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть возможным (всегда в сочетании с 'te').",
                        "en": "To be possible to do something (used in specific constructions with 'te')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть возможным",
                                "target_lang_sense_clarification": "общая возможность"
                            },
                            {
                                "target_lang_word": "поддаваться",
                                "target_lang_sense_clarification": "о действии (изменению, ремонту)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "be possible",
                                "target_lang_sense_clarification": "general possibility"
                            },
                            {
                                "target_lang_word": "remain",
                                "target_lang_sense_clarification": "in 'remains to be seen'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd8bc1a8-3112-3a48-ad2f-ab82678fb988",
                    "sense_definition": "Op een bepaalde manier ervaren worden of overkomen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het slechte nieuws <w>viel</w> hem erg zwaar.",
                            "target_lang_translations": {
                                "ru": "Плохие новости <w>дались</w> ему очень тяжело.",
                                "en": "The bad news <w>hit</w> him very hard."
                            }
                        },
                        {
                            "text": "Zijn grapje <w>viel</w> helemaal verkeerd bij het publiek.",
                            "target_lang_translations": {
                                "ru": "Его шутка была <w>воспринята</w> публикой совершенно неправильно.",
                                "en": "His joke <w>went down</w> completely wrong with the audience."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zwaar vallen",
                        "verkeerd vallen",
                        "goed vallen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восприниматься или ощущаться определенным образом.",
                        "en": "To be received, perceived, or experienced in a particular way by someone."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восприниматься",
                                "target_lang_sense_clarification": "об ощущении или реакции"
                            },
                            {
                                "target_lang_word": "даваться",
                                "target_lang_sense_clarification": "о переживании (тяжело/легко)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "be received (e.g., a joke)"
                            },
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "impact emotionally"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db0ce7a8-2672-3c15-bc4b-a499e4b5bf6a",
                    "sense_definition": "Losjes en soepel naar beneden hangen (van stof of haar).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "hanging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Haar lange haren <w>vielen</w> in krullen over haar schouders.",
                            "target_lang_translations": {
                                "ru": "Её длинные волосы локонами <w>ниспадали</w> на плечи.",
                                "en": "Her long hair <w>fell</w> in curls over her shoulders."
                            }
                        },
                        {
                            "text": "Deze jurk <w>valt</w> heel mooi om je lichaam.",
                            "target_lang_translations": {
                                "ru": "Это платье очень красиво <w>сидит</w> на тебе.",
                                "en": "This dress <w>drapes</w> very beautifully around your body."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mooi vallen",
                        "soepel vallen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободно и плавно свисать (о ткани или волосах).",
                        "en": "To hang down loosely and gracefully, typically referring to fabric or hair."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ниспадать",
                                "target_lang_sense_clarification": "красиво свисать"
                            },
                            {
                                "target_lang_word": "сидеть",
                                "target_lang_sense_clarification": "об одежде"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "hang down loosely"
                            },
                            {
                                "target_lang_word": "drape",
                                "target_lang_sense_clarification": "hang in folds (fabric)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7bb1e72-da2b-3a43-91a6-0836e09294cd",
                    "sense_definition": "Veroverd worden door de vijand of ten einde komen (van een stad of regering).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "defeat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stad is na een lange strijd eindelijk <w>gevallen</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой битвы город наконец <w>пал</w>.",
                                "en": "The city finally <w>fell</w> after a long battle."
                            }
                        },
                        {
                            "text": "Het kabinet is gisteren <w>gevallen</w> door een conflict.",
                            "target_lang_translations": {
                                "ru": "Вчера из-за конфликта кабинет министров <w>пал</w>.",
                                "en": "The cabinet <w>fell</w> yesterday due to a conflict."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het kabinet valt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть захваченным врагом или прекратить существование (о городе или правительстве).",
                        "en": "To be defeated, conquered, or overthrown (referring to a city or government)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "быть побежденным"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "be defeated or overthrown"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02ed9962-89a5-3b36-bd28-0dcaeed9364a",
                    "sense_definition": "Sterven tijdens een gevecht of oorlog.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vele soldaten zijn <w>gevallen</w> voor de vrijheid van hun land.",
                            "target_lang_translations": {
                                "ru": "Многие солдаты <w>пали</w> за свободу своей страны.",
                                "en": "Many soldiers have <w>fallen</w> for the freedom of their country."
                            }
                        },
                        {
                            "text": "Hij <w>viel</w> tijdens de slag bij Waterloo.",
                            "target_lang_translations": {
                                "ru": "Он <w>пал</w> в битве при Ватерлоо.",
                                "en": "He <w>fell</w> during the Battle of Waterloo."
                            }
                        }
                    ],
                    "synonyms": [
                        "neervallen",
                        "afvallen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Умереть во время битвы или войны.",
                        "en": "To die, specifically in battle or war."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "погибнуть в бою"
                            },
                            {
                                "target_lang_word": "погибнуть",
                                "target_lang_sense_clarification": "смерть на войне"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "die in battle"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c2cbcb61-9831-3b24-a290-e48ff9279087",
                    "sense_definition": "Meervoud van <w>val</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "form_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jager zette verschillende <w>vallen</w> in het bos.",
                            "target_lang_translations": {
                                "ru": "Охотник поставил в лесу несколько <w>ловушек</w>.",
                                "en": "The hunter set several <w>traps</w> in the forest."
                            }
                        },
                        {
                            "text": "Tijdens de judoles oefenden we onze <w>vallen</w> en worpen.",
                            "target_lang_translations": {
                                "ru": "На уроке дзюдо мы отрабатывали <w>падения</w> и броски.",
                                "en": "During the judo lesson we practiced our <w>falls</w> and throws."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова 'val' (падение или ловушка).",
                        "en": "Plural form of 'val', referring to devices for catching animals or acts of falling."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ловушки",
                                "target_lang_sense_clarification": "устройства для поимки"
                            },
                            {
                                "target_lang_word": "падения",
                                "target_lang_sense_clarification": "акты падения"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "traps",
                                "target_lang_sense_clarification": "devices to catch animals"
                            },
                            {
                                "target_lang_word": "falls",
                                "target_lang_sense_clarification": "acts of falling"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f0fe8811-dd98-3bd4-9318-b0c0430cc99d",
                    "sense_definition": "Utrwalić treść na piśmie, sporządzić dokument lub notatkę na podstawie ustnych ustaleń lub innych tekstów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "writing_creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy wspólnie <spisać> umowę, aby uniknąć nieporozumień w przyszłości.",
                            "target_lang_translations": {
                                "ru": "Мы должны вместе <составить> договор, чтобы избежать недоразумений в будущем."
                            }
                        },
                        {
                            "text": "Notariusz pomógł nam <spisać> testament zgodnie z wolą dziadka.",
                            "target_lang_translations": {
                                "ru": "Нотариус помог нам <составить> завещание в соответствии с волей дедушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "sporządzić",
                        "zanotować",
                        "zapisać"
                    ],
                    "common_phrases": [
                        "spisać umowę",
                        "spisać protokół",
                        "spisać testament",
                        "spisać oświadczenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зафиксировать содержание письменно, составить документ или заметку на основе устных договоренностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составить",
                                "target_lang_sense_clarification": "договор, документ"
                            },
                            {
                                "target_lang_word": "написать",
                                "target_lang_sense_clarification": "завещание, текст"
                            },
                            {
                                "target_lang_word": "записать",
                                "target_lang_sense_clarification": "зафиксировать информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5d980dc-03f2-3d54-80bf-de009fb8ddbe",
                    "sense_definition": "Wykonać zadanie z dobrym rezultatem lub zachować się w określony sposób (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy samochód świetnie <spisał> się podczas długiej podróży w góry.",
                            "target_lang_translations": {
                                "ru": "Моя новая машина отлично <показала> себя во время долгой поездки в горы."
                            }
                        },
                        {
                            "text": "Mimo ogromnego stresu, uczniowie <spisali> się na medal podczas egzaminu.",
                            "target_lang_translations": {
                                "ru": "Несмотря на огромный стресс, ученики <справились> на отлично во время экзамена."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprawdzić się"
                    ],
                    "common_phrases": [
                        "spisać się na medal",
                        "dobrze się spisać",
                        "dzielnie się spisać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнить задачу с хорошим результатом или проявить себя определенным образом (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показать себя",
                                "target_lang_sense_clarification": "проявить свои качества"
                            },
                            {
                                "target_lang_word": "справиться",
                                "target_lang_sense_clarification": "успешно выполнить задачу"
                            },
                            {
                                "target_lang_word": "проявить себя",
                                "target_lang_sense_clarification": "продемонстрировать способности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4dedf35-0823-3650-87a3-7a516d5c8920",
                    "sense_definition": "Zrobić wykaz lub listę osób, rzeczy lub danych w celu ich policzenia lub inwentaryzacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "writing_creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracownik gazowni przyszedł <spisać> stan licznika, aby wystawić rachunek.",
                            "target_lang_translations": {
                                "ru": "Сотрудник газовой службы пришел <снять> показания счетчика, чтобы выставить счет."
                            }
                        },
                        {
                            "text": "Magazynier musi dokładnie <spisać> wszystkie towary, które zostały w sklepie.",
                            "target_lang_translations": {
                                "ru": "Кладовщик должен точно <переписать> все товары, которые остались в магазине."
                            }
                        }
                    ],
                    "synonyms": [
                        "zinwentaryzować",
                        "wyliczyć"
                    ],
                    "common_phrases": [
                        "spisać stan licznika",
                        "spisać inwentarz",
                        "spisać listę obecności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Составить список людей, вещей или данных для подсчета или инвентаризации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переписать",
                                "target_lang_sense_clarification": "составить список имущества"
                            },
                            {
                                "target_lang_word": "снять",
                                "target_lang_sense_clarification": "показания счетчика"
                            },
                            {
                                "target_lang_word": "записать",
                                "target_lang_sense_clarification": "внести в список"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4caf7d38-ea29-3ab9-a5d0-7bcfcf7e91d7",
                    "sense_definition": "Wylegitymować kogoś i zanotować jego dane osobowe w celu nałożenia kary lub pouczenia (zwykle przez policję lub kontrolerów).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policjanci postanowili <spisać> grupę młodzieży hałasującą w parku po zmroku.",
                            "target_lang_translations": {
                                "ru": "Полицейские решили <записать> данные группы молодежи, шумевшей в парке после наступления темноты."
                            }
                        },
                        {
                            "text": "Bałem się, że kontroler biletów może mnie <spisać>, bo zapomniałem legitymacji.",
                            "target_lang_translations": {
                                "ru": "Я боялся, что контролер билетов может меня <записать>, так как я забыл удостоверение."
                            }
                        }
                    ],
                    "synonyms": [
                        "wylegitymować"
                    ],
                    "common_phrases": [
                        "zostać spisanym",
                        "policja spisała"
                    ],
                    "traits": [

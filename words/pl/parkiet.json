                {
                    "sense_id": "5cf9f1ba-2895-39e6-9021-8b2a58eea081",
                    "sense_definition": "Podłoga wykonana z ułożonych we wzory drewnianych deszczułek (klepek).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "floor_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starym mieszkaniu musieliśmy wycyklinować zniszczony <w>parkiet</w>.",
                            "target_lang_translations": {
                                "ru": "В старой квартире нам пришлось отциклевать испорченный <w>паркет</w>."
                            }
                        },
                        {
                            "text": "W salonie położono piękny, dębowy <w>parkiet</w>, który dodaje wnętrzu elegancji.",
                            "target_lang_translations": {
                                "ru": "В гостиной положили красивый дубовый <w>паркет</w>, который придает интерьеру элегантности."
                            }
                        }
                    ],
                    "synonyms": [
                        "posadzka"
                    ],
                    "common_phrases": [
                        "dębowy parkiet",
                        "układać parkiet",
                        "cyklinować parkiet",
                        "lakierować parkiet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пол, выложенный деревянными планками, образующими узор."

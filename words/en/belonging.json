            "senses": [
                {
                    "sense_id": "31a013a6-c63c-37eb-8d2d-7f2d99ca8aec",
                    "sense_definition": "Something physical that is owned.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure you take all your <w>belongings</w> when you leave.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что вы забрали все свои <w>вещи</w>, когда будете уходить."
                            }
                        },
                        {
                            "text": "She packed her few personal <w>belongings</w> into a small bag.",
                            "target_lang_translations": {
                                "ru": "Она упаковала свои немногочисленные личные <w>вещи</w> в маленькую сумку."
                            }
                        }
                    ],
                    "synonyms": [
                        "possession",
                        "thing"

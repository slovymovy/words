{
    "word_family": [
        "uw",
        "uwe"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "c59163f4-58b5-3359-b024-cb51d2b8493a",
                    "sense_definition": "Persoonlijk voornaamwoord, tweede persoon enkelvoud en meervoud (beleefdheidsvorm). Gebruikt om één of meerdere personen op een beleefde of formele manier aan te spreken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "formal_you",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kunt <w>u</w> mij vertellen hoe laat de trein vertrekt?",
                            "target_lang_translations": {
                                "en": "Could <w>you</w> tell me what time the train leaves?",
                                "ru": "Не могли бы <w>Вы</w> подсказать мне, во сколько отправляется поезд?"
                            }
                        },
                        {
                            "text": "Ik zal het later vandaag aan <w>u</w> geven.",
                            "target_lang_translations": {
                                "en": "I will give it to <w>you</w> later today.",
                                "ru": "Я передам это <w>Вам</w> сегодня позже."
                            }
                        },
                        {
                            "text": "Heeft <w>u</w> zin om vanavond mee te gaan?",
                            "target_lang_translations": {
                                "en": "Would <w>you</w> like to come along tonight?",
                                "ru": "Не хотите ли <w>Вы</w> пойти с нами сегодня вечером?"
                            }
                        }
                    ],
                    "antonyms": [
                        "jij",
                        "jullie"
                    ],
                    "common_phrases": [
                        "dank u wel",
                        "alstublieft",
                        "u ook",
                        "gaat het met u"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Personal pronoun, second person singular and plural (polite form). Used to address one or more people in a polite or formal way.",
                        "ru": "Личное местоимение второго лица единственного и множественного числа (вежливая форма). Используется для вежливого или официального обращения к одному или нескольким лицам."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you",
                                "target_lang_sense_clarification": "polite form"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Вы",
                                "target_lang_sense_clarification": "вежливое обращение"
                            },
                            {
                                "target_lang_word": "Вас",
                                "target_lang_sense_clarification": "вежливое обращение (падежная форма)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b063d2b0-a1f9-3be0-b2fb-497a109b2194",
                    "sense_definition": "Wederkerend voornaamwoord bij de beleefdheidsvorm. Verwijst terug naar het onderwerp wanneer dit 'u' is.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "formal_you",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "U heeft <w>u</w> waarschijnlijk vergist in de datum.",
                            "target_lang_translations": {
                                "en": "You were probably mistaken about the date.",
                                "ru": "Вы, вероятно, ошиблись датой."
                            }
                        },
                        {
                            "text": "Voelt <w>u</w> <w>u</w> vandaag al wat beter?",
                            "target_lang_translations": {
                                "en": "Do <w>you</w> feel any better today?",
                                "ru": "Чувствуете ли <w>Вы</w> <w>себя</w> сегодня уже немного лучше?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zich"
                    ],
                    "common_phrases": [
                        "u vergissen",
                        "u voelen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Reflexive pronoun used with the polite form. Refers back to the subject when the subject is 'u'.",
                        "ru": "Возвратное местоимение при вежливой форме обращения. Отсылает к подлежащему, если подлежащим является «u»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "yourself",
                                "target_lang_sense_clarification": "reflexive"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a23ed600-ed85-32a6-b68c-3e544f616fc3",
                    "sense_definition": "De voorwerpsvorm van 'gij'. Wordt voornamelijk gebruikt in Vlaanderen en zuidelijk Nederland in informele context.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "regional_you",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <w>u</w> gisteren niet gezien op het feest.",
                            "target_lang_translations": {
                                "en": "I didn't see <w>you</w> at the party yesterday.",
                                "ru": "Я не видел <w>тебя</w> вчера на вечеринке."
                            }
                        },
                        {
                            "text": "Dat boek is van <w>u</w>, toch?",
                            "target_lang_translations": {
                                "en": "That book belongs to <w>you</w>, right?",
                                "ru": "Эта книга принадлежит <w>тебе</w>, верно?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Flanders and Southern Netherlands"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The object form of 'gij'. Primarily used in Flanders and the southern Netherlands in informal contexts.",
                        "ru": "Объектная форма местоимения «gij». Используется преимущественно во Фландрии и на юге Нидерландов в неформальном контексте."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you",
                                "target_lang_sense_clarification": "regional/informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тебя",
                                "target_lang_sense_clarification": "неформальное"
                            },
                            {
                                "target_lang_word": "вас",
                                "target_lang_sense_clarification": "региональное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b20f8c2a-ec5b-3140-a4c1-f44f014730ab",
                    "sense_definition": "De eenentwintigste letter van het alfabet.",
                    "learner_level": "A1",
                    "frequency": "Low",
                    "semantic_group_id": "letter_u",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het woord 'uur' begint met de letter <w>u</w>.",
                            "target_lang_translations": {
                                "en": "The word 'uur' starts with the letter <w>u</w>.",
                                "ru": "Слово 'uur' начинается с буквы <w>u</w>."
                            }
                        },
                        {
                            "text": "Er staat een <w>u</w> in mijn achternaam.",
                            "target_lang_translations": {
                                "en": "There is a <w>u</w> in my surname.",
                                "ru": "В моей фамилии есть <w>u</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The twenty-first letter of the alphabet.",
                        "ru": "Двадцать первая буква алфавита."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "u",
                                "target_lang_sense_clarification": "letter"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "u",
                                "target_lang_sense_clarification": "буква"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "06b56e14-78b8-36a8-a5f7-7c5c96ecb347",
                    "sense_definition": "Een taal die gesproken wordt in China (Angkuïsche taal).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Er zijn maar weinig mensen die <w>U</w> spreken.",
                            "target_lang_translations": {
                                "en": "There are only few people who speak <w>U</w>.",
                                "ru": "Мало кто говорит на <w>У</w>."
                            }
                        },
                        {
                            "text": "De taal <w>U</w> wordt in een specifiek deel van China gesproken.",
                            "target_lang_translations": {
                                "en": "The language <w>U</w> is spoken in a specific part of China.",
                                "ru": "Язык <w>У</w> используется в определенной части Китая."
                            }
                        }
                    ],
                    "synonyms": [
                        "Puman"
                    ],
                    "target_lang_definitions": {
                        "en": "A language spoken in China (Angkuic language).",
                        "ru": "Язык, на котором говорят в Китае (ангкуйский язык)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "U",
                                "target_lang_sense_clarification": "language"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "У",
                                "target_lang_sense_clarification": "язык"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
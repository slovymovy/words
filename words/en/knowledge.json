            "senses": [
                {
                    "sense_id": "9bcc48c3-14d1-35c3-83af-bbcc536f4db7",
                    "sense_definition": "A demanding course of study regarding routes and places of interest that must be completed by prospective London taxi drivers.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "london_taxi",
                    "name_type": "document_or_program_name",
                    "examples": [
                        {
                            "text": "It takes years of study to master the <w>Knowledge</w> and become a licensed cab driver.",
                            "target_lang_translations": {
                                "ru": "Требуются годы учебы, чтобы освоить <w>Знание</w> и стать лицензированным водителем такси."
                            }
                        },
                        {
                            "text": "You often see people on scooters studying for the <w>Knowledge</w> in London.",
                            "target_lang_translations": {
                                "ru": "В Лондоне часто можно увидеть людей на скутерах, готовящихся к сдаче <w>Знания</w>."
                            }
                        }
                    ],
                    "common_phrases": [

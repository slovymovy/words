    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b6d34cbf-2868-3476-b874-e9b6901d1acd",
                    "sense_definition": "Живущий в природных условиях (о животных) или растущий без ухода человека (о растениях); не домашний.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "nature_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом лесу живут <дикие> кабаны и медведи.",
                            "target_lang_translations": {
                                "en": "<Wild> boars and bears live in this forest."
                            }
                        },
                        {
                            "text": "На лугу расцвели красивые <дикие> цветы.",
                            "target_lang_translations": {
                                "en": "Beautiful <wild> flowers bloomed in the meadow."
                            }
                        }
                    ],
                    "synonyms": [
                        "неодомашненный",
                        "неприручённый",
                        "некультивированный"
                    ],
                    "antonyms": [
                        "домашний",
                        "ручной",
                        "культурный"
                    ],
                    "common_phrases": [
                        "дикие животные",
                        "дикая природа",
                        "дикие птицы",
                        "дикие растения"
                    ],
                    "target_lang_definitions": {
                        "en": "Living in natural conditions (of animals) or growing without human care (of plants); not domestic."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wild",
                                "target_lang_sense_clarification": "animals/plants"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb9113e1-a311-3725-aaee-ec47c88def23",
                    "sense_definition": "Не тронутый деятельностью человека, сохранивший первозданный вид; пустынный (о местности).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "nature_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Путешественники шли через густой <дикий> лес.",
                            "target_lang_translations": {
                                "en": "The travelers walked through a dense <wild> forest."
                            }
                        },
                        {
                            "text": "Перед нами открылся <дикий> и прекрасный горный пейзаж.",
                            "target_lang_translations": {
                                "en": "A <wild> and beautiful mountain landscape opened up before us."
                            }
                        }
                    ],
                    "synonyms": [
                        "первозданный"
                    ],
                    "antonyms": [
                        "цивилизованный"
                    ],
                    "common_phrases": [
                        "дикий лес",
                        "дикий край",
                        "дикое поле",
                        "дикая местность"
                    ],
                    "target_lang_definitions": {
                        "en": "Untouched by human activity, preserving its primordial appearance; desolate (of a locality)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wild",
                                "target_lang_sense_clarification": "nature/landscape"
                            },
                            {
                                "target_lang_word": "savage",
                                "target_lang_sense_clarification": "rugged/untamed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e8b2803-f7e2-3285-8993-441c6b7d6809",
                    "sense_definition": "Очень сильный, чрезмерный, превышающий обычную меру (о чувствах, состояниях).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "figurative_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От этой новости он пришёл в <дикий> восторг.",
                            "target_lang_translations": {
                                "en": "He went into <wild> delight at this news."
                            }
                        },
                        {
                            "text": "У меня сегодня просто <дикая> усталость после работы.",
                            "target_lang_translations": {
                                "en": "I have simply <terrible> fatigue after work today."
                            }
                        }
                    ],
                    "synonyms": [
                        "ужасный",
                        "необузданный",
                        "необычный"
                    ],
                    "common_phrases": [
                        "дикий восторг",
                        "дикий крик",
                        "дикая жара",
                        "дикая боль",
                        "дикий страх"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Very strong, excessive, exceeding the usual measure (of feelings, states)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wild",
                                "target_lang_sense_clarification": "intense emotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa1c07e0-c469-3a69-a84f-a01f26570e39",
                    "sense_definition": "Находящийся на уровне первобытной культуры, нецивилизованный (о людях или племенах).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "civilization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Раньше на этих землях жили <дикие> племена.",
                            "target_lang_translations": {
                                "en": "<Wild> tribes used to live on these lands."
                            }
                        },
                        {
                            "text": "Их обычаи могут показаться современному человеку <дикими>.",
                            "target_lang_translations": {
                                "en": "Their customs might seem <savage> to a modern person."
                            }
                        }
                    ],
                    "synonyms": [
                        "первобытный",
                        "примитивный"
                    ],
                    "antonyms": [
                        "цивилизованный",
                        "развитый",
                        "культурный"
                    ],
                    "common_phrases": [
                        "дикие люди",
                        "дикие племена",
                        "дикие нравы"
                    ],
                    "target_lang_definitions": {
                        "en": "Being at the level of primitive culture, uncivilized (of people or tribes)."
                    },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4bd43d9a-fdd0-3c99-a26d-ad6c83fd9702",
                    "sense_definition": "Ołówek z kolorowym wkładem używany do rysowania i kolorowania.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "drawing_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko rysuje dom kolorowymi <kredkami>.",
                            "target_lang_translations": {
                                "ru": "Ребёнок рисует дом цветными <w>карандашами</w>."
                            }
                        },
                        {
                            "text": "Muszę kupić nowe pudełko <kredek> do szkoły.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую коробку <w>карандашей</w> в школу."
                            }
                        }
                    ],
                    "synonyms": [
                        "kredka ołówkowa"
                    ],
                    "common_phrases": [
                        "pudełko kredek",
                        "kolorowe kredki",
                        "temperować kredki",
                        "rysować kredkami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Карандаш с цветным грифелем, используемый для рисования и раскрашивания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цветной карандаш",
                                "target_lang_sense_clarification": "инструмент для рисования"
                            },
                            {
                                "target_lang_word": "карандаш",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9c977ab-a8dc-3e7b-83e6-7b4726073d92",
                    "sense_definition": "Pałeczka wykonana z barwionej masy, na przykład wosku, służąca do rysowania.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "drawing_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczycielka rozdała dzieciom <kredki> świecowe do zabawy.",
                            "target_lang_translations": {
                                "ru": "Учительница раздала детям восковые <w>мелки</w> для игры."
                            }
                        },
                        {
                            "text": "Te <kredki> są miękkie i mają bardzo żywe kolory.",
                            "target_lang_translations": {
                                "ru": "Эти <w>мелки</w> мягкие, и у них очень яркие цвета."
                            }
                        }
                    ],
                    "synonyms": [
                        "kredka świecowa"
                    ],
                    "common_phrases": [
                        "kredki świecowe",
                        "kredki pastelowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Палочка из окрашенной массы (например, воска или пастели) для рисования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелок",
                                "target_lang_sense_clarification": "восковой или пастельный"
                            },
                            {
                                "target_lang_word": "пастель",
                                "target_lang_sense_clarification": "художественный материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0021ff96-cc99-30b6-8a3e-bce924fdad60",
                    "sense_definition": "Kosmetyk w postaci cienkiego ołówka używany do makijażu oczu lub ust.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cosmetics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zgubiłam moją ulubioną czarną <kredkę> do oczu.",
                            "target_lang_translations": {
                                "ru": "Я потеряла свой любимый чёрный <w>карандаш</w> для глаз."
                            }
                        },
                        {
                            "text": "Ta <kredka> do ust idealnie pasuje do twojej szminki.",
                            "target_lang_translations": {
                                "ru": "Этот <w>карандаш</w> для губ идеально подходит к твоей помаде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kredka do oczu",
                        "kredka do ust",
                        "wodoodporna kredka",
                        "czarna kredka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Косметическое средство в виде тонкого карандаша для макияжа глаз или губ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карандаш",
                                "target_lang_sense_clarification": "косметический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "868d7834-63cd-394b-bc44-5f62e5e56323",
                    "sense_definition": "Mały kawałek kredy; zdrobnienie od słowa <w>kreda</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "chalk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel wziął do ręki <kredkę> i napisał temat na tablicy.",
                            "target_lang_translations": {
                                "ru": "Учитель взял в руку <w>мелок</w> и написал тему на доске."
                            }
                        },
                        {
                            "text": "Pod tablicą leżała tylko jedna mała, biała <kredka>.",
                            "target_lang_translations": {
                                "ru": "Под доской лежал только один маленький белый <w>мелок</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький кусок мела; уменьшительная форма слова «мел»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелок",
                                "target_lang_sense_clarification": "кусочек мела"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4ecbbf5a-c8ad-329c-bdfb-3537a3a20b65",
                    "sense_definition": "A violent and destructive release of energy, often accompanied by a loud noise and high temperature.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gas <w>explosion</w> caused significant damage to the building.",
                            "target_lang_translations": {
                                "ru": "<w>Взрыв</w> газа нанес зданию значительный ущерб."
                            }
                        },
                        {
                            "text": "Scientists carefully monitored the controlled <w>explosion</w> at the demolition site.",
                            "target_lang_translations": {
                                "ru": "Ученые внимательно следили за контролируемым <w>взрывом</w> на месте сноса."
                            }
                        }
                    ],
                    "synonyms": [
                        "blast",
                        "detonation",
                        "eruption"
                    ],
                    "antonyms": [
                        "implosion"
                    ],
                    "common_phrases": [
                        "loud explosion",
                        "gas explosion",
                        "nuclear explosion",
                        "cause an explosion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мощный и разрушительный выброс энергии, часто сопровождающийся громким шумом и высокой температурой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрыв",
                                "target_lang_sense_clarification": "физическое явление"
                            },
                            {
                                "target_lang_word": "подрыв",
                                "target_lang_sense_clarification": "преднамеренное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cbacf7c-ca5f-3e24-9bc5-0270e36854c3",
                    "sense_definition": "A sudden, rapid, and often uncontrolled increase or expansion of something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city is facing a major housing shortage due to the population <w>explosion</w>.",
                            "target_lang_translations": {
                                "ru": "Город сталкивается с острой нехваткой жилья из-за демографического <w>взрыва</w>."
                            }
                        },
                        {
                            "text": "There was an <w>explosion</w> of applause when the band finally walked on stage.",
                            "target_lang_translations": {
                                "ru": "Раздался <w>взрыв</w> аплодисментов, когда группа наконец вышла на сцену."
                            }
                        }
                    ],
                    "synonyms": [
                        "boom",
                        "burst"
                    ],
                    "common_phrases": [
                        "population explosion",
                        "explosion of interest",
                        "explosion of anger"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное, быстрое и часто неконтролируемое увеличение или расширение чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрыв",
                                "target_lang_sense_clarification": "бурный рост"
                            },
                            {
                                "target_lang_word": "бум",
                                "target_lang_sense_clarification": "резкое увеличение"
                            },
                            {
                                "target_lang_word": "всплеск",
                                "target_lang_sense_clarification": "внезапное проявление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a836c11-9f1f-3c4f-bec3-924646e4636c",
                    "sense_definition": "The loud sound produced by a violent release of energy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We heard a deafening <w>explosion</w> from miles away.",
                            "target_lang_translations": {
                                "ru": "Мы услышали оглушительный <w>взрыв</w> за несколько миль отсюда."
                            }
                        },
                        {
                            "text": "The sudden <w>explosion</w> made everyone in the room jump with fright.",
                            "target_lang_translations": {
                                "ru": "От внезапного <w>взрыва</w> все в комнате подпрыгнули от испуга."
                            }
                        }
                    ],
                    "synonyms": [
                        "bang",
                        "boom",
                        "blast"
                    ],
                    "common_phrases": [
                        "hear an explosion",
                        "deafening explosion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громкий звук, возникающий при мощном выбросе энергии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрыв",
                                "target_lang_sense_clarification": "звук"
                            },
                            {
                                "target_lang_word": "хлопок",
                                "target_lang_sense_clarification": "резкий звук"
                            }
                        ]
                    }
                }
            ]
        }
    ]

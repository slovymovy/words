    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aff67548-816d-389c-918f-164b7d05d624",
                    "sense_definition": "Окатить или облить кого-либо или что-либо, плеснув жидкостью.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_by_agent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Проезжающий мимо автомобиль <заплеснул> пешеходов грязной водой из лужи.",
                            "target_lang_translations": {
                                "en": "A passing car <splashed> the pedestrians with dirty water from a puddle."
                            }
                        },
                        {
                            "text": "Туристы решили <заплеснуть> догорающий костёр остатками чая.",
                            "target_lang_translations": {
                                "en": "The tourists decided to <douse> the dying fire with the remains of their tea."
                            }
                        }
                    ],
                    "synonyms": [
                        "облить",
                        "окатить",
                        "забрызгать"
                    ],
                    "common_phrases": [
                        "заплеснуть водой",
                        "заплеснуть грязью",
                        "заплеснуть костёр"
                    ],
                    "target_lang_definitions": {
                        "en": "To douse or splash someone or something with liquid."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "splash",
                                "target_lang_sense_clarification": "to throw liquid on someone or something"
                            },
                            {
                                "target_lang_word": "douse",
                                "target_lang_sense_clarification": "to wet thoroughly by throwing liquid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31604dca-d585-3abf-8b33-ef8bdc08595a",
                    "sense_definition": "Попасть внутрь или на поверхность чего-либо в результате всплеска (о воде, волнах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "motion_of_liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Во время шторма высокая волна <заплеснула> прямо на палубу корабля.",
                            "target_lang_translations": {
                                "en": "During the storm, a high wave <washed> right onto the ship's deck."
                            }
                        },
                        {
                            "text": "Вода из переполненного ведра <заплеснула> на пол при резком толчке.",
                            "target_lang_translations": {
                                "en": "Water from the overfilled bucket <sloshed> onto the floor with a sudden jolt."
                            }
                        }
                    ],
                    "synonyms": [
                        "захлестнуть",
                        "залиться"
                    ],
                    "common_phrases": [
                        "волна заплеснула",
                        "вода заплеснула"
                    ],
                    "target_lang_definitions": {
                        "en": "To get inside or onto a surface as a result of splashing (regarding water or waves)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "splash",
                                "target_lang_sense_clarification": "to hit or fall onto a surface (of liquid)"
                            },
                            {
                                "target_lang_word": "wash over",
                                "target_lang_sense_clarification": "to flow onto a surface (often of waves)"
                            },
                            {
                                "target_lang_word": "slosh",
                                "target_lang_sense_clarification": "to spill over the edge of a container due to movement"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
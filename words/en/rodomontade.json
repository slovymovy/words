    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d5464ebf-1f1f-3eb3-9ee3-01f0be88ffb2",
                    "sense_definition": "Vain boasting, ranting, or pretentious behavior.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician's speech was nothing but empty <w>rodomontade</w> designed to impress the voters.",
                            "target_lang_translations": {
                                "ru": "Речь политика была не чем иным, как пустым <w>бахвальством</w>, рассчитанным на то, чтобы произвести впечатление на избирателей."
                            }
                        },
                        {
                            "text": "After listening to his <w>rodomontade</w> for an hour, we realized he had no actual plan.",
                            "target_lang_translations": {
                                "ru": "Послушав его <w>разглагольствования</w> в течение часа, мы поняли, что у него нет реального плана."
                            }
                        }
                    ],
                    "synonyms": [
                        "bombast",
                        "bluster",
                        "bragging",
                        "boasting",
                        "vainglory"
                    ],
                    "antonyms": [
                        "modesty",
                        "humility"
                    ],
                    "common_phrases": [
                        "sheer rodomontade",
                        "empty rodomontade"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщетное хвастовство, напыщенная речь или претенциозное поведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бахвальство",
                                "target_lang_sense_clarification": "неумеренное хвастовство"
                            },
                            {
                                "target_lang_word": "фанфаронство",
                                "target_lang_sense_clarification": "поведение человека, который выставляет напоказ свои мнимые достоинства"
                            },
                            {
                                "target_lang_word": "напыщенность",
                                "target_lang_sense_clarification": "высокопарность, преувеличенная важность в речи"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "42ab5ec4-4dfe-3399-aee0-bc1a459af23e",
                    "sense_definition": "Pretentiously boastful.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>rodomontade</w> claims were quickly dismissed by the experts in the field.",
                            "target_lang_translations": {
                                "ru": "Его <w>хвастливые</w> заявления были быстро отвергнуты экспертами в этой области."
                            }
                        },
                        {
                            "text": "She found his <w>rodomontade</w> style of speaking quite annoying and insincere.",
                            "target_lang_translations": {
                                "ru": "Она находила его <w>напыщенную</w> манеру говорить довольно раздражающей и неискренней."
                            }
                        }
                    ],
                    "synonyms": [
                        "boastful",
                        "bombastic",
                        "vainglorious",
                        "pretentious"
                    ],
                    "antonyms": [
                        "modest",
                        "humble"
                    ],
                    "target_lang_definitions": {
                        "ru": "Претенциозно хвастливый или напыщенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвастливый",
                                "target_lang_sense_clarification": "склонный к хвастовству"
                            },
                            {
                                "target_lang_word": "напыщенный",
                                "target_lang_sense_clarification": "преисполненный важности, неестественный"
                            },
                            {
                                "target_lang_word": "фанфаронский",
                                "target_lang_sense_clarification": "свойственный фанфарону, хвастуну"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3ef790ee-9b64-338f-abdb-f09e2cab23db",
                    "sense_definition": "To boast, brag, or bluster in a pretentious manner.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He loves to <w>rodomontade</w> about his supposed adventures abroad whenever he has an audience.",
                            "target_lang_translations": {
                                "ru": "Он любит <w>бахвалиться</w> своими мнимыми приключениями за границей всякий раз, когда у него есть слушатели."
                            }
                        },
                        {
                            "text": "Instead of working, he sat there and <w>rodomontaded</w> to anyone who would listen.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы работать, он сидел и <w>разглагольствовал</w> перед любым, кто готов был слушать."
                            }
                        }
                    ],
                    "synonyms": [
                        "boast",
                        "brag",
                        "bluster",
                        "rant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хвастаться, бахвалиться или говорить напыщенно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бахвалиться",
                                "target_lang_sense_clarification": "хвастаться с чрезмерным усердием"
                            },
                            {
                                "target_lang_word": "разглагольствовать",
                                "target_lang_sense_clarification": "многословно и бессодержательно рассуждать"
                            },
                            {
                                "target_lang_word": "хвастаться",
                                "target_lang_sense_clarification": "говорить о себе с похвалой"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "27dbc366-01f6-326c-92ed-bf47dba26b14",
                    "sense_definition": "Горящий постоянно, не угасающий.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В храме перед старинной иконой теплился <w>негасимый</w> огонь лампады.",
                            "target_lang_translations": {
                                "en": "In the temple, before the ancient icon, the <w>inextinguishable</w> flame of the lamp glowed."
                            }
                        },
                        {
                            "text": "Путникам в ночи светил далёкий и <w>негасимый</w> свет маяка.",
                            "target_lang_translations": {
                                "en": "The distant and <w>inextinguishable</w> light of the lighthouse shone for the travelers in the night."
                            }
                        }
                    ],
                    "synonyms": [
                        "неугасающий",
                        "вечный"
                    ],
                    "antonyms": [
                        "гасимый"
                    ],
                    "common_phrases": [
                        "негасимый свет",
                        "негасимый огонь",
                        "негасимая лампада"
                    ],
                    "target_lang_definitions": {
                        "en": "Burning constantly, not going out."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inextinguishable",
                                "target_lang_sense_clarification": "literal; impossible to put out"
                            },
                            {
                                "target_lang_word": "unquenchable",
                                "target_lang_sense_clarification": "often used for fire or thirst"
                            },
                            {
                                "target_lang_word": "eternal",
                                "target_lang_sense_clarification": "lasting forever, e.g., an eternal flame"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bd04ebe-f90c-3d92-82bf-98a0931f24bc",
                    "sense_definition": "Не ослабевающий, сохраняющийся в течение долгого времени (о чувствах, памяти и т. п.).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Материнская <w>негасимая</w> любовь согревала его даже на далёкой чужбине.",
                            "target_lang_translations": {
                                "en": "His mother's <w>undying</w> love warmed him even in a distant foreign land."
                            }
                        },
                        {
                            "text": "В сердце солдата жила <w>негасимая</w> надежда на скорую победу и возвращение домой.",
                            "target_lang_translations": {
                                "en": "An <w>unquenchable</w> hope for a quick victory and return home lived in the soldier's heart."
                            }
                        }
                    ],
                    "synonyms": [
                        "неизбывный",
                        "вечный",
                        "постоянный"
                    ],
                    "common_phrases": [
                        "негасимая любовь",
                        "негасимая надежда",
                        "негасимая память",
                        "негасимая вера"
                    ],
                    "target_lang_definitions": {
                        "en": "Not weakening, persisting for a long time (regarding feelings, memory, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "undying",
                                "target_lang_sense_clarification": "lasting forever; often used for love or hope"
                            },
                            {
                                "target_lang_word": "unfading",
                                "target_lang_sense_clarification": "retaining intensity or brightness over time"
                            },
                            {
                                "target_lang_word": "inextinguishable",
                                "target_lang_sense_clarification": "figurative; cannot be destroyed or suppressed"
                            }
                        ]
                    }
                }
            ]

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4815a1c3-167e-3661-a9e9-0bd299c07f32",
                    "sense_definition": "Simple past and past participle of <w>peg</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The central bank <w>pegged</w> the currency exchange rate to the dollar.",
                            "target_lang_translations": {
                                "ru": "Центральный банк <w>привязал</w> обменный курс валюты к доллару."
                            }
                        },
                        {
                            "text": "She <w>pegged</w> the wet laundry to the clothesline in the garden.",
                            "target_lang_translations": {
                                "ru": "Она <w>прикрепила прищепками</w> мокрое белье к бельевой веревке в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "fastened",
                        "secured",
                        "fixed"
                    ],
                    "antonyms": [
                        "unpegged",
                        "detached"
                    ],
                    "common_phrases": [
                        "pegged at",
                        "pegged down",
                        "pegged out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола peg (прикреплять, фиксировать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привязал",
                                "target_lang_sense_clarification": "о курсе валют или цене"
                            },
                            {
                                "target_lang_word": "прикрепил",
                                "target_lang_sense_clarification": "прищепкой или колышком"
                            },
                            {
                                "target_lang_word": "зафиксировал",
                                "target_lang_sense_clarification": "закрепил на определенном уровне"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c59125ce-d828-3504-9061-4ceafa30973b",
                    "sense_definition": "Fitted or furnished with one or more pegs.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The antique chair was built with <w>pegged</w> joints rather than metal nails.",
                            "target_lang_translations": {
                                "ru": "Антикварный стул был собран на соединениях <w>с деревянными гвоздями</w>, а не на металлических."
                            }
                        },
                        {
                            "text": "He wore a pair of heavy <w>pegged</w> boots suitable for the rough terrain.",
                            "target_lang_translations": {
                                "ru": "На нем была пара тяжелых сапог <w>на деревянных гвоздях</w>, подходящих для пересеченной местности."
                            }
                        }
                    ],
                    "synonyms": [
                        "pinned"
                    ],
                    "common_phrases": [
                        "pegged joint",
                        "pegged sole"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабженный колышками или скрепленный деревянными гвоздями (нагелями)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на деревянных гвоздях",
                                "target_lang_sense_clarification": "о конструкции мебели или обуви"
                            },
                            {
                                "target_lang_word": "скрепленный нагелями",
                                "target_lang_sense_clarification": "столярный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "55314faf-e7d0-3e61-a281-a0d08f9d49ac",
                    "sense_definition": "Więź łącząca grupę ludzi, oparta na przyjaźni, zaufaniu, solidarności i wzajemnej pomocy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "„Wolność, równość, <w>braterstwo</w>” to słynne motto rewolucji francuskiej.",
                            "target_lang_translations": {
                                "ru": "«Свобода, равенство, <w>братство</w>» — знаменитый девиз Французской революции."
                            }
                        },
                        {
                            "text": "Prawdziwe <w>braterstwo</w> między żołnierzami rodzi się w obliczu wspólnego niebezpieczeństwa.",
                            "target_lang_translations": {
                                "ru": "Настоящее <w>братство</w> между солдатами рождается перед лицом общей опасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "solidarność",
                        "przyjaźń",
                        "jedność",
                        "koleżeństwo"
                    ],
                    "antonyms": [
                        "wrogość",
                        "nienawiść",
                        "niezgoda"
                    ],
                    "common_phrases": [
                        "braterstwo broni",
                        "braterstwo krwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узы, связывающие группу людей, основанные на дружбе, доверии, солидарности и взаимопомощи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "братство",
                                "target_lang_sense_clarification": "единение, солидарность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24d57703-792a-31b3-bb72-0fa6efff5006",
                    "sense_definition": "Naturalny związek pokrewieństwa między synami tych samych rodziców.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Łączyło ich <w>braterstwo</w>, ale ich charaktery były zupełnie różne.",
                            "target_lang_translations": {
                                "ru": "Их связывало <w>братство</w>, но их характеры были совершенно разными."
                            }
                        },
                        {
                            "text": "Mimo że łączy ich <w>braterstwo</w>, rzadko ze sobą rozmawiają.",
                            "target_lang_translations": {
                                "ru": "Хотя их связывает <w>братство</w>, они редко разговаривают друг с другом."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokrewieństwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественная родственная связь между сыновьями одних родителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "братство",
                                "target_lang_sense_clarification": "кровная связь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68af8a0c-b121-3ece-8bc6-3671fa1b2e7f",
                    "sense_definition": "Brat wraz ze swoją żoną.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na uroczystość przybyło całe <w>braterstwo</w> z sąsiedniej wioski.",
                            "target_lang_translations": {
                                "ru": "На торжество прибыл <w>брат с женой</w> из соседней деревни."
                            }
                        },
                        {
                            "text": "Moje <w>braterstwo</w> odwiedzi nas w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Мой <w>брат с женой</w> навестят нас на следующей неделе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "znaczenie dawne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Брат вместе со своей женой."
                    },

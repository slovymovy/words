                            }
                        },
                        {
                            "text": "A dangerous <w>arc</w> of electricity jumped between the damaged wires.",
                            "target_lang_translations": {
                                "ru": "Опасная электрическая <w>дуга</w> проскочила между поврежденными проводами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "electric arc",
                        "arc welding"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"

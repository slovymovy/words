{
    "word_family": [
        "spuiten",
        "spuien",
        "spuitbus",
        "spuiter"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0b1072c-d603-357d-9f5d-8e7feaaed83f",
                    "sense_definition": "Een medisch instrument bestaande uit een buisje met een zuiger en vaak een naald, gebruikt om vloeistoffen in te spuiten of op te zuigen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verpleegkundige haalde een steriele <w>spuit</w> uit de verpakking.",
                            "target_lang_translations": {
                                "en": "The nurse took a sterile <w>syringe</w> out of the package.",
                                "ru": "Медсестра достала из упаковки стерильный <w>шприц</w>."
                            }
                        },
                        {
                            "text": "Dit medicijn wordt met een <w>spuit</w> direct in de ader toegediend.",
                            "target_lang_translations": {
                                "en": "This medicine is administered directly into the vein with a <w>syringe</w>.",
                                "ru": "Это лекарство вводится <w>шприцем</w> прямо в вену."
                            }
                        }
                    ],
                    "synonyms": [
                        "injectiespuit"
                    ],
                    "common_phrases": [
                        "een steriele spuit",
                        "een spuit vullen"
                    ],
                    "target_lang_definitions": {
                        "en": "A medical instrument consisting of a tube with a piston and often a needle, used to inject or withdraw fluids.",
                        "ru": "Медицинский инструмент, состоящий из цилиндра с поршнем и часто иглы, используемый для введения жидкостей или их отсасывания."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "syringe",
                                "target_lang_sense_clarification": "medical instrument"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шприц",
                                "target_lang_sense_clarification": "медицинский инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a616b6a5-4ea6-31cf-a44b-7c51c2235360",
                    "sense_definition": "De handeling van het inspuiten van een vloeistof (zoals een medicijn of vaccin) in het lichaam.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De patiënt kreeg een <w>spuit</w> in zijn arm tegen de pijn.",
                            "target_lang_translations": {
                                "en": "The patient received a <w>shot</w> in his arm for the pain.",
                                "ru": "Пациенту сделали <w>укол</w> в руку от боли."
                            }
                        },
                        {
                            "text": "Veel mensen zijn bang als ze bij de dokter een <w>spuit</w> moeten halen.",
                            "target_lang_translations": {
                                "en": "Many people are afraid when they have to get a <w>shot</w> at the doctor's.",
                                "ru": "Многие люди боятся, когда им нужно сделать <w>укол</w> у врача."
                            }
                        }
                    ],
                    "synonyms": [
                        "injectie",
                        "prik"
                    ],
                    "common_phrases": [
                        "een spuit krijgen",
                        "een spuit geven"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of injecting a liquid (such as a medicine or vaccine) into the body.",
                        "ru": "Действие по введению жидкости (например, лекарства или вакцины) в организм."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shot",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "injection",
                                "target_lang_sense_clarification": "medical procedure"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "укол",
                                "target_lang_sense_clarification": "процедура"
                            },
                            {
                                "target_lang_word": "инъекция",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff8438ad-0c79-3f3f-b6a4-755210afe975",
                    "sense_definition": "Een mondstuk of tuit waaruit een vloeistof met kracht naar buiten komt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "device_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er kwam geen water meer uit de <w>spuit</w> van de tuinslang.",
                            "target_lang_translations": {
                                "en": "No more water came out of the <w>nozzle</w> of the garden hose.",
                                "ru": "Из <w>наконечника</w> садового шланга перестала течь вода."
                            }
                        },
                        {
                            "text": "De brandweer richtte de <w>spuit</w> op het brandende gebouw.",
                            "target_lang_translations": {
                                "en": "The fire department aimed the <w>hose</w> at the burning building.",
                                "ru": "Пожарные направили <w>брандспойт</w> на горящее здание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de spuit erop zetten"
                    ],
                    "target_lang_definitions": {
                        "en": "A mouthpiece or spout from which a liquid comes out with force.",
                        "ru": "Насадка или наконечник, из которого с силой вырывается жидкость."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nozzle",
                                "target_lang_sense_clarification": "device part"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "брандспойт",
                                "target_lang_sense_clarification": "пожарный рукав"
                            },
                            {
                                "target_lang_word": "наконечник",
                                "target_lang_sense_clarification": "шланга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c49e7ea-b1c6-3e60-a9e1-3ce7190e5bee",
                    "sense_definition": "Een dodelijke injectie om een dier of mens pijnloos te laten sterven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "euthanasia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Onze oude hond was erg ziek en heeft bij de dierenarts een <w>spuitje</w> gekregen.",
                            "target_lang_translations": {
                                "en": "Our old dog was very sick and was given a <w>lethal injection</w> at the vet.",
                                "ru": "Наша старая собака была очень больна, и ветеринар сделал ей <w>усыпляющий укол</w>."
                            }
                        },
                        {
                            "text": "Het was een moeilijke beslissing om de kat een <w>spuitje</w> te laten geven.",
                            "target_lang_translations": {
                                "en": "It was a difficult decision to have the cat given a <w>lethal injection</w>.",
                                "ru": "Было трудным решением сделать кошке <w>усыпляющий укол</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een spuitje krijgen",
                        "een spuitje geven"
                    ],
                    "target_lang_definitions": {
                        "en": "A lethal injection to allow an animal or human to die painlessly.",
                        "ru": "Смертельная инъекция для безболезненного умерщвления животного или человека."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lethal injection",
                                "target_lang_sense_clarification": "euthanasia"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "усыпляющий укол",
                                "target_lang_sense_clarification": "эвтаназия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5b6c85a-a7fb-3e84-9899-9011c4ef06b5",
                    "sense_definition": "Een vuurwapen, zoals een geweer of pistool.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De overvaller bedreigde de kassière met een geladen <w>spuit</w>.",
                            "target_lang_translations": {
                                "en": "The robber threatened the cashier with a loaded <w>gun</w>.",
                                "ru": "Грабитель угрожал кассиру заряженной <w>пушкой</w>."
                            }
                        },
                        {
                            "text": "In de actiefilm liep de held rond met een enorme <w>spuit</w>.",
                            "target_lang_translations": {
                                "en": "In the action movie, the hero walked around with a huge <w>piece</w>.",
                                "ru": "В боевике герой ходил с огромной <w>пушкой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "geweer"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A firearm, such as a rifle or pistol.",
                        "ru": "Огнестрельное оружие, такое как винтовка или пистолет."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gun",
                                "target_lang_sense_clarification": "slang"
                            },
                            {
                                "target_lang_word": "piece",
                                "target_lang_sense_clarification": "slang"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пушка",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "ствол",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "822af57e-5260-3b71-8dbb-5d8952463a60",
                    "sense_definition": "Vervoeging van <w>spuiten</w>: eerste, tweede en derde persoon enkelvoud tegenwoordige tijd, en gebiedende wijs.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form_spuiten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fontein <w>spuit</w> water hoog de lucht in.",
                            "target_lang_translations": {
                                "en": "The fountain <w>sprays</w> water high into the air.",
                                "ru": "Фонтан <w>выбрасывает</w> воду высоко в воздух."
                            }
                        },
                        {
                            "text": "Hij <w>spuit</w> wat verf op de muur om de kleur te testen.",
                            "target_lang_translations": {
                                "en": "He <w>sprays</w> some paint on the wall to test the color.",
                                "ru": "Он <w>распыляет</w> немного краски на стену, чтобы проверить цвет."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present tense / imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugation of <w>spuiten</w>: first, second, and third person singular present tense, and imperative.",
                        "ru": "Спряжение глагола \"spuiten\": брызгать, распылять или бить струёй."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sprays",
                                "target_lang_sense_clarification": "present tense"
                            },
                            {
                                "target_lang_word": "squirts",
                                "target_lang_sense_clarification": "present tense"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "брызгает",
                                "target_lang_sense_clarification": "жидкостью"
                            },
                            {
                                "target_lang_word": "распыляет",
                                "target_lang_sense_clarification": "краску/аэрозоль"
                            },
                            {
                                "target_lang_word": "выбрасывает",
                                "target_lang_sense_clarification": "струю воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48a8ab7e-1fdd-3fc8-8c93-6704437ba4d7",
                    "sense_definition": "Vervoeging van <w>spuien</w>: tweede en derde persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form_spuien",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>spuit</w> zijn ongenoegen over de nieuwe regels tijdens de vergadering.",
                            "target_lang_translations": {
                                "en": "He <w>vents</w> his dissatisfaction about the new rules during the meeting.",
                                "ru": "Он <w>изливает</w> свое недовольство новыми правилами во время собрания."
                            }
                        },
                        {
                            "text": "Het gemaal <w>spuit</w> het overtollige water naar de zee.",
                            "target_lang_translations": {
                                "en": "The pumping station <w>discharges</w> the excess water into the sea.",
                                "ru": "Насосная станция <w>сбрасывает</w> лишнюю воду в море."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present tense"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugation of <w>spuien</w>: second and third person singular present tense.",
                        "ru": "Спряжение глагола \"spuien\": изливать (мнение) или сбрасывать (воду)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vents",
                                "target_lang_sense_clarification": "figurative"
                            },
                            {
                                "target_lang_word": "discharges",
                                "target_lang_sense_clarification": "literal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "изливает",
                                "target_lang_sense_clarification": "высказывать мнение"
                            },
                            {
                                "target_lang_word": "сбрасывает",
                                "target_lang_sense_clarification": "воду"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
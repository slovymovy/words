    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "584d5866-ae61-3ff9-ae68-1784dccb89b5",
                    "sense_definition": "Powodować zator na drodze lub blokować swobodny przejazd; często używane w formie zwrotnej, gdy ulica staje się nieprzejezdna z powodu dużej liczby samochodów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "traffic_congestion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wypadek na moście zaczął <korkować> całą okolicę.",
                            "target_lang_translations": {
                                "ru": "Авария на мосту начала <закупоривать> весь район."
                            }
                        },
                        {
                            "text": "W godzinach szczytu centrum miasta lubi się <korkować>.",
                            "target_lang_translations": {
                                "ru": "В часы пик центр города любит <вставать в пробки>."
                            }
                        }
                    ],
                    "synonyms": [
                        "blokować",
                        "tarasować",
                        "zatykać"
                    ],
                    "antonyms": [
                        "udrażniać"
                    ],
                    "common_phrases": [
                        "korkować miasto",
                        "korkować ulicę",
                        "strasznie się korkować"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "powszechnie używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать затор на дороге или блокировать свободный проезд; часто используется в возвратной форме, когда улица становится непроезжей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "создавать пробки",
                                "target_lang_sense_clarification": "о дорожном движении"
                            },
                            {
                                "target_lang_word": "закупоривать",
                                "target_lang_sense_clarification": "блокировать движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9178d9b-1a72-35f7-a921-e4cb620cffbb",
                    "sense_definition": "Zamykać szczelnie butelkę lub inne naczynie, wciskając w jego otwór korek.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "closing_container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Winiarz musi ręcznie <korkować> każdą butelkę swojego wina.",
                            "target_lang_translations": {
                                "ru": "Винодел должен вручную <закупоривать> каждую бутылку своего вина."
                            }
                        },
                        {
                            "text": "Zaczęliśmy <korkować> soki, aby przetrwały zimę w piwnicy.",
                            "target_lang_translations": {
                                "ru": "Мы начали <закупоривать> соки, чтобы они сохранились зимой в подвале."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatykać"
                    ],
                    "antonyms": [
                        "odkorkowywać",
                        "otwierać"
                    ],
                    "common_phrases": [
                        "korkować butelki",
                        "korkować wino",
                        "szczelnie korkować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плотно закрывать бутылку или другой сосуд, вставляя пробку в отверстие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закупоривать",
                                "target_lang_sense_clarification": "закрывать пробкой"
                            },
                            {
                                "target_lang_word": "укупоривать",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f376123d-36ca-3b98-8bf3-89ba2576e5b6",
                    "sense_definition": "Umierać; bardzo potoczne, często lekceważące lub żartobliwe określenie końca życia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "death_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary sąsiad zaczął <korkować>, więc wezwano pogotowie.",
                            "target_lang_translations": {
                                "ru": "Старый сосед начал <загибаться>, поэтому вызвали скорую."
                            }
                        },
                        {
                            "text": "Nie zamierzam jeszcze <korkować>, czuję się całkiem zdrowo.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь еще <загибаться>, чувствую себя вполне здоровым."
                            }
                        }
                    ],
                    "synonyms": [
                        "umierać",
                        "wykitować",
                        "kopnąć w kalendarz"
                    ],
                    "antonyms": [
                        "żyć",
                        "rodzić się"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Умирать; очень разговорное, часто пренебрежительное или шутливое обозначение конца жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загибаться",
                                "target_lang_sense_clarification": "сленг, умирать"
                            },
                            {
                                "target_lang_word": "отдавать концы",
                                "target_lang_sense_clarification": "фразеологизм"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
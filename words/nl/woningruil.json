    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "675893df-f618-3498-8fe0-b7d552b36f7c",
                    "sense_definition": "Het ruilen van een woning waarbij beide partijen permanent naar elkaars huis verhuizen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij hebben hun nieuwe appartement in de stad gevonden door middel van <w>woningruil</w>.",
                            "target_lang_translations": {
                                "ru": "Они нашли свою новую квартиру в городе посредством <w>обмена жильем</w>."
                            }
                        },
                        {
                            "text": "Bij <w>woningruil</w> moet de verhuurder vaak eerst toestemming geven voordat je mag verhuizen.",
                            "target_lang_translations": {
                                "ru": "При <w>обмене жильем</w> арендодатель часто должен сначала дать разрешение, прежде чем вы сможете переехать."
                            }
                        }
                    ],
                    "synonyms": [
                        "huisruil"
                    ],
                    "common_phrases": [
                        "aan woningruil doen",
                        "woningruil aanvragen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обмен жильем, при котором обе стороны переезжают в дома друг друга на постоянной основе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмен жильем",
                                "target_lang_sense_clarification": "постоянный переезд"
                            },
                            {
                                "target_lang_word": "обмен квартирами",
                                "target_lang_sense_clarification": "между арендаторами или собственниками"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ece87b5f-4d51-32e7-afd5-b6a252294ef9",
                    "sense_definition": "Het tijdelijk ruilen van een woning met iemand anders voor een vakantie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door <w>woningruil</w> konden we heel goedkoop twee weken in Londen verblijven.",
                            "target_lang_translations": {
                                "ru": "Благодаря <w>обмену домами</w> мы смогли очень дешево прожить две недели в Лондоне."
                            }
                        },
                        {
                            "text": "Onze eerste ervaring met <w>woningruil</w> in Spanje was geweldig en we willen het volgend jaar weer doen.",
                            "target_lang_translations": {
                                "ru": "Наш первый опыт <w>обмена жильем</w> в Испании был замечательным, и мы хотим повторить это в следующем году."
                            }
                        }
                    ],
                    "synonyms": [
                        "huizenruil"
                    ],
                    "common_phrases": [
                        "op vakantie via woningruil"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временный обмен жильем с другим человеком на время отпуска."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмен домами",
                                "target_lang_sense_clarification": "на время отпуска"
                            },
                            {
                                "target_lang_word": "обмен жильем",
                                "target_lang_sense_clarification": "туристический"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        {
                            "text": "The car hit the wall <w>flush</w>, causing significant damage to the front.",
                            "target_lang_translations": {
                                "ru": "Машина врезалась в стену <w>прямо</w>, вызвав значительные повреждения передней части."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hit flush",
                        "land flush"
                    ],
                    "target_lang_definitions": {

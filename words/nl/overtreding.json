    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b7a3ae37-8dea-319e-a968-fd07d43932ee",
                    "sense_definition": "Het handelen in strijd met een wet, regel of voorschrift.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "violation_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het negeren van het stoplicht is een duidelijke <w>overtreding</w> van de verkeersregels.",
                            "target_lang_translations": {
                                "ru": "Игнорирование светофора — это явное <w>нарушение</w> правил дорожного движения.",
                                "en": "Ignoring the traffic light is a clear <w>violation</w> of the traffic rules."
                            }
                        },
                        {
                            "text": "Hij was in <w>overtreding</w> omdat hij geen geldig kaartje had gekocht.",
                            "target_lang_translations": {
                                "ru": "Он совершил <w>нарушение</w>, так как не купил действительный билет.",
                                "en": "He was in <w>violation</w> because he had not bought a valid ticket."
                            }
                        }
                    ],
                    "synonyms": [
                        "schending",
                        "inbreuk"
                    ],
                    "antonyms": [
                        "naleving"
                    ],
                    "common_phrases": [
                        "in overtreding zijn",
                        "een overtreding begaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, противоречащее закону, правилу или предписанию.",
                        "en": "Acting in conflict with a law, rule, or regulation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарушение",
                                "target_lang_sense_clarification": "закона или правил"
                            },
                            {
                                "target_lang_word": "несоблюдение",
                                "target_lang_sense_clarification": "правил или норм"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "violation",
                                "target_lang_sense_clarification": "of a rule or law"
                            },
                            {
                                "target_lang_word": "offense",
                                "target_lang_sense_clarification": "illegal act"
                            },
                            {
                                "target_lang_word": "infringement",
                                "target_lang_sense_clarification": "breach of regulations"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "285c0871-a959-343f-b4c6-a4c012e7e1de",
                    "sense_definition": "Een handeling tijdens een sportwedstrijd die tegen de spelregels ingaat.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De scheidsrechter floot direct na de zware <w>overtreding</w> op de verdediger.",
                            "target_lang_translations": {
                                "ru": "Судья дал свисток сразу после грубого <w>нарушения</w> против защитника.",
                                "en": "The referee whistled immediately after the heavy <w>foul</w> on the defender."
                            }
                        },
                        {
                            "text": "Door die onnodige <w>overtreding</w> kreeg de tegenpartij een strafschop.",
                            "target_lang_translations": {
                                "ru": "Из-за этого ненужного <w>нарушения</w> команда противника получила право на пенальти.",
                                "en": "Due to that unnecessary <w>foul</w>, the opposing team was awarded a penalty kick."
                            }
                        }
                    ],
                    "synonyms": [
                        "fout"
                    ],
                    "common_phrases": [
                        "een zware overtreding",
                        "een overtreding maken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие во время спортивного состязания, противоречащее правилам игры.",
                        "en": "An action during a sports match that goes against the game rules."

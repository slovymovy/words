    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c4892200-08f7-3613-88f4-fa79820fe216",
                    "sense_definition": "Osoba, która uciekła z miejsca odosobnienia, więzienia, wojska lub od przymusowej pracy, aby uniknąć kary.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja zorganizowała szeroką obławę na niebezpiecznego <zbiega>.",
                            "target_lang_translations": {
                                "ru": "Полиция организовала масштабную облаву на опасного <беглеца>."
                            }
                        },
                        {
                            "text": "Więzienny <zbieg> ukrywał się w gęstym lesie przez trzy dni.",
                            "target_lang_translations": {
                                "ru": "Сбежавший из тюрьмы <беглец> прятался в густом лесу три дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "uciekinier",
                        "dezerter"
                    ],
                    "common_phrases": [
                        "zbieg z więzienia",
                        "poszukiwany zbieg",
                        "chwytać zbiega"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, сбежавший из места заключения, армии или с принудительных работ, чтобы избежать наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беглец",
                                "target_lang_sense_clarification": "человек, совершивший побег"
                            },
                            {
                                "target_lang_word": "дезертир",
                                "target_lang_sense_clarification": "сбежавший с военной службы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10e70d4e-f85f-3cf4-bad5-1c54febdc344",
                    "sense_definition": "Jednoczesne wystąpienie wielu zdarzeń, faktów lub okoliczności, często przypadkowe.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To był tylko niefortunny <zbieg> okoliczności, że spotkaliśmy się w tym miejscu.",
                            "target_lang_translations": {
                                "ru": "Это было лишь неудачное <стечение> обстоятельств, что мы встретились в этом месте."
                            }
                        },
                        {
                            "text": "Przez dziwny <zbieg> wydarzeń spóźniłem się na ostatni pociąg do domu.",
                            "target_lang_translations": {
                                "ru": "Из-за странного <стечения> обстоятельств я опоздал на последний поезд домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "nagromadzenie"
                    ],
                    "common_phrases": [
                        "zbieg okoliczności",
                        "zbieg wydarzeń",
                        "szczęśliwy zbieg okoliczności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одновременное возникновение множества событий, фактов или обстоятельств, часто случайное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стечение",
                                "target_lang_sense_clarification": "обстоятельств или событий"
                            },
                            {
                                "target_lang_word": "совпадение",
                                "target_lang_sense_clarification": "случайное сочетание событий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e898cdef-597b-3bec-a12f-65156a5ef686",
                    "sense_definition": "Miejsce, w którym łączą się lub krzyżują drogi, rzeki lub inne elementy liniowe.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Umówiliśmy się na spotkanie u <zbiegu> ulicy Długiej i Krótkiej.",
                            "target_lang_translations": {
                                "ru": "Мы договорились встретиться на <пересечении> улиц Длугой и Круткой."
                            }
                        },
                        {
                            "text": "To historyczne miasto leży przy malowniczym <zbiegu> dwóch wielkich rzek.",
                            "target_lang_translations": {
                                "ru": "Этот исторический город расположен у живописного <слияния> двух великих рек."
                            }
                        }
                    ],
                    "synonyms": [
                        "styk"
                    ],
                    "common_phrases": [
                        "u zbiegu ulic",
                        "na zbiegu rzek",
                        "zbieg dróg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где соединяются или пересекаются дороги, реки или другие линейные объекты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пересечение",
                                "target_lang_sense_clarification": "дорог, улиц"
                            },
                            {
                                "target_lang_word": "слияние",
                                "target_lang_sense_clarification": "рек, потоков"
                            },
                            {
                                "target_lang_word": "стык",
                                "target_lang_sense_clarification": "место соединения"
                            }
                        ]
                    }
                }
            ]
        },
        {

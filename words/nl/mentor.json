                {
                    "sense_id": "44f89cc4-2fcd-36ec-9995-53e4125ec9c7",
                    "sense_definition": "Een leraar die leerlingen of studenten begeleidt bij hun studie en persoonlijke ontwikkeling op school.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb morgen een gesprek met mijn <w>mentor</w> over mijn cijfers.",
                            "target_lang_translations": {
                                "ru": "Завтра у меня разговор с моим <w>классным руководителем</w> насчет моих оценок.",
                                "en": "I have a meeting with my <w>form tutor</w> tomorrow about my grades."
                            }
                        },
                        {
                            "text": "De <w>mentor</w> helpt de klas om goed samen te werken.",
                            "target_lang_translations": {
                                "ru": "<w>Классный руководитель</w> помогает классу хорошо взаимодействовать.",
                                "en": "The <w>form tutor</w> helps the class work well together."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mentor van de klas",
                        "naar de mentor gaan",
                        "gesprek met de mentor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учитель, который курирует учеников или студентов в их учебе и личностном развитии.",
                        "en": "A teacher who guides pupils or students in their studies and personal development at school."
                    },
                    "translations": {

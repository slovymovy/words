    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a20ea9d8-8b8b-381b-a86e-7b0c381a541d",
                    "sense_definition": "To try to do something again, especially after a previous failure.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The student decided to <w>reattempt</w> the difficult exam next semester.",
                            "target_lang_translations": {
                                "ru": "Студент решил <w>пересдать</w> сложный экзамен в следующем семестре."
                            }
                        },
                        {
                            "text": "If the download fails, the software will automatically <w>reattempt</w> the connection.",
                            "target_lang_translations": {
                                "ru": "Если загрузка прервется, программа автоматически <w>повторит попытку</w> соединения."
                            }
                        }
                    ],
                    "synonyms": [
                        "retry"
                    ],
                    "common_phrases": [
                        "reattempt the test",
                        "reattempt a connection",
                        "reattempt the mission"
                    ],
                    "target_lang_definitions": {
                        "ru": "Попытаться сделать что-то снова, особенно после предыдущей неудачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повторить попытку",
                                "target_lang_sense_clarification": "сделать еще одну попытку выполнить действие"
                            },
                            {
                                "target_lang_word": "попытаться снова",
                                "target_lang_sense_clarification": "попробовать еще раз"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cfa42bc4-64bd-3dd2-a716-57569b032b73",
                    "sense_definition": "A second or subsequent effort to achieve something.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>reattempt</w> at climbing the mountain was successful due to better weather.",
                            "target_lang_translations": {
                                "ru": "Его <w>повторная попытка</w> восхождения на гору увенчалась успехом благодаря лучшей погоде."
                            }
                        },
                        {
                            "text": "The team made a <w>reattempt</w> to solve the puzzle before time ran out.",
                            "target_lang_translations": {
                                "ru": "Команда предприняла <w>повторную попытку</w> решить головоломку до того, как истекло время."
                            }
                        }
                    ],
                    "synonyms": [
                        "retry"
                    ],
                    "common_phrases": [
                        "make a reattempt",
                        "reattempt at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вторая или последующая попытка достичь чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повторная попытка",
                                "target_lang_sense_clarification": "действие по возобновлению усилий"
                            }
                        ]
                    }
                }
            ]
        }
    ]

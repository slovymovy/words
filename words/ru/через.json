    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "47816cb6-2682-3ef1-99ed-614692ace50c",
                    "sense_definition": "По прошествии, спустя определённое время",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы встретимся с тобой <через> один час.",
                            "target_lang_translations": {
                                "en": "We will meet <in> one hour."
                            }
                        },
                        {
                            "text": "Поезд отправится <через> пять минут.",
                            "target_lang_translations": {
                                "en": "The train will depart <in> five minutes."
                            }
                        }
                    ],
                    "synonyms": [
                        "спустя",
                        "чрез"
                    ],
                    "antonyms": [
                        "до"
                    ],
                    "common_phrases": [
                        "через минуту",
                        "через час",
                        "через неделю",
                        "через год",
                        "через некоторое время"
                    ],
                    "target_lang_definitions": {
                        "en": "After a certain period of time has passed"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "used for future events indicating the time until they happen"
                            },
                            {
                                "target_lang_word": "after",
                                "target_lang_sense_clarification": "indicating a sequence of time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8828f1c9-4c0f-3fd5-9ce3-d2cfa5ca7108",
                    "sense_definition": "С одной стороны на другую, пересекая пространство или препятствие",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_crossing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дети осторожно перешли <через> широкую дорогу.",
                            "target_lang_translations": {
                                "en": "The children carefully walked <across> the wide road."
                            }
                        },
                        {
                            "text": "Мяч перелетел <через> высокий деревянный забор.",
                            "target_lang_translations": {
                                "en": "The ball flew <over> the high wooden fence."
                            }
                        }
                    ],
                    "synonyms": [
                        "поперёк",
                        "поверх",
                        "над",
                        "чрез"
                    ],
                    "antonyms": [
                        "снизу"
                    ],
                    "common_phrases": [
                        "через дорогу",
                        "через забор",
                        "через мост",
                        "через плечо",
                        "через край"
                    ],
                    "target_lang_definitions": {
                        "en": "Moving from one side to the other, crossing a space or obstacle"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "across",
                                "target_lang_sense_clarification": "movement from one side of a surface to the other"
                            },
                            {
                                "target_lang_word": "over",
                                "target_lang_sense_clarification": "movement above or surmounting an obstacle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "291e9eb7-3692-39a6-ac48-67cfbfcfade8",
                    "sense_definition": "Следуя по маршруту, пролегающему в определённом месте или через промежуточный пункт",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_route",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш автобус едет в аэропорт <через> центр города.",
                            "target_lang_translations": {
                                "en": "Our bus is going to the airport <through> the city center."
                            }
                        },
                        {
                            "text": "Путь к лесному озеру лежит <через> густой лес.",
                            "target_lang_translations": {
                                "en": "The path to the forest lake lies <through> the dense forest."
                            }
                        }
                    ],
                    "common_phrases": [
                        "через центр",
                        "через лес",
                        "через Москву",
                        "через поле"
                    ],
                    "target_lang_definitions": {
                        "en": "Following a route that passes through a specific place or intermediate point"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "through",
                                "target_lang_sense_clarification": "passing within or across a place"
                            },
                            {
                                "target_lang_word": "via",
                                "target_lang_sense_clarification": "by way of a specific place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50341d26-92cb-307b-ab66-204a6de158b8",
                    "sense_definition": "С помощью, посредством кого-либо или чего-либо",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "instrumental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Билеты в театр можно купить <через> интернет.",
                            "target_lang_translations": {
                                "en": "Theater tickets can be bought <through> the internet."
                            }
                        },
                        {
                            "text": "Он передал важное письмо <через> своего секретаря.",
                            "target_lang_translations": {
                                "en": "He passed an important letter <through> his secretary."
                            }
                        }
                    ],
                    "synonyms": [
                        "с помощью",
                        "посредством",
                        "чрез"
                    ],
                    "common_phrases": [
                        "через интернет",
                        "через знакомых",
                        "через посредника",
                        "через банк"
                    ],
                    "target_lang_definitions": {
                        "en": "By means of or with the help of someone or something"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "through",
                                "target_lang_sense_clarification": "using an intermediary or medium"
                            },
                            {
                                "target_lang_word": "via",
                                "target_lang_sense_clarification": "by means of a system or person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdcf045f-8f4c-33e7-bc4f-a8e3c95444b6",
                    "sense_definition": "Сквозь отверстие или среду, проникая внутрь и выходя наружу",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_penetration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Яркий солнечный свет пробивался <через> плотные шторы.",
                            "target_lang_translations": {
                                "en": "Bright sunlight was coming <through> the thick curtains."
                            }
                        },
                        {
                            "text": "Вода просочилась <через> старую крышу во время дождя.",
                            "target_lang_translations": {
                                "en": "Water leaked <through> the old roof during the rain."
                            }
                        }
                    ],
                    "synonyms": [
                        "сквозь"
                    ],
                    "common_phrases": [
                        "через щель",
                        "через толпу",
                        "через стекло",
                        "через дыру"
                    ],
                    "target_lang_definitions": {
                        "en": "Passing through an opening or medium"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "through",
                                "target_lang_sense_clarification": "penetrating a substance or barrier"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "718350bd-e445-3544-94ae-e0913dc4e0ef",
                    "sense_definition": "С пропуском одной единицы в ряду (о времени или пространстве)",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач сказал принимать это лекарство <через> день.",
                            "target_lang_translations": {
                                "en": "The doctor said to take this medicine <every other> day."
                            }
                        },
                        {
                            "text": "Молодые деревья были посажены <через> каждые пять метров.",
                            "target_lang_translations": {
                                "en": "Young trees were planted <every> five meters."
                            }
                        }
                    ],
                    "common_phrases": [
                        "через день",
                        "через шаг",
                        "через строчку",
                        "через раз"
                    ],
                    "target_lang_definitions": {
                        "en": "Skipping one unit in a series (regarding time or space)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "every other",
                                "target_lang_sense_clarification": "used for time intervals, skipping one"
                            },
                            {
                                "target_lang_word": "every",
                                "target_lang_sense_clarification": "used for spatial intervals"
                            }
                        ]
                    }
                }
            ]
        },

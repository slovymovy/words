{
    "word_family": [
        "neonowy",
        "neonówka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fbe984f8-d962-3a41-82f3-ce9a2422f661",
                    "sense_definition": "Szyld lub reklama świetlna zbudowana z rurek wypełnionych gazem, który świeci pod wpływem prądu elektrycznego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "lighting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad wejściem do starego kina wciąż świeci uszkodzony <w>neon</w>.",
                            "target_lang_translations": {
                                "ru": "Над входом в старый кинотеатр всё ещё светится повреждённая <w>неоновая вывеска</w>."
                            }
                        },
                        {
                            "text": "Nocą ulice miasta rozświetlają kolorowe <w>neony</w> sklepów i restauracji.",
                            "target_lang_translations": {
                                "ru": "Ночью улицы города освещают разноцветные <w>неоновые вывески</w> магазинов и ресторанов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kolorowy neon",
                        "mrugający neon",
                        "światło neonu",
                        "w blasku neonów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вывеска или световая реклама из трубок, наполненных газом, который светится под воздействием электричества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неоновая вывеска",
                                "target_lang_sense_clarification": "рекламная конструкция"
                            },
                            {
                                "target_lang_word": "неон",
                                "target_lang_sense_clarification": "свет или лампа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0504b18b-df60-3873-964b-7ebc049a0de7",
                    "sense_definition": "Pierwiastek chemiczny o symbolu Ne i liczbie atomowej 10; bezwonny i bezbarwny gaz szlachetny.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tablicy Mendelejewa <w>neon</w> znajduje się w grupie helowców.",
                            "target_lang_translations": {
                                "ru": "В таблице Менделеева <w>неон</w> находится в группе инертных газов."
                            }
                        },
                        {
                            "text": "Nauczyciel chemii wyjaśnił nam, jakie właściwości ma <w>neon</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель химии объяснил нам, какими свойствами обладает <w>неон</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Ne"
                    ],
                    "common_phrases": [
                        "gaz szlachetny",
                        "atom neonu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химический элемент с символом Ne и атомным номером 10; инертный газ без цвета и запаха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неон",
                                "target_lang_sense_clarification": "химический элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e30a76d-b3fd-3bad-ae37-13af8265fab4",
                    "sense_definition": "Mała, słodkowodna ryba akwariowa o charakterystycznym, błyszczącym pasku na boku.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W moim nowym akwarium pływa duże stado <w>neonów</w>.",
                            "target_lang_translations": {
                                "ru": "В моём новом аквариуме плавает большая стайка <w>неонов</w>."
                            }
                        },
                        {
                            "text": "Muszę kupić specjalny pokarm dla <w>neonów</w> i innych małych ryb.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить специальный корм для <w>неонов</w> и других маленьких рыбок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "neon Innesa",
                        "hodowla neonów",
                        "stado neonów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькая пресноводная аквариумная рыбка с характерной блестящей полоской на боку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неон",
                                "target_lang_sense_clarification": "рыбка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "a stain/splinter"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "a mark/stain"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97f46555-d000-3710-bd13-2c744b84f37d",
                    "sense_definition": "Прочно прикрепить, зафиксировать или надеть что-либо на что-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attach_fix",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мастер <посадил> отломанную деталь на клей.",
                            "target_lang_translations": {

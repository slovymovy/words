        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a977e3b2-21da-3ab9-bbcd-35adf0606ebe",
                    "sense_definition": "To fail completely; not to be successful at all (of a movie, play, book, song, etc.).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new big-budget movie <flopped> at the box office despite the famous cast."
                        },
                        {
                            "text": "Her attempt to start a business <flopped> because of poor planning."
                        }
                    ],
                    "synonyms": [
                        "box office bomb",
                        "fiasco",
                        "dud"
                    ],
                    "antonyms": [
                        "succeed",
                        "triumph"
                    ],
                    "common_phrases": [

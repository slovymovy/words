            "pos": "noun",
            "senses": [
                {
                    "sense_id": "703b7345-bf05-35ec-a5ad-8ae97b411da4",
                    "sense_definition": "Het plotseling en vaak met geweld binnendringen in een gebouw of gebied, meestal door politie of een leger.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "raid_invasion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie deed vanochtend een <w>inval</w> in een loods in de haven.",
                            "target_lang_translations": {
                                "ru": "Сегодня утром полиция совершила <w>рейд</w> на склад в порту."
                            }
                        },
                        {
                            "text": "Na de militaire <w>inval</w> vluchtten veel bewoners naar het buitenland.",
                            "target_lang_translations": {
                                "ru": "После военного <w>вторжения</w> многие жители бежали за границу."
                            }
                        }
                    ],
                    "synonyms": [
                        "raid",
                        "invasie",
                        "bestorming"
                    ],
                    "antonyms": [
                        "aftocht",
                        "terugtrekking"
                    ],
                    "common_phrases": [
                        "een inval doen",
                        "militaire inval",
                        "gewapende inval"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное, часто насильственное проникновение в здание или на территорию, обычно совершаемое полицией или армией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вторжение",
                                "target_lang_sense_clarification": "военное"
                            },
                            {
                                "target_lang_word": "рейд",
                                "target_lang_sense_clarification": "полицейский"
                            },
                            {
                                "target_lang_word": "налёт",
                                "target_lang_sense_clarification": "внезапная атака"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e6efcc8-34c9-37eb-b255-37c6ed925c0c",
                    "sense_definition": "Een plotseling opkomende, vaak goede gedachte of idee.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "inspiration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schrijver kreeg midden in de nacht een briljante <w>inval</w>.",
                            "target_lang_translations": {
                                "ru": "Посреди ночи на писателя снизошло блестящее <w>озарение</w>."
                            }
                        },
                        {
                            "text": "Het was een gelukkige <w>inval</w> om een paraplu mee te nemen.",
                            "target_lang_translations": {
                                "ru": "Взять с собой зонт было удачной <w>мыслью</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ingeving",
                        "idee",
                        "bevlieging"
                    ],
                    "common_phrases": [
                        "een gelukkige inval",
                        "briljante inval"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно возникшая, часто удачная мысль или идея."
                    },
                    "translations": {

    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3ebc833a-ad1f-3ad5-82a8-d53181465fc0",
                    "sense_definition": "Znajdujący się w środku czegoś, otoczony przez inne części lub warstwy; przeciwieństwo zewnętrznego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Włóż portfel bezpiecznie do <w>wewnętrznej</w> kieszeni kurtki.",
                            "target_lang_translations": {
                                "ru": "Положи кошелек надежно во <w>внутренний</w> карман куртки."
                            }
                        },
                        {
                            "text": "Ten stary budynek posiada piękny <w>wewnętrzny</w> dziedziniec pełen kwiatów.",
                            "target_lang_translations": {
                                "ru": "У этого старого здания есть красивый <w>внутренний</w> дворик, полный цветов."
                            }
                        }
                    ],
                    "antonyms": [
                        "zewnętrzny"
                    ],
                    "common_phrases": [
                        "wewnętrzna kieszeń",
                        "wewnętrzna strona",
                        "drzwi wewnętrzne",
                        "organy wewnętrzne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся внутри чего-либо, окруженный другими частями; противоположность внешнему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутренний",
                                "target_lang_sense_clarification": "о пространстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "046f1fed-aee0-3787-8472-9e64e4257d3b",
                    "sense_definition": "Dotyczący spraw dziejących się w granicach jednego państwa lub organizacji, a nie relacji zagranicznych.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "political",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd zapowiedział nowe reformy, które mają poprawić bezpieczeństwo <w>wewnętrzne</w> kraju.",
                            "target_lang_translations": {
                                "ru": "Правительство объявило о новых реформах, призванных улучшить <w>внутреннюю</w> безопасность страны."
                            }
                        },
                        {
                            "text": "Polityka <w>wewnętrzna</w> państwa skupia się obecnie na walce z bezrobociem.",
                            "target_lang_translations": {
                                "ru": "<w>Внутренняя</w> политика государства в настоящее время сосредоточена на борьбе с безработицей."
                            }
                        }
                    ],
                    "antonyms": [
                        "zewnętrzny"
                    ],
                    "common_phrases": [
                        "polityka wewnętrzna",
                        "sprawy wewnętrzne",
                        "rynek wewnętrzny",
                        "bezpieczeństwo wewnętrzne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Касающийся дел в пределах одного государства или организации, а не внешних отношений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутренний",
                                "target_lang_sense_clarification": "о политике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7631f1d3-4f05-3ef1-aeb8-b1c884fa76e9",
                    "sense_definition": "Związany z psychiką, uczuciami i myślami człowieka; dziejący się w umyśle, niewidoczny na zewnątrz.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "psychological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo trudnej sytuacji zachował spokój <w>wewnętrzny</w> i opanowanie.",
                            "target_lang_translations": {
                                "ru": "Несмотря на сложную ситуацию, он сохранил <w>внутреннее</w> спокойствие и самообладание."
                            }
                        },
                        {
                            "text": "Jej bogate życie <w>wewnętrzne</w> znajduje odzwierciedlenie w pisanych przez nią wierszach.",
                            "target_lang_translations": {
                                "ru": "Её богатая <w>внутренняя</w> жизнь находит отражение в стихах, которые она пишет."
                            }
                        }
                    ],
                    "synonyms": [
                        "duchowy",
                        "psychiczny"
                    ],
                    "antonyms": [
                        "zewnętrzny"
                    ],
                    "common_phrases": [
                        "spokój wewnętrzny",
                        "głos wewnętrzny",
                        "przemiana wewnętrzna",
                        "życie wewnętrzne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с психикой, чувствами и мыслями человека; происходящий в уме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутренний",
                                "target_lang_sense_clarification": "о чувствах"
                            },
                            {
                                "target_lang_word": "душевный",
                                "target_lang_sense_clarification": "о состоянии души"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "783f2d19-1e9f-38d0-838c-faa7f1f8057b",
                    "sense_definition": "Wynikający z istoty lub struktury danego układu; dotyczący relacji między elementami zamkniętej całości.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "systemic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każda firma posiada własny regulamin <w>wewnętrzny</w>, którego pracownicy muszą przestrzegać.",
                            "target_lang_translations": {
                                "ru": "У каждой компании есть свой <w>внутренний</w> распорядок, который сотрудники должны соблюдать."
                            }
                        },
                        {
                            "text": "To twierdzenie zawiera logiczną sprzeczność <w>wewnętrzną</w>, której nie da się wyjaśnić.",
                            "target_lang_translations": {
                                "ru": "Это утверждение содержит логическое <w>внутреннее</w> противоречие, которое невозможно объяснить."
                            }
                        }
                    ],
                    "antonyms": [
                        "zewnętrzny"
                    ],
                    "common_phrases": [
                        "regulamin wewnętrzny",
                        "sprzeczność wewnętrzna",
                        "struktura wewnętrzna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вытекающий из сущности или структуры данной системы; касающийся отношений внутри замкнутого целого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутренний",
                                "target_lang_sense_clarification": "о структуре"
                            }
                        ]
                    }
                }
            ]
        },
        {

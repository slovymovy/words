{
    "word_family": [
        "wide",
        "widen",
        "widely",
        "wideness"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "231e94fe-433d-3b47-910b-0f3535957a68",
                    "sense_definition": "The measurement of the extent of something from side to side.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please measure the length and <w>width</w> of the living room.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, измерь длину и <w>ширину</w> гостиной."
                            }
                        },
                        {
                            "text": "The <w>width</w> of the doorway is too narrow for the sofa.",
                            "target_lang_translations": {
                                "ru": "<w>Ширина</w> дверного проема слишком мала для дивана."
                            }
                        }
                    ],
                    "synonyms": [
                        "breadth"
                    ],
                    "antonyms": [
                        "length"
                    ],
                    "common_phrases": [
                        "length and width",
                        "in width",
                        "full width",
                        "shoulder width"
                    ],
                    "target_lang_definitions": {
                        "ru": "Измерение протяженности чего-либо от одной стороны до другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ширина",
                                "target_lang_sense_clarification": "линейный размер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64fc50e2-5de5-3409-b303-4ce4a6dd6267",
                    "sense_definition": "The state or quality of being wide.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We were surprised by the <w>width</w> of the river at this point.",
                            "target_lang_translations": {
                                "ru": "Мы были удивлены <w>шириной</w> реки в этом месте."
                            }
                        },
                        {
                            "text": "The <w>width</w> of his smile showed how happy he was to see us.",
                            "target_lang_translations": {
                                "ru": "<w>Широта</w> его улыбки показывала, как он был счастлив нас видеть."
                            }
                        }
                    ],
                    "synonyms": [
                        "wideness",
                        "broadness"
                    ],
                    "common_phrases": [
                        "great width",
                        "vary in width"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние или свойство быть широким."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ширина",
                                "target_lang_sense_clarification": "физическое свойство"
                            },
                            {
                                "target_lang_word": "широта",
                                "target_lang_sense_clarification": "абстрактное свойство (улыбки, взглядов)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5ec8308-0bf1-3858-9710-92dbf373b722",
                    "sense_definition": "The use of the full side-to-side space of a sports field during a game.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team played with great <w>width</w> to stretch the opposing defense.",
                            "target_lang_translations": {
                                "ru": "Команда играла, используя всю <w>ширину</w> поля, чтобы растянуть оборону противника."
                            }
                        },
                        {
                            "text": "Using the full <w>width</w> of the pitch creates more opportunities for the attackers.",
                            "target_lang_translations": {
                                "ru": "Использование всей <w>ширины</w> поля создает больше возможностей для нападающих."
                            }
                        }
                    ],
                    "common_phrases": [
                        "play with width",
                        "provide width"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использование всего пространства спортивного поля от края до края во время игры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ширина",
                                "target_lang_sense_clarification": "игрового поля или атаки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "badd30ac-8abd-3029-9569-138aff535247",
                    "sense_definition": "A piece of material measured along its smaller dimension, especially fabric.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "textile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She needed three <w>widths</w> of silk to make the curtains full enough.",
                            "target_lang_translations": {
                                "ru": "Ей понадобилось три <w>полотнища</w> шелка, чтобы сделать шторы достаточно пышными."
                            }
                        },
                        {
                            "text": "The tailor sewed two <w>widths</w> together to create a larger sheet.",
                            "target_lang_translations": {
                                "ru": "Портной сшил два <w>полотнища</w> вместе, чтобы создать большую простыню."
                            }
                        }
                    ],
                    "common_phrases": [
                        "single width",
                        "double width"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок материала, отмеренный по его меньшей стороне, особенно ткани."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полотнище",
                                "target_lang_sense_clarification": "отрез ткани"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbf9074f-eee8-3c87-9e40-c672a598bc56",
                    "sense_definition": "The horizontal distance between a batsman and the ball as it passes him in cricket.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cricket",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bowler gave the batsman too much <w>width</w>, allowing an easy shot.",
                            "target_lang_translations": {
                                "ru": "Боулер дал бэтсмену слишком много <w>ширины</w>, позволив совершить легкий удар."
                            }
                        },
                        {
                            "text": "He punished the extra <w>width</w> outside the off stump with a powerful drive.",
                            "target_lang_translations": {
                                "ru": "Он наказал за лишнюю <w>ширину</w> за пределами офф-стампа мощным драйвом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Горизонтальное расстояние между бэтсменом и мячом в момент его пролета в крикете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ширина",
                                "target_lang_sense_clarification": "расстояние для удара"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unconditional",
                                "target_lang_sense_clarification": "not subject to conditions"
                            },
                            {
                                "target_lang_word": "indisputable",
                                "target_lang_sense_clarification": "undeniable"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "antonyms": [
                        "недоставать"
                    ],
                    "common_phrases": [
                        "денег хватает",
                        "не хватает",
                        "вполне хватает",
                        "сил хватает",
                        "времени хватает"
                    ],
                    "target_lang_definitions": {
                        "en": "To be available in a necessary or sufficient quantity."
                    },
                    "translations": {

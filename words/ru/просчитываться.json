    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8e3a8547-729e-36b6-b623-6c7143509f84",
                    "sense_definition": "Ошибаться в расчётах, планах, ожиданиях или оценке ситуации.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mistake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В бизнесе нельзя <просчитываться>, так как это может привести к банкротству.",
                            "target_lang_translations": {
                                "en": "In business, one must not <miscalculate>, as this can lead to bankruptcy."
                            }
                        },
                        {
                            "text": "Опытный стратег редко <просчитывается> в своих прогнозах на будущее.",
                            "target_lang_translations": {
                                "en": "An experienced strategist rarely <miscalculates> in his forecasts for the future."
                            }
                        }
                    ],
                    "synonyms": [
                        "ошибаться"
                    ],
                    "common_phrases": [
                        "жестоко просчитываться",
                        "просчитываться в людях",
                        "просчитываться с выбором"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a mistake in calculations, plans, expectations, or assessment of a situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "miscalculate",
                                "target_lang_sense_clarification": "to make an error in judgment or planning"
                            },
                            {
                                "target_lang_word": "make a mistake",
                                "target_lang_sense_clarification": "general error in calculation or plan"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8180076c-dd42-3e18-991a-24a9f0942326",
                    "sense_definition": "Допускать ошибку при подсчёте количества предметов или денежных сумм.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mistake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Уставший кассир начал <просчитываться> при выдаче сдачи покупателям.",
                            "target_lang_translations": {
                                "en": "The tired cashier started to <miscount> when giving change to customers."
                            }
                        },
                        {
                            "text": "Если постоянно <просчитываться> при инвентаризации, магазин понесёт убытки.",
                            "target_lang_translations": {
                                "en": "If one constantly <miscounts> during inventory, the store will suffer losses."
                            }
                        }
                    ],
                    "synonyms": [
                        "обсчитываться"
                    ],
                    "common_phrases": [
                        "просчитываться на рубль",
                        "просчитываться в счёте"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a mistake when counting the quantity of items or sums of money."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "miscount",
                                "target_lang_sense_clarification": "to make an error in counting numbers or money"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "677cf30c-5883-35e8-a906-05dbf694c980",
                    "sense_definition": "Поддаваться вычислению или прогнозированию; быть таким, который можно заранее определить.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "passive_calc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все возможные риски должны <просчитываться> ещё на этапе планирования проекта.",
                            "target_lang_translations": {
                                "en": "All possible risks must <be calculated> as early as the project planning stage."
                            }
                        },
                        {
                            "text": "Ходы начинающего шахматиста легко <просчитываются> опытным мастером.",
                            "target_lang_translations": {
                                "en": "A novice chess player's moves are easily <predicted> by an experienced master."
                            }
                        }
                    ],
                    "synonyms": [
                        "вычисляться"
                    ],
                    "common_phrases": [
                        "легко просчитываться",
                        "просчитываться заранее",
                        "просчитываться компьютером"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be capable of being calculated or predicted; to be determinable in advance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be calculated",
                                "target_lang_sense_clarification": "passive; to be computed or estimated"
                            },
                            {
                                "target_lang_word": "be predicted",
                                "target_lang_sense_clarification": "passive; to be foreseen or determined in advance"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
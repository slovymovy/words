    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "cf828cfb-7681-3265-88f7-b138931f4bfd",
                    "sense_definition": "Не отличаясь своеобразием, оригинальностью или новизной; заурядно.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Как ни <w>банально</w> это звучит, но здоровье действительно важнее денег.",
                            "target_lang_translations": {
                                "en": "However <w>banal</w> it sounds, health is indeed more important than money."
                            }
                        },
                        {
                            "text": "Он старался мыслить творчески, а не <w>банально</w>, чтобы найти решение проблемы.",
                            "target_lang_translations": {
                                "en": "He tried to think creatively, and not <w>banally</w>, to find a solution to the problem."
                            }
                        }
                    ],
                    "synonyms": [
                        "заурядно",
                        "обыкновенно"
                    ],
                    "antonyms": [
                        "оригинально",
                        "своеобразно",
                        "необычно"
                    ],
                    "common_phrases": [
                        "звучит банально",
                        "выглядит банально",
                        "слишком банально",
                        "до боли банально"
                    ],
                    "target_lang_definitions": {
                        "en": "Not distinguished by distinctiveness, originality, or novelty; ordinary."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "banally",
                                "target_lang_sense_clarification": "In a manner lacking originality or freshness"
                            },
                            {
                                "target_lang_word": "tritely",
                                "target_lang_sense_clarification": "In a hackneyed or stale manner"
                            },
                            {
                                "target_lang_word": "unoriginally",
                                "target_lang_sense_clarification": "Without creativity or innovation"
                            }
                        ]
                    }
                }
            ]
        },
        {

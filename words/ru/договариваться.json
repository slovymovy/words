    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c1a1526a-3d2d-363b-a156-f61e44568c32",
                    "sense_definition": "Приходить к соглашению с кем-либо относительно каких-либо действий, условий или обязательств.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы заранее <договариваемся> о встрече, чтобы не опоздать.",
                            "target_lang_translations": {
                                "en": "We <arrange> the meeting in advance so as not to be late."
                            }
                        },
                        {
                            "text": "Вам нужно <договариваться> с продавцом о цене товара.",
                            "target_lang_translations": {
                                "en": "You need to <negotiate> the price of the item with the seller."
                            }
                        }
                    ],
                    "synonyms": [
                        "уславливаться",
                        "сговариваться"
                    ],
                    "common_phrases": [
                        "договариваться о встрече",
                        "договариваться о цене",
                        "договариваться с другом",
                        "договариваться заранее"
                    ],
                    "target_lang_definitions": {
                        "en": "To come to an agreement with someone regarding actions, conditions, or obligations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "agree",
                                "target_lang_sense_clarification": "to reach a consensus"
                            },
                            {
                                "target_lang_word": "arrange",
                                "target_lang_sense_clarification": "to make plans or preparations"
                            },
                            {
                                "target_lang_word": "make a deal",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "settle",
                                "target_lang_sense_clarification": "to resolve or decide on"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5eb76671-6f58-390a-a2ae-0dec0e19f45f",
                    "sense_definition": "Вести официальные переговоры (обычно используется в официальных документах и дипломатии).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Высокие <договаривающиеся> стороны подписали мирный договор.",
                            "target_lang_translations": {
                                "en": "The High <Contracting> Parties signed a peace treaty."
                            }
                        },
                        {
                            "text": "Страны начали <договариваться> о новых торговых путях.",
                            "target_lang_translations": {
                                "en": "The countries began to <negotiate> new trade routes."
                            }
                        }
                    ],
                    "synonyms": [
                        "вести переговоры"
                    ],
                    "common_phrases": [
                        "договаривающиеся стороны"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To conduct official negotiations (usually in diplomacy or formal documents)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "negotiate",
                                "target_lang_sense_clarification": "to discuss terms formally"
                            },
                            {
                                "target_lang_word": "treat",
                                "target_lang_sense_clarification": "formal/archaic; to negotiate with"
                            },
                            {
                                "target_lang_word": "contract",
                                "target_lang_sense_clarification": "as in 'contracting parties'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "858414f6-e6fc-3a4d-b4a5-ac0bc93ed589",
                    "sense_definition": "Говорить слишком долго, доходя в своих рассуждениях до нелепости, абсурда или неприятных последствий.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В пылу спора можно <договариваться> до полного абсурда.",
                            "target_lang_translations": {
                                "en": "In the heat of an argument, one can <talk oneself into> complete absurdity."
                            }
                        },
                        {
                            "text": "Не стоит <договариваться> до оскорблений, лучше успокоиться.",
                            "target_lang_translations": {
                                "en": "One shouldn't <get to the point of> insults; it's better to calm down."
                            }
                        }
                    ],
                    "synonyms": [
                        "заговариваться"
                    ],
                    "common_phrases": [
                        "договариваться до абсурда",
                        "договариваться до хрипоты"
                    ],
                    "target_lang_definitions": {
                        "en": "To speak for too long, reaching the point of absurdity or unpleasant consequences."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "talk oneself into",
                                "target_lang_sense_clarification": "to reach a state through excessive talking"
                            },
                            {
                                "target_lang_word": "go so far as to say",
                                "target_lang_sense_clarification": "to reach an extreme point in speech"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
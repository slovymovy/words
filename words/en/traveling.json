        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c1c77797-be12-36e0-84be-caf6a679bc03",
                    "sense_definition": "The activity of going from one place to another, typically over a long distance.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Traveling</w> is one of my favorite hobbies.",
                            "target_lang_translations": {
                                "ru": "<w>Путешествия</w> — одно из моих любимых хобби."
                            }
                        },
                        {
                            "text": "Her job requires a lot of <w>traveling</w> throughout the year.",
                            "target_lang_translations": {
                                "ru": "Её работа требует частых <w>разъездов</w> в течение года."
                            }
                        }
                    ],
                    "synonyms": [
                        "touring",
                        "journeying"
                    ],
                    "common_phrases": [
                        "love of traveling",
                        "safe traveling",
                        "traveling expenses"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность по перемещению из одного места в другое, обычно на большие расстояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путешествие",
                                "target_lang_sense_clarification": "процесс или факт поездки"
                            },
                            {
                                "target_lang_word": "поездки",
                                "target_lang_sense_clarification": "множественное число, часто используется для описания деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7df5b3ae-3fee-3d34-bfcd-558a32e3c9b5",
                    "sense_definition": "A violation in basketball where a player takes too many steps without dribbling the ball.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The referee blew the whistle to call a <w>traveling</w> violation.",
                            "target_lang_translations": {
                                "ru": "Судья дал свисток, чтобы зафиксировать <w>пробежку</w>."
                            }
                        },
                        {
                            "text": "He lost possession of the ball because of <w>traveling</w>.",
                            "target_lang_translations": {
                                "ru": "Он потерял мяч из-за <w>пробежки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "called for traveling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушение правил в баскетболе, когда игрок делает слишком много шагов без ведения мяча."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробежка",
                                "target_lang_sense_clarification": "термин в баскетболе"
                            }
                        ]
                    }
                }
            ]
        },
        {

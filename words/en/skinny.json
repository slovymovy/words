                },
                {
                    "sense_id": "d0c3f305-d050-3fa1-8dff-18556368bcf5",
                    "sense_definition": "Without any clothes on; nude.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teenagers went <w>skinny</w> dipping in the lake under the moonlight.",
                            "target_lang_translations": {

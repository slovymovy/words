{
    "word_family": [
        "rozpad",
        "rozpaść",
        "rozpadlina",
        "rozpadowy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6509a5d0-03ed-38aa-8975-5367bacdf510",
                    "sense_definition": "Ulegać zniszczeniu, dzieląc się na mniejsze części lub tracąc spójność (zwykle o przedmiotach materialnych).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary drewniany dom powoli <w>rozpada</w> się ze starości.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный дом медленно <w>разваливается</w> от старости."
                            }
                        },
                        {
                            "text": "Ta książka jest tak zniszczona, że zaczyna <w>rozpadać</w> się w rękach.",
                            "target_lang_translations": {
                                "ru": "Эта книга настолько потрепана, что начинает <w>рассыпаться</w> в руках."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozlatywać się",
                        "rozsypywać się",
                        "sypać się",
                        "rozwalać się",
                        "dezintegrować się"
                    ],
                    "common_phrases": [
                        "rozpadać się na kawałki",
                        "rozpadać się w pył",
                        "dom się rozpada"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушаться, разделяясь на части или теряя целостность (обычно о материальных предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разваливаться",
                                "target_lang_sense_clarification": "терять целостность, рушиться"
                            },
                            {
                                "target_lang_word": "распадаться",
                                "target_lang_sense_clarification": "делиться на части, исчезать"
                            },
                            {
                                "target_lang_word": "рассыпаться",
                                "target_lang_sense_clarification": "превращаться в мелкие части или пыль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "321b18c3-b9f9-3da1-a58c-313d1f2b00c7",
                    "sense_definition": "Przestawać istnieć jako zorganizowana grupa, związek lub całość; kończyć wspólną relację lub działalność.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ich małżeństwo <w>rozpada</w> się z powodu braku zaufania.",
                            "target_lang_translations": {
                                "ru": "Их брак <w>распадается</w> из-за отсутствия доверия."
                            }
                        },
                        {
                            "text": "Po odejściu lidera partia polityczna zaczęła się <w>rozpadać</w>.",
                            "target_lang_translations": {
                                "ru": "После ухода лидера политическая партия начала <w>распадаться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozlatywać się",
                        "dezintegrować się"
                    ],
                    "common_phrases": [
                        "małżeństwo się rozpada",
                        "związek się rozpada",
                        "grupa się rozpada"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать существовать как организованная группа, союз или целое; прекращать отношения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распадаться",
                                "target_lang_sense_clarification": "о группах, браках, союзах"
                            },
                            {
                                "target_lang_word": "разваливаться",
                                "target_lang_sense_clarification": "разговорное; о планах или организациях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ea39f7f-3d9b-3ff3-bf41-10114cf939c7",
                    "sense_definition": "Zaczynać padać intensywnie i długotrwale (zwykle o deszczu).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chmury pociemniały i wygląda na to, że deszcz <w>rozpada</w> się na dobre.",
                            "target_lang_translations": {
                                "ru": "Тучи потемнели, и похоже, что дождь <w>расходится</w> не на шутку."
                            }
                        },
                        {
                            "text": "Nie wychodźmy teraz na spacer, bo właśnie <w>rozpada</w> się ulewa.",
                            "target_lang_translations": {
                                "ru": "Давай не пойдем сейчас гулять, потому что как раз <w>разыгрался</w> ливень."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozlać się"
                    ],
                    "common_phrases": [
                        "deszcz się rozpada",
                        "rozpadać się na dobre"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать идти сильно и продолжительно (обычно о дожде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расходиться",
                                "target_lang_sense_clarification": "усиливаться (о дожде)"
                            },
                            {
                                "target_lang_word": "зарядить",
                                "target_lang_sense_clarification": "начаться надолго (о дожде)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
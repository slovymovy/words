    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "0595284e-a86a-3e0b-98b8-a929d7a0ebb6",
                    "sense_definition": "Słowo wskazujące, że ktoś lub coś zastępuje inną osobę, rzecz lub sytuację.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wolę pić herbatę <w>zamiast</w> kawy.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю пить чай <w>вместо</w> кофе."
                            }
                        },
                        {
                            "text": "Kupiliśmy nowy samochód <w>zamiast</w> starego.",
                            "target_lang_translations": {
                                "ru": "Мы купили новую машину <w>вместо</w> старой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zamiast tego",
                        "zamiast mnie",
                        "zamiast domu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлог, указывающий на то, что кто-то или что-то заменяет собой другого человека, предмет или ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вместо",
                                "target_lang_sense_clarification": "основной перевод"
                            },
                            {
                                "target_lang_word": "взамен",
                                "target_lang_sense_clarification": "в значении обмена или замены"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "8f0f2c3b-a452-318c-a411-db59e3d4076e",
                    "sense_definition": "Słowo wprowadzające czynność lub stan, które zastępują inne możliwe działanie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zacznij oszczędzać <w>zamiast</w> marnować pieniądze.",
                            "target_lang_translations": {
                                "ru": "Начни экономить, <w>вместо того чтобы</w> тратить деньги."
                            }
                        },
                        {
                            "text": "Powinieneś się uczyć <w>zamiast</w> grać w gry.",
                            "target_lang_translations": {
                                "ru": "Тебе следует учиться, <w>вместо того чтобы</w> играть в игры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zamiast narzekać",
                        "zamiast czekać",
                        "zamiast pracować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, вводящее действие или состояние, которое происходит вместо другого возможного действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вместо того, чтобы",
                                "target_lang_sense_clarification": "союз, используется перед глаголами"
                            },
                            {
                                "target_lang_word": "вместо",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                }
            ]
        }
    ]

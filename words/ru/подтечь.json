    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "12b21256-e810-352b-9efe-9d37e7751b87",
                    "sense_definition": "Слегка пропустить жидкость или стать мокрым из-за небольшого отверстия, трещины или нарушения герметичности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за старой прокладки кран в ванной немного <подтёк>.",
                            "target_lang_translations": {
                                "en": "Due to the old gasket, the faucet in the bathroom <w>leaked</w> a little."
                            }
                        },
                        {
                            "text": "Мы заметили, что труба под раковиной <подтекла> и на полу образовалась лужа.",
                            "target_lang_translations": {
                                "en": "We noticed that the pipe under the sink had <w>leaked</w> and a puddle formed on the floor."
                            }
                        }
                    ],
                    "synonyms": [
                        "протечь",
                        "просочиться"
                    ],
                    "common_phrases": [
                        "кран подтёк",
                        "труба подтекла",
                        "масло подтекло"
                    ],
                    "target_lang_definitions": {
                        "en": "To let liquid through slightly or become wet due to a small hole, crack, or loss of tightness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leak",
                                "target_lang_sense_clarification": "to accidentally allow liquid to escape or enter"
                            },
                            {
                                "target_lang_word": "seep",
                                "target_lang_sense_clarification": "to flow or leak slowly through porous material or small holes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b10baf4-5781-3a9d-a359-76c81785f0d0",
                    "sense_definition": "Переместиться в жидком состоянии под что-либо или приблизиться к чему-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "flow_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дождевая вода <подтекла> под ворота гаража во время ливня.",
                            "target_lang_translations": {
                                "en": "Rainwater <w>flowed</w> under the garage door during the downpour."
                            }
                        },
                        {
                            "text": "Лужа на тротуаре расширилась и <подтекла> к самым ногам прохожего.",
                            "target_lang_translations": {
                                "en": "The puddle on the sidewalk expanded and <w>flowed up</w> to the passerby's very feet."
                            }
                        }
                    ],
                    "synonyms": [
                        "затечь",
                        "подступить"
                    ],
                    "antonyms": [
                        "оттечь"
                    ],
                    "common_phrases": [
                        "вода подтекла под дверь",
                        "кровь подтекла"
                    ],
                    "target_lang_definitions": {
                        "en": "To move in a liquid state underneath something or approach something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flow under",
                                "target_lang_sense_clarification": "movement of liquid beneath an object"
                            },
                            {
                                "target_lang_word": "run under",
                                "target_lang_sense_clarification": "movement of liquid beneath something"
                            },
                            {
                                "target_lang_word": "flow up to",
                                "target_lang_sense_clarification": "movement of liquid approaching a specific point"
                            }
                        ]
                    }
                }
            ]
        }
    ]

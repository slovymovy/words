{
    "word_family": [
        "szacunek",
        "poszanowanie",
        "uszanować",
        "szanowny",
        "szacowny"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d06d9a2-4cf4-364f-9b12-a8f74a9b53e4",
                    "sense_definition": "Mieć szacunek do kogoś lub czegoś, darzyć kogoś lub coś szacunkiem; odnosić się do kogoś z uznaniem i grzecznością.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "respect_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <w>szanować</w> decyzje innych ludzi, nawet jeśli się z nimi nie zgadzamy.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>уважать</w> решения других людей, даже если мы с ними не согласны."
                            }
                        },
                        {
                            "text": "Uczniowie w tej szkole uczą się <w>szanować</w> tradycję i historię swojego kraju.",
                            "target_lang_translations": {
                                "ru": "Ученики в этой школе учатся <w>уважать</w> традиции и историю своей страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "poważać",
                        "liczyć się",
                        "chylić czoła",
                        "czcić"
                    ],
                    "antonyms": [
                        "lekceważyć"
                    ],
                    "common_phrases": [
                        "szanować prawo",
                        "szanować starszych",
                        "szanować czyjeś zdanie",
                        "szanować prywatność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать уважение к кому-либо или чему-либо; относиться к кому-либо с признанием и вежливостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уважать",
                                "target_lang_sense_clarification": "относиться с почтением"
                            },
                            {
                                "target_lang_word": "чтить",
                                "target_lang_sense_clarification": "традиции, память"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f6ac778-d4dc-3aa2-9198-b4e9ae29257f",
                    "sense_definition": "Chronić coś przed zniszczeniem lub zużyciem, dbać o dobry stan czegoś; traktować coś oszczędnie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "care_conserve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powinieneś bardziej <w>szanować</w> swoje zdrowie i zacząć się zdrowo odżywiać.",
                            "target_lang_translations": {
                                "ru": "Тебе следует больше <w>беречь</w> своё здоровье и начать правильно питаться."
                            }
                        },
                        {
                            "text": "Mój dziadek zawsze powtarzał, żeby <w>szanować</w> chleb i nigdy go nie wyrzucać.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка всегда повторял, что нужно <w>беречь</w> хлеб и никогда его не выбрасывать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szanować zdrowie",
                        "szanować czas",
                        "szanować pieniądze",
                        "szanować zieleń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Беречь что-либо от порчи или износа, заботиться о хорошем состоянии чего-либо; экономно расходовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беречь",
                                "target_lang_sense_clarification": "здоровье, вещи, деньги"
                            },
                            {
                                "target_lang_word": "относиться бережно",
                                "target_lang_sense_clarification": "проявлять заботу о сохранности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81f4c99a-55d9-3695-a7da-58b4c2c2aa66",
                    "sense_definition": "Mieć poczucie własnej godności, żyć w zgodzie z własnym sumieniem i nie pozwalać na złe traktowanie siebie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "self_respect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy człowiek powinien się <w>szanować</w> i nie pozwalać sobą manipulować.",
                            "target_lang_translations": {
                                "ru": "Каждый человек должен себя <w>уважать</w> и не позволять собой манипулировать."
                            }
                        },
                        {
                            "text": "Ona potrafi się <w>szanować</w>, dlatego odeszła od męża, który ją źle traktował.",
                            "target_lang_translations": {
                                "ru": "Она умеет себя <w>уважать</w>, поэтому ушла от мужа, который с ней плохо обращался."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szanować się",
                        "trzeba się szanować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь чувство собственного достоинства, жить в согласии с собственной совестью и не позволять плохо с собой обращаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уважать себя",
                                "target_lang_sense_clarification": "иметь чувство собственного достоинства"
                            },
                            {
                                "target_lang_word": "ценить себя",
                                "target_lang_sense_clarification": "знать себе цену"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
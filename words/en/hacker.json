        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d0350015-9ba7-3b43-9bf8-c3e546d36c41",
                    "sense_definition": "To speak with a spasmodic repetition of vocal sounds; to stammer or hesitate in speech.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nervous witness began to <w>hacker</w> when answering the judge's questions.",
                            "target_lang_translations": {
                                "ru": "Нервный свидетель начал <w>заикаться</w>, отвечая на вопросы судьи."
                            }
                        },
                        {
                            "text": "Do not <w>hacker</w> and mumble; please speak clearly so everyone can hear.",
                            "target_lang_translations": {
                                "ru": "Не <w>мямли</w> и не бормочи; пожалуйста, говори ясно, чтобы все могли слышать."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        },
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить прерывисто, заикаться или запинаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заикаться",
                                "target_lang_sense_clarification": "запинаться в речи"
                            },
                            {
                                "target_lang_word": "мямлить",

        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a33e550b-bbc9-390a-a67d-89575796124e",
                    "sense_definition": "Beschrijft personen of dieren die weggegaan zijn uit een onveilige situatie uit angst voor gevaar.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De voor het vuur <w>gevluchte</w> dieren moesten de rivier over zwemmen om veilig te zijn.",
                            "target_lang_translations": {
                                "ru": "Животные, <w>сбежавшие</w> от огня, должны были переплыть реку, чтобы оказаться в безопасности.",
                                "en": "The animals that had <w>fled</w> from the fire had to swim across the river to be safe."
                            }
                        },
                        {
                            "text": "De <w>gevluchte</w> regering zette haar werkzaamheden voort vanuit Londen.",
                            "target_lang_translations": {
                                "ru": "<w>Бежавшее</w> правительство продолжило свою работу из Лондона.",
                                "en": "The <w>exiled</w> government continued its work from London."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gevluchte personen",
                        "gevluchte dieren",
                        "gevluchte gezinnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывает людей или животных, покинувших опасное место из страха перед угрозой.",
                        "en": "Describes persons or animals that have left an unsafe situation out of fear of danger."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбежавший",
                                "target_lang_sense_clarification": "покинувший место опасности"
                            },
                            {
                                "target_lang_word": "бежавший",
                                "target_lang_sense_clarification": "спасшийся бегством"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fled",
                                "target_lang_sense_clarification": "having run away from danger"
                            },
                            {
                                "target_lang_word": "escaped",
                                "target_lang_sense_clarification": "having avoided capture or danger"
                            }
                        ]
                    }
                }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "40147ed7-96d6-3831-9337-9f7a1bec9193",
                    "sense_definition": "Модель объекта (здания, декорации, устройства), обычно выполненная в уменьшенном масштабе и воспроизводящая его внешний вид.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Архитектор показал заказчикам детальный <макет> будущего торгового центра.",
                            "target_lang_translations": {
                                "en": "The architect showed the clients a detailed <model> of the future shopping mall."
                            }
                        },
                        {
                            "text": "Школьники склеили из картона <макет> средневекового замка для урока истории.",
                            "target_lang_translations": {
                                "en": "The schoolchildren glued together a cardboard <model> of a medieval castle for history class."
                            }
                        }
                    ],
                    "synonyms": [
                        "модель"
                    ],
                    "common_phrases": [
                        "макет здания",
                        "архитектурный макет",
                        "сделать макет",
                        "макет в натуральную величину"
                    ],
                    "target_lang_definitions": {
                        "en": "A physical representation of an object (such as a building or device), usually made on a smaller scale to show its appearance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "model",
                                "target_lang_sense_clarification": "physical representation"
                            },
                            {
                                "target_lang_word": "scale model",
                                "target_lang_sense_clarification": "small-scale copy"
                            },
                            {
                                "target_lang_word": "mock-up",
                                "target_lang_sense_clarification": "prototype or sample"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf440921-95cc-3dba-9554-b20f42f20cdd",
                    "sense_definition": "Предварительный образец вёрстки книги, журнала, газеты или веб-сайта, показывающий расположение текста и изображений.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дизайнер отправил <макет> главной страницы сайта на утверждение клиенту.",
                            "target_lang_translations": {
                                "en": "The designer sent a <mockup> of the website's homepage to the client for approval."
                            }
                        },
                        {
                            "text": "Редактор внимательно вычитывал <макет> нового номера журнала перед печатью.",
                            "target_lang_translations": {
                                "en": "The editor carefully proofread the <layout> of the new magazine issue before printing."
                            }
                        }
                    ],
                    "common_phrases": [
                        "макет книги",
                        "утвердить макет",
                        "печатный макет",
                        "дизайнерский макет"
                    ],
                    "target_lang_definitions": {
                        "en": "A preliminary arrangement of text and images for a book, magazine, or website."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "layout",
                                "target_lang_sense_clarification": "arrangement of page elements"
                            },
                            {
                                "target_lang_word": "mockup",
                                "target_lang_sense_clarification": "visual design draft"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
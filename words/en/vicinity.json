{
    "word_family": [
        "vicinal"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9f9ade2d-c49d-3565-a38c-a65be35ff61b",
                    "sense_definition": "The area near or surrounding a particular place.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "location_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are several excellent restaurants in the <w>vicinity</w> of the hotel.",
                            "target_lang_translations": {
                                "ru": "В <w>окрестностях</w> отеля есть несколько отличных ресторанов."
                            }
                        },
                        {
                            "text": "Police searched the immediate <w>vicinity</w> for any sign of the missing keys.",
                            "target_lang_translations": {
                                "ru": "Полиция обыскала ближайшие <w>окрестности</w> на предмет пропавших ключей."
                            }
                        }
                    ],
                    "synonyms": [
                        "neighborhood",
                        "locality",
                        "area",
                        "surroundings"
                    ],
                    "common_phrases": [
                        "in the vicinity",
                        "immediate vicinity",
                        "surrounding vicinity",
                        "general vicinity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория, прилегающая к определенному месту или окружающая его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окрестность",
                                "target_lang_sense_clarification": "окружающая местность"
                            },
                            {
                                "target_lang_word": "округа",
                                "target_lang_sense_clarification": "окружающая местность, разг."
                            },
                            {
                                "target_lang_word": "соседство",
                                "target_lang_sense_clarification": "смежная территория"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0311ced7-9304-31c1-8fb6-a8bea4277602",
                    "sense_definition": "An approximate amount, number, or size.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The repairs to the old car will cost in the <w>vicinity</w> of five hundred dollars.",
                            "target_lang_translations": {
                                "ru": "Ремонт старой машины обойдется в <w>районе</w> пятисот долларов."
                            }
                        },
                        {
                            "text": "He weighs somewhere in the <w>vicinity</w> of eighty kilograms.",
                            "target_lang_translations": {
                                "ru": "Он весит где-то в <w>районе</w> восьмидесяти килограммов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in the vicinity of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приблизительное количество, число или размер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "район",
                                "target_lang_sense_clarification": "в значении «приблизительно» (в районе)"
                            },
                            {
                                "target_lang_word": "пределы",
                                "target_lang_sense_clarification": "диапазон значений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39bdbd91-ce82-373f-8311-f6ce57cdc476",
                    "sense_definition": "The state of being near or close to something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "location_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>vicinity</w> of the forest brings fresh air to the small village.",
                            "target_lang_translations": {
                                "ru": "<w>Близость</w> леса приносит свежий воздух в маленькую деревню."
                            }
                        },
                        {
                            "text": "She felt nervous due to the <w>vicinity</w> of the large dog.",
                            "target_lang_translations": {
                                "ru": "Она нервничала из-за <w>близости</w> большой собаки."
                            }
                        }
                    ],
                    "synonyms": [
                        "proximity",
                        "nearness",
                        "closeness"
                    ],
                    "antonyms": [
                        "remoteness",
                        "distance"
                    ],
                    "common_phrases": [
                        "close vicinity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние нахождения рядом или близко к чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "близость",
                                "target_lang_sense_clarification": "состояние нахождения рядом"
                            },
                            {
                                "target_lang_word": "соседство",
                                "target_lang_sense_clarification": "нахождение бок о бок"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
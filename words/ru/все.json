                    "sense_id": "be36d67b-faae-3d1b-99c8-7aa7887297e6",
                    "sense_definition": "Каждый человек; все люди.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "everyone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тепе́рь <w>все</w> зна́ют её секре́т.",
                            "target_lang_translations": {
                                "en": "Now <w>everyone</w> knows her secret."
                            }
                        },
                        {
                            "text": "На пра́здник пришли́ <w>все</w>.",
                            "target_lang_translations": {
                                "en": "<w>Everyone</w> came to the party."
                            }
                        }
                    ],
                    "antonyms": [
                        "никто"
                    ],
                    "common_phrases": [
                        "все знают",
                        "все вместе",
                        "все говорят"

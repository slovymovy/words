                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_meet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We should <w>link</w> up later this week to catch up.",
                            "target_lang_translations": {
                                "ru": "Нам стоит <w>пересечься</w> на этой неделе, чтобы поболтать."
                            }
                        },
                        {
                            "text": "He is planning to <w>link</w> with his friends at the park.",
                            "target_lang_translations": {
                                "ru": "Он планирует <w>встретиться</w> с друзьями в парке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "link up"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in MLE and urban dialects"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Встречаться или общаться с кем-то в неформальной обстановке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встретиться",
                                "target_lang_sense_clarification": "увидеться"
                            },
                            {
                                "target_lang_word": "пересечься",
                                "target_lang_sense_clarification": "сленг: встретиться"
                            }
                        ]
                    }
                }
            ]
        },

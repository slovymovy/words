{
    "word_family": [
        "zdrada",
        "zdrajca",
        "zdrajczyni",
        "zdradliwy",
        "zdradziecki"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "676c0d80-27eb-372c-988e-98caba683d68",
                    "sense_definition": "Ujawniać informację, która miała pozostać tajemnicą lub sekretem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reveal_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obiecałeś nie <zdradzać> nikomu mojego sekretu.",
                            "target_lang_translations": {
                                "ru": "Ты обещал никому не <выдавать> мой секрет."
                            }
                        },
                        {
                            "text": "Nie powinieneś <zdradzać> szczegółów naszej rozmowy osobom trzecim.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <раскрывать> подробности нашего разговора третьим лицам."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyjawiać",
                        "wygadywać",
                        "sypać",
                        "paplać"
                    ],
                    "antonyms": [
                        "utajniać"
                    ],
                    "common_phrases": [
                        "zdradzać tajemnicę",
                        "zdradzać sekret",
                        "nie zdradzaj nikomu",
                        "zdradzać plany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскрывать информацию, которая должна была оставаться в тайне или секрете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдавать",
                                "target_lang_sense_clarification": "секрет, тайну"
                            },
                            {
                                "target_lang_word": "раскрывать",
                                "target_lang_sense_clarification": "подробности, планы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bf1e2f7-fcd0-3e47-a933-50e33293b706",
                    "sense_definition": "Być niewiernym partnerowi lub małżonkowi; nawiązywać relację intymną z inną osobą będąc w związku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "disloyalty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mąż zaczął ją <zdradzać> po dziesięciu latach małżeństwa.",
                            "target_lang_translations": {
                                "ru": "Муж начал ей <изменять> спустя десять лет брака."
                            }
                        },
                        {
                            "text": "To bolesne, gdy ktoś bliski decyduje się <zdradzać> zaufanie partnera.",
                            "target_lang_translations": {
                                "ru": "Больно, когда кто-то близкий решает <предать> доверие партнера."
                            }
                        }
                    ],
                    "synonyms": [
                        "być niewiernym",
                        "przyprawiać rogi"
                    ],
                    "antonyms": [
                        "być wiernym"
                    ],
                    "common_phrases": [
                        "zdradzać męża",
                        "zdradzać żonę",
                        "zdradzać chłopaka",
                        "zdradzać dziewczynę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть неверным партнеру или супругу; вступать в интимную связь с другим человеком, находясь в отношениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изменять",
                                "target_lang_sense_clarification": "мужу, жене, партнеру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a582cdf-a6b8-3d03-a025-975690350c30",
                    "sense_definition": "Łamać lojalność wobec kogoś lub czegoś (np. ojczyzny, przyjaciela, ideałów); przechodzić na stronę wroga.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "disloyalty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prawdziwy patriota nigdy nie będzie <zdradzać> swojego kraju.",
                            "target_lang_translations": {
                                "ru": "Настоящий патриот никогда не станет <предавать> свою страну."
                            }
                        },
                        {
                            "text": "Nie wolno <zdradzać> przyjaciół dla własnych korzyści majątkowych.",
                            "target_lang_translations": {
                                "ru": "Нельзя <предавать> друзей ради собственной материальной выгоды."
                            }
                        }
                    ],
                    "antonyms": [
                        "być wiernym"
                    ],
                    "common_phrases": [
                        "zdradzać ojczyznę",
                        "zdradzać ideały",
                        "zdradzać przyjaciela",
                        "zdradzać sprawę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушать верность кому-либо или чему-либо (например, родине, другу, идеалам); переходить на сторону врага."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предавать",
                                "target_lang_sense_clarification": "друга, родину"
                            },
                            {
                                "target_lang_word": "изменять",
                                "target_lang_sense_clarification": "идеалам, принципам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86739b98-783a-3abe-b19e-69e3c899a12d",
                    "sense_definition": "Wykazywać pewne cechy, emocje lub oznaki czegoś, często w sposób niezamierzony.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "reveal_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego drżący głos zaczął <zdradzać> ogromne zdenerwowanie.",
                            "target_lang_translations": {
                                "ru": "Его дрожащий голос начал <выдавать> сильное волнение."
                            }
                        },
                        {
                            "text": "Mimo podeszłego wieku wciąż <zdradza> wielką energię do życia.",
                            "target_lang_translations": {
                                "ru": "Несмотря на преклонный возраст, он всё ещё <проявляет> огромную жизненную энергию."
                            }
                        }
                    ],
                    "synonyms": [
                        "objawiać"
                    ],
                    "common_phrases": [
                        "zdradzać objawy",
                        "zdradzać zainteresowanie",
                        "zdradzać emocje",
                        "zdradzać oznaki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявлять какие-либо признаки, эмоции или свойства, часто непреднамеренно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдавать",
                                "target_lang_sense_clarification": "чувства, волнение"
                            },
                            {
                                "target_lang_word": "проявлять",
                                "target_lang_sense_clarification": "интерес, признаки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "20dd0069-cb42-3771-8ce1-a5bab0ab8935",
                    "sense_definition": "Mający cechy wspólne z kimś lub czymś innym; wyglądający lub zachowujący się tak samo lub prawie tak samo.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój syn jest bardzo <w>podobny</w> do ciebie.",
                            "target_lang_translations": {
                                "ru": "Твой сын очень <w>похож</w> на тебя."
                            }
                        },
                        {
                            "text": "Szukam butów w <w>podobnym</w> stylu do tych, które widziałam wczoraj.",
                            "target_lang_translations": {
                                "ru": "Я ищу туфли в <w>подобном</w> стиле, как те, что я видела вчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbliżony",
                        "analogiczny"
                    ],
                    "antonyms": [
                        "różny",
                        "niepodobny",
                        "przystający",
                        "niepodobny"
                    ],
                    "common_phrases": [
                        "podobny do kogoś",
                        "podobny do czegoś",
                        "łudząco podobny",
                        "bardzo podobny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий общие черты с кем-то или чем-то другим; выглядящий или ведущий себя так же или почти так же."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "похожий",
                                "target_lang_sense_clarification": "имеющий сходство во внешности или свойствах"
                            },
                            {
                                "target_lang_word": "подобный",
                                "target_lang_sense_clarification": "аналогичный, такой же"
                            },
                            {
                                "target_lang_word": "схожий",
                                "target_lang_sense_clarification": "обладающий общими признаками"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a6ae4ca-bd30-3a99-a259-cf2baf0b8ff4",
                    "sense_definition": "O figurach geometrycznych: mający ten sam kształt i równe kąty, lecz różniący się wielkością.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "geometria",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel matematyki wytłumaczył nam, kiedy trójkąty są <w>podobne</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель математики объяснил нам, когда треугольники <w>подобны</w>."
                            }
                        },
                        {
                            "text": "Te dwie figury są <w>podobne</w>, ponieważ zachowują te same proporcje boków.",
                            "target_lang_translations": {
                                "ru": "Эти две фигуры <w>подобны</w>, так как сохраняют те же пропорции сторон."
                            }
                        }
                    ],
                    "antonyms": [
                        "niepodobny"
                    ],
                    "common_phrases": [
                        "trójkąty podobne",
                        "figury podobne"
                    ],
                    "target_lang_definitions": {
                        "ru": "О геометрических фигурах: имеющий ту же форму и равные углы, но отличающийся размером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подобный",
                                "target_lang_sense_clarification": "геометрический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8dbd7bd-8858-3aa3-b267-283c91734cde",
                    "sense_definition": "Odznaczający się urodą; ładny, przystojny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Był to młodzieniec wielce <w>podobny</w> i postawny.",
                            "target_lang_translations": {
                                "ru": "Это был юноша весьма <w>видный</w> и статный."
                            }
                        },
                        {
                            "text": "W dawnych czasach mówiono, że panna jest <w>podobna</w>, gdy była urodziwa.",
                            "target_lang_translations": {
                                "ru": "В прежние времена говорили, что девушка <w>пригожая</w>, если она была красива."
                            }
                        }
                    ],
                    "synonyms": [
                        "ładny",
                        "przystojny",
                        "urodziwy"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane dawniej w znaczeniu 'ładny, przystojny'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличающийся красотой; красивый, статный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "видный",
                                "target_lang_sense_clarification": "статный, представительный"
                            },
                            {
                                "target_lang_word": "пригожий",
                                "target_lang_sense_clarification": "устар. красивый, миловидный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

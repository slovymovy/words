{
    "word_family": [
        "hymniczny"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "94c22a0b-a405-36cd-951d-b9b5fd984914",
                    "sense_definition": "Uroczysta pieśń patriotyczna będąca oficjalnym symbolem państwa, wykonywana podczas ważnych uroczystości państwowych i świąt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anthem",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed rozpoczęciem meczu wszyscy wstali, aby odśpiewać <w>hymn</w> narodowy.",
                            "target_lang_translations": {
                                "ru": "Перед началом матча все встали, чтобы спеть национальный <w>гимн</w>."
                            }
                        },
                        {
                            "text": "Polski <w>hymn</w> jest znany jako Mazurek Dąbrowskiego.",
                            "target_lang_translations": {
                                "ru": "Польский <w>гимн</w> известен как Мазурка Домбровского."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hymn narodowy",
                        "hymn państwowy",
                        "śpiewać hymn",
                        "słuchać hymnu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественная патриотическая песня, являющаяся официальным символом государства, исполняемая во время важных государственных торжеств и праздников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гимн",
                                "target_lang_sense_clarification": "государственный символ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c6f1eac-19ac-3591-9194-be29db34a435",
                    "sense_definition": "Uroczysta i podniosła pieśń pochwalna o charakterze religijnym lub świeckim, komponowana na cześć bóstwa, wybitnej osoby, idei lub wydarzenia.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "praise_song",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chór kościelny wykonał piękny <w>hymn</w> ku czci stwórcy.",
                            "target_lang_translations": {
                                "ru": "Церковный хор исполнил прекрасный <w>гимн</w> в честь творца."
                            }
                        },
                        {
                            "text": "Ten wiersz to swoisty <w>hymn</w> na cześć młodości i wolności.",
                            "target_lang_translations": {
                                "ru": "Это стихотворение — своеобразный <w>гимн</w> в честь молодости и свободы."
                            }
                        }
                    ],
                    "synonyms": [
                        "pieśń pochwalna"
                    ],
                    "antonyms": [
                        "tren"
                    ],
                    "common_phrases": [
                        "hymn pochwalny",
                        "hymn o miłości",
                        "hymn liturgiczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественная и возвышенная хвалебная песня религиозного или светского характера, созданная в честь божества, выдающейся личности, идеи или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гимн",
                                "target_lang_sense_clarification": "хвалебная песня"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
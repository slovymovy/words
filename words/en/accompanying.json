    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "12594230-6a97-365f-9f6d-98129cb3a26b",
                    "sense_definition": "Present participle and gerund of <w>accompany</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>accompanying</w> her grandmother to the doctor today.",
                            "target_lang_translations": {
                                "ru": "Она сегодня <w>сопровождает</w> свою бабушку к врачу."
                            }
                        },
                        {
                            "text": "The pianist is <w>accompanying</w> the singer during the performance.",
                            "target_lang_translations": {
                                "ru": "Пианист <w>аккомпанирует</w> певцу во время выступления."
                            }
                        }
                    ],
                    "common_phrases": [
                        "accompanying him",
                        "accompanying her",
                        "accompanying them"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола accompany."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопровождающий",
                                "target_lang_sense_clarification": "причастие; тот, кто идет вместе с кем-то"
                            },
                            {
                                "target_lang_word": "аккомпанирующий",
                                "target_lang_sense_clarification": "в музыке; играющий на инструменте для певца"
                            },
                            {
                                "target_lang_word": "сопровождая",
                                "target_lang_sense_clarification": "деепричастие; выполняя действие сопровождения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "251f2658-04fa-320a-a06a-ef5f95e708ef",
                    "sense_definition": "Appearing or occurring together with something else.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please read the <w>accompanying</w> documents carefully before signing.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, внимательно прочитайте <w>сопроводительные</w> документы перед подписанием."
                            }
                        },
                        {
                            "text": "The <w>accompanying</w> photograph shows the house as it looked in the 1920s.",
                            "target_lang_translations": {
                                "ru": "<w>Прилагаемая</w> фотография показывает дом таким, каким он выглядел в 1920-х годах."
                            }
                        }
                    ],
                    "synonyms": [
                        "associated",
                        "attendant",
                        "related"
                    ],
                    "antonyms": [
                        "separate",
                        "unrelated"
                    ],
                    "common_phrases": [
                        "accompanying letter",
                        "accompanying text",
                        "accompanying notes",
                        "accompanying material"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появляющийся или происходящий вместе с чем-то другим; прилагаемый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопроводительный",
                                "target_lang_sense_clarification": "о документах, письмах или текстах"
                            },
                            {
                                "target_lang_word": "сопутствующий",
                                "target_lang_sense_clarification": "об обстоятельствах, явлениях или симптомах"
                            },
                            {
                                "target_lang_word": "прилагаемый",
                                "target_lang_sense_clarification": "о том, что приложено к чему-то (напр. фото)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b4457963-ba98-31ab-b832-b27d9c6dbf7c",
                    "sense_definition": "Something that accompanies or goes along with something else.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The festival and its various <w>accompanyings</w> lasted for three full days.",
                            "target_lang_translations": {
                                "ru": "Фестиваль и различные <w>сопутствующие мероприятия</w> длились целых три дня."
                            }
                        },
                        {
                            "text": "He enjoyed the rich meal along with all its delicious <w>accompanyings</w>.",
                            "target_lang_translations": {
                                "ru": "Он наслаждался обильной трапезой вместе со всеми её вкусными <w>дополнениями</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "accompaniment"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что сопровождает или идет вместе с чем-то другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопровождение",
                                "target_lang_sense_clarification": "процесс или группа сопровождающих"
                            },
                            {
                                "target_lang_word": "дополнение",
                                "target_lang_sense_clarification": "то, что подается или идет вместе с основным"
                            }
                        ]
                    }
                }
            ]
        }
    ]

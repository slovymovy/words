                {
                    "sense_id": "be1b55a1-b8e4-396f-969c-12c279dfcd34",
                    "sense_definition": "Wykonywać czynności bardzo szybko lub przemieszczać się w szybkim tempie, zazwyczaj z powodu braku czasu (najczęściej używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę się <w>spieszyć</w>, bo za chwilę odjeżdża mój pociąg.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>торопиться</w>, потому что мой поезд скоро уходит."
                            }
                        },
                        {
                            "text": "Nie warto się <w>spieszyć</w> przy podejmowaniu tak ważnych decyzji.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>спешить</w> при принятии столь важных решений."
                            }
                        }
                    ],
                    "synonyms": [
                        "gnać",
                        "pędzić",
                        "biec"
                    ],
                    "antonyms": [
                        "zwlekać",
                        "ociągać się",
                        "guzdrać się"
                    ],
                    "common_phrases": [

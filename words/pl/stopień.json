                },
                {
                    "sense_id": "e6805edb-14f3-3a3e-9226-0d4836ffa4b8",
                    "sense_definition": "Etap rozwoju lub część jakiegoś złożonego procesu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "process_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest dopiero pierwszy <w>stopień</w> wtajemniczenia w naszą działalność.",
                            "target_lang_translations": {
                                "ru": "Это только первая <w>ступень</w> посвящения в нашу деятельность."
                            }
                        },
                        {
                            "text": "Rakieta nośna odrzuciła zużyty <w>stopień</w> i poleciała dalej w kosmos.",
                            "target_lang_translations": {
                                "ru": "Ракета-носитель отбросила отработанную <w>ступень</w> и полетела дальше в космос."
                            }
                        }
                    ],
                    "synonyms": [
                        "etap",
                        "szczebel"
                    ],
                    "common_phrases": [

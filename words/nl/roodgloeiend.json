                {
                    "sense_id": "d01a911c-7b42-3002-856a-3bf48b02d3c7",
                    "sense_definition": "Zo heet dat het materiaal rood licht uitstraalt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_heat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De smid bewerkte het <w>roodgloeiend</w> stuk metaal op het aambeeld.",
                            "target_lang_translations": {
                                "en": "The blacksmith worked the <w>red-hot</w> piece of metal on the anvil.",
                                "ru": "Кузнец обрабатывал <w>раскалённый докрасна</w> кусок металла на наковальне."
                            }
                        },
                        {
                            "text": "Door de wrijving werden de remmen van de raceauto <w>roodgloeiend</w>.",
                            "target_lang_translations": {
                                "en": "Due to the friction, the race car's brakes became <w>red-hot</w>.",
                                "ru": "Из-за трения тормоза гоночного автомобиля <w>раскалились докрасна</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gloeiend heet"
                    ],
                    "antonyms": [
                        "ijskoud"
                    ],
                    "common_phrases": [
                        "roodgloeiend maken",
                        "roodgloeiend worden"
                    ],
                    "target_lang_definitions": {
                        "en": "Heated to such a high temperature that it glows red.",
                        "ru": "Настолько горячий, что материал излучает красный свет."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "red-hot",
                                "target_lang_sense_clarification": "literal heat"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раскалённый докрасна",
                                "target_lang_sense_clarification": "о металле или других материалах"
                            },
                            {
                                "target_lang_word": "раскалённый",
                                "target_lang_sense_clarification": "очень горячий, светящийся от жара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2efd6dbf-c7e6-3002-a4e8-0b84edbd126a",
                    "sense_definition": "Rood aangelopen in het gezicht door warmte of sterke emoties.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij stond <w>roodgloeiend</w> van woede tegenover zijn tegenstander.",
                            "target_lang_translations": {
                                "en": "He stood facing his opponent, <w>flushed</w> with anger.",
                                "ru": "Он стоял напротив своего противника, <w>багровый</w> от ярости."
                            }
                        },
                        {
                            "text": "Met <w>roodgloeiende</w> wangen kwam ze binnen na het spelen in de sneeuw.",
                            "target_lang_translations": {
                                "en": "She came inside with <w>glowing</w> cheeks after playing in the snow.",
                                "ru": "С <w>пылающими</w> щеками она вошла внутрь после игры в снегу."
                            }
                        }
                    ],
                    "synonyms": [
                        "rood aangelopen"
                    ],
                    "antonyms": [
                        "lijkbleek"
                    ],
                    "common_phrases": [
                        "roodgloeiend van woede",
                        "roodgloeiend van schaamte"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a red face caused by heat, exertion, or strong emotion.",
                        "ru": "С покрасневшим лицом от жары или сильных эмоций."
                    },
                    "translations": {

                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient reported a <w>subjective</w> improvement in their mood, though blood tests showed no change."
                        },
                        {
                            "text": "Pain is a <w>subjective</w> experience that is difficult for doctors to measure precisely."
                        }
                    ],
                    "synonyms": [
                        "internal",
                        "mental"
                    ],
                    "antonyms": [
                        "objective"
                    ],
                    "common_phrases": [

    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3253571b-d9d2-383f-adb7-b9b7fd94357a",
                    "sense_definition": "Średni ptak wędrowny o upierzeniu w kolorach pomarańczowym, czarnym i białym, z charakterystycznym, rozkładanym czubem na głowie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "birds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas spaceru po lesie zauważyliśmy kolorowego <w>dudka</w> siedzącego na gałęzi.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки по лесу мы заметили пёстрого <w>удода</w>, сидящего на ветке."
                            }
                        },
                        {
                            "text": "Charakterystyczny czub sprawia, że <w>dudek</w> jest łatwy do rozpoznania.",
                            "target_lang_translations": {
                                "ru": "Благодаря характерному хохолку <w>удода</w> легко узнать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czubaty dudek",
                        "gniazdo dudka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Средняя перелётная птица с оранжево-чёрно-белым оперением и характерным раскрывающимся хохолком на голове."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удод",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "814fe5b1-3c02-3602-bbbd-3dd64dd29bb9",
                    "sense_definition": "Osoba naiwna, niemądra, którą łatwo oszukać; często używane w kontekście bycia wprowadzonym w błąd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "insults",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie pozwól, aby sprzedawca wystrychnął cię na <w>dudka</w> i wcisnął ci ten stary samochód.",
                            "target_lang_translations": {
                                "ru": "Не дай продавцу оставить тебя в <w>дураках</w> и всучить тебе эту старую машину."
                            }
                        },
                        {
                            "text": "Poczuł się jak <w>dudek</w>, gdy zrozumiał, że wszyscy z niego żartowali.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал себя <w>дураком</w>, когда понял, что все над ним подшучивали."
                            }
                        }
                    ],
                    "synonyms": [
                        "głupiec",
                        "głupek",
                        "dureń",
                        "naiwniak"
                    ],
                    "common_phrases": [
                        "wystrychnąć na dudka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used mostly in idioms"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наивный, неумный человек, которого легко обмануть; часто используется в контексте введения в заблуждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простак",
                                "target_lang_sense_clarification": "наивный человек"
                            },
                            {
                                "target_lang_word": "дурак",
                                "target_lang_sense_clarification": "глупый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf9f75ef-a5b3-3457-afe2-4f5d2c0069a7",
                    "sense_definition": "Młody pies; szczenię (użycie regionalne lub rzadkie).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mały <w>dudek</w> biegał wesoło po podwórku, merdając ogonem.",
                            "target_lang_translations": {
                                "ru": "Маленький <w>щенок</w> весело бегал по двору, виляя хвостом."
                            }
                        },
                        {
                            "text": "Suka pilnowała swojego <w>dudka</w>, nie pozwalając nikomu podejść.",
                            "target_lang_translations": {
                                "ru": "Собака охраняла своего <w>щенка</w>, не позволяя никому подойти."
                            }
                        }
                    ],
                    "synonyms": [
                        "szczenię"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "dialectal usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодая собака; щенок (региональное или редкое употребление)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щенок",
                                "target_lang_sense_clarification": "детёныш собаки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e149b428-c6d7-3563-a94f-957600953e76",
                    "sense_definition": "Popularne nazwisko polskie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Dudek</w> jest naszym nowym sąsiadem z naprzeciwka.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Дудек</w> — наш новый сосед из дома напротив."
                            }
                        },
                        {
                            "text": "Jerzy <w>Dudek</w> był znanym polskim bramkarzem.",
                            "target_lang_translations": {
                                "ru": "Ежи <w>Дудек</w> был известным польским вратарём."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Популярная польская фамилия."
                    },
                    "translations": {

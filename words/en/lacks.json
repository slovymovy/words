    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4edb2a21-3ec1-3057-8102-9b8d061b8f01",
                    "sense_definition": "Third-person singular simple present indicative of <w>lack</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>lacks</w> the necessary experience to lead the team effectively.",
                            "target_lang_translations": {
                                "ru": "Ему <w>не хватает</w> необходимого опыта, чтобы эффективно руководить командой."
                            }
                        },
                        {
                            "text": "The soup is quite bland because it <w>lacks</w> salt and pepper.",
                            "target_lang_translations": {
                                "ru": "Суп довольно пресный, потому что в нем <w>не хватает</w> соли и перца."
                            }
                        }
                    ],
                    "synonyms": [
                        "misses",
                        "needs",
                        "requires"
                    ],
                    "antonyms": [
                        "has",
                        "possesses",
                        "owns"
                    ],
                    "common_phrases": [
                        "lacks confidence",
                        "lacks experience",
                        "lacks nothing"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb-form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Третье лицо единственного числа настоящего времени глагола lack (испытывать недостаток, не иметь)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не хватает",
                                "target_lang_sense_clarification": "безличная форма, используется с дательным падежом (кому/чему?)"
                            },
                            {
                                "target_lang_word": "недостает",
                                "target_lang_sense_clarification": "синоним 'не хватает', часто используется в более формальном контексте"
                            },
                            {
                                "target_lang_word": "лишен",
                                "target_lang_sense_clarification": "краткое прилагательное, означает отсутствие какого-либо качества или свойства"
                            }
                        ]
                    }
                }
            ]
        },
        {

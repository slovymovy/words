                                "target_lang_sense_clarification": "brief visit"
                            },
                            {
                                "target_lang_word": "come by",
                                "target_lang_sense_clarification": "visit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "зайти",
                                "target_lang_sense_clarification": "посетить, навестить"
                            },
                            {
                                "target_lang_word": "заглянуть",
                                "target_lang_sense_clarification": "зайти ненадолго"
                            },
                            {

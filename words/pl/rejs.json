                    "sense_id": "d5419558-72ab-30e6-87bd-5b436745b2a8",
                    "sense_definition": "Podróż statkiem wodnym na określonej trasie, od portu początkowego do końcowego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <rejs> po Bałtyku był dla nas niezapomnianą przygodą.",
                            "target_lang_translations": {
                                "ru": "Этот <w>круиз</w> по Балтике был для нас незабываемым приключением."
                            }
                        },
                        {
                            "text": "Wybieramy się na długi <rejs> wycieczkowy luksusowym statkiem.",
                            "target_lang_translations": {
                                "ru": "Мы собираемся в долгий туристический <w>круиз</w> на роскошном судне."
                            }
                        }
                    ],
                    "synonyms": [
                        "podróż morska",
                        "żegluga"
                    ],
                    "common_phrases": [
                        "rejs statkiem",
                        "rejs wycieczkowy",
                        "rejs dookoła świata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествие на водном судне по определенному маршруту, от начального порта до конечного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рейс",
                                "target_lang_sense_clarification": "поездка по маршруту"
                            },
                            {
                                "target_lang_word": "круиз",
                                "target_lang_sense_clarification": "туристическое путешествие"
                            },
                            {
                                "target_lang_word": "плавание",
                                "target_lang_sense_clarification": "морское путешествие"
                            }
                        ]

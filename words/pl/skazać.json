    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "61d6a7b4-5583-3b55-aff2-f3424c19dc1d",
                    "sense_definition": "Wydać wyrok sądowy, uznając kogoś za winnego przestępstwa i wymierzając mu karę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sądownictwo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd postanowił <skazać> oskarżonego na pięć lat pozbawienia wolności.",
                            "target_lang_translations": {
                                "ru": "Суд постановил <приговорить> обвиняемого к пяти годам лишения свободы."
                            }
                        },
                        {
                            "text": "Sędzia musiał <skazać> sprawcę wypadku na wysoką grzywnę.",
                            "target_lang_translations": {
                                "ru": "Судья был вынужден <приговорить> виновника аварии к крупному штрафу."
                            }
                        }
                    ],
                    "synonyms": [
                        "osądzić",
                        "zasądzić"
                    ],
                    "antonyms": [
                        "uniewinnić",
                        "ułaskawić"
                    ],
                    "common_phrases": [
                        "skazać na więzienie",
                        "skazać na śmierć",
                        "skazać prawomocnym wyrokiem",
                        "skazać na grzywnę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вынести судебный приговор, признав кого-либо виновным и назначив наказание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приговорить",
                                "target_lang_sense_clarification": "вынести приговор"
                            },
                            {
                                "target_lang_word": "осудить",
                                "target_lang_sense_clarification": "признать виновным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "470cf1e6-b334-3462-bee8-32f8ace610cd",
                    "sense_definition": "Przesądzić o czyimś losie, sprawiając, że ktoś lub coś znajdzie się w trudnej, nieuchronnej sytuacji bez wyjścia.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "los",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Brak odpowiednich funduszy może <skazać> ten projekt na całkowitą porażkę.",
                            "target_lang_translations": {
                                "ru": "Отсутствие соответствующих средств может <обречь> этот проект на полный провал."
                            }
                        },
                        {
                            "text": "Twoja bierność może <skazać> naszą firmę na bankructwo.",
                            "target_lang_translations": {
                                "ru": "Твоя пассивность может <обречь> нашу фирму на банкротство."
                            }
                        }
                    ],
                    "synonyms": [
                        "przesądzić",
                        "przekreślić"
                    ],
                    "antonyms": [
                        "ocalić",
                        "uratować"
                    ],
                    "common_phrases": [
                        "skazać na porażkę",
                        "skazać na zapomnienie",
                        "skazać na zagładę",
                        "skazać na niepowodzenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предопределить чью-либо судьбу, поставив в безвыходное или трудное положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обречь",
                                "target_lang_sense_clarification": "на неудачу или страдания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "258d03a7-fe65-3e85-9746-a244b8ba0ae3",
                    "sense_definition": "Podjąć decyzję lub działanie, które sprowadza na samego siebie przykre konsekwencje lub trudny los.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "los",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wybierając życie z dala od ludzi, mężczyzna postanowił <skazać> się na samotność.",
                            "target_lang_translations": {
                                "ru": "Выбирая жизнь вдали от людей, мужчина решил <обречь> себя на одиночество."
                            }
                        },
                        {
                            "text": "Nie chcąc iść na żaden kompromis, artysta wolał <skazać> się na biedę.",
                            "target_lang_translations": {
                                "ru": "Не желая идти ни на какой компромисс, художник предпочел <обречь> себя на нищету."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skazać się na samotność",
                        "skazać się na banicję",
                        "skazać się na cierpienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять решение или совершить действие, которое влечет за собой тяжелые последствия для самого себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обречь",
                                "target_lang_sense_clarification": "себя на что-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]

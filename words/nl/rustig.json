    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bc8879ac-d5a1-3ae7-a2ab-97227a1c110d",
                    "sense_definition": "Zonder veel geluid, beweging of drukte; stil en vredig.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "calm_environment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is heerlijk <w>rustig</w> in het bos.",
                            "target_lang_translations": {
                                "en": "It is wonderfully <w>quiet</w> in the forest.",
                                "ru": "В лесу чудесно и <w>тихо</w>."
                            }
                        },
                        {
                            "text": "De kinderen spelen vandaag in een <w>rustige</w> kamer.",
                            "target_lang_translations": {
                                "en": "The children are playing in a <w>quiet</w> room today.",
                                "ru": "Дети играют сегодня в <w>тихой</w> комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "stil",
                        "kalm",
                        "vredig"
                    ],
                    "antonyms": [
                        "druk",
                        "luidruchtig",
                        "lawaaierig"
                    ],
                    "common_phrases": [
                        "een rustige buurt",
                        "lekker rustig"
                    ],
                    "target_lang_definitions": {
                        "en": "With little sound, movement, or activity; peaceful.",
                        "ru": "Без лишнего шума, движения или суеты; тихий и мирный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quiet",
                                "target_lang_sense_clarification": "characterized by an absence of noise or bustle"
                            },
                            {
                                "target_lang_word": "peaceful",
                                "target_lang_sense_clarification": "calm and free from disturbance"
                            },
                            {
                                "target_lang_word": "calm",
                                "target_lang_sense_clarification": "free from agitation or excitement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тихий",
                                "target_lang_sense_clarification": "об отсутствии шума"
                            },
                            {
                                "target_lang_word": "спокойный",
                                "target_lang_sense_clarification": "об атмосфере или месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bbb40db-e1ee-32d4-9c9d-166c4c3f69f8",
                    "sense_definition": "Zonder haast, stress of opwinding; ontspannen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "calm_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Blijf alsjeblieft <w>rustig</w> en vertel wat er is gebeurd.",
                            "target_lang_translations": {
                                "en": "Please stay <w>calm</w> and tell me what happened.",
                                "ru": "Пожалуйста, оставайся <w>спокойным</w> и расскажи, что случилось."
                            }
                        },
                        {
                            "text": "Hij is een heel <w>rustige</w> man die nooit schreeuwt.",
                            "target_lang_translations": {
                                "en": "He is a very <w>quiet</w> man who never shouts.",
                                "ru": "Он очень <w>спокойный</w> человек, который никогда не кричит."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedaard",
                        "ontspannen",
                        "beheerst"
                    ],
                    "antonyms": [
                        "gehaast",
                        "gestrest",
                        "paniekerig"
                    ],
                    "common_phrases": [
                        "blijf rustig",
                        "doe maar rustig"
                    ],
                    "target_lang_definitions": {
                        "en": "Without haste, stress, or excitement; relaxed in nature.",
                        "ru": "Без спешки, стресса или волнения; расслабленный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "calm",
                                "target_lang_sense_clarification": "not showing nervousness, anger, or other strong emotions"
                            },
                            {
                                "target_lang_word": "quiet",
                                "target_lang_sense_clarification": "mild-tempered or reserved in behavior"
                            },
                            {
                                "target_lang_word": "relaxed",
                                "target_lang_sense_clarification": "free from tension or anxiety"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спокойный",
                                "target_lang_sense_clarification": "о характере или поведении"
                            },
                            {
                                "target_lang_word": "уравновешенный",
                                "target_lang_sense_clarification": "сдержанный, не поддающийся панике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e1fcf6b-dbaf-370d-8ed6-9c2fd106ce14",
                    "sense_definition": "Zonder dat je je zorgen hoeft te maken; veilig of zonder bezwaar.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "safe_to_do",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt dat <w>rustig</w> aan hem vragen, hij vindt het niet erg.",
                            "target_lang_translations": {
                                "en": "You can <w>safely</w> ask him that; he won't mind.",
                                "ru": "Ты можешь <w>смело</w> спросить его об этом, он не будет против."
                            }
                        },
                        {
                            "text": "Ik durf hier 's avonds <w>rustig</w> over straat te lopen.",
                            "target_lang_translations": {
                                "en": "I dare to walk the streets here <w>safely</w> in the evening.",
                                "ru": "Я не боюсь <w>спокойно</w> ходить здесь по улицам вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "gerust",
                        "veilig"
                    ],
                    "target_lang_definitions": {
                        "en": "Without fear of consequences; safely or confidently.",
                        "ru": "Без причин для беспокойства; безопасно или без возражений."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "safely",
                                "target_lang_sense_clarification": "without risk or fear of negative outcome"
                            },
                            {
                                "target_lang_word": "without hesitation",
                                "target_lang_sense_clarification": "confidently and freely"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "смело",
                                "target_lang_sense_clarification": "без страха или сомнений"
                            },
                            {
                                "target_lang_word": "спокойно",
                                "target_lang_sense_clarification": "без опаски"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9bd4e5a4-93bc-3a19-b798-884521c15bf6",

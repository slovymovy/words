    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "39ff4ac9-4cab-3c26-a975-cac6e0658434",
                    "sense_definition": "Обязательство производителя или продавца бесплатно ремонтировать или заменять товар при обнаружении дефектов в течение определённого срока.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "warranty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На этот новый телевизор действует <w>гарантия</w> сроком два года.",
                            "target_lang_translations": {
                                "en": "This new TV comes with a two-year <w>warranty</w>."
                            }
                        },
                        {
                            "text": "Если телефон сломается, мы починим его бесплатно по <w>гарантии</w>.",
                            "target_lang_translations": {
                                "en": "If the phone breaks, we will repair it for free under <w>warranty</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "по гарантии",
                        "срок гарантии",
                        "гарантия качества",
                        "гарантия на год"
                    ],
                    "target_lang_definitions": {
                        "en": "A commitment by a manufacturer or seller to repair or replace a product free of charge if defects are found within a specified period."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "warranty",
                                "target_lang_sense_clarification": "product coverage"
                            },
                            {
                                "target_lang_word": "guarantee",
                                "target_lang_sense_clarification": "promise of quality"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20213522-949c-34b5-93cb-958cda78507a",
                    "sense_definition": "Условие или обстоятельство, которое обеспечивает успех дела или даёт уверенность в чём-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "assurance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нет никакой <w>гарантии</w>, что завтра будет хорошая погода.",
                            "target_lang_translations": {
                                "en": "There is no <w>guarantee</w> that the weather will be good tomorrow."
                            }
                        },
                        {
                            "text": "Хорошее образование — это надёжная <w>гарантия</w> будущей карьеры.",
                            "target_lang_translations": {
                                "en": "A good education is a reliable <w>guarantee</w> of a future career."
                            }
                        }
                    ],
                    "synonyms": [
                        "залог",
                        "уверенность"
                    ],
                    "antonyms": [
                        "риск",
                        "неопределённость"
                    ],
                    "common_phrases": [
                        "дать гарантию",
                        "никаких гарантий",
                        "стопроцентная гарантия",
                        "служить гарантией"
                    ],
                    "target_lang_definitions": {
                        "en": "A condition or circumstance that ensures success or provides certainty about a specific outcome."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "guarantee",
                                "target_lang_sense_clarification": "assurance of outcome"
                            },
                            {
                                "target_lang_word": "assurance",
                                "target_lang_sense_clarification": "confidence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7686fd8-edc7-3a5f-96e5-e596f13b6e41",
                    "sense_definition": "Официальное обязательство нести материальную ответственность за выполнение условий договора или оплату долгов другим лицом.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_surety",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Банк согласился выдать кредит только под <w>гарантию</w> государства.",
                            "target_lang_translations": {
                                "en": "The bank agreed to issue the loan only under a state <w>guarantee</w>."
                            }
                        },
                        {
                            "text": "Международные договоры предоставляют <w>гарантии</w> безопасности для всех участников.",
                            "target_lang_translations": {
                                "en": "International treaties provide security <w>guarantees</w> for all participants."
                            }
                        }
                    ],
                    "synonyms": [
                        "поручительство",
                        "обеспечение"
                    ],
                    "common_phrases": [
                        "банковская гарантия",
                        "под гарантию",
                        "социальные гарантии",
                        "государственные гарантии"
                    ],
                    "target_lang_definitions": {
                        "en": "A formal obligation to accept financial responsibility for the fulfillment of a contract or payment of a debt by another party."
                    },
                    "translations": {
                        "en": [
                            {

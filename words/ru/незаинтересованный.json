                    }
                },
                {
                    "sense_id": "743a3ffe-d0f2-3711-b97f-0f0772abe890",
                    "sense_definition": "Не преследующий личной выгоды или пользы; беспристрастный.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {

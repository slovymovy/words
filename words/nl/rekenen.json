                {
                    "sense_id": "334cf636-dea5-3b03-8f87-068ac871eaee",
                    "sense_definition": "Vertrouwen dat iets zal gebeuren of dat iemand zal helpen (altijd gebruikt met 'op').",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "trust_rely",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt altijd op mij <w>rekenen</w> als je hulp nodig hebt.",
                            "target_lang_translations": {
                                "en": "You can always <w>count</w> on me if you need help."
                            }
                        },
                        {
                            "text": "We hadden niet op zoveel bezoekers <w>gerekend</w> bij het feest.",
                            "target_lang_translations": {
                                "en": "We had not <w>counted</w> on so many visitors at the party."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertrouwen",
                        "bouwen"
                    ],
                    "common_phrases": [
                        "rekenen op",
                        "nergens op rekenen"
                    ],
                    "target_lang_definitions": {
                        "en": "To trust that something will happen or that someone will provide support (usually used with 'on')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "count on",
                                "target_lang_sense_clarification": "to rely on someone or expect something to happen"
                            },
                            {
                                "target_lang_word": "rely on",
                                "target_lang_sense_clarification": "to have confidence or trust in someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e3350ca-e1ea-37e7-85aa-cd921db508c9",
                    "sense_definition": "Een bepaald bedrag vragen als betaling voor een product of dienst.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "money_charge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ober <w>rekende</w> vijf euro voor het broodmandje.",
                            "target_lang_translations": {
                                "en": "The waiter <w>charged</w> five euros for the bread basket."
                            }
                        },
                        {
                            "text": "Zij <w>rekenen</w> geen verzendkosten voor bestellingen boven de vijftig euro.",
                            "target_lang_translations": {
                                "en": "They <w>charge</w> no shipping costs for orders over fifty euros."
                            }
                        }
                    ],
                    "synonyms": [
                        "vragen",
                        "aanrekenen",
                        "factureren"
                    ],
                    "common_phrases": [
                        "een prijs rekenen",
                        "kosten rekenen"
                    ],
                    "target_lang_definitions": {
                        "en": "To ask for a specific amount of money as payment for a product or service."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "charge",
                                "target_lang_sense_clarification": "to demand a price or fee"
                            },
                            {
                                "target_lang_word": "bill",
                                "target_lang_sense_clarification": "to send a request for payment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0b97b25-f776-3ce3-bf92-f4ba8f3ae580",
                    "sense_definition": "Iemand of iets indelen bij een bepaalde groep of beschouwen als.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consider_class",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>reken</w> hem tot mijn beste vrienden.",
                            "target_lang_translations": {
                                "en": "I <w>count</w> him among my best friends."
                            }
                        },
                        {
                            "text": "Deze schilder wordt tot de impressionisten <w>gerekend</w>.",
                            "target_lang_translations": {

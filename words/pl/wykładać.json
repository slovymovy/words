{
    "word_family": [
        "wyłożyć",
        "wykład",
        "wykładowca",
        "wykładzina"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "995c172d-cc1d-3305-99e3-d617d254fada",
                    "sense_definition": "Wyjmować przedmioty z wnętrza i kłaść je w widocznym miejscu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprzedawca codziennie rano <w>wykłada</w> świeże owoce na stragan.",
                            "target_lang_translations": {
                                "ru": "Продавец каждое утро <w>выкладывает</w> свежие фрукты на прилавок."
                            }
                        },
                        {
                            "text": "Podczas kontroli na lotnisku musisz <w>wykładać</w> elektronikę z plecaka.",
                            "target_lang_translations": {
                                "ru": "Во время досмотра в аэропорту нужно <w>выкладывать</w> электронику из рюкзака."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystawiać"
                    ],
                    "common_phrases": [
                        "wykładać towar na półki",
                        "wykładać karty na stół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вынимать предметы и класть их на видное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выкладывать",
                                "target_lang_sense_clarification": "доставать и класть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e76cda13-6d92-33e6-ac3f-f42cb920f1e2",
                    "sense_definition": "Prowadzić zajęcia dydaktyczne i wygłaszać prelekcje na uczelni wyższej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "teaching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek od wielu lat <w>wykłada</w> historię na uniwersytecie.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка много лет <w>преподает</w> историю в университете."
                            }
                        },
                        {
                            "text": "Profesor, który <w>wykłada</w> ten przedmiot, jest bardzo wymagający.",
                            "target_lang_translations": {
                                "ru": "Профессор, который <w>преподает</w> этот предмет, очень требователен."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wykładać na uniwersytecie",
                        "wykładać chemię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести учебные занятия и читать лекции в вузе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преподавать",
                                "target_lang_sense_clarification": "вести занятия"
                            },
                            {
                                "target_lang_word": "читать лекции",
                                "target_lang_sense_clarification": "выступать перед студентами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdccc5d4-38cf-30c0-abc8-067df2e333c0",
                    "sense_definition": "Wyjaśniać coś szczegółowo, tłumaczyć sens jakiegoś zagadnienia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "explaining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel cierpliwie <w>wykłada</w> uczniom nowe zasady gramatyki.",
                            "target_lang_translations": {
                                "ru": "Учитель терпеливо <w>разъясняет</w> ученикам новые правила грамматики."
                            }
                        },
                        {
                            "text": "Prawnik starał się <w>wykładać</w> zawiłości sprawy w prosty sposób.",
                            "target_lang_translations": {
                                "ru": "Юрист старался <w>излагать</w> сложности дела простым языком."
                            }
                        }
                    ],
                    "synonyms": [
                        "tłumaczyć",
                        "przedstawiać"
                    ],
                    "common_phrases": [
                        "wykładać swoje racje",
                        "wykładać kawę na ławę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подробно объяснять что-либо, разъяснять суть вопроса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "излагать",
                                "target_lang_sense_clarification": "систематически описывать"
                            },
                            {
                                "target_lang_word": "разъяснять",
                                "target_lang_sense_clarification": "делать понятным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f2e335d-9057-3bf5-b496-924fbe396858",
                    "sense_definition": "Pokrywać powierzchnię warstwą materiału wykończeniowego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ekipa remontowa zaczęła <w>wykładać</w> łazienkę nowymi płytkami.",
                            "target_lang_translations": {
                                "ru": "Ремонтная бригада начала <w>выкладывать</w> ванную новой плиткой."
                            }
                        },
                        {
                            "text": "Planujemy <w>wykładać</w> podłogę w salonie naturalnym drewnem.",
                            "target_lang_translations": {
                                "ru": "Мы планируем <w>выкладывать</w> пол в гостиной натуральным деревом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wykładać kafelkami",
                        "wykładać boazerią"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать поверхность слоем отделочного материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выкладывать",
                                "target_lang_sense_clarification": "покрывать материалом"
                            },
                            {
                                "target_lang_word": "облицовывать",
                                "target_lang_sense_clarification": "отделывать поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e817a4c3-c059-3676-bbe1-b24bc7bdebb8",
                    "sense_definition": "Przeznaczać własne pieniądze na sfinansowanie jakiegoś przedsięwzięcia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie zamierzam ciągle <w>wykładać</w> pieniędzy na twoje nieudane pomysły.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь постоянно <w>выкладывать</w> деньги на твои неудачные идеи."
                            }
                        },
                        {
                            "text": "Inwestor zgodził się <w>wykładać</w> duże sumy na rozwój technologii.",
                            "target_lang_translations": {
                                "ru": "Инвестор согласился <w>выкладывать</w> крупные суммы на развитие технологий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wykładać pieniądze",
                        "wykładać gotówkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тратить собственные деньги на финансирование чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выкладывать",
                                "target_lang_sense_clarification": "тратить деньги (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "add15f25-f468-3715-b0e6-2c9095d79932",
                    "sense_definition": "Przewracać się na ziemię lub ponosić porażkę, na przykład na egzaminie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "failure_falling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten narciarz często <w>wykłada się</w> na trudnych zakrętach trasy.",
                            "target_lang_translations": {
                                "ru": "Этот лыжник часто <w>падает</w> на сложных поворотах трассы."
                            }
                        },
                        {
                            "text": "Wielu studentów <w>wykłada się</w> na pytaniach z historii starożytnej.",
                            "target_lang_translations": {
                                "ru": "Многие студенты <w>сыплются</w> на вопросах по древней истории."
                            }
                        }
                    ],
                    "synonyms": [
                        "przewracać się",
                        "wywalać się",
                        "oblewać"
                    ],
                    "common_phrases": [
                        "wykładać się na egzaminie",
                        "wykładać się na prostej drodze"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Падать на землю или терпеть неудачу (например, на экзамене)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "терять равновесие"
                            },
                            {
                                "target_lang_word": "сыпаться",
                                "target_lang_sense_clarification": "не справляться с экзаменом (разг.)"
                            },
                            {
                                "target_lang_word": "проваливаться",
                                "target_lang_sense_clarification": "терпеть неудачу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d405f44e-341f-3349-8517-53fd768edc9b",
                    "sense_definition": "An adherent of Judaism.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a practicing <w>Jew</w> who attends the synagogue every Saturday.",
                            "target_lang_translations": {
                                "ru": "Он практикующий <w>иудей</w>, который посещает синагогу каждую субботу."
                            }
                        },
                        {
                            "text": "Many <w>Jews</w> around the world celebrate the festival of Passover.",
                            "target_lang_translations": {
                                "ru": "Многие <w>евреи</w> по всему миру празднуют Песах."
                            }
                        }
                    ],
                    "synonyms": [
                        "Judaist"
                    ],
                    "antonyms": [
                        "gentile",
                        "goy"
                    ],
                    "common_phrases": [
                        "orthodox Jew",
                        "practicing Jew",
                        "observant Jew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последователь иудаизма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иудей",
                                "target_lang_sense_clarification": "религиозный контекст"
                            },
                            {
                                "target_lang_word": "еврей",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66a18409-ebd7-36e6-82c2-494923837c90",
                    "sense_definition": "A member or descendant of the Jewish people.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She identifies culturally as a <w>Jew</w> even though she is not religious.",
                            "target_lang_translations": {
                                "ru": "Она культурно идентифицирует себя как <w>еврейка</w>, хотя и не религиозна."
                            }
                        },
                        {
                            "text": "The museum documents the history of every <w>Jew</w> who lived in the city before the war.",
                            "target_lang_translations": {
                                "ru": "Музей документирует историю каждого <w>еврея</w>, жившего в городе до войны."
                            }
                        }
                    ],
                    "antonyms": [
                        "gentile",
                        "goy"
                    ],
                    "common_phrases": [
                        "secular Jew",
                        "Ashkenazi Jew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель или потомок еврейского народа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "еврей",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "еврейка",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3714c7c-749d-3bcc-b87c-1c08fcfb36a0",
                    "sense_definition": "A miserly or greedy person; a cheapskate.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slur",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was criticized heavily for using the word <w>Jew</w> to describe his stingy boss.",
                            "target_lang_translations": {
                                "ru": "Его жестко раскритиковали за использование слова <w>жид</w> для описания своего скупого начальника."
                            }
                        },
                        {
                            "text": "Using the term <w>Jew</w> to mean a greedy person is considered a slur.",
                            "target_lang_translations": {
                                "ru": "Использование термина <w>жид</w> в значении «жадный человек» считается оскорблением."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "highly offensive and derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скупой или жадный человек; скряга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жид",
                                "target_lang_sense_clarification": "оскорбительное, сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c9fa225e-681e-396a-afea-0fb719f5746b",
                    "sense_definition": "To haggle or swindle in order to obtain a better deal.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slur",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shopkeeper was offended when the customer tried to <w>Jew</w> him down on the price.",
                            "target_lang_translations": {
                                "ru": "Владелец магазина обиделся, когда покупатель попытался <w>сбить цену по-жидовски</w>."
                            }
                        },
                        {
                            "text": "It is derogatory to say that someone is <w>Jewing</w> you out of money.",
                            "target_lang_translations": {
                                "ru": "Оскорбительно говорить, что кто-то <w>выжидивает</w> у тебя деньги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Jew down"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "highly offensive and derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Торговаться или обманывать с целью получения выгоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жидиться",
                                "target_lang_sense_clarification": "жадничать (оскорбительное)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7b6413fc-a306-3d26-9dca-5d7e12b0e70a",
                    "sense_definition": "A surname.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We have an appointment scheduled with Mr. <w>Jew</w> at three o'clock.",
                            "target_lang_translations": {
                                "ru": "У нас назначена встреча с мистером <w>Джу</w> на три часа."
                            }
                        },
                        {
                            "text": "The letter was addressed to the <w>Jew</w> family living on Maple Street.",
                            "target_lang_translations": {
                                "ru": "Письмо было адресовано семье <w>Джу</w>, живущей на Мейпл-стрит."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Джу",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3fa7117f-18b6-355b-8323-c434bfee8506",
                    "sense_definition": "Jewish; typically used with derogatory intent when replacing the standard adjective.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slur",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The propaganda spread hateful lies about the <w>Jew</w> population.",
                            "target_lang_translations": {
                                "ru": "Пропаганда распространяла ненавистную ложь о <w>жидовском</w> населении."
                            }
                        },
                        {
                            "text": "He made a <w>Jew</w> joke that made everyone in the room uncomfortable.",
                            "target_lang_translations": {
                                "ru": "Он отпустил <w>жидовскую</w> шутку, от которой всем в комнате стало не по себе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "offensive and non-standard"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Еврейский; обычно используется с уничижительным оттенком вместо стандартного прилагательного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жидовский",
                                "target_lang_sense_clarification": "оскорбительное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c4164cc0-24f7-364c-849c-94eb10b93166",
                    "sense_definition": "Przemieszczać się z jednego miejsca do drugiego przy użyciu środka transportu (np. samochodu, pociągu, roweru) lub na grzbiecie zwierzęcia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro rano <jadę> do Warszawy pociągiem.",
                            "target_lang_translations": {
                                "ru": "Завтра утром я <еду> в Варшаву на поезде."
                            }
                        },
                        {
                            "text": "Czy ty często <jedziesz> do pracy autobusem?",
                            "target_lang_translations": {
                                "ru": "Ты часто <ездишь> на работу на автобусе?"
                            }
                        },
                        {
                            "text": "Moi rodzice <jadą> w tym roku na wakacje nad morze.",
                            "target_lang_translations": {
                                "ru": "Мои родители в этом году <едут> в отпуск на море."
                            }
                        }
                    ],
                    "synonyms": [
                        "podróżować"
                    ],
                    "antonyms": [
                        "iść",
                        "stać"
                    ],
                    "common_phrases": [
                        "jechać samochodem",
                        "jechać pociągiem",
                        "jechać do domu",
                        "jechać na wakacje",
                        "jechać autobusem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться из одного места в другое с использованием транспортного средства (например, автомобиля, поезда, велосипеда) или верхом на животном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ехать",
                                "target_lang_sense_clarification": "двигаться на транспорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4362c11a-cff0-3448-a1e6-a57c1cdaaec9",
                    "sense_definition": "Ślizgać się gładko po powierzchni (używane głównie w kontekście sprzętu zimowego, jak narty czy sanki).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te nowe narty świetnie <jadą> po świeżym śniegu.",
                            "target_lang_translations": {
                                "ru": "Эти новые лыжи отлично <скользят> по свежему снегу."
                            }
                        },
                        {
                            "text": "Dzieci krzyczały z radości, gdy sanki szybko <jechały> w dół górki.",
                            "target_lang_translations": {
                                "ru": "Дети кричали от радости, когда санки быстро <ехали> с горки."
                            }
                        }
                    ],
                    "antonyms": [
                        "stać"
                    ],
                    "common_phrases": [
                        "sanki jadą",
                        "narty jadą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плавно скользить по поверхности (обычно используется в контексте зимнего снаряжения, такого как лыжи или санки)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скользить",
                                "target_lang_sense_clarification": "плавно двигаться по поверхности"
                            },
                            {
                                "target_lang_word": "ехать",
                                "target_lang_sense_clarification": "двигаться (о лыжах, санках)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7fcfa0b-6957-38df-b403-fd2d1be9f19e",
                    "sense_definition": "Wydzielać bardzo nieprzyjemny, intensywny zapach.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "smell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym starym pokoju strasznie <jedzie> wilgocią.",
                            "target_lang_translations": {
                                "ru": "В этой старой комнате ужасно <несет> сыростью."
                            }
                        },
                        {
                            "text": "Od tego bezdomnego <jechało> alkoholem na dużą odległość.",
                            "target_lang_translations": {
                                "ru": "От этого бездомного за версту <несло> алкоголем."
                            }
                        }
                    ],
                    "synonyms": [
                        "śmierdzieć",
                        "cuchnąć",
                        "walić"
                    ],
                    "antonyms": [
                        "stać"
                    ],
                    "common_phrases": [
                        "jechać od kogoś",
                        "strasznie jedzie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать очень неприятный, интенсивный запах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нести",
                                "target_lang_sense_clarification": "сильно пахнуть (безличная форма)"
                            },
                            {
                                "target_lang_word": "вонять",
                                "target_lang_sense_clarification": "издавать дурной запах (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e501f6c-1197-38f1-8d9f-f7f2369e246b",
                    "sense_definition": "Robić coś energicznie lub kontynuować czynność; często używane jako zachęta do działania.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie zatrzymuj się teraz, <jedziesz> z tym zadaniem do końca!",
                            "target_lang_translations": {
                                "ru": "Не останавливайся сейчас, <жми> с этим заданием до конца!"
                            }
                        },
                        {
                            "text": "Wszyscy są już gotowi, więc <jedziemy> z tematem.",
                            "target_lang_translations": {
                                "ru": "Все уже готовы, так что <поехали> с темой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jedziesz z tym",
                        "dajesz jedziesz"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то энергично или продолжать действие; часто используется как побуждение к действию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давай",
                                "target_lang_sense_clarification": "побуждение к действию"
                            },
                            {
                                "target_lang_word": "погнали",
                                "target_lang_sense_clarification": "призыв начать действие (слэнг)"
                            },
                            {
                                "target_lang_word": "жми",
                                "target_lang_sense_clarification": "действуй энергично (слэнг)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66b54a85-d4be-318d-b6ac-addcfb1ed1b6",
                    "sense_definition": "Ostro kogoś krytykować, wyzywać lub atakować słownie (zwykle łączy się z przyimkiem 'po').",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "aggression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szef w pracy ostro <jechał> po pracowniku za spóźnienie.",
                            "target_lang_translations": {
                                "ru": "Начальник на работе жестко <наезжал> на сотрудника за опоздание."
                            }
                        },
                        {
                            "text": "Nie powinieneś tak na niego <jechać> przy wszystkich ludziach.",
                            "target_lang_translations": {
                                "ru": "Ты не должен так на него <наезжать> при всех."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jechać po kimś",
                        "nie jedź po mnie"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "aggressive context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко критиковать кого-то, оскорблять или словесно нападать (обычно с предлогом 'po' - на)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наезжать",
                                "target_lang_sense_clarification": "агрессивно предъявлять претензии (слэнг)"
                            },
                            {
                                "target_lang_word": "гнать",
                                "target_lang_sense_clarification": "говорить гадости или ложь (слэнг)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
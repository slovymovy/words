    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "34c8107b-7a6d-3efd-989a-a7c3461cb551",
                    "sense_definition": "Powierzenie komuś wyższego stanowiska w pracy lub nadanie wyższego stopnia służbowego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_advancement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój ojciec dostał wczoraj <awans> na kierownika działu.",
                            "target_lang_translations": {
                                "ru": "Мой отец вчера получил <повышение> до начальника отдела."
                            }
                        },
                        {
                            "text": "Ciężka praca i zaangażowanie to najlepsza droga, by zdobyć <awans>.",
                            "target_lang_translations": {
                                "ru": "Упорный труд и самоотдача — лучший способ получить <повышение>."
                            }
                        }
                    ],
                    "antonyms": [
                        "degradacja"
                    ],
                    "common_phrases": [
                        "awans zawodowy",
                        "dostać awans",
                        "szybki awans"
                    ],
                    "target_lang_definitions": {
                        "ru": "Назначение на более высокую должность или присвоение более высокого звания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повышение",
                                "target_lang_sense_clarification": "по службе"
                            },
                            {
                                "target_lang_word": "продвижение",
                                "target_lang_sense_clarification": "карьерный рост"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5630152-75fe-3f12-a589-110c9af591be",
                    "sense_definition": "Przejście drużyny lub zawodnika do wyższej klasy rozgrywkowej albo do dalszego etapu zawodów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_advancement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza reprezentacja świętowała wczoraj <awans> do mistrzostw świata.",
                            "target_lang_translations": {
                                "ru": "Вчера наша сборная праздновала <выход> на чемпионат мира."
                            }
                        },
                        {
                            "text": "Po zaciętym meczu drużyna zapewniła sobie <awans> do ekstraklasy.",
                            "target_lang_translations": {
                                "ru": "После напряженного матча команда обеспечила себе <выход> в высшую лигу."
                            }
                        }
                    ],
                    "antonyms": [
                        "spadek"
                    ],
                    "common_phrases": [
                        "awans do finału",
                        "wywalczyć awans",
                        "brak awansu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переход команды или спортсмена в более высокую лигу или в следующий этап соревнований."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выход",
                                "target_lang_sense_clarification": "в следующий этап"
                            },
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "в высшую лигу"
                            }

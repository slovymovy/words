    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c430b255-bc14-3d6e-b3b7-2c91400ec005",
                    "sense_definition": "A work of outstanding creativity, skill, or workmanship.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "outstanding_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum purchased a rare <w>masterpiece</w> for its permanent collection.",
                            "target_lang_translations": {
                                "ru": "Музей приобрел редкий <w>шедевр</w> для своей постоянной коллекции."
                            }
                        },
                        {
                            "text": "Critics described the new film as a cinematic <w>masterpiece</w>.",
                            "target_lang_translations": {
                                "ru": "Критики назвали новый фильм кинематографическим <w>шедевром</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "masterwork",
                        "work of art",
                        "chef d'œuvre",
                        "pièce de résistance"
                    ],
                    "antonyms": [
                        "piece of junk",
                        "trash"
                    ],
                    "common_phrases": [
                        "absolute masterpiece",
                        "cinematic masterpiece",
                        "literary masterpiece",
                        "true masterpiece"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произведение, отличающееся выдающимся творчеством, мастерством или качеством исполнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шедевр",
                                "target_lang_sense_clarification": "выдающееся произведение искусства"
                            },
                            {
                                "target_lang_word": "произведение искусства",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4095cd43-663f-3cab-9b6b-32977b307ca2",
                    "sense_definition": "The greatest work of a person's career or lifetime.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "outstanding_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most scholars consider this symphony to be the composer's <w>masterpiece</w>.",
                            "target_lang_translations": {
                                "ru": "Большинство ученых считают эту симфонию <w>вершиной творчества</w> композитора."
                            }
                        },
                        {
                            "text": "He spent twenty years writing what he hoped would be his <w>masterpiece</w>.",
                            "target_lang_translations": {
                                "ru": "Он потратил двадцать лет на написание того, что, как он надеялся, станет его <w>главным трудом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "magnum opus"
                    ],
                    "common_phrases": [
                        "acknowledged masterpiece",
                        "undisputed masterpiece"
                    ],
                    "target_lang_definitions": {
                        "ru": "Величайшая работа в карьере или жизни человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шедевр",
                                "target_lang_sense_clarification": "лучшее творение автора"
                            },
                            {
                                "target_lang_word": "вершина творчества",
                                "target_lang_sense_clarification": "главный труд жизни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6841bf87-3013-310c-8658-933176257d1f",
                    "sense_definition": "A work created in order to qualify as a master craftsman and member of a guild.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The apprentice presented his <w>masterpiece</w> to the guild to prove his skills.",
                            "target_lang_translations": {
                                "ru": "Подмастерье представил свой <w>шедевр</w> гильдии, чтобы доказать свои навыки."
                            }
                        },
                        {
                            "text": "Only after submitting a flawless <w>masterpiece</w> could he open his own shop.",
                            "target_lang_translations": {
                                "ru": "Только представив безупречный <w>шедевр</w>, он смог открыть собственную мастерскую."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context regarding guilds"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Работа, созданная для получения звания мастера и члена гильдии (цеха)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шедевр",
                                "target_lang_sense_clarification": "историческое: образцовое изделие"
                            },
                            {
                                "target_lang_word": "образцовая работа",
                                "target_lang_sense_clarification": "экзаменационное изделие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

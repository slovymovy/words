                {
                    "sense_id": "154eb773-11c9-377a-90b5-4c516bfc5680",
                    "sense_definition": "Tekst of afbeeldingen op papier zetten met behulp van een printer of drukpers.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "printing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet nog even mijn vliegtickets <w>afdrukken</w> voor de reis.",
                            "target_lang_translations": {
                                "ru": "Мне нужно еще <w>распечатать</w> билеты на самолет перед поездкой.",
                                "en": "I still need to <w>print</w> my flight tickets for the trip."
                            }
                        },
                        {
                            "text": "Kun jij dit document voor mij in kleur <w>afdrukken</w>?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>распечатать</w> этот документ для меня в цвете?",
                                "en": "Can you <w>print</w> this document in color for me?"
                            }
                        }
                    ],
                    "synonyms": [
                        "printen"
                    ],
                    "common_phrases": [
                        "foto's afdrukken",
                        "een bestand afdrukken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Печатать текст или изображения на бумаге с помощью принтера или печатного станка.",
                        "en": "To put text or images on paper using a printer or printing press."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распечатывать",
                                "target_lang_sense_clarification": "выводить на печать (на принтере)"
                            },
                            {
                                "target_lang_word": "печатать",
                                "target_lang_sense_clarification": "общий процесс печати"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "print",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "print out",
                                "target_lang_sense_clarification": "specifically producing a hard copy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5766c5bd-2428-3a2d-b02c-44fc04a87941",
                    "sense_definition": "Een mechanisme in werking stellen door op een knop of trekker te drukken, bijvoorbeeld bij een camera of wapen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "triggering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fotograaf wachtte op het perfecte licht om te kunnen <w>afdrukken</w>.",

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засаживает лесом",
                                "target_lang_sense_clarification": "покрывает территорию деревьями"
                            },
                            {
                                "target_lang_word": "снабжает дровами",

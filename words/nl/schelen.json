    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0b55dc85-5f66-365a-979a-fdc0cb44f4a1",
                    "sense_definition": "Een verschil maken of van belang zijn; eruit doen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "matter_differ",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het kan me echt niets <w>schelen</w> wat anderen daarvan denken.",
                            "target_lang_translations": {
                                "ru": "Меня совершенно не <w>волнует</w>, что об этом думают другие.",
                                "en": "I really don't <w>care</w> what others think about that."
                            }
                        },
                        {
                            "text": "Dat <w>scheelt</w> een hoop tijd als we de trein nemen.",
                            "target_lang_translations": {
                                "ru": "Это <w>сэкономит</w> кучу времени, если мы поедем на поезде.",
                                "en": "It <w>saves</w> a lot of time if we take the train."
                            }
                        },
                        {
                            "text": "Dat <w>scheelt</w> een slok op een borrel.",
                            "target_lang_translations": {
                                "ru": "Это существенно <w>меняет</w> дело.",
                                "en": "That <w>makes a</w> big <w>difference</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitmaken",
                        "afwijken"
                    ],
                    "common_phrases": [
                        "niet kunnen schelen",
                        "dat scheelt",
                        "wat kan het schelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь значение, быть важным; составлять разницу (например, во времени или количестве).",
                        "en": "To make a difference, to be important, or to save (time or money)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь значение",
                                "target_lang_sense_clarification": "быть важным"
                            },
                            {
                                "target_lang_word": "волновать",
                                "target_lang_sense_clarification": "заботить (часто в отрицании)"
                            },
                            {
                                "target_lang_word": "экономить",
                                "target_lang_sense_clarification": "сберегать время или деньги"
                            },
                            {
                                "target_lang_word": "составлять разницу",
                                "target_lang_sense_clarification": "отличаться на определенную величину"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "make a difference",
                                "target_lang_sense_clarification": "to have an effect or change a situation"
                            },
                            {
                                "target_lang_word": "matter",
                                "target_lang_sense_clarification": "to be of importance"
                            },
                            {
                                "target_lang_word": "save",
                                "target_lang_sense_clarification": "regarding time or money"
                            },
                            {
                                "target_lang_word": "care",
                                "target_lang_sense_clarification": "used in negative phrases (e.g., I don't care)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13a7917b-a6ae-3ef9-828d-c4db526c6320",
                    "sense_definition": "Iets mankeren of mis zijn; last hebben van iets.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "ail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik vraag me af wat er met hem <w>scheelt</w> vandaag.",
                            "target_lang_translations": {
                                "ru": "Интересно, что с ним сегодня <w>не так</w>.",
                                "en": "I wonder what is <w>wrong</w> with him today."
                            }
                        },
                        {
                            "text": "Er <w>scheelt</w> niets aan de motor, hij loopt als een zonnetje.",
                            "target_lang_translations": {
                                "ru": "В моторе ничего не <w>барахлит</w>, он работает как часы.",
                                "en": "There is nothing <w>wrong</w> with the engine, it runs like a charm."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wat scheelt er"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть не в порядке, беспокоить (о здоровье или состоянии); случаться.",
                        "en": "To be wrong with someone or something; to be the matter."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть не так",
                                "target_lang_sense_clarification": "о ситуации или состоянии"
                            },
                            {
                                "target_lang_word": "беспокоить",
                                "target_lang_sense_clarification": "о проблеме или боли"
                            },
                            {
                                "target_lang_word": "случаться",
                                "target_lang_sense_clarification": "происходить (в вопросах)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "be wrong",
                                "target_lang_sense_clarification": "regarding a problem or illness"
                            },
                            {
                                "target_lang_word": "be the matter",
                                "target_lang_sense_clarification": "used in questions about problems"
                            }
                        ]
                    }
                }
            ]
        },

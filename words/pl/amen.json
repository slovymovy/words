    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "43e0b932-27df-364c-be85-0e828b623a01",
                    "sense_definition": "Uroczysta formuła wypowiadana na zakończenie modlitwy chrześcijańskiej, oznaczająca potwierdzenie, zgodę lub życzenie, aby tak się stało.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W imię Ojca i Syna, i Ducha Świętego, <w>amen</w>.",
                            "target_lang_translations": {
                                "ru": "Во имя Отца и Сына и Святого Духа, <w>аминь</w>."
                            }
                        },
                        {
                            "text": "Wierni zgromadzeni w kościele odpowiedzieli głośno <w>amen</w>.",
                            "target_lang_translations": {
                                "ru": "Верующие, собравшиеся в церкви, громко ответили <w>аминь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Śląsk Cieszyński i Górny Śląsk) amyn",
                        "gw. (Górny Śląsk) amynt"
                    ],
                    "common_phrases": [
                        "na wieki wieków amen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественная формула, произносимая в конце христианской молитвы, означающая подтверждение, согласие или пожелание, чтобы так и было."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аминь",
                                "target_lang_sense_clarification": "религиозное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d810d170-a63f-3649-b92c-9ea1cb8370fa",
                    "sense_definition": "Słowo używane do kategorycznego zakończenia wypowiedzi, ucięcia dyskusji lub podkreślenia nieodwołalności decyzji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powiedziałem, że nigdzie nie pójdę i <w>amen</w>.",
                            "target_lang_translations": {
                                "ru": "Я сказал, что никуда не пойду, и <w>аминь</w>."
                            }
                        },
                        {
                            "text": "To jest moja ostateczna decyzja, <w>amen</w> w pacierzu.",
                            "target_lang_translations": {
                                "ru": "Это моё окончательное решение, и <w>аминь</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "i amen",
                        "amen w pacierzu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое для категорического завершения высказывания, прекращения дискуссии или подчеркивания окончательности решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аминь",
                                "target_lang_sense_clarification": "конец разговора"
                            },
                            {
                                "target_lang_word": "и всё",
                                "target_lang_sense_clarification": "категоричное завершение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
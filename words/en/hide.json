{
    "word_family": [
        "hideout",
        "hideaway",
        "hiding",
        "hidden"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9629afaa-8eff-35e2-88a0-111536a0a02d",
                    "sense_definition": "To put something in a place where it cannot be seen or found.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "concealment_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She tried to <w>hide</w> the birthday gift behind her back.",
                            "target_lang_translations": {
                                "ru": "Она пыталась <w>спрятать</w> подарок на день рождения у себя за спиной."
                            }
                        },
                        {
                            "text": "Thick clouds often <w>hide</w> the top of the mountain.",
                            "target_lang_translations": {
                                "ru": "Густые облака часто <w>скрывают</w> вершину горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "conceal",
                        "bury",
                        "hide away",
                        "secrete"
                    ],
                    "antonyms": [
                        "reveal",
                        "show",
                        "uncover",
                        "expose",
                        "disclose"
                    ],
                    "common_phrases": [
                        "hide the truth",
                        "hide feelings",
                        "hide evidence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать что-либо в место, где это нельзя увидеть или найти; делать невидимым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прятать",
                                "target_lang_sense_clarification": "класть в тайное место"
                            },
                            {
                                "target_lang_word": "скрывать",
                                "target_lang_sense_clarification": "делать невидимым, утаивать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19e8aa20-eed1-3642-927c-e55aa201c6d0",
                    "sense_definition": "To put oneself in a place where one cannot be seen or found.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "concealment_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The frightened cat ran to <w>hide</w> under the sofa.",
                            "target_lang_translations": {
                                "ru": "Испуганная кошка побежала <w>прятаться</w> под диван."
                            }
                        },
                        {
                            "text": "The children like to run and <w>hide</w> during the game.",
                            "target_lang_translations": {
                                "ru": "Дети любят бегать и <w>прятаться</w> во время игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "hide out",
                        "lie low",
                        "hole up"
                    ],
                    "antonyms": [
                        "show",
                        "reveal"
                    ],
                    "common_phrases": [
                        "hide from someone",
                        "nowhere to hide",
                        "run and hide"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать себя в место, где нельзя быть увиденным или найденным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прятаться",
                                "target_lang_sense_clarification": "укрываться в тайном месте"
                            },
                            {
                                "target_lang_word": "скрываться",
                                "target_lang_sense_clarification": "исчезать из виду, быть в бегах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bcf2a066-40ca-3078-8022-78f2f8314506",
                    "sense_definition": "The skin of an animal, especially when treated for use as leather.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_skin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They used a buffalo <w>hide</w> to make a warm blanket.",
                            "target_lang_translations": {
                                "ru": "Они использовали <w>шкуру</w> буйвола, чтобы сделать тёплое одеяло."
                            }
                        },
                        {
                            "text": "The expensive boots were made from high-quality <w>hide</w>.",
                            "target_lang_translations": {
                                "ru": "Дорогие ботинки были сделаны из высококачественной <w>кожи</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pelt",
                        "skin"
                    ],
                    "common_phrases": [
                        "cow hide",
                        "tan a hide"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кожный покров животного, особенно обработанный для использования в качестве материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шкура",
                                "target_lang_sense_clarification": "снятая с животного, часто с мехом"
                            },
                            {
                                "target_lang_word": "кожа",
                                "target_lang_sense_clarification": "выделанный материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b4c4b58-8161-37f0-9322-7c359f5d63c9",
                    "sense_definition": "One's own life or personal safety, especially when in danger.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "personal_safety",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He lied to the police just to save his own <w>hide</w>.",
                            "target_lang_translations": {
                                "ru": "Он солгал полиции только ради того, чтобы спасти свою собственную <w>шкуру</w>."
                            }
                        },
                        {
                            "text": "I am not going to risk my <w>hide</w> for such a foolish plan.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь рисковать своей <w>шкурой</w> ради такого глупого плана."
                            }
                        }
                    ],
                    "common_phrases": [
                        "save one's hide",
                        "risk one's hide"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Собственная жизнь или личная безопасность (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шкура",
                                "target_lang_sense_clarification": "жизнь, безопасность (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49059f7d-005d-37f8-9b06-b35694a691ac",
                    "sense_definition": "A covered structure used by birdwatchers or hunters to observe animals without scaring them.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shelter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The photographer waited patiently in the <w>hide</w> for the eagle to appear.",
                            "target_lang_translations": {
                                "ru": "Фотограф терпеливо ждал в <w>укрытии</w> появления орла."
                            }
                        },
                        {
                            "text": "We built a small <w>hide</w> near the lake to watch the ducks.",
                            "target_lang_translations": {
                                "ru": "Мы построили небольшое <w>укрытие</w> у озера, чтобы наблюдать за утками."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bird hide"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mainly British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крытое сооружение, используемое наблюдателями за птицами или охотниками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрытие",
                                "target_lang_sense_clarification": "место для наблюдения"
                            },
                            {
                                "target_lang_word": "засидка",
                                "target_lang_sense_clarification": "охотничий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23ce6984-452d-3eda-b150-6a58dcebddbe",
                    "sense_definition": "An old unit of land measurement used in history.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The estate was measured as one <w>hide</w> of land.",
                            "target_lang_translations": {
                                "ru": "Поместье было измерено как одна <w>гайда</w> земли."
                            }
                        },
                        {
                            "text": "Historians debate the exact size of a <w>hide</w> in the tenth century.",
                            "target_lang_translations": {
                                "ru": "Историки спорят о точном размере <w>гайды</w> в десятом веке."
                            }
                        }
                    ],
                    "synonyms": [
                        "carucate"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная единица измерения земельной площади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гайда",
                                "target_lang_sense_clarification": "историческая мера земли"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {

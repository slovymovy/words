    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "80326288-16e9-335f-a5ba-8a8fe8e661c1",
                    "sense_definition": "Gotowość do wypełniania obowiązków, znoszenia trudów i wiara w sukces, zwłaszcza w odniesieniu do grupy ludzi, np. żołnierzy, pracowników lub sportowców.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spirit_attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wczorajszej porażce <morale> w drużynie znacznie spadło.",
                            "target_lang_translations": {
                                "ru": "После вчерашнего поражения <боевой дух> в команде значительно упал."
                            }
                        },
                        {
                            "text": "Dobry dowódca potrafi skutecznie podnosić <morale> swoich żołnierzy w trudnych chwilach.",
                            "target_lang_translations": {
                                "ru": "Хороший командир умеет эффективно поднимать <боевой дух> своих солдат в трудные моменты."
                            }
                        }
                    ],
                    "synonyms": [
                        "duch bojowy",
                        "nastroje",
                        "wola walki"
                    ],
                    "antonyms": [
                        "zniechęcenie",
                        "defetyzm"
                    ],
                    "common_phrases": [
                        "podnosić morale",
                        "niskie morale",
                        "wysokie morale",
                        "upadek morale"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовность выполнять обязанности, переносить трудности и вера в успех, особенно применительно к группе людей (например, солдатам или спортсменам)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "боевой дух",
                                "target_lang_sense_clarification": "настрой на борьбу и победу"
                            },
                            {
                                "target_lang_word": "моральный дух",
                                "target_lang_sense_clarification": "психологическое состояние группы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5efb9458-c795-3b47-9cf0-5737f067f2d5",
                    "sense_definition": "Forma miejscownika i wołacza liczby pojedynczej rzeczownika <w>morał</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel długo opowiadał uczniom o <morale> płynącym z tej starej bajki.",
                            "target_lang_translations": {
                                "ru": "Учитель долго рассказывал ученикам о <морали>, следующей из этой старой сказки."
                            }
                        },
                        {
                            "text": "W tym prostym <morale> kryje się głęboka życiowa mądrość.",
                            "target_lang_translations": {
                                "ru": "В этой простой <морали> скрывается глубокая жизненная мудрость."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "of morał"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма предложного и звательного падежа единственного числа существительного \"morał\" (мораль, нравоучение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мораль",
                                "target_lang_sense_clarification": "поучительный вывод"
                            },
                            {
                                "target_lang_word": "нравоучение",
                                "target_lang_sense_clarification": "наставление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5bd3d0c-3b0a-3b09-baaa-946f124b747c",
                    "sense_definition": "Ogólna postawa etyczna i zasady postępowania reprezentowane przez daną osobę lub grupę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "spirit_attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego wątpliwe <morale> sprawiło, że stracił zaufanie swoich współpracowników.",
                            "target_lang_translations": {
                                "ru": "Его сомнительный <моральный облик> привел к тому, что он потерял доверие своих коллег."
                            }
                        },
                        {
                            "text": "Osoba na tym stanowisku powinna reprezentować nienaganne <morale>.",
                            "target_lang_translations": {
                                "ru": "Человек на этой должности должен демонстрировать безупречную <нравственность>."
                            }
                        }
                    ],
                    "synonyms": [
                        "moralność",
                        "etyka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общая этическая позиция и принципы поведения, представляемые данным лицом или группой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моральный облик",
                                "target_lang_sense_clarification": "совокупность моральных качеств"
                            },
                            {
                                "target_lang_word": "нравственность",
                                "target_lang_sense_clarification": "этические нормы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
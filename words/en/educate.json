                {
                    "sense_id": "43a7450e-2a06-383d-870b-b9dededf7dde",
                    "sense_definition": "To give intellectual, moral, and social instruction to someone, typically at a school or university.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Parents and teachers work together to <w>educate</w> children about safety.",
                            "target_lang_translations": {
                                "ru": "Родители и учителя работают вместе, чтобы <w>обучать</w> детей правилам безопасности."
                            }
                        },
                        {
                            "text": "The museum aims to <w>educate</w> the public about local history.",
                            "target_lang_translations": {
                                "ru": "Музей стремится <w>просвещать</w> общественность в вопросах местной истории."
                            }
                        }
                    ],
                    "synonyms": [
                        "teach",
                        "instruct"
                    ],
                    "common_phrases": [
                        "educate the public",
                        "educate yourself",
                        "educate children"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e20e52fb-4e33-347d-befd-4136437985ea",
                    "sense_definition": "Натуральная ткань или нить, изготовленная из волокон хлопчатника.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_cotton",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом очень приятно носить одежду из лёгкого <w>хлопка</w>.",
                            "target_lang_translations": {
                                "en": "In summer, it is very pleasant to wear clothes made of light <w>cotton</w>."
                            }
                        },
                        {
                            "text": "На этикетке было написано, что эта рубашка — стопроцентный <w>хлопок</w>.",
                            "target_lang_translations": {
                                "en": "The label said that this shirt is one hundred percent <w>cotton</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "хлопчатобума́жная ткань"
                    ],
                    "common_phrases": [
                        "стопроцентный хлопок",
                        "одежда из хлопка",
                        "органический хлопок"
                    ],
                    "target_lang_definitions": {
                        "en": "Natural fabric or thread made from cotton fibers."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cotton",
                                "target_lang_sense_clarification": "fabric or material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "feb635fa-eae0-39e0-a2a7-b8b3f5ad8f4b",
                    "sense_definition": "Короткий, резкий удар ладонью о ладонь или звук, возникающий при таком ударе.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель сделал громкий <w>хлопок</w> в ладоши, чтобы привлечь внимание учеников.",
                            "target_lang_translations": {
                                "en": "The teacher gave a loud <w>clap</w> of his hands to attract the students' attention."
                            }
                        },
                        {
                            "text": "После выступления артиста в зале раздались одинокие <w>хлопки</w>.",
                            "target_lang_translations": {
                                "en": "After the artist's performance, solitary <w>claps</w> were heard in the hall."
                            }
                        }
                    ],
                    "common_phrases": [
                        "громкий хлопок",
                        "хлопок в ладоши",
                        "один хлопок"
                    ],
                    "target_lang_definitions": {
                        "en": "A short, sharp blow of palm against palm or the sound produced by such a blow."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clap",
                                "target_lang_sense_clarification": "sound of hands"
                            },
                            {
                                "target_lang_word": "handclap",
                                "target_lang_sense_clarification": "action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7df1f83-e91e-31da-bcd7-c6817879588b",
                    "sense_definition": "Культурное растение семейства мальвовых, из коробочек которого получают волокно для пряжи.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_cotton",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В южных регионах страны фермеры выращивают <w>хлопок</w> на больших полях.",
                            "target_lang_translations": {
                                "en": "In the southern regions of the country, farmers grow <w>cotton</w> in large fields."
                            }
                        },
                        {
                            "text": "Сбор <w>хлопка</w> является важной частью экономики этого государства.",
                            "target_lang_translations": {
                                "en": "The harvesting of <w>cotton</w> is an important part of this state's economy."
                            }
                        }
                    ],
                    "synonyms": [
                        "хлопча́тник"
                    ],
                    "common_phrases": [
                        "сбор хлопка",
                        "выращивать хлопок",
                        "поля хлопка"
                    ],
                    "target_lang_definitions": {
                        "en": "A cultivated plant of the mallow family, from whose bolls fiber for yarn is obtained."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cotton",
                                "target_lang_sense_clarification": "plant"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afab9e5f-d980-3d6e-ab11-5ff66c05878d",
                    "sense_definition": "Резкий, отрывистый звук, напоминающий удар или выстрел.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы услышали громкий <w>хлопок</w>, когда у проезжающей машины лопнуло колесо.",
                            "target_lang_translations": {
                                "en": "We heard a loud <w>pop</w> when the passing car's tire burst."
                            }
                        },
                        {
                            "text": "Пробка вылетела из бутылки шампанского с весёлым <w>хлопком</w>.",
                            "target_lang_translations": {
                                "en": "The cork flew out of the champagne bottle with a cheerful <w>pop</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "шлепок"
                    ],
                    "common_phrases": [
                        "глухой хлопок",
                        "услышать хлопок",
                        "хлопок выстрела"
                    ],
                    "target_lang_definitions": {
                        "en": "A sharp, abrupt sound resembling a blow or a shot."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pop",
                                "target_lang_sense_clarification": "sharp sound like a cork or bursting tire"
                            },
                            {
                                "target_lang_word": "bang",
                                "target_lang_sense_clarification": "loud noise"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bef42d6-5604-37fa-8380-370069216223",
                    "sense_definition": "Взрыв (обычно газа или техногенного характера), используемое в официальных сообщениях СМИ.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В утренних новостях сообщили про <w>хлопок</w> бытового газа в жилом доме.",
                            "target_lang_translations": {
                                "en": "The morning news reported a gas <w>explosion</w> in a residential building."
                            }
                        },
                        {
                            "text": "Специалисты выясняют причины, вызвавшие <w>хлопок</w> газовоздушной смеси.",
                            "target_lang_translations": {
                                "en": "Specialists are investigating the causes that triggered the gas-air mixture <w>blast</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "взрыв"
                    ],
                    "common_phrases": [
                        "хлопок газа",
                        "произошел хлопок"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "бюрократический эвфемизм"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An explosion (usually of gas or technical nature), used in official media reports."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "explosion",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "blast",
                                "target_lang_sense_clarification": "destructive wave or event"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "729b9645-0d75-3a42-ae3e-5e719c2a689c",
                    "sense_definition": "Рыхлый комок или клок чего-либо (обычно шерсти, ваты или снега).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_flake",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Снег падал на землю крупными белыми <w>хлопками</w>, покрывая всё вокруг.",
                            "target_lang_translations": {
                                "en": "Snow fell to the ground in large white <w>flakes</w>, covering everything around."
                            }
                        },
                        {
                            "text": "Старая игрушка порвалась, и из неё торчал грязный <w>хлопок</w> ваты.",
                            "target_lang_translations": {
                                "en": "The old toy tore, and a dirty <w>tuft</w> of cotton wool stuck out of it."
                            }
                        }
                    ],
                    "synonyms": [
                        "хлопья"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "чаще используется форма мн.ч. 'хлопья'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A loose lump or shred of something (usually wool, cotton wool, or snow)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flake",
                                "target_lang_sense_clarification": "of snow"
                            },
                            {
                                "target_lang_word": "tuft",
                                "target_lang_sense_clarification": "of wool or cotton"
                            },
                            {
                                "target_lang_word": "clump",
                                "target_lang_sense_clarification": "loose mass"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
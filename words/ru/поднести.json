                {
                    "sense_id": "ac6f556d-d2fc-39d2-abfe-a7da8f3673fa",
                    "sense_definition": "Приблизить что-либо к кому-либо или чему-либо, держа в руках.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <поднёс> бинокль к глазам, чтобы лучше рассмотреть птицу на дереве.",
                            "target_lang_translations": {
                                "en": "He <raised> the binoculars to his eyes to get a better look at the bird in the tree."
                            }
                        },
                        {
                            "text": "Мама осторожно <поднесла> ложку с горячим супом ко рту ребёнка.",
                            "target_lang_translations": {
                                "en": "Mom carefully <brought> the spoon of hot soup to the child's mouth."
                            }
                        }
                    ],
                    "synonyms": [
                        "приблизить"
                    ],
                    "antonyms": [
                        "отодвинуть",
                        "отвести",
                        "убрать"
                    ],
                    "common_phrases": [
                        "поднести к глазам",
                        "поднести руку к козырьку",
                        "поднести ко рту",
                        "поднести спичку"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring something close to someone or something while holding it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bring up to",
                                "target_lang_sense_clarification": "move an object close to a specific point (eyes, mouth, etc.)"
                            },
                            {
                                "target_lang_word": "raise to",
                                "target_lang_sense_clarification": "lift something towards a specific level or body part"
                            },
                            {
                                "target_lang_word": "hold up to",
                                "target_lang_sense_clarification": "lift and position something near something else"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a4bafec-f07a-3f18-a475-dab5cd7d1d50",
                    "sense_definition": "Принести и доставить что-либо (обычно тяжелое) в нужное место.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_physical",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "abfee82a-c96d-3834-a49c-8896a5702be8",
                    "sense_definition": "A flat piece of coarse material used for wiping one’s feet, or as a decorative or protective floor covering.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "covering_pad",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please wipe your dirty shoes on the <w>mat</w> before entering the house.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, вытри грязную обувь о <w>коврик</w> перед входом в дом."
                            }
                        },
                        {
                            "text": "There is a welcome <w>mat</w> right in front of the front door.",
                            "target_lang_translations": {
                                "ru": "Прямо перед входной дверью лежит приветственный <w>коврик</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "doormat",
                        "rug"
                    ],
                    "common_phrases": [
                        "welcome mat",
                        "door mat",
                        "bath mat",
                        "floor mat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский кусок грубого материала, используемый для вытирания ног, или как декоративное или защитное покрытие пола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коврик",
                                "target_lang_sense_clarification": "небольшой ковер"
                            },
                            {
                                "target_lang_word": "половик",
                                "target_lang_sense_clarification": "для вытирания ног"
                            },
                            {
                                "target_lang_word": "циновка",
                                "target_lang_sense_clarification": "плетеная из соломы или тростника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df381682-2983-353d-96d1-f1e6e5bea139",
                    "sense_definition": "A small flat piece of material used to protect a surface from anything hot or rough; a coaster.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "covering_pad",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The waiter placed a beer <w>mat</w> under my glass to keep the table dry.",
                            "target_lang_translations": {
                                "ru": "Официант положил <w>подставку</w> под мой стакан с пивом, чтобы стол оставался сухим."
                            }
                        },
                        {
                            "text": "We always use a <w>mat</w> on the dining table when serving hot dishes.",
                            "target_lang_translations": {
                                "ru": "Мы всегда используем <w>подставку</w> на обеденном столе при подаче горячих блюд."
                            }
                        }
                    ],
                    "synonyms": [
                        "coaster",
                        "placemat"
                    ],
                    "common_phrases": [
                        "place mat",
                        "beer mat",
                        "table mat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой плоский кусок материала, используемый для защиты поверхности от чего-либо горячего или шероховатого; подставка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подставка",
                                "target_lang_sense_clarification": "под кружку или тарелку"
                            },
                            {
                                "target_lang_word": "салфетка",
                                "target_lang_sense_clarification": "сервировочная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "877095f8-b78c-39d2-84e8-20033b8d446f",
                    "sense_definition": "A floor pad to protect athletes.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "covering_pad",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gymnast landed safely on the soft <w>mat</w> after her jump.",
                            "target_lang_translations": {
                                "ru": "Гимнастка благополучно приземлилась на мягкий <w>мат</w> после прыжка."
                            }
                        },
                        {
                            "text": "You should bring your own yoga <w>mat</w> to the exercise class.",

{
    "word_family": [
        "горб",
        "горбатый",
        "сгорбиться",
        "горбун",
        "пригорбиться",
        "сгорбленный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "99433c61-ffdf-35b3-85c4-99b676d98c78",
                    "sense_definition": "Сидеть или ходить с согнутой спиной, опустив плечи и голову; сутулиться.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "posture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама всегда напоминала мне, чтобы я не <w>горбился</w> за столом.",
                            "target_lang_translations": {
                                "en": "Mom always reminded me not to <w>slouch</w> at the table."
                            }
                        },
                        {
                            "text": "От усталости и тяжелой работы старик начал сильно <w>горбиться</w>.",
                            "target_lang_translations": {
                                "en": "From fatigue and hard work, the old man began to <w>stoop</w> heavily."
                            }
                        }
                    ],
                    "synonyms": [
                        "сутулиться"
                    ],
                    "antonyms": [
                        "выпрямляться",
                        "разгибаться"
                    ],
                    "common_phrases": [
                        "перестать горбиться",
                        "горбиться за столом",
                        "привычка горбиться",
                        "начать горбиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To sit or walk with a curved spine, lowering one's shoulders and head; to have poor posture."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slouch",
                                "target_lang_sense_clarification": "sit or stand with a lazy, drooping posture"
                            },
                            {
                                "target_lang_word": "stoop",
                                "target_lang_sense_clarification": "bend one's head or body forward and downward"
                            },
                            {
                                "target_lang_word": "hunch",
                                "target_lang_sense_clarification": "raise one's shoulders and bend the top of one's body forward"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94ab568d-0dc1-30b5-85ab-53b9b48b662e",
                    "sense_definition": "Становиться выпуклым, подниматься бугром или дугой (о поверхности или предмете).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старый деревянный пол начал <w>горбиться</w> от постоянной сырости.",
                            "target_lang_translations": {
                                "en": "The old wooden floor began to <w>warp</w> from the constant dampness."
                            }
                        },
                        {
                            "text": "Кошка увидела собаку и стала угрожающе <w>горбиться</w>.",
                            "target_lang_translations": {
                                "en": "The cat saw the dog and began to threateningly <w>arch</w> its back."
                            }
                        }
                    ],
                    "synonyms": [
                        "вздуваться",
                        "выгибаться"
                    ],
                    "common_phrases": [
                        "пол горбится",
                        "земля горбится"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "употребляется в разговорной речи"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become convex, to rise up in a hump or arc (referring to a surface or object)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "warp",
                                "target_lang_sense_clarification": "become bent or twisted out of shape (e.g., wood)"
                            },
                            {
                                "target_lang_word": "buckle",
                                "target_lang_sense_clarification": "bend and give way under pressure or strain"
                            },
                            {
                                "target_lang_word": "arch",
                                "target_lang_sense_clarification": "form a curved shape (e.g., a cat's back)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
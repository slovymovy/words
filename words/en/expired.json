    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7ce31781-1407-3fc6-b933-9c064e6051ac",
                    "sense_definition": "No longer valid because a fixed period of time has ended.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "validity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You cannot travel internationally with an <w>expired</w> passport.",
                            "target_lang_translations": {
                                "ru": "Вы не можете выезжать за границу с <w>просроченным</w> паспортом."
                            }
                        },
                        {
                            "text": "Please check the fridge and throw away any <w>expired</w> milk.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверь холодильник и выброси всё <w>просроченное</w> молоко."
                            }
                        }
                    ],
                    "synonyms": [
                        "invalid",
                        "lapsed",
                        "obsolete"
                    ],
                    "antonyms": [
                        "valid",
                        "current"
                    ],
                    "common_phrases": [
                        "expired passport",
                        "expired license",
                        "expired contract",
                        "expired food"
                    ],
                    "target_lang_definitions": {
                        "ru": "Больше не действительный, так как установленный срок действия истёк."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просроченный",
                                "target_lang_sense_clarification": "о документах, продуктах или лекарствах"
                            },
                            {
                                "target_lang_word": "недействительный",
                                "target_lang_sense_clarification": "о документах или соглашениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1eae9da4-985b-35f4-88e2-0ad24ec3dd22",
                    "sense_definition": "Having been breathed out from the lungs.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "breath",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>expired</w> air contains more carbon dioxide than the air we inhale.",
                            "target_lang_translations": {

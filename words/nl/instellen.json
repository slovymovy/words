{
    "word_family": [
        "de instelling"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "33c0e748-3cb6-37a4-a8e9-26aa3fcc0b9f",
                    "sense_definition": "Een apparaat, programma of systeem klaarmaken voor gebruik door de juiste opties of waarden te kiezen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "technical_adjustment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt de taal van je telefoon eenvoudig zelf <w>instellen</w>.",
                            "target_lang_translations": {
                                "ru": "Ты можешь легко сам <w>настроить</w> язык в своем телефоне."
                            }
                        },
                        {
                            "text": "Heb jij de wekker al <w>ingesteld</w> voor morgenochtend?",
                            "target_lang_translations": {
                                "ru": "Ты уже <w>установил</w> будильник на завтрашнее утро?"
                            }
                        }
                    ],
                    "synonyms": [
                        "afkondigen"
                    ],
                    "common_phrases": [
                        "een wachtwoord instellen",
                        "de temperatuur instellen",
                        "het volume instellen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подготавливать устройство, программу или систему к использованию, выбирая нужные опции или значения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настраивать",
                                "target_lang_sense_clarification": "параметры, технику"
                            },
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "значения, время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aca69288-ed6d-31a4-b517-42a7a5a78981",
                    "sense_definition": "Officieel laten beginnen of geldig maken, zoals een regel, wet, actie of onderzoek.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "official_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie gaat een groot onderzoek <w>instellen</w> naar de diefstal.",
                            "target_lang_translations": {
                                "ru": "Полиция собирается <w>начать</w> крупное расследование кражи."
                            }
                        },
                        {
                            "text": "De regering heeft nieuwe regels <w>ingesteld</w> om het verkeer veiliger te maken.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>ввело</w> новые правила, чтобы сделать дорожное движение безопаснее."
                            }
                        }
                    ],
                    "synonyms": [
                        "afkondigen",
                        "aanstellen"
                    ],
                    "common_phrases": [
                        "een onderzoek instellen",
                        "een verbod instellen",
                        "beroep instellen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально вводить в действие правило, закон или инициировать процесс (например, расследование)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вводить",
                                "target_lang_sense_clarification": "правила, законы"
                            },
                            {
                                "target_lang_word": "начинать",
                                "target_lang_sense_clarification": "расследование, действия"
                            },
                            {
                                "target_lang_word": "учреждать",
                                "target_lang_sense_clarification": "официально создавать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d69cecd3-eae9-3d1f-80ff-50b7b0c61d79",
                    "sense_definition": "Mentaal voorbereid zijn op een bepaalde situatie of een bepaalde houding hebben.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik was er niet op <w>ingesteld</w> dat het zo koud zou zijn.",
                            "target_lang_translations": {
                                "ru": "Я не был <w>настроен</w> на то, что будет так холодно."
                            }
                        },
                        {
                            "text": "Zij is heel praktisch <w>ingesteld</w> en lost problemen snel op.",
                            "target_lang_translations": {
                                "ru": "Она очень практично <w>настроена</w> и быстро решает проблемы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ergens op ingesteld zijn",
                        "sociaal ingesteld zijn",
                        "praktisch ingesteld"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть морально готовым к ситуации или иметь определенный склад ума."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть настроенным",
                                "target_lang_sense_clarification": "иметь установку, склад ума"
                            },
                            {
                                "target_lang_word": "настраиваться",
                                "target_lang_sense_clarification": "готовиться морально"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
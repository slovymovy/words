                {
                    "sense_id": "1cc80901-4e40-3de7-b035-01d514df5d28",
                    "sense_definition": "Overblijven; nog aanwezig of te doen zijn nadat een deel is verdwenen of afgerond.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "remain_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er <w>rest</w> ons niets anders dan geduldig af te wachten.",
                            "target_lang_translations": {
                                "en": "Nothing <w>remains</w> for us but to wait patiently.",
                                "ru": "Нам не <w>остаётся</w> ничего иного, кроме как терпеливо ждать."
                            }
                        },
                        {
                            "text": "Na de grote brand <w>restte</w> er van het huis alleen nog een ruïne.",
                            "target_lang_translations": {
                                "en": "After the big fire, only a ruin <w>remained</w> of the house.",
                                "ru": "После сильного пожара от дома <w>остались</w> одни руины."
                            }
                        }
                    ],
                    "synonyms": [
                        "overblijven",
                        "resteren"
                    ],
                    "common_phrases": [
                        "er rest ons",
                        "er rest nog",
                        "rest de vraag"
                    ],
                    "target_lang_definitions": {
                        "en": "To be left over; to still be present or to be done after a part has disappeared or been completed.",
                        "ru": "Оставаться; быть в наличии после того, как остальное исчезло или было использовано."
                    },

                    "synonyms": [
                        "nuta",
                        "odcień",
                        "zabarwienie"
                    ],
                    "common_phrases": [
                        "domieszka ironii",
                        "domieszka humoru",
                        "domieszka goryczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Незначительный дополнительный элемент, оттенок чувства или качества, придающий своеобразие."
                    },
                    "translations": {
                        "ru": [
                            {

{
    "word_family": [
        "sollicitant",
        "sollicitatie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8fbf91f4-282f-336d-913f-98a46f188674",
                    "sense_definition": "Zich officieel kandidaat stellen voor een baan of functie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "job_application",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga <w>solliciteren</w> bij het postkantoor.",
                            "target_lang_translations": {
                                "en": "I am going to <w>apply</w> for a job at the post office.",
                                "ru": "Я собираюсь <w>подать заявление</w> о приеме на работу на почту."
                            }
                        },
                        {
                            "text": "Hij <w>solliciteerde</w> voor de functie van programmeur.",
                            "target_lang_translations": {
                                "en": "He <w>applied</w> for the position of programmer.",
                                "ru": "Он <w>подал заявление</w> на должность программиста."
                            }
                        },
                        {
                            "text": "Heb jij al op die interessante vacature <w>gesolliciteerd</w>?",
                            "target_lang_translations": {
                                "en": "Have you already <w>applied</w> for that interesting vacancy?",
                                "ru": "Ты уже <w>откликнулся</w> на ту интересную вакансию?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "solliciteren naar een functie",
                        "solliciteren op een vacature",
                        "solliciteren bij een bedrijf"
                    ],
                    "target_lang_definitions": {
                        "en": "To officially put oneself forward as a candidate for a job or position.",
                        "ru": "Официально выдвигать свою кандидатуру на работу или должность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "apply",
                                "target_lang_sense_clarification": "for a job or position"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подавать заявление",
                                "target_lang_sense_clarification": "о приеме на работу"
                            },
                            {
                                "target_lang_word": "претендовать",
                                "target_lang_sense_clarification": "на должность"
                            },
                            {
                                "target_lang_word": "откликаться",
                                "target_lang_sense_clarification": "на вакансию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "263a1927-3d54-3132-89ba-e8e8dcb1550e",
                    "sense_definition": "Iets negatiefs uitlokken door bepaald gedrag; vragen om moeilijkheden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "provocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ben je naar klappen aan het <w>solliciteren</w>?",
                            "target_lang_translations": {
                                "en": "Are you <w>asking for</w> a beating?",
                                "ru": "Ты <w>напрашиваешься</w> на трепку?"
                            }
                        },
                        {
                            "text": "Met dat onbeleefde gedrag ben je naar problemen aan het <w>solliciteren</w>.",
                            "target_lang_translations": {
                                "en": "With that rude behavior, you are <w>asking for</w> trouble.",
                                "ru": "С таким грубым поведением ты <w>напрашиваешься</w> на неприятности."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitlokken",
                        "vragen om"
                    ],
                    "common_phrases": [
                        "solliciteren naar problemen",
                        "solliciteren naar klappen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "wordt vaak gebruikt in een dreigende of waarschuwende context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To provoke something negative through certain behavior; to ask for trouble.",
                        "ru": "Провоцировать что-то негативное своим поведением; напрашиваться на неприятности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask for",
                                "target_lang_sense_clarification": "trouble or negative consequences"
                            },
                            {
                                "target_lang_word": "look for",
                                "target_lang_sense_clarification": "trouble or a fight"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "напрашиваться",
                                "target_lang_sense_clarification": "на неприятности"
                            }
                        ]
                    }
                }
            ]
        }

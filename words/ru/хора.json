            "senses": [
                {
                    "sense_id": "98168250-e3fa-316f-83a9-f1dc0830000e",
                    "sense_definition": "Форма родительного падежа единственного числа имени собственного <w>Хор</w>",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В музее мы увидели древнюю статую египетского бога <Хора> с головой сокола.",
                            "target_lang_translations": {
                                "en": "In the museum, we saw an ancient statue of the Egyptian god <Horus> with the head of a falcon."
                            }
                        },
                        {
                            "text": "Мифология описывает борьбу <Хора> с Сетом за власть над Египтом.",
                            "target_lang_translations": {
                                "en": "Mythology describes the struggle of <Horus> with Seth for power over Egypt."
                            }
                        }
                    ],
                    "common_phrases": [
                        "глаз Хора",
                        "храм Хора"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive singular form of the proper name 'Horus'"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Horus",
                                "target_lang_sense_clarification": "Egyptian god"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bf328e2-fe09-351d-9c48-e101efae34b6",
                    "sense_definition": "Мужское личное имя",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Моего иностранного друга зовут <Хора>, и он приехал к нам учиться.",
                            "target_lang_translations": {
                                "en": "My foreign friend is named <Hora>, and he came to study with us."
                            }
                        },
                        {
                            "text": "В списке приглашённых гостей значился господин <Хора>.",
                            "target_lang_translations": {
                                "en": "Mr. <Hora> was listed among the invited guests."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Male personal name"

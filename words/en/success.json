    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b624a378-70f3-3bfd-8ac9-d3bdc814b410",
                    "sense_definition": "The achievement of one's aim or goal.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_achievement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hard work and dedication are often the key to <w>success</w>.",
                            "target_lang_translations": {
                                "ru": "Упорный труд и самоотверженность часто являются ключом к <w>успеху</w>."
                            }
                        },
                        {
                            "text": "We wish you great <w>success</w> in your new job.",
                            "target_lang_translations": {
                                "ru": "Мы желаем вам больших <w>успехов</w> на новой работе."
                            }
                        }
                    ],
                    "antonyms": [
                        "failure"
                    ],
                    "common_phrases": [
                        "key to success",
                        "achieve success",
                        "wish someone success"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достижение поставленной цели или желаемого результата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "успех",
                                "target_lang_sense_clarification": "достижение цели"
                            },
                            {
                                "target_lang_word": "удача",
                                "target_lang_sense_clarification": "благоприятный исход"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e60ae71-98c4-359a-bb5f-13f5e358fe4a",
                    "sense_definition": "One who, or that which, achieves assumed goals.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "concrete_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The surprise birthday party was a huge <w>success</w>.",
                            "target_lang_translations": {
                                "ru": "Вечеринка-сюрприз по случаю дня рождения имела огромный <w>успех</w>."
                            }
                        },
                        {
                            "text": "Her latest novel is a commercial <w>success</w> around the world.",
                            "target_lang_translations": {
                                "ru": "Её последний роман пользуется коммерческим <w>успехом</w> во всём мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "hit"
                    ],
                    "antonyms": [
                        "failure"
                    ],
                    "common_phrases": [
                        "resounding success",
                        "huge success",
                        "roaring success"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или событие, достигшие желаемого результата или признания."

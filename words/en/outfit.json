            "senses": [
                {
                    "sense_id": "d8be03fb-6128-3b58-82df-bdfe33ed8483",
                    "sense_definition": "A set of clothes worn together, usually for a specific occasion or purpose.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing_set",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a fashionable new <w>outfit</w> for the wedding.",
                            "target_lang_translations": {
                                "ru": "Она купила модный новый <w>наряд</w> на свадьбу."
                            }
                        },
                        {
                            "text": "He decided to wear a casual <w>outfit</w> to the party.",
                            "target_lang_translations": {
                                "ru": "Он решил надеть на вечеринку повседневный <w>наряд</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "fit",
                        "equip"
                    ],
                    "common_phrases": [
                        "wear an outfit",
                        "matching outfit",
                        "whole outfit",
                        "expensive outfit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплект одежды, который носят вместе, обычно для определенного случая или цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наряд",
                                "target_lang_sense_clarification": "одежда, костюм, обычно праздничная"
                            },
                            {
                                "target_lang_word": "костюм",
                                "target_lang_sense_clarification": "комплект верхней одежды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9364ab77-5b61-34aa-8cce-abb4bb474f1f",
                    "sense_definition": "A business company, firm, or organization.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He works for a small construction <w>outfit</w> in the city.",
                            "target_lang_translations": {
                                "ru": "Он работает в небольшой строительной <w>фирме</w> в городе."
                            }
                        },
                        {
                            "text": "They are a professional <w>outfit</w> that always delivers on time.",
                            "target_lang_translations": {
                                "ru": "Это профессиональная <w>компания</w>, которая всегда выполняет работу вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "fit",
                        "equip"
                    ],
                    "common_phrases": [
                        "small outfit",
                        "professional outfit",
                        "run an outfit"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to describe a business informally"

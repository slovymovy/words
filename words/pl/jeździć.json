    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "34c81719-6e93-3a08-9716-682c49a2a842",
                    "sense_definition": "Przenosić się z miejsca na miejsce przy użyciu środka transportu (np. samochodu, roweru) lub kierować nim.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój tata codziennie <jeździ> do pracy samochodem.",
                            "target_lang_translations": {
                                "ru": "Мой папа каждый день <ездит> на работу на машине."
                            }
                        },
                        {
                            "text": "Bardzo lubię <jeździć> na rowerze w weekendy.",
                            "target_lang_translations": {
                                "ru": "Я очень люблю <кататься> на велосипеде по выходным."
                            }
                        }
                    ],
                    "synonyms": [
                        "podróżować"
                    ],
                    "common_phrases": [
                        "jeździć samochodem",
                        "jeździć na rowerze",
                        "jeździć do pracy",
                        "szybko jeździć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться с места на место, используя транспортное средство (машину, велосипед) или управляя им."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ездить",
                                "target_lang_sense_clarification": "перемещаться на транспорте (регулярно или в разных направлениях)"
                            },
                            {
                                "target_lang_word": "кататься",
                                "target_lang_sense_clarification": "ездить ради удовольствия (на велосипеде, машине)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cc48570-d177-3e2d-9f65-7b73175aeb16",
                    "sense_definition": "O środkach transportu publicznego: kursować regularnie określoną trasą.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_schedule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten autobus <jeździ> tylko w dni robocze.",
                            "target_lang_translations": {
                                "ru": "Этот автобус <ходит> только по рабочим дням."
                            }
                        },
                        {
                            "text": "Tramwaje w nocy <jeżdżą> rzadziej niż w dzień.",
                            "target_lang_translations": {
                                "ru": "Трамваи ночью <ходят> реже, чем днём."
                            }
                        }
                    ],
                    "common_phrases": [
                        "autobus jeździ",
                        "pociąg jeździ",
                        "często jeździć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Об общественном транспорте: регулярно курсировать по определенному маршруту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "курсировать по маршруту (об автобусах, поездах)"
                            },
                            {
                                "target_lang_word": "ездить",
                                "target_lang_sense_clarification": "совершать рейсы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60392b92-d117-33ae-8537-418fb763a8e4",
                    "sense_definition": "Poruszać się, siedząc na grzbiecie zwierzęcia, najczęściej konia.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ona potrafi świetnie <jeździć> konno.",
                            "target_lang_translations": {
                                "ru": "Она умеет отлично <ездить> верхом."
                            }
                        },
                        {
                            "text": "W wakacje będziemy uczyć się <jeździć> na kucykach.",
                            "target_lang_translations": {
                                "ru": "На каникулах мы будем учиться <кататься> на пони."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jeździć konno",
                        "uczyć się jeździć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передвигаться верхом на животном, чаще всего на лошади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ездить верхом",
                                "target_lang_sense_clarification": "сидя на лошади"
                            },
                            {
                                "target_lang_word": "кататься",
                                "target_lang_sense_clarification": "верхом ради развлечения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "835ddcbe-0206-3d29-bb33-d96c27e1364d",
                    "sense_definition": "Употребляется для выражения сильного недовольства, гнева или раздражения кем-либо или чем-либо; дурной, доставляющий неприятности.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Куда же подевались эти <w>проклятые</w> ключи от машины?",
                            "target_lang_translations": {
                                "en": "Where have those <w>damned</w> car keys gone?"
                            }
                        },
                        {
                            "text": "Эта <w>проклятая</w> жара не даёт мне нормально работать.",
                            "target_lang_translations": {
                                "en": "This <w>damned</w> heat doesn't let me work properly."
                            }
                        }
                    ],
                    "synonyms": [
                        "дурной",
                        "мерзкий"
                    ],
                    "common_phrases": [
                        "проклятая работа",
                        "проклятый холод",
                        "проклятые деньги"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express strong dissatisfaction, anger, or irritation with someone or something; bad, causing trouble."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "damned",
                                "target_lang_sense_clarification": "expressing anger or frustration"
                            },
                            {
                                "target_lang_word": "cursed",
                                "target_lang_sense_clarification": "annoying or troublesome"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbdd6336-3582-3b9b-8aff-063bbb54f70f",
                    "sense_definition": "Ненавистный, заслуживающий проклятия; крайне неприятный.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдаты сражались против <w>проклятого</w> врага до последней капли крови.",
                            "target_lang_translations": {
                                "en": "The soldiers fought against the <w>cursed</w> enemy to the last drop of blood."
                            }
                        },
                        {
                            "text": "Он хотел забыть эту <w>проклятую</w> войну и начать новую жизнь.",
                            "target_lang_translations": {
                                "en": "He wanted to forget this <w>cursed</w> war and start a new life."
                            }
                        }
                    ],
                    "synonyms": [
                        "ненавистный",
                        "проклинаемый"
                    ],
                    "common_phrases": [
                        "проклятый враг",
                        "проклятый вопрос"
                    ],
                    "target_lang_definitions": {
                        "en": "Hateful, deserving of a curse; extremely unpleasant."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cursed",
                                "target_lang_sense_clarification": "hateful or detestable"
                            },
                            {
                                "target_lang_word": "damned",
                                "target_lang_sense_clarification": "deserving condemnation"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "84c9ec4b-cb52-3a5e-8f76-597acb5eca04",
                    "sense_definition": "Страдательное причастие прошедшего времени от глагола <w>проклясть</w>; тот, кого подвергли проклятию.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Да будет <w>проклят</w> тот день, когда я согласился на это предложение.",
                            "target_lang_translations": {
                                "en": "<w>Cursed</w> be the day when I agreed to this proposal."
                            }
                        },
                        {
                            "text": "В старинной легенде говорилось, что весь их род был <w>проклят</w> колдуном.",
                            "target_lang_translations": {
                                "en": "An ancient legend said that their entire lineage was <w>cursed</w> by a sorcerer."
                            }
                        }
                    ],
                    "antonyms": [
                        "благословлённый"
                    ],
                    "common_phrases": [
                        "будь проклят",
                        "будьте прокляты"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Passive past participle of the verb 'to curse'; one who has been subjected to a curse."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cursed",
                                "target_lang_sense_clarification": "placed under a curse"
                            },
                            {
                                "target_lang_word": "damned",
                                "target_lang_sense_clarification": "condemned"
                            }
                        ]
                    }
                }
            ]
        }
    ]

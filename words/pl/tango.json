{
    "word_family": [
        "tangowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6a78c4d7-1f49-3432-a001-cf513df433ba",
                    "sense_definition": "Taniec towarzyski pochodzący z Buenos Aires, charakteryzujący się ekspresją i bliskością partnerów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Para wirowała na parkiecie, tańcząc namiętne <w>tango</w>.",
                            "target_lang_translations": {
                                "ru": "Пара кружилась на паркете, танцуя страстное <w>танго</w>."
                            }
                        },
                        {
                            "text": "Zapisaliśmy się na kurs <w>tanga</w> dla początkujących.",
                            "target_lang_translations": {
                                "ru": "Мы записались на курс <w>танго</w> для начинающих."
                            }
                        },
                        {
                            "text": "Mówi się, że do <w>tanga</w> trzeba dwojga.",
                            "target_lang_translations": {
                                "ru": "Говорят, что для <w>танго</w> нужны двое."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tańczyć tango",
                        "tango argentyńskie",
                        "do tanga trzeba dwojga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бальный танец родом из Буэнос-Айреса, отличающийся экспрессией и близостью партнеров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танго",
                                "target_lang_sense_clarification": "танец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da52515b-0232-3e58-a8f7-d47596bea327",
                    "sense_definition": "Forma muzyczna w metrum parzystym, na której opiera się taniec o tej samej nazwie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Orkiestra zagrała słynne <w>tango</w> z lat trzydziestych.",
                            "target_lang_translations": {
                                "ru": "Оркестр сыграл знаменитое <w>танго</w> тридцатых годов."
                            }
                        },
                        {
                            "text": "Charakterystyczny rytm <w>tanga</w> rozbrzmiewał w całej sali koncertowej.",
                            "target_lang_translations": {
                                "ru": "Характерный ритм <w>танго</w> звучал во всем концертном зале."
                            }
                        }
                    ],
                    "common_phrases": [
                        "słuchać tanga",
                        "rytm tanga",
                        "polskie tango"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальная форма в четном размере, на которой основан одноименный танец."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танго",
                                "target_lang_sense_clarification": "музыка"
                            }
                        ]
                    }
                }
            ]
        }
    ]

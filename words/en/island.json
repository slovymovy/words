{
    "word_family": [
        "islander",
        "islet"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "33574c09-4c23-31d9-bd1f-1f7edefc8975",
                    "sense_definition": "A contiguous area of land, smaller than a continent, totally surrounded by water.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "land_mass",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sumatra is the second largest <w>island</w> in the East Indies and covers a vast area.",
                            "target_lang_translations": {
                                "ru": "Суматра — второй по величине <w>остров</w> в Ост-Индии, занимающий огромную территорию."
                            }
                        },
                        {
                            "text": "They dreamed of living on a tropical <w>island</w> with white sandy beaches.",
                            "target_lang_translations": {
                                "ru": "Они мечтали жить на тропическом <w>острове</w> с белыми песчаными пляжами."
                            }
                        }
                    ],
                    "synonyms": [
                        "isle"
                    ],
                    "antonyms": [
                        "mainland"
                    ],
                    "common_phrases": [
                        "desert island",
                        "tropical island",
                        "island nation",
                        "treasure island"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок суши, меньше континента, полностью окруженный водой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остров",
                                "target_lang_sense_clarification": "география"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28f4b3aa-13a9-34bc-9da2-c7680ca06693",
                    "sense_definition": "A raised area in the middle of a road to help pedestrians cross safely or to separate traffic lanes.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "traffic_infrastructure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pedestrians should wait on the <w>island</w> until the road is clear of cars.",
                            "target_lang_translations": {
                                "ru": "Пешеходы должны ждать на <w>островке</w>, пока дорога не освободится от машин."
                            }
                        },
                        {
                            "text": "The car swerved to avoid the traffic <w>island</w> in the center of the street.",
                            "target_lang_translations": {
                                "ru": "Машина вильнула, чтобы объехать <w>островок безопасности</w> в центре улицы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "traffic island"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приподнятый участок посреди дороги для безопасности пешеходов или разделения полос движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "островок безопасности",
                                "target_lang_sense_clarification": "на дороге"
                            },
                            {
                                "target_lang_word": "островок",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ecb083f-a177-34b7-8051-8e0ef98f6305",
                    "sense_definition": "A freestanding counter in a kitchen that is accessible from all sides.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She chopped vegetables on the kitchen <w>island</w> while talking to her guests.",
                            "target_lang_translations": {
                                "ru": "Она резала овощи на кухонном <w>острове</w>, беседуя с гостями."
                            }
                        },
                        {
                            "text": "The family gathered around the large granite <w>island</w> for breakfast.",
                            "target_lang_translations": {
                                "ru": "Семья собралась за завтраком вокруг большого гранитного <w>острова</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kitchen island",
                        "central island"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельно стоящий стол в центре кухни, доступный со всех сторон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кухонный остров",
                                "target_lang_sense_clarification": "мебель"
                            },
                            {
                                "target_lang_word": "остров",
                                "target_lang_sense_clarification": "элемент кухни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "142d6d7b-2522-3167-9728-17551713f586",
                    "sense_definition": "An entity or area that is distinct and isolated from its surroundings, often providing a contrast.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_isolation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The quiet library was an <w>island</w> of tranquility in the noisy, bustling city.",
                            "target_lang_translations": {
                                "ru": "Тихая библиотека была <w>островком</w> спокойствия в шумном, суетливом городе."
                            }
                        },
                        {
                            "text": "He remained an <w>island</w> of calm despite the chaos erupting around him.",
                            "target_lang_translations": {
                                "ru": "Он оставался <w>островом</w> спокойствия, несмотря на хаос, разразившийся вокруг него."
                            }
                        }
                    ],
                    "synonyms": [
                        "oasis"
                    ],
                    "common_phrases": [
                        "island of peace",
                        "island of stability",
                        "island of calm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечто обособленное и изолированное от своего окружения, часто создающее контраст."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "островок",
                                "target_lang_sense_clarification": "метафорическое"
                            },
                            {
                                "target_lang_word": "остров",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "641bffb8-6b25-3bf7-a59c-25b38beb0abb",
                    "sense_definition": "A traffic roundabout or circle.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "traffic_infrastructure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to turn right at the <w>island</w> to get to the city center.",
                            "target_lang_translations": {
                                "ru": "Вам нужно повернуть направо на <w>кольце</w>, чтобы попасть в центр города."
                            }
                        },
                        {
                            "text": "The driver was confused by the signs at the large <w>island</w> near Birmingham.",
                            "target_lang_translations": {
                                "ru": "Водитель был сбит с толку знаками на большом <w>кольце</w> недалеко от Бирмингема."
                            }
                        }
                    ],
                    "synonyms": [
                        "island"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Used in parts of the UK, particularly the West Midlands"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Круговая транспортная развязка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кольцо",
                                "target_lang_sense_clarification": "дорожное движение"
                            },
                            {
                                "target_lang_word": "круговая развязка",
                                "target_lang_sense_clarification": "официальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50ea265d-ab45-3ad7-b597-e4148136201a",
                    "sense_definition": "A superstructure on the deck of an aircraft carrier containing the command center.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain observed the flight deck operations from the carrier's <w>island</w>.",
                            "target_lang_translations": {
                                "ru": "Капитан наблюдал за операциями на летной палубе с <w>надстройки</w> авианосца."
                            }
                        },
                        {
                            "text": "Smoke rose from the <w>island</w> of the ship after the simulated attack.",
                            "target_lang_translations": {
                                "ru": "Дым поднимался от <w>надстройки</w> корабля после имитации атаки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Надстройка на палубе авианосца, в которой находится командный центр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надстройка",
                                "target_lang_sense_clarification": "морской термин"
                            },
                            {
                                "target_lang_word": "остров",
                                "target_lang_sense_clarification": "профессиональный жаргон"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "89a3c353-e56a-324f-bd7b-a7bbc68db2db",
                    "sense_definition": "Used locally to refer to a specific nearby island, such as Long Island (New York) or Vancouver Island (British Columbia).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place_name_reference",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We are planning to head out to the <w>Island</w> for a weekend of wine tasting.",
                            "target_lang_translations": {
                                "ru": "Мы планируем отправиться на <w>Остров</w> на выходные для дегустации вин."
                            }
                        },
                        {
                            "text": "Traffic on the <w>Island</w> can be terrible during the summer rush.",
                            "target_lang_translations": {
                                "ru": "Движение на <w>Острове</w> может быть ужасным во время летнего наплыва людей."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Used by locals to refer to a prominent nearby island"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется местными жителями для обозначения конкретного близлежащего острова."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Остров",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "93ae1a34-677b-3a38-8222-7d7a71811cc0",
                    "sense_definition": "To isolate or separate something as if placing it on an island.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_isolation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt <w>islanded</w> by his lack of language skills in the foreign country.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя <w>изолированным</w> из-за недостаточного знания языка в чужой стране."
                            }
                        },
                        {
                            "text": "The old farmhouse was <w>islanded</w> by the encroaching modern development.",
                            "target_lang_translations": {
                                "ru": "Старый фермерский дом оказался <w>в изоляции</w> из-за наступающей современной застройки."
                            }
                        }
                    ],
                    "synonyms": [
                        "island"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изолировать или отделять что-либо, словно помещая на остров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изолировать",
                                "target_lang_sense_clarification": "отделять от других"
                            },
                            {
                                "target_lang_word": "обособлять",
                                "target_lang_sense_clarification": "выделять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a31c7839-0294-3256-b5ff-81c67ff1d775",
                    "sense_definition": "To surround with water or make into an island.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_surrounding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy floodwaters <w>islanded</w> the houses, cutting them off from the road.",
                            "target_lang_translations": {
                                "ru": "Сильные паводковые воды <w>окружили</w> дома, отрезав их от дороги."
                            }
                        },
                        {
                            "text": "After the storm, the car was <w>islanded</w> in a deep pool of water.",
                            "target_lang_translations": {
                                "ru": "После шторма машина оказалась <w>окружена</w> глубокой лужей воды."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Окружать водой или превращать в остров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окружать водой",
                                "target_lang_sense_clarification": "физически"
                            },
                            {
                                "target_lang_word": "отрезать",
                                "target_lang_sense_clarification": "водой или препятствием"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
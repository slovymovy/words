{
    "word_family": [
        "terminal",
        "terminate",
        "termination"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a09af915-9fd0-3547-99ba-32cb5bc2c2e2",
                    "sense_definition": "The final station or stop at the end of a transportation line, such as a railway or bus route.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "transport_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train arrived at the <w>terminus</w> five minutes behind schedule.",
                            "target_lang_translations": {
                                "ru": "Поезд прибыл на <w>конечную станцию</w> с опозданием на пять минут."
                            }
                        },
                        {
                            "text": "Passengers must disembark when the bus reaches the <w>terminus</w>.",
                            "target_lang_translations": {
                                "ru": "Пассажиры должны выйти, когда автобус доедет до <w>конечной остановки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bus terminus",
                        "railway terminus",
                        "northern terminus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конечная станция или остановка в конце транспортного маршрута, например, железнодорожного или автобусного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конечная станция",
                                "target_lang_sense_clarification": "железная дорога, метро"
                            },
                            {
                                "target_lang_word": "конечная остановка",
                                "target_lang_sense_clarification": "автобус, трамвай"
                            },
                            {
                                "target_lang_word": "вокзал",
                                "target_lang_sense_clarification": "крупный транспортный узел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bad7553-af41-32c2-843a-0763ac763425",
                    "sense_definition": "The end or final point of something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hiked all the way to the <w>terminus</w> of the glacier.",
                            "target_lang_translations": {
                                "ru": "Мы дошли пешком до самого <w>конца</w> ледника."
                            }
                        },
                        {
                            "text": "The <w>terminus</w> of the project was marked by a final report.",
                            "target_lang_translations": {
                                "ru": "<w>Завершение</w> проекта было ознаменовано итоговым отчетом."
                            }
                        }
                    ],
                    "antonyms": [
                        "origin"
                    ],
                    "common_phrases": [
                        "reached the terminus",
                        "terminus of the glacier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конец или завершающая точка чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конечная точка",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "конец",
                                "target_lang_sense_clarification": "пространственный или временной предел"
                            },
                            {
                                "target_lang_word": "завершение",
                                "target_lang_sense_clarification": "окончание процесса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abc8eac4-f7fe-3ad9-81da-a3844d1bd7bc",
                    "sense_definition": "A post or stone used to mark a boundary or border.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "boundary_marker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "An ancient stone <w>terminus</w> marked the edge of the Roman estate.",
                            "target_lang_translations": {
                                "ru": "Древний каменный <w>межевой знак</w> отмечал край римского поместья."
                            }
                        },
                        {
                            "text": "The surveyor located the old <w>terminus</w> buried in the field.",
                            "target_lang_translations": {
                                "ru": "Землемер обнаружил старый <w>пограничный камень</w>, зарытый в поле."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Столб или камень, используемый для обозначения границы или предела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "межевой знак",
                                "target_lang_sense_clarification": "знак границы земельного участка"
                            },
                            {
                                "target_lang_word": "пограничный камень",
                                "target_lang_sense_clarification": "камень на границе"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "96c4990d-f55f-3785-b00d-ca7cab5d7acb",
                    "sense_definition": "The Roman god who presides over boundaries and landmarks.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "deity",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "The Romans offered sacrifices to <w>Terminus</w> to ensure peace with their neighbors.",
                            "target_lang_translations": {
                                "ru": "Римляне приносили жертвы <w>Термину</w>, чтобы обеспечить мир с соседями."
                            }
                        },
                        {
                            "text": "A statue of <w>Terminus</w> stood at the corner of the field.",
                            "target_lang_translations": {
                                "ru": "Статуя <w>Термина</w> стояла на углу поля."
                            }
                        }
                    ],
                    "synonyms": [
                        "terminal figure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Римский бог, покровительствующий границам и межевым знакам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Термин",
                                "target_lang_sense_clarification": "имя божества"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
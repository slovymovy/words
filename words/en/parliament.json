                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legislature_group",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "The Prime Minister will address <w>Parliament</w> regarding the new policy tomorrow.",
                            "target_lang_translations": {
                                "ru": "Завтра премьер-министр выступит перед <w>Парламентом</w> по поводу новой политики."

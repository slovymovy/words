                    "sense_id": "d78b16ab-4985-3e3f-9b5c-675170b7c2a3",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Screen</w> has been working at the firm for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Скрин</w> работает в фирме уже более двадцати лет."
                            }
                        },
                        {
                            "text": "I saw the name <w>Screen</w> listed in the directory of local artists.",
                            "target_lang_translations": {
                                "ru": "Я видел фамилию <w>Скрин</w> в справочнике местных художников."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "rare"
                        }
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "shaver",
        "shaving",
        "aftershave",
        "unshaven"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ac29d7c8-db3a-3186-bef1-3e5eda0d4e45",
                    "sense_definition": "To remove hair from the body, especially the face, using a razor.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "hair_removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>shaves</w> every morning before he goes to work.",
                            "target_lang_translations": {
                                "ru": "Он <w>бреется</w> каждое утро перед уходом на работу."
                            }
                        },
                        {
                            "text": "I usually <w>shave</w> after taking a hot shower.",
                            "target_lang_translations": {
                                "ru": "Я обычно <w>бреюсь</w> после горячего душа."
                            }
                        }
                    ],
                    "antonyms": [
                        "grow"
                    ],
                    "common_phrases": [
                        "shave daily",
                        "shave carefully"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалять волосы с тела, особенно с лица, используя бритву."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бриться",
                                "target_lang_sense_clarification": "удалять волосы у себя"
                            },
                            {
                                "target_lang_word": "брить",
                                "target_lang_sense_clarification": "удалять волосы"
                            },
                            {
                                "target_lang_word": "побриться",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79b95e1b-f56d-3362-a30d-d3afe5399894",
                    "sense_definition": "To cut hair close to the skin from a specific body part.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hair_removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The barber <w>shaved</w> the customer's head completely bald.",
                            "target_lang_translations": {
                                "ru": "Парикмахер <w>побрил</w> голову клиента налысо."
                            }
                        },
                        {
                            "text": "She decided to <w>shave</w> her legs before going to the beach.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>побрить</w> ноги перед походом на пляж."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shave one's head",
                        "shave one's legs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Срезать волосы близко к коже с определенной части тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брить",
                                "target_lang_sense_clarification": "кого-то или часть тела"
                            },
                            {
                                "target_lang_word": "побрить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "238cf5a5-e49a-398b-8ebd-09c17b3584fe",
                    "sense_definition": "To reduce something by a small amount, such as time, price, or quantity.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reduce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The runner managed to <w>shave</w> two seconds off the world record.",
                            "target_lang_translations": {
                                "ru": "Бегуну удалось <w>сбросить</w> две секунды с мирового рекорда."
                            }
                        },
                        {
                            "text": "The company is trying to <w>shave</w> costs to increase profits.",
                            "target_lang_translations": {
                                "ru": "Компания пытается <w>урезать</w> расходы, чтобы увеличить прибыль."
                            }
                        }
                    ],
                    "synonyms": [
                        "ching",
                        "chef"
                    ],
                    "antonyms": [
                        "increase"
                    ],
                    "common_phrases": [
                        "shave off",
                        "shave costs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшать что-либо на небольшое количество, например время, цену или количество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срезать",
                                "target_lang_sense_clarification": "уменьшить (время, дистанцию)"
                            },
                            {
                                "target_lang_word": "урезать",
                                "target_lang_sense_clarification": "сократить (расходы)"
                            },
                            {
                                "target_lang_word": "сбросить",
                                "target_lang_sense_clarification": "уменьшить значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84f748a4-5083-30ca-ab9a-ac68c7d00cac",
                    "sense_definition": "To cut thin slices from a material, such as wood or food.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cut_thin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>shave</w> some parmesan cheese over the pasta.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>настрогать</w> немного пармезана в пасту."
                            }
                        },
                        {
                            "text": "He used a sharp tool to <w>shave</w> the wood into shape.",
                            "target_lang_translations": {
                                "ru": "Он использовал острый инструмент, чтобы <w>обстрогать</w> дерево и придать ему форму."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shave wood",
                        "shave cheese"
                    ],
                    "target_lang_definitions": {
                        "ru": "Срезать тонкие слои с материала, такого как дерево или еда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строгать",
                                "target_lang_sense_clarification": "дерево"
                            },
                            {
                                "target_lang_word": "настрогать",
                                "target_lang_sense_clarification": "еду или дерево тонкими слоями"
                            },
                            {
                                "target_lang_word": "срезать",
                                "target_lang_sense_clarification": "тонкий слой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33e584ca-e61c-3000-80d7-e5ef731e2575",
                    "sense_definition": "To pass very close to something, almost touching it.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "pass_close",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bird flew low and <w>shaved</w> the surface of the water.",
                            "target_lang_translations": {
                                "ru": "Птица летела низко и <w>задела</w> поверхность воды."
                            }
                        },
                        {
                            "text": "The speeding car <w>shaved</w> the edge of the wall but did not crash.",
                            "target_lang_translations": {
                                "ru": "Мчащаяся машина <w>задела</w> край стены, но не разбилась."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shave past"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить очень близко к чему-либо, почти касаясь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задеть",
                                "target_lang_sense_clarification": "коснуться при движении"
                            },
                            {
                                "target_lang_word": "пройти вплотную",
                                "target_lang_sense_clarification": "без касания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b30fc7c-2ec4-3e2b-be7e-974881c14543",
                    "sense_definition": "To stab or cut someone with a knife.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gang member threatened to <w>shave</w> his rival during the fight.",
                            "target_lang_translations": {
                                "ru": "Член банды угрожал <w>порезать</w> своего соперника во время драки."
                            }
                        },
                        {
                            "text": "He was afraid he might get <w>shaved</w> if he walked down that alley alone.",
                            "target_lang_translations": {
                                "ru": "Он боялся, что его могут <w>пырнуть</w>, если он пойдет по этому переулку один."
                            }
                        }
                    ],
                    "synonyms": [
                        "stab",
                        "shank",
                        "shiv"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Used in street slang, particularly MLE."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пырнуть или порезать кого-то ножом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порезать",
                                "target_lang_sense_clarification": "нанести рану"
                            },
                            {
                                "target_lang_word": "пырнуть",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9f01064e-eeea-3023-8b47-3bc0df3f0a23",
                    "sense_definition": "An act of removing hair with a razor.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hair_removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need a good <w>shave</w> before the wedding.",
                            "target_lang_translations": {
                                "ru": "Мне нужно хорошо <w>побриться</w> перед свадьбой."
                            }
                        },
                        {
                            "text": "He went to the barber for a haircut and a <w>shave</w>.",
                            "target_lang_translations": {
                                "ru": "Он пошел к парикмахеру на стрижку и <w>бритьё</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have a shave",
                        "need a shave"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс удаления волос бритвой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бритьё",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2e0a902-788f-3115-bc59-031ee865c688",
                    "sense_definition": "A situation where danger or failure is barely avoided.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pass_close",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We had a close <w>shave</w> when the truck almost hit our car.",
                            "target_lang_translations": {
                                "ru": "Мы были <w>на волосок</w> от гибели, когда грузовик почти врезался в нашу машину."
                            }
                        },
                        {
                            "text": "It was a close <w>shave</w>, but we managed to catch the last train.",
                            "target_lang_translations": {
                                "ru": "Это было <w>на грани</w>, но мы успели на последний поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "jook"
                    ],
                    "common_phrases": [
                        "close shave"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, когда опасности или неудачи едва удалось избежать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на волосок",
                                "target_lang_sense_clarification": "в идиоме 'close shave'"
                            },
                            {
                                "target_lang_word": "на грани",
                                "target_lang_sense_clarification": "рискованная ситуация"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "143fd5a9-19c4-38fd-b76f-290f26501e06",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Shave</w> has lived in this village for fifty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Шейв</w> живет в этой деревне уже пятьдесят лет."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <w>Shave</w> at the conference.",
                            "target_lang_translations": {
                                "ru": "Я встретил женщину по имени Сара <w>Шейв</w> на конференции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Шейв",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
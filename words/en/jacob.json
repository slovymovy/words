{
    "word_family": [
        "Jacobian",
        "Jacobite",
        "Jacobean"
    ],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "ad3c5957-bc15-3901-9c6c-5038e2ae43a5",
                    "sense_definition": "A male given name from Hebrew.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My brother is named <w>Jacob</w>.",
                            "target_lang_translations": {
                                "ru": "Моего брата зовут <w>Джейкоб</w>."
                            }
                        },
                        {
                            "text": "We are waiting for <w>Jacob</w> to arrive at the party.",
                            "target_lang_translations": {
                                "ru": "Мы ждем, когда <w>Джейкоб</w> придет на вечеринку."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя еврейского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Джейкоб",
                                "target_lang_sense_clarification": "транслитерация английского имени"
                            },
                            {
                                "target_lang_word": "Яков",
                                "target_lang_sense_clarification": "русский эквивалент имени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f88d2a68-bb5c-30c5-b421-dde398fc4bc6",
                    "sense_definition": "The biblical patriarch who was the son of Isaac and Rebecca, the twin brother of Esau, and the father of the twelve tribes of Israel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "In the story, <w>Jacob</w> tricked his brother out of his birthright.",
                            "target_lang_translations": {
                                "ru": "В этой истории <w>Иаков</w> хитростью лишил брата первородства."
                            }
                        },
                        {
                            "text": "The twelve sons of <w>Jacob</w> became the ancestors of the Israelites.",
                            "target_lang_translations": {
                                "ru": "Двенадцать сыновей <w>Иакова</w> стали прародителями израильтян."
                            }
                        }
                    ],
                    "synonyms": [
                        "Israel",
                        "Yaakov"
                    ],
                    "common_phrases": [
                        "God of Jacob",
                        "Jacob's ladder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Библейский патриарх, сын Исаака и Ревекки, брат-близнец Исава и отец двенадцати колен Израилевых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Иаков",
                                "target_lang_sense_clarification": "библейский персонаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b09a3637-8a33-3c13-b384-c6facec0a071",
                    "sense_definition": "A prophet in the Book of Mormon who was the fifth son of Lehi.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "<w>Jacob</w> taught the people in the temple about faith.",
                            "target_lang_translations": {
                                "ru": "<w>Иаков</w> учил людей в храме вере."
                            }
                        },
                        {
                            "text": "The writings of <w>Jacob</w> are preserved on the plates.",
                            "target_lang_translations": {
                                "ru": "Писания <w>Иакова</w> сохранились на пластинах."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пророк из Книги Мормона, пятый сын Легия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Иаков",
                                "target_lang_sense_clarification": "пророк"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f6d21735-08cc-30ac-9534-3f560ab9e81c",
                    "sense_definition": "A breed of sheep characterized by having multiple horns and spotted wool.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer keeps a small flock of <w>Jacobs</w> in the pasture.",
                            "target_lang_translations": {
                                "ru": "Фермер держит на пастбище небольшое стадо <w>овец породы Джейкоб</w>."
                            }
                        },
                        {
                            "text": "A <w>Jacob</w> can have two, four, or even six horns.",
                            "target_lang_translations": {
                                "ru": "<w>Овца породы Джейкоб</w> может иметь два, четыре или даже шесть рогов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Порода овец, отличающаяся наличием нескольких рогов и пятнистой шерстью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "овца породы Джейкоб",
                                "target_lang_sense_clarification": "порода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5823bc6-458f-38a8-bdba-9641caaad94c",
                    "sense_definition": "A ladder.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He grabbed the <w>Jacob</w> to climb up to the roof.",
                            "target_lang_translations": {
                                "ru": "Он схватил <w>лестницу</w>, чтобы залезть на крышу."
                            }
                        },
                        {
                            "text": "Put the <w>Jacob</w> away in the shed when you are done.",
                            "target_lang_translations": {
                                "ru": "Убери <w>лестницу</w> в сарай, когда закончишь."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "old slang for a ladder"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лестница."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лестница",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
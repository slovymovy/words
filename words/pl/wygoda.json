                {
                    "sense_id": "d0adad54-8997-3d83-9d67-d35c509aae49",
                    "sense_definition": "Stan fizycznego odprężenia, braku trudu i przyjemności wynikający z dobrych warunków otoczenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "comfort_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy fotel zapewnia niesamowitą <w>wygodę</w> podczas oglądania telewizji.",
                            "target_lang_translations": {
                                "ru": "Это новое кресло обеспечивает невероятный <w>комфорт</w> при просмотре телевизора."
                            }
                        },
                        {
                            "text": "Dla własnej <w>wygody</w> postanowiłem zamówić taksówkę zamiast iść pieszo.",
                            "target_lang_translations": {
                                "ru": "Ради собственного <w>удобства</w> я решил заказать такси, вместо того чтобы идти пешком."
                            }
                        }
                    ],
                    "synonyms": [
                        "komfort",
                        "dobrobyt"
                    ],
                    "antonyms": [
                        "niewygoda",
                        "dyskomfort"
                    ],
                    "common_phrases": [
                        "dla własnej wygody",
                        "cenić sobie wygodę",
                        "poczucie wygody"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние физического расслабления, отсутствия трудностей и удовольствия, обусловленное хорошими условиями окружающей среды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комфорт",
                                "target_lang_sense_clarification": "физическое благополучие"
                            },
                            {
                                "target_lang_word": "удобство",
                                "target_lang_sense_clarification": "комфортные условия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b49653fa-eaf5-3f3a-9480-80325736d734",
                    "sense_definition": "Cecha tego, co jest praktyczne, łatwe w użyciu i ułatwia wykonywanie czynności lub oszczędza czas.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "convenience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zakupy przez Internet to nie tylko <w>wygoda</w>, ale i duża oszczędność czasu.",
                            "target_lang_translations": {
                                "ru": "Покупки через интернет — это не только <w>удобство</w>, но и большая экономия времени."
                            }
                        },
                        {
                            "text": "Dużą <w>wygodą</w> tego hotelu jest jego lokalizacja blisko centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Большим <w>удобством</w> этого отеля является его расположение недалеко от центра города."
                            }
                        }
                    ],
                    "synonyms": [

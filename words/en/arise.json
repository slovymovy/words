        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8eee9785-a3d0-3733-b66b-d880a3da4d78",
                    "sense_definition": "To begin to exist, happen, or become apparent.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Problems can <w>arise</w> unexpectedly, but we need to be prepared for them.",
                            "target_lang_translations": {
                                "ru": "Проблемы могут <w>возникнуть</w> неожиданно, но мы должны быть к ним готовы."
                            }
                        },
                        {
                            "text": "A new challenge will <w>arise</w> every time you think you've solved the last one.",
                            "target_lang_translations": {
                                "ru": "Новая проблема будет <w>возникать</w> каждый раз, когда вы будете думать, что решили предыдущую."
                            }
                        },
                        {
                            "text": "Serious questions may <w>arise</w> regarding the safety of the new procedure.",
                            "target_lang_translations": {
                                "ru": "Могут <w>возникнуть</w> серьёзные вопросы касательно безопасности новой процедуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "occur",
                        "happen",
                        "emerge",
                        "appear",
                        "come about",
                        "spring",
                        "come into being",
                        "come to pass",
                        "pop up",
                        "surface"
                    ],
                    "common_phrases": [
                        "problems arise",
                        "questions arise",
                        "if the need arises",
                        "situation arises",
                        "conflict arises"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать существовать, случаться или становиться очевидным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возникать",
                                "target_lang_sense_clarification": "появляться, начинаться (о проблемах, вопросах)"
                            },
                            {
                                "target_lang_word": "появляться",
                                "target_lang_sense_clarification": "становиться видимым или известным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e221207-248a-37ee-ad1e-1b96b8762754",
                    "sense_definition": "To get out of bed; to stand up after sleeping or resting.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>arose</w> early in the morning to prepare for the long journey.",
                            "target_lang_translations": {
                                "ru": "Он <w>встал</w> рано утром, чтобы подготовиться к долгому путешествию."
                            }
                        },
                        {
                            "text": "The villagers <w>arose</w> at dawn to begin their daily work in the fields.",
                            "target_lang_translations": {
                                "ru": "Жители деревни <w>встали</w> на рассвете, чтобы начать свою повседневную работу в полях."
                            }
                        }
                    ],
                    "synonyms": [
                        "awaken",
                        "rise"
                    ],
                    "common_phrases": [
                        "arise early",
                        "arise at dawn"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вставать с постели; подниматься на ноги после сна или отдыха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вставать",
                                "target_lang_sense_clarification": "подниматься с постели"
                            },
                            {
                                "target_lang_word": "просыпаться",
                                "target_lang_sense_clarification": "пробуждаться ото сна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db1d2877-3e16-3216-b16e-249624506502",
                    "sense_definition": "To stand up or move upward from a lower position.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knight slowly <w>arose</w> from his kneeling posture to address the king.",
                            "target_lang_translations": {
                                "ru": "Рыцарь медленно <w>поднялся</w> с колен, чтобы обратиться к королю."
                            }
                        },
                        {
                            "text": "Thick smoke began to <w>arise</w> from the chimney as the fire grew.",
                            "target_lang_translations": {
                                "ru": "Густой дым начал <w>подниматься</w> из трубы, когда огонь разгорелся."
                            }
                        }
                    ],
                    "synonyms": [

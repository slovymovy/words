{
    "word_family": [
        "kożuch",
        "kożuszkarz",
        "kożuchowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "db371c91-4dec-37ba-b560-5b3ad4cde3a8",
                    "sense_definition": "Zdrobnienie od słowa <w>kożuch</w>; małe lub krótkie okrycie wierzchnie uszyte z owczej skóry z futrem do wewnątrz, często noszone przez dzieci.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ile ma pan te dziecięce <w>kożuszki</w> z owczej skóry?",
                            "target_lang_translations": {
                                "ru": "Почём у вас эти детские <w>дублёнки</w> из овчины?"
                            }
                        },
                        {
                            "text": "Na zimę kupiłam córce ciepły <w>kożuszek</w> z kapturem.",
                            "target_lang_translations": {
                                "ru": "На зиму я купила дочери тёплый <w>полушубок</w> с капюшоном."
                            }
                        }
                    ],
                    "synonyms": [
                        "futerko"
                    ],
                    "common_phrases": [
                        "ciepły kożuszek",
                        "dziecięcy kożuszek",
                        "wełniany kożuszek"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова «kożuch»; короткая верхняя одежда из овчины мехом внутрь, часто носимая детьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полушубок",
                                "target_lang_sense_clarification": "короткая шуба"
                            },
                            {
                                "target_lang_word": "дублёнка",
                                "target_lang_sense_clarification": "одежда из дублёной овчины"
                            },
                            {
                                "target_lang_word": "тулупчик",
                                "target_lang_sense_clarification": "уменьшительное от тулуп"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd67465e-27ee-3b48-bf03-929326068915",
                    "sense_definition": "Cienka warstwa zastygłego tłuszczu i białka, która tworzy się na powierzchni gorącego mleka lub kakao podczas stygnięcia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "food_texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zdejmij, proszę, Stasiowi ten <w>kożuszek</w> z mleka.",
                            "target_lang_translations": {
                                "ru": "Сними, пожалуйста, Стасю эту <w>пенку</w> с молока."
                            }
                        },
                        {
                            "text": "Wiele dzieci nie lubi pić kakao, gdy pływa w nim <w>kożuszek</w>.",
                            "target_lang_translations": {
                                "ru": "Многие дети не любят пить какао, когда в нём плавает <w>пенка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "skórka"
                    ],
                    "common_phrases": [
                        "kożuszek na mleku",
                        "kakao z kożuszkiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий слой застывшего жира и белка, образующийся на поверхности горячего молока или какао при остывании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пенка",
                                "target_lang_sense_clarification": "на молоке или какао"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df3bd019-56f2-31da-aad3-f6f055ad0178",
                    "sense_definition": "Potoczna nazwa jadalnego grzyba z gatunku pieczarka biaława.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas spaceru po łące znaleźliśmy rzadki <w>kożuszek</w>.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки по лугу мы нашли редкий <w>шампиньон полевой</w>."
                            }
                        },
                        {
                            "text": "Ten grzyb to <w>kożuszek</w>, który jest bardzo smaczny po usmażeniu.",
                            "target_lang_translations": {
                                "ru": "Этот гриб — <w>шампиньон полевой</w>, который очень вкусен в жареном виде."
                            }
                        }
                    ],
                    "synonyms": [
                        "pieczarka biaława"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "biological name"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название съедобного гриба вида шампиньон полевой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шампиньон полевой",
                                "target_lang_sense_clarification": "вид гриба"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3d2f4580-b60d-3f36-9017-1317b1058f31",
                    "sense_definition": "Licht of warmte uitzenden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emit_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zon <w>straalt</w> vandaag heel fel aan de hemel.",
                            "target_lang_translations": {
                                "ru": "Солнце сегодня очень ярко <w>светит</w> в небе.",
                                "en": "The sun is <w>shining</w> very brightly in the sky today."
                            }
                        },
                        {
                            "text": "De open haard <w>straalde</w> een aangename warmte uit in de kamer.",
                            "target_lang_translations": {
                                "ru": "Камин <w>излучал</w> приятное тепло в комнату.",
                                "en": "The fireplace <w>radiated</w> a pleasant warmth into the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakken",
                        "zakken"
                    ],
                    "common_phrases": [
                        "de zon straalt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Излучать свет или тепло.",
                        "en": "To emit light or heat."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сиять",
                                "target_lang_sense_clarification": "ярко светить"
                            },
                            {
                                "target_lang_word": "светить",
                                "target_lang_sense_clarification": "испускать свет"
                            },
                            {
                                "target_lang_word": "излучать",
                                "target_lang_sense_clarification": "испускать (тепло или свет)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "shine",
                                "target_lang_sense_clarification": "to emit light"
                            },
                            {
                                "target_lang_word": "radiate",
                                "target_lang_sense_clarification": "to emit heat or light"
                            },
                            {
                                "target_lang_word": "beam",
                                "target_lang_sense_clarification": "to shine brightly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4cbccb6f-ea62-3760-a79d-dd16c0fbc878",
                    "sense_definition": "Een uitdrukking van grote vreugde of geluk op het gezicht hebben.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bruid <w>straalde</w> van geluk op haar trouwdag.",
                            "target_lang_translations": {
                                "ru": "Невеста <w>сияла</w> от счастья в день своей свадьбы.",
                                "en": "The bride <w>beamed</w> with happiness on her wedding day."
                            }
                        },
                        {
                            "text": "Hij <w>straalt</w> helemaal als hij over zijn nieuwe baan vertelt.",
                            "target_lang_translations": {
                                "ru": "Он весь <w>светится</w>, когда рассказывает о своей новой работе.",
                                "en": "He <w>beams</w> all over when he talks about his new job."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakken",
                        "zakken"
                    ],
                    "common_phrases": [
                        "stralen van geluk",
                        "stralen van trots",
                        "stralen van plezier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь выражение большой радости или счастья на лице.",
                        "en": "To show great joy or happiness through one's expression."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сиять",
                                "target_lang_sense_clarification": "выглядеть очень счастливым"
                            },
                            {
                                "target_lang_word": "светиться",
                                "target_lang_sense_clarification": "излучать радость"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "beam",
                                "target_lang_sense_clarification": "to smile radiantly"
                            },
                            {
                                "target_lang_word": "radiate",
                                "target_lang_sense_clarification": "to project an emotion"
                            },
                            {
                                "target_lang_word": "glow",
                                "target_lang_sense_clarification": "to look happy and healthy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10dd2a7c-cb4e-366b-a31c-73b945b21718",
                    "sense_definition": "Met kracht in een straal naar buiten spuiten (van vloeistof).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "liquid_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water <w>straalde</w> uit de kapotte leiding de straat op.",
                            "target_lang_translations": {
                                "ru": "Вода <w>била струёй</w> из сломанной трубы на улицу.",
                                "en": "The water <w>spurted</w> out of the broken pipe onto the street."
                            }
                        },
                        {
                            "text": "Er <w>straalde</w> bloed uit de diepe wond.",
                            "target_lang_translations": {
                                "ru": "Кровь <w>хлестала</w> из глубокой раны.",
                                "en": "Blood <w>gushed</w> from the deep wound."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakken",
                        "zakken"
                    ],
                    "target_lang_definitions": {
                        "ru": "С силой вырываться наружу струей (о жидкости).",
                        "en": "To squirt or flow out forcibly in a stream (of liquid)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить струёй",
                                "target_lang_sense_clarification": "вытекать под напором"
                            },
                            {
                                "target_lang_word": "брызгать",
                                "target_lang_sense_clarification": "разлетаться каплями или струей"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "spurt",
                                "target_lang_sense_clarification": "to gush out suddenly"
                            },
                            {
                                "target_lang_word": "jet",
                                "target_lang_sense_clarification": "to flow out with force"
                            },
                            {
                                "target_lang_word": "gush",
                                "target_lang_sense_clarification": "to flow out rapidly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f8ff346-1243-36b7-9be5-b6a83e00a7b8",
                    "sense_definition": "Niet slagen voor een examen of toets.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "exam_fail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is helaas <w>gestraald</w> voor zijn eindexamen wiskunde.",
                            "target_lang_translations": {
                                "ru": "К сожалению, он <w>провалился</w> на выпускном экзамене по математике.",
                                "en": "Unfortunately, he <w>failed</w> his final math exam."
                            }
                        },
                        {
                            "text": "Als ik niet beter studeer, ga ik zeker <w>stralen</w>.",
                            "target_lang_translations": {
                                "ru": "Если я не буду лучше учиться, я точно <w>провалюсь</w>.",
                                "en": "If I don't study better, I'm definitely going to <w>flunk</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakken"
                    ],
                    "antonyms": [
                        "slagen"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "studententaal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не сдать экзамен или тест (сленг).",
                        "en": "To fail an exam or test (informal)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провалиться",
                                "target_lang_sense_clarification": "не сдать экзамен"
                            },
                            {
                                "target_lang_word": "завалить",
                                "target_lang_sense_clarification": "не сдать (разговорное)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fail",
                                "target_lang_sense_clarification": "to not pass"
                            },
                            {
                                "target_lang_word": "flunk",
                                "target_lang_sense_clarification": "to fail (informal)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "61f3a4d2-32da-3f3a-85f2-ce43312005a9",
                    "sense_definition": "Podejmować działania w celu odnalezienia kogoś lub czegoś, co zginęło lub jest ukryte.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "search_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Od godziny bezskutecznie <w>szukam</w> moich okularów.",
                            "target_lang_translations": {
                                "ru": "Я уже час безуспешно <w>ищу</w> свои очки."
                            }
                        },
                        {
                            "text": "Marek stracił posadę i teraz musi <w>szukać</w> nowej pracy.",
                            "target_lang_translations": {
                                "ru": "Марек потерял должность, и теперь он должен <w>искать</w> новую работу."
                            }
                        },
                        {
                            "text": "Czy możesz mi pomóc <w>szukać</w> kluczy do samochodu?",
                            "target_lang_translations": {
                                "ru": "Ты можешь помочь мне <w>искать</w> ключи от машины?"
                            }
                        }
                    ],
                    "synonyms": [
                        "poszukiwać"
                    ],
                    "antonyms": [
                        "znaleźć",
                        "znajdować"
                    ],
                    "common_phrases": [
                        "szukać pracy",
                        "szukać pomocy",
                        "szukać kluczy",
                        "szukać rozwiązania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предпринимать действия с целью найти кого-то или что-то, что пропало или спрятано."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "кого-либо или что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70372efa-014c-3e75-b92c-eddabb9a1f25",
                    "sense_definition": "Próbować odnaleźć się nawzajem (o dwóch lub więcej osobach).",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "search_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zgubiliśmy się w lesie i <w>szukaliśmy</w> się przez godzinę.",
                            "target_lang_translations": {
                                "ru": "Мы потерялись в лесу и <w>искали</w> друг друга целый час."
                            }
                        },
                        {
                            "text": "Podczas koncertu przyjaciele <w>szukali</w> się wzrokiem w tłumie.",
                            "target_lang_translations": {
                                "ru": "Во время концерта друзья <w>искали</w> друг друга взглядом в толпе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szukać się wzrokiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пытаться найти друг друга (о двух или более лицах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искать друг друга",
                                "target_lang_sense_clarification": "взаимное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c32a0b57-af5a-3892-94a8-461013942961",
                    "sense_definition": "Dążyć do osiągnięcia lub zdobycia czegoś niematerialnego, np. prawdy, sprawiedliwości lub sławy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "search_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu filozofów przez całe życie <w>szuka</w> prawdy o świecie.",
                            "target_lang_translations": {
                                "ru": "Многие философы всю жизнь <w>ищут</w> истину о мире."
                            }
                        },
                        {
                            "text": "Nie warto <w>szukać</w> zemsty, lepiej skupić się na przyszłości.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>искать</w> мести, лучше сосредоточиться на будущем."
                            }
                        },
                        {
                            "text": "Młodzi artyści często wyjeżdżają do stolicy, by <w>szukać</w> sławy.",
                            "target_lang_translations": {
                                "ru": "Молодые художники часто уезжают в столицу, чтобы <w>искать</w> славы."
                            }
                        }
                    ],
                    "synonyms": [
                        "poszukiwać"
                    ],
                    "common_phrases": [
                        "szukać prawdy",
                        "szukać zemsty",
                        "szukać sławy",
                        "szukać inspiracji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стремиться достичь или обрести что-то нематериальное, например, правду, справедливость или славу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "абстрактные понятия (истину, славу)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3c85922-e442-3978-a734-7197ac07d797",
                    "sense_definition": "Zachowywać się w sposób prowokujący, narażając się na kłopoty lub konflikt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "search_idiomatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie odzywaj się do nich, chyba że <w>szukasz</w> guza.",
                            "target_lang_translations": {
                                "ru": "Не разговаривай с ними, если не <w>ищешь</w> неприятностей."
                            }
                        },
                        {
                            "text": "Zamiast cieszyć się z sukcesu, on ciągle <w>szuka</w> dziury w całym.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы радоваться успеху, он постоянно <w>ищет</w> недостатки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szukać guza",
                        "szukać kłopotów",
                        "szukać dziury w całym",
                        "szukać zaczepki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in idiomatic expressions"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя провокационно, навлекая на себя неприятности или конфликт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "неприятностей (в идиомах)"
                            },
                            {
                                "target_lang_word": "нарываться",
                                "target_lang_sense_clarification": "разг. на неприятности"
                            }
                        ]
                    }
                }
            ]
        }

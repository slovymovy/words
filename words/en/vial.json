            "senses": [
                {
                    "sense_id": "34619ca5-08eb-37ea-96d6-ec7d176fb4c5",
                    "sense_definition": "A small glass or plastic bottle, especially a tube-shaped one used to store medicine, perfume, or chemicals.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nurse drew the vaccine dose from a sterile glass <w>vial</w>.",
                            "target_lang_translations": {
                                "ru": "Медсестра набрала дозу вакцины из стерильного стеклянного <w>флакона</w>."
                            }
                        },
                        {
                            "text": "She kept a small <w>vial</w> of essential oil in her travel bag.",
                            "target_lang_translations": {
                                "ru": "Она хранила маленький <w>пузырёк</w> с эфирным маслом в своей дорожной сумке."
                            }
                        }
                    ],
                    "synonyms": [
                        "phial",
                        "flask"
                    ],
                    "antonyms": [
                        "ampoule"
                    ],
                    "common_phrases": [
                        "glass vial",
                        "sterile vial",
                        "vial of blood",
                        "plastic vial"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая стеклянная или пластиковая бутылочка, обычно цилиндрической формы, используемая для хранения лекарств, духов или химикатов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "флакон",
                                "target_lang_sense_clarification": "для духов или лекарств"
                            },
                            {
                                "target_lang_word": "пузырёк",
                                "target_lang_sense_clarification": "небольшая бутылочка, обычно для лекарств"
                            },
                            {
                                "target_lang_word": "склянка",
                                "target_lang_sense_clarification": "для химических веществ"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "999caade-8f85-332d-9304-6d7a59a6d4e4",
                    "sense_definition": "To put a liquid or substance into a vial for storage.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The laboratory technicians <w>vial</w> the samples immediately after collection.",
                            "target_lang_translations": {
                                "ru": "Лаборанты <w>разливают</w> образцы <w>по флаконам</w> сразу после сбора."
                            }
                        },
                        {
                            "text": "Machines can <w>vial</w> thousands of doses per hour in the factory.",
                            "target_lang_translations": {
                                "ru": "Машины на заводе могут <w>расфасовывать во флаконы</w> тысячи доз в час."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vial the sample",
                        "vial the liquid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разливать жидкость или помещать вещество во флаконы для хранения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разливать по флаконам",
                                "target_lang_sense_clarification": "наполнять флаконы жидкостью"
                            },
                            {
                                "target_lang_word": "расфасовывать",
                                "target_lang_sense_clarification": "распределять по емкостям"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "isoleren",
        "isolator"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1d935fc7-8a1f-3c76-986b-0b6920d8acab",
                    "sense_definition": "Materiaal dat wordt gebruikt om warmte, geluid of elektriciteit tegen te houden of binnen te houden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physics_insulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door goede <w>isolatie</w> blijft het huis in de winter lekker warm.",
                            "target_lang_translations": {
                                "ru": "Благодаря хорошей <w>изоляции</w> зимой в доме остается тепло."
                            }
                        },
                        {
                            "text": "We moeten de oude <w>isolatie</w> op zolder vervangen om energie te besparen.",
                            "target_lang_translations": {
                                "ru": "Нам нужно заменить старую <w>изоляцию</w> на чердаке, чтобы сэкономить энергию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "goede isolatie",
                        "thermische isolatie",
                        "slechte isolatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материал, используемый для защиты от проникновения тепла, звука или электричества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изоляция",
                                "target_lang_sense_clarification": "материал"
                            },
                            {
                                "target_lang_word": "утеплитель",
                                "target_lang_sense_clarification": "теплоизоляционный материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9106aecb-785d-39b3-9245-eb4f23a056b6",
                    "sense_definition": "De toestand waarin iemand of iets gescheiden is van de omgeving of van anderen, vaak om medische of sociale redenen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "social_isolation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De patiënt moest een week in <w>isolatie</w> blijven om besmetting te voorkomen.",
                            "target_lang_translations": {
                                "ru": "Пациент должен был оставаться в <w>изоляции</w> неделю, чтобы предотвратить заражение."
                            }
                        },
                        {
                            "text": "Veel ouderen hadden last van eenzaamheid en <w>isolatie</w> tijdens de pandemie.",
                            "target_lang_translations": {
                                "ru": "Многие пожилые люди страдали от одиночества и <w>изоляции</w> во время пандемии."
                            }
                        }
                    ],
                    "synonyms": [
                        "afzondering",
                        "isolering"
                    ],
                    "antonyms": [
                        "contact"
                    ],
                    "common_phrases": [
                        "in isolatie",
                        "sociale isolatie",
                        "in isolatie gaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние отделенности от окружения или других людей, часто по медицинским или социальным причинам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изоляция",
                                "target_lang_sense_clarification": "состояние"
                            },
                            {
                                "target_lang_word": "обособленность",
                                "target_lang_sense_clarification": "отделенность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10f2b634-a648-3bf2-928b-4ae1752be981",
                    "sense_definition": "Het proces of de werking van het tegenhouden van geleiding van energie, zoals warmte, geluid of stroom.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physics_insulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rubber wordt vaak gebruikt voor de <w>isolatie</w> van elektriciteitskabels.",
                            "target_lang_translations": {
                                "ru": "Резина часто используется для <w>изоляции</w> электрических кабелей."
                            }
                        },
                        {
                            "text": "De <w>isolatie</w> van geluid is erg belangrijk in een muziekstudio.",
                            "target_lang_translations": {
                                "ru": "<w>Изоляция</w> звука очень важна в музыкальной студии."
                            }
                        }
                    ],
                    "synonyms": [
                        "isolering"
                    ],
                    "common_phrases": [
                        "elektrische isolatie",
                        "geluidsisolatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс создания барьера для энергии, такой как тепло, звук или электричество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изоляция",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "изолирование",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
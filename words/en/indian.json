        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "39e0316e-e1fc-3a84-8a9e-21baa33d170b",
                    "sense_definition": "Of or relating to India, its people, or its culture.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "india_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She loves <w>Indian</w> food because of the rich spices.",
                            "target_lang_translations": {
                                "ru": "Она любит <w>индийскую</w> еду из-за обилия специй."
                            }
                        },
                        {
                            "text": "The <w>Indian</w> economy has grown significantly in recent years.",
                            "target_lang_translations": {
                                "ru": "<w>Индийская</w> экономика значительно выросла за последние годы."
                            }
                        }
                    ],
                    "synonyms": [
                        "Asian Indian"
                    ],
                    "common_phrases": [
                        "Indian Ocean",
                        "Indian cuisine",
                        "Indian culture"
                    ],
                    "target_lang_definitions": {

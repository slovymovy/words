                {
                    "sense_id": "3c3a3063-75a0-3686-9b97-d2cbd5ecf5d5",
                    "sense_definition": "A main public road, especially a multi-lane, high-speed thoroughfare connecting major towns or cities.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "road_infrastructure",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "826a18ef-d5f1-3352-a17e-e9cbbb84c9f3",
                    "sense_definition": "Op een manier die weinig geluid maakt; niet luid.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet <w>zachtjes</w> praten als de baby boven ligt te slapen.",
                            "target_lang_translations": {
                                "en": "You must speak <w>softly</w> when the baby is sleeping upstairs.",
                                "ru": "Тебе нужно говорить <w>тихо</w>, когда наверху спит ребенок."
                            }
                        },
                        {
                            "text": "De radio stond <w>zachtjes</w> aan op de achtergrond tijdens het gesprek.",
                            "target_lang_translations": {
                                "en": "The radio was playing <w>softly</w> in the background during the conversation.",
                                "ru": "Радио <w>тихо</w> играло на фоне во время разговора."
                            }
                        }
                    ],
                    "synonyms": [
                        "stilletjes",
                        "stil"
                    ],
                    "antonyms": [
                        "hard",
                        "luid"
                    ],
                    "common_phrases": [
                        "zachtjes praten",
                        "zachtjes zingen",
                        "zachtjes huilen"
                    ],
                    "target_lang_definitions": {
                        "en": "In a manner that makes little noise; not loudly.",
                        "ru": "Тихо, производя мало шума; негромко."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "softly",
                                "target_lang_sense_clarification": "regarding volume"
                            },
                            {
                                "target_lang_word": "quietly",
                                "target_lang_sense_clarification": "without much noise"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тихо",
                                "target_lang_sense_clarification": "негромко"
                            },
                            {
                                "target_lang_word": "вполголоса",
                                "target_lang_sense_clarification": "о речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bbd1750-bd06-32d0-886d-d40f0006b6e1",
                    "sense_definition": "Op een voorzichtige manier, zonder veel kracht te gebruiken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "touch_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij klopte <w>zachtjes</w> op de deur voordat hij de kamer binnenkwam.",
                            "target_lang_translations": {
                                "en": "He knocked <w>gently</w> on the door before he entered the room.",
                                "ru": "Он <w>легонько</w> постучал в дверь, прежде чем войти в комнату."
                            }
                        },
                        {
                            "text": "De moeder wiegde haar kind <w>zachtjes</w> in slaap op haar arm.",
                            "target_lang_translations": {
                                "en": "The mother rocked her child <w>gently</w> to sleep in her arms.",
                                "ru": "Мать <w>нежно</w> укачивала ребенка на руках, укладывая спать."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorzichtig",
                        "teder"
                    ],
                    "antonyms": [
                        "hard",
                        "ruw"
                    ],
                    "common_phrases": [
                        "zachtjes aaien",
                        "zachtjes duwen",
                        "zachtjes kloppen"
                    ],
                    "target_lang_definitions": {
                        "en": "In a careful manner, without using much force.",
                        "ru": "Осторожно, без применения большой силы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gently",
                                "target_lang_sense_clarification": "carefully or tenderly"
                            },
                            {
                                "target_lang_word": "softly",
                                "target_lang_sense_clarification": "with little force"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "легонько",
                                "target_lang_sense_clarification": "слабо, без усилий"
                            },
                            {
                                "target_lang_word": "нежно",
                                "target_lang_sense_clarification": "ласково"
                            },
                            {
                                "target_lang_word": "осторожно",
                                "target_lang_sense_clarification": "бережно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72bea9cd-2bcd-3544-8ed6-6596861b8198",
                    "sense_definition": "Met weinig snelheid; langzaam en rustig.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We reden <w>zachtjes</w> door de smalle straatjes van het oude dorp.",
                            "target_lang_translations": {
                                "en": "We drove <w>slowly</w> through the narrow streets of the old village.",
                                "ru": "Мы <w>медленно</w> ехали по узким улочкам старой деревни."
                            }
                        },
                        {
                            "text": "Het water van de beek stroomde <w>zachtjes</w> over de gladde stenen.",
                            "target_lang_translations": {
                                "en": "The water of the brook flowed <w>gently</w> over the smooth stones.",
                                "ru": "Вода в ручье <w>спокойно</w> текла по гладким камням."
                            }
                        }
                    ],
                    "synonyms": [
                        "langzaam",
                        "rustig"
                    ],
                    "antonyms": [
                        "snel",
                        "hard"
                    ],
                    "common_phrases": [
                        "zachtjes rijden",
                        "zachtjes lopen"
                    ],
                    "target_lang_definitions": {
                        "en": "With little speed; slowly and calmly.",
                        "ru": "С небольшой скоростью; медленно и спокойно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slowly",
                                "target_lang_sense_clarification": "at a low speed"
                            },
                            {
                                "target_lang_word": "gently",
                                "target_lang_sense_clarification": "calmly or smoothly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "медленно",
                                "target_lang_sense_clarification": "не быстро"
                            },
                            {
                                "target_lang_word": "потихоньку",
                                "target_lang_sense_clarification": "не спеша"
                            }
                        ]
                    }
                }
            ]
        }

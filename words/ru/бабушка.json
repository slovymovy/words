            "senses": [
                {
                    "sense_id": "707f9f13-f34c-3c54-9455-5f3747b2b4c3",
                    "sense_definition": "Мать одного из родителей (отца или матери).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kinship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое лето дети ездят в деревню к своей любимой <w>бабушке</w>.",
                            "target_lang_translations": {
                                "en": "Every summer, the children go to the village to visit their beloved <w>grandmother</w>."
                            }
                        },
                        {
                            "text": "Моя <w>бабушка</w> печёт самые вкусные пироги и рассказывает сказки.",
                            "target_lang_translations": {
                                "en": "My <w>grandmother</w> bakes the most delicious pies and tells fairy tales."
                            }
                        },
                        {
                            "text": "Почтительно подошел он к своей <w>бабушке</w>, поцеловал у нее руку и справился о здоровье.",
                            "target_lang_translations": {
                                "en": "He respectfully approached his <w>grandmother</w>, kissed her hand, and inquired about her health."
                            }
                        }
                    ],
                    "synonyms": [
                        "бабуля",
                        "бабка",
                        "прародительница"
                    ],
                    "antonyms": [
                        "дедушка",
                        "внук",
                        "внучка"
                    ],
                    "common_phrases": [
                        "родная бабушка",
                        "бабушка и дедушка",
                        "в гостях у бабушки",
                        "пирожки бабушки"
                    ],
                    "target_lang_definitions": {
                        "en": "The mother of one's father or mother."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grandmother",
                                "target_lang_sense_clarification": "formal or standard term"
                            },
                            {
                                "target_lang_word": "grandma",
                                "target_lang_sense_clarification": "common informal term"
                            },
                            {
                                "target_lang_word": "granny",
                                "target_lang_sense_clarification": "affectionate or informal term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b71d8400-0356-3c1b-a473-271eb3764b6d",
                    "sense_definition": "Пожилая женщина (обычно в разговорной речи).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "person_age",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В общественном транспорте принято уступать место <w>бабушке</w>.",
                            "target_lang_translations": {
                                "en": "In public transport, it is customary to give up one's seat to an <w>old lady</w>."
                            }
                        },
                        {
                            "text": "Возле нашего подъезда часто сидят <w>бабушки</w> и обсуждают последние новости.",
                            "target_lang_translations": {
                                "en": "<w>Old ladies</w> often sit near our entrance and discuss the latest news."
                            }
                        },
                        {
                            "text": "Мальчик помог незнакомой <w>бабушке</w> донести тяжёлую сумку до дома.",
                            "target_lang_translations": {
                                "en": "The boy helped an unfamiliar <w>old lady</w> carry a heavy bag home."
                            }
                        }
                    ],
                    "synonyms": [
                        "старушка",
                        "старуха",
                        "бабулька",
                        "старушенция"
                    ],
                    "antonyms": [

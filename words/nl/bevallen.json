                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give birth",
                                "target_lang_sense_clarification": "general term for the mother's action"
                            },
                            {
                                "target_lang_word": "deliver",
                                "target_lang_sense_clarification": "formal or medical context"
                            }
                        ]
                    }
                }
            ]

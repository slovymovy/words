{
    "word_family": [
        "vermoeden"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "47575cfe-44b2-320b-8d55-ee602cca460e",
                    "sense_definition": "Waarvan men denkt dat het zo is, maar wat nog niet met zekerheid is vastgesteld.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>vermoedelijke</w> dader werd kort na de inbraak door de politie gearresteerd.",
                            "target_lang_translations": {
                                "en": "The <w>suspected</w> perpetrator was arrested by the police shortly after the burglary.",
                                "ru": "<w>Предполагаемый</w> преступник был арестован полицией вскоре после взлома."
                            }
                        },
                        {
                            "text": "Een technisch mankement is de <w>vermoedelijke</w> oorzaak van de vertraging.",
                            "target_lang_translations": {
                                "en": "A technical defect is the <w>probable</w> cause of the delay.",
                                "ru": "Техническая неисправность является <w>предполагаемой</w> причиной задержки."
                            }
                        }
                    ],
                    "synonyms": [
                        "waarschijnlijk",
                        "aannemelijk",
                        "plausibel"
                    ],
                    "antonyms": [
                        "zeker",
                        "onwaarschijnlijk",
                        "bewezen"
                    ],
                    "common_phrases": [
                        "de vermoedelijke dader",
                        "de vermoedelijke oorzaak",
                        "de vermoedelijke winnaar"
                    ],
                    "target_lang_definitions": {
                        "en": "Believed to be the case, but not yet established with certainty.",
                        "ru": "То, что считается истинным, но еще не установлено с уверенностью."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "probable",
                                "target_lang_sense_clarification": "likely to be true or the cause"
                            },
                            {
                                "target_lang_word": "suspected",
                                "target_lang_sense_clarification": "believed to be responsible or the cause, without proof"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "предполагаемый",
                                "target_lang_sense_clarification": "о ком-то или чем-то, в отношении кого есть подозрения или догадки"
                            },
                            {
                                "target_lang_word": "вероятный",
                                "target_lang_sense_clarification": "вполне возможный, правдоподобный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "063b5909-7fff-3bd1-9b4b-13459fa6e4f0",
                    "sense_definition": "Zoals men verwacht of aanneemt; naar alle waarschijnlijkheid.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het gaat morgen <w>vermoedelijk</w> de hele dag regenen, dus neem een paraplu mee.",
                            "target_lang_translations": {
                                "en": "It will <w>presumably</w> rain all day tomorrow, so take an umbrella.",
                                "ru": "Завтра, <w>вероятно</w>, весь день будет идти дождь, так что возьми с собой зонт."
                            }
                        },
                        {
                            "text": "Hij komt <w>vermoedelijk</w> iets later omdat hij nog in de file staat.",
                            "target_lang_translations": {
                                "en": "He will <w>probably</w> arrive a little later because he is still stuck in traffic.",
                                "ru": "Он, <w>по-видимому</w>, приедет немного позже, так как все еще стоит в пробке."
                            }
                        }
                    ],
                    "synonyms": [
                        "waarschijnlijk",
                        "wellicht",
                        "allicht"
                    ],
                    "antonyms": [
                        "ongetwijfeld",
                        "zeker"
                    ],
                    "common_phrases": [
                        "vermoedelijk wel",
                        "vermoedelijk niet"
                    ],
                    "target_lang_definitions": {
                        "en": "As one expects or assumes; in all probability.",
                        "ru": "Как ожидается или предполагается; по всей вероятности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "presumably",
                                "target_lang_sense_clarification": "used to convey that what is asserted is very likely though not known for certain"
                            },
                            {
                                "target_lang_word": "probably",
                                "target_lang_sense_clarification": "almost certainly; very likely"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вероятно",
                                "target_lang_sense_clarification": "по всей видимости, скорее всего"
                            },
                            {
                                "target_lang_word": "предположительно",
                                "target_lang_sense_clarification": "на основе предположения"
                            },
                            {
                                "target_lang_word": "по-видимому",
                                "target_lang_sense_clarification": "судя по всему"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
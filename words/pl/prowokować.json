                            {
                                "target_lang_word": "провоцировать",
                                "target_lang_sense_clarification": "вызывать реакцию или дискуссию"
                            },
                            {
                                "target_lang_word": "подталкивать",
                                "target_lang_sense_clarification": "склонять к чему-либо"

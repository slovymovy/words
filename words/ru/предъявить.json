                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bring",
                                "target_lang_sense_clarification": "charges or a lawsuit"
                            },
                            {
                                "target_lang_word": "file",
                                "target_lang_sense_clarification": "a lawsuit or claim"
                            },
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "charges"
                            }
                        ]
                    }
                }

{
    "word_family": [
        "nadawca",
        "nadajnik",
        "nadanie",
        "nadawczy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "653956ef-30d1-3904-a5cf-44caf5d3e2a2",
                    "sense_definition": "Wysyłać przesyłkę, paczkę lub list, zazwyczaj korzystając z usług poczty lub firmy kurierskiej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transfer_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść dzisiaj na pocztę, aby <w>nadać</w> ważny list polecony do urzędu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно сегодня пойти на почту, чтобы <w>отправить</w> важное заказное письмо в ведомство."
                            }
                        },
                        {
                            "text": "Możesz <w>nadać</w> tę paczkę kurierem, żeby dotarła do odbiorcy szybciej.",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>отправить</w> эту посылку курьером, чтобы она дошла до получателя быстрее."
                            }
                        }
                    ],
                    "synonyms": [
                        "wysyłać",
                        "przesyłać"
                    ],
                    "antonyms": [
                        "odbierać"
                    ],
                    "common_phrases": [
                        "nadawać list",
                        "nadawać paczkę",
                        "nadawać przesyłkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправлять посылку или письмо, обычно пользуясь услугами почты или курьерской службы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправлять",
                                "target_lang_sense_clarification": "посылать почтой или курьером"
                            },
                            {
                                "target_lang_word": "посылать",
                                "target_lang_sense_clarification": "отправлять что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26330300-8377-3145-9f5f-155865f69198",
                    "sense_definition": "Emitować audycję, program lub sygnał za pomocą radia, telewizji lub internetu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transfer_signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta stacja radiowa zaczęła <w>nadawać</w> swoje programy w latach dziewięćdziesiątych.",
                            "target_lang_translations": {
                                "ru": "Эта радиостанция начала <w>вещать</w> свои программы в девяностых годах."
                            }
                        },
                        {
                            "text": "Telewizja będzie <w>nadawać</w> na żywo relację z finałowego meczu mistrzostw.",
                            "target_lang_translations": {
                                "ru": "Телевидение будет <w>транслировать</w> в прямом эфире репортаж с финального матча чемпионата."
                            }
                        }
                    ],
                    "synonyms": [
                        "emitować",
                        "transmitować"
                    ],
                    "antonyms": [
                        "odbierać"
                    ],
                    "common_phrases": [
                        "nadawać na żywo",
                        "nadawać audycję",
                        "nadawać komunikat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать передачу, программу или сигнал по радио, телевидению или интернету."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вещать",
                                "target_lang_sense_clarification": "передавать сигнал (о радио/ТВ)"
                            },
                            {
                                "target_lang_word": "транслировать",
                                "target_lang_sense_clarification": "передавать в эфир"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad547c7b-15bb-30cf-80f0-a21b32037ba5",
                    "sense_definition": "Być właściwym, użytecznym lub pasującym do jakiegoś celu, funkcji lub roli (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "suitability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary samochód już nie <w>nadaje</w> się do bezpiecznej jazdy po autostradzie.",
                            "target_lang_translations": {
                                "ru": "Этот старый автомобиль уже не <w>годится</w> для безопасной езды по автомагистрали."
                            }
                        },
                        {
                            "text": "Uważam, że on idealnie <w>nadaje</w> się na stanowisko kierownika naszego zespołu.",
                            "target_lang_translations": {
                                "ru": "Я считаю, что он идеально <w>подходит</w> на должность руководителя нашей команды."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierować"
                    ],
                    "common_phrases": [
                        "nadawać się do czegoś",
                        "nie nadawać się",
                        "nadawać się na stanowisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть пригодным, полезным или подходящим для какой-либо цели (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "годиться",
                                "target_lang_sense_clarification": "быть пригодным"
                            },
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "соответствовать требованиям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2c01633-f01e-3d13-82aa-e4e0a02baccf",
                    "sense_definition": "Oficjalnie przyznawać komuś tytuł, odznaczenie, uprawnienie lub imię.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "bestow_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice długo zastanawiali się, jakie imię <w>nadać</w> swojemu nowo narodzonemu dziecku.",
                            "target_lang_translations": {
                                "ru": "Родители долго думали, какое имя <w>дать</w> своему новорождённому ребёнку."
                            }
                        },
                        {
                            "text": "Prezydent postanowił <w>nadać</w> wybitnemu naukowcowi wysokie odznaczenie państwowe.",
                            "target_lang_translations": {
                                "ru": "Президент решил <w>присвоить</w> выдающемуся учёному высокую государственную награду."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozdać"
                    ],
                    "antonyms": [
                        "odbierać"
                    ],
                    "common_phrases": [
                        "nadawać imię",
                        "nadawać tytuł",
                        "nadawać obywatelstwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально присваивать кому-либо титул, награду, право или давать имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присваивать",
                                "target_lang_sense_clarification": "звание, титул или право"
                            },
                            {
                                "target_lang_word": "давать",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "622a3892-9bfe-39cb-a4f1-f94a2af14fc7",
                    "sense_definition": "Sprawiać, że ktoś lub coś zyskuje określone cechy, charakter, kształt lub znaczenie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "bestow_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Odpowiednie przyprawy potrafią <w>nadać</w> tej prostej potrawie wyjątkowy smak.",
                            "target_lang_translations": {
                                "ru": "Подходящие приправы могут <w>придать</w> этому простому блюду исключительный вкус."
                            }
                        },
                        {
                            "text": "Twoja ciężka praca może <w>nadać</w> nowy sens całemu temu projektowi.",
                            "target_lang_translations": {
                                "ru": "Твоя усердная работа может <w>придать</w> новый смысл всему этому проекту."
                            }
                        }
                    ],
                    "synonyms": [
                        "kształtować"
                    ],
                    "common_phrases": [
                        "nadawać kształt",
                        "nadawać ton",
                        "nadawać tempo",
                        "nadawać sens"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать чему-либо определённые свойства, характер, форму или значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "придавать",
                                "target_lang_sense_clarification": "форму, вид или свойство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0328f1e-735d-3910-8f6c-40fffad6494f",
                    "sense_definition": "Mówić bardzo dużo, często bez przerwy, lub złośliwie plotkować o innych osobach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "speech_colloquial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestań wreszcie <w>nadawać</w> na sąsiadkę i zajmij się swoimi własnymi sprawami.",
                            "target_lang_translations": {
                                "ru": "Перестань наконец <w>наговаривать</w> на соседку и займись своими собственными делами."
                            }
                        },
                        {
                            "text": "Przez całą podróż pociągiem ona ciągle <w>nadawała</w> o swoich problemach w pracy.",
                            "target_lang_translations": {
                                "ru": "Всю поездку в поезде она постоянно <w>болтала</w> о своих проблемах на работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "plotkować"
                    ],
                    "common_phrases": [
                        "nadawać na kogoś",
                        "ciągle nadawać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Много говорить, часто без умолку, или злословить о других людях (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болтать",
                                "target_lang_sense_clarification": "много говорить"
                            },
                            {
                                "target_lang_word": "наговаривать",
                                "target_lang_sense_clarification": "злословить на кого-то"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
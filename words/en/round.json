                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter used a tool to <w>round</w> the sharp edges of the table.",
                            "target_lang_translations": {
                                "ru": "Плотник использовал инструмент, чтобы <w>скруглить</w> острые края стола."
                            }

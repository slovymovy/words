{
    "word_family": [
        "aankomst",
        "aankomend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8109ca16-0627-36fc-b2fb-e85f8d4344d5",
                    "sense_definition": "Een bestemming bereiken na een reis.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De trein zal over vijf minuten op het station <w>aankomen</w>.",
                            "target_lang_translations": {
                                "en": "The train will <w>arrive</w> at the station in five minutes."
                            }
                        },
                        {
                            "text": "We zijn gisteren veilig in het hotel <w>aangekomen</w>.",
                            "target_lang_translations": {
                                "en": "We <w>arrived</w> safely at the hotel yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "arriveren",
                        "toekomen",
                        "bereiken"
                    ],
                    "common_phrases": [
                        "veilig aankomen",
                        "op tijd aankomen",
                        "thuis aankomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To reach a destination after a journey."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arrive",
                                "target_lang_sense_clarification": "reach a destination"
                            },
                            {
                                "target_lang_word": "reach",
                                "target_lang_sense_clarification": "get to a place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0eb5d6d-cde1-385e-b491-45582d5a7b25",
                    "sense_definition": "Zwaarder worden in lichaamsgewicht.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de vakantie ben ik twee kilo <w>aangekomen</w>.",
                            "target_lang_translations": {
                                "en": "I <w>gained</w> two kilos during the holidays."
                            }
                        },
                        {
                            "text": "Hij eet heel veel omdat hij graag wil <w>aankomen</w>.",
                            "target_lang_translations": {
                                "en": "He eats a lot because he wants to <w>gain weight</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "afvallen"
                    ],
                    "common_phrases": [
                        "kilo's aankomen",
                        "snel aankomen",
                        "veel aankomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase in body weight."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gain weight",
                                "target_lang_sense_clarification": "become heavier"
                            },
                            {
                                "target_lang_word": "put on weight",
                                "target_lang_sense_clarification": "become heavier"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b66604c-e5a3-35d4-85ea-991ce2779708",
                    "sense_definition": "Met de handen aanraken (vaak gebruikt als verbod).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je mag in dit museum nergens <w>aankomen</w>.",
                            "target_lang_translations": {
                                "en": "You are not allowed to <w>touch</w> anything in this museum."
                            }
                        },
                        {
                            "text": "Niet <w>aankomen</w>, want de verf is nog nat!",
                            "target_lang_translations": {
                                "en": "Don't <w>touch</w>, because the paint is still wet!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "nergens aankomen",
                        "niet aankomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To touch with hands (often used as a prohibition)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "touch",
                                "target_lang_sense_clarification": "make physical contact"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4f425ef-b7a5-3860-aa9b-6bfd079475ba",
                    "sense_definition": "Dichterbij komen in tijd of ruimte; iets zien of horen naderen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zag de auto helemaal niet <w>aankomen</w>.",
                            "target_lang_translations": {
                                "en": "I didn't see the car <w>coming</w> at all."
                            }
                        },
                        {
                            "text": "Ze voelde de griep al dagen <w>aankomen</w>.",
                            "target_lang_translations": {
                                "en": "She felt the flu <w>coming on</w> for days."
                            }
                        }
                    ],
                    "synonyms": [
                        "naderen"
                    ],
                    "common_phrases": [
                        "zien aankomen",
                        "horen aankomen",
                        "voelen aankomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To come closer in time or space; to see or hear something approaching."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "approach",
                                "target_lang_sense_clarification": "come near"
                            },
                            {
                                "target_lang_word": "come",
                                "target_lang_sense_clarification": "move towards"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba615860-d317-398f-b72a-48ae9a641ee7",
                    "sense_definition": "Een sterke emotionele invloed hebben; hard binnenkomen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het slechte nieuws is erg hard <w>aangekomen</w> bij de familie.",
                            "target_lang_translations": {
                                "en": "The bad news <w>hit</w> the family very hard."
                            }
                        },
                        {
                            "text": "Zijn kritiek kan soms verkeerd <w>aankomen</w>.",
                            "target_lang_translations": {
                                "en": "His criticism can sometimes <w>land</w> wrong."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hard aankomen",
                        "verkeerd aankomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a strong emotional influence; to hit hard."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hit hard",
                                "target_lang_sense_clarification": "have an emotional impact"
                            },
                            {
                                "target_lang_word": "land",
                                "target_lang_sense_clarification": "be received (of a message)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d12f4c04-c056-3a06-b009-6c881f04ed3e",
                    "sense_definition": "Afhankelijk zijn van; neerkomen op.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het zal uiteindelijk op jouw inzet <w>aankomen</w>.",
                            "target_lang_translations": {
                                "en": "It will eventually <w>come down to</w> your effort."
                            }
                        },
                        {
                            "text": "In deze moeilijke tijden laten we het op elkaar <w>aankomen</w>.",
                            "target_lang_translations": {
                                "en": "In these difficult times, we let it <w>depend on</w> each other."
                            }
                        }
                    ],
                    "synonyms": [
                        "afhangen"
                    ],
                    "common_phrases": [
                        "erop aankomen",
                        "op iemand aankomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To depend on; to come down to."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come down to",
                                "target_lang_sense_clarification": "depend on the outcome"
                            },
                            {
                                "target_lang_word": "depend on",
                                "target_lang_sense_clarification": "rely on"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3adf973-e3da-3b7b-bf7c-7b3a61b063c1",
                    "sense_definition": "Een kort bezoek brengen; even langsgaan.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zullen we vanavond even bij oma <w>aankomen</w>?",
                            "target_lang_translations": {
                                "en": "Shall we <w>drop by</w> grandma's tonight?"
                            }
                        },
                        {
                            "text": "Als we in de buurt zijn, komen we zeker even <w>aankomen</w>.",
                            "target_lang_translations": {
                                "en": "If we are in the neighborhood, we will certainly <w>stop by</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "even aankomen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as 'even aankomen'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To pay a short visit; to drop by."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drop by",
                                "target_lang_sense_clarification": "visit briefly"
                            },
                            {
                                "target_lang_word": "stop by",
                                "target_lang_sense_clarification": "visit briefly"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
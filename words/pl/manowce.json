{
    "word_family": [
        "manowiec"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fff7a729-7972-30aa-800a-df14429aef01",
                    "sense_definition": "Sytuacja oznaczająca zejście na złą drogę, popełnianie błędów życiowych lub zagubienie moralne.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_error",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złe towarzystwo szybko sprowadziło młodego chłopaka na <w>manowce</w>.",
                            "target_lang_translations": {
                                "ru": "Плохая компания быстро толкнула молодого парня на <w>кривую дорожку</w>."
                            }
                        },
                        {
                            "text": "Obietnice łatwego zarobku często wyprowadzają naiwnych ludzi na <w>manowce</w>.",
                            "target_lang_translations": {
                                "ru": "Обещания легкого заработка часто сбивают наивных людей с <w>пути</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sprowadzić na manowce",
                        "zejść na manowce",
                        "wyprowadzić na manowce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, означающая уход с правильного пути, совершение жизненных ошибок или моральную дезориентацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кривая дорожка",
                                "target_lang_sense_clarification": "образно: неправильный, аморальный образ жизни"
                            },
                            {
                                "target_lang_word": "ложный путь",
                                "target_lang_sense_clarification": "ошибочное направление в жизни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bac4369-a9aa-31c3-9a75-b39c7b24be16",
                    "sense_definition": "Bezdroża lub rzadko używane, trudne do przebycia drogi, które często prowadzą donikąd.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "literal_path",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Turyści zgubili szlak i musieli przedzierać się przez leśne <w>manowce</w>.",
                            "target_lang_translations": {
                                "ru": "Туристы потеряли тропу и были вынуждены пробираться через лесные <w>дебри</w>."
                            }
                        },
                        {
                            "text": "Samochód terenowy utknął na błotnistych <w>manowcach</w> z dala od wioski.",
                            "target_lang_translations": {
                                "ru": "Внедорожник застрял на грязном <w>бездорожье</w> вдали от деревни."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezdroże",
                        "wertep"
                    ],
                    "common_phrases": [
                        "leśne manowce",
                        "błądzić po manowcach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бездорожье или редко используемые, труднопроходимые дороги, часто ведущие в никуда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дебри",
                                "target_lang_sense_clarification": "труднопроходимые, густые заросли или места"
                            },
                            {
                                "target_lang_word": "бездорожье",
                                "target_lang_sense_clarification": "отсутствие хороших дорог, плохая дорога"
                            },
                            {
                                "target_lang_word": "глушь",
                                "target_lang_sense_clarification": "удаленное, заброшенное место"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
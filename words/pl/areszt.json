    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "79e78d27-5a2d-3484-a1d7-c7fe89140cbe",
                    "sense_definition": "Tymczasowe pozbawienie wolności przez organy ścigania, stosowane wobec osoby podejrzanej o przestępstwo.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd zdecydował o trzymiesięcznym <w>areszcie</w> dla głównego podejrzanego w sprawie.",
                            "target_lang_translations": {
                                "ru": "Суд принял решение о трехмесячном <w>аресте</w> для главного подозреваемого по делу."
                            }
                        },
                        {
                            "text": "Policja zastosowała <w>areszt</w> tymczasowy wobec agresywnego mężczyzny.",
                            "target_lang_translations": {
                                "ru": "Полиция применила временный <w>арест</w> к агрессивному мужчине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "areszt tymczasowy",
                        "areszt domowy",
                        "uchylić areszt",
                        "zastosować areszt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временное лишение свободы правоохранительными органами, применяемое к лицу, подозреваемому в совершении преступления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арест",
                                "target_lang_sense_clarification": "лишение свободы"
                            },
                            {
                                "target_lang_word": "заключение под стражу",
                                "target_lang_sense_clarification": "мера пресечения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84d4f3d5-33fb-32df-b6e2-78d6964a0385",
                    "sense_definition": "Miejsce, w którym przetrzymuje się osoby zatrzymane lub odbywające karę pozbawienia wolności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podejrzany został przewieziony do <w>aresztu</w> śledczego w Warszawie.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый был перевезен в следственный <w>изолятор</w> в Варшаве."
                            }
                        },
                        {
                            "text": "Warunki sanitarne w starym <w>areszcie</w> wymagały natychmiastowej poprawy.",
                            "target_lang_translations": {
                                "ru": "Санитарные условия в старом <w>изоляторе</w> требовали немедленного улучшения."
                            }
                        }
                    ],
                    "synonyms": [
                        "koza",
                        "odwach",
                        "gw. (Śląsk Cieszyński i Górny Śląsk) hareszt",
                        "harest"
                    ],
                    "common_phrases": [
                        "areszt śledczy",
                        "trafić do aresztu",
                        "siedzieć w areszcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где содержатся задержанные лица или лица, отбывающие наказание в виде лишения свободы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следственный изолятор",
                                "target_lang_sense_clarification": "учреждение"
                            },
                            {
                                "target_lang_word": "изолятор",
                                "target_lang_sense_clarification": "место содержания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bb5d277-a833-367a-a76c-bd0921fed0bf",
                    "sense_definition": "Kara nałożona na dziecko przez rodziców, polegająca na zakazie wychodzenia z domu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mogę iść z wami do kina, bo dostałem <w>areszt</w> na tydzień.",
                            "target_lang_translations": {
                                "ru": "Я не могу пойти с вами в кино, потому что меня посадили под <w>домашний арест</w> на неделю."
                            }
                        },
                        {
                            "text": "Za złe oceny w szkole rodzice nałożyli na syna <w>areszt</w>.",
                            "target_lang_translations": {
                                "ru": "За плохие оценки в школе родители посадили сына под <w>домашний арест</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "szlaban"
                    ],
                    "common_phrases": [
                        "mieć areszt",
                        "dostać areszt"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w kontekście wychowawczym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наказание, налагаемое родителями на ребенка, заключающееся в запрете выходить из дома."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "домашний арест",
                                "target_lang_sense_clarification": "наказание от родителей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5dad49f0-34c9-373b-974b-d65fb2c90710",
                    "sense_definition": "Urzędowe zajęcie przedmiotów lub majątku dłużnika na poczet spłaty zobowiązań.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Komornik sądowy dokonał <w>aresztu</w> ruchomości należących do dłużnika.",
                            "target_lang_translations": {
                                "ru": "Судебный пристав произвел <w>арест</w> движимого имущества, принадлежащего должнику."
                            }
                        },
                        {
                            "text": "Wierzyciele domagali się nałożenia <w>aresztu</w> na konta bankowe firmy.",
                            "target_lang_translations": {
                                "ru": "Кредиторы требовали наложения <w>ареста</w> на банковские счета фирмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "sekwestr",
                        "sekwestracja"
                    ],
                    "common_phrases": [
                        "areszt majątku",
                        "nałożyć areszt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная опись и запрет на распоряжение имуществом или активами должника в счет погашения обязательств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арест",
                                "target_lang_sense_clarification": "имущества"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f3e7fb86-a143-3ddb-811c-ddc312cfee8a",
                    "sense_definition": "To start to be; to turn into or change into a particular state or condition.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>became</w> a doctor when she was 25."
                        },
                        {
                            "text": "The weather will <w>become</w> cold after the sun goes down."
                        }
                    ],
                    "synonyms": [
                        "get",
                        "grow",
                        "turn",
                        "go"
                    ],
                    "antonyms": [
                        "remain",
                        "stay"
                    ],
                    "common_phrases": [

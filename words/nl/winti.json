            "pos": "noun",
            "senses": [
                {
                    "sense_id": "88a5d441-a10a-3353-b1ad-5bbd30eef421",
                    "sense_definition": "Een Afro-Surinaamse natuurgodsdienst waarin het contact met geesten, goden en voorouders centraal staat.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "religion_or_philosophy_name",
                    "examples": [
                        {
                            "text": "In Suriname is <w>winti</w> een belangrijk onderdeel van het culturele erfgoed.",
                            "target_lang_translations": {
                                "en": "In Suriname, <w>Winti</w> is an important part of the cultural heritage.",
                                "ru": "В Суринаме <w>винти</w> является важной частью культурного наследия."
                            }
                        },
                        {
                            "text": "Zij heeft veel respect voor de tradities binnen de <w>winti</w>.",
                            "target_lang_translations": {
                                "en": "She has a lot of respect for the traditions within <w>Winti</w>.",
                                "ru": "Она с большим уважением относится к традициям <w>винти</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "natuurgodsdienst"
                    ],
                    "common_phrases": [
                        "winti belijden",
                        "cultuur van winti"
                    ],
                    "target_lang_definitions": {
                        "en": "An Afro-Surinamese nature religion in which contact with spirits, gods, and ancestors is central.",
                        "ru": "Афро-суринамская природная религия, в которой центральное место занимает контакт с духами, богами и предками."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Winti",
                                "target_lang_sense_clarification": "Afro-Surinamese religion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "винти",
                                "target_lang_sense_clarification": "афро-суринамская религия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a72e7d2-3bd2-3e5a-bf34-1f224e62b775",
                    "sense_definition": "Een bovennatuurlijk wezen of geest binnen de Afro-Surinaamse religie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het ritueel werd een offer gebracht aan de <w>winti</w>.",
                            "target_lang_translations": {
                                "en": "During the ritual, an offering was made to the <w>spirit</w>.",
                                "ru": "Во время ритуала было принесено подношение <w>винти</w>."
                            }
                        },
                        {
                            "text": "Men gelooft dat een <w>winti</w> zich zo snel als de wind kan verplaatsen.",
                            "target_lang_translations": {
                                "en": "It is believed that a <w>Winti</w> can move as fast as the wind.",
                                "ru": "Считается, что <w>винти</w> может перемещаться так же быстро, как ветер."
                            }
                        }
                    ],
                    "synonyms": [
                        "geest",
                        "godheid"
                    ],
                    "common_phrases": [
                        "een goede winti",
                        "winti aanroepen"
                    ],
                    "target_lang_definitions": {
                        "en": "A supernatural being or spirit within the Afro-Surinamese religion.",
                        "ru": "Сверхъестественное существо или дух в афро-суринамской религии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spirit",
                                "target_lang_sense_clarification": "supernatural entity"
                            },
                            {
                                "target_lang_word": "Winti",
                                "target_lang_sense_clarification": "specific spirit type"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "винти",
                                "target_lang_sense_clarification": "дух или божество"
                            },
                            {
                                "target_lang_word": "дух",
                                "target_lang_sense_clarification": "сверхъестественное существо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb9e4504-ff9d-3493-bb6e-463b069d6f3d",
                    "sense_definition": "Een toestand van trance of bezetenheid door een geest.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de dans raakte de vrouw in <w>winti</w>.",
                            "target_lang_translations": {
                                "en": "During the dance, the woman went into a <w>trance</w>.",
                                "ru": "Во время танца женщина впала в <w>транс</w>."
                            }
                        },
                        {
                            "text": "Als iemand <w>winti</w> krijgt, verandert zijn gedrag tijdelijk.",
                            "target_lang_translations": {
                                "en": "When someone becomes <w>possessed</w>, their behavior changes temporarily.",
                                "ru": "Если кем-то овладевает <w>винти</w>, его поведение временно меняется."
                            }
                        }
                    ],
                    "synonyms": [
                        "trance",
                        "bezetenheid"
                    ],
                    "common_phrases": [
                        "in winti raken",
                        "winti krijgen"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of trance or possession by a spirit.",
                        "ru": "Состояние транса или одержимости духом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trance",
                                "target_lang_sense_clarification": "state of consciousness"
                            },
                            {
                                "target_lang_word": "possession",
                                "target_lang_sense_clarification": "spiritual state"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "транс",
                                "target_lang_sense_clarification": "состояние измененного сознания"
                            },
                            {
                                "target_lang_word": "одержимость",
                                "target_lang_sense_clarification": "состояние под влиянием духа"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a21edcfb-1b3e-3f36-a32a-9e38947c2693",
                    "sense_definition": "To try to find someone or something by looking carefully.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_look_for",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>searched</w> for her glasses everywhere in the room.",
                            "target_lang_translations": {
                                "ru": "Она <w>искала</w> свои очки повсюду в комнате."
                            }
                        },
                        {
                            "text": "Investors are <w>searching</w> for new business opportunities.",
                            "target_lang_translations": {
                                "ru": "Инвесторы <w>ищут</w> новые возможности для бизнеса."
                            }
                        }
                    ],
                    "synonyms": [
                        "look for",
                        "seek"
                    ],
                    "common_phrases": [
                        "search for",
                        "search in vain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пытаться найти кого-то или что-то, внимательно осматривая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "пытаться найти"
                            },
                            {
                                "target_lang_word": "разыскивать",
                                "target_lang_sense_clarification": "искать тщательно или официально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "169ecd52-e3ed-3e2d-8216-410dd0ba0441",
                    "sense_definition": "To look for information on a computer or the internet using a search engine.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_digital",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>searched</w> online for the best pizza restaurant in town.",
                            "target_lang_translations": {
                                "ru": "Я <w>искал</w> в интернете лучшую пиццерию в городе."
                            }
                        },
                        {
                            "text": "You can <w>search</w> the web to find the answer to your question.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>поискать</w> в интернете, чтобы найти ответ на свой вопрос."
                            }
                        }
                    ],
                    "common_phrases": [
                        "search online",
                        "search the web",
                        "search the internet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искать информацию на компьютере или в интернете с помощью поисковой системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "искать информацию в интернете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87e476e1-cda1-3a13-aa33-e7d374c550a7",
                    "sense_definition": "To look thoroughly inside a specific place or area to find something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_examine_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>searched</w> the garden for the keys but could not find them.",
                            "target_lang_translations": {
                                "ru": "Я <w>обыскал</w> сад в поисках ключей, но не смог их найти."
                            }
                        },
                        {
                            "text": "The police <w>searched</w> the house for any evidence of the crime.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>обыскала</w> дом в поисках улик преступления."
                            }
                        }
                    ],
                    "synonyms": [
                        "comb",
                        "scour"
                    ],
                    "common_phrases": [
                        "search the house",
                        "search the area",
                        "search a room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщательно осматривать определенное место или территорию, чтобы найти что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обыскивать",
                                "target_lang_sense_clarification": "осматривать место или человека с целью найти что-то спрятанное"
                            },
                            {
                                "target_lang_word": "осматривать",
                                "target_lang_sense_clarification": "внимательно смотреть вокруг"
                            }
                        ]
                    }
                }
            ]
        },

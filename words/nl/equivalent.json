{
    "word_family": [
        "equivalentie"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "839b378b-8037-320e-b990-7d49eb9c603b",
                    "sense_definition": "Gelijkwaardig; van dezelfde waarde, grootte of betekenis.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is een <w>equivalente</w> vorm van energie.",
                            "target_lang_translations": {
                                "ru": "Это <w>эквивалентная</w> форма энергии."
                            }
                        },
                        {
                            "text": "Dit diploma is <w>equivalent</w> aan een mastertitel in Nederland.",
                            "target_lang_translations": {
                                "ru": "Этот диплом <w>эквивалентен</w> степени магистра в Нидерландах."
                            }
                        }
                    ],
                    "synonyms": [
                        "gelijkwaardig",
                        "overeenkomend"
                    ],
                    "antonyms": [
                        "ongelijkwaardig"
                    ],
                    "common_phrases": [
                        "equivalent aan",
                        "ruwweg equivalent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Равноценный; имеющий ту же ценность, размер или значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эквивалентный",
                                "target_lang_sense_clarification": "равный, равнозначный"
                            },
                            {
                                "target_lang_word": "равноценный",
                                "target_lang_sense_clarification": "имеющий ту же стоимость или важность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1367ba07-9e10-38c7-9558-449b1bcd7b2c",
                    "sense_definition": "Iets wat in waarde, betekenis of functie gelijk is aan iets anders.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is het <w>equivalent</w> van drie glazen bier.",
                            "target_lang_translations": {
                                "ru": "Это <w>эквивалент</w> трех бокалов пива."
                            }
                        },
                        {
                            "text": "De elektrische fiets is voor velen het moderne <w>equivalent</w> van de brommer.",
                            "target_lang_translations": {
                                "ru": "Для многих электрический велосипед — это современный <w>эквивалент</w> мопеда."
                            }
                        }
                    ],
                    "synonyms": [
                        "gelijkwaardig",
                        "overeenkomend"
                    ],
                    "common_phrases": [
                        "het equivalent van",
                        "een modern equivalent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечто, равное чему-либо другому по ценности, значению или функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эквивалент",
                                "target_lang_sense_clarification": "равноценный предмет или явление"
                            },
                            {
                                "target_lang_word": "аналог",
                                "target_lang_sense_clarification": "нечто сходное, подобное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "843b8a90-efec-3718-bb1a-afc690963fc4",
                    "sense_definition": "Een woord of uitdrukking in een taal die dezelfde betekenis heeft als een woord in een andere taal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "linguistic_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chambres d'hôte is het Franse <w>equivalent</w> van 'bed and breakfast'.",
                            "target_lang_translations": {
                                "ru": "Chambres d'hôte — это французский <w>эквивалент</w> понятия \"bed and breakfast\"."
                            }
                        },
                        {
                            "text": "Ik kan het juiste Engelse <w>equivalent</w> voor dit Nederlandse spreekwoord niet vinden.",
                            "target_lang_translations": {
                                "ru": "Я не могу найти подходящий английский <w>эквивалент</w> для этой голландской пословицы."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertaalequivalent"
                    ],
                    "common_phrases": [
                        "het Engelse equivalent",
                        "het Nederlandse equivalent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово или выражение в одном языке, имеющее то же значение, что и слово в другом языке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эквивалент",
                                "target_lang_sense_clarification": "межъязыковое соответствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f30a409a-6198-3c52-a367-224db1d59af7",
                    "sense_definition": "Een woord dat precies hetzelfde betekent als een ander woord binnen dezelfde taal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "linguistic_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "'Plezant' is het Vlaamse <w>equivalent</w> van 'leuk'.",
                            "target_lang_translations": {
                                "ru": "\"Plezant\" — это фламандский <w>эквивалент</w> слова \"leuk\"."
                            }
                        },
                        {
                            "text": "De schrijver zocht naar een <w>equivalent</w> om woordherhaling te vermijden.",
                            "target_lang_translations": {
                                "ru": "Писатель искал <w>эквивалент</w>, чтобы избежать повторения слов."
                            }
                        }
                    ],
                    "synonyms": [
                        "synoniem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, которое означает то же самое, что и другое слово в том же языке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синоним",
                                "target_lang_sense_clarification": "слово с тем же значением"
                            },
                            {
                                "target_lang_word": "эквивалент",
                                "target_lang_sense_clarification": "равнозначное слово"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
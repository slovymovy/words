                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clearly",
                                "target_lang_sense_clarification": "In a clear manner; plainly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ясно",
                                "target_lang_sense_clarification": "понятно"
                            },
                            {
                                "target_lang_word": "чётко",
                                "target_lang_sense_clarification": "разборчиво, конкретно"
                            },
                            {
                                "target_lang_word": "внятно",
                                "target_lang_sense_clarification": "разборчиво (о речи)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

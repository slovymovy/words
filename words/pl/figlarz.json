    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b4dae27b-25a8-3880-a06b-99715e04d14b",
                    "sense_definition": "Osoba, która lubi żartować, bawić się i płatać innym figle.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jacyś <w>figlarze</w> pomazali farbą samochód pani dyrektor.",
                            "target_lang_translations": {
                                "ru": "Какие-то <w>проказники</w> измазали краской машину директора."
                            }
                        },
                        {
                            "text": "Mój młodszy brat to straszny <w>figlarz</w>, ciągle chowa mi klucze.",
                            "target_lang_translations": {
                                "ru": "Мой младший брат — ужасный <w>шалун</w>, он постоянно прячет мои ключи."
                            }
                        }
                    ],
                    "synonyms": [
                        "psotnik",
                        "swawolnik",
                        "zbytnik",
                        "żartowniś"
                    ],
                    "common_phrases": [
                        "mały figlarz",
                        "niepoprawny figlarz",
                        "wesoły figlarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который любит шутить, веселиться и устраивать розыгрыши."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шалун",
                                "target_lang_sense_clarification": "озорной человек, часто о детях"
                            },
                            {
                                "target_lang_word": "проказник",
                                "target_lang_sense_clarification": "любитель шалостей"
                            },
                            {
                                "target_lang_word": "шутник",
                                "target_lang_sense_clarification": "любитель пошутить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5faee39-92c3-346b-90b4-eb9f56031f67",
                    "sense_definition": "Dawne określenie artysty cyrkowego, linoskoczka lub osoby wykonującej sztuczki na jarmarkach.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_profession_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na średniowiecznym rynku występował <w>figlarz</w>, zabawierając tłum swoimi akrobacjami.",
                            "target_lang_translations": {
                                "ru": "На средневековой рыночной площади выступал <w>фигляр</w>, развлекая толпу своими акробатическими трюками."
                            }
                        },
                        {
                            "text": "Wędrowny <w>figlarz</w> pokazywał sztuczki z kartami i monetami.",
                            "target_lang_translations": {
                                "ru": "Бродячий <w>фокусник</w> показывал трюки с картами и монетами."
                            }
                        }
                    ],
                    "synonyms": [
                        "kuglarz"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo używane dawniej w znaczeniu kuglarza"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название циркового артиста, канатоходца или ярмарочного фокусника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фигляр",
                                "target_lang_sense_clarification": "устаревшее; ярмарочный фокусник, акробат"
                            },
                            {
                                "target_lang_word": "скоморох",
                                "target_lang_sense_clarification": "бродячий артист"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ce994ca-f376-332c-9150-5203f39c62d1",
                    "sense_definition": "Osoba nieuczciwa, która oszukuje innych dla korzyści.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person_negative_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten kupiec okazał się zwykłym <w>figlarzem</w>, sprzedając nam fałszywe klejnoty.",
                            "target_lang_translations": {
                                "ru": "Этот купец оказался обычным <w>плутом</w>, продав нам фальшивые драгоценности."
                            }
                        },
                        {
                            "text": "Nie ufaj mu, to znany w okolicy <w>figlarz</w> i krętacz.",
                            "target_lang_translations": {
                                "ru": "Не доверяй ему, это известный в округе <w>мошенник</w> и пройдоха."
                            }
                        }
                    ],
                    "synonyms": [
                        "matacz",
                        "oszust"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rzadkie, dawne znaczenie negatywne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечестный человек, который обманывает других ради выгоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плут",
                                "target_lang_sense_clarification": "хитрый обманщик"
                            },
                            {
                                "target_lang_word": "мошенник",
                                "target_lang_sense_clarification": "нечестный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fe9db38-ca38-3bf9-9735-030087fe10d8",
                    "sense_definition": "Ptak z rodzaju Alectrurus należący do rodziny tyrankowatych, występujący w Ameryce Południowej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Figlarz</w> trójbarwny jest gatunkiem zagrożonym wyginięciem.",
                            "target_lang_translations": {
                                "ru": "<w>Петухохвостый тиранн</w> является видом, находящимся под угрозой исчезновения."
                            }
                        },
                        {
                            "text": "Ornitolodzy obserwowali, jak <w>figlarz</w> buduje gniazdo w wysokich trawach.",
                            "target_lang_translations": {
                                "ru": "Орнитологи наблюдали, как <w>петухохвостый тиранн</w> строит гнездо в высокой траве."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Птица из рода Alectrurus семейства тиранновых, обитающая в Южной Америке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "петухохвостый тиранн",
                                "target_lang_sense_clarification": "зоологическое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e1a9f9e-fd1a-3147-86dd-f03833f2b516",
                    "sense_definition": "Roślina z rodzaju Mimulus, znana również jako kroplik.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wilgotnej części ogrodu zakwitł piękny żółty <w>figlarz</w>.",
                            "target_lang_translations": {
                                "ru": "Во влажной части сада расцвел красивый желтый <w>губастик</w>."
                            }
                        },
                        {
                            "text": "<w>Figlarz</w> jest rośliną, która lubi stanowiska blisko wody.",
                            "target_lang_translations": {
                                "ru": "<w>Губастик</w> — это растение, которое любит места у воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "kroplik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение из рода Mimulus, также известное как губастик."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "губастик",
                                "target_lang_sense_clarification": "ботаническое название"
                            },
                            {
                                "target_lang_word": "мимулюс",
                                "target_lang_sense_clarification": "садоводческое название"
                            }
                        ]
                    }
                }
            ]
        },
        {

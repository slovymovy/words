                {
                    "sense_id": "0325ca84-ee03-31d9-b627-d2dff22bd06d",
                    "sense_definition": "Twarda, zewnętrzna powłoka chroniąca ciało niektórych zwierząt, na przykład żółwi, skorupiaków lub owadów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żółw przestraszył się hałasu i szybko schował głowę pod swój twardy <w>pancerz</w>."
                        },
                        {
                            "text": "Ten chrząszcz ma chitynowy <w>pancerz</w>, który skutecznie chroni go przed atakami ptaków."
                        }
                    ],
                    "common_phrases": [
                        "chitynowy pancerz",
                        "twardy pancerz"
                    ]
                },
                {
                    "sense_id": "b841d0e5-25ed-3da5-84ec-60c277ff7861",
                    "sense_definition": "Ochronne okrycie ciała wykonane z metalu lub skóry, noszone dawniej przez rycerzy i wojowników podczas walki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "human_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz założył lśniący <w>pancerz</w> i dosiadł swojego konia bojowego."
                        },
                        {
                            "text": "W muzeum oglądaliśmy stary <w>pancerz</w>, który ważył ponad trzydzieści kilogramów."
                        }
                    ],
                    "synonyms": [
                        "zbroja"
                    ],
                    "common_phrases": [

            "senses": [
                {
                    "sense_id": "e2497db3-6644-34e2-adc6-57e8f39624e3",
                    "sense_definition": "Connected with something else or occurring together with it.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "connected_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The disease is <w>associated</w> with poor sanitation.",
                            "target_lang_translations": {
                                "ru": "Это заболевание <w>связано</w> с плохой санитарией."
                            }
                        },
                        {
                            "text": "There are several risks <w>associated</w> with this medical procedure.",
                            "target_lang_translations": {
                                "ru": "Существует несколько рисков, <w>связанных</w> с этой медицинской процедурой."
                            }
                        }
                    ],

                    "frequency": "High",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He paid for the coffee with a ten-pound <w>note</w>.",
                            "target_lang_translations": {
                                "ru": "Он заплатил за кофе десятифунтовой <w>банкнотой</w>."
                            }
                        },
                        {
                            "text": "I do not have any coins, only a large <w>note</w>.",
                            "target_lang_translations": {
                                "ru": "У меня нет монет, только крупная <w>купюра</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bill"
                    ],
                    "common_phrases": [
                        "ten-pound note",
                        "bank note"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Common in UK, Australia, etc. The US usually uses 'bill'."
                        }

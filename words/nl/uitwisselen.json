    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "484e639c-eada-304d-b970-d59409a5331c",
                    "sense_definition": "Iets aan iemand geven en daarvoor iets anders van diegene terugkrijgen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exchange_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de vergadering zullen we onze contactgegevens <w>uitwisselen</w>.",
                            "target_lang_translations": {
                                "ru": "После собрания мы <w>обменяемся</w> нашими контактными данными."
                            }
                        },
                        {
                            "text": "De studenten <w>wisselden</w> ervaringen <w>uit</w> over hun studie in het buitenland.",
                            "target_lang_translations": {
                                "ru": "Студенты <w>обменивались</w> опытом о своей учебе за границей."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruilen"
                    ],
                    "common_phrases": [
                        "ideeën uitwisselen",
                        "gegevens uitwisselen",
                        "ervaringen uitwisselen",
                        "gevangenen uitwisselen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать что-то кому-то и получить взамен что-то другое от этого человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмениваться",
                                "target_lang_sense_clarification": "идеями, информацией, вещами"
                            },
                            {
                                "target_lang_word": "меняться",
                                "target_lang_sense_clarification": "разговорное; предметами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "499b85e7-e327-37b0-ab32-1cd69a4b331a",
                    "sense_definition": "Elkaar kort aankijken of iets tegen elkaar zeggen om een gevoel of gedachte over te brengen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "exchange_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen de leraar een fout maakte, <w>wisselden</w> de leerlingen een veelbetekenende blik <w>uit</w>.",
                            "target_lang_translations": {
                                "ru": "Когда учитель сделал ошибку, ученики <w>обменялись</w> многозначительным взглядом."
                            }
                        },
                        {
                            "text": "De buren hebben gisteren enkele harde woorden <w>uitgewisseld</w> over het lawaai.",
                            "target_lang_translations": {
                                "ru": "Вчера соседи <w>обменялись</w> несколькими резкими словами из-за шума."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een blik uitwisselen",
                        "woorden uitwisselen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Коротко посмотреть друг на друга или сказать что-то друг другу, чтобы передать чувство или мысль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмениваться",
                                "target_lang_sense_clarification": "взглядами, репликами"
                            },
                            {
                                "target_lang_word": "перебрасываться",
                                "target_lang_sense_clarification": "словами, взглядами"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
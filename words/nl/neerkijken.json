    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d97052ae-c698-30fa-aa98-5c97758b07ac",
                    "sense_definition": "Iemand of iets als minderwaardig beschouwen; denken dat men beter is dan een ander.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>kijkt</w> vaak <w>neer</w> op mensen die minder geld hebben.",
                            "target_lang_translations": {
                                "en": "He often <w>looks down</w> on people who have less money.",
                                "ru": "Он часто <w>смотрит свысока</w> на людей, у которых меньше денег."
                            }
                        },
                        {
                            "text": "Het is arrogant om op je collega's <w>neer</w> te <w>kijken</w>.",
                            "target_lang_translations": {
                                "en": "It is arrogant to <w>look down</w> on your colleagues.",
                                "ru": "Высокомерно <w>смотреть свысока</w> на своих коллег."
                            }
                        }
                    ],
                    "synonyms": [
                        "minachten"
                    ],
                    "antonyms": [
                        "opkijken"
                    ],
                    "common_phrases": [
                        "neerkijken op iemand",
                        "neerkijken op anderen"
                    ],
                    "target_lang_definitions": {
                        "en": "To regard someone or something as inferior; to think one is better than another.",
                        "ru": "Считать кого-то или что-то ниже себя; думать, что ты лучше других."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look down on",
                                "target_lang_sense_clarification": "to feel superior to someone"
                            },
                            {
                                "target_lang_word": "despise",
                                "target_lang_sense_clarification": "to feel contempt for"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "смотреть свысока",
                                "target_lang_sense_clarification": "чувствовать превосходство над кем-то"
                            },
                            {
                                "target_lang_word": "презирать",
                                "target_lang_sense_clarification": "относиться с пренебрежением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91409f42-262b-3dfa-913a-ad59b2536721",
                    "sense_definition": "Vanaf een hoge positie naar beneden kijken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vanaf de hoge toren <w>keken</w> we <w>neer</w> op de drukke stad.",
                            "target_lang_translations": {
                                "en": "From the high tower, we <w>looked down</w> on the busy city.",
                                "ru": "С высокой башни мы <w>смотрели вниз</w> на оживленный город."
                            }
                        },
                        {
                            "text": "Hij stond op de brug en <w>keek</w> <w>neer</w> in het donkere water.",
                            "target_lang_translations": {
                                "en": "He stood on the bridge and <w>looked down</w> into the dark water.",
                                "ru": "Он стоял на мосту и <w>смотрел вниз</w> в темную воду."
                            }
                        }
                    ],
                    "synonyms": [
                        "omlaagkijken"
                    ],
                    "antonyms": [
                        "opkijken"
                    ],
                    "common_phrases": [
                        "neerkijken op de stad",
                        "neerkijken in het dal"
                    ],
                    "target_lang_definitions": {
                        "en": "To look downwards from a high position.",
                        "ru": "Смотреть вниз с высокой позиции."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look down",
                                "target_lang_sense_clarification": "literal visual action"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "смотреть вниз",
                                "target_lang_sense_clarification": "направлять взгляд вниз с высоты"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
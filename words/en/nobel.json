            "pos": "name",
            "senses": [
                {
                    "sense_id": "88ce8a6b-0dda-3e64-a442-51554999cf88",
                    "sense_definition": "A surname of Swedish origin, most notably referring to Alfred Nobel, the inventor of dynamite and founder of the Nobel Prizes.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Alfred <w>Nobel</w> established the prizes in his will to honor those who confer the greatest benefit to humankind.",
                            "target_lang_translations": {
                                "ru": "Альфред <w>Нобель</w> учредил эти премии в своем завещании, чтобы почтить тех, кто принес наибольшую пользу человечеству."
                            }
                        },
                        {
                            "text": "The <w>Nobel</w> family was prominent in the industrial development of the 19th century.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Нобелей</w> играла видную роль в промышленном развитии XIX века."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "Swedish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия шведского происхождения, наиболее известная благодаря Альфреду Нобелю, изобретателю динамита и учредителю Нобелевских премий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Нобель",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {

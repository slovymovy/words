            "pos": "verb",
            "senses": [
                {
                    "sense_id": "57e9e40c-68fd-3cab-b33d-69e18254f0fb",
                    "sense_definition": "Болезненно переживать отсутствие кого-либо или чего-либо, испытывать тоску по кому-то или чему-то.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotion_longing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я буду очень <скучать> по тебе, когда ты уедешь в другой город.",
                            "target_lang_translations": {
                                "en": "I will <miss> you very much when you leave for another city."
                            }
                        },
                        {
                            "text": "Студенты часто <скучают> по дому и родителям в первый год учёбы.",
                            "target_lang_translations": {
                                "en": "Students often <miss> home and their parents during the first year of study."
                            }
                        }
                    ],
                    "synonyms": [
                        "тосковать",
                        "грустить"
                    ],
                    "common_phrases": [
                        "скучать по дому",
                        "скучать по тебе",
                        "скучать без друзей",
                        "сильно скучать"
                    ],
                    "target_lang_definitions": {
                        "en": "To feel sad or nostalgic because someone or something is absent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "miss",
                                "target_lang_sense_clarification": "to feel sad because of absence"
                            },
                            {
                                "target_lang_word": "long for",
                                "target_lang_sense_clarification": "to have a strong desire for something absent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "773c86cd-4129-321e-9f87-539fbf224422",
                    "sense_definition": "Испытывать скуку, томиться от безделья или отсутствия интереса к происходящему.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotion_boredom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дети начали <скучать>, потому что на улице шёл дождь и гулять было нельзя.",
                            "target_lang_translations": {
                                "en": "The children started to <get bored> because it was raining outside and they couldn't go for a walk."
                            }
                        },
                        {
                            "text": "На этой лекции невозможно <скучать>, так как профессор рассказывает очень интересно.",
                            "target_lang_translations": {
                                "en": "It is impossible to <be bored> at this lecture because the professor speaks very interestingly."
                            }
                        }
                    ],
                    "synonyms": [
                        "испытывать скуку"
                    ],
                    "antonyms": [
                        "веселиться",
                        "интересоваться",
                        "радоваться"
                    ],
                    "common_phrases": [
                        "скучать от безделья",
                        "сидеть и скучать",
                        "никогда не скучать"
                    ],
                    "target_lang_definitions": {
                        "en": "To feel weary and restless through lack of interest or activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be bored",
                                "target_lang_sense_clarification": "state of feeling boredom"
                            },
                            {
                                "target_lang_word": "get bored",
                                "target_lang_sense_clarification": "process of becoming bored"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc092149-a137-3248-b55a-341a1652b039",
                    "sense_definition": "Печалиться, горевать, испытывать душевную боль.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "emotion_grief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старушка села у окна и начала <скучать> о своей тяжёлой судьбе.",
                            "target_lang_translations": {
                                "en": "The old woman sat by the window and began to <grieve> over her hard fate."
                            }
                        },
                        {
                            "text": "Не надо так <скучать>, всё со временем наладится.",
                            "target_lang_translations": {
                                "en": "Don't <grieve> so much, everything will work out in time."

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "успех",
                                "target_lang_sense_clarification": "удачное событие или предприятие"
                            },
                            {
                                "target_lang_word": "удача",
                                "target_lang_sense_clarification": "что-то, что прошло хорошо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c410763-d524-3b08-8b15-1e2f1e3a8501",
                    "sense_definition": "The fact of getting or achieving wealth, respect, or fame.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_achievement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band enjoyed considerable <w>success</w> during the 1990s.",
                            "target_lang_translations": {
                                "ru": "Группа пользовалась значительным <w>успехом</w> в 1990-е годы."
                            }
                        },
                        {
                            "text": "Don't let sudden <w>success</w> go to your head.",
                            "target_lang_translations": {
                                "ru": "Не позволяй внезапному <w>успеху</w> вскружить тебе голову."
                            }
                        }
                    ],
                    "antonyms": [
                        "failure"
                    ],
                    "common_phrases": [
                        "financial success",
                        "enjoy success"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт обретения богатства, уважения или славы; общественное признание."
                    },
                    "translations": {

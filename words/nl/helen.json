                            {
                                "target_lang_word": "fence",
                                "target_lang_sense_clarification": "to buy or sell stolen goods"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "скупать краденое",
                                "target_lang_sense_clarification": "покупать вещи, зная, что они украдены"
                            },
                            {
                                "target_lang_word": "сбывать краденое",
                                "target_lang_sense_clarification": "продавать украденные вещи"
                            }
                        ]
                    }
                },
                {

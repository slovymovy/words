            "pos": "noun",
            "senses": [
                {
                    "sense_id": "242c8495-3051-3270-a46b-568e32638570",
                    "sense_definition": "Resources, such as air, water, or land, that are shared by a community or humanity in general and are not privately owned.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "shared_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tragedy of the <w>commons</w> occurs when individuals deplete a shared resource for their own gain.",
                            "target_lang_translations": {
                                "ru": "Трагедия <w>общин</w> происходит, когда отдельные лица истощают общий ресурс ради собственной выгоды."
                            }
                        },
                        {
                            "text": "We must protect the global <w>commons</w> to ensure a sustainable future for everyone.",
                            "target_lang_translations": {
                                "ru": "Мы должны защищать глобальное <w>общее достояние</w>, чтобы обеспечить устойчивое будущее для всех."
                            }
                        }
                    ],
                    "synonyms": [
                        "public goods"
                    ],
                    "antonyms": [
                        "private property"
                    ],
                    "common_phrases": [
                        "tragedy of the commons",
                        "global commons",
                        "creative commons"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ресурсы, такие как воздух, вода или земля, которые находятся в общем пользовании сообщества или человечества и не являются частной собственностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общее достояние",
                                "target_lang_sense_clarification": "ресурсы, принадлежащие всем"
                            },
                            {
                                "target_lang_word": "общинные ресурсы",
                                "target_lang_sense_clarification": "экономический термин"
                            },
                            {
                                "target_lang_word": "общинные угодья",
                                "target_lang_sense_clarification": "исторический контекст (земля)"
                            }
                        ]
                    }
                },

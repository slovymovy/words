    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5c6a9728-757a-320e-9ee3-21ee2283f913",
                    "sense_definition": "The act of providing physical care and emotional support, especially to children or dependents.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "act_of_nurturing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The child thrived under the constant <w>nurturance</w> of her loving parents.",
                            "target_lang_translations": {
                                "ru": "Ребенок расцвел благодаря постоянной <w>заботе</w> своих любящих родителей."
                            }
                        },
                        {
                            "text": "Psychologists emphasize the importance of early emotional <w>nurturance</w> for healthy development.",
                            "target_lang_translations": {
                                "ru": "Психологи подчеркивают важность ранней эмоциональной <w>поддержки</w> для здорового развития."
                            }
                        }
                    ],
                    "synonyms": [
                        "care",
                        "nurturing",
                        "tending"
                    ],
                    "antonyms": [
                        "neglect",
                        "deprivation"
                    ],
                    "common_phrases": [
                        "maternal nurturance",
                        "emotional nurturance",
                        "need for nurturance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по обеспечению физического ухода и эмоциональной поддержки, особенно в отношении детей или подопечных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забота",
                                "target_lang_sense_clarification": "внимание и уход за кем-либо"
                            },
                            {
                                "target_lang_word": "воспитание",
                                "target_lang_sense_clarification": "процесс формирования личности и ухода за ребенком"
                            },
                            {
                                "target_lang_word": "поддержка",
                                "target_lang_sense_clarification": "оказание помощи, в том числе эмоциональной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6607e49e-5e6b-3b92-8213-6d6dd97bab4d",
                    "sense_definition": "The process of encouraging the growth, development, or success of something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "act_of_nurturing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school is dedicated to the <w>nurturance</w> of artistic talent in young students.",
                            "target_lang_translations": {
                                "ru": "Школа посвящает себя <w>развитию</w> художественных талантов у юных учеников."
                            }
                        },
                        {
                            "text": "Successful innovation requires the careful <w>nurturance</w> of new ideas within the company.",
                            "target_lang_translations": {
                                "ru": "Успешные инновации требуют тщательного <w>взращивания</w> новых идей внутри компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "fostering",
                        "cultivation",
                        "promotion",
                        "development"
                    ],
                    "antonyms": [
                        "suppression",
                        "hindrance"
                    ],
                    "common_phrases": [
                        "nurturance of talent",
                        "nurturance of ideas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс поощрения роста, развития или успеха чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взращивание",
                                "target_lang_sense_clarification": "бережное развитие чего-либо (идей, талантов)"
                            },
                            {
                                "target_lang_word": "развитие",
                                "target_lang_sense_clarification": "процесс совершенствования"
                            },
                            {
                                "target_lang_word": "культивирование",
                                "target_lang_sense_clarification": "создание условий для роста и развития"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
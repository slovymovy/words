    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a57926eb-ffc5-3429-9fb5-840551f87f5e",
                    "sense_definition": "Сделать, произвести какую-либо вещь, предмет или продукт (обычно с использованием инструментов или оборудования).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Завод должен <w>изготовить</w> новую партию деталей к концу месяца.",
                            "target_lang_translations": {
                                "en": "The factory must <w>manufacture</w> a new batch of parts by the end of the month."
                            }
                        },
                        {
                            "text": "Мастер сумел <w>изготовить</w> точную копию ключа за пять минут.",
                            "target_lang_translations": {
                                "en": "The craftsman managed to <w>make</w> an exact copy of the key in five minutes."
                            }
                        }
                    ],
                    "synonyms": [
                        "сделать",
                        "произвести",
                        "создать",
                        "выработать"
                    ],
                    "antonyms": [
                        "уничтожить",
                        "сломать",
                        "разрушить"
                    ],
                    "common_phrases": [
                        "изготовить деталь",
                        "изготовить макет",
                        "изготовить мебель",
                        "изготовить по заказу"
                    ],
                    "target_lang_definitions": {
                        "en": "To make or produce an object or product (usually using tools or equipment)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "manufacture",
                                "target_lang_sense_clarification": "industrial production"
                            },
                            {
                                "target_lang_word": "produce",
                                "target_lang_sense_clarification": "general production"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "general creation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "265f7336-05f7-3cf5-b7d4-06e631b1ed04",
                    "sense_definition": "Приготовить еду, напиток или смесь (часто подразумевается сложный или кустарный процесс).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Повар обещал быстро <w>изготовить</w> нам вкусный ужин из того, что было.",
                            "target_lang_translations": {
                                "en": "The cook promised to quickly <w>prepare</w> us a delicious dinner from what was available."
                            }
                        },
                        {
                            "text": "Аптекарь может <w>изготовить</w> лекарство по индивидуальному рецепту.",
                            "target_lang_translations": {
                                "en": "The pharmacist can <w>prepare</w> medicine according to an individual prescription."
                            }
                        }
                    ],
                    "synonyms": [
                        "приготовить",
                        "состряпать"
                    ],
                    "common_phrases": [
                        "изготовить обед",
                        "изготовить лекарство"
                    ],
                    "target_lang_definitions": {
                        "en": "To prepare food, a drink, or a mixture (often implying a complex process)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prepare",
                                "target_lang_sense_clarification": "food or mixtures"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "food preparation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54c5be5d-5d75-35d7-a768-8659b07051ce",
                    "sense_definition": "Привести в состояние готовности к использованию или действию; подготовить.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир отдал приказ <w>изготовить</w> оружие к бою.",
                            "target_lang_translations": {
                                "en": "The commander gave the order to <w>prepare</w> weapons for battle."
                            }
                        },
                        {
                            "text": "Механики успели <w>изготовить</w> самолёт к вылету до начала шторма.",
                            "target_lang_translations": {
                                "en": "The mechanics managed to <w>prepare</w> the plane for takeoff before the storm started."
                            }
                        }
                    ],
                    "synonyms": [
                        "подготовить"
                    ],
                    "common_phrases": [
                        "изготовить к бою",
                        "изготовить к стрельбе"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring to a state of readiness for use or action; to prepare."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prepare",
                                "target_lang_sense_clarification": "get ready for use"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "70743256-7527-32cf-a348-5003d7d232d1",
                    "sense_definition": "Wskazuje kierunek ruchu, miejsce docelowe lub cel podróży.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano jeżdżę autobusem <w>do</w> pracy.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я езжу на автобусе <w>на</w> работу."
                            }
                        },
                        {
                            "text": "W te wakacje planujemy pojechać <w>do</w> Włoch.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы планируем поехать <w>в</w> Италию."
                            }
                        }
                    ],
                    "synonyms": [
                        "ku"
                    ],
                    "antonyms": [
                        "z",
                        "od"
                    ],
                    "common_phrases": [
                        "do domu",
                        "do pracy",
                        "do szkoły",
                        "do miasta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает направление движения, место назначения или цель поездки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "указывает на направление внутрь или в страну/город"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "указывает на направление на поверхность или мероприятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad32420d-9c70-3f0a-8e27-6d91473833ac",
                    "sense_definition": "Wskazuje na ruch skierowany do wnętrza czegoś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę włożyć brudne naczynia <w>do</w> zmywarki.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поставьте грязную посуду <w>в</w> посудомоечную машину."
                            }
                        },
                        {
                            "text": "Dziecko wskoczyło <w>do</w> wody na głębokim basenie.",
                            "target_lang_translations": {
                                "ru": "Ребёнок прыгнул <w>в</w> воду в глубоком бассейне."
                            }
                        }
                    ],
                    "antonyms": [
                        "z"
                    ],
                    "common_phrases": [
                        "do środka",
                        "do łóżka",
                        "do wody"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на движение, направленное внутрь чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "указывает на перемещение внутрь объекта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "246f96a5-11bd-3512-b101-f8c8a9155897",
                    "sense_definition": "Wyznacza końcowy moment trwania czynności lub stanu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sklep jest otwarty od ósmej <w>do</w> dwudziestej.",
                            "target_lang_translations": {
                                "ru": "Магазин открыт с восьми <w>до</w> двадцати."
                            }
                        },
                        {
                            "text": "Będę czekać na ciebie <w>do</w> wieczora.",
                            "target_lang_translations": {
                                "ru": "Я буду ждать тебя <w>до</w> вечера."
                            }
                        }
                    ],
                    "antonyms": [
                        "od"
                    ],
                    "common_phrases": [
                        "do widzenia",
                        "do jutra",
                        "do zobaczenia",
                        "od rana do wieczora"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает конечный момент действия или состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "указывает на временной предел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0530a697-15a2-3126-a074-802b9f12badc",
                    "sense_definition": "Wskazuje adresata lub odbiorcę czynności, wypowiedzi lub uczucia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "recipient",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napisałem długi list <w>do</w> mojej babci.",
                            "target_lang_translations": {
                                "ru": "Я написал длинное письмо <w>к</w> моей бабушке."
                            }
                        },
                        {
                            "text": "Mówię <w>do</w> ciebie, więc proszę, słuchaj mnie uważnie.",
                            "target_lang_translations": {
                                "ru": "Я обращаюсь <w>к</w> тебе, поэтому, пожалуйста, слушай меня внимательно."
                            }
                        }
                    ],
                    "antonyms": [
                        "od"
                    ],
                    "common_phrases": [
                        "pisać do kogoś",
                        "dzwonić do kogoś",
                        "mówić do siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает адресата или получателя действия, высказывания или чувства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "указывает на лицо, к которому обращено действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edbee49d-7e66-391e-b486-ceafd99cfece",
                    "sense_definition": "Określa przeznaczenie przedmiotu lub cel czynności.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem nową szczoteczkę <w>do</w> zębów.",
                            "target_lang_translations": {
                                "ru": "Я купил новую щётку <w>для</w> зубов."
                            }
                        },
                        {
                            "text": "Czy masz coś zimnego <w>do</w> picia?",
                            "target_lang_translations": {
                                "ru": "У тебя есть что-нибудь холодное <w>для</w> питья?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "do wynajęcia",
                        "do sprzedania",
                        "do jedzenia",
                        "do picia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определяет назначение предмета или цель действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для",
                                "target_lang_sense_clarification": "указывает на предназначение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2abbf62d-489a-3cb9-87f1-4d7392bd49e0",
                    "sense_definition": "Wskazuje ostateczny termin wykonania czynności.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz oddać ten projekt <w>do</w> końca tygodnia.",
                            "target_lang_translations": {
                                "ru": "Ты должен сдать этот проект <w>до</w> конца недели."
                            }
                        },
                        {
                            "text": "Raport powinien być gotowy <w>do</w> południa.",
                            "target_lang_translations": {
                                "ru": "Отчёт должен быть готов <w>к</w> полудню."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do teraz",
                        "do tej pory",
                        "do niedawna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает крайний срок выполнения действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "до наступления срока"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "к определенному моменту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0b8b43f-21e2-3043-b78e-6fb0dc93e62a",
                    "sense_definition": "Wskazuje na przynależność do kogoś lub czegoś (łączy się z czasownikiem 'należeć').",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "belonging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten piękny dom należy <w>do</w> moich rodziców.",
                            "target_lang_translations": {
                                "ru": "Этот красивый дом принадлежит моим родителям."
                            }
                        },
                        {
                            "text": "Polska od wielu lat należy <w>do</w> Unii Europejskiej.",
                            "target_lang_translations": {
                                "ru": "Польша уже много лет входит <w>в</w> Европейский Союз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "należeć do"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на принадлежность кому-либо или чему-либо (с глаголом 'принадлежать')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "входить в состав (группы, организации)"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "принадлежать к (категории)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "167b6696-59dc-39c7-b0fa-76c7c1e27b6e",
                    "sense_definition": "Określa górną granicę liczby, miary lub zakresu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód rozpędza się <w>do</w> stu kilometrów na godzinę.",
                            "target_lang_translations": {
                                "ru": "Автомобиль разгоняется <w>до</w> ста километров в час."
                            }
                        },
                        {
                            "text": "W tej sali mieści się <w>do</w> pięćdziesięciu osób.",
                            "target_lang_translations": {
                                "ru": "В этом зале помещается <w>до</w> пятидесяти человек."
                            }
                        }
                    ],
                    "common_phrases": [
                        "liczyć do dziesięciu",
                        "do pełna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определяет верхнюю границу числа, меры или диапазона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "указывает на количественный предел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93ce5e62-7992-371c-b68f-8fefd1de8c99",
                    "sense_definition": "Określa okres trwania jakiegoś stanu (użycie rzadkie, zastępowane przez 'przez').",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chorowała ciężko <w>do</w> pół roku.",
                            "target_lang_translations": {
                                "ru": "Она тяжело болела <w>до</w> полугода."
                            }
                        },
                        {
                            "text": "Został tam <w>do</w> kilku lat.",
                            "target_lang_translations": {
                                "ru": "Он оставался там <w>до</w> нескольких лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "przez"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie częściej używa się przyimka 'przez'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Определяет период длительности состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "указывает на продолжительность"
                            }
                        ]
                    }
                }
            ]
        },
        {

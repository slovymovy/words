                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pani <w>Ołów</w> jest naszą nową nauczycielką chemii w szkole podstawowej.",
                            "target_lang_translations": {
                                "ru": "Госпожа <w>Олув</w> — наша новая учительница химии в начальной школе."
                            }

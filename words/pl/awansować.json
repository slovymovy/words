{
    "word_family": [
        "awans",
        "awansowy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "05c3e363-2e9f-3a59-ba0b-8c61ac77cc42",
                    "sense_definition": "Otrzymywać wyższe stanowisko, stopień lub przechodzić na wyższy szczebel w hierarchii zawodowej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "kariera",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat ciężko pracował przez lata i w końcu <awansował> na kierownika działu.",
                            "target_lang_translations": {
                                "ru": "Мой brat усердно работал много лет и в конце концов <получил повышение> до начальника отдела."
                            }
                        },
                        {
                            "text": "Jeśli chcesz szybko <awansować> w tej firmie, musisz wykazać się dużą inicjatywą.",
                            "target_lang_translations": {
                                "ru": "Если ты хочешь быстро <продвинуться по службе> в этой компании, тебе нужно проявить большую инициативу."
                            }
                        }
                    ],
                    "synonyms": [
                        "piąć się",
                        "wyawansować",
                        "obejmować stanowisko"
                    ],
                    "common_phrases": [
                        "awansować na stanowisko",
                        "szybko awansować",
                        "awansować społecznie",
                        "awansować w hierarchii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать более высокую должность или переходить на более высокую ступень в профессиональной иерархии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получить повышение",
                                "target_lang_sense_clarification": "карьера"
                            },
                            {
                                "target_lang_word": "продвинуться по службе",
                                "target_lang_sense_clarification": "карьера"
                            },
                            {
                                "target_lang_word": "пойти на повышение",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "522c9d8a-114f-328f-9619-a35040ab3130",
                    "sense_definition": "Nadawać komuś wyższe stanowisko, stopień służbowy lub rangę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "kariera",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor postanowił <awansować> ją za wybitne osiągnięcia w zarządzaniu projektami.",
                            "target_lang_translations": {
                                "ru": "Директор решил <повысить> её за выдающиеся достижения в управлении проектами."
                            }
                        },
                        {
                            "text": "W wojsku można <awansować> żołnierza za odwagę na polu walki.",
                            "target_lang_translations": {
                                "ru": "В армии можно <повысить> солдата в звании за отвагу на поле боя."
                            }
                        }
                    ],
                    "synonyms": [
                        "mianować",
                        "promować",
                        "dawać",
                        "dać awans",
                        "honorować",
                        "podnosić"
                    ],
                    "antonyms": [
                        "degradować",
                        "zdegradować"
                    ],
                    "common_phrases": [
                        "awansować pracownika",
                        "awansować podwładnego",
                        "awansować na stopień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Назначать кого-либо на более высокую должность, присваивать служебную степень или звание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повысить",
                                "target_lang_sense_clarification": "кого-то в должности"
                            },
                            {
                                "target_lang_word": "присвоить звание",
                                "target_lang_sense_clarification": "военное/служебное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10245e60-5af9-3594-8480-4574cb29c1b2",
                    "sense_definition": "Przechodzić do kolejnego, wyższego etapu rozgrywek sportowych lub konkursu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza reprezentacja wygrała ostatni mecz i <awansowała> do finału mistrzostw świata.",
                            "target_lang_translations": {
                                "ru": "Наша сборная выиграла последний матч и <вышла> в финал чемпионата мира."
                            }
                        },
                        {
                            "text": "Tylko dwie najlepsze drużyny z grupy mogą <awansować> do dalszych rozgrywek.",
                            "target_lang_translations": {
                                "ru": "Только две лучшие команды из группы могут <пройти> в следующий этап соревнований."
                            }
                        }
                    ],
                    "common_phrases": [
                        "awansować do finału",
                        "awansować do grupy",
                        "awansować do ligi",
                        "awansować do ćwierćfinału"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переходить в следующий, более высокий этап спортивных соревнований или конкурса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "в финал/следующий круг"
                            },
                            {
                                "target_lang_word": "пройти",
                                "target_lang_sense_clarification": "дальше в соревнованиях"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
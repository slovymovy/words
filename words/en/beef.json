{
    "word_family": [
        "beefy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "22048679-803f-3cf1-84ec-cd682c0e3a25",
                    "sense_definition": "The meat obtained from cattle (cows, bulls, or oxen).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "meat_food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are having roast <w>beef</w> and potatoes for dinner.",
                            "target_lang_translations": {
                                "ru": "У нас на ужин запеченная <w>говядина</w> с картошкой."
                            }
                        },
                        {
                            "text": "I prefer chicken, but my brother loves <w>beef</w>.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю курицу, но мой брат любит <w>говядину</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "meat"
                    ],
                    "common_phrases": [
                        "roast beef",
                        "ground beef",
                        "corned beef",
                        "beef jerky"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясо крупного рогатого скота (коров, быков или волов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говядина",
                                "target_lang_sense_clarification": "мясо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e7805a4-e7f8-3dc7-b77b-9a0b2f5586d2",
                    "sense_definition": "A complaint, argument, or long-standing disagreement between people.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has a serious <w>beef</w> with his neighbor about the noise.",
                            "target_lang_translations": {
                                "ru": "У него серьезные <w>тёрки</w> с соседом из-за шума."
                            }
                        },
                        {
                            "text": "They finally settled their <w>beef</w> and shook hands.",
                            "target_lang_translations": {
                                "ru": "Они наконец уладили свой <w>конфликт</w> и пожали руки."
                            }
                        }
                    ],
                    "synonyms": [
                        "beef up",
                        "meat"
                    ],
                    "common_phrases": [
                        "have a beef",
                        "start beef",
                        "settle a beef"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жалоба, спор или давние разногласия между людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разборка",
                                "target_lang_sense_clarification": "сленг: конфликт, выяснение отношений"
                            },
                            {
                                "target_lang_word": "конфликт",
                                "target_lang_sense_clarification": "разногласие"
                            },
                            {
                                "target_lang_word": "тёрки",
                                "target_lang_sense_clarification": "сленг: неприятности, споры"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7c5d99bf-0824-3ebc-95b6-54999b653e72",
                    "sense_definition": "Consisting of or containing beef as an ingredient.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "meat_food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She ordered a hearty <w>beef</w> stew for lunch.",
                            "target_lang_translations": {
                                "ru": "Она заказала сытное <w>говяжье</w> рагу на обед."
                            }
                        },
                        {
                            "text": "The <w>beef</w> sandwich comes with a side of fries.",
                            "target_lang_translations": {
                                "ru": "Сэндвич с <w>говядиной</w> подается с картошкой фри."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beef stew",
                        "beef stock",
                        "beef patty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий из говядины или содержащий её в качестве ингредиента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говяжий",
                                "target_lang_sense_clarification": "сделанный из говядины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56f03672-4bc1-32cf-971d-d927d5a6f94c",
                    "sense_definition": "Relating to the raising of cattle for meat production.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "meat_food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The region is famous for its many <w>beef</w> farms.",
                            "target_lang_translations": {
                                "ru": "Этот регион славится своими многочисленными <w>мясными</w> фермами."
                            }
                        },
                        {
                            "text": "They raise <w>beef</w> cattle in the open fields.",
                            "target_lang_translations": {
                                "ru": "Они выращивают <w>мясной</w> скот на открытых полях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beef cattle",
                        "beef farming",
                        "beef industry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к разведению крупного рогатого скота для производства мяса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мясной",
                                "target_lang_sense_clarification": "о породе скота или направлении хозяйства"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a85381da-8440-383a-bb17-2ee672fb4bd2",
                    "sense_definition": "To make something stronger, bigger, or more effective (usually followed by 'up').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "strength",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company decided to <w>beef</w> up its security systems.",
                            "target_lang_translations": {
                                "ru": "Компания решила <w>усилить</w> свои системы безопасности."
                            }
                        },
                        {
                            "text": "We need to <w>beef</w> up the team with some experienced players.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>укрепить</w> команду несколькими опытными игроками."
                            }
                        }
                    ],
                    "synonyms": [
                        "beef up"
                    ],
                    "common_phrases": [
                        "beef up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Укрепить, усилить или сделать что-то более эффективным (обычно с предлогом 'up')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усиливать",
                                "target_lang_sense_clarification": "делать мощнее"
                            },
                            {
                                "target_lang_word": "укреплять",
                                "target_lang_sense_clarification": "делать надежнее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29a52e8d-8510-3990-90b2-9b2a42e0eda6",
                    "sense_definition": "To have a long-standing disagreement or fight with someone.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Those two groups have been <w>beefing</w> for years.",
                            "target_lang_translations": {
                                "ru": "Эти две группы <w>враждуют</w> уже много лет."
                            }
                        },
                        {
                            "text": "It is better to talk it out than to keep <w>beefing</w> with each other.",
                            "target_lang_translations": {
                                "ru": "Лучше всё обсудить, чем продолжать <w>грызться</w> друг с другом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь давние разногласия или враждовать с кем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "враждовать",
                                "target_lang_sense_clarification": "находиться в конфликте"
                            },
                            {
                                "target_lang_word": "грызться",
                                "target_lang_sense_clarification": "сленг: постоянно ссориться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "786b0d81-7afa-3089-886a-53d513884519",
                    "sense_definition": "To complain or grumble about something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stop <w>beefing</w> about the weather and let's go.",
                            "target_lang_translations": {
                                "ru": "Хватит <w>ныть</w> из-за погоды, пошли."
                            }
                        },
                        {
                            "text": "He sat in the corner <w>beefing</w> about his bad luck.",
                            "target_lang_translations": {
                                "ru": "Он сидел в углу, <w>жалуясь</w> на свое невезение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beef about"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жаловаться или ворчать по поводу чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жаловаться",
                                "target_lang_sense_clarification": "выражать недовольство"
                            },
                            {
                                "target_lang_word": "ворчать",
                                "target_lang_sense_clarification": "высказывать недовольство в раздраженной манере"
                            },
                            {
                                "target_lang_word": "ныть",
                                "target_lang_sense_clarification": "разг.: надоедливо жаловаться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "obsession",
        "obsessive",
        "obsessively",
        "obsessiveness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4d9aa824-bcbf-3237-97c7-05a7c656a82f",
                    "sense_definition": "Intensely preoccupied with or by a given topic or emotion; driven by a specified obsession.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "preoccupied",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is completely <w>obsessed</w> with the new video game she bought yesterday.",
                            "target_lang_translations": {
                                "ru": "Она совершенно <w>помешана</w> на новой видеоигре, которую купила вчера."
                            }
                        },
                        {
                            "text": "He became <w>obsessed</w> with finding the truth behind the mysterious letter.",
                            "target_lang_translations": {
                                "ru": "Он стал <w>одержим</w> идеей найти правду, скрывающуюся за таинственным письмом."
                            }
                        }
                    ],
                    "synonyms": [
                        "fixated",
                        "preoccupied",
                        "infatuated",
                        "consumed",
                        "hooked"
                    ],
                    "antonyms": [
                        "indifferent",
                        "unconcerned"
                    ],
                    "common_phrases": [
                        "obsessed with",
                        "totally obsessed",
                        "become obsessed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно увлеченный чем-либо или кем-либо; зацикленный на определенной теме или эмоции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одержимый",
                                "target_lang_sense_clarification": "страстно увлеченный"
                            },
                            {
                                "target_lang_word": "помешанный",
                                "target_lang_sense_clarification": "сильно увлеченный (разг.)"
                            },
                            {
                                "target_lang_word": "зацикленный",
                                "target_lang_sense_clarification": "сфокусированный на одном"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07eaa231-2d41-395f-8983-62ab15cc3267",
                    "sense_definition": "Influenced or controlled by evil spirits, but less than possessed in that the spirits do not actually reside in the victim.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "spirits",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villagers believed the strange man was <w>obsessed</w> by a dark spirit.",
                            "target_lang_translations": {
                                "ru": "Жители деревни верили, что этот странный человек был <w>одержим</w> темным духом."
                            }
                        },
                        {
                            "text": "In the ancient legend, the hero is <w>obsessed</w> by demons that cloud his judgment.",
                            "target_lang_translations": {
                                "ru": "В древней легенде герой <w>одержим</w> демонами, которые затуманивают его рассудок."
                            }
                        }
                    ],
                    "synonyms": [
                        "haunted",
                        "plagued",
                        "tormented"
                    ],
                    "antonyms": [
                        "free"
                    ],
                    "common_phrases": [
                        "obsessed by spirits"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "usage regarding spirits"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся под влиянием злых духов или навязчивых идей (устаревшее или специфическое значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одержимый",
                                "target_lang_sense_clarification": "находящийся во власти духов"
                            },
                            {
                                "target_lang_word": "обуянный",
                                "target_lang_sense_clarification": "охваченный (страстью или духами)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1f3660a5-7e04-3ea3-a88f-00e49b892383",
                    "sense_definition": "Simple past and past participle of <w>obsess</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The unsolved mystery <w>obsessed</w> the detective for many years.",
                            "target_lang_translations": {
                                "ru": "Нерешенная загадка много лет <w>не давала покоя</w> детективу."
                            }
                        },
                        {
                            "text": "She has <w>obsessed</w> over every small detail of the wedding plan.",
                            "target_lang_translations": {
                                "ru": "Она <w>зациклилась</w> на каждой мелочи свадебного плана."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие от глагола obsess (преследовать, не давать покоя, зацикливаться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не давал покоя",
                                "target_lang_sense_clarification": "о мыслях или идеях"
                            },
                            {
                                "target_lang_word": "зациклился",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "преследовал",
                                "target_lang_sense_clarification": "о навязчивой идее"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "06e12a57-94c9-3347-be95-f6d4f8837ce4",
                    "sense_definition": "Результат, к которому привело какое-либо действие, событие или процесс.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы долго спорили, но в конечном <итоге> пришли к согласию.",
                            "target_lang_translations": {
                                "en": "We argued for a long time, but in the <w>end</w> we came to an agreement."
                            }
                        },
                        {
                            "text": "Печальный <итог> этой истории всех расстроил.",
                            "target_lang_translations": {
                                "en": "The sad <w>outcome</w> of this story upset everyone."
                            }
                        }
                    ],
                    "synonyms": [
                        "результат",
                        "исход",
                        "конец",
                        "вывод",
                        "следствие"
                    ],
                    "antonyms": [
                        "процесс"
                    ],
                    "common_phrases": [
                        "подвести итог",
                        "в конечном итоге",
                        "по итогам",
                        "печальный итог"
                    ],
                    "target_lang_definitions": {
                        "en": "The result or consequence produced by an action, event, or process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "result",
                                "target_lang_sense_clarification": "consequence or effect"
                            },
                            {
                                "target_lang_word": "outcome",
                                "target_lang_sense_clarification": "final product or consequence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87be275a-34c9-3097-99c0-87f1574e9c1f",
                    "sense_definition": "Общая сумма, полученная при сложении чисел или подсчёте расходов.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бухгалтер проверил общий <итог> в конце отчёта.",
                            "target_lang_translations": {
                                "en": "The accountant checked the grand <w>total</w> at the end of the report."
                            }
                        },
                        {
                            "text": "Если сложить все расходы, <итог> получится слишком большим.",
                            "target_lang_translations": {
                                "en": "If you add up all the expenses, the <w>total</w> will be too large."
                            }
                        }
                    ],
                    "synonyms": [
                        "сумма"
                    ],
                    "common_phrases": [
                        "общий итог",
                        "подводить итог"
                    ],
                    "target_lang_definitions": {
                        "en": "The total amount obtained by adding numbers or calculating expenses."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "total",
                                "target_lang_sense_clarification": "sum of smaller amounts"
                            },
                            {
                                "target_lang_word": "sum",
                                "target_lang_sense_clarification": "quantity obtained by addition"
                            }
                        ]
                    }
                }
            ]

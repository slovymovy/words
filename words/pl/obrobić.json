    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7df24f8d-508f-36e6-b7e4-5c940b736f56",
                    "sense_definition": "Nadać czemuś ostateczny kształt, wygląd lub właściwości poprzez wykonanie odpowiednich czynności technicznych (np. szlifowanie, skrawanie, edycję cyfrową).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "processing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Grafik musi starannie <obrobić> zdjęcia z sesji, zanim trafią one do druku.",
                            "target_lang_translations": {
                                "ru": "Графический дизайнер должен тщательно <обработать> фотографии с сессии, прежде чем они пойдут в печать."
                            }
                        },
                        {
                            "text": "Doświadczony stolarz potrafi pięknie <obrobić> drewno, nadając mu gładką powierzchnię.",
                            "target_lang_translations": {
                                "ru": "Опытный плотник может красиво <обработать> дерево, придав ему гладкую поверхность."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykończyć"
                    ],
                    "common_phrases": [
                        "obrobić zdjęcia",
                        "obrobić materiał",
                        "obrobić skrawaniem",
                        "obrobić dane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придать чему-либо окончательную форму, вид или свойства с помощью технических действий (например, шлифовка, резка, цифровая обработка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обработать",
                                "target_lang_sense_clarification": "материал, данные, фотографии"
                            },
                            {
                                "target_lang_word": "отредактировать",
                                "target_lang_sense_clarification": "фотографии, цифровой контент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "920b1329-b2db-3b41-b1da-667c7e52e5e7",
                    "sense_definition": "Pozbawić kogoś pieniędzy lub mienia poprzez kradzież; okraść jakieś miejsce.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "theft",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodzieje zaplanowali, jak w nocy <obrobić> sklep jubilerski w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Воры спланировали, как ночью <обчистить> ювелирный магазин в центре города."
                            }
                        },
                        {
                            "text": "Musisz uważać w tłumie, bo kieszonkowcy mogą cię łatwo <obrobić> z portfela.",
                            "target_lang_translations": {
                                "ru": "Нужно быть осторожным в толпе, так как карманники могут легко <обчистить> тебя, украв кошелек."
                            }
                        }
                    ],
                    "synonyms": [
                        "okraść",
                        "obrabować",
                        "oskubać",
                        "opędzlować",
                        "zwędzić"
                    ],
                    "common_phrases": [
                        "obrobić bank",
                        "obrobić mieszkanie",
                        "obrobić kogoś",
                        "dać się obrobić"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишить кого-либо денег или имущества путем кражи; обокрасть какое-либо место (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обчистить",
                                "target_lang_sense_clarification": "разговорное; украсть всё ценное"
                            },
                            {
                                "target_lang_word": "ограбить",
                                "target_lang_sense_clarification": "совершить кражу или грабеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "474df7e0-54d4-3d0b-9a38-c137f5aba665",
                    "sense_definition": "Mówić o kimś źle, krytykować lub plotkować na czyjś temat, zazwyczaj pod nieobecność tej osoby.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiadki uwielbiają siedzieć przed blokiem i <obrobić> każdego, kto akurat przechodzi.",
                            "target_lang_translations": {
                                "ru": "Соседки обожают сидеть перед домом и <перемывать косточки> каждому, кто проходит мимо."
                            }
                        },
                        {
                            "text": "Nie powinieneś tak <obrobić> swojego przyjaciela przed innymi ludźmi.",
                            "target_lang_translations": {
                                "ru": "Ты не должен так <обсуждать> своего друга перед другими людьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "obgadać",
                        "obmówić"
                    ],
                    "common_phrases": [
                        "obrobić komuś tyłek"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить о ком-то плохо, критиковать или сплетничать, обычно в отсутствие этого человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перемыть косточки",
                                "target_lang_sense_clarification": "идиома; сплетничать, обсуждать кого-то"
                            },
                            {
                                "target_lang_word": "обсудить",
                                "target_lang_sense_clarification": "говорить о ком-то (часто в негативном ключе)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a34a9abb-d8c0-3b2e-8618-3f1cf7b4a04b",
                    "sense_definition": "Przygotować ziemię do uprawy roślin poprzez wykonywanie prac polowych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik musi szybko <obrobić> pole, zanim nadejdą pierwsze jesienne przymrozki.",
                            "target_lang_translations": {
                                "ru": "Фермер должен быстро <обработать> поле, пока не наступили первые осенние заморозки."
                            }
                        },
                        {
                            "text": "Wiosną cała rodzina pomagała dziadkowi <obrobić> duży ogród warzywny.",
                            "target_lang_translations": {
                                "ru": "Весной вся семья помогала дедушке <обработать> большой огород."
                            }
                        }
                    ],
                    "synonyms": [
                        "uprawić"
                    ],
                    "common_phrases": [

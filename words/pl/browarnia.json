                {
                    "sense_id": "5ee680a2-16e0-399b-84ab-6cd7d54f6aef",
                    "sense_definition": "Budynek lub zakład przemysłowy, w którym produkuje się piwo.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "production_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stara <w>browarnia</w> została odnowiona i przekształcona w muzeum.",
                            "target_lang_translations": {
                                "ru": "Старая <w>пивоварня</w> была отреставрирована и превращена в музей."
                            }
                        },
                        {
                            "text": "W tym mieście działała kiedyś duża <w>browarnia</w>, zatrudniająca setki osób.",
                            "target_lang_translations": {
                                "ru": "В этом городе когда-то работала большая <w>пивоварня</w>, где трудились сотни людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "browar"
                    ],
                    "common_phrases": [
                        "stara browarnia",
                        "miejska browarnia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание или промышленное предприятие, на котором производят пиво."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пивоварня",
                                "target_lang_sense_clarification": "место производства"
                            },
                            {
                                "target_lang_word": "пивоваренный завод",
                                "target_lang_sense_clarification": "крупное предприятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4e98cb2-50be-3bb5-8502-33a5e7734dd6",
                    "sense_definition": "Lokal gastronomiczny, w którym podaje się gościom piwo, często warzone na miejscu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "serving_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczorem chętnie odwiedzamy lokalną <w>browarnię</w>, by spróbować świeżego trunku.",
                            "target_lang_translations": {
                                "ru": "Вечером мы охотно посещаем местную <w>пивоварню</w>, чтобы попробовать свежего напитка."
                            }
                        },
                        {
                            "text": "Ta <w>browarnia</w> słynie z głośnej muzyki i doskonałej atmosfery.",
                            "target_lang_translations": {
                                "ru": "Эта <w>пивная</w> славится громкой музыкой и отличной атмосферой."
                            }
                        }
                    ],
                    "synonyms": [
                        "piwiarnia"
                    ],
                    "common_phrases": [
                        "lokalna browarnia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гастрономическое заведение, в котором гостям подают пиво, часто сваренное на месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пивной ресторан",
                                "target_lang_sense_clarification": "заведение общепита"
                            },
                            {
                                "target_lang_word": "пивная",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a68a75e1-1450-340a-a919-c1236a9c942a",
                    "sense_definition": "Miejsce lub dział zakładu zajmujący się rozlewaniem piwa do naczyń.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "production_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Automatyczna <w>browarnia</w> napełnia tysiące butelek w ciągu jednej godziny.",
                            "target_lang_translations": {
                                "ru": "Автоматическая <w>линия розлива</w> наполняет тысячи бутылок за один час."
                            }
                        },
                        {
                            "text": "Pracownicy kontrolują proces, za który odpowiada <w>browarnia</w> przy taśmie produkcyjnej.",
                            "target_lang_translations": {
                                "ru": "Работники контролируют процесс, за который отвечает <w>цех розлива</w> у конвейерной ленты."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или отдел предприятия, занимающийся розливом пива в тару."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цех розлива",

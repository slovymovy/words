    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ff77b2b4-5af5-31c1-82a4-2d219ff7bd2e",
                    "sense_definition": "Крупная авария или катастрофа, происходящая с транспортным средством (поездом, судном, самолётом) в пути.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В новостях сообщили, что причиной <крушения> поезда стала неисправность путей.",
                            "target_lang_translations": {
                                "en": "The news reported that the train <wreck> was caused by a track malfunction."
                            }
                        },
                        {
                            "text": "Капитан корабля чудом выжил, когда его судно потерпело <крушение> у скалистых берегов.",
                            "target_lang_translations": {
                                "en": "The ship's captain miraculously survived when his vessel was <wrecked> off the rocky shores."
                            }
                        }
                    ],
                    "synonyms": [
                        "катастрофа",
                        "авария",
                        "кораблекрушение"
                    ],
                    "common_phrases": [
                        "крушение поезда",
                        "потерпеть крушение",
                        "место крушения",
                        "крушение самолёта"
                    ],
                    "target_lang_definitions": {
                        "en": "A major accident or disaster involving a vehicle (train, ship, plane) during a journey."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wreck",
                                "target_lang_sense_clarification": "accident involving a train or ship"
                            },
                            {
                                "target_lang_word": "crash",
                                "target_lang_sense_clarification": "violent collision, especially of a plane or car"
                            },
                            {
                                "target_lang_word": "shipwreck",
                                "target_lang_sense_clarification": "destruction of a ship at sea"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4eff911-35eb-3e10-85a9-0f9127af4873",
                    "sense_definition": "Полный крах, гибель или утрата чего-либо значимого (надежд, планов, идеалов).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Внезапная болезнь привела к <крушению> всех его амбициозных планов на будущее.",
                            "target_lang_translations": {
                                "en": "A sudden illness led to the <collapse> of all his ambitious plans for the future."
                            }
                        },
                        {
                            "text": "Это событие означало окончательное <крушение> их надежд на мирное решение конфликта.",
                            "target_lang_translations": {
                                "en": "This event meant the final <ruin> of their hopes for a peaceful resolution to the conflict."
                            }
                        }
                    ],
                    "synonyms": [
                        "крах",
                        "провал",
                        "гибель",
                        "коллапс"
                    ],
                    "common_phrases": [
                        "крушение надежд",
                        "крушение планов",
                        "крушение империи",
                        "крушение иллюзий"
                    ],
                    "target_lang_definitions": {
                        "en": "Complete failure, ruin, or loss of something significant, such as hopes or plans."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "collapse",
                                "target_lang_sense_clarification": "sudden failure or breakdown of plans or hopes"
                            },
                            {
                                "target_lang_word": "ruin",
                                "target_lang_sense_clarification": "complete destruction of value or status"
                            },
                            {
                                "target_lang_word": "downfall",
                                "target_lang_sense_clarification": "loss of power, prosperity, or status"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81d3a933-680a-344c-99cd-816e0c618d25",
                    "sense_definition": "Действие по глаголу «крушить»; физическое разрушение или ломка чего-либо с силой.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Шум от <крушения> старых построек мешал жителям соседних домов спать.",
                            "target_lang_translations": {
                                "en": "The noise from the <demolition> of old buildings kept the residents of neighboring houses from sleeping."
                            }
                        },
                        {
                            "text": "Вандалы занимались бессмысленным <крушением> скамеек и фонарей в городском парке.",
                            "target_lang_translations": {
                                "en": "Vandals engaged in the senseless <smashing> of benches and streetlights in the city park."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрушение",
                        "ломка",
                        "разламывание",
                        "сокрушение"
                    ],
                    "antonyms": [
                        "восстановление"
                    ],
                    "target_lang_definitions": {
                        "en": "The physical act of destroying, breaking, or smashing something with force."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "destruction",
                                "target_lang_sense_clarification": "general action of destroying something"
                            },
                            {
                                "target_lang_word": "demolition",
                                "target_lang_sense_clarification": "tearing down of buildings or structures"
                            },
                            {
                                "target_lang_word": "smashing",
                                "target_lang_sense_clarification": "violent breaking into pieces"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
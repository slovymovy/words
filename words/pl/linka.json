                    "sense_definition": "Cienka lina, sznurek lub przewód, często używany w mechanizmach lub do mocowania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rope_cable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W moim rowerze zerwała się <w>linka</w> hamulcowa.",
                            "target_lang_translations": {
                                "ru": "На моем велосипеде порвался тормозной <w>тросик</w>."
                            }
                        },
                        {
                            "text": "Potrzebna nam mocna <w>linka</w>, żeby przymocować bagaż do dachu.",
                            "target_lang_translations": {
                                "ru": "Нам нужна крепкая <w>верёвка</w>, чтобы закрепить багаж на крыше."
                            }
                        }
                    ],
                    "synonyms": [
                        "linienie"
                    ],
                    "common_phrases": [
                        "linka hamulcowa",
                        "linka holownicza",
                        "linka zabezpieczająca"
                    ],
                    "target_lang_definitions": {

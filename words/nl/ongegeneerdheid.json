                {
                    "sense_id": "74d04981-7543-3d4b-8392-e4ddd0224e8d",
                    "sense_definition": "Het volledig ontbreken van schaamte of terughoudendheid.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>ongegeneerdheid</w> tijdens het officiële diner verbaasde alle aanwezige gasten.",
                            "target_lang_translations": {
                                "en": "His <w>shamelessness</w> during the official dinner surprised all the guests present.",
                                "ru": "Его <w>бесцеремонность</w> во время официального ужина удивила всех присутствующих гостей."
                            }
                        },
                        {
                            "text": "Zij sprak met een zekere <w>ongegeneerdheid</w> over haar persoonlijke problemen.",
                            "target_lang_translations": {
                                "en": "She spoke with a certain <w>unabashedness</w> about her personal problems.",
                                "ru": "Она говорила о своих личных проблемах с некоторой <w>развязностью</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "onbeschaamdheid"
                    ],
                    "antonyms": [
                        "schaamte",
                        "terughoudendheid"
                    ],
                    "common_phrases": [
                        "met ongegeneerdheid",
                        "totale ongegeneerdheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The complete lack of shame or restraint.",
                        "ru": "Полное отсутствие стыда или сдержанности."
                    },
                    "translations": {

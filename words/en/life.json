    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3d1727ad-d41e-3403-8f86-5b4393ae4290",
                    "sense_definition": "The state of being alive, characterized by growth, reproduction, and functional activity, which distinguishes living things from dead ones or inanimate matter.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "biological_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doctors worked very hard to save the patient's <w>life</w>.",
                            "target_lang_translations": {
                                "ru": "Врачи очень старались спасти <w>жизнь</w> пациента."
                            }
                        },
                        {
                            "text": "Do you believe that there is intelligent <w>life</w> on other planets?",
                            "target_lang_translations": {
                                "ru": "Вы верите, что на других планетах есть разумная <w>жизнь</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "existence",
                        "the world in general"
                    ],
                    "antonyms": [
                        "death"
                    ],
                    "common_phrases": [
                        "bring to life",
                        "come to life",
                        "sign of life",
                        "circle of life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние живого организма, характеризующееся ростом, размножением и функциональной активностью, отличающее живое от неживого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жизнь",
                                "target_lang_sense_clarification": "физиологическое состояние"
                            },
                            {
                                "target_lang_word": "существование",
                                "target_lang_sense_clarification": "наличие, бытие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44d540e3-aad8-3cd6-9e2e-88d63b7734d4",
                    "sense_definition": "The period of time between birth and death, or the period during which something functions or exists.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has lived in this small village all her <w>life</w>.",
                            "target_lang_translations": {
                                "ru": "Она прожила в этой маленькой деревне всю свою <w>жизнь</w>."
                            }
                        },
                        {
                            "text": "I have never seen such a beautiful sunset in my entire <w>life</w>.",
                            "target_lang_translations": {
                                "ru": "Я никогда в <w>жизни</w> не видел такого красивого заката."
                            }
                        }
                    ],
                    "synonyms": [
                        "existence"
                    ],
                    "common_phrases": [
                        "all my life",
                        "early in life",
                        "rest of your life",
                        "later in life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени от рождения до смерти или период существования чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жизнь",
                                "target_lang_sense_clarification": "период времени"
                            },
                            {
                                "target_lang_word": "век",
                                "target_lang_sense_clarification": "срок жизни (перен.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5de9012b-5418-3524-969a-bdee406292fd",
                    "sense_definition": "A particular way of living or the experiences that a person has.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "City <w>life</w> can be very noisy and busy compared to the country.",
                            "target_lang_translations": {
                                "ru": "Городская <w>жизнь</w> может быть очень шумной и суетливой по сравнению с деревенской."
                            }
                        },
                        {
                            "text": "He wants to build a new <w>life</w> with his family in Canada.",
                            "target_lang_translations": {
                                "ru": "Он хочет построить новую <w>жизнь</w> со своей семьей в Канаде."
                            }
                        }
                    ],
                    "synonyms": [
                        "experience"
                    ],
                    "common_phrases": [
                        "everyday life",
                        "private life",
                        "quality of life",
                        "way of life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный уклад жизни или совокупность пережитого опыта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жизнь",
                                "target_lang_sense_clarification": "быт, условия проживания"

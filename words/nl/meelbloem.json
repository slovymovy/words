                    "sense_id": "6eae72f2-6089-3c0e-9c76-1710814ca3f7",
                    "sense_definition": "Zeer fijn gemalen meel, meestal van tarwe, gebruikt voor het bakken van brood en gebak.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze gebruikte <w>meelbloem</w> om de cake extra luchtig te maken.",
                            "target_lang_translations": {
                                "en": "She used <w>fine flour</w> to make the cake extra airy.",
                                "ru": "Она использовала <w>муку</w>, чтобы сделать кекс особенно воздушным."
                            }
                        },
                        {
                            "text": "Voor dit recept heb je een kilo <w>meelbloem</w> en wat gist nodig.",
                            "target_lang_translations": {
                                "en": "For this recipe, you need a kilo of <w>flour</w> and some yeast.",
                                "ru": "Для этого рецепта тебе понадобится килограмм <w>муки</w> и немного дрожжей."
                            }
                        }
                    ],
                    "synonyms": [
                        "bloem",
                        "tarwebloem",
                        "patentbloem"
                    ],
                    "antonyms": [
                        "volkorenmeel"
                    ],
                    "common_phrases": [
                        "pak meelbloem",
                        "gezeefde meelbloem"
                    ],
                    "target_lang_definitions": {
                        "en": "Very finely ground flour, usually made from wheat, used for baking bread and pastries.",
                        "ru": "Очень мелкая мука, обычно пшеничная, используемая для выпечки хлеба и кондитерских изделий."
                    },

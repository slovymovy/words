    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "09806902-992a-3223-b113-cfad3d6419d3",
                    "sense_definition": "De manier waarop iets of iemand wordt voorgesteld, vaak in een artistieke context zoals toneel, film of beeldende kunst.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "representation_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De acteur kreeg veel lof voor zijn overtuigende <w>uitbeelding</w> van de historische figuur.",
                            "target_lang_translations": {
                                "ru": "Актер получил много похвалы за свое убедительное <w>воплощение</w> исторической личности."
                            }
                        },
                        {
                            "text": "In dit schilderij zien we een prachtige <w>uitbeelding</w> van het boerenleven in de negentiende eeuw.",
                            "target_lang_translations": {
                                "ru": "На этой картине мы видим прекрасное <w>изображение</w> крестьянской жизни девятнадцатого века."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertolking",
                        "voorstelling"
                    ],
                    "common_phrases": [
                        "artistieke uitbeelding",
                        "levendige uitbeelding",
                        "visuele uitbeelding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ представления кого-либо или чего-либо, часто в художественном контексте (театр, кино, изобразительное искусство)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изображение",
                                "target_lang_sense_clarification": "в живописи или описании"
                            },
                            {
                                "target_lang_word": "воплощение",
                                "target_lang_sense_clarification": "актерская игра, создание образа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d82581b-d32a-315e-bbd5-b64450823090",
                    "sense_definition": "Een concrete vorm of weergave die een abstract begrip, idee of gevoel zichtbaar maakt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "representation_symbolic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het standbeeld dient als een krachtige <w>uitbeelding</w> van de vrijheid die het volk heeft bevochten.",
                            "target_lang_translations": {
                                "ru": "Статуя служит мощным <w>олицетворением</w> свободы, за которую боролся народ."
                            }
                        },
                        {
                            "text": "Volgens de kunstenaar is deze sculptuur een <w>uitbeelding</w> van menselijk verdriet.",
                            "target_lang_translations": {
                                "ru": "По словам художника, эта скульптура является <w>воплощением</w> человеческой скорби."
                            }
                        }
                    ],
                    "synonyms": [
                        "representatie",
                        "verbeelding",
                        "beeltenis"
                    ],
                    "common_phrases": [
                        "symbolische uitbeelding",
                        "abstracte uitbeelding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная форма, делающая видимым абстрактное понятие, идею или чувство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "олицетворение",
                                "target_lang_sense_clarification": "воплощение качества или идеи"
                            },
                            {
                                "target_lang_word": "выражение",
                                "target_lang_sense_clarification": "проявление чувства или мысли"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "name",
        "nameless",
        "namesake"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "59aa2429-8112-38cd-abe0-bfc4e1a76ba2",
                    "sense_definition": "Used to mention the specific name or detail of something just referred to.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The group has a specific goal, <w>namely</w> to reduce local pollution.",
                            "target_lang_translations": {
                                "ru": "У группы есть конкретная цель, <w>а именно</w> уменьшить загрязнение окружающей среды в этом районе."
                            }
                        },
                        {
                            "text": "Only one person can answer that question, <w>namely</w> the manager.",
                            "target_lang_translations": {
                                "ru": "На этот вопрос может ответить только один человек, <w>а именно</w> менеджер."
                            }
                        }
                    ],
                    "synonyms": [
                        "specifically",
                        "that is to say",
                        "to wit",
                        "viz."
                    ],
                    "common_phrases": [
                        "namely that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания конкретного названия или детали того, о чем только что говорилось."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а именно",
                                "target_lang_sense_clarification": "употребляется для уточнения или конкретизации"
                            },
                            {
                                "target_lang_word": "то есть",
                                "target_lang_sense_clarification": "иными словами, пояснение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1051f77a-d4e3-3579-8f54-4237fb34779c",
                    "sense_definition": "Especially or above all others.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was loved by everyone, and <w>namely</w> by the queen.",
                            "target_lang_translations": {
                                "ru": "Его любили все, и <w>особенно</w> королева."
                            }
                        },
                        {
                            "text": "The knight fought many foes, <w>namely</w> the giants of the north.",
                            "target_lang_translations": {
                                "ru": "Рыцарь сражался со многими врагами, <w>особенно</w> с великанами севера."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This sense is no longer in common use."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Особенно или прежде всего (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "особенно",
                                "target_lang_sense_clarification": "выделение из ряда других"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "181720d5-4045-3be6-a69b-4ba37d3381e1",
                    "sense_definition": "Notable, distinguished, or famous.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was a <w>namely</w> scholar in his time.",
                            "target_lang_translations": {
                                "ru": "В свое время он был <w>видным</w> ученым."
                            }
                        },
                        {
                            "text": "There was no <w>namelier</w> hero in the ancient legends.",
                            "target_lang_translations": {
                                "ru": "В древних легендах не было более <w>прославленного</w> героя."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Rarely used in modern English."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выдающийся, знаменитый или известный (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знаменитый",
                                "target_lang_sense_clarification": "пользующийся известностью"
                            },
                            {
                                "target_lang_word": "видный",
                                "target_lang_sense_clarification": "выдающийся, значительный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "8cb08ff9-b06c-3a28-8d7f-390b0532d578",
                    "sense_definition": "Een ziekte, verwonding, schade of vertraging onbedoeld krijgen of opdoen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "incur_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft tijdens de wedstrijd een zware blessure <w>opgelopen</w>.",
                            "target_lang_translations": {
                                "en": "He <w>sustained</w> a serious injury during the match.",
                                "ru": "Он <w>получил</w> тяжелую травму во время матча."
                            }
                        },
                        {
                            "text": "Door de staking hebben we veel vertraging <w>opgelopen</w>.",
                            "target_lang_translations": {
                                "en": "Due to the strike, we <w>incurred</w> a lot of delay.",
                                "ru": "Из-за забастовки мы сильно <w>задержались</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "opdoen",
                        "krijgen"
                    ],
                    "common_phrases": [
                        "vertraging oplopen",
                        "schade oplopen",
                        "een blessure oplopen",
                        "een virus oplopen",
                        "een infectie oplopen"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fcb3bff8-faf8-33fe-b431-9a61e141cca7",
                    "sense_definition": "Utwardzona droga o twardej nawierzchni, przystosowana do ruchu pojazdów mechanicznych, zazwyczaj łącząca miejscowości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochody pędziły po szerokiej <w>szosie</w> w stronę dużego miasta.",
                            "target_lang_translations": {
                                "ru": "Машины мчались по широкому <w>шоссе</w> в сторону большого города."
                            }
                        },
                        {
                            "text": "Główna <w>szosa</w> była zablokowana z powodu trwającego remontu nawierzchni.",
                            "target_lang_translations": {
                                "ru": "Главное <w>шоссе</w> было перекрыто из-за ремонта дорожного покрытия."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) szosyjo"
                    ],
                    "common_phrases": [
                        "główna szosa",
                        "kręta szosa",
                        "wypadł z szosy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Автомобильная дорога с твёрдым покрытием, обычно соединяющая населённые пункты"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоссе",
                                "target_lang_sense_clarification": "дорога с твёрдым покрытием"
                            },
                            {
                                "target_lang_word": "трасса",
                                "target_lang_sense_clarification": "крупная автомобильная дорога"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f48d6f6-0acc-333f-bdd5-4687120a43ab",
                    "sense_definition": "Rower szosowy; lekki rower o wąskich oponach i charakterystycznej kierownicy, przeznaczony do szybkiej jazdy po asfalcie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat kupił nową <w>szosę</w>, żeby trenować do letnich zawodów kolarskich.",
                            "target_lang_translations": {
                                "ru": "Мой брат купил новый <w>шоссейник</w>, чтобы тренироваться к летним велогонкам."
                            }
                        },
                        {
                            "text": "Szybka jazda na <w>szosie</w> wymaga dobrej kondycji i skupienia na drodze.",
                            "target_lang_translations": {
                                "ru": "Быстрая езда на <w>шоссейном велосипеде</w> требует хорошей формы и сосредоточенности на дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolarzówka",
                        "szosówka",
                        "rower szosowy"
                    ],
                    "common_phrases": [
                        "jazda na szosie",
                        "używana szosa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шоссейный велосипед; лёгкий велосипед с узкими шинами для быстрой езды по асфальту"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоссейный велосипед",
                                "target_lang_sense_clarification": "тип велосипеда"
                            },
                            {
                                "target_lang_word": "шоссейник",
                                "target_lang_sense_clarification": "разговорное название"
                            }
                        ]
                    }
                }
            ]
        }

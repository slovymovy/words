                {
                    "sense_id": "450c72ad-d24d-3534-bc87-6120b64014c6",
                    "sense_definition": "Of, from, or relating to the country of Afghanistan.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "nationality_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Afghan</w> government has implemented new policies to improve education.",
                            "target_lang_translations": {
                                "ru": "<w>Афганское</w> правительство внедрило новую политику для улучшения образования."
                            }
                        },
                        {
                            "text": "We enjoyed a delicious meal at a local <w>Afghan</w> restaurant last night.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы наслаждались вкусным ужином в местном <w>афганском</w> ресторане."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Afghan refugee",
                        "Afghan war",
                        "Afghan culture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Афганистану или происходящий из этой страны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "афганский",
                                "target_lang_sense_clarification": "относящийся к стране"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cd56d6a-2857-3cc4-87dd-488608d0644e",

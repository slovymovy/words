    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ff7b653c-9c4e-3810-a432-68259c6a2751",
                    "sense_definition": "Taki, który ma dużo pracy, wykonuje jakąś czynność i nie ma wolnego czasu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "busy_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przepraszam, nie mogę teraz rozmawiać, jestem bardzo <w>zajęty</w>.",
                            "target_lang_translations": {
                                "ru": "Извини, не могу сейчас разговаривать, я очень <w>занят</w>."
                            }
                        },
                        {
                            "text": "Mój tata jest ciągle <w>zajęty</w> pracą i rzadko bywa w domu.",
                            "target_lang_translations": {
                                "ru": "Мой папа постоянно <w>занят</w> работой и редко бывает дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapracowany"
                    ],
                    "antonyms": [
                        "wolny"
                    ],
                    "common_phrases": [
                        "być bardzo zajętym",
                        "zajęty pracą",
                        "zajęty człowiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, у кого много работы, кто занят каким-либо делом и не имеет свободного времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "занятый",
                                "target_lang_sense_clarification": "не имеющий свободного времени"
                            },
                            {
                                "target_lang_word": "занятой",
                                "target_lang_sense_clarification": "характеристика человека, постоянно обремененного делами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfebde9d-ad00-3a3f-8ca8-5074c2c6d09d",
                    "sense_definition": "Taki, z którego ktoś już korzysta lub który jest zarezerwowany (o miejscu, przedmiocie lub linii telefonicznej).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "occupied_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy to miejsce przy oknie jest <w>zajęte</w>?",
                            "target_lang_translations": {
                                "ru": "Это место у окна <w>занято</w>?"
                            }
                        },
                        {
                            "text": "Próbowałem się do ciebie dodzwonić, ale numer był ciągle <w>zajęty</w>.",
                            "target_lang_translations": {
                                "ru": "Я пытался до тебя дозвониться, но номер был постоянно <w>занят</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "wolny",
                        "niezajęty"
                    ],
                    "common_phrases": [
                        "numer zajęty",
                        "toaleta zajęta",
                        "miejsce zajęte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который уже кем-то используется или забронирован (о месте, предмете или телефонной линии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "занятый",
                                "target_lang_sense_clarification": "несвободный, используемый кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65695d54-7441-3ec3-bd47-a0178c7790e4",
                    "sense_definition": "Taki, który jest w stałym związku z inną osobą i ma partnera lub partnerkę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie podrywaj jej, ona jest już <w>zajęta</w>.",
                            "target_lang_translations": {
                                "ru": "Не клейся к ней, она уже <w>занята</w>."
                            }
                        },
                        {
                            "text": "Mój brat jest szczęśliwie <w>zajęty</w> od dwóch lat.",
                            "target_lang_translations": {
                                "ru": "Мой брат счастливо <w>несвободен</w> уже два года."
                            }
                        }
                    ],
                    "antonyms": [
                        "wolny"
                    ],
                    "common_phrases": [
                        "zajęty chłopak",
                        "zajęta dziewczyna",
                        "serce zajęte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий в постоянных отношениях с другим человеком; имеющий партнера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несвободный",
                                "target_lang_sense_clarification": "состоящий в отношениях"
                            },
                            {
                                "target_lang_word": "занятый",
                                "target_lang_sense_clarification": "разговорное: имеющий партнера"
                            }
                        ]
                    }
                }
            ]
        },
        {

                    "frequency": "Middle",
                    "semantic_group_id": "emphatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You <w>yourself</w> admitted that it was a mistake."
                        },
                        {
                            "text": "If you want the job done right, you must do it <w>yourself</w>."
                        }
                    ],
                    "synonyms": [
                        "personally"
                    ],
                    "common_phrases": [

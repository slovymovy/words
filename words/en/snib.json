            "senses": [
                {
                    "sense_id": "93f20867-133c-3190-8387-1e9246361e13",
                    "sense_definition": "A small latch or fastening mechanism used to secure a door or window.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fastening_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She checked that the <w>snib</w> on the back door was fastened before going to bed.",
                            "target_lang_translations": {
                                "ru": "Она проверила, закрыта ли <w>защёлка</w> на задней двери, перед тем как лечь спать."
                            }
                        },
                        {
                            "text": "The window has a broken <w>snib</w>, so it cannot be locked properly.",
                            "target_lang_translations": {
                                "ru": "У окна сломана <w>щеколда</w>, поэтому его нельзя нормально закрыть."
                            }
                        }
                    ],
                    "synonyms": [
                        "latch",
                        "catch",
                        "fastening",
                        "bolt"
                    ],
                    "common_phrases": [
                        "on the snib",
                        "door snib"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly Scottish and Northern English"
                        }
                    ],
                    "target_lang_definitions": {

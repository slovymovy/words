            "pos": "noun",
            "senses": [
                {
                    "sense_id": "371d796e-2c76-31f1-8d30-12c86efbcff7",
                    "sense_definition": "Instytucja lub wyodrębniona formacja powołana do wykonywania określonych zadań na rzecz państwa lub społeczeństwa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polska <służba> zdrowia boryka się z wieloma problemami finansowymi.",
                            "target_lang_translations": {
                                "ru": "Польская <служба> здравоохранения сталкивается со многими финансовыми проблемами."
                            }
                        },
                        {
                            "text": "Na miejscu wypadku szybko pojawiły się odpowiednie <służby> ratunkowe.",
                            "target_lang_translations": {
                                "ru": "На месте аварии быстро появились соответствующие спасательные <службы>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "służba zdrowia",
                        "służby specjalne",
                        "służby porządkowe",
                        "służby ratunkowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение или специальное формирование, созданное для выполнения определенных задач в интересах государства или общества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "служба",
                                "target_lang_sense_clarification": "организация, ведомство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29c7000d-0c4e-3a35-8ffb-8b237b743f54",
                    "sense_definition": "Wykonywanie obowiązków w wojsku, policji lub innych formacjach mundurowych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "military_duty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <służba> w marynarce wojennej trwała ponad dziesięć lat.",
                            "target_lang_translations": {
                                "ru": "Его <служба> в военно-морском флоте длилась более десяти лет."
                            }
                        },
                        {
                            "text": "Zasadnicza <służba> wojskowa była kiedyś obowiązkowa dla wszystkich mężczyzn.",
                            "target_lang_translations": {
                                "ru": "Срочная военная <служба> когда-то была обязательной для всех мужчин."
                            }
                        }
                    ],
                    "synonyms": [
                        "służba wojskowa"
                    ],
                    "common_phrases": [
                        "służba wojskowa",
                        "czynna służba",
                        "zwolnienie ze służby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исполнение обязанностей в армии, полиции или других военизированных формированиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "служба",
                                "target_lang_sense_clarification": "воинская или государственная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5ac47d2-0381-3cbb-8a44-30147457bcb7",
                    "sense_definition": "Czas pełnienia obowiązków zawodowych, zwłaszcza w formacjach mundurowych; dyżur.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "shift_duty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policjant nie może pić alkoholu, gdy jest na <służbie>.",
                            "target_lang_translations": {
                                "ru": "Полицейский не может пить алкоголь, когда он на <службе>."
                            }
                        },
                        {
                            "text": "Jestem już po <służbie>, więc możemy porozmawiać o sprawach prywatnych.",
                            "target_lang_translations": {
                                "ru": "Я уже после <службы>, так что мы можем поговорить о личных делах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "być na służbie",
                        "po służbie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время выполнения служебных обязанностей, особенно в военизированных формированиях; дежурство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "служба",
                                "target_lang_sense_clarification": "исполнение обязанностей"
                            },
                            {
                                "target_lang_word": "дежурство",
                                "target_lang_sense_clarification": "смена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3af0d45c-3e05-34f4-8405-18fb4d6d6037",
                    "sense_definition": "Działanie na rzecz jakiejś idei, wspólnoty lub wartości, wykonywane z oddaniem i poczuciem misji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Całe jej życie to była bezinteresowna <służba> potrzebującym i chorym.",
                            "target_lang_translations": {
                                "ru": "Вся её жизнь была бескорыстным <служением> нуждающимся и больным."
                            }
                        },
                        {
                            "text": "Traktował swoją pracę polityczną jako <służbę> ojczyźnie, a nie sposób na zarobek.",
                            "target_lang_translations": {
                                "ru": "Он рассматривал свою политическую работу как <служение> родине, а не способ заработка."
                            }
                        }
                    ],
                    "synonyms": [
                        "posługa"
                    ],
                    "common_phrases": [
                        "służba Bogu",
                        "służba ojczyźnie",
                        "służba publiczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность на благо какой-либо идеи, общности или ценности, выполняемая с самоотдачей и чувством миссии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "служение",
                                "target_lang_sense_clarification": "высшая цель, миссия"
                            },
                            {
                                "target_lang_word": "служба",
                                "target_lang_sense_clarification": "деятельность на благо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b318a8b-f5e0-3348-a913-674ed654a066",
                    "sense_definition": "Ogół osób zatrudnionych do prowadzenia domu i obsługiwania domowników.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "servants_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych pałacach <służba> miała swoje pokoje w oddzielnym skrzydle budynku.",
                            "target_lang_translations": {
                                "ru": "В старинных дворцах <прислуга> имела свои комнаты в отдельном крыле здания."
                            }
                        },
                        {
                            "text": "Hrabia zadzwonił dzwonkiem, aby wezwać <służbę> do posprzątania stołu.",
                            "target_lang_translations": {
                                "ru": "Граф позвонил в колокольчик, чтобы позвать <прислугу> убрать со стола."
                            }
                        }
                    ],
                    "synonyms": [
                        "czeladź"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to historical context of domestic staff"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность людей, нанятых для ведения домашнего хозяйства и обслуживания хозяев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прислуга",
                                "target_lang_sense_clarification": "персонал, слуги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1165a147-22b1-3c22-904b-df4a0bfd42a2",
                    "sense_definition": "Praca w charakterze pomocy domowej, polegająca na usługiwaniu komuś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "servant_job",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda dziewczyna ze wsi poszła na <służbę> do bogatego domu w mieście.",
                            "target_lang_translations": {
                                "ru": "Молодая девушка из деревни пошла в <услужение> в богатый дом в городе."
                            }
                        },
                        {
                            "text": "Warunki na <służbie> były ciężkie, a wynagrodzenie bardzo niskie.",
                            "target_lang_translations": {
                                "ru": "Условия в <услужении> были тяжелыми, а жалованье очень низким."
                            }
                        }
                    ],
                    "synonyms": [
                        "usługiwanie"
                    ],
                    "common_phrases": [
                        "iść na służbę",
                        "przyjąć na służbę"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Работа в качестве домашней прислуги, заключающаяся в обслуживании кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "услужение",
                                "target_lang_sense_clarification": "работа слугой (устар.)"
                            }
                        ]
                    }
                }
            ]
        }

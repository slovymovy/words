{
    "word_family": [
        "opener",
        "openness",
        "openly"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0d708cbf-4e01-3808-9c8e-d96359ff6522",
                    "sense_definition": "A hole or empty space in something that allows light, air, or objects to pass through.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mouse ran into a small <opening> in the wall.",
                            "target_lang_translations": {
                                "ru": "Мышь забежала в маленькое <отверстие> в стене."
                            }
                        },
                        {
                            "text": "Sunlight shone through the narrow <opening> in the curtains.",
                            "target_lang_translations": {
                                "ru": "Солнечный свет светил сквозь узкую <щель> в шторах."
                            }
                        }
                    ],
                    "synonyms": [
                        "hole",
                        "gap",
                        "crevice"
                    ],
                    "common_phrases": [
                        "narrow opening",
                        "small opening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отверстие или пустое пространство, позволяющее проходить свету, воздуху или предметам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отверстие",
                                "target_lang_sense_clarification": "дыра, проход"
                            },
                            {
                                "target_lang_word": "щель",
                                "target_lang_sense_clarification": "узкое отверстие"
                            },
                            {
                                "target_lang_word": "проём",
                                "target_lang_sense_clarification": "отверстие в стене для двери или окна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01b78767-d85a-3541-b557-d05757bce5c9",
                    "sense_definition": "A job or position in an organization that is available for someone to fill.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We currently have an <opening> in our marketing department.",
                            "target_lang_translations": {
                                "ru": "В данный момент у нас есть <вакансия> в отделе маркетинга."
                            }
                        },
                        {
                            "text": "She checked the website for any new job <openings>.",
                            "target_lang_translations": {
                                "ru": "Она проверила сайт на наличие новых <вакансий>."
                            }
                        }
                    ],
                    "synonyms": [
                        "job opening"
                    ],
                    "common_phrases": [
                        "job opening",
                        "current opening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Работа или должность в организации, на которую ищут сотрудника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вакансия",
                                "target_lang_sense_clarification": "свободная должность"
                            },
                            {
                                "target_lang_word": "свободное место",
                                "target_lang_sense_clarification": "незанятая позиция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10cf9366-0eef-3409-ba96-ea0bb7c103a4",
                    "sense_definition": "A ceremony or public event to celebrate the start of something, such as a new shop, exhibition, or show.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hundreds of people attended the grand <opening> of the new museum.",
                            "target_lang_translations": {
                                "ru": "Сотни людей посетили торжественное <открытие> нового музея."
                            }
                        },
                        {
                            "text": "The gallery held a private <opening> for the artist's friends and family.",
                            "target_lang_translations": {
                                "ru": "Галерея провела закрытое <открытие> для друзей и семьи художника."
                            }
                        }
                    ],
                    "antonyms": [
                        "closing"
                    ],
                    "common_phrases": [
                        "grand opening",
                        "official opening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Церемония или публичное мероприятие по случаю начала работы чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытие",
                                "target_lang_sense_clarification": "торжественное начало работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a6f29c8-4b9b-3e30-b61a-8b3fc9c12bf4",
                    "sense_definition": "The first part or beginning of something, such as a book, movie, piece of music, or game.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "beginning_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <opening> of the film was exciting and full of action.",
                            "target_lang_translations": {
                                "ru": "<Начало> фильма было захватывающим и полным действия."
                            }
                        },
                        {
                            "text": "He memorized a famous chess <opening> to surprise his opponent.",
                            "target_lang_translations": {
                                "ru": "Он выучил знаменитый шахматный <дебют>, чтобы удивить соперника."
                            }
                        }
                    ],
                    "antonyms": [
                        "ending",
                        "conclusion"
                    ],
                    "common_phrases": [
                        "chess opening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Первая часть или начало чего-либо, например книги, фильма или игры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начало",
                                "target_lang_sense_clarification": "первая часть"
                            },
                            {
                                "target_lang_word": "дебют",
                                "target_lang_sense_clarification": "начало шахматной партии"
                            },
                            {
                                "target_lang_word": "вступление",
                                "target_lang_sense_clarification": "вводная часть произведения"
                            },
                            {
                                "target_lang_word": "увертюра",
                                "target_lang_sense_clarification": "музыкальное вступление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32f197c6-f636-3c85-994c-c6060c8094c2",
                    "sense_definition": "A specific time in a schedule when someone is free or a service is available.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have an <opening> in my calendar at three o'clock if you want to meet.",
                            "target_lang_translations": {
                                "ru": "У меня есть <окошко> в календаре в три часа, если вы хотите встретиться."
                            }
                        },
                        {
                            "text": "The dentist has no <openings> left for this week.",
                            "target_lang_translations": {
                                "ru": "У стоматолога не осталось <свободного времени> на этой неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "slot",
                        "availability"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное время в расписании, когда кто-то свободен или услуга доступна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окошко",
                                "target_lang_sense_clarification": "свободное время в графике (разг.)"
                            },
                            {
                                "target_lang_word": "свободное время",
                                "target_lang_sense_clarification": "время для записи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "285295d6-5b37-394f-a705-6bb4ffd1d452",
                    "sense_definition": "The act of making something open, unwrapping something, or starting a process.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children were excited for the <opening> of their presents on Christmas morning.",
                            "target_lang_translations": {
                                "ru": "Дети с волнением ждали <открытия> своих подарков рождественским утром."
                            }
                        },
                        {
                            "text": "The <opening> of the borders allowed people to travel freely again.",
                            "target_lang_translations": {
                                "ru": "<Открытие> границ снова позволило людям свободно путешествовать."
                            }
                        }
                    ],
                    "antonyms": [
                        "closing",
                        "closure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, делающее что-то открытым, или начало процесса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытие",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "раскрытие",
                                "target_lang_sense_clarification": "обнаружение или распаковка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32cd18cc-a6c5-3a6e-9511-ca4d3aaacee2",
                    "sense_definition": "An opportunity or chance to do something, especially in a competitive situation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "opportunity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The striker saw an <opening> in the defense and scored a goal.",
                            "target_lang_translations": {
                                "ru": "Нападающий увидел <брешь> в обороне и забил гол."
                            }
                        },
                        {
                            "text": "She waited for an <opening> in the conversation to ask her question.",
                            "target_lang_translations": {
                                "ru": "Она ждала <удобного момента> в разговоре, чтобы задать свой вопрос."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность или шанс сделать что-то, особенно в конкурентной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возможность",
                                "target_lang_sense_clarification": "шанс"
                            },
                            {
                                "target_lang_word": "брешь",
                                "target_lang_sense_clarification": "слабое место в обороне"
                            },
                            {
                                "target_lang_word": "удобный момент",
                                "target_lang_sense_clarification": "подходящее время"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9c354a38-2fb2-3f53-8f2a-55e9960f0004",
                    "sense_definition": "Happening at the beginning or start of an event or series.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "initial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The president gave the <opening> speech at the conference.",
                            "target_lang_translations": {
                                "ru": "Президент произнес <вступительную> речь на конференции."
                            }
                        },
                        {
                            "text": "We missed the <opening> scene of the play because of traffic.",
                            "target_lang_translations": {
                                "ru": "Мы пропустили <начальную> сцену пьесы из-за пробок."
                            }
                        }
                    ],
                    "antonyms": [
                        "closing",
                        "final"
                    ],
                    "common_phrases": [
                        "opening hours",
                        "opening ceremony",
                        "opening remarks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий в начале события или серии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вступительный",
                                "target_lang_sense_clarification": "вводный"
                            },
                            {
                                "target_lang_word": "начальный",
                                "target_lang_sense_clarification": "первый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "289dd4e3-af2b-35c1-b1c0-083a308c1716",
                    "sense_definition": "Present participle and gerund of <w>open</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <opening> the window to let some fresh air into the room.",
                            "target_lang_translations": {
                                "ru": "Она <открывает> окно, чтобы впустить в комнату свежий воздух."
                            }
                        },
                        {
                            "text": "Thank you for <opening> the door for me when my hands were full.",
                            "target_lang_translations": {
                                "ru": "Спасибо, что <открыли> мне дверь, когда мои руки были заняты."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола 'open' (открывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открывающий",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "открывание",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "cartoonist",
        "cartoonish",
        "cartoony"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b5f3837d-9ae6-3a07-83b0-f5f081e51c24",
                    "sense_definition": "An animated film or television show, often humorous and intended for children.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children sat on the sofa to watch a funny <w>cartoon</w> on TV.",
                            "target_lang_translations": {
                                "ru": "Дети сели на диван, чтобы посмотреть смешной <w>мультик</w> по телевизору."
                            }
                        },
                        {
                            "text": "My favorite <w>cartoon</w> features a cat chasing a mouse.",
                            "target_lang_translations": {
                                "ru": "В моем любимом <w>мультфильме</w> кот гоняется за мышью."
                            }
                        }
                    ],
                    "synonyms": [
                        "animation",
                        "animated cartoon"
                    ],
                    "common_phrases": [
                        "watch cartoons",
                        "cartoon character",
                        "Saturday morning cartoons"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мультипликационный фильм или телешоу, часто юмористическое и предназначенное для детей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мультфильм",
                                "target_lang_sense_clarification": "стандартное сокращение от мультипликационный фильм"
                            },
                            {
                                "target_lang_word": "мультик",
                                "target_lang_sense_clarification": "разговорное название"
                            },
                            {
                                "target_lang_word": "мультипликационный фильм",
                                "target_lang_sense_clarification": "полное официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73126766-d68d-3047-ae10-d6ac6c02ed96",
                    "sense_definition": "A humorous drawing, often with a caption, or a sequence of such drawings found in newspapers or magazines.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "static_drawing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I always read the <w>cartoon</w> on the back page of the newspaper.",
                            "target_lang_translations": {
                                "ru": "Я всегда читаю <w>комикс</w> на последней странице газеты."
                            }
                        },
                        {
                            "text": "The artist drew a quick <w>cartoon</w> to make his friend laugh.",
                            "target_lang_translations": {
                                "ru": "Художник нарисовал быструю <w>карикатуру</w>, чтобы рассмешить друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "comic strip",
                        "strip cartoon"
                    ],
                    "common_phrases": [
                        "newspaper cartoon",
                        "funny cartoon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юмористический рисунок, часто с подписью, или серия таких рисунков в газетах или журналах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комикс",
                                "target_lang_sense_clarification": "серия рисунков, история в картинках"
                            },
                            {
                                "target_lang_word": "карикатура",
                                "target_lang_sense_clarification": "смешной рисунок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e257ba5-635d-3e49-93b6-3c954981c948",
                    "sense_definition": "A drawing that satirizes current public figures, politicians, or events.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "static_drawing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The newspaper published a political <w>cartoon</w> mocking the mayor.",
                            "target_lang_translations": {
                                "ru": "Газета опубликовала политическую <w>карикатуру</w>, высмеивающую мэра."
                            }
                        },
                        {
                            "text": "The editorial <w>cartoon</w> criticized the government's new tax policy.",
                            "target_lang_translations": {
                                "ru": "Редакционная <w>карикатура</w> критиковала новую налоговую политику правительства."
                            }
                        }
                    ],
                    "synonyms": [
                        "caricature",
                        "political cartoon"
                    ],
                    "common_phrases": [
                        "political cartoon",
                        "editorial cartoon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рисунок, высмеивающий современных общественных деятелей, политиков или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карикатура",
                                "target_lang_sense_clarification": "сатирическое изображение"
                            },
                            {
                                "target_lang_word": "шарж",
                                "target_lang_sense_clarification": "шуточное, искаженное изображение лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "436c610c-fbbf-33bb-8398-243b83587df6",
                    "sense_definition": "A full-sized preliminary drawing used as a template for a painting, fresco, or tapestry.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "art_template",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum displays the original <w>cartoon</w> used to create the famous tapestry.",
                            "target_lang_translations": {
                                "ru": "В музее выставлен оригинальный <w>картон</w>, использованный для создания знаменитого гобелена."
                            }
                        },
                        {
                            "text": "Da Vinci prepared a detailed <w>cartoon</w> before starting work on the fresco.",
                            "target_lang_translations": {
                                "ru": "Да Винчи подготовил детальный <w>картон</w> перед началом работы над фреской."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "art history context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полноразмерный предварительный набросок, используемый как шаблон для картины, фрески или гобелена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "картон",
                                "target_lang_sense_clarification": "термин в живописи"
                            },
                            {
                                "target_lang_word": "эскиз",
                                "target_lang_sense_clarification": "предварительный рисунок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c22e7c91-3c2d-36cd-9b58-33171122e00b",
                    "sense_definition": "To make a humorous drawing or caricature of someone or something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>cartoons</w> local politicians for the weekly community news.",
                            "target_lang_translations": {
                                "ru": "Он <w>рисует карикатуры</w> на местных политиков для еженедельных общественных новостей."
                            }
                        },
                        {
                            "text": "The street artist <w>cartooned</w> the tourists in the park for a small fee.",
                            "target_lang_translations": {
                                "ru": "Уличный художник <w>нарисовал шарж</w> на туристов в парке за небольшую плату."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать юмористический рисунок или карикатуру на кого-либо или что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рисовать карикатуру",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "изображать в карикатурном виде",
                                "target_lang_sense_clarification": "описывать или рисовать смешно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
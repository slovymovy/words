    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "58fe10af-6d4d-3c90-a64a-cdaa8ab9a369",
                    "sense_definition": "Voor dat tijdstip; in de tijd die voorafging aan wat genoemd is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sinds kort woon ik in Utrecht, maar <w>daarvoor</w> woonde ik jarenlang in Amsterdam.",
                            "target_lang_translations": {
                                "ru": "С недавних пор я живу в Утрехте, но <w>до этого</w> я много лет жил в Амстердаме.",
                                "en": "I've been living in Utrecht since recently, but <w>before that</w> I lived in Amsterdam for years."
                            }
                        },
                        {
                            "text": "Het feest begint om acht uur, maar kort <w>daarvoor</w> gaan we nog even wat eten.",
                            "target_lang_translations": {
                                "ru": "Вечеринка начинается в восемь часов, но незадолго <w>до этого</w> мы перекусим.",
                                "en": "The party starts at eight o'clock, but shortly <w>before that</w> we're going to grab a bite to eat."
                            }
                        }
                    ],
                    "synonyms": [
                        "eerder",
                        "tevoren",
                        "voorheen",
                        "vroeger"
                    ],
                    "antonyms": [
                        "daarna",
                        "later",
                        "vervolgens"
                    ],
                    "common_phrases": [
                        "kort daarvoor",
                        "lang daarvoor",
                        "de dag daarvoor",
                        "jaren daarvoor"
                    ],
                    "target_lang_definitions": {
                        "ru": "До этого момента; в то время, которое предшествовало упомянутому.",
                        "en": "Before that time; in the time preceding what was mentioned."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до этого",
                                "target_lang_sense_clarification": "временное значение"
                            },
                            {
                                "target_lang_word": "раньше",
                                "target_lang_sense_clarification": "ранее"
                            },
                            {
                                "target_lang_word": "перед этим",
                                "target_lang_sense_clarification": "незадолго до"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "before that",
                                "target_lang_sense_clarification": "time reference"
                            },
                            {
                                "target_lang_word": "previously",
                                "target_lang_sense_clarification": "formal adverb"
                            },
                            {
                                "target_lang_word": "earlier",
                                "target_lang_sense_clarification": "comparison in time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ada380e4-6504-385c-986a-45f6bcb3b346",
                    "sense_definition": "Ten behoeve van dat doel of die zaak; om die reden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft een ernstige fout gemaakt en <w>daarvoor</w> krijgt hij nu straf.",
                            "target_lang_translations": {
                                "ru": "Он совершил серьезную ошибку, и <w>за это</w> его теперь наказывают.",
                                "en": "He made a serious mistake and <w>for that</w> he is now being punished."
                            }
                        },
                        {
                            "text": "Als je hulp nodig hebt bij het verhuizen, kan ik <w>daarvoor</w> wel zorgen.",
                            "target_lang_translations": {
                                "ru": "Если тебе нужна помощь с переездом, я могу <w>об этом</w> позаботиться.",
                                "en": "If you need help moving, I can take care <w>of that</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "daartoe"
                    ],
                    "common_phrases": [
                        "daarvoor zorgen",
                        "daarvoor dienen",
                        "daarvoor kiezen",
                        "daarvoor betalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Для этой цели или по этой причине; ради этого.",
                        "en": "For that purpose or matter; for that reason."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "для этого",
                                "target_lang_sense_clarification": "цель"
                            },
                            {
                                "target_lang_word": "за это",
                                "target_lang_sense_clarification": "причина или возмездие"
                            },
                            {
                                "target_lang_word": "ради этого",
                                "target_lang_sense_clarification": "намерение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "for that",
                                "target_lang_sense_clarification": "reason or purpose"
                            },
                            {
                                "target_lang_word": "for it",
                                "target_lang_sense_clarification": "referring to an object or action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f6ed82f-fb11-364f-8c4e-e3ba017d0778",
                    "sense_definition": "Aan de voorzijde van dat object of die plaats.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er stond een groot oud huis met <w>daarvoor</w> een prachtige tuin vol bloemen.",
                            "target_lang_translations": {
                                "ru": "Там стоял большой старый дом, а <w>перед ним</w> был прекрасный сад, полный цветов.",
                                "en": "There was a big old house with a beautiful garden <w>in front of it</w> full of flowers."
                            }
                        },
                        {
                            "text": "In het museum zag ik een groot schilderij met vlak <w>daarvoor</w> een bankje om op te zitten.",
                            "target_lang_translations": {
                                "ru": "В музее я увидел большую картину, а прямо <w>перед ней</w> стояла скамейка.",
                                "en": "In the museum I saw a large painting with a bench to sit on right <w>in front of it</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "daarachter"
                    ],
                    "common_phrases": [
                        "vlak daarvoor",
                        "net daarvoor",
                        "schuin daarvoor"
                    ],
                    "target_lang_definitions": {
                        "ru": "С лицевой стороны этого объекта или места.",
                        "en": "At the front side of that object or place."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перед этим",
                                "target_lang_sense_clarification": "пространственное значение"
                            },
                            {
                                "target_lang_word": "спереди",
                                "target_lang_sense_clarification": "на переднем плане"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "in front of it",
                                "target_lang_sense_clarification": "spatial location relative to an object"
                            },
                            {
                                "target_lang_word": "in front of that",
                                "target_lang_sense_clarification": "spatial location"
                            }
                        ]
                    }
                }
            ]

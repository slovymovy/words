                },
                {
                    "sense_id": "1ed74e7b-05dd-315c-ac50-1df201f72b58",
                    "sense_definition": "A group of people of any gender; used as a form of address.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "person_ref",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hey <w>guys</w>, do you want to go to the cinema tonight?",
                            "target_lang_translations": {
                                "ru": "Эй, <w>ребята</w>, хотите пойти сегодня вечером в кино?"
                            }
                        },
                        {
                            "text": "Come on, <w>guys</w>, we are going to be late for the train.",
                            "target_lang_translations": {
                                "ru": "Давайте, <w>ребята</w>, мы опоздаем на поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "everyone",
                        "folks",
                        "people"
                    ],
                    "common_phrases": [
                        "you guys",
                        "hey guys",
                        "hi guys"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей любого пола; часто используется как обращение."

{
    "word_family": [
        "overtreding",
        "overtreder"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a85b5ca4-1e0a-3ceb-8b6c-d3ec28470f83",
                    "sense_definition": "Een regel, wet of voorschrift niet naleven; iets doen wat niet mag.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "law_and_rules",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De automobilist <w>overtrad</w> de maximumsnelheid op de snelweg.",
                            "target_lang_translations": {
                                "en": "The motorist <w>exceeded</w> the speed limit on the highway."
                            }
                        },
                        {
                            "text": "Als je de regels <w>overtreedt</w>, kun je een boete verwachten.",
                            "target_lang_translations": {
                                "en": "If you <w>break</w> the rules, you can expect a fine."
                            }
                        }
                    ],
                    "synonyms": [
                        "schenden",
                        "breken"
                    ],
                    "antonyms": [
                        "naleven",
                        "gehoorzamen",
                        "respecteren"
                    ],
                    "common_phrases": [
                        "de wet overtreden",
                        "een regel overtreden",
                        "een verbod overtreden"
                    ],
                    "target_lang_definitions": {
                        "en": "To fail to comply with a rule, law, or regulation; to do something prohibited."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "violate",
                                "target_lang_sense_clarification": "formal; often used with laws, rights, or agreements"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "general; used with rules, laws, or promises"
                            },
                            {
                                "target_lang_word": "infringe",
                                "target_lang_sense_clarification": "formal; often used with rights, copyrights, or specific regulations"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aebc6d52-6109-380f-a141-ffd309d88094",
                    "sense_definition": "Buiten de oevers treden (van water).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nature_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de zware regenval <w>trad</w> de rivier <w>over</w>.",
                            "target_lang_translations": {
                                "en": "After the heavy rainfall, the river <w>overflowed</w>."
                            }
                        },
                        {
                            "text": "Het water is op verschillende plaatsen <w>overgetreden</w>.",
                            "target_lang_translations": {
                                "en": "The water has <w>overflowed</w> in several places."
                            }
                        }
                    ],
                    "synonyms": [
                        "overstromen"
                    ],
                    "target_lang_definitions": {
                        "en": "To rise above or go beyond the banks (of a body of water)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overflow",
                                "target_lang_sense_clarification": "regarding water bodies"
                            },
                            {
                                "target_lang_word": "burst its banks",
                                "target_lang_sense_clarification": "idiomatic; specifically for rivers"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
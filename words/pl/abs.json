            "senses": [
                {
                    "sense_id": "44554c04-d01d-3810-9378-ec2cb8eb28ef",
                    "sense_definition": "System stosowany w pojazdach mechanicznych, który zapobiega blokowaniu się kół podczas hamowania.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy samochód jest wyposażony w system <w>ABS</w>.",
                            "target_lang_translations": {
                                "ru": "Моя новая машина оснащена системой <w>АБС</w>."
                            }
                        },
                        {
                            "text": "Podczas gwałtownego hamowania na lodzie zadziałał <w>ABS</w>.",
                            "target_lang_translations": {
                                "ru": "Во время резкого торможения на льду сработала <w>АБС</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "układ przeciwpoślizgowy"
                    ],
                    "common_phrases": [
                        "system ABS",
                        "kontrolka ABS",
                        "hamulce z ABS"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система, предотвращающая блокировку колёс транспортного средства при торможении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "АБС",
                                "target_lang_sense_clarification": "аббревиатура"
                            },
                            {
                                "target_lang_word": "антиблокировочная система",
                                "target_lang_sense_clarification": "полный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60e4832b-d14f-3dbf-9ca5-31e2119b4d5e",
                    "sense_definition": "Mężczyzna bardzo umięśniony, szczególnie w okolicach karku (skrót od: Absolutny Brak Szyi).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_men",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na siłowni jest mnóstwo <w>ABS-ów</w> podnoszących ciężary.",
                            "target_lang_translations": {
                                "ru": "В спортзале полно <w>качков</w>, тягающих железо."
                            }
                        },
                        {
                            "text": "Ten wielki <w>ABS</w> ledwo mieści się w drzwiach wejściowych.",
                            "target_lang_translations": {
                                "ru": "Этот огромный <w>шкаф</w> едва пролезает во входную дверь."
                            }

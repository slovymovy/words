{
    "word_family": [
        "ziel",
        "zieligheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "58c550f4-6d7a-3427-9464-30e2ab23b0ea",
                    "sense_definition": "Medelijden of compassie oproepend; in een toestand verkerend die anderen verdrietig maakt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pitiful",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij maakte een <w>zielige</w> indruk toen ze helemaal alleen in de regen stond.",
                            "target_lang_translations": {
                                "ru": "Она производила <w>жалкое</w> впечатление, когда стояла совсем одна под дождем.",
                                "en": "She made a <w>pitiful</w> impression when she stood all alone in the rain."
                            }
                        },
                        {
                            "text": "Ik vind het <w>zielig</w> voor de kinderen dat hun vakantie niet doorgaat.",
                            "target_lang_translations": {
                                "ru": "Мне <w>жаль</w> детей из-за того, что их каникулы не состоятся.",
                                "en": "I feel <w>sorry</w> for the children that their holiday is not going ahead."
                            }
                        }
                    ],
                    "synonyms": [
                        "meelijwekkend",
                        "beklagenswaardig",
                        "deerniswekkend"
                    ],
                    "antonyms": [
                        "benijdenswaardig",
                        "gelukkig"
                    ],
                    "common_phrases": [
                        "zielig vinden",
                        "een zielig gezicht",
                        "een zielig hoopje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий жалость или сострадание; находящийся в состоянии, которое заставляет других грустить.",
                        "en": "Arousing pity or compassion; being in a state that makes others sad."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалкий",
                                "target_lang_sense_clarification": "вызывающий сочувствие, несчастный на вид"
                            },
                            {
                                "target_lang_word": "вызывающий жалость",
                                "target_lang_sense_clarification": "трогательный, печальный"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "pitiful",
                                "target_lang_sense_clarification": "arousing pity or sadness"
                            },
                            {
                                "target_lang_word": "miserable",
                                "target_lang_sense_clarification": "extremely unhappy or uncomfortable"
                            },
                            {
                                "target_lang_word": "poor",
                                "target_lang_sense_clarification": "deserving of sympathy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4b841f2-aa2f-3382-a7c0-133b2e5492a6",
                    "sense_definition": "Aanstellerig, belachelijk of minachting oproepend; gebruikt om gedrag af te keuren.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "pathetic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doe niet zo <w>zielig</w>, man, het is maar een klein krasje op je auto.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>жалким</w>, чувак, это всего лишь маленькая царапина на твоей машине.",
                                "en": "Don't be so <w>pathetic</w>, man, it's just a small scratch on your car."
                            }
                        },
                        {
                            "text": "Het is nogal <w>zielig</w> om te liegen over hoeveel geld je verdient.",
                            "target_lang_translations": {
                                "ru": "Это довольно <w>убого</w> — врать о том, сколько денег ты зарабатываешь.",
                                "en": "It is quite <w>pathetic</w> to lie about how much money you earn."
                            }
                        }
                    ],
                    "synonyms": [
                        "beklagenswaardig",
                        "erbarmelijk",
                        "meelijwekkend"
                    ],
                    "antonyms": [
                        "stoer",
                        "indrukwekkend"
                    ],
                    "common_phrases": [
                        "doe niet zo zielig",
                        "een zielige vertoning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий презрение, нелепый или наигранный; используется для неодобрения поведения.",
                        "en": "Affectated, ridiculous, or arousing contempt; used to disapprove of behavior."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалкий",
                                "target_lang_sense_clarification": "ничтожный, достойный презрения"
                            },
                            {
                                "target_lang_word": "убогий",
                                "target_lang_sense_clarification": "морально низкий, нелепый"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "pathetic",
                                "target_lang_sense_clarification": "arousing scorn or contempt"
                            },
                            {
                                "target_lang_word": "pitiful",
                                "target_lang_sense_clarification": "contemptibly inadequate"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
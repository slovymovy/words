                {
                    "sense_id": "4d804fc0-1463-3c10-a727-3fc8acdffa6a",
                    "sense_definition": "Heel hard studeren, meestal om feiten uit het hoofd te leren vlak voor een examen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "study_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan vanavond niet mee uit, want ik moet <w>blokken</w> voor mijn tentamen.",
                            "target_lang_translations": {
                                "ru": "Я не могу пойти гулять сегодня вечером, так как мне нужно <w>зубрить</w> перед экзаменом.",
                                "en": "I can't go out tonight, because I have to <w>cram</w> for my exam."
                            }
                        },
                        {
                            "text": "Hij heeft het hele weekend zitten te <w>blokken</w> om zijn achterstand in te halen.",
                            "target_lang_translations": {
                                "ru": "Он все выходные <w>зубрил</w>, чтобы наверстать упущенное.",
                                "en": "He has been <w>cramming</w> all weekend to catch up."
                            }
                        }
                    ],
                    "synonyms": [
                        "stampen"
                    ],
                    "common_phrases": [
                        "hard blokken",
                        "zitten te blokken",
                        "blokken voor een examen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informeel"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Усердно учиться, обычно заучивая факты наизусть перед экзаменом.",
                        "en": "To study very hard, usually to memorize facts by heart right before an exam."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зубрить",
                                "target_lang_sense_clarification": "учить наизусть, разговорное"
                            },
                            {
                                "target_lang_word": "усердно заниматься",
                                "target_lang_sense_clarification": "интенсивно учиться"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "cram",
                                "target_lang_sense_clarification": "study intensively before an exam"
                            },
                            {

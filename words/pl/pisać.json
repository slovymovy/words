    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cccf4152-e8dc-3a4a-8c26-07cc305b9cf6",
                    "sense_definition": "Stawiać znaki, litery lub słowa na papierze lub innej powierzchni za pomocą narzędzia do pisania (np. długopisu, ołówka, kredy) lub klawiatury.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_writing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczeń powoli <pisze> swoje imię i nazwisko na czystej kartce papieru.",
                            "target_lang_translations": {
                                "ru": "Ученик медленно <w>пишет</w> свое имя и фамилию на чистом листе бумаги."
                            }
                        },
                        {
                            "text": "Nauczycielka matematyki <pisze> kredą na tablicy trudne zadanie dla całej klasy.",
                            "target_lang_translations": {
                                "ru": "Учительница математики <w>пишет</w> мелом на доске трудную задачу для всего класса."
                            }
                        }
                    ],
                    "synonyms": [
                        "nazywać"
                    ],
                    "antonyms": [
                        "rysować"
                    ],
                    "common_phrases": [
                        "pisać długopisem",
                        "pisać na komputerze",
                        "pisać na kartce",
                        "pisać na tablicy",
                        "pisać niewyraźnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить знаки, буквы или слова на бумагу или другую поверхность с помощью пишущего инструмента или клавиатуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "писать",
                                "target_lang_sense_clarification": "наносить знаки на поверхность"
                            },
                            {
                                "target_lang_word": "записывать",
                                "target_lang_sense_clarification": "фиксировать информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc4d6f85-36c6-3b56-8ac9-4e6235d25fee",
                    "sense_definition": "Przekazywać komuś informacje, wiadomości lub uczucia za pomocą tekstu (np. w liście, SMS-ie, e-mailu).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra codziennie <pisze> do mnie długie wiadomości tekstowe.",
                            "target_lang_translations": {
                                "ru": "Моя сестра каждый день <w>пишет</w> мне длинные текстовые сообщения."
                            }
                        },
                        {
                            "text": "Kiedy byłem na wakacjach, lubiłem <pisać> listy do moich dziadków.",
                            "target_lang_translations": {
                                "ru": "Когда я был в отпуске, я любил <w>писать</w> письма своим дедушке и бабушке."
                            }
                        }
                    ],
                    "antonyms": [
                        "mówić",
                        "rysować"
                    ],
                    "common_phrases": [
                        "pisać do kogoś",
                        "pisać list",
                        "pisać SMS-y",
                        "pisać maile",
                        "pisać prawdę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать кому-либо информацию или сообщения посредством текста (например, в письме, СМС, электронной почте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "писать",
                                "target_lang_sense_clarification": "сообщать письменно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47c7c2a5-d75f-3334-b6ec-f0c7c84a9e72",
                    "sense_definition": "Używać odpowiednich liter i znaków do zapisu słowa, przestrzegając zasad ortografii.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "orthography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie jestem pewien, jak <pisze> się to trudne słowo po francusku.",
                            "target_lang_translations": {
                                "ru": "Я не уверен, как <w>пишется</w> это трудное слово по-французски."
                            }
                        },
                        {
                            "text": "Pamiętaj, że imiona i nazwiska zawsze <piszemy> wielką literą.",
                            "target_lang_translations": {
                                "ru": "Помни, что имена и фамилии мы всегда <w>пишем</w> с большой буквы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jak się pisze",
                        "pisać wielką literą",
                        "pisać małą literą",
                        "pisać łącznie",
                        "pisać oddzielnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать правильные буквы и знаки для записи слова, соблюдая правила орфографии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "писать",
                                "target_lang_sense_clarification": "соблюдать орфографию"
                            },
                            {
                                "target_lang_word": "писаться",
                                "target_lang_sense_clarification": "о написании слова (пассив)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8ee30ce-7140-3439-96ff-f854145a0ced",
                    "sense_definition": "Tworzyć dzieła literackie, artykuły, wiersze, muzykę lub programy komputerowe jako autor.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creative_writing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany dziennikarz <pisze> ciekawy artykuł o zmianach klimatu do jutrzejszej gazety.",
                            "target_lang_translations": {
                                "ru": "Известный журналист <w>пишет</w> интересную статью об изменении климата для завтрашней газеты."
                            }
                        },
                        {
                            "text": "Mój ulubiony autor właśnie <pisze> nową powieść kryminalną, na którą wszyscy czekają.",
                            "target_lang_translations": {
                                "ru": "Мой любимый автор как раз <w>пишет</w> новый детективный роман, который все ждут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pisać książkę",
                        "pisać wiersze",
                        "pisać artykuł",
                        "pisać bloga",
                        "pisać pracę magisterską"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать литературные произведения, статьи, стихи или музыку в качестве автора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "писать",
                                "target_lang_sense_clarification": "сочинять произведения"
                            },
                            {
                                "target_lang_word": "сочинять",
                                "target_lang_sense_clarification": "творить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "067c45c3-9896-33a1-99f3-262e7dbf6730",
                    "sense_definition": "Rozwiązywać zadania na egzaminie, sprawdzianie lub teście w formie pisemnej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "exam",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym tygodniu wszyscy studenci będą <pisać> trudny egzamin z historii.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе все студенты будут <w>писать</w> трудный экзамен по истории."
                            }
                        },
                        {
                            "text": "Stresuję się, bo jutro <piszemy> ważny sprawdzian z matematyki.",
                            "target_lang_translations": {
                                "ru": "Я нервничаю, потому что завтра мы <w>пишем</w> важную контрольную по математике."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pisać egzamin",
                        "pisać sprawdzian",
                        "pisać test",
                        "pisać maturę",
                        "pisać kolokwium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять задания на экзамене, контрольной работе или тесте в письменной форме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "писать",
                                "target_lang_sense_clarification": "выполнять письменную работу"
                            },
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "проходить проверку знаний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1d147c5-545a-33ed-b11a-ddeee64085d0",
                    "sense_definition": "Zostawiać ślad na powierzchni (o narzędziu, np. długopisie, flamastrze); nadawać się do pisania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tool_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić nowy wkład, ponieważ ten niebieski długopis przestał <pisać>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый стержень, так как эта синяя ручка перестала <w>писать</w>."
                            }
                        },
                        {
                            "text": "To pióro jest bardzo drogie, ale <pisze> niezwykle lekko i gładko.",
                            "target_lang_translations": {
                                "ru": "Эта перьевая ручка очень дорогая, но <w>пишет</w> невероятно легко и гладко."
                            }
                        }
                    ],
                    "common_phrases": [
                        "długopis nie pisze",
                        "pióro ładnie pisze",
                        "cienko pisać",
                        "grubo pisać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставлять след на поверхности (об инструменте); быть в рабочем состоянии для письма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "писать",
                                "target_lang_sense_clarification": "об инструменте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30ba6bc3-ef84-304a-82e0-b47deee737f1",
                    "sense_definition": "Zawierać jakąś treść lub informację (użycie potoczne, często zastępujące formę 'jest napisane').",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "content_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zobacz, na tych drzwiach wyraźnie <pisze>, że wstęp jest wzbroniony.",
                            "target_lang_translations": {
                                "ru": "Смотри, на этой двери ясно <w>написано</w>, что вход воспрещен."
                            }
                        },
                        {
                            "text": "W dzisiejszej gazecie <pisze> o planowanych remontach dróg w naszym mieście.",
                            "target_lang_translations": {
                                "ru": "В сегодняшней газете <w>пишут</w> о запланированном ремонте дорог в нашем городе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tu pisze",
                        "w gazecie pisze",
                        "co tam pisze",
                        "na kartce pisze"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Forma często używana w mowie potocznej, ale w starannym języku preferowane jest wyrażenie 'jest napisane'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержать информацию или текст (разговорное употребление, вместо «написано»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть написанным",
                                "target_lang_sense_clarification": "содержать текст"
                            },
                            {
                                "target_lang_word": "говориться",
                                "target_lang_sense_clarification": "сообщаться в тексте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55ec500d-9fa3-3154-bc4f-93c582935791",
                    "sense_definition": "Malować obrazy o tematyce religijnej (ikony) lub ozdabiać jajka wielkanocne wzorami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "artistic_painting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mnisi w tym starym klasztorze od wieków ręcznie <piszą> piękne ikony.",
                            "target_lang_translations": {
                                "ru": "Монахи в этом старом монастыре веками вручную <w>пишут</w> прекрасные иконы."
                            }
                        },
                        {
                            "text": "Przed Wielkanocą cała nasza rodzina siada przy stole i <pisze> kolorowe pisanki.",
                            "target_lang_translations": {
                                "ru": "Перед Пасхой вся наша семья садится за стол и <w>расписывает</w> цветные писанки."
                            }
                        }
                    ],
                    "synonyms": [
                        "nazywać",
                        "uważać"
                    ],
                    "antonyms": [
                        "malować"
                    ],
                    "common_phrases": [
                        "pisać ikony",
                        "pisać pisanki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать иконы или наносить узоры на пасхальные яйца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "писать",
                                "target_lang_sense_clarification": "иконы, картины"
                            },
                            {
                                "target_lang_word": "расписывать",
                                "target_lang_sense_clarification": "пасхальные яйца"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
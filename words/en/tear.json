    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "180adb1c-f072-3489-9473-3ef899d09ad0",
                    "sense_definition": "A drop of clear, salty liquid produced from the eyes by crying or irritation.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "crying_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Big <w>tears</w> streamed from her eyes as she watched the sad movie.",
                            "target_lang_translations": {
                                "ru": "Крупные <w>слёзы</w> текли из её глаз, пока она смотрела грустный фильм."
                            }
                        },
                        {
                            "text": "He wiped a <w>tear</w> from his cheek.",
                            "target_lang_translations": {
                                "ru": "Он смахнул <w>слезу</w> со своей щеки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "burst into tears",
                        "in tears",
                        "shed a tear",
                        "wipe away a tear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Капля прозрачной соленой жидкости, выделяемая из глаз при плаче или раздражении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слеза",
                                "target_lang_sense_clarification": "капля жидкости из глаз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88af63d3-4381-384e-b0f8-1865c57bea61",

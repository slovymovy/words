    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e709a419-c214-33f8-a18b-94722178dd6f",
                    "sense_definition": "Имеется информация или сведения о ком-либо или чём-либо; является установленным фактом.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "predicative_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Насколько мне <известно>, этот магазин открывается в девять утра.",
                            "target_lang_translations": {
                                "en": "As far as I <know>, this store opens at nine in the morning."
                            }
                        },
                        {
                            "text": "Всем <известно>, что курение вредит здоровью.",
                            "target_lang_translations": {
                                "en": "It is <known> to everyone that smoking harms health."
                            }
                        },
                        {
                            "text": "Нам стало <известно> о его планах только вчера.",
                            "target_lang_translations": {
                                "en": "We became <aware> of his plans only yesterday."
                            }
                        }
                    ],
                    "antonyms": [
                        "неизвестно"
                    ],
                    "common_phrases": [
                        "стало известно",
                        "хорошо известно",
                        "насколько мне известно",
                        "доподлинно известно"
                    ],
                    "target_lang_definitions": {
                        "en": "Information is available or established as a fact; often used in impersonal constructions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "known",
                                "target_lang_sense_clarification": "general state of knowledge"
                            },
                            {
                                "target_lang_word": "aware",
                                "target_lang_sense_clarification": "having knowledge or perception of a situation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "716945b0-941a-3cce-8915-42040b07bc4d",
                    "sense_definition": "Краткая форма среднего рода прилагательного <известный>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "morphology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это имя <известно> читателям во всём мире.",
                            "target_lang_translations": {
                                "en": "This name is <known> to readers all over the world."
                            }
                        },
                        {
                            "text": "Его лицо мне <известно>, но я не помню, где мы встречались.",
                            "target_lang_translations": {
                                "en": "His face is <familiar> to me, but I don't remember where we met."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short neuter singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short neuter form of the adjective 'known'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "known",
                                "target_lang_sense_clarification": "recognized or identified"
                            },
                            {
                                "target_lang_word": "familiar",
                                "target_lang_sense_clarification": "recognizable from past experience"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1194224-7df8-30ab-9f00-007da99160c1",
                    "sense_definition": "Употребляется как вводное слово для выражения уверенности или подтверждения; конечно, разумеется.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "parenthetic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он, <известно>, не хотел признавать свою ошибку.",
                            "target_lang_translations": {
                                "en": "He, <naturally>, did not want to admit his mistake."
                            }
                        },
                        {
                            "text": "В такую погоду, <известно>, лучше сидеть дома.",
                            "target_lang_translations": {
                                "en": "In such weather, <of course>, it is better to stay home."
                            }
                        }
                    ],
                    "synonyms": [
                        "конечно",
                        "понятно"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "просторечие"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used as a parenthetical word to express certainty or confirmation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "naturally",
                                "target_lang_sense_clarification": "as might be expected"
                            },
                            {
                                "target_lang_word": "of course",
                                "target_lang_sense_clarification": "expressing certainty"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "game_mechanic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you collect one hundred coins, you get an extra <w>life</w>.",
                            "target_lang_translations": {
                                "ru": "Если соберешь сто монет, получишь дополнительную <w>жизнь</w>."
                            }
                        },
                        {
                            "text": "I lost my last <w>life</w> trying to defeat the final boss.",
                            "target_lang_translations": {
                                "ru": "Я потерял последнюю <w>жизнь</w>, пытаясь победить финального босса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "extra life",
                        "lose a life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из шансов игрока продолжить игру после проигрыша."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жизнь",
                                "target_lang_sense_clarification": "в играх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6f3471a-d47f-3386-b8dc-7f0840319eb1",

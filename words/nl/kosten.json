    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aed9a5fd-0142-3c06-a27e-1a472ea35871",
                    "sense_definition": "Een bepaalde prijs of bedrag hebben dat betaald moet worden om iets te verkrijgen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "price",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze boeken <w>kosten</w> maar vijf euro per stuk.",
                            "target_lang_translations": {
                                "en": "These books <w>cost</w> only five euros each.",
                                "ru": "Эти книги <w>стоят</w> всего пять евро за штуку."
                            }
                        },
                        {
                            "text": "Dat grapje heeft hem uiteindelijk veel geld <w>gekost</w>.",
                            "target_lang_translations": {
                                "en": "That joke eventually <w>cost</w> him a lot of money.",
                                "ru": "Эта шутка в конечном итоге <w>стоила</w> ему кучу денег."
                            }
                        }
                    ],
                    "common_phrases": [
                        "veel geld kosten",
                        "niets kosten",
                        "hoeveel kost dat"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a specific price or amount that must be paid to obtain something.",
                        "ru": "Иметь определенную цену или сумму, которую нужно заплатить для получения чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cost",
                                "target_lang_sense_clarification": "to have a price of"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "стоить",
                                "target_lang_sense_clarification": "иметь цену"
                            },
                            {
                                "target_lang_word": "обходиться",
                                "target_lang_sense_clarification": "о сумме затрат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da0df482-13cd-3875-8bd2-29c4081a1222",
                    "sense_definition": "Iets vereisen of wegnemen als noodzakelijke inzet of gevolg, zoals tijd, moeite of gezondheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "require",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat werk <w>kost</w> veel tijd, moeite en geduld om goed te doen.",
                            "target_lang_translations": {
                                "en": "That work <w>takes</w> a lot of time, effort, and patience to do well.",
                                "ru": "Эта работа <w>требует</w> много времени, усилий и терпения, чтобы сделать её хорошо."
                            }
                        },
                        {
                            "text": "Het <w>kostte</w> me wat tijd om het hele project af te maken.",
                            "target_lang_translations": {
                                "en": "It <w>took</w> me some time to finish the whole project.",
                                "ru": "У меня <w>заняло</w> некоторое время, чтобы закончить весь проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "vereisen",
                        "vergen"
                    ],
                    "common_phrases": [
                        "tijd kosten",
                        "moeite kosten",
                        "het leven kosten"
                    ],
                    "target_lang_definitions": {
                        "en": "To require or involve something as a necessary input or consequence, such as time, effort, or health.",
                        "ru": "Требовать чего-либо или отнимать что-либо (время, усилия, здоровье) как необходимое условие или следствие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cost",
                                "target_lang_sense_clarification": "to require or involve loss/sacrifice"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "to require time or effort"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "требовать",
                                "target_lang_sense_clarification": "быть необходимым (усилия, время)"
                            },
                            {
                                "target_lang_word": "отнимать",
                                "target_lang_sense_clarification": "забирать (время, силы)"
                            },
                            {
                                "target_lang_word": "стоить",
                                "target_lang_sense_clarification": "приводить к потере (напр. жизни)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b4cad89a-d074-3dd7-8e63-9cf1f2d537d4",
                    "sense_definition": "Het totaalbedrag dat betaald moet worden voor goederen, diensten of door veroorzaakte schade; uitgaven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "expenses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We begrijpen dat de <w>kosten</w> voor de universiteit erg hoog zijn.",
                            "target_lang_translations": {
                                "en": "We understand that the <w>costs</w> for university are very high.",
                                "ru": "Мы понимаем, что <w>расходы</w> на университет очень высоки."
                            }
                        },
                        {
                            "text": "De <w>kosten</w> voor het levensonderhoud zijn de laatste jaren flink gestegen.",
                            "target_lang_translations": {
                                "en": "The <w>cost</w> of living has risen sharply in recent years.",
                                "ru": "<w>Расходы</w> на жизнь значительно выросли за последние годы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op kosten van",
                        "ten koste van",
                        "kosten maken"
                    ],
                    "target_lang_definitions": {
                        "en": "The total amount that must be paid for goods, services, or incurred damage; expenses.",
                        "ru": "Общая сумма, которую необходимо заплатить за товары, услуги или причиненный ущерб; расходы."
                    },
                    "translations": {

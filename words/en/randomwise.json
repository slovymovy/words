            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f2348c10-a6d9-31c1-9b50-951b52943e1d",
                    "sense_definition": "In a random manner; without a specific pattern, method, or plan.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The books were stacked <w>randomwise</w> on the floor rather than on the shelves.",
                            "target_lang_translations": {
                                "ru": "Книги были сложены <w>беспорядочно</w> на полу, а не на полках."
                            }
                        },
                        {
                            "text": "Participants were chosen <w>randomwise</w> from the large group of volunteers.",
                            "target_lang_translations": {
                                "ru": "Участники были выбраны <w>случайным образом</w> из большой группы добровольцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "randomly",
                        "haphazardly",
                        "arbitrarily"
                    ],
                    "antonyms": [
                        "systematically",
                        "methodically",
                        "orderly"
                    ],
                    "common_phrases": [
                        "chosen randomwise",
                        "scattered randomwise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случайным образом; без определенного шаблона, метода или плана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "случайным образом",

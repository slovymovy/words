    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "01185bf6-aea9-376d-97ae-4b30ad543d81",
                    "sense_definition": "Mający ciemnoczerwony kolor z odcieniem fioletu, przypominający barwę czerwonego wina; przymiotnik nieodmienny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "color_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej nowa sukienka ma piękny odcień <w>bordo</w>, który pasuje do jej cery.",
                            "target_lang_translations": {
                                "ru": "У её нового платья красивый оттенок <w>бордо</w>, который подходит к её цвету лица."
                            }
                        },
                        {
                            "text": "W tym sezonie bardzo modne są dodatki w kolorze <w>bordo</w>.",
                            "target_lang_translations": {
                                "ru": "В этом сезоне очень модны аксессуары цвета <w>бордо</w>."
                            }
                        },
                        {
                            "text": "Czarna krowa w kropki <w>bordo</w> gryzła trawę kręcąc mordą.",
                            "target_lang_translations": {
                                "ru": "Чёрная корова в пятнышках <w>бордо</w> щипала траву, вертя мордой."
                            }
                        }
                    ],
                    "synonyms": [
                        "bordowy",
                        "winny"
                    ],
                    "common_phrases": [
                        "w kolorze bordo",
                        "odcień bordo",
                        "kropki bordo"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "nieodmienny (indeclinable)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий темно-красный цвет с фиолетовым оттенком, напоминающий цвет красного вина; несклоняемое прилагательное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бордовый",
                                "target_lang_sense_clarification": "склоняемое прилагательное"
                            },
                            {
                                "target_lang_word": "бордо",
                                "target_lang_sense_clarification": "несклоняемое прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fb5c7be0-d0ec-3f07-8c6f-bdab30de5f87",
                    "sense_definition": "Ciemnoczerwony, ciemnowiśniowy kolor; barwa wina Bordeaux.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "color_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głębokie <w>bordo</w> na ścianach w salonie nadaje wnętrzu elegancji.",
                            "target_lang_translations": {
                                "ru": "Глубокое <w>бордо</w> на стенах в гостиной придает интерьеру элегантности."
                            }
                        },
                        {
                            "text": "To nie jest zwykła czerwień, to raczej ciemne <w>bordo</w>.",
                            "target_lang_translations": {
                                "ru": "Это не обычный красный, это скорее темное <w>бордо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bordeaux"
                    ],
                    "common_phrases": [
                        "głębokie bordo",
                        "ciemne bordo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Темно-красный, темно-вишневый цвет; цвет вина бордо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бордо",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "бордовый цвет",
                                "target_lang_sense_clarification": "словосочетание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9161e32-dc85-30a9-a200-f1eb5c9da8fa",
                    "sense_definition": "Wysokiej jakości wino (zazwyczaj czerwone) pochodzące z rejonu Bordeaux we Francji.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "wine_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do uroczystego obiadu podano wyśmienite francuskie <w>bordo</w>.",
                            "target_lang_translations": {
                                "ru": "К торжественному обеду подали восхитительное французское <w>бордо</w>."
                            }
                        },
                        {
                            "text": "Kelner polecił nam wytrawne <w>bordo</w> z dobrego rocznika.",
                            "target_lang_translations": {
                                "ru": "Официант порекомендовал нам сухое <w>бордо</w> хорошего урожая."
                            }
                        }
                    ],
                    "synonyms": [
                        "bordeaux"
                    ],
                    "common_phrases": [
                        "wytrawne bordo",
                        "francuskie bordo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высококачественное вино (обычно красное) из региона Бордо во Франции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бордо",
                                "target_lang_sense_clarification": "вино"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
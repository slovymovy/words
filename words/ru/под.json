                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "towards",
                                "target_lang_sense_clarification": "approaching a specific time"
                            },
                            {
                                "target_lang_word": "nearly",
                                "target_lang_sense_clarification": "approaching an age or amount"
                            },
                            {
                                "target_lang_word": "getting on for",
                                "target_lang_sense_clarification": "approaching an age (idiomatic)"
                            }
                        ]

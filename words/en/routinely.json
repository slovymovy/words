{
    "word_family": [
        "routine",
        "routinize",
        "routinization"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b129a993-799a-3d75-9b75-0ee5c94f6f2d",
                    "sense_definition": "In a way that is done frequently and follows a standard procedure or custom.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "regular_occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doctors <w>routinely</w> check a patient's blood pressure during every visit.",
                            "target_lang_translations": {
                                "ru": "Врачи <w>регулярно</w> проверяют артериальное давление пациента во время каждого визита."
                            }
                        },
                        {
                            "text": "Large corporations <w>routinely</w> monitor internet usage to ensure productivity.",
                            "target_lang_translations": {
                                "ru": "Крупные корпорации <w>в плановом порядке</w> отслеживают использование интернета для обеспечения продуктивности."
                            }
                        }
                    ],
                    "synonyms": [
                        "customarily",
                        "normally",
                        "ordinarily"
                    ],
                    "antonyms": [
                        "rarely",
                        "exceptionally"
                    ],
                    "common_phrases": [
                        "routinely checked",
                        "routinely used",
                        "routinely available"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часто и в соответствии с установленным порядком или обычаем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регулярно",
                                "target_lang_sense_clarification": "выполняемое через равные промежутки времени"
                            },
                            {
                                "target_lang_word": "обычно",
                                "target_lang_sense_clarification": "как правило, по обыкновению"
                            },
                            {
                                "target_lang_word": "в плановом порядке",
                                "target_lang_sense_clarification": "как часть стандартной процедуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4417014-669e-3240-bbcf-877780d32e6f",
                    "sense_definition": "Done by rote or habit, often without active thought, attention, or concern.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "habitual_rote",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>routinely</w> ignored the safety warnings until an accident finally occurred.",
                            "target_lang_translations": {
                                "ru": "Он <w>привычно</w> игнорировал предупреждения о безопасности, пока наконец не произошел несчастный случай."
                            }
                        },
                        {
                            "text": "She <w>routinely</w> dismisses any criticism without considering if it might be valid.",
                            "target_lang_translations": {
                                "ru": "Она <w>по инерции</w> отвергает любую критику, не задумываясь о том, может ли она быть обоснованной."
                            }
                        }
                    ],
                    "synonyms": [
                        "customarily"
                    ],
                    "antonyms": [
                        "thoughtfully",
                        "deliberately"
                    ],
                    "common_phrases": [
                        "routinely ignored",
                        "routinely dismissed"
                    ],
                    "target_lang_definitions": {
                        "ru": "По привычке или инерции, часто бездумно или без должного внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привычно",
                                "target_lang_sense_clarification": "в силу укоренившейся привычки"
                            },
                            {
                                "target_lang_word": "по инерции",
                                "target_lang_sense_clarification": "машинально, без активного участия сознания"
                            },
                            {
                                "target_lang_word": "постоянно",
                                "target_lang_sense_clarification": "неизменно (часто с негативным оттенком)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
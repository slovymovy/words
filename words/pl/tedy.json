    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "969ef1c0-71cc-3f74-b595-e92b4168a297",
                    "sense_definition": "Słowo łączące zdania lub ich części, wskazujące na to, że treść drugiego zdania wynika z treści pierwszego; to samo co: więc, zatem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie było innej drogi, <tedy> musieliśmy iść przez gęsty las.",
                            "target_lang_translations": {
                                "ru": "Не было другой дороги, <w>следовательно</w>, нам пришлось идти через густой лес."
                            }
                        },
                        {
                            "text": "Skoro tak postanowiłeś, <tedy> niech tak się stanie.",
                            "target_lang_translations": {
                                "ru": "Раз ты так решил, <w>то</w> пусть так и будет."
                            }
                        }
                    ],
                    "synonyms": [
                        "więc",
                        "przeto",
                        "toteż",
                        "w takim razie"
                    ],
                    "common_phrases": [
                        "rzekł tedy"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "używane w stylu książkowym lub podniosłym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, соединяющее предложения или их части, указывающее на то, что содержание второго предложения вытекает из содержания первого; то же, что: итак, следовательно, поэтому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следовательно",
                                "target_lang_sense_clarification": "вывод или следствие"
                            },
                            {
                                "target_lang_word": "то",
                                "target_lang_sense_clarification": "в конструкции «если/раз..., то...»"
                            },
                            {
                                "target_lang_word": "посему",
                                "target_lang_sense_clarification": "устаревшее/книжное «поэтому»"
                            },
                            {
                                "target_lang_word": "итак",
                                "target_lang_sense_clarification": "подведение итога"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "77752106-2e5d-38c6-a115-e61bff3e31bc",
                    "sense_definition": "Słowo oznaczające moment lub okres w przeszłości, o którym mowa; to samo co: wtedy, wówczas.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słońce chyliło się ku zachodowi, <tedy> w obozie nastała cisza.",
                            "target_lang_translations": {
                                "ru": "Солнце клонилось к закату, <w>тогда</w> в лагере наступила тишина."
                            }
                        },
                        {
                            "text": "Gdy król przybył do miasta, <tedy> wszyscy wyszli na ulice.",
                            "target_lang_translations": {
                                "ru": "Когда король прибыл в город, <w>тогда</w> все вышли на улицы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, обозначающее момент или период в прошлом, о котором идет речь; то же, что: тогда, в то время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тогда",
                                "target_lang_sense_clarification": "в то время"
                            }
                        ]
                    }
                }
            ]
        }
    ]

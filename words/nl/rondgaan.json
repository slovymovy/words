    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d1c961cf-bb0a-39ce-93dd-721d48932eae",
                    "sense_definition": "Zich verspreiden onder mensen of doorgegeven worden in een groep.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "circulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Welke geruchten over de oorlog in Oekraïne <w>gaan</w> er <w>rond</w>?",
                            "target_lang_translations": {
                                "ru": "Какие слухи о войне в Украине <w>ходят</w>?"
                            }
                        },
                        {
                            "text": "Tijdens de verjaardag <w>ging</w> er een schaal met lekkere hapjes <w>rond</w>.",
                            "target_lang_translations": {
                                "ru": "На дне рождения блюдо с вкусными закусками <w>ходило по кругу</w>."
                            }
                        },
                        {
                            "text": "Er <w>gaat</w> momenteel een besmettelijk virus <w>rond</w> op de basisschool.",
                            "target_lang_translations": {
                                "ru": "В начальной школе сейчас <w>ходит</w> заразный вирус."
                            }
                        }
                    ],
                    "synonyms": [
                        "circuleren",
                        "rondzingen",
                        "rouleren"
                    ],
                    "common_phrases": [
                        "snel rondgaan",
                        "laten rondgaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространяться среди людей или передаваться по кругу в группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "о слухах, вирусах"
                            },
                            {
                                "target_lang_word": "распространяться",
                                "target_lang_sense_clarification": "об информации, болезнях"
                            },
                            {
                                "target_lang_word": "передаваться по кругу",
                                "target_lang_sense_clarification": "о предметах"
                            },
                            {
                                "target_lang_word": "циркулировать",
                                "target_lang_sense_clarification": "быть в обращении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdfea183-6a58-32f5-aaf1-986307d89d96",
                    "sense_definition": "Een draaiende beweging maken of om een as draaien.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De sleutel <w>ging</w> moeizaam <w>rond</w> in het oude slot.",
                            "target_lang_translations": {
                                "ru": "Ключ с трудом <w>поворачивался</w> в старом замке."
                            }
                        },
                        {
                            "text": "Het reuzenrad <w>ging</w> langzaam <w>rond</w> zodat iedereen kon instappen.",
                            "target_lang_translations": {
                                "ru": "Колесо обозрения медленно <w>вращалось</w>, чтобы все могли сесть."
                            }
                        }
                    ],
                    "synonyms": [
                        "draaien",
                        "ronddraaien"
                    ],
                    "antonyms": [
                        "stilstaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершать вращательное движение или поворачиваться вокруг оси."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вращаться",
                                "target_lang_sense_clarification": "двигаться по кругу"
                            },
                            {
                                "target_lang_word": "поворачиваться",
                                "target_lang_sense_clarification": "делать оборот (напр. о ключе)"
                            },
                            {
                                "target_lang_word": "крутиться",
                                "target_lang_sense_clarification": "вертеться"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "66c80def-93a2-369c-bec9-4d02cb07ffa0",
                    "sense_definition": "Een leider van een orkest, koor of ander muziekgezelschap.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>dirigent</w> gaf met zijn stokje het tempo aan voor de muzikanten.",
                            "target_lang_translations": {
                                "en": "The <w>conductor</w> indicated the tempo for the musicians with his baton.",
                                "ru": "<w>Дирижёр</w> своей палочкой задавал темп музыкантам."
                            }
                        },
                        {
                            "text": "Alle leden van het koor moeten goed naar de <w>dirigent</w> kijken.",
                            "target_lang_translations": {
                                "en": "All members of the choir must watch the <w>conductor</w> closely.",
                                "ru": "Все участники хора должны внимательно смотреть на <w>дирижёра</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "notenzwaaier"
                    ],
                    "common_phrases": [
                        "naar de dirigent kijken",
                        "de dirigent volgen"
                    ],
                    "target_lang_definitions": {
                        "en": "A leader of an orchestra, choir, or other musical group.",
                        "ru": "Руководитель оркестра, хора или другого музыкального коллектива."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conductor",
                                "target_lang_sense_clarification": "music"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дирижёр",
                                "target_lang_sense_clarification": "музыкальный руководитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff1bdd3a-0c7b-3431-b2d6-4017e1b68513",
                    "sense_definition": "Iemand die op de achtergrond de leiding heeft en bepaalt wat er gebeurt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was de stille <w>dirigent</w> achter de succesvolle verkiezingscampagne.",
                            "target_lang_translations": {
                                "en": "He was the silent <w>orchestrator</w> behind the successful election campaign.",
                                "ru": "Он был тайным <w>дирижёром</w> успешной избирательной кампании."
                            }
                        },
                        {
                            "text": "De eerste cellist was de <w>dirigent</w> van een actie tegen het bestuur.",
                            "target_lang_translations": {
                                "en": "The first cellist was the <w>orchestrator</w> of an action against the board.",
                                "ru": "Первый виолончелист был <w>организатором</w> акции против правления."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who leads from the background and determines what happens.",
                        "ru": "Тот, кто негласно руководит и определяет ход событий (в переносном смысле)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "orchestrator",
                                "target_lang_sense_clarification": "figurative"
                            },
                            {
                                "target_lang_word": "mastermind",
                                "target_lang_sense_clarification": "figurative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "организатор",
                                "target_lang_sense_clarification": "инициатор действий"
                            },
                            {
                                "target_lang_word": "вдохновитель",
                                "target_lang_sense_clarification": "идейный лидер"
                            },
                            {
                                "target_lang_word": "дирижёр",
                                "target_lang_sense_clarification": "метафорическое значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

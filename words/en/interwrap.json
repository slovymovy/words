    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3e92e5d7-0b97-3580-bc5b-e7d73988b7d2",
                    "sense_definition": "To intertwine; to wrap together.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The roots of the ancient trees <w>interwrap</w> beneath the forest floor.",
                            "target_lang_translations": {
                                "ru": "Корни древних деревьев <w>переплетаются</w> под лесной подстилкой."
                            }
                        },
                        {
                            "text": "Two distinct melodies <w>interwrap</w> to create a complex harmony.",
                            "target_lang_translations": {
                                "ru": "Две разные мелодии <w>сплетаются</w>, создавая сложную гармонию."
                            }
                        }
                    ],
                    "synonyms": [
                        "intertwine",
                        "entwine",
                        "interlace",
                        "tangle"
                    ],
                    "antonyms": [
                        "unwrap",
                        "unravel",
                        "disentangle"
                    ],
                    "common_phrases": [
                        "interwrap with",
                        "closely interwrap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переплетаться; обвивать друг друга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переплетаться",
                                "target_lang_sense_clarification": "обвиваться друг вокруг друга"
                            },
                            {
                                "target_lang_word": "сплетаться",
                                "target_lang_sense_clarification": "соединяться в одно целое (о корнях, мелодиях)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ebb8c047-72d9-35ed-b56c-592783b1cb97",
                    "sense_definition": "Situated or occurring between wraps or layers of winding material.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineer designed the coil to minimize <w>interwrap</w> friction.",
                            "target_lang_translations": {
                                "ru": "Инженер спроектировал катушку так, чтобы минимизировать <w>межвитковое</w> трение."
                            }
                        },
                        {
                            "text": "We detected significant <w>interwrap</w> movement within the cable structure.",
                            "target_lang_translations": {
                                "ru": "Мы обнаружили значительное <w>межвитковое</w> смещение внутри структуры кабеля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "interwrap slippage",
                        "interwrap pressure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся или происходящий между витками или слоями намоточного материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "межвитковый",
                                "target_lang_sense_clarification": "технический термин, относящийся к пространству между витками катушки или кабеля"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5b4e972d-7666-3939-b717-02dc37237db7",
                    "sense_definition": "The act of wrapping together or the condition of being intertwined.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tight <w>interwrap</w> of the fibers gives the rope its immense strength.",
                            "target_lang_translations": {
                                "ru": "Плотное <w>переплетение</w> волокон придает канату его огромную прочность."
                            }
                        },
                        {
                            "text": "Researchers studied the thermal effects caused by the <w>interwrap</w> of the metal sheets.",
                            "target_lang_translations": {
                                "ru": "Исследователи изучили тепловые эффекты, вызванные <w>переплетением</w> металлических листов."
                            }
                        }
                    ],
                    "synonyms": [
                        "intertwining",
                        "entanglement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по совместному обертыванию или состояние переплетения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переплетение",
                                "target_lang_sense_clarification": "состояние переплетенных объектов"
                            },
                            {
                                "target_lang_word": "сплетение",
                                "target_lang_sense_clarification": "место или результат соединения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "5e79a5cb-68bc-3718-8203-5627eb464c76",
                    "sense_definition": "Forma rodzaju nijakiego liczebnika <w>jeden</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "number_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na stole zostało tylko <w>jedno</w> jabłko.",
                            "target_lang_translations": {
                                "ru": "На столе осталось только <w>одно</w> яблоко."
                            }
                        },
                        {
                            "text": "Mam do ciebie <w>jedno</w> ważne pytanie.",
                            "target_lang_translations": {
                                "ru": "У меня к тебе есть <w>один</w> важный вопрос."
                            }
                        }
                    ],
                    "antonyms": [
                        "wiele",
                        "dużo",
                        "zero"
                    ],
                    "common_phrases": [
                        "jedno z dwóch",
                        "ani jedno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма среднего рода числительного «один»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одно",
                                "target_lang_sense_clarification": "числительное (средний род)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "9191ad63-3867-37e7-a3c3-d9bba8723e7b",
                    "sense_definition": "Jedna, konkretna rzecz, sytuacja lub sprawa, o której mowa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "specific_thing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powiem ci <w>jedno</w>: nigdy się nie poddawaj.",
                            "target_lang_translations": {
                                "ru": "Скажу тебе <w>одно</w>: никогда не сдавайся."
                            }
                        },
                        {
                            "text": "Wszystko mi <w>jedno</w>, co zrobisz z tymi pieniędzmi.",
                            "target_lang_translations": {
                                "ru": "Мне <w>всё равно</w>, что ты сделаешь с этими деньгами."
                            }
                        }
                    ],
                    "antonyms": [
                        "wiele"
                    ],
                    "common_phrases": [
                        "wszystko jedno",
                        "jedno z dwojga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна конкретная вещь, ситуация или дело, о котором идет речь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одно",
                                "target_lang_sense_clarification": "местоимение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d8f1a73-640d-3fd9-a6d8-6a9c2cb2f612",
                    "sense_definition": "Ta sama rzecz lub treść; brak różnicy między omawianymi elementami.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sameness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dla mnie to <w>jedno</w> i to samo, więc wybierz sam.",
                            "target_lang_translations": {
                                "ru": "Для меня это <w>одно</w> и то же, так что выбирай сам."
                            }
                        },
                        {
                            "text": "On wciąż powtarza <w>jedno</w>, nie słuchając innych argumentów.",
                            "target_lang_translations": {
                                "ru": "Он всё твердит <w>одно</w>, не слушая других аргументов."
                            }
                        }
                    ],
                    "antonyms": [
                        "inne",
                        "różne"
                    ],
                    "common_phrases": [
                        "jedno i to samo",
                        "wciąż jedno"
                    ],
                    "target_lang_definitions": {
                        "ru": "То же самое; отсутствие разницы между обсуждаемыми элементами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одно",
                                "target_lang_sense_clarification": "в значении «одно и то же»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d383c09-12ae-335a-94cb-07679afea9ac",
                    "sense_definition": "Nierozerwalna całość złożona z elementów; jedność.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "unity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W obliczu zagrożenia wszyscy musimy stanowić <w>jedno</w>.",
                            "target_lang_translations": {
                                "ru": "Перед лицом опасности мы все должны стать <w>единым целым</w>."
                            }
                        },
                        {
                            "text": "Ich myśli i serca zlały się w <w>jedno</w>.",
                            "target_lang_translations": {
                                "ru": "Их мысли и сердца слились в <w>одно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "jedynie",
                        "tylko",
                        "wyłącznie"
                    ],
                    "common_phrases": [
                        "stanowić jedno",
                        "zlać się w jedno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неразрывное целое, состоящее из элементов; единство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "единое целое",
                                "target_lang_sense_clarification": "сущность"
                            },
                            {
                                "target_lang_word": "одно",
                                "target_lang_sense_clarification": "единство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "44794d3b-41c8-376e-89a5-41f7f00f98d2",
                    "sense_definition": "Słowo ograniczające wypowiedź do wskazanego elementu; dawniej używane w znaczeniu: tylko.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "limitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie pragnął bogactw, <w>jedno</w> spokoju ducha na stare lata.",
                            "target_lang_translations": {
                                "ru": "Он не жаждал богатств, <w>лишь</w> душевного покоя на старости лет."
                            }
                        },
                        {
                            "text": "Wokoło nie było widać żywej duszy, <w>jedno</w> ciemny las.",
                            "target_lang_translations": {
                                "ru": "Вокруг не было видно ни души, <w>только</w> темный лес."
                            }
                        }
                    ],
                    "synonyms": [
                        "jeno",
                        "jedynie",
                        "tylko",
                        "wyłącznie"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie zastępowane przez 'tylko' lub 'jedynie'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, ограничивающее высказывание указанным элементом; устаревшее значение: только."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лишь",
                                "target_lang_sense_clarification": "частица"
                            },
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "частица"
                            }
                        ]
                    }
                }
            ]

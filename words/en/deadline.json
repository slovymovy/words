                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Граница вокруг тюрьмы, при пересечении которой охранники имеют право стрелять в заключенных."
                    },
                    "translations": {
                        "ru": [
                            {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d6b1ec64-f5c5-3de5-97b1-371cf5168033",
                    "sense_definition": "The bodies that have political or administrative power and control in a particular sphere.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "official_bodies",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local <w>authorities</w> have decided to close the park for renovations.",
                            "target_lang_translations": {
                                "ru": "Местные <w>власти</w> решили закрыть парк на реконструкцию."
                            }
                        },
                        {
                            "text": "You must submit your application to the relevant housing <w>authorities</w>.",
                            "target_lang_translations": {
                                "ru": "Вы должны подать заявление в соответствующие жилищные <w>органы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "officials",
                        "administration",
                        "government"
                    ],
                    "common_phrases": [
                        "local authorities",
                        "health authorities",
                        "public authorities",
                        "civil authorities"
                    ],
                    "target_lang_definitions": {
                        "ru": "Органы, обладающие политической или административной властью и контролем в определенной сфере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "власти",
                                "target_lang_sense_clarification": "правительство, администрация"
                            },
                            {
                                "target_lang_word": "органы власти",
                                "target_lang_sense_clarification": "государственные учреждения"
                            },
                            {
                                "target_lang_word": "начальство",
                                "target_lang_sense_clarification": "руководство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb946c92-5fa8-33bc-a971-ec8910a0cae6",
                    "sense_definition": "The bodies that enforce law and order or provide a public service.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "official_bodies",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Witnesses were asked to contact the <w>authorities</w> if they saw anything suspicious.",
                            "target_lang_translations": {
                                "ru": "Свидетелей попросили связаться с <w>властями</w>, если они заметят что-либо подозрительное."
                            }
                        },
                        {
                            "text": "The federal <w>authorities</w> took over the investigation yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера федеральные <w>власти</w> взяли расследование под свой контроль."
                            }
                        }
                    ],
                    "synonyms": [
                        "police",
                        "law enforcement"
                    ],
                    "common_phrases": [
                        "contact the authorities",
                        "federal authorities",
                        "alert the authorities",
                        "call the authorities"
                    ],
                    "target_lang_definitions": {
                        "ru": "Органы, обеспечивающие соблюдение закона и порядка или предоставляющие государственные услуги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "власти",
                                "target_lang_sense_clarification": "полиция, официальные лица"
                            },
                            {
                                "target_lang_word": "правоохранительные органы",
                                "target_lang_sense_clarification": "силовые структуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58568f8d-aae2-3374-b690-e01aa2376d48",
                    "sense_definition": "Plural of <w>authority</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She cited several leading <w>authorities</w> on the subject in her research paper.",
                            "target_lang_translations": {
                                "ru": "В своей научной работе она сослалась на нескольких ведущих <w>авторитетов</w> в этой области."
                            }
                        },
                        {
                            "text": "Different <w>authorities</w> have offered conflicting interpretations of the ancient text.",
                            "target_lang_translations": {
                                "ru": "Разные <w>специалисты</w> предложили противоречивые толкования древнего текста."
                            }
                        }
                    ],
                    "synonyms": [
                        "experts",
                        "specialists"
                    ],
                    "common_phrases": [
                        "leading authorities",
                        "medical authorities"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Признанные эксперты или специалисты в определенной области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "авторитеты",
                                "target_lang_sense_clarification": "эксперты"
                            },
                            {
                                "target_lang_word": "специалисты",
                                "target_lang_sense_clarification": "знатоки дела"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "071b78e1-1656-36ac-9229-9e4b69058071",
                    "sense_definition": "Komenda wydawana zwierzętom pociągowym, zazwyczaj koniom lub wołom, nakazująca im skręt w prawo.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "commands_animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woźnica pociągnął za lejce i krzyknął <hetta>, kierując wóz na boczną drogę.",
                            "target_lang_translations": {
                                "ru": "Кучер натянул вожжи и крикнул <цобе>, направляя повозку на боковую дорогу."
                            }
                        },
                        {
                            "text": "Konie doskonale wiedziały, że głośne <hetta> oznacza konieczność skrętu w prawo.",
                            "target_lang_translations": {
                                "ru": "Лошади прекрасно знали, что громкое <цобе> означает необходимость поворота направо."
                            }
                        }
                    ],
                    "synonyms": [
                        "hejt",
                        "hejta"
                    ],
                    "antonyms": [
                        "wiśta"
                    ],
                    "common_phrases": [
                        "krzyczeć hetta",
                        "wiśta-hetta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Команда, отдаваемая тягловым животным (обычно лошадям или волам) с требованием повернуть направо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цобе",
                                "target_lang_sense_clarification": "команда повернуть направо"
                            },
                            {
                                "target_lang_word": "цабе",
                                "target_lang_sense_clarification": "вариант команды для поворота направо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f398097f-ab92-3ad3-ab0e-c14a3243d6b7",
                    "sense_definition": "Okrzyk zachęcający konie do ruszenia z miejsca lub szybszego biegu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "commands_animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz zawołał <hetta> i zaprzęg powoli ruszył w stronę pola.",
                            "target_lang_translations": {
                                "ru": "Хозяин крикнул <но>, и упряжка медленно двинулась в сторону поля."
                            }
                        },
                        {
                            "text": "Zmęczone zwierzęta niechętnie reagowały na kolejne <hetta> woźnicy.",
                            "target_lang_translations": {
                                "ru": "Уставшие животные неохотно реагировали на очередное <но> кучера."
                            }
                        }
                    ],
                    "synonyms": [
                        "wio"
                    ],
                    "antonyms": [
                        "prr"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, побуждающий лошадей тронуться с места или ускорить бег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "но",
                                "target_lang_sense_clarification": "команда к движению"
                            },
                            {
                                "target_lang_word": "пошёл",
                                "target_lang_sense_clarification": "команда начать движение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

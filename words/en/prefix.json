{
    "word_family": [
        "prefixation",
        "prefixal"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c88a56d-0e5d-3034-a005-b797676790b0",
                    "sense_definition": "A letter or group of letters added to the beginning of a word to change its meaning.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "linguistics_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can change the meaning of the word by adding a negative <w>prefix</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете изменить значение слова, добавив отрицательную <w>приставку</w>."
                            }
                        },
                        {
                            "text": "The <w>prefix</w> 're-' usually indicates that an action is being repeated.",
                            "target_lang_translations": {
                                "ru": "<w>Приставка</w> «re-» обычно указывает на то, что действие повторяется."
                            }
                        }
                    ],
                    "synonyms": [
                        "forefix"
                    ],
                    "antonyms": [
                        "suffix"
                    ],
                    "common_phrases": [
                        "common prefix",
                        "add a prefix",
                        "negative prefix"
                    ],
                    "target_lang_definitions": {
                        "ru": "Буква или группа букв, добавляемая в начало слова для изменения его значения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приставка",
                                "target_lang_sense_clarification": "грамматика"
                            },
                            {
                                "target_lang_word": "префикс",
                                "target_lang_sense_clarification": "лингвистический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79f8410f-7a4b-350f-860a-efc615136d7d",
                    "sense_definition": "A set of numbers placed before a telephone number to indicate the region or country.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "telecom_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to dial the country <w>prefix</w> if you are calling from abroad.",
                            "target_lang_translations": {
                                "ru": "Вам нужно набрать <w>код</w> страны, если вы звоните из-за границы."
                            }
                        },
                        {
                            "text": "Numbers with the 0800 <w>prefix</w> are usually free to call.",
                            "target_lang_translations": {
                                "ru": "Номера с <w>кодом</w> 0800 обычно бесплатны для звонков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "country prefix",
                        "dialling prefix",
                        "area prefix"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор цифр перед телефонным номером, указывающий на регион или страну."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "код",
                                "target_lang_sense_clarification": "телефонный"
                            },
                            {
                                "target_lang_word": "префикс",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "221b3bca-09f7-387c-a6b8-ba3c238b1d20",
                    "sense_definition": "A title added before a person's name, such as Mr., Dr., or Mrs.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "titles_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please select the appropriate <w>prefix</w> for your name on the form.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, выберите подходящее <w>обращение</w> к вашему имени в форме."
                            }
                        },
                        {
                            "text": "He prefers to use the <w>prefix</w> 'Doctor' in professional settings.",
                            "target_lang_translations": {
                                "ru": "В профессиональной среде он предпочитает использовать <w>титул</w> «Доктор»."
                            }
                        }
                    ],
                    "common_phrases": [
                        "honorific prefix",
                        "name prefix"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул или форма вежливости, добавляемая перед именем человека (например, г-н, д-р)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обращение",
                                "target_lang_sense_clarification": "форма вежливости"
                            },
                            {
                                "target_lang_word": "титул",
                                "target_lang_sense_clarification": "звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5babd5e8-d62d-373e-911c-d8ac02425c32",
                    "sense_definition": "An initial segment of a string of characters.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The algorithm checks if the string contains a specific <w>prefix</w>.",
                            "target_lang_translations": {
                                "ru": "Алгоритм проверяет, содержит ли строка определенный <w>префикс</w>."
                            }
                        },
                        {
                            "text": "In this code, 'abra' is a <w>prefix</w> of the word 'abracadabra'.",
                            "target_lang_translations": {
                                "ru": "В этом коде «abra» — это <w>префикс</w> слова «abracadabra»."
                            }
                        }
                    ],
                    "antonyms": [
                        "suffix"
                    ],
                    "common_phrases": [
                        "string prefix",
                        "file prefix"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начальный сегмент строки символов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "префикс",
                                "target_lang_sense_clarification": "информатика"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b0ec6c1e-72a1-343d-aa3c-f9d57a72f72f",
                    "sense_definition": "To place something at the beginning of a word, phrase, or identifier.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author <w>prefixed</w> the chapter with a short quotation.",
                            "target_lang_translations": {
                                "ru": "Автор <w>предварил</w> главу короткой цитатой."
                            }
                        },
                        {
                            "text": "You should <w>prefix</w> the variable name with an underscore to indicate it is private.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>начать</w> имя переменной с символа подчеркивания, чтобы указать, что она приватная."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prefix with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать что-либо в начало слова, фразы или идентификатора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предварять",
                                "target_lang_sense_clarification": "чем-либо"
                            },
                            {
                                "target_lang_word": "начинать",
                                "target_lang_sense_clarification": "с чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3dd8fef-9d24-3e00-9f3b-c9a00488a33a",
                    "sense_definition": "To determine or set something in advance.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>prefixed</w> a day for the final meeting.",
                            "target_lang_translations": {
                                "ru": "Они <w>заранее назначили</w> день для заключительной встречи."
                            }
                        },
                        {
                            "text": "Fate seemed to have <w>prefixed</w> the limits of his ambition.",
                            "target_lang_translations": {
                                "ru": "Казалось, судьба <w>предопределила</w> границы его амбиций."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Определять или устанавливать что-либо заранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "заранее"
                            },
                            {
                                "target_lang_word": "предопределять",
                                "target_lang_sense_clarification": "о судьбе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
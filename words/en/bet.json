{
    "word_family": [
        "bettor",
        "betting"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f19d807e-8bcc-3283-81f7-f5c6c7ce97fb",
                    "sense_definition": "To risk money or something valuable on the result of an event or competition.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_wager",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>bet</w> five dollars that my team will win the game.",
                            "target_lang_translations": {
                                "ru": "Я <w>поставил</w> пять долларов на то, что моя команда выиграет игру."
                            }
                        },
                        {
                            "text": "He rarely <w>bets</w> on horse races because he dislikes gambling.",
                            "target_lang_translations": {
                                "ru": "Он редко <w>делает ставки</w> на скачках, потому что не любит азартные игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "gamble",
                        "wager",
                        "stake"
                    ],
                    "common_phrases": [
                        "bet on",
                        "I bet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рисковать деньгами или чем-то ценным, ставя на исход события или соревнования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставить",
                                "target_lang_sense_clarification": "делать ставку в азартной игре"
                            },
                            {
                                "target_lang_word": "делать ставку",
                                "target_lang_sense_clarification": "участвовать в азартной игре"
                            },
                            {
                                "target_lang_word": "держать пари",
                                "target_lang_sense_clarification": "заключать спор на деньги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f263d31c-e72c-36ff-9053-7bc8040f12f7",
                    "sense_definition": "To be certain or confident that something is true or will happen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>bet</w> that it will rain before the evening is over.",
                            "target_lang_translations": {
                                "ru": "<w>Держу пари</w>, что до конца вечера пойдет дождь."
                            }
                        },
                        {
                            "text": "You can <w>bet</w> that she will be late again.",
                            "target_lang_translations": {
                                "ru": "Можешь <w>быть уверен</w>, что она снова опоздает."
                            }
                        }
                    ],
                    "synonyms": [
                        "predict",
                        "expect",
                        "reckon"
                    ],
                    "antonyms": [
                        "doubt"
                    ],
                    "common_phrases": [
                        "you bet",
                        "I bet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть уверенным в том, что что-то является правдой или обязательно произойдет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть уверенным",
                                "target_lang_sense_clarification": "не сомневаться"
                            },
                            {
                                "target_lang_word": "спорить",
                                "target_lang_sense_clarification": "выражать уверенность (готов поспорить)"
                            },
                            {
                                "target_lang_word": "биться об заклад",
                                "target_lang_sense_clarification": "быть абсолютно уверенным (идиома)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "43b746e1-4e3e-3ad0-881f-adb8ee077885",
                    "sense_definition": "An agreement to risk money on the outcome of an event, where the winner takes the loser's stake.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_wager",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She won a <w>bet</w> with her brother about who could run faster.",
                            "target_lang_translations": {
                                "ru": "Она выиграла <w>пари</w> у брата о том, кто быстрее бегает."
                            }
                        },
                        {
                            "text": "They decided to place a <w>bet</w> on the final score of the match.",
                            "target_lang_translations": {
                                "ru": "Они решили сделать <w>ставку</w> на финальный счет матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "wager",
                        "stake",
                        "gamble"
                    ],
                    "common_phrases": [
                        "place a bet",
                        "win a bet",
                        "lose a bet",
                        "make a bet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соглашение рискнуть деньгами на исходе события, где победитель забирает ставку проигравшего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пари",
                                "target_lang_sense_clarification": "спор с условием выигрыша"
                            },
                            {
                                "target_lang_word": "ставка",
                                "target_lang_sense_clarification": "сумма денег в игре"
                            },
                            {
                                "target_lang_word": "заклад",
                                "target_lang_sense_clarification": "предмет спора (устар.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40c36f29-bcbe-38e9-8a4d-99a275e337d0",
                    "sense_definition": "An action, choice, or possibility that is likely to be successful or correct.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is a safe <w>bet</w> that the shops will be closed on Christmas Day.",
                            "target_lang_translations": {
                                "ru": "Можно смело <w>ставить</w> на то, что магазины будут закрыты в Рождество."
                            }
                        },
                        {
                            "text": "Taking the train is your best <w>bet</w> to avoid the city traffic.",
                            "target_lang_translations": {
                                "ru": "Поезд — твой лучший <w>вариант</w>, чтобы избежать городских пробок."
                            }
                        }
                    ],
                    "synonyms": [
                        "choice",
                        "option",
                        "possibility"
                    ],
                    "common_phrases": [
                        "safe bet",
                        "best bet",
                        "good bet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, выбор или возможность, которые, скорее всего, будут успешными или правильными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вариант",
                                "target_lang_sense_clarification": "выбор или возможность"
                            },
                            {
                                "target_lang_word": "выбор",
                                "target_lang_sense_clarification": "предпочтительное решение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "c7f18d0c-2872-3233-8518-cd35c933cafa",
                    "sense_definition": "An informal expression used to indicate agreement, acceptance, or confirmation.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will meet you at the park in ten minutes. — <w>Bet</w>, see you there.",
                            "target_lang_translations": {
                                "ru": "Я встречу тебя в парке через десять минут. — <w>Замётано</w>, увидимся там."
                            }
                        },
                        {
                            "text": "When he asked if I wanted to grab some food, I just said, <w>Bet</w>.",
                            "target_lang_translations": {
                                "ru": "Когда он спросил, хочу ли я перекусить, я просто ответил: «<w>Идёт</w>»."
                            }
                        }
                    ],
                    "synonyms": [
                        "okay",
                        "sure",
                        "alright",
                        "agreed"
                    ],
                    "common_phrases": [
                        "alright bet"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in modern informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное выражение, используемое для обозначения согласия, принятия или подтверждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замётано",
                                "target_lang_sense_clarification": "сленговое выражение согласия"
                            },
                            {
                                "target_lang_word": "идёт",
                                "target_lang_sense_clarification": "выражение согласия"
                            },
                            {
                                "target_lang_word": "договорились",
                                "target_lang_sense_clarification": "подтверждение договоренности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e058330a-db76-3456-98a1-99009d8ec698",
                    "sense_definition": "A short form of the female given name Elizabeth.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "group_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My grandmother <w>Bet</w> tells the most amazing stories.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка <w>Бет</w> рассказывает самые удивительные истории."
                            }
                        },
                        {
                            "text": "We sent a birthday card to Aunt <w>Bet</w> yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера мы отправили поздравительную открытку тете <w>Бет</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращенная форма женского имени Элизабет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бет",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
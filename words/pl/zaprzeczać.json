    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6000d3b2-e059-382d-b37b-47ce5c89325a",
                    "sense_definition": "Stwierdzać, że coś nie jest prawdą lub że dany fakt nie miał miejsca.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verbal_denial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polityk stanowczo <w>zaprzeczał</w> wszystkim oskarżeniom korupcyjnym.",
                            "target_lang_translations": {
                                "ru": "Политик решительно <w>отрицал</w> все обвинения в коррупции."
                            }
                        },
                        {
                            "text": "Nie mogę <w>zaprzeczać</w>, że ta sytuacja mnie zaniepokoiła.",
                            "target_lang_translations": {
                                "ru": "Я не могу <w>отрицать</w>, что эта ситуация меня обеспокоила."
                            }
                        }
                    ],
                    "synonyms": [
                        "negować"
                    ],
                    "antonyms": [
                        "potwierdzać",
                        "afirmować"
                    ],
                    "common_phrases": [
                        "stanowczo zaprzeczać",
                        "zaprzeczać plotkom",
                        "zaprzeczać faktom",
                        "zaprzeczać zarzutom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утверждать, что что-то не является правдой или что данный факт не имел места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрицать",
                                "target_lang_sense_clarification": "заявлять о ложности чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92a56aed-68ba-3d01-bd1d-508bfb54036a",
                    "sense_definition": "Być niezgodnym z innymi informacjami, faktami lub dowodami; stanowić przeciwieństwo czegoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "inconsistency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego zachowanie całkowicie <w>zaprzecza</w> temu, co deklarował wcześniej.",
                            "target_lang_translations": {
                                "ru": "Его поведение полностью <w>противоречит</w> тому, что он заявлял ранее."
                            }
                        },
                        {
                            "text": "Zebrane dowody <w>zaprzeczają</w> twojej teorii na temat wypadku.",
                            "target_lang_translations": {
                                "ru": "Собранные доказательства <w>противоречат</w> твоей теории об аварии."
                            }
                        }
                    ],
                    "antonyms": [
                        "potwierdzać"
                    ],
                    "common_phrases": [
                        "zaprzeczać sobie",
                        "zaprzeczać logice",
                        "zaprzeczać idei"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть несогласованным с другой информацией, фактами или доказательствами; составлять противоположность чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противоречить",
                                "target_lang_sense_clarification": "быть несогласным, идти вразрез"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
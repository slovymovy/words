    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e46939ad-5473-3228-a9f3-8170ac0b014f",
                    "sense_definition": "A young sheep.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer fed the little <w>lamb</w> with a bottle of milk.",
                            "target_lang_translations": {
                                "ru": "Фермер покормил маленького <w>ягнёнка</w> молоком из бутылочки."
                            }
                        },
                        {
                            "text": "We saw a white <w>lamb</w> running in the green field.",
                            "target_lang_translations": {
                                "ru": "Мы видели белого <w>ягнёнка</w>, бегущего по зелёному полю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "little lamb",
                        "spring lamb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Детёныш овцы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ягнёнок",
                                "target_lang_sense_clarification": "детёныш овцы"
                            },
                            {
                                "target_lang_word": "барашек",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43700d80-5f6e-39b6-a03e-3e20bdd4c8be",
                    "sense_definition": "The flesh of a young sheep used as food.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She ordered roast <w>lamb</w> with mint sauce for dinner.",
                            "target_lang_translations": {
                                "ru": "Она заказала на ужин жареную <w>ягнятину</w> с мятным соусом."
                            }
                        },
                        {
                            "text": "My favorite dish at this restaurant is the leg of <w>lamb</w>.",
                            "target_lang_translations": {
                                "ru": "Моё любимое блюдо в этом ресторане — нога <w>ягнёнка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "roast lamb",
                        "leg of lamb",
                        "lamb chop",
                        "rack of lamb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясо молодой овцы, употребляемое в пищу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ягнятина",
                                "target_lang_sense_clarification": "мясо ягнёнка"
                            },
                            {
                                "target_lang_word": "баранина",
                                "target_lang_sense_clarification": "мясо овцы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a143bcc2-f37b-3995-9cf5-3d25d0ab65a6",
                    "sense_definition": "A person who is very gentle, innocent, or mild-mannered.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Despite his intimidating appearance, he is actually as gentle as a <w>lamb</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на свою пугающую внешность, на самом деле он кроток как <w>ягнёнок</w>."
                            }
                        },
                        {
                            "text": "She is such a sweet <w>lamb</w> that everyone loves to help her.",
                            "target_lang_translations": {
                                "ru": "Она такая милая <w>овечка</w>, что все любят ей помогать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gentle as a lamb",
                        "like a lamb to the slaughter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, отличающийся кротостью, невинностью или мягким характером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агнец",
                                "target_lang_sense_clarification": "символ невинности и кротости"
                            },
                            {
                                "target_lang_word": "овечка",
                                "target_lang_sense_clarification": "о кротком человеке"
                            },
                            {
                                "target_lang_word": "ягнёнок",
                                "target_lang_sense_clarification": "метафора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19f7fe82-9f0c-36ac-a201-940faa347f10",
                    "sense_definition": "A devoted fan of the American singer Mariah Carey.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fandom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every true <w>lamb</w> knows all the lyrics to her obscure songs.",
                            "target_lang_translations": {
                                "ru": "Каждый настоящий <w>фанат</w> знает тексты всех её малоизвестных песен."
                            }
                        },
                        {
                            "text": "The singer thanked every <w>lamb</w> in the audience for their support.",
                            "target_lang_translations": {
                                "ru": "Певица поблагодарила каждого <w>поклонника</w> в зале за поддержку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "fandom culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Преданный поклонник американской певицы Мэрайи Кэри."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fa806025-a780-30bd-ae2a-072ec18c6130",
                    "sense_definition": "To give birth to a lamb (used of sheep).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "farming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ewes usually <w>lamb</w> in the early spring when the weather warms up.",
                            "target_lang_translations": {
                                "ru": "Овцематки обычно <w>ягнятся</w> ранней весной, когда погода становится теплее."
                            }
                        },
                        {
                            "text": "Farmers work long hours during the season when the sheep <w>lamb</w>.",
                            "target_lang_translations": {
                                "ru": "Фермеры работают допоздна в сезон, когда овцы <w>ягнятся</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lambing season"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рожать ягнёнка (об овцах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ягниться",
                                "target_lang_sense_clarification": "рожать ягнят"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "0136f59b-35b9-3436-a125-b66d17f82552",
                    "sense_definition": "A title for Jesus Christ, symbolizing sacrifice and innocence.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "The choir sang a beautiful hymn praising the <w>Lamb</w>.",
                            "target_lang_translations": {
                                "ru": "Хор пел прекрасный гимн, восхваляющий <w>Агнца</w>."
                            }
                        },
                        {
                            "text": "In Christian theology, the <w>Lamb</w> represents the ultimate sacrifice for humanity.",
                            "target_lang_translations": {
                                "ru": "В христианском богословии <w>Агнец</w> олицетворяет высшую жертву ради человечества."
                            }
                        }
                    ],
                    "synonyms": [
                        "Lamb of God",
                        "Agnus Dei",
                        "Paschal Lamb"
                    ],
                    "common_phrases": [
                        "blood of the Lamb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул Иисуса Христа, символизирующий жертвенность и невинность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Агнец",
                                "target_lang_sense_clarification": "религиозный титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3270016d-be1c-3b4a-8113-5d7ebfec991d",

                {
                    "sense_id": "224eee2c-9ff9-3186-a4f6-8d4e6c9491c3",
                    "sense_definition": "Een adellijke titel voor een man, in rang tussen jonkheer en burggraaf.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nobility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>baron</w> woonde in een groot kasteel op de heuvel.",
                            "target_lang_translations": {
                                "en": "The <w>baron</w> lived in a large castle on the hill.",
                                "ru": "<w>Барон</w> жил в большом замке на холме."
                            }
                        },
                        {
                            "text": "Haar vader, de <w>baron</w>, organiseerde elk jaar een groot feest.",
                            "target_lang_translations": {
                                "en": "Her father, the <w>baron</w>, organized a big party every year.",
                                "ru": "Её отец, <w>барон</w>, каждый год устраивал большой праздник."
                            }
                        }
                    ],
                    "synonyms": [
                        "edelman"
                    ],
                    "common_phrases": [
                        "de baron en barones",
                        "de baron van"
                    ],
                    "target_lang_definitions": {
                        "en": "A noble title for a man, ranking between jonkheer and viscount.",
                        "ru": "Дворянский титул мужчины, по рангу занимающий место между нетитулованным дворянином и виконтом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "baron",
                                "target_lang_sense_clarification": "noble title"
                            }

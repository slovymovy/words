    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "49971131-cd9d-384c-bd46-9e2df93881c7",
                    "sense_definition": "Sklep zajmujący się sprzedażą artykułów krawieckich i dodatków do odzieży, takich jak nici, guziki, tasiemki czy zamki błyskawiczne.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę pójść do <w>pasmanterii</w>, żeby kupić nici pasujące do mojego płaszcza.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти в <w>магазин швейной фурнитуры</w>, чтобы купить нитки, подходящие к моему пальто."
                            }
                        },
                        {
                            "text": "Ta mała <w>pasmanteria</w> na rogu ulicy ma największy wybór guzików w mieście.",
                            "target_lang_translations": {
                                "ru": "В этой маленькой <w>галантерее</w> на углу улицы самый большой выбор пуговиц в городе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dobrze zaopatrzona pasmanteria",
                        "iść do pasmanterii",
                        "sklep z pasmanterią"
                    ],
                    "target_lang_definitions": {
                        "ru": "Магазин, занимающийся продажей швейных принадлежностей и аксессуаров для одежды, таких как нитки, пуговицы, ленты или молнии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магазин швейной фурнитуры",
                                "target_lang_sense_clarification": "специализированный магазин"
                            },
                            {
                                "target_lang_word": "галантерея",
                                "target_lang_sense_clarification": "магазин мелких товаров"
                            },
                            {
                                "target_lang_word": "магазин рукоделия",
                                "target_lang_sense_clarification": "магазин товаров для хобби и шитья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "809d4ec5-efad-35b2-82f1-2913483e3d0f",
                    "sense_definition": "Wyroby włókiennicze i drobne elementy służące do wykańczania, obszywania lub ozdabiania ubrań i tkanin, np. koronki, frędzle, taśmy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "goods",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ozdobna <w>pasmanteria</w> przy mankietach nadaje tej sukience bardzo elegancki wygląd.",
                            "target_lang_translations": {
                                "ru": "Декоративная <w>отделка</w> на манжетах придает этому платью очень элегантный вид."
                            }
                        },
                        {
                            "text": "W tym pudełku znajduje się stara <w>pasmanteria</w>, którą odziedziczyłam po babci.",
                            "target_lang_translations": {
                                "ru": "В этой коробке лежит старая <w>швейная фурнитура</w>, которую я унаследовала от бабушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "dodatki krawieckie"
                    ],
                    "common_phrases": [
                        "pasmanteria ozdobna",
                        "dział z pasmanterią",
                        "pasmanteria odzieżowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Текстильные изделия и мелкие элементы, служащие для отделки, обшивки или украшения одежды и тканей, например, кружева, кисти, ленты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "швейная фурнитура",
                                "target_lang_sense_clarification": "вспомогательные изделия для шитья"
                            },
                            {
                                "target_lang_word": "басонные изделия",
                                "target_lang_sense_clarification": "плетеные украшения, тесьма, бахрома"
                            },
                            {
                                "target_lang_word": "галантерея",
                                "target_lang_sense_clarification": "мелкие принадлежности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

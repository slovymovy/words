    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "1cc18b58-e87f-3894-8fa2-3478628cca1a",
                    "sense_definition": "Wykrzyknik wyrażający wstręt, obrzydzenie lub silną niechęć, często w reakcji na coś brudnego, brzydkiego lub mającego nieprzyjemny zapach.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "disgust_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Fu</w>, co za nieprzyjemny zapach unosi się w tym pokoju!",
                            "target_lang_translations": {
                                "ru": "<w>Фу</w>, какой неприятный запах стоит в этой комнате!"
                            }
                        },
                        {
                            "text": "<w>Fu</w>, to jedzenie smakuje jakby było zepsute.",
                            "target_lang_translations": {
                                "ru": "<w>Фу</w>, эта еда на вкус как испорченная."
                            }
                        }
                    ],
                    "synonyms": [
                        "fuj",
                        "fe",
                        "pfu",
                        "pfuj"
                    ],
                    "common_phrases": [
                        "fu, ale śmierdzi",
                        "fu, jakie brzydkie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, выражающее отвращение, брезгливость или сильную неприязнь, часто в ответ на что-то грязное, уродливое или имеющее неприятный запах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фу",
                                "target_lang_sense_clarification": "выражение отвращения"
                            },
                            {
                                "target_lang_word": "фи",
                                "target_lang_sense_clarification": "выражение презрения или брезгливости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "412325de-b25e-3115-8322-b7f331a0980d",
                    "sense_definition": "Wykrzyknik wyrażający naganę, dezaprobatę lub zakaz, używany najczęściej w stosunku do dzieci lub zwierząt, aby powstrzymać je przed niewłaściwym zachowaniem.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "reprimand_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Fu</w>, nie wolno gryźć innych dzieci!",
                            "target_lang_translations": {
                                "ru": "<w>Фу</w>, нельзя кусать других детей!"
                            }
                        },
                        {
                            "text": "<w>Fu</w>, zejdź natychmiast ze stołu!",
                            "target_lang_translations": {
                                "ru": "<w>Фу</w>, слезь немедленно со стола!"
                            }
                        }
                    ],
                    "synonyms": [
                        "fe"
                    ],
                    "common_phrases": [
                        "fu, nie wolno",
                        "fu, zostaw to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, выражающее порицание, неодобрение или запрет, чаще всего используемое по отношению к детям или животным, чтобы остановить нежелательное поведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фу",
                                "target_lang_sense_clarification": "команда запрета"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "223050fa-b834-32d5-b67d-3198b3458d27",
                    "sense_definition": "Gatunek literatury chińskiej; poemat opisujący krajobraz, architekturę lub przedmioty, pisany rytmiczną prozą, popularny w starożytnych Chinach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten słynny poeta napisał wspaniałe <w>fu</w> wychwalające cesarskie ogrody.",
                            "target_lang_translations": {
                                "ru": "Этот знаменитый поэт написал великолепное <w>фу</w>, восхваляющее императорские сады."
                            }
                        },
                        {
                            "text": "Na zajęciach z literatury omawialiśmy strukturę i rytmikę starożytnego <w>fu</w>.",
                            "target_lang_translations": {
                                "ru": "На занятиях по литературе мы обсуждали структуру и ритмику древнего <w>фу</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жанр китайской литературы; поэма, описывающая пейзаж, архитектуру или предметы, написанная ритмической прозой, популярная в древнем Китае."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фу",
                                "target_lang_sense_clarification": "жанр китайской литературы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
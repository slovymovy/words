    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "005b271d-311b-38c9-b672-151bc69eb284",
                    "sense_definition": "A specific thought, image, or experience from the past that a person remembers.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cognitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have many happy <w>memories</w> of my childhood in the countryside.",
                            "target_lang_translations": {
                                "ru": "У меня много счастливых <w>воспоминаний</w> о детстве в деревне."
                            }
                        },
                        {
                            "text": "The old photograph brought back a painful <w>memory</w>.",
                            "target_lang_translations": {
                                "ru": "Старая фотография навеяла болезненное <w>воспоминание</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "recollection",
                        "reminiscence"
                    ],
                    "common_phrases": [
                        "happy memory",
                        "childhood memory",
                        "in memory of",
                        "fond memory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная мысль, образ или переживание из прошлого, которые человек помнит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспоминание",
                                "target_lang_sense_clarification": "мысленный образ прошлого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "280c86c0-dd62-32e0-89ad-d039c9f6f4fe",
                    "sense_definition": "The ability of the mind to store and recall information.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cognitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a terrible <w>memory</w> for names and faces.",
                            "target_lang_translations": {
                                "ru": "У неё ужасная <w>память</w> на имена и лица."
                            }
                        },
                        {
                            "text": "He recited the entire poem from <w>memory</w> without looking at the book.",
                            "target_lang_translations": {
                                "ru": "Он продекламировал всё стихотворение по <w>памяти</w>, не заглядывая в книгу."
                            }
                        }
                    ],
                    "synonyms": [
                        "recall",
                        "recollection"
                    ],
                    "common_phrases": [
                        "good memory",
                        "bad memory",
                        "lose memory",
                        "from memory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность разума сохранять и воспроизводить информацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "память",
                                "target_lang_sense_clarification": "способность запоминать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a37fc20-f53f-3a4b-93e6-bc128a6c1e30",
                    "sense_definition": "The part of a computer or electronic device where data and programs are stored.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My computer needs more <w>memory</w> to run this video game smoothly.",
                            "target_lang_translations": {
                                "ru": "Моему компьютеру нужно больше <w>памяти</w>, чтобы эта видеоигра работала плавно."
                            }
                        },
                        {
                            "text": "You should save your photos to an external <w>memory</w> drive.",
                            "target_lang_translations": {
                                "ru": "Тебе следует сохранить фотографии на внешний накопитель <w>памяти</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "core"
                    ],
                    "common_phrases": [
                        "memory stick",
                        "flash memory",
                        "out of memory",
                        "random access memory"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often refers to RAM or storage capacity"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть компьютера или электронного устройства, где хранятся данные и программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "память",
                                "target_lang_sense_clarification": "компьютерная"
                            },
                            {
                                "target_lang_word": "запоминающее устройство",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2596ad09-ab34-3b56-b927-438568c3cde2",
                    "sense_definition": "A card game in which players turn over cards to find matching pairs.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "game",
                    "name_type": "game_or_sport_name",
                    "examples": [
                        {
                            "text": "The children sat on the floor playing a game of <w>Memory</w>.",
                            "target_lang_translations": {
                                "ru": "Дети сидели на полу, играя в <w>Мемори</w>."
                            }
                        },
                        {
                            "text": "He is very good at <w>Memory</w> because he pays close attention to where the cards are.",
                            "target_lang_translations": {
                                "ru": "Он очень хорош в <w>Мемори</w>, потому что внимательно следит за тем, где лежат карты."
                            }
                        }
                    ],
                    "synonyms": [
                        "pelmanism"
                    ],
                    "target_lang_definitions": {
                        "ru": "Карточная игра, в которой игроки переворачивают карты, чтобы найти парные картинки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мемори",
                                "target_lang_sense_clarification": "название игры"
                            },
                            {
                                "target_lang_word": "игра на запоминание",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c06edef4-f437-3e34-9d1c-be081226bca4",
                    "sense_definition": "The length of time that people can remember past events.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cognitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was the coldest winter in living <w>memory</w>.",
                            "target_lang_translations": {
                                "ru": "Это была самая холодная зима на <w>памяти</w> живущих."
                            }
                        },
                        {
                            "text": "The political scandal has already faded from recent <w>memory</w>.",
                            "target_lang_translations": {
                                "ru": "Этот политический скандал уже стёрся из недавней <w>памяти</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "living memory",
                        "recent memory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени, события которого люди способны помнить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "память",
                                "target_lang_sense_clarification": "историческая или коллективная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aca7cec6-2a94-3faf-adb1-cf3715104846",
                    "sense_definition": "The property of a material, such as metal or plastic, to return to its original shape after being deformed.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This new mattress is made of high-quality <w>memory</w> foam.",
                            "target_lang_translations": {
                                "ru": "Этот новый матрас сделан из высококачественной пены с <w>памятью</w> формы."
                            }
                        },
                        {
                            "text": "The eyeglass frames are made of a <w>memory</w> metal that cannot be bent out of shape.",
                            "target_lang_translations": {
                                "ru": "Оправа очков сделана из металла с <w>памятью</w>, который невозможно погнуть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "memory foam",
                        "memory metal"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often used attributively"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство материала, например металла или пластика, возвращаться к первоначальной форме после деформации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "память",
                                "target_lang_sense_clarification": "эффект памяти формы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3e9d274-b45a-30a4-9ca2-1820940de6b1",
                    "sense_definition": "A group of elephants.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The safari guide pointed out a <w>memory</w> of elephants near the river.",
                            "target_lang_translations": {
                                "ru": "Гид на сафари указал на <w>стадо</w> слонов у реки."
                            }
                        },
                        {
                            "text": "A <w>memory</w> of elephants moved slowly across the savannah.",
                            "target_lang_translations": {
                                "ru": "<w>Стадо</w> слонов медленно двигалось по саванне."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "collective noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа слонов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стадо",
                                "target_lang_sense_clarification": "группа животных"
                            }
                        ]
                    }
                }
            ]
        }
    ]

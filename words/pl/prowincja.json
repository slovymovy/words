    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb6a7f92-893f-3622-9f7d-cc4053c08730",
                    "sense_definition": "Obszar kraju położony z dala od stolicy i głównych ośrodków kulturalnych; teren poza dużymi miastami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geography_remote",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi dziadkowie mieszkają na głębokiej <w>prowincji</w>, gdzie życie toczy się wolniej.",
                            "target_lang_translations": {
                                "ru": "Мои дедушка и бабушка живут в глубокой <w>провинции</w>, где жизнь течет медленнее."
                            }
                        },
                        {
                            "text": "Wielu młodych ludzi wyjeżdża z <w>prowincji</w> do Warszawy w poszukiwaniu pracy.",
                            "target_lang_translations": {
                                "ru": "Многие молодые люди уезжают из <w>провинции</w> в Варшаву в поисках работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "peryferie",
                        "teren"
                    ],
                    "antonyms": [
                        "stolica",
                        "centrum",
                        "metropolia"
                    ],
                    "common_phrases": [
                        "głęboka prowincja",
                        "mieszkać na prowincji",
                        "pochodzić z prowincji",
                        "wyjechać na prowincję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Местность, удаленная от столицы и крупных культурных центров; территория за пределами больших городов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провинция",
                                "target_lang_sense_clarification": "местность вдали от центра"
                            },
                            {
                                "target_lang_word": "глубинка",
                                "target_lang_sense_clarification": "отдаленная местность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85449af7-f641-3af9-946e-c97c178eafc3",
                    "sense_definition": "Główna jednostka podziału administracyjnego w niektórych państwach (np. w Kanadzie, Włoszech, Chinach) lub w starożytnym Rzymie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "admin_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Quebec to największa <w>prowincja</w> Kanady pod względem powierzchni.",
                            "target_lang_translations": {
                                "ru": "Квебек — это самая большая <w>провинция</w> Канады по площади."
                            }
                        },
                        {
                            "text": "Galia była jedną z najważniejszych <w>prowincji</w> Cesarstwa Rzymskiego.",
                            "target_lang_translations": {
                                "ru": "Галлия была одной из важнейших <w>провинций</w> Римской империи."
                            }
                        }
                    ],
                    "synonyms": [
                        "region"
                    ],
                    "common_phrases": [
                        "prowincja rzymska",
                        "zarządzać prowincją",
                        "autonomia prowincji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная административно-территориальная единица в некоторых государствах (например, в Канаде, Италии, Китае) или в Древнем Риме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провинция",
                                "target_lang_sense_clarification": "административная единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a73099b8-976a-339b-aeca-b55ebf513d8a",
                    "sense_definition": "Miejsce postrzegane jako zacofane, nudne i pozbawione dostępu do kultury (często w znaczeniu negatywnym).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "geography_remote",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę spędzić całego życia w tej dziurze, to straszna <w>prowincja</w>.",
                            "target_lang_translations": {
                                "ru": "Я не хочу провести всю жизнь в этой дыре, это жуткая <w>провинция</w>."
                            }
                        },
                        {
                            "text": "Ten teatr, mimo małego budżetu, wcale nie trąci <w>prowincją</w>.",
                            "target_lang_translations": {
                                "ru": "Этот театр, несмотря на маленький бюджет, совсем не отдаёт <w>провинцией</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaścianek",
                        "dziura",
                        "zapadły kąt"
                    ],
                    "antonyms": [
                        "wielki świat"
                    ],
                    "common_phrases": [
                        "trącić prowincją",
                        "mentalna prowincja"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, воспринимаемое как отсталое, скучное и лишенное доступа к культуре (часто в негативном смысле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провинция",
                                "target_lang_sense_clarification": "символ отсталости"
                            },
                            {
                                "target_lang_word": "захолустье",
                                "target_lang_sense_clarification": "глухое место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd4afc62-1750-3f99-9d1c-4a500958b2b3",
                    "sense_definition": "Jednostka administracyjna w strukturze Kościoła katolickiego lub zakonu, obejmująca kilka diecezji lub klasztorów.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "admin_church",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zakon dominikanów w Polsce tworzy oddzielną <w>prowincję</w>.",
                            "target_lang_translations": {
                                "ru": "Орден доминиканцев в Польше образует отдельную <w>провинцию</w>."
                            }
                        },
                        {
                            "text": "Decyzję o zamknięciu klasztoru musi zatwierdzić zarząd <w>prowincji</w>.",
                            "target_lang_translations": {
                                "ru": "Решение о закрытии монастыря должно утвердить руководство <w>провинции</w>."
                            }
                        }
                    ],
                    "synonyms": [

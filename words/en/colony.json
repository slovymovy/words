    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "204f3c1b-37dd-35d1-b9ab-0a6d0acc33be",
                    "sense_definition": "An area under the political control of another country and typically occupied by settlers from it.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "political_settlement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The island remained a British <w>colony</w> until it gained independence in the last century.",
                            "target_lang_translations": {
                                "ru": "Остров оставался британской <w>колонией</w>, пока не получил независимость в прошлом веке."
                            }
                        },
                        {
                            "text": "Many European countries established <w>colonies</w> in the Americas during the age of exploration.",
                            "target_lang_translations": {
                                "ru": "Многие европейские страны основали <w>колонии</w> в Северной и Южной Америке в эпоху Великих географических открытий."
                            }
                        }
                    ],
                    "synonyms": [
                        "settlement",
                        "territory",
                        "dependency",
                        "possession"
                    ],
                    "antonyms": [
                        "metropole"
                    ],
                    "common_phrases": [
                        "crown colony",
                        "former colony",
                        "British colony",
                        "penal colony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория, находящаяся под политическим контролем другой страны и обычно заселенная выходцами из нее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колония",
                                "target_lang_sense_clarification": "зависимая территория"
                            },
                            {
                                "target_lang_word": "поселение",
                                "target_lang_sense_clarification": "место жительства переселенцев"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0235155e-d67a-3407-97c8-15ccb0ee2280",
                    "sense_definition": "A group of organisms of the same species living together in close association.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "biological_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We found a large ant <w>colony</w> under the old paving stones in the garden.",
                            "target_lang_translations": {
                                "ru": "Мы нашли большую муравьиную <w>колонию</w> под старой брусчаткой в саду."
                            }
                        },
                        {
                            "text": "The coral <w>colony</w> provides a vital habitat for many small fish.",
                            "target_lang_translations": {
                                "ru": "Коралловая <w>колония</w> служит жизненно важной средой обитания для множества мелких рыб."
                            }
                        }
                    ],
                    "synonyms": [
                        "community",
                        "swarm"
                    ],
                    "common_phrases": [
                        "ant colony",
                        "bee colony",
                        "coral colony",
                        "bird colony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа организмов одного вида, живущих вместе в тесной связи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колония",
                                "target_lang_sense_clarification": "группа организмов"
                            },
                            {
                                "target_lang_word": "семья",
                                "target_lang_sense_clarification": "пчел или муравьев"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ad95adb-6a65-31b2-9f3c-6b04aa7df00c",
                    "sense_definition": "A group of people with similar interests, occupations, or characteristics, living in a particular area.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The painter moved to an artist <w>colony</w> to find inspiration among other creatives.",
                            "target_lang_translations": {
                                "ru": "Художник переехал в <w>колонию</w> живописцев, чтобы найти вдохновение среди других творческих людей."
                            }
                        },
                        {
                            "text": "They visited a nudist <w>colony</w> while on vacation in the south of France.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска на юге Франции они посетили нудистскую <w>колонию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "community",
                        "camp",
                        "settlement"
                    ],
                    "common_phrases": [
                        "artist colony",
                        "nudist colony",
                        "leper colony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей с общими интересами, занятиями или характеристиками, проживающих в определенной местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колония",
                                "target_lang_sense_clarification": "сообщество людей"
                            },
                            {
                                "target_lang_word": "поселение",
                                "target_lang_sense_clarification": "населенный пункт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c9630a5-7e0c-3871-af97-4339beb05dae",
                    "sense_definition": "A visible cluster of bacteria, fungi, or other cells cultured together, typically growing from a single cell.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biological_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The student counted the number of bacterial <w>colonies</w> growing in the petri dish.",
                            "target_lang_translations": {
                                "ru": "Студент подсчитал количество бактериальных <w>колоний</w>, растущих в чашке Петри."
                            }
                        },
                        {
                            "text": "Each <w>colony</w> on the plate represents a distinct strain of the microorganism.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>колония</w> на пластине представляет собой отдельный штамм микроорганизма."
                            }
                        }
                    ],
                    "synonyms": [
                        "culture",
                        "growth"
                    ],
                    "common_phrases": [
                        "bacterial colony",
                        "fungal colony"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "scientific"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Видимое скопление бактерий, грибков или других клеток, выращенных вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колония",
                                "target_lang_sense_clarification": "микроорганизмов"
                            }
                        ]
                    }
                }
            ]
        },

                },
                {
                    "sense_id": "96f50910-62bd-37ee-8b35-8161eaeefb4c",
                    "sense_definition": "Postępować niezgodnie z przyjętymi zasadami, prawem lub obietnicami",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_violation",
                    "name_type": "no",

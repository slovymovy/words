        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07de551b-0375-3ea7-a73e-c7ea17656fed",
                    "sense_definition": "To care for someone in a motherly way, often excessively or intrusively.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stop trying to <w>mom</w> me; I can handle this myself.",
                            "target_lang_translations": {
                                "ru": "Перестань меня <w>опекать</w>; я сам с этим справлюсь."
                            }
                        },
                        {
                            "text": "She tends to <w>mom</w> her friends by constantly checking if they have eaten.",
                            "target_lang_translations": {
                                "ru": "Она склонна <w>опекать</w> своих друзей, постоянно проверяя, поели ли они."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заботиться о ком-то по-матерински, часто чрезмерно или навязчиво."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опекать",
                                "target_lang_sense_clarification": "заботиться, контролировать"
                            },
                            {
                                "target_lang_word": "нянчить",
                                "target_lang_sense_clarification": "обращаться как с ребенком"
                            }
                        ]
                    }
                }

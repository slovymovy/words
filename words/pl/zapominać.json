{
    "word_family": [
        "zapomnienie",
        "zapominalski",
        "niezapomniany",
        "pamięć",
        "pamiętać",
        "przypominać",
        "wspominać",
        "upominać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6c656361-d429-32e0-b753-7223d1fa8be8",
                    "sense_definition": "Przestawać pamiętać o faktach, wydarzeniach lub ludziach; tracić informacje z pamięci.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "memory_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety często <zapominam> imiona nowo poznanych osób.",
                            "target_lang_translations": {
                                "ru": "К сожалению, я часто <забываю> имена новых знакомых."
                            }
                        },
                        {
                            "text": "Mój dziadek powoli <zapomina> wydarzenia z dalekiej przeszłości.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка постепенно <забывает> события из далекого прошлого."
                            }
                        }
                    ],
                    "antonyms": [
                        "przypominać (sobie)",
                        "zapamiętywać",
                        "przypominać (sobie)"
                    ],
                    "common_phrases": [
                        "zapominać o wszystkim",
                        "zapominać słów",
                        "zapominać nazwiska",
                        "szybko zapominać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать помнить факты, события или людей; терять информацию из памяти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забывать",
                                "target_lang_sense_clarification": "о фактах/людях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "264c9e74-b170-303b-910a-36475b3b97aa",
                    "sense_definition": "Nie wykonać jakiejś czynności, ponieważ wyleciała ona z głowy; zaniedbać coś przez roztargnienie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "neglect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze <zapominam> kupić mleko, gdy wracam z pracy do domu.",
                            "target_lang_translations": {
                                "ru": "Я всегда <забываю> купить молоко, когда возвращаюсь с работы домой."
                            }
                        },
                        {
                            "text": "Proszę, nie <zapominaj> o zgaszeniu światła przed wyjściem.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <забывай> выключить свет перед уходом."
                            }
                        }
                    ],
                    "antonyms": [
                        "przypominać (sobie)"
                    ],
                    "common_phrases": [
                        "zapominać o obowiązkach",
                        "zapominać języka w gębie",
                        "zapominać o bożym świecie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не сделать что-либо, потому что это вылетело из головы; упустить что-то из-за рассеянности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забывать",
                                "target_lang_sense_clarification": "не сделать что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44445fca-0e15-3ef2-98a9-d94a57c50eb7",
                    "sense_definition": "Zostawić przedmiot w innym miejscu niż zamierzone z powodu roztargnienia; nie wziąć czegoś ze sobą.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "leave_behind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj w pośpiechu <zapomniałem> telefonu z biura i musiałem wracać.",
                            "target_lang_translations": {
                                "ru": "Вчера в спешке я <забыл> телефон в офисе, и мне пришлось возвращаться."
                            }
                        },
                        {
                            "text": "Uczniowie często <zapominają> zeszytów na zajęcia matematyki.",
                            "target_lang_translations": {
                                "ru": "Ученики часто <забывают> тетради на уроки математики."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zapominać kluczy",
                        "zapominać portfela",
                        "zapominać dokumentów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставить предмет не там, где нужно, по рассеянности; не взять что-то с собой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забывать",
                                "target_lang_sense_clarification": "оставлять вещь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12be1372-ae8e-3e15-9852-4f00454de9fe",
                    "sense_definition": "Przestawać chować urazę; wybaczać komuś wyrządzone krzywdy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "forgive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powinniśmy <zapomnieć> o dawnych sporach i zacząć naszą relację od nowa.",
                            "target_lang_translations": {
                                "ru": "Мы должны <забыть> о старых спорах и начать наши отношения заново."
                            }
                        },
                        {
                            "text": "Trudno jest <zapomnieć> krzywdy, które wyrządzili nam bliscy ludzie.",
                            "target_lang_translations": {
                                "ru": "Трудно <забыть> обиды, которые причинили нам близкие люди."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zapominać o urazach",
                        "zapominać winy",
                        "zapomnieć i wybaczyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать держать обиду; прощать кому-то причиненное зло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забывать",
                                "target_lang_sense_clarification": "прощать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0ac32a8-6fdc-3e52-845d-096aa67e0e60",
                    "sense_definition": "Tracić panowanie nad sobą, zachowywać się niewłaściwie lub przekraczać ustalone granice (używane z zaimkiem \"się\").",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reflexive_misbehave",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas kłótni on często <zapomina się> i mówi bardzo przykre słowa.",
                            "target_lang_translations": {
                                "ru": "Во время ссоры он часто <забывается> и говорит очень обидные слова."
                            }
                        },
                        {
                            "text": "Nauczyciel upomniał ucznia, który zaczął się <zapominać> w stosunku do kolegów.",
                            "target_lang_translations": {
                                "ru": "Учитель сделал замечание ученику, который начал <забываться> по отношению к одноклассникам."
                            }
                        }
                    ],
                    "synonyms": [
                        "zagalopowywać się (imperfective)",
                        "zagalopować się",
                        "posuwać się za daleko (imperfective)",
                        "posunąć się za daleko",
                        "przebierać miarę (imperfective)",
                        "przebrać miarę"
                    ],
                    "common_phrases": [
                        "zapominać się w gniewie",
                        "zapominać się w dyskusji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Терять самообладание, вести себя неподобающим образом или переходить границы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забываться",
                                "target_lang_sense_clarification": "вести себя неподобающе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
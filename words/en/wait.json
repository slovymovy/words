                        "ru": [
                            {
                                "target_lang_word": "прислуживать",
                                "target_lang_sense_clarification": "устаревшее или формальное"
                            },
                            {
                                "target_lang_word": "обслуживать",
                                "target_lang_sense_clarification": "оказывать услуги"
                            }
                        ]

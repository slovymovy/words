    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1a169f53-12d2-3a34-8195-e5dc1a746135",
                    "sense_definition": "Część przedmiotu służąca do chwytania go ręką",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "part_for_holding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten garnek ma bardzo gorący <w>uchwyt</w>, więc uważaj.",
                            "target_lang_translations": {
                                "ru": "У этой кастрюли очень горячая <w>ручка</w>, так что будь осторожен."
                            }
                        },
                        {
                            "text": "Muszę dokręcić poluzowany <w>uchwyt</w> przy szafce kuchennej.",
                            "target_lang_translations": {
                                "ru": "Мне нужно подкрутить разболтавшуюся <w>ручку</w> на кухонном шкафчике."
                            }
                        }
                    ],
                    "synonyms": [
                        "rączka",
                        "rękojeść",
                        "trzonek"
                    ],
                    "common_phrases": [
                        "wygodny uchwyt",
                        "uchwyt meblowy",
                        "uchwyt do drzwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть предмета, предназначенная для того, чтобы держать его рукой"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ручка",
                                "target_lang_sense_clarification": "самый распространенный перевод: двери, сумки, кастрюли"
                            },
                            {
                                "target_lang_word": "рукоятка",
                                "target_lang_sense_clarification": "инструмента, оружия или прибора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a8132d8-6e70-3632-b897-8b5fa1faf356",
                    "sense_definition": "Element służący do przymocowania, zawieszenia lub przytrzymywania czegoś w określonym miejscu",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "part_for_mounting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem nowy <w>uchwyt</w> na telefon do samochodu.",
                            "target_lang_translations": {
                                "ru": "Я купил новый <w>держатель</w> для телефона в машину."
                            }
                        },
                        {
                            "text": "Telewizor wisi na ścianie dzięki specjalnemu stalowemu <w>uchwytowi</w>.",
                            "target_lang_translations": {
                                "ru": "Телевизор висит на стене благодаря специальному стальному <w>кронштейну</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mocowanie",
                        "zaczep",
                        "przymocowanie",
                        "przytwierdzenie"
                    ],
                    "common_phrases": [
                        "uchwyt samochodowy",
                        "uchwyt ścienny",
                        "uchwyt rowerowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приспособление для крепления, подвешивания или фиксации чего-либо в определенном месте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "держатель",
                                "target_lang_sense_clarification": "для телефона, стакана и т.д."
                            },
                            {
                                "target_lang_word": "кронштейн",
                                "target_lang_sense_clarification": "опорная конструкция (для телевизора, полки)"
                            },
                            {
                                "target_lang_word": "крепление",
                                "target_lang_sense_clarification": "механизм фиксации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f5ea6f7-120a-34d6-9453-a724989f1414",
                    "sense_definition": "Czynność chwytania lub sposób trzymania kogoś lub czegoś",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "act_of_holding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego silny <w>uchwyt</w> sprawił, że nie mogłem się wyrwać.",
                            "target_lang_translations": {
                                "ru": "Его сильная <w>хватка</w> не дала мне вырваться."
                            }
                        },
                        {
                            "text": "Wspinaczka górska wymaga, aby każdy <w>uchwyt</w> był precyzyjny i pewny.",
                            "target_lang_translations": {
                                "ru": "Скалолазание требует, чтобы каждый <w>захват</w> был точным и надежным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pewny uchwyt",
                        "silny uchwyt",
                        "żelazny uchwyt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие хватания или способ удержания кого-либо или чего-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хватка",
                                "target_lang_sense_clarification": "сила или манера держать"
                            },
                            {
                                "target_lang_word": "захват",
                                "target_lang_sense_clarification": "действие или прием в борьбе"
                            },
                            {
                                "target_lang_word": "хват",
                                "target_lang_sense_clarification": "способ расположения рук (в спорте)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

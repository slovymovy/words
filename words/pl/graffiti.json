                },
                {
                    "sense_id": "541d2fc8-c371-3171-bc7a-a942a0b8a3fc",
                    "sense_definition": "Technika artystyczna polegająca na tworzeniu kompozycji malarskich lub napisów na ścianach i murach, zazwyczaj przy użyciu farb w sprayu",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "modern_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młodzi artyści poznawali tajniki <w>graffiti</w> podczas warsztatów w domu kultury."
                        },
                        {
                            "text": "Ten znany twórca łączy klasyczne malarstwo olejne z nowoczesnym <w>graffiti</w>."
                        }
                    ],
                    "common_phrases": [
                        "sztuka graffiti",
                        "warsztaty graffiti",
                        "styl graffiti"
                    ]
                },
                {
                    "sense_id": "4a066573-9152-3847-91dc-f62461e4e4a5",
                    "sense_definition": "Starożytny napis lub rysunek wyryty na murze, naczyniu lub innym trwałym podłożu, będący przedmiotem badań historycznych",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_inscription",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W podziemiu starożytnej budowli archeolodzy odkryli <w>graffiti</w> z III wieku."
                        },
                        {
                            "text": "Rzymskie <w>graffiti</w> dostarcza badaczom cennych informacji o życiu codziennym ówczesnych ludzi."
                        }
                    ],
                    "common_phrases": [
                        "starożytne graffiti",
                        "odkryć graffiti"
                    ]
                }
            ]
        }

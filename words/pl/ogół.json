                {
                    "sense_id": "37ba7828-24b7-364e-b28e-35b73bcd9d33",
                    "sense_definition": "Całość, zbiór wszystkich elementów, zjawisk lub przedmiotów rozpatrywanych łącznie; przeciwieństwo szczegółu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "totality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na <w>ogół</w> wstaję bardzo wcześnie, ale w niedzielę śpię dłużej.",
                            "target_lang_translations": {
                                "ru": "В <w>целом</w> я встаю очень рано, но в воскресенье сплю дольше."
                            }
                        },
                        {
                            "text": "Nie zgadzam się z tobą, to w <w>ogóle</w> nie jest prawda.",
                            "target_lang_translations": {
                                "ru": "Я не согласен с тобой, это <w>вообще</w> неправда."
                            }
                        },
                        {
                            "text": "Przeprowadzone badania dotyczą <w>ogółu</w> zjawisk atmosferycznych w tym regionie.",
                            "target_lang_translations": {
                                "ru": "Проведенные исследования касаются <w>совокупности</w> атмосферных явлений в этом регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "całość",
                        "suma"
                    ],
                    "antonyms": [
                        "szczegół",
                        "część"
                    ],
                    "common_phrases": [
                        "w ogóle",
                        "na ogół",
                        "z ogółu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность, множество всех элементов, явлений или предметов, рассматриваемых как единое целое; противоположность частности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совокупность",
                                "target_lang_sense_clarification": "сочетание, соединение, общий итог"
                            },
                            {
                                "target_lang_word": "общее",
                                "target_lang_sense_clarification": "то, что касается всего в целом, главное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e8453ff-0e33-3c61-b03d-96d2e43ccac8",
                    "sense_definition": "Wszyscy ludzie tworzący jakąś grupę, społeczność lub naród.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "community",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie decyzje rządu powinny mieć na uwadze dobro <w>ogółu</w>.",
                            "target_lang_translations": {
                                "ru": "Все решения правительства должны учитывать благо <w>общества</w>."
                            }
                        },
                        {
                            "text": "Wyniki wyborów prezydenckich zostały udostępnione <w>ogółowi</w> społeczeństwa.",
                            "target_lang_translations": {
                                "ru": "Результаты президентских выборов были предоставлены <w>широкой общественности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "społeczeństwo",
                        "społeczność",
                        "ludność"
                    ],
                    "antonyms": [
                        "jednostka"
                    ],
                    "common_phrases": [
                        "dobro ogółu",
                        "interes ogółu",
                        "ogół obywateli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Все люди, составляющие какую-либо группу, сообщество или народ; общество в целом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общественность",
                                "target_lang_sense_clarification": "передовая или активная часть общества"
                            },
                            {
                                "target_lang_word": "общество",
                                "target_lang_sense_clarification": "круг людей, объединенных общностью положения или интересов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

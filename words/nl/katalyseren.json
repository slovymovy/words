    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "83f43001-9925-3a64-9eab-69977a381d6e",
                    "sense_definition": "Het sneller laten verlopen van een chemische reactie door middel van een hulpstof (katalysator) die zelf niet verbruikt wordt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bepaalde enzymen in ons lichaam <w>katalyseren</w> de spijsvertering.",
                            "target_lang_translations": {
                                "en": "Certain enzymes in our body <w>catalyze</w> digestion.",
                                "ru": "Определенные ферменты в нашем организме <w>катализируют</w> пищеварение."
                            }
                        },
                        {
                            "text": "In de industrie worden metalen vaak gebruikt om een reactie te <w>katalyseren</w>.",
                            "target_lang_translations": {
                                "en": "In industry, metals are often used to <w>catalyze</w> a reaction.",
                                "ru": "В промышленности металлы часто используются для того, чтобы <w>катализировать</w> реакцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "versnellen"
                    ],
                    "antonyms": [
                        "vertragen",
                        "remmen"
                    ],
                    "common_phrases": [
                        "een reactie katalyseren",
                        "chemisch katalyseren"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a chemical reaction to proceed faster using an auxiliary substance (catalyst) that is not consumed itself.",
                        "ru": "Ускорять химическую реакцию с помощью вещества (катализатора), которое само не расходуется в процессе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catalyze",
                                "target_lang_sense_clarification": "chemistry (US spelling)"
                            },
                            {
                                "target_lang_word": "catalyse",
                                "target_lang_sense_clarification": "chemistry (UK spelling)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "катализировать",
                                "target_lang_sense_clarification": "химическую реакцию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa9524c7-aa91-32f0-a3b5-dc311f76ae04",
                    "sense_definition": "Een proces, ontwikkeling of gebeurtenis versnellen of bespoedigen (figuurlijk gebruik).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "acceleration_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze nieuwe wetgeving zal de energietransitie hopelijk <w>katalyseren</w>.",
                            "target_lang_translations": {
                                "en": "This new legislation will hopefully <w>catalyze</w> the energy transition.",
                                "ru": "Будем надеяться, что это новое законодательство <w>катализирует</w> энергетический переход."
                            }
                        },
                        {
                            "text": "Het conflict kan onbedoeld radicale veranderingen in de regio <w>katalyseren</w>.",
                            "target_lang_translations": {
                                "en": "The conflict could unintentionally <w>catalyze</w> radical changes in the region.",
                                "ru": "Конфликт может непреднамеренно <w>катализировать</w> радикальные изменения в регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "bespoedigen",
                        "aanjagen",
                        "bevorderen"
                    ],
                    "antonyms": [
                        "afremmen",
                        "belemmeren"
                    ],
                    "common_phrases": [
                        "verandering katalyseren",
                        "innovatie katalyseren",
                        "een proces katalyseren"
                    ],
                    "target_lang_definitions": {
                        "en": "To accelerate or hasten a process, development, or event (figurative use).",
                        "ru": "Ускорять процесс, развитие или событие; служить толчком к чему-либо (в переносном смысле)."
                    },

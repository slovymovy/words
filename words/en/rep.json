                },
                {
                    "sense_id": "e43f6c2f-c94b-3bc4-9943-3b007597a3a0",
                    "sense_definition": "The general opinion that people have about someone or something; a short form of <w>reputation</w>.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_reputation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That restaurant has a bad <w>rep</w> for slow service.",
                            "target_lang_translations": {
                                "ru": "У этого ресторана дурная <w>репутация</w> из-за медленного обслуживания."
                            }

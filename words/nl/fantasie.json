    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bf6343f4-8001-35a1-8c60-aa978832d910",
                    "sense_definition": "Het vermogen om nieuwe gedachten, beelden of ideeën te vormen die niet direct worden waargenomen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "imagination_faculty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die jongen had een geweldige <w>fantasie</w> en verzon de mooiste verhalen.",
                            "target_lang_translations": {
                                "ru": "У этого мальчика была потрясающая <w>фантазия</w>, и он придумывал прекрасные истории.",
                                "en": "That boy had a great <w>imagination</w> and made up the most beautiful stories."
                            }
                        },
                        {
                            "text": "Je hebt een rijke <w>fantasie</w> nodig om een goed boek te kunnen schrijven.",
                            "target_lang_translations": {
                                "ru": "Нужна богатая <w>фантазия</w>, чтобы написать хорошую книгу.",
                                "en": "You need a rich <w>imagination</w> to be able to write a good book."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbeeldingskracht",
                        "inbeeldingsvermogen"
                    ],
                    "antonyms": [
                        "realiteitszin"
                    ],
                    "common_phrases": [
                        "rijke fantasie",
                        "levendige fantasie",
                        "fantasie gebruiken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность формировать новые мысли, образы или идеи, которые не воспринимаются непосредственно.",
                        "en": "The ability to form new thoughts, images, or ideas that are not directly perceived."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воображение",
                                "target_lang_sense_clarification": "способность сознания создавать образы"
                            },
                            {
                                "target_lang_word": "фантазия",
                                "target_lang_sense_clarification": "творческое воображение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "imagination",
                                "target_lang_sense_clarification": "the ability to create mental images"
                            },
                            {
                                "target_lang_word": "fantasy",
                                "target_lang_sense_clarification": "creative imagination"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb5937d2-9630-38b1-ab0f-6401d1ee5ad6",
                    "sense_definition": "Iets wat verzonnen is en niet op de werkelijkheid berust; een droombeeld of illusie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "imagination_product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In haar <w>fantasie</w> was ze een beroemde zangeres die overal ter wereld optrad.",
                            "target_lang_translations": {
                                "ru": "В своей <w>фантазии</w> она была известной певицей, выступающей по всему миру.",
                                "en": "In her <w>fantasy</w>, she was a famous singer performing all over the world."
                            }
                        },
                        {
                            "text": "Het verhaal over het monster was pure <w>fantasie</w>.",
                            "target_lang_translations": {
                                "ru": "История о монстре была чистой <w>фантазией</w>.",
                                "en": "The story about the monster was pure <w>fantasy</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verzinsel",
                        "droombeeld",
                        "illusie",
                        "hersenspinsel"
                    ],
                    "antonyms": [
                        "werkelijkheid",
                        "realiteit",
                        "feit"
                    ],
                    "common_phrases": [
                        "in mijn fantasie",
                        "pure fantasie",
                        "seksuele fantasie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то выдуманное, не основанное на реальности; мечта или иллюзия.",
                        "en": "Something invented that is not based on reality; a dream image or illusion."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фантазия",
                                "target_lang_sense_clarification": "плод воображения, мечта"
                            },
                            {
                                "target_lang_word": "выдумка",
                                "target_lang_sense_clarification": "то, что не соответствует действительности"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fantasy",
                                "target_lang_sense_clarification": "something imagined or invented"
                            },
                            {
                                "target_lang_word": "illusion",
                                "target_lang_sense_clarification": "a false idea or belief"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
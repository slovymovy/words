{
    "word_family": [
        "zadanie",
        "zadawać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "15458591-ffe7-3931-85d4-a85435e7d773",
                    "sense_definition": "Skierować do kogoś pytanie w celu uzyskania informacji lub odpowiedzi.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym <zadać> ci jedno bardzo ważne pytanie.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>задать</w> тебе один очень важный вопрос."
                            }
                        },
                        {
                            "text": "Uczeń podniósł rękę, aby <zadać> pytanie nauczycielowi.",
                            "target_lang_translations": {
                                "ru": "Ученик поднял руку, чтобы <w>задать</w> вопрос учителю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zadać pytanie",
                        "zadać zagadkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обратиться к кому-либо с вопросом для получения информации или ответа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задать",
                                "target_lang_sense_clarification": "вопрос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed1c2ee5-ea0e-3780-a4d7-7d7047d87cac",
                    "sense_definition": "Wyznaczyć komuś pracę, obowiązek lub ćwiczenia do wykonania.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczycielka matematyki <zadała> nam dużo pracy domowej na weekend.",
                            "target_lang_translations": {
                                "ru": "Учительница математики <w>задала</w> нам много домашней работы на выходные."
                            }
                        },
                        {
                            "text": "Szef <zadał> mi przygotowanie raportu na jutrzejsze spotkanie.",
                            "target_lang_translations": {
                                "ru": "Начальник <w>поручил</w> мне подготовить отчет к завтрашней встрече."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zadać pracę domową",
                        "zadać lekcje",
                        "zadać temat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Назначить кому-либо работу, обязанность или упражнения для выполнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задать",
                                "target_lang_sense_clarification": "урок, домашнее задание"
                            },
                            {
                                "target_lang_word": "поручить",
                                "target_lang_sense_clarification": "дать задание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80665328-8489-32e5-bc89-46bf0db10e09",
                    "sense_definition": "Włożyć coś ciężkiego na plecy lub ramiona; umieścić ładunek na sobie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "regional_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik <zadał> sobie ciężki worek mąki na plecy.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>взвалил</w> себе на спину тяжелый мешок муки."
                            }
                        },
                        {
                            "text": "Musisz mi pomóc <zadać> ten tobół, bo sam nie dam rady.",
                            "target_lang_translations": {
                                "ru": "Ты должен помочь мне <w>закинуть</w> этот узел, потому что сам я не справлюсь."
                            }
                        }
                    ],
                    "synonyms": [
                        "zarzucić",
                        "podać"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "użycie gwarowe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Взвалить что-то тяжелое на спину или плечи; поместить груз на себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взвалить",
                                "target_lang_sense_clarification": "груз на спину"
                            },
                            {
                                "target_lang_word": "закинуть",
                                "target_lang_sense_clarification": "на плечи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f07f926-856a-3094-afb4-d34633cb6a61",
                    "sense_definition": "Stwarzać pozory, udawać kogoś lub coś.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "regional_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna tylko <zadał>, że jest chory, aby uniknąć pracy.",
                            "target_lang_translations": {
                                "ru": "Мужчина только <w>сделал вид</w>, что болен, чтобы избежать работы."
                            }
                        },
                        {
                            "text": "<Zadał> się, że nic nie słyszy i poszedł dalej.",
                            "target_lang_translations": {
                                "ru": "Он <w>притворился</w>, что ничего не слышит, и пошел дальше."
                            }
                        }
                    ],
                    "synonyms": [
                        "udać"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "użycie gwarowe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать видимость, притворяться кем-то или чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "притвориться",
                                "target_lang_sense_clarification": "сделать вид"
                            },
                            {
                                "target_lang_word": "сделать вид",
                                "target_lang_sense_clarification": "симулировать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6ef73e4c-7fb0-3683-a98d-9fcfaff1b956",
                    "sense_definition": "Zmienić swoje ubranie na inne, na przykład po powrocie do domu lub przed wyjściem (używane z zaimkiem „się”)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "change_clothes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę się szybko <w>przebrać</w>, bo zmokłem na deszczu.",
                            "target_lang_translations": {
                                "ru": "Я должен быстро <w>переодеться</w>, потому что промок под дождем."
                            }
                        },
                        {
                            "text": "Po powrocie z pracy ojciec od razu <w>przebrał się</w> w wygodny dres.",
                            "target_lang_translations": {
                                "ru": "Вернувшись с работы, отец сразу <w>переоделся</w> в удобный спортивный костюм."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmienić ubranie"
                    ],
                    "common_phrases": [
                        "przebrać się w dres",
                        "przebrać się do wyjścia",
                        "przebrać się po pracy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сменить свою одежду на другую, например, вернувшись домой или перед выходом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переодеться",
                                "target_lang_sense_clarification": "сменить одежду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf0c5d20-5516-3381-ab96-0a097c781cb4",
                    "sense_definition": "Założyć kostium lub nietypowy strój, aby upodobnić się do kogoś innego (używane z zaimkiem „się”)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "costume",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na bal karnawałowy Ania <w>przebrała się</w> za księżniczkę.",
                            "target_lang_translations": {
                                "ru": "На карнавальный бал Аня <w>переоделась</w> в принцессу."
                            }
                        },
                        {
                            "text": "Dzieci uwielbiają się <w>przebierać</w> i udawać superbohaterów.",
                            "target_lang_translations": {
                                "ru": "Дети обожают <w>переодеваться</w> и изображать супергероев."
                            }
                        }
                    ],
                    "synonyms": [
                        "ucharakteryzować się"
                    ],
                    "common_phrases": [
                        "przebrać się za ducha",
                        "przebrać się na bal",
                        "przebrać się za kogoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надеть костюм или маскарадный наряд, чтобы изобразить кого-то другого"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переодеться",
                                "target_lang_sense_clarification": "в костюм, замаскироваться"

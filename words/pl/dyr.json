                    "sense_id": "f3b89b75-279a-3f09-bf2b-4417b38a39f9",
                    "sense_definition": "Potoczne, skrótowe określenie dyrektora, używane najczęściej przez uczniów w odniesieniu do dyrektora szkoły.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "school_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Matematyk krzyczał, że wszystkim wstawi pały, a potem wysłał nas do <w>dyra</w>.",
                            "target_lang_translations": {
                                "ru": "Математик кричал, что всем поставит двойки, а потом отправил нас к <w>диру</w>."
                            }
                        },
                        {
                            "text": "Nasz nowy <w>dyr</w> wprowadził zakaz używania telefonów na przerwach.",
                            "target_lang_translations": {
                                "ru": "Наш новый <w>дир</w> ввел запрет на использование телефонов на переменах."
                            }
                        }
                    ],
                    "synonyms": [
                        "dyrektor",
                        "dyro"
                    ],
                    "common_phrases": [
                        "iść do dyra",
                        "gabinet dyra",
                        "wezwanie do dyra"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "szkolne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, сокращенное название директора, чаще всего используемое учениками по отношению к директору школы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дир",
                                "target_lang_sense_clarification": "школьный сленг"
                            },
                            {
                                "target_lang_word": "директор",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "71a92d76-a084-38b5-9e3a-c8a92dc76462",
                    "sense_definition": "Wyraz dźwiękonaśladowczy imitujący odgłos szybkiego biegu lub nagłej ucieczki.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "onomatopoeia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zając wyskoczył z nory i <w>dyr</w> w gęste zarośla.",
                            "target_lang_translations": {
                                "ru": "Заяц выскочил из норы и <w>шмыг</w> в густые заросли."
                            }
                        },
                        {
                            "text": "Złodziej chwycił torebkę i <w>dyr</w> za róg ulicy.",
                            "target_lang_translations": {
                                "ru": "Вор схватил сумочку и <w>шмыг</w> за угол улицы."
                            }
                        }
                    ],

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e3e0cd50-18da-3611-9784-26319e6abed3",
                    "sense_definition": "Comparative form of <w>long</w>: having greater length or duration.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "comp_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My hair is much <w>longer</w> than it was last year.",
                            "target_lang_translations": {
                                "ru": "Мои волосы намного <w>длиннее</w>, чем были в прошлом году."
                            }
                        },
                        {
                            "text": "The winter nights seem <w>longer</w> when you are alone.",
                            "target_lang_translations": {
                                "ru": "Зимние ночи кажутся <w>длиннее</w>, когда ты одинок."
                            }
                        }
                    ],
                    "synonyms": [
                        "lengthier",
                        "extended"
                    ],
                    "antonyms": [
                        "shorter"
                    ],
                    "common_phrases": [
                        "much longer",
                        "longer than"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "comparative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сравнительная степень прилагательного \"long\": имеющий большую длину или продолжительность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "длиннее",
                                "target_lang_sense_clarification": "о физической длине или расстоянии"
                            },
                            {
                                "target_lang_word": "дольше",
                                "target_lang_sense_clarification": "о продолжительности времени"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "94ff0757-3639-3190-a285-71c7337c24ac",
                    "sense_definition": "Comparative form of <w>long</w>: for a greater amount of time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "comp_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot wait any <w>longer</w> for the package to arrive.",
                            "target_lang_translations": {
                                "ru": "Я не могу <w>больше</w> ждать прибытия посылки."
                            }
                        },
                        {
                            "text": "He no <w>longer</w> works at this company.",
                            "target_lang_translations": {
                                "ru": "Он <w>больше</w> не работает в этой компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "more"
                    ],
                    "antonyms": [
                        "less"
                    ],
                    "common_phrases": [
                        "no longer",
                        "any longer",
                        "a little longer"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "comparative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сравнительная степень наречия \"long\": в течение большего количества времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дольше",
                                "target_lang_sense_clarification": "наречие времени"
                            },
                            {
                                "target_lang_word": "больше",
                                "target_lang_sense_clarification": "в отрицательных конструкциях (напр. no longer)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "56686017-4ac7-3dcc-a29d-cba0b23636f5",
                    "sense_definition": "One who longs or yearns for something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He remained a silent <w>longer</w> for peace throughout the conflict.",
                            "target_lang_translations": {
                                "ru": "Он оставался молчаливым <w>жаждущим</w> мира на протяжении всего конфликта."
                            }
                        },
                        {
                            "text": "She is a <w>longer</w> for the past, unable to accept the changes of the present.",
                            "target_lang_translations": {
                                "ru": "Она — <w>тоскует</w> по прошлому, неспособная принять перемены настоящего."
                            }
                        }
                    ],
                    "synonyms": [
                        "yearner"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rare usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто страстно желает чего-либо или тоскует по чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жаждущий",
                                "target_lang_sense_clarification": "субстантивированное причастие"
                            },
                            {
                                "target_lang_word": "тоскующий",
                                "target_lang_sense_clarification": "человек, испытывающий тоску"
                            }
                        ]
                    }
                }
            ]
        }
    ]

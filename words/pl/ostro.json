                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пронзительно",
                                "target_lang_sense_clarification": "о ветре или холоде"
                            },
                            {
                                "target_lang_word": "ярко",
                                "target_lang_sense_clarification": "о солнце"
                            },
                            {
                                "target_lang_word": "сильно",
                                "target_lang_sense_clarification": "об интенсивности погоды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca05ce19-c578-3877-a965-536e5ca681fb",
                    "sense_definition": "W sposób umożliwiający łatwe przecinanie lub kłucie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nóż został naostrzony tak <w>ostro</w>, że tnie papier w powietrzu.",
                            "target_lang_translations": {
                                "ru": "Этот нож был заточен так <w>остро</w>, что режет бумагу в воздухе."
                            }
                        },
                        {
                            "text": "Uważaj na ten kawałek metalu, bo jest zakończony bardzo <w>ostro</w>.",
                            "target_lang_translations": {
                                "ru": "Осторожно с этим куском металла, потому что он заканчивается очень <w>остро</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "tępo"
                    ],
                    "common_phrases": [
                        "zakończony ostro",
                        "naostrzyć ostro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Так, что можно легко порезаться или уколоться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остро",
                                "target_lang_sense_clarification": "о заточке лезвия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16308628-31fa-3848-9c31-62c3b0c260ae",
                    "sense_definition": "Pod dużym kątem nachylenia; nagle (o zmianie kierunku).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Droga skręca tu <w>ostro</w> w prawo, więc zwolnij.",
                            "target_lang_translations": {
                                "ru": "Дорога здесь <w>резко</w> поворачивает направо, так что сбавь скорость."
                            }
                        },
                        {
                            "text": "Ścieżka pnie się <w>ostro</w> w górę aż na sam szczyt.",
                            "target_lang_translations": {
                                "ru": "Тропинка <w>круто</w> поднимается вверх до самой вершины."
                            }
                        }
                    ],
                    "synonyms": [
                        "stromo"
                    ],
                    "common_phrases": [
                        "skręcać ostro",
                        "ostro w górę",
                        "ostro w dół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Под большим углом наклона; внезапно (о смене направления)."

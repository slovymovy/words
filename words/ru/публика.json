    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f6e8895e-f756-3b4f-b175-9acbbd4eca1a",
                    "sense_definition": "Люди, находящиеся где-либо в качестве зрителей или слушателей (в театре, на концерте, лекции и т. п.).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "audience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Актёр вышел на сцену, и <w>публика</w> громко зааплодировала.",
                            "target_lang_translations": {
                                "en": "The actor came out onto the stage, and the <w>audience</w> applauded loudly."
                            }
                        },
                        {
                            "text": "Эта новая книга рассчитана на самую широкую <w>публику</w>.",
                            "target_lang_translations": {
                                "en": "This new book is intended for the widest possible <w>public</w>."
                            }
                        },
                        {
                            "text": "В зале собралась изысканная <w>публика</w>, ожидающая начала концерта.",
                            "target_lang_translations": {
                                "en": "A refined <w>audience</w> gathered in the hall, waiting for the concert to start."
                            }
                        }
                    ],
                    "synonyms": [
                        "зрители",
                        "слушатели",
                        "аудитория"
                    ],
                    "common_phrases": [
                        "широкая публика",
                        "любимец публики",
                        "играть на публику",
                        "почтенная публика",
                        "выступать на публике"
                    ],
                    "target_lang_definitions": {
                        "en": "People present as spectators or listeners (at a theater, concert, lecture, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "audience",
                                "target_lang_sense_clarification": "spectators or listeners at an event"
                            },
                            {
                                "target_lang_word": "public",
                                "target_lang_sense_clarification": "general body of people or readership"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53e3725c-b7c8-3acf-aaaa-81809f3852a2",
                    "sense_definition": "Люди, общество, народ (обычно при характеристике состава или поведения группы людей).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом дорогом ресторане обычно собирается солидная <w>публика</w>.",
                            "target_lang_translations": {
                                "en": "A respectable <w>crowd</w> usually gathers in this expensive restaurant."
                            }
                        },
                        {
                            "text": "На городской площади гуляла разношёрстная <w>публика</w>.",
                            "target_lang_translations": {
                                "en": "A motley <w>crowd</w> was walking around the town square."
                            }
                        }
                    ],
                    "synonyms": [
                        "люди",
                        "общество",
                        "народ"
                    ],
                    "common_phrases": [
                        "разношёрстная публика",
                        "солидная публика",
                        "читающая публика"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "People, society, or a specific group of people (usually describing composition or behavior)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crowd",
                                "target_lang_sense_clarification": "group of people gathered in a place"
                            },
                            {
                                "target_lang_word": "public",
                                "target_lang_sense_clarification": "society or community in general"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "046c6e25-d512-3076-96af-7a80be077a5c",
                    "sense_definition": "Люди, вызывающие неодобрение, насмешку или подозрение (употребляется с оттенком иронии).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ну и <w>публика</w> собралась сегодня у подъезда!",
                            "target_lang_translations": {
                                "en": "What a <w>bunch</w> has gathered by the entrance today!"
                            }
                        },
                        {
                            "text": "Что за странная <w>публика</w> ходит здесь по ночам?",
                            "target_lang_translations": {
                                "en": "What kind of strange <w>characters</w> walk here at night?"
                            }
                        }
                    ],
                    "synonyms": [
                        "типы",
                        "субъекты"
                    ],
                    "common_phrases": [
                        "ну и публика",
                        "что за публика"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "прост."
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "устар."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "People causing disapproval, mockery, or suspicion (used with a shade of irony)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bunch",
                                "target_lang_sense_clarification": "colloquial, often negative group of people"
                            },
                            {
                                "target_lang_word": "lot",
                                "target_lang_sense_clarification": "colloquial, specific set of people"
                            },
                            {
                                "target_lang_word": "characters",
                                "target_lang_sense_clarification": "suspicious or strange individuals"
                            }
                        ]
                    }
                }
            ]
        }
    ]

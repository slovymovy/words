            "senses": [
                {
                    "sense_id": "62d5e4a2-a033-3d25-8d97-847bb7ffbc1b",
                    "sense_definition": "Grupa uczniów, którzy uczą się razem w szkole i mają wspólne lekcje.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "edukacja",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja <klasa> jedzie jutro na wycieczkę do Krakowa.",
                            "target_lang_translations": {
                                "ru": "Мой <класс> едет завтра на экскурсию в Краков."
                            }
                        },
                        {
                            "text": "W naszej <klasie> jest dwadzieścia osób, w tym dwunastu chłopców.",
                            "target_lang_translations": {
                                "ru": "В нашем <классе> двадцать человек, в том числе двенадцать мальчиков."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddział"
                    ],
                    "common_phrases": [
                        "cała klasa",
                        "zdjęcie klasowe",
                        "przewodniczący klasy",
                        "wychowawca klasy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа учеников, которые учатся вместе в школе и посещают общие уроки."

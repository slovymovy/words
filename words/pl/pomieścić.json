{
    "word_family": [
        "mieścić",
        "zmieścić",
        "umieścić",
        "zamieścić",
        "rozmieścić",
        "przemieścić",
        "pomieszczenie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f3432309-8cff-3a07-a2f1-2abfc3117540",
                    "sense_definition": "Mieć wystarczająco dużo miejsca wewnątrz, aby przyjąć lub objąć określoną liczbę osób lub rzeczy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "capacity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa sala koncertowa może <pomieścić> ponad tysiąc widzów.",
                            "target_lang_translations": {
                                "ru": "Новый концертный зал может <вместить> более тысячи зрителей."
                            }
                        },
                        {
                            "text": "Ten mały samochód nie zdoła <pomieścić> bagaży całej naszej rodziny.",
                            "target_lang_translations": {
                                "ru": "Эта маленькая машина не сможет <вместить> багаж всей нашей семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmieścić"
                    ],
                    "common_phrases": [
                        "pomieścić wszystkich gości",
                        "pomieścić dużą ilość",
                        "trudno pomieścić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь достаточно места внутри, чтобы принять определенное количество людей или вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вместить",
                                "target_lang_sense_clarification": "о помещении или емкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f587b419-6a6a-3e7a-bc94-9ef491250bd1",
                    "sense_definition": "Znaleźć dla siebie miejsce wewnątrz czegoś; zmieścić się (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_fit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo tłoku wszyscy pasażerowie <pomieścili> się w autobusie.",
                            "target_lang_translations": {
                                "ru": "Несмотря на тесноту, все пассажиры <поместились> в автобусе."
                            }
                        },
                        {
                            "text": "Nie wiem, czy <pomieścimy> się przy jednym małym stole.",
                            "target_lang_translations": {
                                "ru": "Не знаю, <поместимся> ли мы за одним маленьким столом."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmieścić się"
                    ],
                    "common_phrases": [
                        "pomieścić się w samochodzie",
                        "ledwo się pomieścić",
                        "wszyscy się pomieszczą"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Найти для себя место внутри чего-либо; влезть (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поместиться",
                                "target_lang_sense_clarification": "занять место, влезть"
                            },
                            {
                                "target_lang_word": "уместиться",
                                "target_lang_sense_clarification": "полностью войти в ограниченное пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c489afa-099e-3a80-b06a-fb8da83f90a8",
                    "sense_definition": "Zdołać ułożyć, schować lub ulokować wiele przedmiotów w ograniczonej przestrzeni.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz jakoś <pomieścić> te wszystkie ubrania w jednej szafie.",
                            "target_lang_translations": {
                                "ru": "Ты должен как-то <уместить> всю эту одежду в одном шкафу."
                            }
                        },
                        {
                            "text": "Udało mi się <pomieścić> wszystkie książki na tej półce.",
                            "target_lang_translations": {
                                "ru": "Мне удалось <уместить> все книги на этой полке."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmieścić",
                        "upchać",
                        "ulokować",
                        "umieścić"
                    ],
                    "common_phrases": [
                        "pomieścić wszystko w walizce",
                        "pomieścić zakupy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суметь уложить, спрятать или расположить много предметов в ограниченном пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уместить",
                                "target_lang_sense_clarification": "уложить компактно"
                            },
                            {
                                "target_lang_word": "разместить",
                                "target_lang_sense_clarification": "расположить, расставить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
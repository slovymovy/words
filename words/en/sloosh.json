{
    "word_family": [],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "46ebde0d-501d-3fe9-b741-37ae64a758e4",
                    "sense_definition": "To move or flow with a splashing sound and force.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The water <w>slooshed</w> around in the bottom of the old boat.",
                            "target_lang_translations": {
                                "ru": "Вода <w>плескалась</w> на дне старой лодки."
                            }
                        },
                        {
                            "text": "Rainwater <w>slooshed</w> down the drainpipe during the heavy storm.",
                            "target_lang_translations": {
                                "ru": "Дождевая вода <w>с шумом лилась</w> по водосточной трубе во время сильного шторма."
                            }
                        }
                    ],
                    "synonyms": [
                        "slosh",
                        "surge",
                        "gush"
                    ],
                    "antonyms": [
                        "trickle",
                        "drip"
                    ],
                    "common_phrases": [
                        "sloosh around",
                        "sloosh over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться или течь с плеском и силой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плескаться",
                                "target_lang_sense_clarification": "о жидкости, движущейся внутри чего-либо"
                            },
                            {
                                "target_lang_word": "хлюпать",
                                "target_lang_sense_clarification": "издавать характерный влажный звук при движении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3069b4c8-316d-30e8-af99-1d75b1a3f7ec",
                    "sense_definition": "To clean or rinse something by pouring a flow of water over or through it.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>slooshed</w> her face with cold water to wake herself up.",
                            "target_lang_translations": {
                                "ru": "Она <w>ополоснула</w> лицо холодной водой, чтобы проснуться."
                            }
                        },
                        {
                            "text": "He <w>slooshed</w> the muddy boots in the bucket before bringing them inside.",
                            "target_lang_translations": {
                                "ru": "Он <w>промыл</w> грязные сапоги в ведре, прежде чем занести их внутрь."
                            }
                        }
                    ],
                    "synonyms": [
                        "rinse",
                        "sluice",
                        "swill"
                    ],
                    "common_phrases": [
                        "sloosh out",
                        "sloosh off"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мыть или ополаскивать что-либо, поливая потоком воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ополаскивать",
                                "target_lang_sense_clarification": "обливать водой для очистки"
                            },
                            {
                                "target_lang_word": "промывать",
                                "target_lang_sense_clarification": "тщательно мыть в большом количестве воды"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f586eba9-5cd5-366e-8dd0-aa89b089623d",
                    "sense_definition": "A sudden splashing movement or sound of liquid.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With a loud <w>sloosh</w>, the bucket of water emptied onto the floor.",
                            "target_lang_translations": {
                                "ru": "С громким <w>плеском</w> ведро воды вылилось на пол."
                            }
                        },
                        {
                            "text": "We heard the <w>sloosh</w> of the waves against the side of the pier.",
                            "target_lang_translations": {
                                "ru": "Мы слышали <w>плеск</w> волн о край пирса."
                            }
                        }
                    ],
                    "synonyms": [
                        "splash",
                        "slosh",
                        "gush"
                    ],
                    "common_phrases": [
                        "a great sloosh",
                        "sloosh of water"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное плещущее движение или звук жидкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плеск",
                                "target_lang_sense_clarification": "звук удара воды"
                            },
                            {
                                "target_lang_word": "хлюпанье",
                                "target_lang_sense_clarification": "звук переливающейся жидкости"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
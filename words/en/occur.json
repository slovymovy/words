{
    "word_family": [
        "occurrence",
        "recurrence",
        "reoccur",
        "concurrent"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5e15040b-d1d1-357c-9680-f97f66bb8159",
                    "sense_definition": "To happen or take place.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "event_happening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accident <w>occurred</w> late last night on the highway.",
                            "target_lang_translations": {
                                "ru": "Авария <w>произошла</w> поздно ночью на шоссе."
                            }
                        },
                        {
                            "text": "Unexpected errors can <w>occur</w> during the installation process.",
                            "target_lang_translations": {
                                "ru": "В процессе установки могут <w>возникать</w> неожиданные ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "happen",
                        "take place",
                        "come to pass"
                    ],
                    "common_phrases": [
                        "occur frequently",
                        "occur suddenly",
                        "occur naturally"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случаться, происходить или иметь место (о событии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "происходить",
                                "target_lang_sense_clarification": "о событиях, процессах"
                            },
                            {
                                "target_lang_word": "случаться",
                                "target_lang_sense_clarification": "обычно неожиданно или стихийно"
                            },
                            {
                                "target_lang_word": "возникать",
                                "target_lang_sense_clarification": "появляться, начинаться (об ошибках, проблемах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2c61820-a504-363b-91b4-9b34fea8accb",
                    "sense_definition": "To come into the mind; to be realized by someone.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mental_realization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It didn't <w>occur</w> to me that she might be waiting outside.",
                            "target_lang_translations": {
                                "ru": "Мне не <w>пришло в голову</w>, что она может ждать снаружи."
                            }
                        },
                        {
                            "text": "A brilliant idea suddenly <w>occurred</w> to him while he was walking.",
                            "target_lang_translations": {
                                "ru": "Блестящая идея внезапно <w>пришла</w> ему <w>в голову</w>, пока он шел."
                            }
                        }
                    ],
                    "synonyms": [
                        "hit",
                        "come up"
                    ],
                    "common_phrases": [
                        "occur to someone",
                        "never occur to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить на ум; быть осознанным кем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приходить в голову",
                                "target_lang_sense_clarification": "об идеях, мыслях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a347cdf3-a166-3fd8-86ec-751254cd50cb",
                    "sense_definition": "To be present or found in a specific place or condition.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "existence_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "These rare plants only <w>occur</w> in tropical rainforests.",
                            "target_lang_translations": {
                                "ru": "Эти редкие растения <w>встречаются</w> только в тропических лесах."
                            }
                        },
                        {
                            "text": "Minerals <w>occur</w> naturally in the earth's crust.",
                            "target_lang_translations": {
                                "ru": "Минералы <w>встречаются</w> в земной коре в естественном виде."
                            }
                        }
                    ],
                    "synonyms": [
                        "appear"
                    ],
                    "common_phrases": [
                        "occur in nature",
                        "occur naturally"
                    ],
                    "target_lang_definitions": {
                        "ru": "Присутствовать или находиться в определенном месте или условиях (часто о природе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречаться",
                                "target_lang_sense_clarification": "быть распространенным, попадаться"
                            },
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "о местоположении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab156b78-3712-36cb-96ca-e40e8af56a36",
                    "sense_definition": "To present itself or become available.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will write to you immediately if the opportunity <w>occurs</w>.",
                            "target_lang_translations": {
                                "ru": "Я напишу вам немедленно, если <w>представится</w> возможность."
                            }
                        },
                        {
                            "text": "A chance to solve the problem <w>occurred</w> unexpectedly.",
                            "target_lang_translations": {
                                "ru": "Шанс решить проблему <w>представился</w> неожиданно."
                            }
                        }
                    ],
                    "synonyms": [
                        "arise",
                        "come up",
                        "appear"
                    ],
                    "common_phrases": [
                        "opportunity occurs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появляться или становиться доступным (о возможности, шансе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представляться",
                                "target_lang_sense_clarification": "о случае, возможности"
                            },
                            {
                                "target_lang_word": "появляться",
                                "target_lang_sense_clarification": "возникать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
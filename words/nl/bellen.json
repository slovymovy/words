    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fc382c16-1566-388e-b495-5137f4b1d09d",
                    "sense_definition": "Telefoneren; contact opnemen met iemand via de telefoon.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal je vanavond even <w>bellen</w>.",
                            "target_lang_translations": {
                                "en": "I will <w>call</w> you this evening.",
                                "ru": "Я тебе сегодня вечером <w>позвоню</w>."
                            }
                        },
                        {
                            "text": "Zij <w>belt</w> elke week met haar moeder in het buitenland.",
                            "target_lang_translations": {
                                "en": "She <w>calls</w> her mother abroad every week.",
                                "ru": "Она каждую неделю <w>звонит</w> маме за границу."
                            }
                        },
                        {
                            "text": "Heb jij de dokter al <w>gebeld</w> voor een afspraak?",
                            "target_lang_translations": {
                                "en": "Have you <w>called</w> the doctor for an appointment yet?",
                                "ru": "Ты уже <w>позвонил</w> врачу, чтобы записаться на прием?"
                            }
                        }
                    ],
                    "synonyms": [
                        "telefoneren",
                        "opbellen"
                    ],
                    "common_phrases": [
                        "even bellen",
                        "iemand bellen",
                        "mobiel bellen",
                        "later bellen"
                    ],
                    "target_lang_definitions": {
                        "en": "To contact someone by telephone.",
                        "ru": "Звонить по телефону; связываться с кем-либо по телефону."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "to contact by telephone"
                            },
                            {
                                "target_lang_word": "ring",
                                "target_lang_sense_clarification": "UK/informal: to telephone someone"
                            },
                            {
                                "target_lang_word": "phone",
                                "target_lang_sense_clarification": "to contact by telephone"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "звонить",
                                "target_lang_sense_clarification": "по телефону"
                            },
                            {
                                "target_lang_word": "позвонить",
                                "target_lang_sense_clarification": "совершить звонок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd7ad69a-08d9-3223-aec3-b84a58941779",
                    "sense_definition": "Een bel laten klinken, bijvoorbeeld aan de voordeur of op de fiets.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er wordt <w>gebeld</w>, kun jij de deur opendoen?",
                            "target_lang_translations": {
                                "en": "The doorbell is <w>ringing</w>, can you open the door?",
                                "ru": "В дверь <w>звонят</w>, ты можешь открыть?"
                            }
                        },
                        {
                            "text": "De fietser <w>belde</w> luid om de voetgangers te waarschuwen.",
                            "target_lang_translations": {
                                "en": "The cyclist <w>rang</w> loudly to warn the pedestrians.",
                                "ru": "Велосипедист громко <w>позвонил</w>, чтобы предупредить пешеходов."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanbellen",
                        "schellen"
                    ],
                    "common_phrases": [
                        "aan de deur bellen"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a bell to sound, for example at a door or on a bicycle.",
                        "ru": "Издавать звук звонка (например, дверного или велосипедного)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ring",
                                "target_lang_sense_clarification": "to cause a bell to sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "звонить",
                                "target_lang_sense_clarification": "в дверь, в звонок"
                            },
                            {
                                "target_lang_word": "звенеть",
                                "target_lang_sense_clarification": "издавать звон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1a1a7a7-ac42-33e3-84ce-a05618e636c2",
                    "sense_definition": "Blaffen (voornamelijk gezegd van honden).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jachthond <w>belde</w> toen hij het wild rook.",
                            "target_lang_translations": {
                                "en": "The hunting dog <w>bayed</w> when he smelled the game.",
                                "ru": "Охотничья собака <w>залаяла</w>, почуяв дичь."
                            }
                        },
                        {
                            "text": "In de verte hoorden we de honden <w>bellen</w>.",
                            "target_lang_translations": {
                                "en": "In the distance we heard the dogs <w>barking</w>.",
                                "ru": "Вдали мы слышали, как <w>лают</w> собаки."
                            }
                        }
                    ],
                    "synonyms": [
                        "blaffen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Dit gebruik is verouderd of gewestelijk."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To bark (primarily said of dogs).",
                        "ru": "Лаять (преимущественно о собаках, устаревшее значение)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bark",
                                "target_lang_sense_clarification": "to make a loud cry like a dog"
                            },
                            {
                                "target_lang_word": "bay",
                                "target_lang_sense_clarification": "to howl or bark loudly, especially of hunting dogs"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лаять",
                                "target_lang_sense_clarification": "о собаках"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

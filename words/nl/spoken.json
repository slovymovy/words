            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f112c76e-ae08-3df5-b030-dfe41f2f849d",
                    "sense_definition": "Meervoud van <w>spook</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen zijn bang dat er <w>spoken</w> onder hun bed leven.",
                            "target_lang_translations": {
                                "en": "The children are afraid that <w>ghosts</w> live under their beds.",
                                "ru": "Дети боятся, что у них под кроватью живут <w>привидения</w>."
                            }
                        },
                        {
                            "text": "In veel oude verhalen komen heksen en <w>spoken</w> voor.",
                            "target_lang_translations": {
                                "en": "Witches and <w>ghosts</w> appear in many old stories.",
                                "ru": "Во многих старых сказках встречаются ведьмы и <w>призраки</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of *spook*.",
                        "ru": "Множественное число от слова <w>spook</w> (привидение)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ghosts",
                                "target_lang_sense_clarification": "spirits of the dead"
                            },
                            {
                                "target_lang_word": "spirits",
                                "target_lang_sense_clarification": "supernatural beings"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "привидения",
                                "target_lang_sense_clarification": "духи умерших"
                            },
                            {
                                "target_lang_word": "призраки",
                                "target_lang_sense_clarification": "фантомы, видения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "11872dea-2de0-3e47-a7a9-cefd04480ceb",
                    "sense_definition": "Rondwaren als een geest of onverklaarbare, griezelige verschijnselen vertonen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "haunt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er gaan geruchten dat het in dat verlaten kasteel <w>spookt</w>.",
                            "target_lang_translations": {
                                "en": "Rumors are going around that the abandoned castle is <w>haunted</w>.",
                                "ru": "Ходят слухи, что в этом заброшенном замке <w>обитают привидения</w>."
                            }
                        },
                        {
                            "text": "Die vervelende melodie blijft maar door mijn hoofd <w>spoken</w>.",
                            "target_lang_translations": {
                                "en": "That annoying melody keeps <w>haunting</w> my head.",
                                "ru": "Эта навязчивая мелодия продолжает <w>вертеться</w> у меня в голове."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het spookt",
                        "door het hoofd spoken"
                    ],
                    "target_lang_definitions": {
                        "en": "To wander around like a ghost or exhibit inexplicable, creepy phenomena.",
                        "ru": "Бродить (о призраках), проявлять сверхъестественную активность или не давать покоя (о мыслях)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to haunt",
                                "target_lang_sense_clarification": "of a ghost or spirit"
                            },
                            {
                                "target_lang_word": "to haunt",
                                "target_lang_sense_clarification": "of a thought or melody"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бродить",
                                "target_lang_sense_clarification": "о духах или призраках"
                            },
                            {
                                "target_lang_word": "вертеться",
                                "target_lang_sense_clarification": "в голове, о навязчивых мыслях"
                            },
                            {
                                "target_lang_word": "мерещиться",
                                "target_lang_sense_clarification": "казаться, видеться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "240adbd0-36ed-35ad-861f-96c3a0edbf0c",
                    "sense_definition": "Hevig tekeergaan van het weer, vaak met harde wind en onweer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het heeft vannacht flink <w>gespookt</w> met zware windstoten en regen.",
                            "target_lang_translations": {
                                "en": "It <w>stormed</w> heavily last night with severe gusts of wind and rain.",
                                "ru": "Прошлой ночью погода сильно <w>бушевала</w>: были мощные порывы ветра и дождь."
                            }
                        },
                        {
                            "text": "De vissers bleven aan wal omdat het op zee te erg <w>spookte</w>.",
                            "target_lang_translations": {
                                "en": "The fishermen stayed ashore because it was <w>storming</w> too badly at sea.",
                                "ru": "Рыбаки остались на берегу, потому что на море слишком сильно <w>штормило</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stormen"
                    ],
                    "target_lang_definitions": {
                        "en": "Violent carrying on of the weather, often with strong wind and thunder.",
                        "ru": "Бушевать (о погоде), часто сопровождаясь сильным ветром и грозой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to storm",
                                "target_lang_sense_clarification": "weather condition"
                            },
                            {
                                "target_lang_word": "to rage",
                                "target_lang_sense_clarification": "violent weather"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бушевать",
                                "target_lang_sense_clarification": "о стихии"
                            },
                            {
                                "target_lang_word": "штормить",
                                "target_lang_sense_clarification": "о море или ветре"
                            }
                        ]
                    }
                }
            ]
        }
    ]

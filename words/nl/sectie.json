                {
                    "sense_id": "5dfdcd78-2100-32ce-b380-efa9b253751b",
                    "sense_definition": "Een medisch onderzoek waarbij het lichaam van een overledene wordt geopend om de doodsoorzaak te achterhalen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts besloot dat er <w>sectie</w> verricht moest worden om de exacte doodsoorzaak vast te stellen.",
                            "target_lang_translations": {
                                "en": "The doctor decided that an <w>autopsy</w> had to be performed to determine the exact cause of death.",
                                "ru": "Врач решил, что необходимо провести <w>вскрытие</w>, чтобы установить точную причину смерти."
                            }
                        },
                        {
                            "text": "Uit de <w>sectie</w> bleek dat de patiënt aan een natuurlijke oorzaak was overleden.",
                            "target_lang_translations": {
                                "en": "The <w>autopsy</w> showed that the patient had died from a natural cause.",
                                "ru": "<w>Вскрытие</w> показало, что пациент умер от естественных причин."
                            }
                        }
                    ],
                    "synonyms": [
                        "autopsie",
                        "obductie"
                    ],
                    "common_phrases": [
                        "sectie verrichten"
                    ],
                    "target_lang_definitions": {
                        "en": "A medical examination of a deceased person's body to determine the cause of death.",
                        "ru": "Медицинское исследование тела умершего для выяснения причины смерти."
                    },
                    "translations": {

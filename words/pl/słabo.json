    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "208a893e-5cb8-3f8a-8b49-c1dab4478833",
                    "sense_definition": "Źle, na niskim poziomie lub w sposób niezadowalający; często używane do opisania złego samopoczucia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quality_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziś bardzo <słabo> się czuję i chyba mam gorączkę.",
                            "target_lang_translations": {
                                "ru": "Сегодня я очень <плохо> себя чувствую, и, кажется, у меня температура."
                            }
                        },
                        {
                            "text": "Twoja drużyna zagrała <słabo> w pierwszej połowie meczu.",
                            "target_lang_translations": {
                                "ru": "Твоя команда сыграла <слабо> в первой половине матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "źle",
                        "kiepsko",
                        "marnie"
                    ],
                    "antonyms": [
                        "mocno",
                        "mocno",
                        "mocno"
                    ],
                    "common_phrases": [
                        "słabo się czuć",
                        "słabo wyglądać",
                        "słabo iść"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плохо, на низком уровне или неудовлетворительно; часто используется для описания плохого самочувствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плохо",
                                "target_lang_sense_clarification": "о самочувствии или качестве"
                            },
                            {
                                "target_lang_word": "слабо",
                                "target_lang_sense_clarification": "о низком уровне выполнения чего-либо"
                            },
                            {
                                "target_lang_word": "неважно",
                                "target_lang_sense_clarification": "о самочувствии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b54c3777-92c5-3e30-9d50-74f3a5c1aa1e",
                    "sense_definition": "Niewyraźnie, z małą intensywnością; w sposób trudny do dostrzeżenia lub usłyszenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensity_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez zakłócenia bardzo <słabo> cię słyszę w telefonie.",
                            "target_lang_translations": {
                                "ru": "Из-за помех я очень <плохо> слышу тебя по телефону."
                            }
                        },
                        {
                            "text": "Zimą słońce świeci krótko i <słabo>.",
                            "target_lang_translations": {
                                "ru": "Зимой солнце светит недолго и <слабо>."
                            }
                        }
                    ],
                    "synonyms": [
                        "niewyraźnie",
                        "cicho",
                        "blado"
                    ],
                    "antonyms": [
                        "mocno"
                    ],
                    "common_phrases": [
                        "słabo widzieć",
                        "słabo słyszeć",
                        "słabo pamiętać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечетко, с малой интенсивностью; так, что трудно увидеть или услышать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плохо",
                                "target_lang_sense_clarification": "когда трудно видеть или слышать"
                            },
                            {
                                "target_lang_word": "слабо",
                                "target_lang_sense_clarification": "об интенсивности проявления (света, сигнала)"
                            },
                            {
                                "target_lang_word": "нечетко",
                                "target_lang_sense_clarification": "размыто, неясно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "560c887d-f35b-3656-b0c8-4c45c1b689d5",
                    "sense_definition": "Z użyciem niewielkiej siły fizycznej; lekko.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zbyt <słabo> dokręciłeś śrubę i koło odpadło.",
                            "target_lang_translations": {
                                "ru": "Ты слишком <слабо> затянул болт, и колесо отвалилось."
                            }
                        },
                        {
                            "text": "Dziecko <słabo> rzuciło piłką, więc nie doleciała do bramki.",
                            "target_lang_translations": {
                                "ru": "Ребенок <слабо> бросил мяч, поэтому он не долетел до ворот."
                            }
                        }
                    ],
                    "synonyms": [
                        "lekko",
                        "delikatnie"
                    ],
                    "antonyms": [
                        "mocno"
                    ],
                    "common_phrases": [
                        "słabo trzymać",
                        "słabo uderzyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "С применением небольшой физической силы; легко."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слабо",
                                "target_lang_sense_clarification": "без применения силы"
                            },
                            {
                                "target_lang_word": "несильно",
                                "target_lang_sense_clarification": "легко, без нажима"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be10e06e-5da9-3a2a-852f-3be625f5a41e",
                    "sense_definition": "W matematyce: w odniesieniu do słabej topologii lub zbieżności.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten ciąg funkcyjny jest <słabo> zbieżny do zera.",
                            "target_lang_translations": {
                                "ru": "Эта функциональная последовательность <слабо> сходится к нулю."
                            }
                        },
                        {
                            "text": "Badamy operatory <słabo> zwarte w przestrzeniach Banacha.",
                            "target_lang_translations": {
                                "ru": "Мы исследуем <слабо> компактные операторы в банаховых пространствах."
                            }
                        }
                    ],
                    "antonyms": [
                        "mocno"
                    ],
                    "common_phrases": [
                        "słabo zbieżny"
                    ],
                    "target_lang_definitions": {
                        "ru": "В математике: по отношению к слабой топологии или сходимости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слабо",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

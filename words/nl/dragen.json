                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "transport physically while holding"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "421fc73a-c1ac-3a5f-966b-2eb949261dd1",
                    "sense_definition": "Het gewicht van iets steunen of de verantwoordelijkheid voor iets hebben.",

            "senses": [
                {
                    "sense_id": "a742bbf0-9100-3f08-99d1-ec03d72b1908",
                    "sense_definition": "Naturalna pusta przestrzeń w skale, wystarczająco duża, aby mógł wejść do niej człowiek.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Turyści chętnie odwiedzają tę słynną <w>jaskinię</w> w górach.",
                            "target_lang_translations": {
                                "ru": "Туристы охотно посещают эту знаменитую <w>пещеру</w> в горах."
                            }
                        },
                        {
                            "text": "Sklepienie <w>jaskini</w> pokrywały gęsto stalaktyty.",
                            "target_lang_translations": {
                                "ru": "Свод <w>пещеры</w> был густо покрыт сталактитами."
                            }
                        }
                    ],
                    "synonyms": [
                        "grota",
                        "pieczara",
                        "jama"
                    ],
                    "common_phrases": [
                        "głęboka jaskinia",
                        "wejście do jaskini",
                        "zwiedzać jaskinię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественная полость в горной породе, достаточно большая, чтобы в нее мог войти человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пещера",
                                "target_lang_sense_clarification": "основное значение; естественное углубление в скале"
                            },
                            {
                                "target_lang_word": "грот",
                                "target_lang_sense_clarification": "неглубокая пещера с широким входом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3307c7a0-544e-3a46-8e8f-c51d69ba55e4",
                    "sense_definition": "Miejsce nielegalnych lub występnych zgromadzeń, często kojarzone z hazardem lub przestępczością.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Las Vegas uważa się za największą <w>jaskinię</w> hazardu na świecie.",
                            "target_lang_translations": {
                                "ru": "Лас-Вегас считается крупнейшим <w>притоном</w> азартных игр в мире."
                            }
                        },
                        {
                            "text": "Policja zlikwidowała tajną <w>jaskinię</w> przemytników w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Полиция ликвидировала тайное <w>логово</w> контрабандистов в центре города."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jaskinia hazardu",
                        "jaskinia rozpusty",
                        "jaskinia zła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место незаконных или порочных сборищ, часто ассоциируемое с азартными играми или преступностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "притон",
                                "target_lang_sense_clarification": "место сборища преступных или деклассированных элементов"
                            },
                            {
                                "target_lang_word": "логово",
                                "target_lang_sense_clarification": "укрытие врага или преступников"
                            },
                            {
                                "target_lang_word": "вертеп",
                                "target_lang_sense_clarification": "устар./книжн. притон разврата или преступлений"

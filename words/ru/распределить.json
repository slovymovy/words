                        "назначить"
                    ],
                    "common_phrases": [
                        "распределить выпускников",
                        "распределить на работу"
                    ],
                    "target_lang_definitions": {
                        "en": "To officially assign a graduate to a specific place of work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "assign",
                                "target_lang_sense_clarification": "to appoint to a post or duty"
                            },
                            {
                                "target_lang_word": "post",
                                "target_lang_sense_clarification": "to send to a specific location for work"
                            },
                            {
                                "target_lang_word": "place",
                                "target_lang_sense_clarification": "to find a job or position for someone"
                            }
                        ]
                    }
                }

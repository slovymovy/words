    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "5ee62fbc-1359-3cf2-822b-82f7ebf93279",
                    "sense_definition": "Wyrażenie używane bezpośrednio do pogratulowania komuś sukcesu, osiągnięcia lub szczęśliwego wydarzenia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słyszałem, że dostałeś nową pracę, <w>gratulacje</w>!",
                            "target_lang_translations": {
                                "ru": "Я слышал, что ты получил новую работу, <w>поздравляю</w>!"
                            }
                        },
                        {
                            "text": "<w>Gratulacje</w>, to był naprawdę wspaniały występ.",
                            "target_lang_translations": {
                                "ru": "<w>Поздравляю</w>, это было действительно замечательное выступление."
                            }
                        }
                    ],
                    "common_phrases": [
                        "moje gratulacje",
                        "szczere gratulacje",
                        "wielkie gratulacje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение, используемое непосредственно для поздравления кого-либо с успехом, достижением или счастливым событием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поздравляю",
                                "target_lang_sense_clarification": "восклицание"
                            },
                            {
                                "target_lang_word": "мои поздравления",
                                "target_lang_sense_clarification": "фраза"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "90c04d9b-9a2b-3d3a-a531-4b4b2cf77b7c",
                    "sense_definition": "Słowa uznania i radości przekazywane komuś z powodu jego osiągnięcia lub ważnego wydarzenia życiowego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złożyłem jej serdeczne <w>gratulacje</w> z okazji ukończenia studiów.",
                            "target_lang_translations": {
                                "ru": "Я передал ей сердечные <w>поздравления</w> по случаю окончания учебы."
                            }
                        },
                        {
                            "text": "Dyrektor przyjął <w>gratulacje</w> od całego zespołu za udany projekt.",
                            "target_lang_translations": {
                                "ru": "Директор принял <w>поздравления</w> от всей команды за успешный проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "gratulacja"
                    ],
                    "common_phrases": [
                        "składać gratulacje",
                        "przyjmować gratulacje",
                        "przesyłać gratulacje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слова признания и радости, выражаемые кому-либо в связи с его достижением или важным жизненным событием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поздравления",
                                "target_lang_sense_clarification": "слова одобрения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43804829-5250-3e98-8ac7-ba2ead7fa197",
                    "sense_definition": "Mianownik, biernik i wołacz liczby mnogiej od słowa <w>gratulacja</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie te <w>gratulacje</w> sprawiły jej ogromną przyjemność.",
                            "target_lang_translations": {
                                "ru": "Все эти <w>поздравления</w> доставили ей огромное удовольствие."
                            }
                        },
                        {
                            "text": "Czytałem twoje <w>gratulacje</w> pod postem na portalu społecznościowym.",

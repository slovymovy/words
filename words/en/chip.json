    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7ea45036-63ba-308d-b382-ed31ab3a431a",
                    "sense_definition": "A thin, crisp, fried slice of potato, often eaten as a snack.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_us",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He opened a bag of potato <w>chips</w> while watching the movie.",
                            "target_lang_translations": {
                                "ru": "Он открыл пакет картофельных <w>чипсов</w> во время просмотра фильма."
                            }
                        },
                        {
                            "text": "We served tortilla <w>chips</w> with spicy salsa at the party.",
                            "target_lang_translations": {
                                "ru": "Мы подали кукурузные <w>чипсы</w> с острой сальсой на вечеринке."
                            }
                        }
                    ],
                    "synonyms": [
                        "crisp"
                    ],
                    "common_phrases": [
                        "potato chips",
                        "tortilla chips",
                        "corn chips",
                        "bag of chips"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America, Australia, New Zealand"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий, хрустящий, обжаренный ломтик картофеля, который часто едят как закуску."
                    },
                    "translations": {
                        "ru": [
                            {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "776665ed-ed86-32ca-a735-8bf986013c9e",
                    "sense_definition": "Совокупность действий и событий, в которых раскрывается основное содержание литературного произведения, фильма или спектакля.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "narrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Фильм имел настолько захватывающий <w>сюжет</w>, что зрители не могли оторваться от экрана.",
                            "target_lang_translations": {
                                "en": "The movie had such a gripping <w>plot</w> that the audience couldn't tear their eyes away from the screen."
                            }
                        },
                        {
                            "text": "В основе пьесы лежит классический <w>сюжет</w> о борьбе добра и зла.",
                            "target_lang_translations": {
                                "en": "At the heart of the play lies a classic <w>plot</w> about the struggle between good and evil."
                            }
                        }
                    ],
                    "synonyms": [
                        "фабула",
                        "содержание"
                    ],
                    "common_phrases": [
                        "сюжет фильма",
                        "развитие сюжета",
                        "закрученный сюжет",
                        "главный сюжет"
                    ],
                    "target_lang_definitions": {
                        "en": "The sequence of actions and events that make up the main content of a literary work, film, or play."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plot",
                                "target_lang_sense_clarification": "sequence of events"
                            },
                            {
                                "target_lang_word": "storyline",
                                "target_lang_sense_clarification": "narrative structure"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74e7de14-3814-3aee-8c77-b77f60a44e11",
                    "sense_definition": "Тема или предмет изображения в искусстве, а также содержание какого-либо рассказа или происшествия.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "narrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Художник долго искал подходящий <w>сюжет</w> для своей новой исторической картины.",
                            "target_lang_translations": {
                                "en": "The artist spent a long time looking for a suitable <w>subject</w> for his new historical painting."
                            }
                        },
                        {
                            "text": "Жизнь часто подбрасывает нам готовый <w>сюжет</w> для небольшого рассказа.",
                            "target_lang_translations": {
                                "en": "Life often throws us a ready-made <w>subject</w> for a short story."
                            }
                        }
                    ],
                    "synonyms": [
                        "содержание",
                        "канва"
                    ],
                    "common_phrases": [
                        "сюжет для картины",
                        "сюжет рассказа",
                        "найти сюжет"
                    ],
                    "target_lang_definitions": {
                        "en": "The theme or subject matter depicted in art, or the content of a story or incident."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "subject",
                                "target_lang_sense_clarification": "topic or theme"
                            },
                            {
                                "target_lang_word": "subject matter",
                                "target_lang_sense_clarification": "content of art"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12550434-30b3-36b3-b64c-23f077179399",
                    "sense_definition": "Жертва преступника, обычно вора или мошенника.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный карманник сразу заметил, что его <w>сюжет</w> отвлёкся на телефонный разговор.",
                            "target_lang_translations": {
                                "en": "The experienced pickpocket immediately noticed that his <w>mark</w> was distracted by a phone conversation."
                            }
                        },
                        {
                            "text": "Мошенники вели свой <w>сюжет</w> от самого банка до безлюдного переулка.",
                            "target_lang_translations": {
                                "en": "The con artists trailed their <w>target</w> from the bank all the way to a deserted alley."
                            }
                        }
                    ],
                    "traits": [

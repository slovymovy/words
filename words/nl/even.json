                {
                    "sense_id": "5b49e910-fabe-372c-a26c-522630dbd1c1",
                    "sense_definition": "Voor een korte tijd; gedurende een kort moment.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet <w>even</w> naar de winkel om brood te halen.",
                            "target_lang_translations": {
                                "en": "I need to go to the store <w>briefly</w> to get bread.",
                                "ru": "Мне нужно <w>ненадолго</w> сходить в магазин за хлебом."
                            }
                        },
                        {
                            "text": "Wacht <w>even</w> op mij, ik ben zo klaar.",
                            "target_lang_translations": {
                                "en": "Wait <w>a moment</w> for me, I'll be ready in a second.",
                                "ru": "Подожди меня <w>минутку</w>, я сейчас буду готов."
                            }
                        }
                    ],
                    "synonyms": [
                        "eventjes",
                        "effentjes"
                    ],
                    "common_phrases": [
                        "heel even",
                        "wacht even",
                        "even wachten"
                    ],
                    "target_lang_definitions": {
                        "en": "For a short time; during a short moment.",
                        "ru": "На короткое время; ненадолго."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "briefly",
                                "target_lang_sense_clarification": "for a short duration"
                            },
                            {
                                "target_lang_word": "for a moment",
                                "target_lang_sense_clarification": "short period of time"
                            },
                            {
                                "target_lang_word": "just",
                                "target_lang_sense_clarification": "indicating a quick action"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ненадолго",
                                "target_lang_sense_clarification": "на короткое время"
                            },
                            {
                                "target_lang_word": "на минутку",
                                "target_lang_sense_clarification": "очень быстро"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8dfeda54-4045-3f37-9f9e-f380323a3a49",
                    "sense_definition": "Een woordje dat gebruikt wordt om een vraag, verzoek of mededeling vriendelijker, minder dwingend of luchtiger te maken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "modal_particle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zou je <w>even</w> het zout aan mij kunnen geven?",
                            "target_lang_translations": {
                                "en": "Could you <w>just</w> pass me the salt?",
                                "ru": "Не мог бы ты <w>просто</w> передать мне соль?"
                            }
                        },
                        {
                            "text": "Ik zal <w>even</w> kijken of ik dat voor je kan vinden.",
                            "target_lang_translations": {
                                "en": "I will <w>just</w> see if I can find that for you.",
                                "ru": "Я <w>сейчас</w> посмотрю, смогу ли я найти это для тебя."
                            }
                        }
                    ],
                    "synonyms": [
                        "eventjes"
                    ],
                    "common_phrases": [
                        "even kijken",
                        "even vragen",
                        "even doen"

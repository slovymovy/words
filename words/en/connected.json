    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8e24d0a0-31f7-33b4-9f94-1f50878f2683",
                    "sense_definition": "Joined or linked together, especially through a communication network or technology.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "network_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please ensure that your device is <w>connected</w> to the internet before starting the update.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, убедитесь, что ваше устройство <w>подключено</w> к интернету перед началом обновления."
                            }
                        },
                        {
                            "text": "It is easier to stay <w>connected</w> with friends abroad thanks to social media.",
                            "target_lang_translations": {
                                "ru": "Благодаря социальным сетям легче оставаться <w>на связи</w> с друзьями за границей."
                            }
                        }
                    ],
                    "synonyms": [
                        "associated",
                        "related"
                    ],
                    "antonyms": [
                        "disconnected",
                        "unconnected"
                    ],
                    "common_phrases": [
                        "stay connected",
                        "get connected",
                        "connected device"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединённый или связанный, особенно через сеть связи или технологии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подключённый",
                                "target_lang_sense_clarification": "к интернету или сети"
                            },
                            {
                                "target_lang_word": "соединённый",
                                "target_lang_sense_clarification": "физически или технически"
                            },
                            {
                                "target_lang_word": "на связи",
                                "target_lang_sense_clarification": "в контексте общения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45a54ba5-4540-370f-b535-8a63f99c72d5",
                    "sense_definition": "Having a relationship, link, or association with someone or something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police suspect that the two crimes are <w>connected</w> in some way.",
                            "target_lang_translations": {
                                "ru": "Полиция подозревает, что эти два преступления каким-то образом <w>связаны</w>."
                            }
                        },
                        {
                            "text": "She is <w>connected</w> to the royal family through her marriage.",
                            "target_lang_translations": {
                                "ru": "Она <w>связана</w> с королевской семьёй через свой брак."
                            }
                        }
                    ],
                    "synonyms": [
                        "associated",
                        "related",
                        "affiliated",
                        "allied"
                    ],
                    "antonyms": [
                        "unconnected",
                        "disconnected"
                    ],
                    "common_phrases": [
                        "connected with",
                        "connected to",
                        "closely connected"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий отношение, связь или ассоциацию с кем-либо или чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связанный",
                                "target_lang_sense_clarification": "имеющий отношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fa5e9f2-23b5-3165-b896-b70f8a0a334f",
                    "sense_definition": "Feeling close, intimate, or having strong bonds of affection.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After talking for hours, they felt deeply <w>connected</w> to one another.",
                            "target_lang_translations": {
                                "ru": "Проговорив несколько часов, они почувствовали глубокую <w>связь</w> друг с другом."
                            }
                        },
                        {
                            "text": "I feel <w>connected</w> to this place because I spent my childhood here.",
                            "target_lang_translations": {
                                "ru": "Я чувствую <w>связь</w> с этим местом, потому что провёл здесь своё детство."
                            }
                        }
                    ],
                    "antonyms": [
                        "disconnected"
                    ],
                    "common_phrases": [
                        "feel connected",
                        "deeply connected"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий близость, духовное родство или сильную привязанность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ощущающий связь",
                                "target_lang_sense_clarification": "эмоционально"
                            },
                            {
                                "target_lang_word": "близкий",
                                "target_lang_sense_clarification": "по духу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62769385-3de7-3678-9168-3bb0bcbbdbbe",
                    "sense_definition": "Having useful social or professional contacts with powerful or influential people.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He got the job easily because he is very well <w>connected</w> in the industry.",
                            "target_lang_translations": {
                                "ru": "Он легко получил работу, потому что у него большие <w>связи</w> в этой индустрии."
                            }
                        },
                        {
                            "text": "The politician was <w>connected</w> enough to avoid the scandal affecting his colleagues.",
                            "target_lang_translations": {
                                "ru": "Политик имел достаточно <w>связей</w>, чтобы избежать скандала, затронувшего его коллег."
                            }
                        }
                    ],
                    "synonyms": [
                        "important"
                    ],
                    "common_phrases": [
                        "well connected",
                        "politically connected"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий полезные социальные или профессиональные контакты с влиятельными людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "со связями",
                                "target_lang_sense_clarification": "имеющий влиятельных знакомых"
                            },
                            {
                                "target_lang_word": "влиятельный",
                                "target_lang_sense_clarification": "благодаря знакомствам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cab10ce5-dc70-3f45-bbbd-98cf6688b390",
                    "sense_definition": "In graph theory, having a path that links every pair of vertices in the graph.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>connected</w> graph allows travel between any two points within the network.",
                            "target_lang_translations": {
                                "ru": "<w>Связный</w> граф позволяет перемещаться между любыми двумя точками внутри сети."
                            }
                        },
                        {
                            "text": "The algorithm checks if the nodes form a <w>connected</w> component.",
                            "target_lang_translations": {
                                "ru": "Алгоритм проверяет, образуют ли узлы <w>связный</w> компонент."
                            }
                        }
                    ],
                    "antonyms": [
                        "disconnected"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В теории графов: имеющий путь, связывающий любую пару вершин в графе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связный",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63f68fa1-e42f-37ed-a602-436da1b41002",
                    "sense_definition": "Involved with organized crime, often implying a specific status or association.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "crime",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Everyone in the neighborhood knew he was <w>connected</w> and avoided crossing him.",
                            "target_lang_translations": {
                                "ru": "Все в округе знали, что он <w>связан с мафией</w>, и избегали переходить ему дорогу."
                            }
                        },
                        {
                            "text": "The investigation focused on several <w>connected</w> individuals running the illegal operation.",
                            "target_lang_translations": {
                                "ru": "Расследование сосредоточилось на нескольких <w>связанных с криминалом</w> лицах, управляющих незаконной операцией."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "organized crime context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вовлечённый в организованную преступность, часто подразумевающий определённый статус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связанный с криминалом",
                                "target_lang_sense_clarification": "преступный мир"
                            },
                            {
                                "target_lang_word": "блатной",
                                "target_lang_sense_clarification": "жаргон"
                            }
                        ]
                    }
                }
            ]
        },
        {

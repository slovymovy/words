    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ded073e7-ddd8-36df-a722-1e580ba46966",
                    "sense_definition": "Jadalny, kulisty owoc jabłoni o smaku słodkim lub kwaśnym, mający czerwoną, żółtą lub zieloną skórkę.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_fruit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano zjadam jedno świeże <w>jabłko</w> na drugie śniadanie.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я съедаю одно свежее <w>яблоко</w> на второй завтрак."
                            }
                        },
                        {
                            "text": "Do upieczenia tego ciasta potrzebne będą kwaśne <w>jabłka</w> z naszego sadu.",
                            "target_lang_translations": {
                                "ru": "Для выпечки этого пирога понадобятся кислые <w>яблоки</w> из нашего сада."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) jabko, płónka"
                    ],
                    "common_phrases": [
                        "soczyste jabłko",
                        "kwaśne jabłko",
                        "obierać jabłko",
                        "szarlotka z jabłkami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобный круглый плод яблони сладкого или кислого вкуса, имеющий красную, желтую или зеленую кожуру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яблоко",
                                "target_lang_sense_clarification": "фрукт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49cd55e1-7686-3c06-9451-f8c8ade4ed1f",
                    "sense_definition": "Widoczna wypukłość na przedniej części szyi, utworzona przez chrząstkę tarczycowatą, występująca głównie u mężczyzn.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy mężczyzna przełykał ślinę, jego <w>jabłko</w> Adama wyraźnie się poruszało.",
                            "target_lang_translations": {
                                "ru": "Когда мужчина глотал слюну, его адамово <w>яблоко</w> отчетливо двигалось."
                            }
                        },
                        {
                            "text": "Chudy chłopak miał bardzo wydatne <w>jabłko</w> Adama na swojej szyi.",
                            "target_lang_translations": {
                                "ru": "У хулого парня было очень заметное адамово <w>яблоко</w> на шее."
                            }
                        }
                    ],
                    "synonyms": [
                        "jabłko Adama"
                    ],
                    "common_phrases": [
                        "wystające jabłko Adama",
                        "duże jabłko Adama"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заметный выступ на передней части шеи, образованный щитовидным хрящом, встречающийся преимущественно у мужчин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адамово яблоко",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "кадык",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15ee6bb2-68b4-3ddb-bc86-5b753483cfa4",
                    "sense_definition": "Złota kula zwieńczona krzyżem, stanowiąca jeden z symboli władzy królewskiej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "symbols",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas uroczystej koronacji monarcha trzymał w dłoni złote <w>jabłko</w>.",
                            "target_lang_translations": {
                                "ru": "Во время торжественной коронации монарх держал в руке золотую <w>державу</w>."
                            }
                        },
                        {
                            "text": "Insygnia koronacyjne obejmują zazwyczaj koronę, berło oraz królewskie <w>jabłko</w>.",
                            "target_lang_translations": {
                                "ru": "Коронационные инсигнии обычно включают корону, скипетр и королевскую <w>державу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jabłko królewskie",
                        "berło i jabłko"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Золотой шар, увенчанный крестом, являющийся одним из символов королевской власти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "держава",
                                "target_lang_sense_clarification": "символ власти"
                            },
                            {
                                "target_lang_word": "яблоко",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1a94346-ee9c-3609-8422-8d711c7ddb36",
                    "sense_definition": "Regionalne lub gwarowe określenie ziemniaka.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "food_vegetable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W niektórych regionach Polski na obiad gotuje się <w>jabłka</w> zamiast ziemniaków.",
                            "target_lang_translations": {
                                "ru": "В некоторых регионах Польши на обед варят <w>картошку</w> вместо обычного названия."
                            }
                        },
                        {
                            "text": "Gospodarz wykopał z pola pełen worek dorodnych <w>jabłek</w> ziemnych.",
                            "target_lang_translations": {
                                "ru": "Хозяин выкопал в поле полный мешок отборного <w>картофеля</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ziemniaki",
                        "bulba",
                        "grul",
                        "perka",
                        "ziemiak",
                        "jabłko ziemne",
                        "bałaban",
                        "gajda",
                        "gajdak",
                        "harcak",
                        "knul",
                        "pątak"
                    ],
                    "common_phrases": [
                        "jabłka ziemne"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in some dialects"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное или диалектное название картофеля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "картофель",
                                "target_lang_sense_clarification": "овощ"
                            },
                            {
                                "target_lang_word": "картошка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1effe525-128a-3b1e-8d36-8bea68222599",
                    "sense_definition": "Okrągła, ciemniejsza plama występująca na sierści koni, zazwyczaj siwych.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "animals_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wybiegu pasł się piękny, duży siwy koń w ciemne <w>jabłka</w>.",
                            "target_lang_translations": {
                                "ru": "В загоне паслась красивая, большая серая лошадь в темных <w>яблоках</w>."
                            }
                        },
                        {
                            "text": "Sierść tego ogiera pokrywały liczne <w>jabłka</w>, co dodawało mu uroku.",
                            "target_lang_translations": {
                                "ru": "Шерсть этого жеребца покрывали многочисленные <w>яблоки</w>, что придавало ему очарования."
                            }
                        }
                    ],
                    "common_phrases": [
                        "koń w jabłka",
                        "maść w jabłka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Круглое, более темное пятно на шерсти лошадей, обычно серых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яблоко",
                                "target_lang_sense_clarification": "окрас лошади"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebdcb156-5ba2-38c8-af18-1c85998364b5",
                    "sense_definition": "Przestarzałe lub potoczne określenie rzepki w kolanie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Upadł niefortunnie na chodnik i mocno stłukł sobie prawe <w>jabłko</w>.",
                            "target_lang_translations": {
                                "ru": "Он неудачно упал на тротуар и сильно ушиб правую <w>коленную чашечку</w>."
                            }
                        },
                        {
                            "text": "Dawniej lekarze używali określenia <w>jabłko</w> na tę część stawu kolanowego.",
                            "target_lang_translations": {
                                "ru": "Раньше врачи использовали название <w>яблоко</w> для этой части коленного сустава."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzepa"
                    ],
                    "traits": [
                        {
                            "trait_type": "obsolete",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее или разговорное название коленной чашечки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коленная чашечка",
                                "target_lang_sense_clarification": "анатомия"
                            },
                            {
                                "target_lang_word": "надколенник",
                                "target_lang_sense_clarification": "медицинское"
                            }
                        ]
                    }
                }
            ]
        }

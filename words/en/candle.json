                {
                    "sense_id": "46e79936-cf4a-3277-bf2f-ef2d3ce2a4d2",
                    "sense_definition": "A light source made of a block or stick of wax with a wick in the middle that burns to give light.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She lit a <w>candle</w> to make the room smell nice."
                        },
                        {
                            "text": "Please blow out the <w>candle</w> before you go to sleep."
                        }
                    ],
                    "synonyms": [
                        "taper"
                    ],
                    "common_phrases": [

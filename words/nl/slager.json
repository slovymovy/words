                {
                    "sense_id": "0b0c67c3-9617-399b-a2af-52b1f881bcec",
                    "sense_definition": "Iemand die voor zijn beroep vlees snijdt, bereidt en verkoopt in een winkel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "profession_shop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga even naar de <w>slager</w> om gehakt te kopen.",
                            "target_lang_translations": {
                                "en": "I'm just going to the <w>butcher</w> to buy minced meat.",
                                "ru": "Я ненадолго пойду к <w>мяснику</w>, чтобы купить фарш."
                            }
                        },
                        {
                            "text": "De <w>slager</w> had vandaag verse worst in de aanbieding.",
                            "target_lang_translations": {
                                "en": "The <w>butcher</w> had fresh sausage on offer today.",
                                "ru": "У <w>мясника</w> сегодня была свежая колбаса по акции."
                            }
                        }
                    ],
                    "synonyms": [
                        "beenhouwer"
                    ],
                    "common_phrases": [
                        "naar de slager",
                        "bij de slager"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who cuts, prepares, and sells meat in a shop.",
                        "ru": "Человек, который разделывает, готовит и продает мясо в магазине."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "butcher",
                                "target_lang_sense_clarification": "shopkeeper"
                            }

        "растрата"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "675df797-9bab-3f1a-808d-78139af82a17",
                    "sense_definition": "Расходовать, использовать какой-либо ресурс (деньги, время, силы) для определенной цели.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы стараемся не <тратить> слишком много денег на развлечения.",
                            "target_lang_translations": {
                                "en": "We try not to <w>spend</w> too much money on entertainment."
                            }
                        },
                        {
                            "text": "Я не хочу <тратить> своё время на споры в интернете.",
                            "target_lang_translations": {
                                "en": "I don't want to <w>spend</w> my time on arguments on the internet."
                            }
                        }
                    ],
                    "synonyms": [
                        "расходовать"
                    ],
                    "antonyms": [
                        "накапливать",
                        "сберегать",
                        "беречь",
                        "беречь",
                        "накапливать"
                    ],
                    "common_phrases": [
                        "тратить деньги",
                        "тратить время",
                        "тратить силы",
                        "тратить энергию"
                    ],
                    "target_lang_definitions": {
                        "en": "To use money, time, or energy to pay for something or to achieve a specific purpose."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spend",
                                "target_lang_sense_clarification": "use money or time"
                            },
                            {
                                "target_lang_word": "expend",
                                "target_lang_sense_clarification": "use energy, effort, or resources (formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06113b4e-1a8f-3d12-bc66-43b10fb80568",
                    "sense_definition": "Использовать что-либо напрасно, без пользы; растрачивать.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не стоит <тратить> слова, если тебя всё равно не слушают.",
                            "target_lang_translations": {
                                "en": "It's not worth <w>wasting</w> words if they aren't listening to you anyway."
                            }
                        },
                        {
                            "text": "Он продолжает <тратить> свою жизнь на бессмысленные обиды.",
                            "target_lang_translations": {
                                "en": "He continues to <w>waste</w> his life on meaningless grievances."
                            }
                        }
                    ],
                    "synonyms": [
                        "расходовать"
                    ],
                    "antonyms": [
                        "беречь",
                        "сберегать"
                    ],
                    "common_phrases": [
                        "тратить зря",
                        "тратить впустую",
                        "тратить попусту"
                    ],
                    "target_lang_definitions": {
                        "en": "To use something in a way that is not necessary, effective, or sensible; to use without a good result."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "waste",
                                "target_lang_sense_clarification": "use uselessly or carelessly"
                            },
                            {
                                "target_lang_word": "squander",
                                "target_lang_sense_clarification": "waste recklessly or foolishly"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "antonyms": [
                        "откупорить"
                    ],
                    "common_phrases": [
                        "забить окно",
                        "забить дверь",
                        "забить щель"
                    ],
                    "target_lang_definitions": {
                        "en": "To tightly close an opening, doorway, or gap using boards, plugs, or other materials."
                    },
                    "translations": {

            "senses": [
                {
                    "sense_id": "9f4316f5-e5ca-36bf-b126-9a89934308c1",
                    "sense_definition": "Покидать какое-либо место, отправляться куда-либо на транспортном средстве.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы завтра рано утром <w>выезжаем</w> в Москву на машине.",
                            "target_lang_translations": {
                                "en": "We are <w>leaving</w> for Moscow by car early tomorrow morning."
                            }
                        },
                        {
                            "text": "Автобус медленно <w>выезжает</w> с парковки на главную дорогу.",
                            "target_lang_translations": {
                                "en": "The bus is slowly <w>pulling out</w> of the parking lot onto the main road."
                            }
                        }
                    ],
                    "synonyms": [
                        "уезжать",
                        "отбывать"
                    ],
                    "antonyms": [
                        "въезжать",
                        "приезжать"
                    ],
                    "common_phrases": [
                        "выезжать из дома",
                        "выезжать на дорогу",
                        "выезжать из города",
                        "выезжать за границу"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave a place or set off for a destination using a vehicle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "depart by vehicle"
                            },
                            {
                                "target_lang_word": "drive out",
                                "target_lang_sense_clarification": "exit a space like a garage or parking lot"
                            },
                            {
                                "target_lang_word": "pull out",
                                "target_lang_sense_clarification": "move a vehicle out into traffic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50360b3e-45b4-3cce-b5de-6f1145f5e931",
                    "sense_definition": "Освобождать помещение, покидать место жительства (обычно с вещами).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гости должны <w>выезжать</w> из номера отеля до двенадцати часов дня.",
                            "target_lang_translations": {
                                "en": "Guests must <w>check out</w> of the hotel room before twelve noon."
                            }
                        },
                        {
                            "text": "Соседи <w>выезжают</w> из старой квартиры и перевозят мебель.",
                            "target_lang_translations": {
                                "en": "The neighbors are <w>moving out</w> of the old apartment and transporting furniture."
                            }
                        }
                    ],
                    "synonyms": [
                        "съезжать",
                        "выселяться"
                    ],
                    "antonyms": [
                        "въезжать"
                    ],
                    "common_phrases": [
                        "выезжать из квартиры",
                        "выезжать из номера",
                        "выезжать с вещами"
                    ],
                    "target_lang_definitions": {
                        "en": "To vacate a room or move out of a residence permanently."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c79b9d1a-a097-3cac-9fed-585ed8c77b85",
                    "sense_definition": "A person who behaves in a selfish, rude, or stupid way.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "character_insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was acting like a real <w>jerk</w> when he refused to help us.",
                            "target_lang_translations": {
                                "ru": "Он вёл себя как настоящий <w>придурок</w>, когда отказался нам помочь."
                            }
                        },
                        {
                            "text": "Don't be such a <w>jerk</w> to your little brother.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>хамом</w> по отношению к младшему брату."
                            }
                        }
                    ],
                    "synonyms": [
                        "idiot",
                        "fool",
                        "brute"
                    ],
                    "common_phrases": [
                        "such a jerk",
                        "real jerk",
                        "don't be a jerk"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe an unlikable person"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который ведёт себя эгоистично, грубо или глупо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "придурок",
                                "target_lang_sense_clarification": "глупый или неприятный человек (разг.)"
                            },
                            {
                                "target_lang_word": "хам",
                                "target_lang_sense_clarification": "грубый человек"
                            },
                            {
                                "target_lang_word": "негодяй",
                                "target_lang_sense_clarification": "подлый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d60d8c1-fcaa-3240-948a-6d9398802876",
                    "sense_definition": "A sudden, sharp, and often uncontrolled movement.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sudden_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train stopped with a sudden <w>jerk</w> that nearly threw me off my feet.",
                            "target_lang_translations": {
                                "ru": "Поезд остановился с резким <w>толчком</w>, который чуть не сбил меня с ног."
                            }
                        },
                        {
                            "text": "He woke up with a <w>jerk</w> when the alarm went off.",
                            "target_lang_translations": {
                                "ru": "Он проснулся, <w>вздрогнув</w>, когда зазвонил будильник."
                            }
                        }
                    ],
                    "synonyms": [
                        "jolt",
                        "lurch",
                        "twitch",
                        "spasm"
                    ],
                    "common_phrases": [
                        "with a jerk",
                        "sudden jerk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное, резкое и часто неконтролируемое движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толчок",
                                "target_lang_sense_clarification": "резкое сотрясение"
                            },
                            {
                                "target_lang_word": "рывок",
                                "target_lang_sense_clarification": "резкое движение"
                            },
                            {
                                "target_lang_word": "вздрагивание",
                                "target_lang_sense_clarification": "непроизвольное движение от испуга или пробуждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8547461-705b-33af-8916-0c6ae8ed2554",
                    "sense_definition": "A quick, sharp pull on something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sudden_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She gave the rope a quick <w>jerk</w> to test its strength.",
                            "target_lang_translations": {
                                "ru": "Она проверила прочность веревки быстрым <w>рывком</w>."
                            }
                        },
                        {
                            "text": "With one strong <w>jerk</w>, he opened the stuck door.",
                            "target_lang_translations": {
                                "ru": "Одним сильным <w>рывком</w> он открыл заклинившую дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "tug",
                        "yank",
                        "pull"
                    ],
                    "common_phrases": [
                        "give a jerk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстрое, резкое тянущее движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рывок",
                                "target_lang_sense_clarification": "резкое движение на себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88b689a6-bc72-3227-b96e-7b14c6eb7847",
                    "sense_definition": "A spicy marinade or style of cooking native to Jamaica.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We ordered spicy <w>jerk</w> chicken from the street vendor.",
                            "target_lang_translations": {
                                "ru": "Мы заказали острую курицу <w>джерк</w> у уличного торговца."
                            }
                        },
                        {
                            "text": "The chef prepared a traditional <w>jerk</w> sauce with scotch bonnet peppers.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар приготовил традиционный соус <w>джерк</w> с перцем скотч-боннет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jerk chicken",
                        "jerk pork",
                        "jerk seasoning",
                        "jerk sauce"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Caribbean / Jamaican"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Острый маринад или стиль приготовления пищи, родом с Ямайки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "джерк",
                                "target_lang_sense_clarification": "кулинарный стиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "470f00cd-31eb-35a3-ada9-a123902dd5c8",
                    "sense_definition": "A weightlifting movement where the weight is raised from the shoulders to above the head.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The clean and <w>jerk</w> is a challenging Olympic lift.",
                            "target_lang_translations": {
                                "ru": "Взятие на грудь и <w>толчок</w> — это сложное олимпийское упражнение."
                            }
                        },
                        {
                            "text": "He failed to complete the <w>jerk</w> portion of the lift.",
                            "target_lang_translations": {
                                "ru": "Ему не удалось выполнить фазу <w>толчка</w> в этом упражнении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clean and jerk"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "sports terminology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Движение в тяжелой атлетике, при котором вес поднимается с плеч над головой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толчок",
                                "target_lang_sense_clarification": "спортивное упражнение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b3d238ba-bc1e-33e8-b34b-2a251292e72e",
                    "sense_definition": "To move or make something move with a sudden, short, and uncontrolled motion.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sudden_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car <w>jerked</w> forward and then the engine died.",
                            "target_lang_translations": {
                                "ru": "Машина <w>дёрнулась</w> вперед, и двигатель заглох."
                            }
                        },
                        {
                            "text": "His head <w>jerked</w> up when he heard the loud noise.",
                            "target_lang_translations": {
                                "ru": "Его голова <w>дёрнулась</w> вверх, когда он услышал громкий шум."
                            }
                        }
                    ],
                    "synonyms": [
                        "twitch",
                        "jolt",
                        "lurch"
                    ],
                    "common_phrases": [
                        "jerk awake",
                        "jerk around"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться или заставлять что-то двигаться резким, коротким и неконтролируемым движением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дёргаться",
                                "target_lang_sense_clarification": "совершать резкие движения"
                            },
                            {
                                "target_lang_word": "дёрнуться",
                                "target_lang_sense_clarification": "сделать одно резкое движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad030779-cfc9-3ba7-8a01-afe58b1e6fea",
                    "sense_definition": "To pull or twist something with a quick, sharp movement.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sudden_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>jerked</w> the door open and stormed out of the room.",
                            "target_lang_translations": {
                                "ru": "Он <w>рванул</w> дверь на себя и вылетел из комнаты."
                            }
                        },
                        {
                            "text": "She <w>jerked</w> her hand away from the hot stove.",
                            "target_lang_translations": {
                                "ru": "Она <w>отдёрнула</w> руку от горячей плиты."
                            }
                        }
                    ],
                    "synonyms": [
                        "yank",
                        "tug",
                        "wrench"
                    ],
                    "common_phrases": [
                        "jerk open",
                        "jerk away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тянуть или крутить что-то быстрым, резким движением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дёрнуть",
                                "target_lang_sense_clarification": "резко потянуть"
                            },
                            {
                                "target_lang_word": "рвануть",
                                "target_lang_sense_clarification": "сильно и резко дёрнуть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "493e39fd-1bf5-3338-9404-5d1ed741b453",
                    "sense_definition": "To preserve meat by cutting it into long, thin strips and drying it in the sun or over a fire.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hunters would <w>jerk</w> the venison to ensure they had food for the winter.",
                            "target_lang_translations": {
                                "ru": "Охотники <w>вялили</w> оленину, чтобы запастись едой на зиму."
                            }
                        },
                        {
                            "text": "They learned how to <w>jerk</w> beef to make it last longer on the trail.",
                            "target_lang_translations": {
                                "ru": "Они научились <w>вялить</w> говядину, чтобы она дольше хранилась в пути."
                            }
                        }
                    ],
                    "synonyms": [
                        "cure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Консервировать мясо, нарезая его длинными тонкими полосками и высушивая на солнце или огне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вялить",
                                "target_lang_sense_clarification": "сушить мясо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
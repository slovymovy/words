    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0ee9aa0a-0439-30e1-9b34-627b0d0e9113",
                    "sense_definition": "In a way that produces a successful result or a desired effect.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new marketing strategy helped us reach our target audience more <w>effectively</w>.",
                            "target_lang_translations": {
                                "ru": "Новая маркетинговая стратегия помогла нам более <w>эффективно</w> охватить нашу целевую аудиторию."
                            }
                        },
                        {
                            "text": "She communicated <w>effectively</w> with the team to ensure everyone understood the plan.",
                            "target_lang_translations": {
                                "ru": "Она <w>эффективно</w> взаимодействовала с командой, чтобы убедиться, что все поняли план."
                            }
                        }
                    ],
                    "synonyms": [
                        "efficiently",
                        "successfully",
                        "productively",
                        "competently"
                    ],
                    "antonyms": [
                        "ineffectively",
                        "poorly",
                        "badly"
                    ],
                    "common_phrases": [
                        "work effectively",
                        "communicate effectively",
                        "deal effectively",
                        "manage effectively"
                    ],
                    "target_lang_definitions": {
                        "ru": "Таким образом, который приводит к успешному результату или желаемому эффекту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эффективно",
                                "target_lang_sense_clarification": "результативно"
                            },
                            {
                                "target_lang_word": "действенно",
                                "target_lang_sense_clarification": "оказывая нужное воздействие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c769a58-c3d9-36b1-b190-dcc9fb2a67bd",
                    "sense_definition": "Used to describe what is actually true in practice, even if it is not officially stated.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Although he has no official title, he <w>effectively</w> runs the entire department.",
                            "target_lang_translations": {
                                "ru": "Хотя у него нет официальной должности, он <w>фактически</w> руководит всем отделом."
                            }
                        },
                        {
                            "text": "The heavy rain meant that the match was <w>effectively</w> over before it began.",
                            "target_lang_translations": {
                                "ru": "Сильный дождь означал, что матч был <w>по сути</w> закончен, не успев начаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "practically",
                        "essentially",
                        "virtually",
                        "basically"
                    ],
                    "antonyms": [
                        "theoretically",
                        "officially",
                        "technically"
                    ],
                    "common_phrases": [
                        "effectively the same",
                        "effectively over",
                        "effectively banned"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для описания того, что происходит на самом деле, даже если это не закреплено официально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фактически",
                                "target_lang_sense_clarification": "на деле"
                            },
                            {
                                "target_lang_word": "по сути",
                                "target_lang_sense_clarification": "в сущности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dd041760-9374-3014-ad04-80f8194b88e0",
                    "sense_definition": "Płomienie, światło i ciepło powstające w procesie spalania.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_fire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W kominku wesoło trzaskał <ogień>, ogrzewając cały salon.",
                            "target_lang_translations": {
                                "ru": "В камине весело трещал <w>огонь</w>, согревая всю гостиную."
                            }
                        },
                        {
                            "text": "Harcerze zebrali drewno, aby rozpalić wieczorem duży <ogień>.",
                            "target_lang_translations": {
                                "ru": "Скауты собрали дрова, чтобы разжечь вечером большой <w>костёр</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "płomień",
                        "żar",
                        "blask"
                    ],
                    "antonyms": [
                        "woda",
                        "lód"
                    ],
                    "common_phrases": [
                        "siedzieć przy ogniu",
                        "dokładać do ognia",
                        "bać się jak ognia",
                        "nie ma dymu bez ognia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пламя, свет и тепло, возникающие в процессе горения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "огонь",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "пламя",
                                "target_lang_sense_clarification": "видимая часть огня"
                            },
                            {
                                "target_lang_word": "костёр",
                                "target_lang_sense_clarification": "специально разведенный огонь на открытом воздухе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0214335-2423-3b5d-a97d-e565386e400a",
                    "sense_definition": "Przedmiot lub źródło płomienia służące do zapalenia czegoś, na przykład papierosa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "lighter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przepraszam, czy ma pan może <ogień>, żeby przypalić?",
                            "target_lang_translations": {
                                "ru": "Извините, у вас не найдется <w>огонька</w>, чтобы прикурить?"
                            }
                        },
                        {
                            "text": "Chciałem zapalić świeczkę na torcie, ale nikt nie miał <ognia>.",
                            "target_lang_translations": {
                                "ru": "Я хотел зажечь свечку на торте, но ни у кого не было <w>огня</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć ogień",
                        "prosić o ogień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет или источник пламени для прикуривания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "огонёк",
                                "target_lang_sense_clarification": "разговорное, часто используется в просьбе прикурить"
                            },
                            {
                                "target_lang_word": "огонь",
                                "target_lang_sense_clarification": "источник пламени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21372432-0fa8-3230-9eb4-f8147327cb19",
                    "sense_definition": "Żywioł niszczący budynki, lasy lub inne obiekty; wielki pożar.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_fire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary drewniany dom w kilka minut stanął w <ogniu>.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный дом за несколько минут был охвачен <w>огнём</w>."
                            }
                        },
                        {
                            "text": "Strażacy przez wiele godzin walczyli z <ogniem>, aby uratować las.",
                            "target_lang_translations": {
                                "ru": "Пожарные много часов боролись с <w>огнём</w>, чтобы спасти лес."
                            }
                        }
                    ],
                    "synonyms": [
                        "pożar"
                    ],
                    "common_phrases": [
                        "trawić ogniem",
                        "stanąć w ogniu",
                        "pastwa ognia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стихия, уничтожающая здания, леса или другие объекты; большой пожар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожар",
                                "target_lang_sense_clarification": "бедствие, неконтролируемое горение"
                            },
                            {
                                "target_lang_word": "огонь",
                                "target_lang_sense_clarification": "разрушительная стихия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17715bca-a80b-31cf-b326-46be09962dbd",
                    "sense_definition": "Strzelanie z broni palnej w kierunku nieprzyjaciela.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dowódca wydał rozkaz, aby natychmiast otworzyć <ogień> do celu.",
                            "target_lang_translations": {
                                "ru": "Командир отдал приказ немедленно открыть <w>огонь</w> по цели."
                            }
                        },
                        {
                            "text": "Żołnierze znaleźli się w krzyżowym <ogniu> i musieli szukać osłony.",
                            "target_lang_translations": {
                                "ru": "Солдаты оказались под перекрёстным <w>огнём</w> и были вынуждены искать укрытие."
                            }
                        }
                    ],
                    "synonyms": [
                        "ostrzał",
                        "kanonada",
                        "strzał"
                    ],
                    "common_phrases": [
                        "otworzyć ogień",
                        "wstrzymać ogień",
                        "linia ognia",
                        "ogień zaporowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стрельба из огнестрельного оружия по противнику."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "огонь",
                                "target_lang_sense_clarification": "военный термин"
                            },
                            {
                                "target_lang_word": "стрельба",
                                "target_lang_sense_clarification": "процесс произведения выстрелов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6731b6ca-55ac-31ee-b0a7-ee97368db98b",
                    "sense_definition": "Bardzo silne emocje, wielki zapał, energia lub namiętność.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody pianista grał z takim <ogniem>, że publiczność wstała z miejsc.",
                            "target_lang_translations": {
                                "ru": "Молодой пианист играл с таким <w>огнём</w>, что публика встала с мест."
                            }
                        },
                        {
                            "text": "W jej oczach widać było <ogień> i determinację do osiągnięcia celu.",
                            "target_lang_translations": {
                                "ru": "В её глазах был виден <w>огонь</w> и решимость достичь цели."
                            }
                        }
                    ],
                    "synonyms": [
                        "namiętność",
                        "pasja",
                        "żar"
                    ],
                    "antonyms": [
                        "chłód",
                        "beznamiętność"
                    ],
                    "common_phrases": [
                        "słomiany ogień",
                        "bawić się z ogniem",
                        "ogień w oczach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень сильные эмоции, большой пыл, энергия или страсть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пыл",
                                "target_lang_sense_clarification": "душевный подъем, горячность"
                            },
                            {
                                "target_lang_word": "страсть",
                                "target_lang_sense_clarification": "сильное чувство"
                            },
                            {
                                "target_lang_word": "огонь",
                                "target_lang_sense_clarification": "метафорическое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07c33d6f-69bb-37dc-ab76-b423f3257161",
                    "sense_definition": "Zaczerwienienie twarzy lub uczucie gorąca spowodowane silnymi emocjami lub chorobą.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "body_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyna poczuła <ogień> na policzkach, gdy usłyszała ten komplement.",
                            "target_lang_translations": {
                                "ru": "Девушка почувствовала <w>жар</w> на щеках, когда услышала этот комплимент."
                            }
                        },
                        {
                            "text": "Chore dziecko trawił wewnętrzny <ogień>, a leki nie pomagały.",
                            "target_lang_translations": {
                                "ru": "Больного ребёнка сжигал внутренний <w>жар</w>, и лекарства не помогали."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypiek",
                        "gorączka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покраснение лица или чувство жара, вызванное эмоциями или болезнью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жар",
                                "target_lang_sense_clarification": "ощущение высокой температуры или лихорадка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "cf139eac-b76d-3d7e-b791-e4dc8d64970b",
                    "sense_definition": "Przeanalizować coś dokładnie, głęboko zastanowić się nad czymś, biorąc pod uwagę różne aspekty i konsekwencje przed podjęciem decyzji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_analysis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz dokładnie <rozważyć> wszystkie za i przeciw, zanim podejmiesz ostateczną decyzję.",
                            "target_lang_translations": {
                                "ru": "Ты должен тщательно <взвесить> все за и против, прежде чем примешь окончательное решение."
                            }
                        },
                        {
                            "text": "Zarząd firmy obiecał <rozważyć> naszą propozycję współpracy w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Правление компании обещало <рассмотреть> наше предложение о сотрудничестве на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "przemyśleć",
                        "rozpatrzeć"
                    ],
                    "antonyms": [
                        "zignorować",
                        "zlekceważyć"
                    ],
                    "common_phrases": [
                        "rozważyć propozycję",
                        "rozważyć za i przeciw",
                        "rozważyć opcję",
                        "rozważyć możliwość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщательно проанализировать, глубоко обдумать что-либо, учитывая различные аспекты и последствия перед принятием решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обдумать",
                                "target_lang_sense_clarification": "всесторонне взвесить мысленно"
                            },
                            {
                                "target_lang_word": "рассмотреть",
                                "target_lang_sense_clarification": "изучить, обсудить (например, предложение)"
                            },
                            {
                                "target_lang_word": "взвесить",
                                "target_lang_sense_clarification": "оценить доводы (например, за и против)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8739b589-6f32-3565-9561-95a11f64fce2",
                    "sense_definition": "Podzielić większą ilość towaru lub substancji na mniejsze porcje, sprawdzając ich ciężar na wadze.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "measuring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ekspedientka <rozważyła> cukierki do kilku mniejszych torebek dla dzieci.",
                            "target_lang_translations": {
                                "ru": "Продавщица <развесила> конфеты по нескольким маленьким пакетикам для детей."
                            }
                        },
                        {
                            "text": "Kupiłam duży kawałek mięsa i <rozważyłam> go na równe porcje do zamrożenia.",
                            "target_lang_translations": {
                                "ru": "Я купила большой кусок мяса и <развесила> его на равные порции для заморозки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rozważyć towar",
                        "rozważyć na porcje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разделить большое количество товара или вещества на меньшие порции, проверяя их вес на весах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развесить",
                                "target_lang_sense_clarification": "разделить на части по весу"

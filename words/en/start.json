    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4868ce2c-3999-3499-9d3d-02b88f1574b4",
                    "sense_definition": "To begin an activity, process, or event; to bring something into existence.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "begin_commence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>start</w> the meeting right now.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>начать</w> встречу прямо сейчас."
                            }
                        },
                        {
                            "text": "She decided to <w>start</w> a new hobby this year.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>заняться</w> новым хобби в этом году."
                            }
                        },
                        {
                            "text": "The rain <w>started</w> early in the morning.",
                            "target_lang_translations": {
                                "ru": "Дождь <w>начался</w> рано утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "begin",
                        "jump"
                    ],
                    "antonyms": [
                        "stop",
                        "end",
                        "finish",
                        "cease",
                        "conclude",
                        "complete"
                    ],
                    "common_phrases": [
                        "start a business",
                        "start over",
                        "start a family",
                        "start to cry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать деятельность, процесс или событие; создавать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начинать",
                                "target_lang_sense_clarification": "приступать к действию (несов. вид)"
                            },
                            {
                                "target_lang_word": "начать",
                                "target_lang_sense_clarification": "приступить к действию (сов. вид)"
                            },
                            {
                                "target_lang_word": "начинаться",
                                "target_lang_sense_clarification": "о событиях или явлениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9f5d4e7-2a45-392e-81fe-69e0bf03b5c2",
                    "sense_definition": "To cause a machine, vehicle, or engine to begin working.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "machine_operate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I could not <w>start</w> the car because the battery was dead.",
                            "target_lang_translations": {
                                "ru": "Я не смог <w>завести</w> машину, потому что сел аккумулятор."
                            }
                        },
                        {
                            "text": "Please press the green button to <w>start</w> the machine.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, нажмите зеленую кнопку, чтобы <w>запустить</w> станок."
                            }
                        }
                    ],
                    "antonyms": [
                        "stop"
                    ],
                    "common_phrases": [
                        "start the car",
                        "start the engine",
                        "won't start"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить машину, транспортное средство или двигатель в действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заводить",
                                "target_lang_sense_clarification": "о машине или двигателе"
                            },
                            {
                                "target_lang_word": "запускать",
                                "target_lang_sense_clarification": "о механизмах или программах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51def858-16c8-3c6c-8074-b2d6d6768547",
                    "sense_definition": "To have a specific beginning point in time or space.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "origin_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hiking trail <w>starts</w> near the old wooden bridge.",
                            "target_lang_translations": {
                                "ru": "Туристическая тропа <w>начинается</w> у старого деревянного моста."
                            }
                        },
                        {
                            "text": "The movie <w>starts</w> at eight o'clock in the evening.",
                            "target_lang_translations": {
                                "ru": "Фильм <w>начинается</w> в восемь часов вечера."
                            }
                        }
                    ],
                    "synonyms": [
                        "begin"
                    ],
                    "antonyms": [
                        "end"
                    ],
                    "common_phrases": [
                        "start at",
                        "start from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь определенную начальную точку во времени или пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начинаться",
                                "target_lang_sense_clarification": "брать начало"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3edc2380-220c-3f53-aa2b-ae43e118a62c",
                    "sense_definition": "To make a sudden involuntary movement due to surprise, fear, or pain.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sudden_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loud noise made the cat <w>start</w> and run away.",
                            "target_lang_translations": {
                                "ru": "От громкого шума кошка <w>вздрогнула</w> и убежала."
                            }
                        },
                        {
                            "text": "He <w>started</w> when the door suddenly slammed shut.",
                            "target_lang_translations": {
                                "ru": "Он <w>вздрогнул</w>, когда дверь внезапно захлопнулась."
                            }
                        }
                    ],
                    "synonyms": [
                        "jump"
                    ],
                    "common_phrases": [
                        "start with surprise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать резкое непроизвольное движение от удивления, страха или боли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вздрагивать",
                                "target_lang_sense_clarification": "от неожиданности"
                            },
                            {
                                "target_lang_word": "дёрнуться",
                                "target_lang_sense_clarification": "резко двинуться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cb3b910-f2e3-351a-97c2-6258443d9d0e",

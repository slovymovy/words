            "senses": [
                {
                    "sense_id": "f09ac978-7589-3620-83e3-0c0f30d9e6c4",
                    "sense_definition": "Zdrobnienie od słowa <w>malina</w> (mały, jadalny owoc w kolorze różowym lub czerwonym).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fruit_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja mama robi pyszny deser z leśnymi <w>malinkami</w>.",
                            "target_lang_translations": {
                                "ru": "Моя мама готовит вкусный десерт с лесными <w>малинками</w>."
                            }
                        },
                        {
                            "text": "Wnusiu, chodź tu, dziadziuś nazbierał ci miseczkę pysznych <w>malinek</w>.",
                            "target_lang_translations": {
                                "ru": "Внучек, иди сюда, дедушка набрал тебе мисочку вкусных <w>малинок</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "słodkie malinki",
                        "zbierać malinki"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова <w>малина</w> (маленькая съедобная ягода розового или красного цвета)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "малинка",
                                "target_lang_sense_clarification": "ягода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac0e8776-b912-34ba-8e26-0eff887e1d2f",
                    "sense_definition": "Czerwony ślad lub siniak na skórze, zazwyczaj na szyi, który powstaje w wyniku mocnego, ssącego pocałunku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "skin_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jacek zrobił Adzie widoczną <w>malinkę</w> na dekolcie.",
                            "target_lang_translations": {
                                "ru": "Яцек поставил Аде заметный <w>засос</w> в зоне декольте."
                            }
                        },
                        {
                            "text": "Musiała założyć golf, żeby ukryć <w>malinkę</w> przed rodzicami.",
                            "target_lang_translations": {
                                "ru": "Ей пришлось надеть водолазку, чтобы скрыть <w>засос</w> от родителей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zrobić malinkę",
                        "mieć malinkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Красный след или синяк на коже, обычно на шее, возникающий в результате сильного поцелуя взасос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засос",
                                "target_lang_sense_clarification": "след от поцелуя"
                            }
                        ]
                    }
                }
            ]
        }
    ]

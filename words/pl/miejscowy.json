            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "87cec876-1f14-3da1-9e26-09b7359e613f",
                    "sense_definition": "Związany z danym miejscem, pochodzący z określonej okolicy lub w niej występujący.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej restauracji serwują tylko <w>miejscowe</w> produkty.",
                            "target_lang_translations": {
                                "ru": "В этом ресторане подают только <w>местные</w> продукты."
                            }
                        },
                        {
                            "text": "Musimy dostosować się do <w>miejscowego</w> czasu, żeby nie spóźnić się na spotkanie.",
                            "target_lang_translations": {
                                "ru": "Мы должны подстроиться под <w>местное</w> время, чтобы не опоздать на встречу."
                            }
                        }
                    ],
                    "synonyms": [
                        "lokalny",
                        "tutejszy"
                    ],
                    "antonyms": [
                        "zamiejscowy"
                    ],
                    "common_phrases": [
                        "czas miejscowy",
                        "samorząd miejscowy",
                        "rynek miejscowy",
                        "ludność miejscowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с данным местом, происходящий из определённой местности или встречающийся в ней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "местный",
                                "target_lang_sense_clarification": "относящийся к данной территории"
                            },
                            {
                                "target_lang_word": "здешний",
                                "target_lang_sense_clarification": "находящийся здесь, не приезжий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7631f7f2-18b1-31d8-89f4-e31fe942ac81",
                    "sense_definition": "Dotyczący ograniczonego obszaru, na przykład fragmentu ciała; nieobejmujący całości.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "scope_limited",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zdecydował się na znieczulenie <w>miejscowe</w> zamiast ogólnego.",
                            "target_lang_translations": {
                                "ru": "Врач решил применить <w>местную</w> анестезию вместо общей."
                            }
                        },
                        {
                            "text": "To tylko <w>miejscowy</w> stan zapalny, który szybko ustąpi po zastosowaniu maści.",
                            "target_lang_translations": {
                                "ru": "Это лишь <w>местное</w> воспаление, которое быстро пройдёт после применения мази."
                            }
                        }
                    ],
                    "synonyms": [
                        "częściowy",
                        "lokalny"
                    ],
                    "antonyms": [
                        "rozległy"
                    ],
                    "common_phrases": [
                        "znieczulenie miejscowe",
                        "ból miejscowy",
                        "zasięg miejscowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Касающийся ограниченной области, например, части тела; не охватывающий целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "местный",
                                "target_lang_sense_clarification": "ограниченный определенным участком (напр. в медицине)"
                            },
                            {
                                "target_lang_word": "локальный",
                                "target_lang_sense_clarification": "сосредоточенный в одном месте, не общий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fde4e14d-0544-3d15-943d-48a80e88c147",
                    "sense_definition": "Stały mieszkaniec danej miejscowości lub okolicy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zapytałem jednego z <w>miejscowych</w> o drogę do najbliższego sklepu.",
                            "target_lang_translations": {
                                "ru": "Я спросил одного из <w>местных</w> дорогу к ближайшему магазину."
                            }
                        },
                        {
                            "text": "<w>Miejscowi</w> są zazwyczaj bardzo pomocni dla turystów odwiedzających ich region.",
                            "target_lang_translations": {
                                "ru": "<w>Местные</w> обычно очень отзывчивы к туристам, посещающим их регион."
                            }
                        }
                    ],
                    "synonyms": [
                        "tutejszy"
                    ],
                    "antonyms": [
                        "nietutejszy"
                    ],
                    "common_phrases": [
                        "rozmowa z miejscowym",
                        "jeden z miejscowych",
                        "rdzenny miejscowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянный житель данной местности или округи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "местный",
                                "target_lang_sense_clarification": "житель данной местности (субстантивированное прилагательное)"
                            },
                            {
                                "target_lang_word": "местный житель",
                                "target_lang_sense_clarification": "человек, проживающий в данной местности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d375d6b0-4263-3e57-82f5-d99a18fa96d7",
                    "sense_definition": "Круглая деталь машины или механизма, которая вращается вокруг оси и служит для движения.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У моего велосипеда спустило переднее <w>колесо</w>, и мне пришлось идти пешком.",
                            "target_lang_translations": {
                                "en": "My bicycle's front <w>wheel</w> went flat, and I had to walk."
                            }
                        },
                        {
                            "text": "Водитель быстро заменил пробитое <w>колесо</w> на запасное и продолжил путь.",
                            "target_lang_translations": {
                                "en": "The driver quickly replaced the punctured <w>wheel</w> with a spare and continued the journey."
                            }
                        }
                    ],
                    "common_phrases": [
                        "запасное колесо",
                        "рулевое колесо",
                        "зубчатое колесо",
                        "пятое колесо"
                    ],
                    "target_lang_definitions": {
                        "en": "A round part of a machine or mechanism that rotates around an axis and serves for movement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wheel",
                                "target_lang_sense_clarification": "part of a vehicle or machine"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9a1fc38-78f5-3bfb-84eb-064a790785df",
                    "sense_definition": "Большой аттракцион в парках в виде вращающегося круга с кабинками для обзора окрестностей.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "entertainment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В парке мы решили прокатиться на <w>колесе</w> обозрения, чтобы увидеть весь город.",
                            "target_lang_translations": {
                                "en": "In the park, we decided to ride the <w>Ferris wheel</w> to see the whole city."
                            }
                        },
                        {
                            "text": "Это огромное <w>колесо</w> медленно поднимает посетителей на большую высоту.",
                            "target_lang_translations": {
                                "en": "This huge <w>wheel</w> slowly lifts visitors to a great height."
                            }
                        }
                    ],
                    "common_phrases": [
                        "колесо обозрения",
                        "прокатиться на колесе"
                    ],
                    "target_lang_definitions": {
                        "en": "A large attraction in parks in the form of a rotating circle with cabins for viewing the surroundings."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Ferris wheel",
                                "target_lang_sense_clarification": "amusement ride"
                            },
                            {
                                "target_lang_word": "wheel",
                                "target_lang_sense_clarification": "short for Ferris wheel"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42b1507d-9cb8-33cf-9a5f-60869e940e5c",
                    "sense_definition": "Автомобиль или другое транспортное средство.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Теперь я снова на <w>колёсах</w>, поэтому могу подвезти тебя до дома.",
                            "target_lang_translations": {
                                "en": "Now I'm back on <w>wheels</w>, so I can give you a lift home."
                            }
                        },
                        {
                            "text": "Жить за городом без машины сложно, там обязательно нужно быть на <w>колёсах</w>.",
                            "target_lang_translations": {
                                "en": "Living out of town without a car is hard; you definitely need to be on <w>wheels</w> there."
                            }
                        }
                    ],
                    "synonyms": [
                        "автомобиль"
                    ],
                    "common_phrases": [
                        "быть на колёсах",
                        "остаться без колёс"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "usually used in the phrase 'на колёсах'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A car or other vehicle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wheels",
                                "target_lang_sense_clarification": "slang for a car"
                            },
                            {

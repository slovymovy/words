    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e8c876ff-ab76-377b-8f0b-ca81b7b447af",
                    "sense_definition": "Действия, связанные с использованием сверхъестественных сил; колдовство, магия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В детстве мы все искренне верили в <w>волшебство</w> и чудеса.",
                            "target_lang_translations": {
                                "en": "In childhood, we all sincerely believed in <w>magic</w> and miracles."
                            }
                        },
                        {
                            "text": "Добрая фея использовала своё <w>волшебство</w>, чтобы помочь героям сказки.",
                            "target_lang_translations": {
                                "en": "The kind fairy used her <w>magic</w> to help the fairy tale heroes."
                            }
                        }
                    ],
                    "synonyms": [
                        "колдовство",
                        "магия",
                        "чародейство"
                    ],
                    "common_phrases": [
                        "верить в волшебство",
                        "настоящее волшебство",
                        "творить волшебство"
                    ],
                    "target_lang_definitions": {
                        "en": "Actions related to the use of supernatural powers; witchcraft, magic."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "magic",
                                "target_lang_sense_clarification": "supernatural power"
                            },
                            {
                                "target_lang_word": "sorcery",
                                "target_lang_sense_clarification": "witchcraft"
                            },
                            {
                                "target_lang_word": "wizardry",
                                "target_lang_sense_clarification": "magical skill"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92dd1496-a1d3-3cf1-916e-868239446df1",
                    "sense_definition": "Чарующее, завораживающее действие или свойство чего-либо, вызывающее восхищение.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зрители в зале ощутили настоящее <w>волшебство</w> музыки.",
                            "target_lang_translations": {
                                "en": "The audience in the hall felt the true <w>magic</w> of the music."
                            }
                        },
                        {
                            "text": "Мы наслаждались тишиной и <w>волшебством</w> зимнего леса.",
                            "target_lang_translations": {
                                "en": "We enjoyed the silence and <w>enchantment</w> of the winter forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "магия"
                    ],
                    "common_phrases": [
                        "волшебство музыки",
                        "волшебство звуков",
                        "волшебство любви"
                    ],
                    "target_lang_definitions": {
                        "en": "A charming, mesmerizing effect or quality of something that evokes admiration."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "magic",
                                "target_lang_sense_clarification": "metaphorical charm"
                            },
                            {
                                "target_lang_word": "enchantment",
                                "target_lang_sense_clarification": "captivating quality"
                            }
                        ]
                    }
                }
            ]
        }
    ]

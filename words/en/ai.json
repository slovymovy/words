                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ai</w>, let's go to the cinema if you are ready.",
                            "target_lang_translations": {
                                "ru": "<w>Ай</w>, пошли в кино, если ты готов."
                            }

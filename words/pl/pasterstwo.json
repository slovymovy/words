                            {
                                "target_lang_word": "скотоводство",
                                "target_lang_sense_clarification": "отрасль хозяйства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "633c48f1-1b1a-3222-a585-bd2e9833505f",
                    "sense_definition": "Wypełnianie posługi religijnej przez biskupa lub kapłana; opieka nad wiernymi.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rel_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego wieloletnie <w>pasterstwo</w> przyniosło diecezji wiele pozytywnych zmian.",

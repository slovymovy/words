    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3527c88a-27aa-3e7b-9d6b-74503d94fc36",
                    "sense_definition": "A contest for a prize or award.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "contest_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The newspaper is featuring a <w>competition</w> to win a car.",
                            "target_lang_translations": {
                                "ru": "В газете проводится <w>конкурс</w>, в котором можно выиграть автомобиль."
                            }
                        },
                        {
                            "text": "She decided to enter a singing <w>competition</w> at her school.",
                            "target_lang_translations": {
                                "ru": "Она решила принять участие в певческом <w>конкурсе</w> в своей школе."
                            }
                        }
                    ],
                    "synonyms": [
                        "contest",
                        "tournament",
                        "championship"
                    ],
                    "common_phrases": [
                        "enter a competition",
                        "win a competition",
                        "talent competition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мероприятие, в котором участники борются за приз или награду"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соревнова́ние",
                                "target_lang_sense_clarification": "Спортивное или общее состязание"
                            },
                            {
                                "target_lang_word": "ко́нкурс",
                                "target_lang_sense_clarification": "Состязание за приз, в искусстве или отбор на должность"
                            },
                            {
                                "target_lang_word": "состяза́ние",
                                "target_lang_sense_clarification": "Спортивная борьба или поединок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "202cab8d-666d-3e2e-a6ff-8ebb82e8ffdc",
                    "sense_definition": "The action of competing.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "act_of_competing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>competition</w> for this job is strong.",
                            "target_lang_translations": {
                                "ru": "<w>Конкуренция</w> за эту работу очень высока."
                            }
                        },
                        {
                            "text": "There is fierce <w>competition</w> between the two large companies.",
                            "target_lang_translations": {
                                "ru": "Между двумя крупными компаниями идет жесткая <w>конкуренция</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rivalry"
                    ],
                    "antonyms": [
                        "cooperation"
                    ],
                    "common_phrases": [
                        "stiff competition",
                        "fierce competition",
                        "in competition with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс соперничества, борьбы за достижение лучших результатов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конкуре́нция",
                                "target_lang_sense_clarification": "Соперничество в экономике, бизнесе или за ресурсы"
                            },
                            {
                                "target_lang_word": "сопе́рничество",
                                "target_lang_sense_clarification": "Борьба за первенство или превосходство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97079ce5-afc0-394f-b53b-1529f30a28a4",
                    "sense_definition": "The competitors in such a contest.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "competitors_collective",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new stain remover was ten times more effective than the <w>competition</w>.",
                            "target_lang_translations": {
                                "ru": "Новый пятновыводитель оказался в десять раз эффективнее <w>конкурентов</w>."
                            }
                        },
                        {
                            "text": "We need to stay ahead of the <w>competition</w> to succeed in this market.",
                            "target_lang_translations": {
                                "ru": "Нам нужно опережать <w>конкурентов</w>, чтобы преуспеть на этом рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "opposition",
                        "rivals"
                    ],
                    "common_phrases": [
                        "beat the competition",
                        "ahead of the competition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участники соревнования, соперники или конкуренты как группа"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конкуре́нты",
                                "target_lang_sense_clarification": "Люди или компании, с которыми ведется борьба"
                            },
                            {
                                "target_lang_word": "сопе́рники",
                                "target_lang_sense_clarification": "Противники в состязании"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
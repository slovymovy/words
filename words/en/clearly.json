{
    "word_family": [
        "clear",
        "clarity",
        "unclear",
        "clearness"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4253b384-f07d-3c18-adcd-ebafed18575a",
                    "sense_definition": "In a way that is easy to see, hear, or understand.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please speak <w>clearly</w> so that everyone can understand you.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, говорите <w>ясно</w>, чтобы все могли вас понять."
                            }
                        },
                        {
                            "text": "I cannot see the road <w>clearly</w> because of the heavy rain.",
                            "target_lang_translations": {
                                "ru": "Я не вижу дорогу <w>чётко</w> из-за сильного дождя."
                            }
                        }
                    ],
                    "synonyms": [
                        "unambiguously",
                        "explicitly"
                    ],
                    "common_phrases": [
                        "speak clearly",
                        "see clearly",
                        "think clearly",
                        "write clearly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Так, что это легко увидеть, услышать или понять; отчётливо и разборчиво."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ясно",
                                "target_lang_sense_clarification": "понятно, недвусмысленно"
                            },
                            {
                                "target_lang_word": "чётко",
                                "target_lang_sense_clarification": "разборчиво, резко (о зрении или речи)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dafa2904-8780-34d6-92c4-ac9245674a8b",
                    "sense_definition": "Used to emphasize that something is obvious, certain, or without doubt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>clearly</w> the best player on the team.",
                            "target_lang_translations": {
                                "ru": "Она <w>явно</w> лучший игрок в команде."
                            }
                        },
                        {
                            "text": "The instructions were <w>clearly</w> wrong about the installation process.",
                            "target_lang_translations": {
                                "ru": "Инструкции были <w>очевидно</w> неверными насчёт процесса установки."
                            }
                        }
                    ],
                    "synonyms": [
                        "obviously"
                    ],
                    "common_phrases": [
                        "clearly visible",
                        "clearly wrong",
                        "clearly defined",
                        "quite clearly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для подчеркивания того, что факт является очевидным, несомненным или бесспорным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очевидно",
                                "target_lang_sense_clarification": "несомненно, безусловно"
                            },
                            {
                                "target_lang_word": "явно",
                                "target_lang_sense_clarification": "открыто, заметно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

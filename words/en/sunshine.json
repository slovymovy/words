                },
                {
                    "sense_id": "6ccafade-7f65-335d-8118-b32e045d9903",
                    "sense_definition": "A form of address used affectionately, ironically, or to someone who has just woken up.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Good morning, <w>sunshine</w>! Did you sleep well?",
                            "target_lang_translations": {
                                "ru": "Доброе утро, <w>солнышко</w>! Ты хорошо спал?"
                            }
                        },
                        {
                            "text": "Listen to me, <w>sunshine</w>, you need to be more careful next time.",
                            "target_lang_translations": {
                                "ru": "Послушай меня, <w>солнышко</w>, в следующий раз тебе нужно быть осторожнее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "good morning sunshine",
                        "hello sunshine"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used ironically or affectionately"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое или ироничное обращение к кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "со́лнышко",
                                "target_lang_sense_clarification": "обращение"
                            }
                        ]
                    }
                }
            ]
        },

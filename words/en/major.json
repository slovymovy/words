                    "synonyms": [
                        "course"
                    ],
                    "common_phrases": [
                        "declare a major",
                        "double major"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Основной предмет или курс обучения, выбранный студентом в колледже или университете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "специа́льность",
                                "target_lang_sense_clarification": "основное направление обучения"
                            },
                            {
                                "target_lang_word": "профили́рующая дисципли́на",
                                "target_lang_sense_clarification": "официальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3412f45f-15e5-317d-8898-2c0a3277a424",
                    "sense_definition": "A university student who is specializing in a specific subject.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "academic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As a history <w>major</w>, he spends a lot of time in the library.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>студе́нтом</w> исторического факультета, он проводит много времени в библиотеке."
                            }
                        },
                        {
                            "text": "The physics <w>majors</w> are organizing a science fair this weekend.",
                            "target_lang_translations": {
                                "ru": "<w>Студе́нты</w>-физики организуют научную ярмарку в эти выходные."
                            }
                        }
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "67c4478c-b3fe-3adf-887b-1f669755dd91",
                    "sense_definition": "Umowa międzynarodowa regulująca określone zagadnienia prawne, polityczne lub społeczne między państwami.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta <w>konwencja</w> nie narusza żadnych praw człowieka.",
                            "target_lang_translations": {
                                "ru": "Эта <w>конвенция</w> не нарушает никаких прав человека."
                            }
                        },
                        {
                            "text": "Polska ratyfikowała <w>konwencję</w> dotyczącą ochrony środowiska w zeszłym roku.",
                            "target_lang_translations": {
                                "ru": "Польша ратифицировала <w>конвенцию</w> по охране окружающей среды в прошлом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "traktat"
                    ],
                    "common_phrases": [
                        "konwencja genewska",
                        "podpisać konwencję",
                        "ratyfikować konwencję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Международный договор, регулирующий определенные правовые, политические или социальные вопросы между государствами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конвенция",
                                "target_lang_sense_clarification": "международный договор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec868324-d83f-3d7e-b228-ffc3e7bf3100",
                    "sense_definition": "Zjazd delegatów partii politycznej lub innej organizacji w celu ustalenia programu lub wyboru władz.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas ostatniej <w>konwencji</w> partia wybrała nowego przewodniczącego.",
                            "target_lang_translations": {
                                "ru": "Во время последнего <w>съезда</w> партия избрала нового председателя."
                            }
                        },
                        {
                            "text": "Wielka <w>konwencja</w> wyborcza przyciągnęła tłumy zwolenników i dziennikarzy.",
                            "target_lang_translations": {
                                "ru": "Большой предвыборный <w>съезд</w> привлек толпы сторонников и журналистов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "konwencja wyborcza",
                        "konwencja partyjna",
                        "krajowa konwencja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собрание делегатов политической партии или другой организации для утверждения программы или выборов руководства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "съезд",
                                "target_lang_sense_clarification": "собрание делегатов"
                            },
                            {
                                "target_lang_word": "конференция",
                                "target_lang_sense_clarification": "партийная встреча"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae505f7f-399a-3d61-b27f-e97387a712f1",
                    "sense_definition": "Ogólnie przyjęte w danym środowisku normy zachowania, zwyczaje lub reguły postępowania.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego swobodne zachowanie łamało wszelkie przyjęte w towarzystwie <w>konwencje</w>.",
                            "target_lang_translations": {
                                "ru": "Его свободное поведение нарушало все принятые в обществе <w>условности</w>."
                            }
                        },
                        {
                            "text": "To spotkanie odbyło się bez zbędnej <w>konwencji</w> i oficjalnych przemówień.",
                            "target_lang_translations": {
                                "ru": "Эта встреча прошла без лишних <w>формальностей</w> и официальных речей."
                            }
                        }
                    ],
                    "synonyms": [
                        "konwenans",
                        "zwyczaj"
                    ],
                    "common_phrases": [
                        "łamać konwencje",
                        "przyjęta konwencja",
                        "społeczna konwencja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общепринятые в данной среде нормы поведения, обычаи или правила действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "условность",
                                "target_lang_sense_clarification": "общественная норма"
                            },
                            {
                                "target_lang_word": "формальность",
                                "target_lang_sense_clarification": "этикет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21e142ca-6072-3a08-878b-1a1972964310",
                    "sense_definition": "Zespół cech charakterystycznych dla dzieł artystycznych, przyjętych przez twórcę lub epokę.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Reżyser zrealizował ten spektakl w nowoczesnej <w>konwencji</w> teatru absurdu.",
                            "target_lang_translations": {
                                "ru": "Режиссер поставил этот спектакль в современной <w>стилистике</w> театра абсурда."
                            }
                        },
                        {
                            "text": "Powieść utrzymana jest w <w>konwencji</w> klasycznego kryminału z lat trzydziestych.",
                            "target_lang_translations": {
                                "ru": "Роман выдержан в <w>стиле</w> классического детектива тридцатых годов."
                            }
                        }
                    ],
                    "synonyms": [
                        "styl"
                    ],
                    "common_phrases": [
                        "konwencja literacka",
                        "konwencja teatralna",
                        "w konwencji żartu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность характерных черт художественных произведений, принятых автором или эпохой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стилистика",
                                "target_lang_sense_clarification": "художественный стиль"
                            },
                            {
                                "target_lang_word": "манера",
                                "target_lang_sense_clarification": "способ исполнения"
                            },
                            {
                                "target_lang_word": "жанр",
                                "target_lang_sense_clarification": "категория произведения"
                            }
                        ]
                    }
                }
            ]
        }

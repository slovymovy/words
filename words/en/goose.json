            "senses": [
                {
                    "sense_id": "d154a93e-c5bb-3028-b2a9-0392b041a48b",
                    "sense_definition": "To poke or pinch someone's buttocks unexpectedly.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>goosed</w> him as a joke while he was cooking.",
                            "target_lang_translations": {
                                "ru": "Она в шутку <w>ущипнула</w> его, пока он готовил."
                            }
                        },
                        {
                            "text": "He complained that someone had <w>goosed</w> him in the crowd.",
                            "target_lang_translations": {
                                "ru": "Он пожаловался, что кто-то <w>ущипнул</w> его в толпе."

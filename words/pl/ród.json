                },
                {
                    "sense_id": "e0931a64-94e7-3982-a975-82aa391c1bd4",
                    "sense_definition": "Grupa roślin wyhodowanych jako potomstwo pojedynczej rośliny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy <w>ród</w> pszenicy jest wyjątkowo odporny na suszę.",
                            "target_lang_translations": {
                                "ru": "Эта новая <w>линия</w> пшеницы исключительно устойчива к засухе."
                            }

            "senses": [
                {
                    "sense_id": "1dfe6034-16b3-3b12-8434-0e9f73cd8cb2",
                    "sense_definition": "Być żywym, funkcjonować jako organizm biologiczny; przeciwieństwo bycia martwym.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "biological_existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek <żył> dziewięćdziesiąt lat i cieszył się dobrym zdrowiem.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка <жил> девяносто лет и отличался хорошим здоровьем."
                            }
                        },
                        {
                            "text": "Niektóre gatunki żółwi mogą <żyć> nawet ponad sto lat.",
                            "target_lang_translations": {
                                "ru": "Некоторые виды черепах могут <жить> даже более ста лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "istnieć",
                        "egzystować"
                    ],
                    "antonyms": [
                        "umrzeć"
                    ],
                    "common_phrases": [
                        "chcieć żyć",
                        "niech żyje",
                        "żyć wiecznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть живым, функционировать как биологический организм; противоположность состоянию смерти."

{
    "word_family": [
        "modeler",
        "modeling",
        "remodel",
        "supermodel"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "541b9bc4-5b85-35a3-8904-c91916fcf0cd",
                    "sense_definition": "A person who serves as a human template for artwork or fashion.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "person_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist asked the <w>model</w> to sit still for another hour.",
                            "target_lang_translations": {
                                "ru": "Художник попросил <w>модель</w> сидеть неподвижно еще час."
                            }
                        },
                        {
                            "text": "She started her career as a fashion <w>model</w> in Paris.",
                            "target_lang_translations": {
                                "ru": "Она начала свою карьеру как <w>модель</w> в Париже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fashion model",
                        "top model",
                        "male model"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который позирует для произведений искусства или демонстрирует одежду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модель",
                                "target_lang_sense_clarification": "в моде или искусстве"
                            },
                            {
                                "target_lang_word": "натурщик",
                                "target_lang_sense_clarification": "для художников"
                            },
                            {
                                "target_lang_word": "манекенщица",
                                "target_lang_sense_clarification": "демонстратор одежды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a744be41-012a-30ad-a482-a23bf79beb79",
                    "sense_definition": "A style, type, or design of a product.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "product_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This is the latest <w>model</w> of their most popular car.",
                            "target_lang_translations": {
                                "ru": "Это последняя <w>модель</w> их самого популярного автомобиля."
                            }
                        },
                        {
                            "text": "We need to order parts for an older <w>model</w> of the washing machine.",
                            "target_lang_translations": {
                                "ru": "Нам нужно заказать запчасти для старой <w>модели</w> стиральной машины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "latest model",
                        "new model",
                        "car model",
                        "base model"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стиль, тип или конструкция изделия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модель",
                                "target_lang_sense_clarification": "тип изделия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb02c225-c0f4-3d00-b050-19b049fe818f",
                    "sense_definition": "A representation of a physical object, often smaller than the original.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The architect showed us a scale <w>model</w> of the proposed building.",
                            "target_lang_translations": {
                                "ru": "Архитектор показал нам масштабный <w>макет</w> предлагаемого здания."
                            }
                        },
                        {
                            "text": "He spent the weekend building a plastic <w>model</w> of an airplane.",
                            "target_lang_translations": {
                                "ru": "Он провел выходные, собирая пластиковую <w>модель</w> самолета."
                            }
                        }
                    ],
                    "synonyms": [
                        "template"
                    ],
                    "common_phrases": [
                        "scale model",
                        "clay model",
                        "build a model"
                    ],
                    "target_lang_definitions": {
                        "ru": "Копия или схема физического объекта, обычно в уменьшенном размере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "макет",
                                "target_lang_sense_clarification": "архитектурный или пробный"
                            },
                            {
                                "target_lang_word": "модель",
                                "target_lang_sense_clarification": "копия объекта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "246599d5-71b0-3fc9-bd49-d91e97ae932c",
                    "sense_definition": "A successful example to be copied or a standard of excellence.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exemplar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a <w>model</w> of patience and kindness for her children.",
                            "target_lang_translations": {
                                "ru": "Она — <w>образец</w> терпения и доброты для своих детей."
                            }
                        },
                        {
                            "text": "The teacher used his essay as a <w>model</w> for the rest of the class.",
                            "target_lang_translations": {
                                "ru": "Учитель использовал его эссе как <w>образец</w> для остального класса."
                            }
                        }
                    ],
                    "synonyms": [
                        "ideal",
                        "example",
                        "exemplar",
                        "paradigm",
                        "paragon",
                        "pattern",
                        "epitome",
                        "archetype"
                    ],
                    "antonyms": [
                        "antimodel"
                    ],
                    "common_phrases": [
                        "role model",
                        "model of virtue",
                        "model of consistency"
                    ],
                    "target_lang_definitions": {
                        "ru": "Успешный пример для подражания или стандарт совершенства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "образец",
                                "target_lang_sense_clarification": "пример для подражания"
                            },
                            {
                                "target_lang_word": "эталон",
                                "target_lang_sense_clarification": "мерило, стандарт"
                            },
                            {
                                "target_lang_word": "пример",
                                "target_lang_sense_clarification": "достойный случай"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe5cfc6c-5958-37b2-b3f0-4d02665471e0",
                    "sense_definition": "A simulation or theoretical description used to analyze a system or event.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "simulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists developed a computer <w>model</w> to predict climate change.",
                            "target_lang_translations": {
                                "ru": "Ученые разработали компьютерную <w>модель</w> для прогнозирования изменения климата."
                            }
                        },
                        {
                            "text": "The economic <w>model</w> suggests that inflation will decrease next year.",
                            "target_lang_translations": {
                                "ru": "Экономическая <w>модель</w> предполагает, что инфляция снизится в следующем году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "computer model",
                        "mathematical model",
                        "weather model"
                    ],
                    "target_lang_definitions": {
                        "ru": "Симуляция или теоретическое описание, используемое для анализа системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модель",
                                "target_lang_sense_clarification": "теоретическая или компьютерная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cb2962e-6c0f-3afb-8897-c0a1e0e1ae27",
                    "sense_definition": "The structural design or plan of a complex system.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "simulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company adopted a new business <w>model</w> to increase profits.",
                            "target_lang_translations": {
                                "ru": "Компания приняла новую бизнес-<w>модель</w> для увеличения прибыли."
                            }
                        },
                        {
                            "text": "Their subscription <w>model</w> has proven to be very successful.",
                            "target_lang_translations": {
                                "ru": "Их <w>модель</w> подписки оказалась очень успешной."
                            }
                        }
                    ],
                    "synonyms": [
                        "template"
                    ],
                    "common_phrases": [
                        "business model",
                        "subscription model"
                    ],
                    "target_lang_definitions": {
                        "ru": "Структурный план или схема сложной системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модель",
                                "target_lang_sense_clarification": "схема организации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45705443-ecf4-3070-85a4-e65d8d645166",
                    "sense_definition": "An organism used to study a biological process or disease.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "scientific",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Researchers used a mouse <w>model</w> to test the new drug.",
                            "target_lang_translations": {
                                "ru": "Исследователи использовали мышиную <w>модель</w> для тестирования нового лекарства."
                            }
                        },
                        {
                            "text": "The fruit fly is a common <w>model</w> for genetic studies.",
                            "target_lang_translations": {
                                "ru": "Плодовая мушка — распространенная <w>модель</w> для генетических исследований."
                            }
                        }
                    ],
                    "synonyms": [
                        "model organism"
                    ],
                    "common_phrases": [
                        "mouse model",
                        "animal model"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "scientific"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Организм, используемый для изучения биологических процессов или болезней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "модель",
                                "target_lang_sense_clarification": "объект исследования"
                            },
                            {
                                "target_lang_word": "модельный организм",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0ae8673f-527d-360e-b799-6dede49cc3dd",
                    "sense_definition": "To work as a model in art or fashion.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "work_fashion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She used to <w>model</w> for a famous agency before she became an actress.",
                            "target_lang_translations": {
                                "ru": "Раньше она <w>работала моделью</w> в известном агентстве, прежде чем стать актрисой."
                            }
                        },
                        {
                            "text": "He has been <w>modeling</w> professionally for over five years.",
                            "target_lang_translations": {
                                "ru": "Он <w>работает моделью</w> профессионально уже более пяти лет."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Работать в качестве модели в искусстве или моде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работать моделью",
                                "target_lang_sense_clarification": "профессия"
                            },
                            {
                                "target_lang_word": "позировать",
                                "target_lang_sense_clarification": "для художника/фотографа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59ced003-2f26-3a1d-b089-3177511ccac6",
                    "sense_definition": "To display clothing or items on one's body for others to see.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "work_fashion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The runway stars <w>modelled</w> the latest summer collection.",
                            "target_lang_translations": {
                                "ru": "Звезды подиума <w>демонстрировали</w> последнюю летнюю коллекцию."
                            }
                        },
                        {
                            "text": "She <w>modelled</w> the new shoes for her friends to see how they looked.",
                            "target_lang_translations": {
                                "ru": "Она <w>демонстрировала</w> новые туфли своим друзьям, чтобы они увидели, как они смотрятся."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Демонстрировать одежду или предметы на себе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демонстрировать",
                                "target_lang_sense_clarification": "показывать одежду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b42d478a-b0bc-3a13-b767-9cff0e0ad8cb",
                    "sense_definition": "To devise a representation or simulation of something to understand it better.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "create_simulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>modelled</w> the weather patterns using a powerful supercomputer.",
                            "target_lang_translations": {
                                "ru": "Они <w>смоделировали</w> погодные условия с помощью мощного суперкомпьютера."
                            }
                        },
                        {
                            "text": "The software allows us to <w>model</w> the effects of traffic on the bridge.",
                            "target_lang_translations": {
                                "ru": "Программное обеспечение позволяет нам <w>смоделировать</w> воздействие транспорта на мост."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать представление или симуляцию чего-либо для изучения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моделировать",
                                "target_lang_sense_clarification": "создавать модель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddfaff05-421e-3829-927f-3e4a539a4bdb",
                    "sense_definition": "To create a shape or form from a substance such as clay.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sculpt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sculptor <w>modelled</w> the clay into the shape of a horse.",
                            "target_lang_translations": {
                                "ru": "Скульптор <w>вылепил</w> из глины фигуру лошади."
                            }
                        },
                        {
                            "text": "Children learn to <w>model</w> figures using soft dough.",
                            "target_lang_translations": {
                                "ru": "Дети учатся <w>лепить</w> фигурки из мягкого теста."
                            }
                        }
                    ],
                    "common_phrases": [
                        "model clay"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать форму из пластичного материала, например, глины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лепить",
                                "target_lang_sense_clarification": "из глины, теста"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3427bf0f-31f0-374e-994e-ec3147395bf2",
                    "sense_definition": "Worthy of being a model; exemplary.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "exemplary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was a <w>model</w> student who always finished his homework on time.",
                            "target_lang_translations": {
                                "ru": "Он был <w>примерным</w> студентом, который всегда вовремя делал домашнее задание."
                            }
                        },
                        {
                            "text": "The farm is considered a <w>model</w> example of sustainable agriculture.",
                            "target_lang_translations": {
                                "ru": "Эта ферма считается <w>образцовым</w> примером устойчивого сельского хозяйства."
                            }
                        }
                    ],
                    "synonyms": [
                        "ideal",
                        "exemplar",
                        "paradigm",
                        "pattern"
                    ],
                    "common_phrases": [
                        "model student",
                        "model citizen",
                        "model behavior"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достойный быть примером; образцовый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "образцовый",
                                "target_lang_sense_clarification": "примерный"
                            },
                            {
                                "target_lang_word": "примерный",
                                "target_lang_sense_clarification": "достойный подражания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "b129989e-ef99-3c5f-952b-7e319a9c9c07",
                    "sense_definition": "A barangay of the city of Puerto Princesa, Philippines.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We visited the community in <w>Model</w> during our trip to Palawan.",
                            "target_lang_translations": {
                                "ru": "Мы посетили общину в <w>Моделе</w> во время нашей поездки на Палаван."
                            }
                        },
                        {
                            "text": "The local government of <w>Model</w> announced new regulations.",
                            "target_lang_translations": {
                                "ru": "Местное правительство <w>Модела</w> объявило о новых правилах."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Барангай города Пуэрто-Принцеса, Филиппины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Модел",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00391860-07b8-308d-afa8-7e898baf00ef",
                    "sense_definition": "Grass that has been cut and dried to be used as food for animals.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_fodder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The horses are eating fresh <w>hay</w> in the stable.",
                            "target_lang_translations": {
                                "ru": "Лошади едят свежее <w>сено</w> в конюшне."
                            }
                        },
                        {
                            "text": "Farmers must make <w>hay</w> while the weather is dry and sunny.",
                            "target_lang_translations": {
                                "ru": "Фермеры должны заготавливать <w>сено</w>, пока погода сухая и солнечная."
                            }
                        }
                    ],
                    "synonyms": [
                        "fodder",
                        "forage"
                    ],
                    "common_phrases": [
                        "make hay",
                        "bale of hay",
                        "hit the hay"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скошенная и высушенная трава, используемая как корм для животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сено",
                                "target_lang_sense_clarification": "сухая трава для корма"
                            }
                        ]
                    }
                },
                {

        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b5fb1f98-1ec7-33fe-915f-8b94b8036551",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>vossen</w>.",
                    "learner_level": "B1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>vos</w> liever niet met de regels van het spel.",
                            "target_lang_translations": {
                                "en": "I prefer not to <w>mess</w> with the rules of the game.",
                                "ru": "Я предпочитаю не <w>хитрить</w> с правилами игры."
                            }
                        },
                        {
                            "text": "Als ik <w>vos</w>, bedoel ik dat ik aan het jagen ben.",
                            "target_lang_translations": {
                                "en": "When I <w>fox hunt</w>, I mean that I am hunting.",
                                "ru": "Когда я <w>охочусь на лис</w>, я имею в виду, что я на охоте."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'vossen'.",
                        "ru": "Первое лицо единственного числа настоящего времени глагола 'vossen' (охотиться на лис или хитрить)."
                    },
                    "translations": {

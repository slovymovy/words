                    "sense_id": "5c019277-56b2-3734-8756-d8219638c5f4",
                    "sense_definition": "Subiektywnie pozbawiony piękna lub wartości estetycznych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary budynek jest bardzo <w>brzydki</w> i wymaga natychmiastowego remontu.",
                            "target_lang_translations": {
                                "ru": "Это старое здание очень <w>некрасивое</w> и требует немедленного ремонта."
                            }
                        },
                        {
                            "text": "Nie kupuj tej sukienki, bo ma wyjątkowo <w>brzydki</w> kolor.",
                            "target_lang_translations": {
                                "ru": "Не покупай это платье, потому что у него исключительно <w>некрасивый</w> цвет."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieładny",
                        "nieestetyczny",
                        "obrzydliwy"
                    ],
                    "antonyms": [

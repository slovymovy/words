{
    "word_family": [
        "naam",
        "noemen",
        "benaming",
        "bijnaam",
        "voornaam",
        "achternaam",
        "naamgeving"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "623974c3-2e21-3ef9-b945-fe557b20ef0c",
                    "sense_definition": "Met die naam; zo genoemd (vaak gebruikt om een specifieke term te introduceren).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "neutral_naming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is een <w>zogenaamde</w> slimme meter die het energieverbruik meet.",
                            "target_lang_translations": {
                                "en": "This is a <w>so-called</w> smart meter that measures energy consumption."
                            }
                        },
                        {
                            "text": "De <w>zogenaamde</w> gulden regel is in veel culturen bekend.",
                            "target_lang_translations": {
                                "en": "The <w>so-called</w> golden rule is known in many cultures."
                            }
                        }
                    ],
                    "synonyms": [
                        "zogeheten"
                    ],
                    "common_phrases": [
                        "een zogenaamde expert",
                        "de zogenaamde feiten"
                    ],
                    "target_lang_definitions": {
                        "en": "With that name; named as such (often used to introduce a specific term)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "so-called",
                                "target_lang_sense_clarification": "used to introduce a specific name or term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44cb7057-24a7-3243-b2d4-ea75f4f5f2eb",
                    "sense_definition": "Ten onrechte zo genoemd; niet werkelijk wat de naam suggereert (drukt twijfel of ironie uit).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "skeptical_naming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>zogenaamde</w> vrienden lieten hem vallen toen hij geen geld meer had.",
                            "target_lang_translations": {
                                "en": "His <w>so-called</w> friends dropped him when he ran out of money."
                            }
                        },
                        {
                            "text": "Ik geloof niets van die <w>zogenaamde</w> verbeteringen in het systeem.",
                            "target_lang_translations": {
                                "en": "I don't believe any of those <w>so-called</w> improvements to the system."
                            }
                        }
                    ],
                    "synonyms": [
                        "zogeheten"
                    ],
                    "antonyms": [
                        "echt",
                        "werkelijk"
                    ],
                    "common_phrases": [
                        "zogenaamde vrienden",
                        "een zogenaamde oplossing"
                    ],
                    "target_lang_definitions": {
                        "en": "Wrongly named; not truly what the name suggests (expresses doubt or irony)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "so-called",
                                "target_lang_sense_clarification": "used ironically to imply the name is inaccurate"
                            },
                            {
                                "target_lang_word": "supposed",
                                "target_lang_sense_clarification": "claimed to be true but doubtful"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a75b8adc-bdb0-3a86-a4ea-0d2a6523208a",
                    "sense_definition": "Volgens een bewering die vaak niet geloofd wordt; alsof.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "supposedly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was <w>zogenaamd</w> ziek, maar ik zag hem gewoon in de stad lopen.",
                            "target_lang_translations": {
                                "en": "He was <w>supposedly</w> sick, but I just saw him walking in the city."
                            }
                        },
                        {
                            "text": "Ze had <w>zogenaamd</w> geen tijd om mijn berichtje te lezen.",
                            "target_lang_translations": {
                                "en": "She <w>supposedly</w> didn't have time to read my message."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zogenaamd vergeten",
                        "zogenaamd druk"
                    ],
                    "target_lang_definitions": {
                        "en": "According to a claim that is often not believed; as if."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "supposedly",
                                "target_lang_sense_clarification": "according to what is generally assumed or claimed"
                            },
                            {
                                "target_lang_word": "ostensibly",
                                "target_lang_sense_clarification": "apparently or purportedly, but perhaps not actually"
                            },
                            {
                                "target_lang_word": "allegedly",
                                "target_lang_sense_clarification": "used when something is claimed to be the case but not proven"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "semantic_group_id": "form_pl",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Небесные <w>мощи</w> в древних текстах иногда описываются как ангельские чины.",
                            "target_lang_translations": {
                                "en": "Celestial <w>powers</w> are sometimes described in ancient texts as angelic orders."
                            }

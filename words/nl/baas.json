        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3dec80c5-9d2e-30f4-b49a-0b1405740dbf",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>bazen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>baas</w> maar wat rond zonder echt doel.",
                            "target_lang_translations": {
                                "en": "I just <w>boss around</w> without a real goal.",
                                "ru": "Я просто <w>командую</w> тут без какой-либо цели."
                            }
                        },
                        {
                            "text": "Soms <w>baas</w> ik over dingen waar ik geen verstand van heb.",
                            "target_lang_translations": {
                                "en": "Sometimes I <w>rant</w> about things I know nothing about.",
                                "ru": "Иногда я <w>распоряжаюсь</w> вещами, в которых не разбираюсь."
                            }
                        }

                    "target_lang_definitions": {
                        "ru": "Снабжать кого-либо или что-либо необходимым оборудованием или одеждой для определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снаряжать",
                                "target_lang_sense_clarification": "снабжать всем необходимым"
                            },
                            {
                                "target_lang_word": "экипировать",
                                "target_lang_sense_clarification": "снабжать снаряжением"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "00ef910f-8b07-38b9-a6af-19fda0e3325e",
                    "sense_definition": "Dotyczący początkowego etapu nauki, edukacji lub najprostszych elementów wiedzy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja młodsza siostra chodzi do szkoły <w>podstawowej</w>.",
                            "target_lang_translations": {
                                "ru": "Моя младшая сестра ходит в <w>начальную</w> школу."
                            }
                        },
                        {
                            "text": "Znam tylko <w>podstawowe</w> zwroty w języku hiszpańskim.",
                            "target_lang_translations": {
                                "ru": "Я знаю только <w>базовые</w> фразы на испанском языке."
                            }
                        }
                    ],
                    "synonyms": [
                        "elementarny",
                        "fundamentalny",
                        "rudymentarny"
                    ],
                    "antonyms": [
                        "zaawansowany",
                        "wyższy"
                    ],
                    "common_phrases": [
                        "szkoła podstawowa",
                        "poziom podstawowy",
                        "wiedza podstawowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к начальному этапу обучения или самым простым элементам знаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "базовый",
                                "target_lang_sense_clarification": "о знаниях или навыках"
                            },
                            {
                                "target_lang_word": "начальный",
                                "target_lang_sense_clarification": "об уровне образования"
                            },
                            {
                                "target_lang_word": "элементарный",
                                "target_lang_sense_clarification": "простейший"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93f1fbc1-c1f4-3989-b990-b93c9fb57dff",
                    "sense_definition": "Stanowiący fundament, bazę lub najważniejszą część czegoś; taki, bez którego coś nie może istnieć.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bezpieczeństwo pasażerów jest <w>podstawowym</w> priorytetem naszej firmy.",
                            "target_lang_translations": {
                                "ru": "Безопасность пассажиров является <w>основным</w> приоритетом нашей компании."
                            }
                        },
                        {
                            "text": "Woda jest <w>podstawowym</w> składnikiem niezbędnym do życia.",
                            "target_lang_translations": {
                                "ru": "Вода является <w>основным</w> компонентом, необходимым для жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "zasadniczy",
                        "fundamentalny",
                        "fundamentalny",
                        "zasadniczy"
                    ],
                    "antonyms": [
                        "drugorzędny",
                        "poboczny",
                        "dodatkowy"
                    ],
                    "common_phrases": [

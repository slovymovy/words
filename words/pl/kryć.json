    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7c5f3eee-4732-35d2-b422-cfe8e761f03a",
                    "sense_definition": "Umieszczać kogoś lub coś w miejscu niewidocznym dla innych; chować w bezpiecznym miejscu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hiding_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodziej próbował <kryć> skradzione pieniądze w starej piwnicy.",
                            "target_lang_translations": {
                                "ru": "Вор пытался <прятать> украденные деньги в старом подвале."
                            }
                        },
                        {
                            "text": "Matka instynktownie chciała <kryć> swoje dziecko przed niebezpieczeństwem.",
                            "target_lang_translations": {
                                "ru": "Мать инстинктивно хотела <укрывать> своего ребенка от опасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "chować",
                        "ukrywać"
                    ],
                    "common_phrases": [
                        "kryć twarz",
                        "kryć w dłoniach",
                        "kryć przed światem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать кого-либо или что-либо в место, невидимое для других; прятать в безопасном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прятать",
                                "target_lang_sense_clarification": "класть в укромное место"
                            },
                            {
                                "target_lang_word": "скрывать",
                                "target_lang_sense_clarification": "делать невидимым"
                            },
                            {
                                "target_lang_word": "укрывать",
                                "target_lang_sense_clarification": "прятать для защиты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d026df5-263b-3afd-a1ee-a8b1cbd680c8",
                    "sense_definition": "Przebywać w ukryciu, chować się, aby nie zostać zauważonym (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hiding_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musieliśmy <kryć się> w bramie przed ulewnym deszczem.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <прятаться> в подворотне от проливного дождя."
                            }
                        },
                        {
                            "text": "Zbiegły więzień musiał <kryć się> w lesie przez wiele dni.",
                            "target_lang_translations": {
                                "ru": "Беглый заключенный должен был <скрываться> в лесу много дней."
                            }
                        }
                    ],
                    "synonyms": [
                        "chować się",
                        "ukrywać się",
                        "skrywać się"
                    ],
                    "common_phrases": [
                        "kryć się po kątach",
                        "kryć się przed deszczem",
                        "nie mam co kryć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в укрытии, прятаться, чтобы не быть замеченным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прятаться",
                                "target_lang_sense_clarification": "уходить в укрытие"
                            },
                            {
                                "target_lang_word": "скрываться",
                                "target_lang_sense_clarification": "избегать обнаружения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a28677b-c276-367a-9e34-6ca7e6ead73c",
                    "sense_definition": "Nie ujawniać swoich myśli, uczuć, zamiarów lub posiadanych informacji; zachowywać coś w tajemnicy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "hiding_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna starał się <kryć> swoje zdenerwowanie sztucznym uśmiechem.",
                            "target_lang_translations": {
                                "ru": "Мужчина старался <скрывать> свою нервозность за искусственной улыбкой."
                            }
                        },
                        {
                            "text": "Nie powinieneś <kryć> prawdy przed rodziną, bo to tylko pogorszy sytuację.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <скрывать> правду от семьи, так как это только ухудшит ситуацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "taić",
                        "ukrywać"
                    ],
                    "common_phrases": [
                        "kryć urazę",
                        "kryć emocje",
                        "kryć prawdę",
                        "nie ma co kryć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не раскрывать своих мыслей, чувств, намерений или информации; хранить что-либо в тайне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрывать",
                                "target_lang_sense_clarification": "не разглашать, утаивать"
                            },
                            {
                                "target_lang_word": "таить",
                                "target_lang_sense_clarification": "хранить в секрете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf42af88-5246-3bd1-bc0d-dd44f924cb84",
                    "sense_definition": "Pokrywać powierzchnię warstwą materiału, farby lub innej substancji; stanowić wierzchnią warstwę czegoś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "covering_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy zaczęli <kryć> dach czerwoną dachówką wczesnym rankiem.",
                            "target_lang_translations": {
                                "ru": "Рабочие начали <крыть> крышу красной черепицей рано утром."
                            }
                        },
                        {
                            "text": "Ta tania farba jest słabej jakości i nie chce dokładnie <kryć> plam na ścianie.",
                            "target_lang_translations": {
                                "ru": "Эта дешевая краска плохого качества и не <закрашивает> пятна на стене полностью."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokrywać"
                    ],
                    "common_phrases": [
                        "kryć dach",
                        "dobrze kryć",
                        "słabo kryć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать поверхность слоем материала, краски или другого вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крыть",
                                "target_lang_sense_clarification": "покрывать крышу или наносить слой"
                            },
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "наносить покрытие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "253fbe9c-b8bc-31f2-8b51-cc4b9e23f97b",
                    "sense_definition": "Chronić kogoś przed odpowiedzialnością lub karą, często poprzez zatajanie jego winy lub kłamstwo.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "protecting_blame",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Skorumpowany urzędnik przez lata próbował <kryć> swoich nieuczciwych wspólników.",
                            "target_lang_translations": {
                                "ru": "Коррумпированный чиновник годами пытался <покрывать> своих нечестных сообщников."
                            }
                        },
                        {
                            "text": "Nie będę cię dłużej <kryć>, musisz sam ponieść konsekwencje swoich czynów.",
                            "target_lang_translations": {
                                "ru": "Я не буду тебя больше <покрывать>, ты должен сам понести последствия своих поступков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kryć kolegę",
                        "kryć sprawcę",
                        "wzajemnie się kryć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защищать кого-либо от ответственности или наказания, часто скрывая его вину или обманывая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "выгораживать кого-то"
                            },
                            {
                                "target_lang_word": "выгораживать",
                                "target_lang_sense_clarification": "защищать от обвинений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e2004fc-5e67-3202-8e46-3ab8440abd1c",
                    "sense_definition": "W grach zespołowych: pilnować zawodnika drużyny przeciwnej, utrudniając mu grę i przejęcie piłki.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trener nakazał obrońcy ściśle <kryć> najlepszego napastnika rywali.",
                            "target_lang_translations": {
                                "ru": "Тренер приказал защитнику плотно <опекать> лучшего нападающего соперников."
                            }
                        },
                        {
                            "text": "Musisz go <kryć> na całym boisku, żeby nie miał szans na strzał.",
                            "target_lang_translations": {
                                "ru": "Ты должен <держать> его по всему полю, чтобы у него не было шансов на удар."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kryć każdego",
                        "kryć strefą",
                        "kryć indywidualnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "В командных играх: плотно опекать игрока соперника, мешая ему принять мяч или сыграть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опекать",
                                "target_lang_sense_clarification": "спортивный термин"
                            },
                            {
                                "target_lang_word": "держать",
                                "target_lang_sense_clarification": "блокировать игрока"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe0fcd32-00a4-39ab-8a45-dab0fee40e36",
                    "sense_definition": "Być zawartym w czymś; stanowić ukrytą przyczynę, sens lub treść czegoś (zwykle z zaimkiem „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "containing_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zastanawiam się, jaka mroczna tajemnica może <kryć się> w historii tego zamku.",
                            "target_lang_translations": {
                                "ru": "Интересно, какая мрачная тайна может <скрываться> в истории этого замка."
                            }
                        },
                        {
                            "text": "Za jego dziwnym zachowaniem musi <kryć się> jakiś poważny problem osobisty.",
                            "target_lang_translations": {
                                "ru": "За его странным поведением должна <скрываться> какая-то серьезная личная проблема."
                            }
                        }
                    ],
                    "common_phrases": [
                        "co się za tym kryje",
                        "kryć w sobie",
                        "kryć tajemnicę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержаться в чем-либо; быть скрытой причиной или смыслом чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрываться",
                                "target_lang_sense_clarification": "быть неявным, подразумеваться"
                            },
                            {
                                "target_lang_word": "кроиться",
                                "target_lang_sense_clarification": "заключаться в чем-то"
                            },
                            {
                                "target_lang_word": "таиться",
                                "target_lang_sense_clarification": "содержаться внутри"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "027f8d5d-4b24-37ac-94af-38c2e5c050ef",
                    "sense_definition": "O samcach zwierząt: kopulować z samicą w celu jej zapłodnienia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_breeding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony ogier zaczął <kryć> klacze w stadninie tej wiosny.",
                            "target_lang_translations": {
                                "ru": "Опытный жеребец начал <крыть> кобыл на конном заводе этой весной."
                            }
                        },
                        {
                            "text": "Hodowca zdecydował, że ten byk będzie <kryć> tylko wybrane krowy.",
                            "target_lang_translations": {
                                "ru": "Заводчик решил, что этот бык будет <крыть> только избранных коров."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О самцах животных: спариваться с самкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крыть",
                                "target_lang_sense_clarification": "оплодотворять (о животных)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "prowadzić",
        "wprowadzać",
        "wyprowadzać",
        "doprowadzać",
        "odprowadzać",
        "przeprowadzać",
        "naprowadzać",
        "sprowadzenie",
        "sprowadzanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "64df962e-d7e7-3c37-be4a-142dffa936f8",
                    "sense_definition": "Sprawiać, że ktoś lub coś znajdzie się w danym miejscu, na przykład poprzez przywiezienie, przyprowadzenie lub zamówienie z innego kraju.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_import",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma planuje <w>sprowadzać</w> nowe maszyny z Niemiec, aby przyspieszyć produkcję.",
                            "target_lang_translations": {
                                "ru": "Фирма планирует <w>привозить</w> новые машины из Германии, чтобы ускорить производство."
                            }
                        },
                        {
                            "text": "Musimy szybko <w>sprowadzać</w> lekarza, ponieważ stan pacjenta się pogorszył.",
                            "target_lang_translations": {
                                "ru": "Мы должны быстро <w>привести</w> врача, так как состояние пациента ухудшилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywozić",
                        "przyprowadzać",
                        "importować",
                        "przynosić",
                        "transportować"
                    ],
                    "common_phrases": [
                        "sprowadzać towary z zagranicy",
                        "sprowadzać pomoc",
                        "sprowadzać rodzinę",
                        "sprowadzać samochody"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доставлять кого-то или что-то в определенное место, привозить или заказывать (например, из-за границы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привозить",
                                "target_lang_sense_clarification": "доставлять транспортом"
                            },
                            {
                                "target_lang_word": "импортировать",
                                "target_lang_sense_clarification": "ввозить товары из-за границы"
                            },
                            {
                                "target_lang_word": "приводить",
                                "target_lang_sense_clarification": "сопровождать кого-то пешком"
                            },
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "просить прийти (например, врача)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2287134-a8ae-3c83-8783-915bdbad35e0",
                    "sense_definition": "Wprowadzać się do nowego miejsca zamieszkania (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowi sąsiedzi zaczęli <w>sprowadzać</w> się do domu obok w zeszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Новые соседи начали <w>заселяться</w> в дом рядом на прошлой неделе."
                            }
                        },
                        {
                            "text": "Młodzi ludzie wolą <w>sprowadzać</w> się do dużych miast w poszukiwaniu pracy.",
                            "target_lang_translations": {
                                "ru": "Молодые люди предпочитают <w>переезжать</w> в большие города в поисках работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "osiedlać się"
                    ],
                    "common_phrases": [
                        "sprowadzać się do miasta",
                        "sprowadzać się do nowego domu",
                        "sprowadzać się na wieś"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Переезжать на новое место жительства (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переезжать",
                                "target_lang_sense_clarification": "менять место жительства"
                            },
                            {
                                "target_lang_word": "заселяться",
                                "target_lang_sense_clarification": "въезжать в помещение"
                            },
                            {
                                "target_lang_word": "вселяться",
                                "target_lang_sense_clarification": "въезжать в жилье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ef646cc-8874-3db3-a466-0e2ee5940e45",
                    "sense_definition": "Pomagać komuś lub czemuś zejść z wyższego poziomu na niższy.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "transport_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ratownicy musieli ostrożnie <w>sprowadzać</w> turystów ze szczytu góry podczas burzy.",
                            "target_lang_translations": {
                                "ru": "Спасателям пришлось осторожно <w>спускать</w> туристов с вершины горы во время грозы."
                            }
                        },
                        {
                            "text": "Będę powoli <w>sprowadzać</w> rower po stromych schodach do piwnicy.",
                            "target_lang_translations": {
                                "ru": "Я буду медленно <w>спускать</w> велосипед по крутой лестнице в подвал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sprowadzać na parter",
                        "sprowadzać na dół",
                        "sprowadzać ze szczytu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помогать кому-то или чему-то спуститься с высоты вниз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спускать",
                                "target_lang_sense_clarification": "перемещать вниз объект или помогать спуститься"
                            },
                            {
                                "target_lang_word": "сводить",
                                "target_lang_sense_clarification": "вести кого-то вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1254921-e3bc-35d5-880a-d46e83cbda2f",
                    "sense_definition": "Ograniczać się do czegoś lub polegać na czymś w ostatecznym rozrachunku; być sednem sprawy (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "essence_simplification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała dyskusja zdaje się <w>sprowadzać</w> do kwestii braku pieniędzy w budżecie.",
                            "target_lang_translations": {
                                "ru": "Вся дискуссия, кажется, <w>сводится</w> к вопросу нехватки денег в бюджете."
                            }
                        },
                        {
                            "text": "Jego codzienna praca <w>sprowadza</w> się głównie do wypełniania dokumentów.",
                            "target_lang_translations": {
                                "ru": "Его ежедневная работа <w>сводится</w> в основном к заполнению документов."
                            }
                        }
                    ],
                    "synonyms": [
                        "ograniczać się",
                        "polegać"
                    ],
                    "common_phrases": [
                        "wszystko sprowadza się do",
                        "problem sprowadza się do",
                        "to się sprowadza do jednego"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограничиваться чем-то или заключаться в чем-то в конечном итоге (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сводиться",
                                "target_lang_sense_clarification": "заключаться в чем-то, иметь итогом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ba61f67-7e10-3dd1-bf97-6b6045b69e5e",
                    "sense_definition": "Być przyczyną czegoś negatywnego lub trudnego; ściągać coś na kogoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "causation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nieodpowiedzialne zachowanie może <w>sprowadzać</w> na nas poważne kłopoty.",
                            "target_lang_translations": {
                                "ru": "Его безответственное поведение может <w>навлечь</w> на нас серьезные неприятности."
                            }
                        },
                        {
                            "text": "Nie chcę <w>sprowadzać</w> na ciebie niebezpieczeństwa, więc lepiej tam nie idźmy.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <w>навлекать</w> на тебя опасность, поэтому лучше туда не пойдем."
                            }
                        }
                    ],
                    "synonyms": [
                        "powodować",
                        "wywoływać"
                    ],
                    "common_phrases": [
                        "sprowadzać nieszczęście",
                        "sprowadzać kłopoty",
                        "sprowadzać zagrożenie",
                        "sprowadzać na złą drogę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть причиной чего-то негативного; навлекать что-то на кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навлекать",
                                "target_lang_sense_clarification": "вызывать неприятности или опасность"
                            },
                            {
                                "target_lang_word": "приносить",
                                "target_lang_sense_clarification": "быть причиной (например, несчастья)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7140a890-bbee-30e5-9246-2315f1fd9623",
                    "sense_definition": "Przekształcać coś złożonego w formę prostszą lub ograniczać zagadnienie do jednego aspektu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "essence_simplification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel starał się <w>sprowadzać</w> skomplikowane teorie do prostych przykładów z życia.",
                            "target_lang_translations": {
                                "ru": "Учитель старался <w>сводить</w> сложные теории к простым примерам из жизни."
                            }
                        },
                        {
                            "text": "Nie można wszystkiego <w>sprowadzać</w> tylko do polityki, bo świat jest bardziej złożony.",
                            "target_lang_translations": {
                                "ru": "Нельзя всё <w>сводить</w> только к политике, потому что мир сложнее."
                            }
                        }
                    ],
                    "synonyms": [
                        "ograniczać"
                    ],
                    "common_phrases": [
                        "sprowadzać do wspólnego mianownika",
                        "sprowadzać do absurdu",
                        "sprowadzać do parteru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упрощать что-то сложное или ограничивать вопрос одним аспектом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сводить",
                                "target_lang_sense_clarification": "упрощать или приравнивать к чему-то"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
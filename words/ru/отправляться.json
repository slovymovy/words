{
    "word_family": [
        "отправлять",
        "отправление",
        "отправитель",
        "отправка"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c7601be6-1e92-3e75-98db-3e10a8db1465",
                    "sense_definition": "Уходить или уезжать, начиная путь куда-либо; приступать к перемещению в определённое место.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "start_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы завтра рано утром <отправляемся> в путешествие.",
                            "target_lang_translations": {
                                "en": "We are <w>setting off</w> on a trip early tomorrow morning."
                            }
                        },
                        {
                            "text": "Уже поздно, нам пора <отправляться> домой.",
                            "target_lang_translations": {
                                "en": "It's already late, it's time for us to <w>head</w> home."
                            }
                        }
                    ],
                    "synonyms": [
                        "направляться",
                        "уходить",
                        "уезжать"
                    ],
                    "antonyms": [
                        "прибывать",
                        "возвращаться",
                        "приходить"
                    ],
                    "common_phrases": [
                        "отправляться в путь",
                        "отправляться домой",
                        "отправляться спать"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave a place to begin a journey or go to a specific destination."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "set off",
                                "target_lang_sense_clarification": "start a journey"
                            },
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "go away from a place"
                            },
                            {
                                "target_lang_word": "head",
                                "target_lang_sense_clarification": "move towards a destination"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b18fde7-b57d-34ba-8ef9-709efff99d79",
                    "sense_definition": "Начинать движение, отъезжать от станции или остановки (о транспортных средствах).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "start_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Поезд <отправляется> со второй платформы через пять минут.",
                            "target_lang_translations": {
                                "en": "The train <w>departs</w> from the second platform in five minutes."
                            }
                        },
                        {
                            "text": "Автобус в аэропорт <отправляется> каждые полчаса.",
                            "target_lang_translations": {
                                "en": "The bus to the airport <w>leaves</w> every half hour."
                            }
                        }
                    ],
                    "synonyms": [
                        "отходить",
                        "отъезжать"
                    ],
                    "antonyms": [
                        "прибывать"
                    ],
                    "common_phrases": [
                        "поезд отправляется",
                        "автобус отправляется"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave a station or stop according to a schedule (referring to transport)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "depart",
                                "target_lang_sense_clarification": "leave a station"
                            },
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "start moving"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db558644-f71d-35f8-9c9c-0c786b86e360",
                    "sense_definition": "Страдательный залог к глаголу <w>отправлять</w>; быть посылаемым или пересылаемым куда-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вся почта <отправляется> в конце рабочего дня.",
                            "target_lang_translations": {
                                "en": "All mail <w>is sent</w> at the end of the workday."
                            }
                        },
                        {
                            "text": "Этот груз <отправляется> заказчику сразу после оплаты.",
                            "target_lang_translations": {
                                "en": "This cargo <w>is shipped</w> to the customer immediately after payment."
                            }
                        }
                    ],
                    "synonyms": [
                        "посылаться"
                    ],
                    "antonyms": [
                        "получаться",
                        "приниматься"
                    ],
                    "common_phrases": [
                        "письмо отправляется",
                        "груз отправляется"
                    ],
                    "target_lang_definitions": {
                        "en": "To be transmitted or transported to a destination (passive voice of 'to send')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be sent",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "be shipped",
                                "target_lang_sense_clarification": "goods or cargo"
                            },
                            {
                                "target_lang_word": "be dispatched",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
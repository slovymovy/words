                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ко́нчиться",
                                "target_lang_sense_clarification": "иссякнуть"
                            },
                            {
                                "target_lang_word": "зако́нчиться",
                                "target_lang_sense_clarification": "израсходоваться"

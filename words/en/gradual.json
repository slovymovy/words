    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "48751a69-4018-3f63-b9a6-b34b5a11057e",
                    "sense_definition": "Happening or changing slowly over a long period of time or in small stages rather than suddenly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There has been a <w>gradual</w> change in the climate over the last few decades.",
                            "target_lang_translations": {
                                "ru": "За последние несколько десятилетий произошло <w>постепенное</w> изменение климата."
                            }
                        },
                        {
                            "text": "The doctor was pleased with the patient's <w>gradual</w> recovery after the surgery.",
                            "target_lang_translations": {
                                "ru": "Врач был доволен <w>постепенным</w> выздоровлением пациента после операции."
                            }
                        }
                    ],
                    "synonyms": [
                        "slow",
                        "incremental",
                        "stepwise"
                    ],
                    "antonyms": [
                        "sudden",
                        "abrupt"
                    ],
                    "common_phrases": [
                        "gradual change",
                        "gradual increase",
                        "gradual process",
                        "gradual decline"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий или меняющийся медленно в течение длительного времени или небольшими этапами, а не внезапно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постепенный",
                                "target_lang_sense_clarification": "происходящий шаг за шагом, не сразу"
                            },
                            {
                                "target_lang_word": "плавный",
                                "target_lang_sense_clarification": "без резких скачков или переходов"
                            }
                        ]
                    }
                }
            ]
        },

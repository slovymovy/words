                        "ru": [
                            {
                                "target_lang_word": "отделал пико",
                                "target_lang_sense_clarification": "украсил край петельками (прошедшее время)"
                            },
                            {
                                "target_lang_word": "обвязал пико",
                                "target_lang_sense_clarification": "обработал край петельками (прошедшее время)"

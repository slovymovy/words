                        "sprawa rozwodowa",
                        "wnieść sprawę",
                        "wygrać sprawę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Судебное разбирательство или процесс по разрешению правового спора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дело",

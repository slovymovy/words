                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "в значении 'помимо, без участия'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Without someone's participation or knowledge; bypassing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bypassing",
                                "target_lang_sense_clarification": "avoiding or ignoring authority"
                            },
                            {
                                "target_lang_word": "past",
                                "target_lang_sense_clarification": "skipping over"
                            }
                        ]
                    }
                }
            ]
        }
    ]

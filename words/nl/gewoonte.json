                {
                    "sense_id": "99757399-831e-39c5-a28e-e52f98959205",
                    "sense_definition": "Iets wat je regelmatig doet, vaak zonder erbij na te denken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autorijden wordt heel snel een <w>gewoonte</w>.",
                            "target_lang_translations": {
                                "en": "Driving becomes a <w>habit</w> very quickly.",
                                "ru": "Вождение очень быстро становится <w>привычкой</w>."
                            }
                        },
                        {
                            "text": "Het is een goede <w>gewoonte</w> om elke dag te lezen.",
                            "target_lang_translations": {
                                "en": "It is a good <w>habit</w> to read every day.",
                                "ru": "Это хорошая <w>привычка</w> — читать каждый день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uit gewoonte",
                        "een gewoonte worden",
                        "slechte gewoonte",
                        "vaste gewoonte"
                    ],
                    "target_lang_definitions": {

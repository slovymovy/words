{
    "word_family": [
        "subsidy",
        "subsidize",
        "subsidization",
        "subsidizer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "475131d6-d558-3fad-8044-2dc5a34d9a15",
                    "sense_definition": "A company that is owned or controlled by another larger company, often referred to as the parent company.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The multinational corporation opened a new <w>subsidiary</w> in Japan to handle Asian sales.",
                            "target_lang_translations": {
                                "ru": "Транснациональная корпорация открыла новую <w>дочернюю компанию</w> в Японии для управления продажами в Азии."
                            }
                        },
                        {
                            "text": "As a wholly owned <w>subsidiary</w>, the firm reports directly to the headquarters in London.",
                            "target_lang_translations": {
                                "ru": "Будучи стопроцентным <w>дочерним предприятием</w>, фирма отчитывается непосредственно перед штаб-квартирой в Лондоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "affiliate",
                        "branch",
                        "division"
                    ],
                    "antonyms": [
                        "parent company",
                        "holding company"
                    ],
                    "common_phrases": [
                        "wholly owned subsidiary",
                        "foreign subsidiary",
                        "operating subsidiary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компания, которая принадлежит другой, более крупной компании (часто называемой материнской) или контролируется ею."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дочерняя компания",
                                "target_lang_sense_clarification": "юридически самостоятельная фирма, принадлежащая другой компании"
                            },
                            {
                                "target_lang_word": "дочернее предприятие",
                                "target_lang_sense_clarification": "организация, контролируемая головной компанией"
                            },
                            {
                                "target_lang_word": "дочка",
                                "target_lang_sense_clarification": "разговорное название дочерней компании"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d1e8f582-4d18-3807-ad5c-26cb46449e58",
                    "sense_definition": "Less important than something else to which it is related; secondary or subordinate.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "secondary_aux",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager played only a <w>subsidiary</w> role in the negotiations, leaving the main decisions to the director.",
                            "target_lang_translations": {
                                "ru": "Менеджер играл лишь <w>второстепенную</w> роль в переговорах, оставив принятие основных решений директору."
                            }
                        },
                        {
                            "text": "We should focus on the primary goal rather than getting distracted by <w>subsidiary</w> issues.",
                            "target_lang_translations": {
                                "ru": "Нам следует сосредоточиться на главной цели, а не отвлекаться на <w>второстепенные</w> вопросы."
                            }
                        }
                    ],
                    "synonyms": [
                        "secondary",
                        "subordinate",
                        "minor",
                        "ancillary"
                    ],
                    "antonyms": [
                        "primary",
                        "main",
                        "principal",
                        "major"
                    ],
                    "common_phrases": [
                        "subsidiary role",
                        "subsidiary importance",
                        "subsidiary company"
                    ],
                    "target_lang_definitions": {
                        "ru": "Менее важный, чем что-то другое, с чем это связано; вторичный или подчинённый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второстепенный",
                                "target_lang_sense_clarification": "не основной, имеющий меньшее значение"
                            },
                            {
                                "target_lang_word": "подчинённый",
                                "target_lang_sense_clarification": "зависимый, занимающий более низкое положение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "046f854f-3b76-393d-88d4-3b7ddc516758",
                    "sense_definition": "Serving to assist, supplement, or support something else.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "secondary_aux",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Several <w>subsidiary</w> streams flow down from the hills to feed the main river.",
                            "target_lang_translations": {
                                "ru": "Несколько <w>вспомогательных</w> потоков стекают с холмов, питая главную реку."
                            }
                        },
                        {
                            "text": "The witness provided <w>subsidiary</w> testimony that helped confirm the defendant's alibi.",
                            "target_lang_translations": {
                                "ru": "Свидетель дал <w>дополнительные</w> показания, которые помогли подтвердить алиби обвиняемого."
                            }
                        }
                    ],
                    "synonyms": [
                        "auxiliary",
                        "supplemental",
                        "supplementary",
                        "supporting"
                    ],
                    "common_phrases": [
                        "subsidiary stream",
                        "subsidiary evidence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служащий для помощи, дополнения или поддержки чего-либо другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вспомогательный",
                                "target_lang_sense_clarification": "служащий для содействия или поддержки"
                            },
                            {
                                "target_lang_word": "дополнительный",
                                "target_lang_sense_clarification": "добавочный, дополняющий основное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0ddb087-db3b-3dc4-8836-57c2929d76d5",
                    "sense_definition": "Connected with or consisting of a financial subsidy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government authorized a <w>subsidiary</w> payment to support the struggling agricultural sector.",
                            "target_lang_translations": {
                                "ru": "Правительство санкционировало <w>субсидиарную</w> выплату для поддержки испытывающего трудности сельскохозяйственного сектора."
                            }
                        },
                        {
                            "text": "Historians studied the <w>subsidiary</w> treaties that funded the war effort in the eighteenth century.",
                            "target_lang_translations": {
                                "ru": "Историки изучали <w>субсидные</w> договоры, которые финансировали военные действия в восемнадцатом веке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "subsidiary treaty",
                        "subsidiary payment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с финансовой субсидией или состоящий из неё."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "субсидиарный",
                                "target_lang_sense_clarification": "относящийся к субсидии или резервной ответственности"
                            },
                            {
                                "target_lang_word": "субсидный",
                                "target_lang_sense_clarification": "связанный с выплатой субсидий (часто в историческом контексте)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
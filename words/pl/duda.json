    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "784fc177-bcb2-3a76-99e9-75ae975d8f38",
                    "sense_definition": "Osoba niezdarna, gapowata, powolna lub mało rozgarnięta.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "insults",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat to straszna <duda>, znowu zgubił klucze do domu.",
                            "target_lang_translations": {
                                "ru": "Мой брат — ужасный <растяпа>, опять потерял ключи от дома."
                            }
                        },
                        {
                            "text": "Nie bądź taką <dudą>, weź się wreszcie do roboty.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <недотёпой>, возьмись наконец за работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "gamoń",
                        "fujara",
                        "niedołęga",
                        "ciamajda",
                        "ofiara losu",
                        "niezdara",
                        "oferma"
                    ],
                    "common_phrases": [
                        "straszna duda",
                        "wielka duda",
                        "ale z ciebie duda"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "często o zabarwieniu lekceważącym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неуклюжий, рассеянный или несообразительный человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растяпа",
                                "target_lang_sense_clarification": "рассеянный человек"
                            },
                            {
                                "target_lang_word": "недотёпа",
                                "target_lang_sense_clarification": "неловкий, неуклюжий человек"
                            },
                            {
                                "target_lang_word": "рохля",
                                "target_lang_sense_clarification": "медлительный, нерасторопный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19ad3867-057b-3169-a80f-ee2b3e485ec6",
                    "sense_definition": "Ludowy instrument muzyczny dęty składający się z worka powietrznego i piszczałek; forma liczby pojedynczej używana rzadziej niż liczba mnoga „dudy”.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "instruments",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na ścianie w karczmie wisiała stara, zakurzona <duda>.",
                            "target_lang_translations": {
                                "ru": "На стене в корчме висела старая, запыленная <волынка>."
                            }
                        },
                        {
                            "text": "Muzyk nastroił swoją <dudę> przed wieczornym występem.",
                            "target_lang_translations": {
                                "ru": "Музыкант настроил свою <волынку> перед вечерним выступлением."
                            }
                        }
                    ],
                    "synonyms": [
                        "gajdy",
                        "koza"
                    ],
                    "common_phrases": [
                        "grać na dudzie"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "związane z folklorem (np. góralskim)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Народный духовой музыкальный инструмент (волынка); форма единственного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волынка",
                                "target_lang_sense_clarification": "музыкальный инструмент"
                            },
                            {
                                "target_lang_word": "дуда",
                                "target_lang_sense_clarification": "народное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7003fc21-4050-3c7b-bd07-ddc8dbe52e23",
                    "sense_definition": "Muzyk grający na dudach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "musicians",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miejscowy <duda> przygrywał gościom do tańca.",
                            "target_lang_translations": {
                                "ru": "Местный <волынщик> играл гостям для танцев."
                            }
                        },
                        {
                            "text": "Jako doświadczony <duda>, znał wszystkie lokalne melodie.",
                            "target_lang_translations": {
                                "ru": "Как опытный <дударь>, он знал все местные мелодии."
                            }
                        }
                    ],
                    "synonyms": [
                        "dudziarz",
                        "dudarz"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie częściej używa się słowa dudziarz"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкант, играющий на волынке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волынщик",
                                "target_lang_sense_clarification": "музыкант"
                            },
                            {
                                "target_lang_word": "дударь",
                                "target_lang_sense_clarification": "устаревшее или народное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2545f7aa-5347-3aad-a34c-cbecf3687f49",
                    "sense_definition": "Jedna z piszczałek wchodzących w skład instrumentu muzycznego (dud).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "instrument_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pękła mu jedna <duda> i instrument przestał grać czysto.",
                            "target_lang_translations": {
                                "ru": "У него треснула одна <трубка>, и инструмент перестал играть чисто."
                            }
                        },
                        {
                            "text": "Rzemieślnik wymienił uszkodzoną <dudę> na nową.",
                            "target_lang_translations": {
                                "ru": "Ремесленник заменил поврежденную <трубку> на новую."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "termin techniczny/muzyczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из трубок, составляющих волынку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трубка",
                                "target_lang_sense_clarification": "часть инструмента"
                            },
                            {
                                "target_lang_word": "дудка",
                                "target_lang_sense_clarification": "пищалка"
                            }
                        ]
                    }
                }
            ]
        },
        {

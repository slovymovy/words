                        "metabolizm spowolnił",
                        "spowolnić się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать происходить медленнее или потерять прежнюю скорость (часто в отношении экономики, жизненных функций или явлений)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замедлиться",
                                "target_lang_sense_clarification": "стать более медленным"
                            }
                        ]
                    }
                }
            ]
        }
    ]

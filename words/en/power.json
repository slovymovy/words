                    ],
                    "synonyms": [
                        "ability",
                        "capacity",
                        "capability",
                        "aptitude"
                    ],
                    "antonyms": [
                        "impotence"
                    ],
                    "common_phrases": [
                        "within one's power",
                        "power of speech",
                        "power to choose"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность или возможность делать что-либо или действовать определённым образом."

{
    "word_family": [
        "блокировать",
        "разблокировать",
        "блокировка",
        "блокирование",
        "блокиратор"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b9cdbb55-0423-378b-92b2-e9a0a55e5bce",
                    "sense_definition": "Сделать невозможным использование устройства, системы, счёта или программы; временно остановить работу или доступ.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tech_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Банк решил <w>заблокировать</w> карту из-за подозрительной операции.",
                            "target_lang_translations": {
                                "en": "The bank decided to <w>block</w> the card due to a suspicious transaction."
                            }
                        },
                        {
                            "text": "Если ты забудешь пароль, система может <w>заблокировать</w> твой телефон.",
                            "target_lang_translations": {
                                "en": "If you forget the password, the system might <w>lock</w> your phone."
                            }
                        }
                    ],
                    "synonyms": [
                        "забанить"
                    ],
                    "antonyms": [
                        "разблокировать"
                    ],
                    "common_phrases": [
                        "заблокировать карту",
                        "заблокировать пользователя",
                        "заблокировать телефон",
                        "заблокировать доступ"
                    ],
                    "target_lang_definitions": {
                        "en": "To make it impossible to use a device, system, account, or program; to temporarily stop operation or access."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "block",
                                "target_lang_sense_clarification": "an account, card, or access"
                            },
                            {
                                "target_lang_word": "lock",
                                "target_lang_sense_clarification": "a device or system"
                            },
                            {
                                "target_lang_word": "ban",
                                "target_lang_sense_clarification": "a user"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a95b3cce-d674-36fd-8563-4c095a41d509",
                    "sense_definition": "Преградить путь, закрыть проход или доступ куда-либо; лишить возможности двигаться.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_block",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Грузовик сломался и полностью <w>заблокировал</w> выезд из гаража.",
                            "target_lang_translations": {
                                "en": "The truck broke down and completely <w>blocked</w> the exit from the garage."
                            }
                        },
                        {
                            "text": "Спасателям пришлось <w>заблокировать</w> двери, чтобы огонь не распространился дальше.",
                            "target_lang_translations": {
                                "en": "The rescuers had to <w>block</w> the doors so the fire wouldn't spread further."
                            }
                        }
                    ],
                    "antonyms": [
                        "разблокировать"
                    ],
                    "common_phrases": [
                        "заблокировать дверь",
                        "заблокировать проезд",
                        "заблокировать выход"
                    ],
                    "target_lang_definitions": {
                        "en": "To obstruct a path, close a passage or access to somewhere; to prevent movement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "block",
                                "target_lang_sense_clarification": "passage, exit, or movement"
                            },
                            {
                                "target_lang_word": "obstruct",
                                "target_lang_sense_clarification": "a path or view"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
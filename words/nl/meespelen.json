                    "sense_id": "bacdea5e-106b-33e7-9ad9-229d34cdeee2",
                    "sense_definition": "Actief deelnemen aan een spel, sportwedstrijd, film of muziekuitvoering.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen vroegen aan hun vriendjes of ze mochten <w>meespelen</w>.",
                            "target_lang_translations": {
                                "en": "The children asked their friends if they could <w>join in</w>.",
                                "ru": "Дети спросили у своих друзей, можно ли им <w>поиграть с ними</w>."
                            }
                        },
                        {
                            "text": "De topscorer kon die wedstrijd wegens een blessure niet <w>meespelen</w>.",
                            "target_lang_translations": {
                                "en": "The top scorer could not <w>play</w> in that match due to an injury.",
                                "ru": "Лучший бомбардир не смог <w>сыграть</w> в том матче из-за травмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "meedoen",
                        "deelnemen"
                    ],
                    "antonyms": [
                        "toekijken"
                    ],
                    "common_phrases": [
                        "in een film meespelen",
                        "met de loterij meespelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To actively participate in a game, sports match, film, or musical performance.",
                        "ru": "Активно участвовать в игре, спортивном состязании, фильме или музыкальном исполнении."
                    },
                    "translations": {

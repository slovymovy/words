{
    "word_family": [
        "factor",
        "manufacture",
        "manufacturer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "36bd113a-7480-351f-b162-7ca549f6d607",
                    "sense_definition": "A building or group of buildings where goods are manufactured or assembled by machine.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "production_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandfather worked in a steel <w>factory</w> for forty years.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка сорок лет проработал на сталелитейном <w>заводе</w>."
                            }
                        },
                        {
                            "text": "The <w>factory</w> produces thousands of toys every day.",
                            "target_lang_translations": {
                                "ru": "<w>Фабрика</w> производит тысячи игрушек каждый день."
                            }
                        }
                    ],
                    "synonyms": [
                        "manufactory"
                    ],
                    "common_phrases": [
                        "car factory",
                        "textile factory",
                        "factory worker",
                        "shoe factory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание или группа зданий, где товары производятся или собираются с помощью машин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завод",
                                "target_lang_sense_clarification": "обычно тяжелая промышленность или машиностроение"
                            },
                            {
                                "target_lang_word": "фабрика",
                                "target_lang_sense_clarification": "обычно легкая или пищевая промышленность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "908a5a0c-87e8-3ca0-b23b-eb8a97f9d230",
                    "sense_definition": "The original state or settings of a device as prepared by the manufacturer.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "original_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If the computer crashes, try restoring the <w>factory</w> settings.",
                            "target_lang_translations": {
                                "ru": "Если компьютер выйдет из строя, попробуйте восстановить <w>заводские</w> настройки."
                            }
                        },
                        {
                            "text": "A <w>factory</w> reset will delete all your personal files.",
                            "target_lang_translations": {
                                "ru": "Сброс до <w>заводских</w> настроек удалит все ваши личные файлы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "factory settings",
                        "factory reset",
                        "factory defaults"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исходное состояние или настройки устройства, установленные производителем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заводской",
                                "target_lang_sense_clarification": "относящийся к заводским настройкам"
                            },
                            {
                                "target_lang_word": "фабричный",
                                "target_lang_sense_clarification": "произведенный на фабрике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8aea6f54-f5d1-3fa4-ae5b-90796e8c74f6",
                    "sense_definition": "A place or system that produces a large amount of something, often in a mechanical or unfeeling way.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "production_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tabloid press acts as a gossip <w>factory</w>.",
                            "target_lang_translations": {
                                "ru": "Таблоидная пресса действует как <w>фабрика</w> сплетен."
                            }
                        },
                        {
                            "text": "This college is known as a <w>factory</w> for professional athletes.",
                            "target_lang_translations": {
                                "ru": "Этот колледж известен как <w>кузница</w> профессиональных спортсменов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rumor factory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или система, производящая что-либо в большом количестве, часто механически или бездушно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фабрика",
                                "target_lang_sense_clarification": "в переносном смысле"
                            },
                            {
                                "target_lang_word": "конвейер",
                                "target_lang_sense_clarification": "метафора поточного производства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdd4420f-6348-3e06-b8e4-2cfdc340fdd7",
                    "sense_definition": "A large industrial farm where animals are raised in high density.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "production_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Activists protested the conditions at the local chicken <w>factory</w>.",
                            "target_lang_translations": {
                                "ru": "Активисты протестовали против условий на местной <w>птицефабрике</w>."
                            }
                        },
                        {
                            "text": "The meat from the pig <w>factory</w> is processed nearby.",
                            "target_lang_translations": {
                                "ru": "Мясо со свино<w>фермы</w> перерабатывается неподалеку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chicken factory"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная промышленная ферма, где животные выращиваются в условиях высокой плотности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "птицефабрика",
                                "target_lang_sense_clarification": "для домашней птицы"
                            },
                            {
                                "target_lang_word": "ферма",
                                "target_lang_sense_clarification": "промышленного типа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f59c7591-f617-3ec9-9c0e-da8e57aa9f27",
                    "sense_definition": "A function or method in a computer program that creates new objects.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The developer used a <w>factory</w> to generate the user interface elements.",
                            "target_lang_translations": {
                                "ru": "Разработчик использовал <w>фабрику</w> для генерации элементов пользовательского интерфейса."
                            }
                        },
                        {
                            "text": "This <w>factory</w> pattern helps manage the creation of complex objects.",
                            "target_lang_translations": {
                                "ru": "Этот паттерн <w>фабрика</w> помогает управлять созданием сложных объектов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "factory method",
                        "object factory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Функция или метод в компьютерной программе, создающий новые объекты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фабрика",
                                "target_lang_sense_clarification": "термин программирования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8b8e61a-98fb-33d1-9b6c-5301a7458617",
                    "sense_definition": "A trading station established by merchants in a foreign country.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "trading_post",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company built a fortified <w>factory</w> on the coast of India.",
                            "target_lang_translations": {
                                "ru": "Компания построила укрепленную <w>факторию</w> на побережье Индии."
                            }
                        },
                        {
                            "text": "Life in the <w>factory</w> was isolated from the local community.",
                            "target_lang_translations": {
                                "ru": "Жизнь в <w>фактории</w> была изолирована от местного сообщества."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Торговая станция, основанная купцами в чужой стране."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фактория",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9a74f7e-ab23-38e1-9bf7-2b164e010038",
                    "sense_definition": "A police station.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "police_station",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The detective drove the suspect back to the <w>factory</w> for questioning.",
                            "target_lang_translations": {
                                "ru": "Детектив отвез подозреваемого обратно в <w>участок</w> для допроса."
                            }
                        },
                        {
                            "text": "He spent a few hours in a cell at the <w>factory</w>.",
                            "target_lang_translations": {
                                "ru": "Он провел несколько часов в камере в <w>участке</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полицейский участок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "отделение",
                                "target_lang_sense_clarification": "полиции"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c83b92f5-1621-309d-8299-a715954324ad",
                    "sense_definition": "In the original condition as supplied by the manufacturer; not modified.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "original_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car still has its <w>factory</w> paint job.",
                            "target_lang_translations": {
                                "ru": "На машине все еще <w>заводская</w> краска."
                            }
                        },
                        {
                            "text": "These parts are not <w>factory</w>, so they might not fit perfectly.",
                            "target_lang_translations": {
                                "ru": "Эти детали не <w>заводские</w>, поэтому они могут не подойти идеально."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В первоначальном состоянии, как от производителя; не модифицированный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заводской",
                                "target_lang_sense_clarification": "оригинальный"
                            },
                            {
                                "target_lang_word": "родной",
                                "target_lang_sense_clarification": "разговорное, об оригинальных деталях"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0ec4d22a-c20f-380a-9f8b-2aaa0e672b1a",
                    "sense_definition": "Relating to the art of poetry or the composition of poems.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor explained the complex <w>poetic</w> meter used in the ancient sonnet.",
                            "target_lang_translations": {
                                "ru": "Профессор объяснил сложный <w>поэтический</w> размер, использованный в старинном сонете."
                            }
                        },
                        {
                            "text": "Writers often use <w>poetic</w> license to alter historical facts for the sake of the story.",
                            "target_lang_translations": {
                                "ru": "Писатели часто используют <w>поэтическую</w> вольность, изменяя исторические факты ради сюжета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "poetic license",
                        "poetic justice",
                        "poetic device",
                        "poetic form"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к искусству поэзии или сочинению стихов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поэтический",
                                "target_lang_sense_clarification": "relating to poetry"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e58aa3d-533a-3223-89cb-e7326d3a4f5d",
                    "sense_definition": "Having the beautiful, imaginative, or emotional qualities typical of poetry.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The view of the misty mountains had a <w>poetic</w> beauty that silenced the group.",
                            "target_lang_translations": {
                                "ru": "Вид туманных гор обладал <w>поэтичной</w> красотой, которая заставила группу умолкнуть."
                            }
                        },
                        {
                            "text": "She described her childhood memories in a <w>poetic</w> way that moved everyone to tears.",
                            "target_lang_translations": {
                                "ru": "Она описала свои детские воспоминания в <w>поэтичной</w> манере, которая тронула всех до слёз."
                            }
                        }
                    ],
                    "synonyms": [
                        "lyrical",
                        "imaginative",
                        "artistic"
                    ],
                    "antonyms": [
                        "prosaic"
                    ],
                    "common_phrases": [
                        "poetic beauty",
                        "poetic soul",
                        "poetic imagination"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий красотой, образностью или эмоциональностью, типичными для поэзии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поэтичный",
                                "target_lang_sense_clarification": "having qualities of poetry"

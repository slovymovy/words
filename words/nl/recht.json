                {
                    "sense_id": "240dce2c-cdda-3ff9-a9e8-8754f291c94f",
                    "sense_definition": "Zonder bochten of krommingen; in een lineaire lijn.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De weg naar het dorp is lang en <w>recht</w>.",
                            "target_lang_translations": {
                                "ru": "Дорога в деревню длинная и <w>прямая</w>.",
                                "en": "The road to the village is long and <w>straight</w>."
                            }
                        },
                        {
                            "text": "Kun je een <w>rechte</w> lijn tekenen zonder liniaal?",
                            "target_lang_translations": {
                                "ru": "Ты можешь нарисовать <w>прямую</w> линию без линейки?",
                                "en": "Can you draw a <w>straight</w> line without a ruler?"
                            }
                        }
                    ],
                    "antonyms": [
                        "krom"
                    ],
                    "common_phrases": [
                        "recht vooruit",
                        "kaarsrecht",
                        "in een rechte lijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без изгибов или искривлений; по прямой линии.",
                        "en": "Without bends or curves; in a linear line."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямой",
                                "target_lang_sense_clarification": "о линии или форме"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "straight",
                                "target_lang_sense_clarification": "of a line or shape"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a1a0490-dcd2-38aa-a7ef-243d4b5d6575",
                    "sense_definition": "Niet scheef; in de juiste verticale of horizontale positie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat schilderij hangt niet helemaal <w>recht</w> aan de muur.",
                            "target_lang_translations": {
                                "ru": "Эта картина висит на стене не совсем <w>ровно</w>.",
                                "en": "That painting isn't hanging quite <w>straight</w> on the wall."
                            }
                        },
                        {
                            "text": "Ga <w>recht</w> op je stoel zitten en let op je houding.",
                            "target_lang_translations": {
                                "ru": "Сядь <w>прямо</w> на стуле и следи за осанкой.",
                                "en": "Sit up <w>straight</w> in your chair and mind your posture."
                            }
                        }
                    ],
                    "antonyms": [
                        "scheef"
                    ],
                    "common_phrases": [
                        "recht op",
                        "recht overeind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не косой; в правильном вертикальном или горизонтальном положении.",
                        "en": "Not crooked; in the correct vertical or horizontal position."

                    ],
                    "common_phrases": [
                        "accompanied baggage",
                        "accompanied minor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в сопровождении кого-либо; не одиночный"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопровождаемый",
                                "target_lang_sense_clarification": "о человеке или грузе, который находится под присмотром"
                            },
                            {
                                "target_lang_word": "в сопровождении",
                                "target_lang_sense_clarification": "устойчивое выражение, указывающее на наличие спутников"
                            }
                        ]
                    }
                }
            ]

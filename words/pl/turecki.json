    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "98051f08-d0ac-3464-8767-03895dde4718",
                    "sense_definition": "związany z Turcją, Turkami, ich kulturą, historią lub językiem",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "turkey_country",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój ulubiony <w>turecki</w> deser to baklawa z orzechami.",
                            "target_lang_translations": {
                                "ru": "Мой любимый <w>турецкий</w> десерт — это пахлава с орехами."
                            }
                        },
                        {
                            "text": "Wczoraj kupiłem piękny, ręcznie tkany <w>turecki</w> dywan.",
                            "target_lang_translations": {
                                "ru": "Вчера я купил красивый <w>турецкий</w> ковер ручной работы."
                            }
                        },
                        {
                            "text": "Czy wiesz, gdzie można zjeść dobrą kuchnię <w>turecką</w> w tym mieście?",
                            "target_lang_translations": {
                                "ru": "Ты знаешь, где в этом городе можно попробовать хорошую <w>турецкую</w> кухню?"
                            }
                        }
                    ],
                    "antonyms": [
                        "antyturecki"
                    ],
                    "common_phrases": [
                        "język turecki",
                        "turecki dywan",
                        "kawa po turecku",
                        "tureckie słodycze",
                        "turecki serial"
                    ],
                    "target_lang_definitions": {
                        "ru": "связанный с Турцией, турками, их культурой, историей или языком"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "турецкий",
                                "target_lang_sense_clarification": "относящийся к Турции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbbfa12e-e298-3ebd-9ca5-733b380aea5d",
                    "sense_definition": "dotyczący grupy ludów (tzw. ludów turkijskich) zamieszkujących obszary od Azji po Europę Wschodnią lub ich języków",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "turkic_peoples",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Języki <w>tureckie</w> wykazują duże podobieństwo gramatyczne i słownikowe.",
                            "target_lang_translations": {
                                "ru": "<w>Тюркские</w> языки демонстрируют большое грамматическое и лексическое сходство."
                            }
                        },
                        {
                            "text": "Badacze analizują wpływ kultury <w>tureckiej</w> na historię Azji Środkowej.",
                            "target_lang_translations": {
                                "ru": "Исследователи анализируют влияние <w>тюркской</w> культуры на историю Центральной Азии."
                            }
                        }
                    ],
                    "synonyms": [
                        "turkijski"
                    ],
                    "common_phrases": [
                        "ludy tureckie",
                        "języki tureckie"
                    ],
                    "target_lang_definitions": {
                        "ru": "относящийся к группе народов (т. н. тюркских народов), населяющих территории от Азии до Восточной Европы, или к их языкам"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюркский",
                                "target_lang_sense_clarification": "относящийся к тюркским народам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "757d42a2-aaa3-35cb-a238-a446300e3fa9",

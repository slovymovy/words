            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1996e5a7-a60e-304f-a9db-43a3e94757ae",
                    "sense_definition": "Spotkanie towarzyskie, często z muzyką, tańcami i alkoholem, organizowane dla rozrywki i integracji.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sobotę idziemy na urodzinową <w>imprezę</w> do mojej najlepszej przyjaciółki.",
                            "target_lang_translations": {
                                "ru": "В субботу мы идем на <w>вечеринку</w> по случаю дня рождения к моей лучшей подруге."
                            }
                        },
                        {
                            "text": "To była naprawdę udana <w>impreza</w>, wszyscy świetnie się bawili do rana.",
                            "target_lang_translations": {
                                "ru": "Это была действительно удачная <w>вечеринка</w>, все отлично веселились до утра."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabawa",
                        "przyjęcie",
                        "prywatka",
                        "balanga",
                        "biba",
                        "bibka",
                        "melanż",
                        "balety",
                        "wiksa",
                        "jubel"
                    ],
                    "common_phrases": [
                        "iść na imprezę",
                        "robić imprezę",
                        "udana impreza",
                        "impreza urodzinowa",
                        "król imprezy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дружеская встреча, часто с музыкой, танцами и алкоголем, организованная для развлечения и общения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вечеринка",
                                "target_lang_sense_clarification": "развлечение"
                            },
                            {
                                "target_lang_word": "тусовка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82b3924a-40dd-3a9c-a1d0-bec7058f6e58",
                    "sense_definition": "Zorganizowane wydarzenie o charakterze kulturalnym, sportowym lub rozrywkowym, przeznaczone dla szerszej publiczności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miasto organizuje wielką <w>imprezę</w> plenerową na powitanie lata.",
                            "target_lang_translations": {
                                "ru": "Город организует большое <w>мероприятие</w> на открытом воздухе в честь встречи лета."
                            }
                        },
                        {
                            "text": "Bezpieczeństwo uczestników podczas tej <w>imprezy</w> masowej jest dla nas priorytetem.",
                            "target_lang_translations": {
                                "ru": "Безопасность участников во время этого массового <w>мероприятия</w> является для нас приоритетом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "impreza masowa",
                        "impreza plenerowa",
                        "impreza kulturalna",
                        "impreza charytatywna",
                        "impreza sportowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованное культурное, спортивное или развлекательное событие для широкой публики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мероприятие",
                                "target_lang_sense_clarification": "событие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a59e130a-9ef0-3d88-91c2-470ad33c2db3",
                    "sense_definition": "Ważne zamierzenie, trudne przedsięwzięcie lub wyprawa, często o charakterze wojskowym.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Król planował ryzykowną <w>imprezę</w> wojenną przeciwko sąsiedniemu mocarstwu.",
                            "target_lang_translations": {
                                "ru": "Король планировал рискованное военное <w>предприятие</w> против соседней державы."
                            }
                        },
                        {
                            "text": "Cała ta polityczna <w>impreza</w> zakończyła się niestety wielkim niepowodzeniem.",
                            "target_lang_translations": {
                                "ru": "Вся эта политическая <w>затея</w> закончилась, к сожалению, большим провалом."
                            }
                        }
                    ],
                    "synonyms": [

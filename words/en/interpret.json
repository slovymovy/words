    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d187290a-bc54-3d42-b34f-7029a94fcf5b",
                    "sense_definition": "To explain the meaning of information, actions, or events, or to understand them in a specific way.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "meaning_analysis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is difficult to <w>interpret</w> these statistics without knowing the context."
                        },
                        {
                            "text": "She <w>interpreted</w> his silence as a refusal to help."
                        }
                    ],
                    "synonyms": [
                        "explain",
                        "understand",
                        "construe",
                        "decipher",
                        "elucidate",
                        "take"
                    ],
                    "common_phrases": [
                        "interpret the data",
                        "interpret the rules",
                        "interpret dreams",
                        "hard to interpret"
                    ]
                },
                {
                    "sense_id": "4031b6c1-0356-3908-be0c-0a54a2483c13",
                    "sense_definition": "To translate spoken words from one language into another orally in real time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was hired to <w>interpret</w> for the foreign delegation during the conference."
                        },
                        {
                            "text": "Can you <w>interpret</w> what the speaker is saying for us?"
                        }
                    ],
                    "synonyms": [
                        "translate"
                    ],
                    "common_phrases": [
                        "interpret for someone",
                        "interpret simultaneously"
                    ]
                },
                {
                    "sense_id": "8e99e7ac-a3b9-3864-8a6a-c0d0baa3b935",
                    "sense_definition": "To perform a creative work, such as music or a role, in a way that conveys one's own understanding or feelings about it.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actor <w>interpreted</w> the character with great emotion and depth."
                        },
                        {
                            "text": "Musicians often <w>interpret</w> classical pieces differently depending on their style."
                        }
                    ],
                    "synonyms": [
                        "render"
                    ],
                    "common_phrases": [
                        "interpret a role",
                        "interpret a song"
                    ]
                },
                {
                    "sense_id": "75cd0de2-ab6f-332f-aca3-fbf47056d0a0",
                    "sense_definition": "To execute a computer program by reading the instructions as they are encountered rather than compiling them in advance.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The browser must <w>interpret</w> the script to display the page correctly."
                        },
                        {
                            "text": "Some programming languages are <w>interpreted</w> at runtime instead of being compiled."
                        }
                    ],
                    "common_phrases": [
                        "interpret code",
                        "interpret commands"
                    ]
                }
            ]
        }
    ]

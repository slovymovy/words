                        "сиамские близнецы",
                        "однояйцевые близнецы"
                    ],
                    "target_lang_definitions": {
                        "en": "One of two or more children born to the same mother at the same time"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "twin",
                                "target_lang_sense_clarification": "biological sibling"
                            }
                        ]

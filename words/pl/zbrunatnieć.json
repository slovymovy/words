                        "papier zbrunatniał",
                        "skóra zbrunatniała"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать бурым, приобрести темно-коричневый цвет (часто в результате старения, увядания или порчи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побуреть",

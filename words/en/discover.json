{
    "word_family": [
        "discovery",
        "discoverer",
        "undiscovered",
        "rediscover",
        "discoverable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "514cb300-b583-332c-a271-15f53d6e154c",
                    "sense_definition": "To find or learn something for the first time.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "find_learn",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists hope to <w>discover</w> a cure for the disease soon.",
                            "target_lang_translations": {
                                "ru": "Ученые надеются вскоре <w>открыть</w> лекарство от этой болезни."
                            }
                        },
                        {
                            "text": "She was surprised to <w>discover</w> that the door was unlocked.",
                            "target_lang_translations": {
                                "ru": "Она с удивлением <w>обнаружила</w>, что дверь не заперта."
                            }
                        }
                    ],
                    "synonyms": [
                        "find",
                        "learn",
                        "detect",
                        "locate",
                        "unearth",
                        "spot",
                        "come across"
                    ],
                    "antonyms": [
                        "miss",
                        "overlook"
                    ],
                    "common_phrases": [
                        "discover the truth",
                        "discover a cure",
                        "discover a secret",
                        "discover new places"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находить или узнавать что-то впервые; совершать открытие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "совершать открытие (научное, географическое)"
                            },
                            {
                                "target_lang_word": "обнаруживать",
                                "target_lang_sense_clarification": "находить, замечать что-то скрытое"
                            },
                            {
                                "target_lang_word": "узнавать",
                                "target_lang_sense_clarification": "получать новую информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39d0dd53-06fe-3052-a796-0f74283e8792",
                    "sense_definition": "To create an attack in chess by moving a piece out of another piece's line of attack.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "By moving the knight, the player was able to <w>discover</w> a check on the king.",
                            "target_lang_translations": {
                                "ru": "Сделав ход конем, игрок смог <w>вскрыть</w> шах королю."
                            }
                        },
                        {
                            "text": "This clever move <w>discovers</w> an attack on the opponent's queen.",
                            "target_lang_translations": {
                                "ru": "Этот хитрый ход <w>открывает</w> нападение на ферзя противника."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать угрозу в шахматах, отводя свою фигуру с линии атаки другой дальнобойной фигуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вскрывать",
                                "target_lang_sense_clarification": "линию атаки или шах"
                            },
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "нападение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59d8876b-1981-372d-8b15-b63f64b103ce",
                    "sense_definition": "To question a person to obtain information as part of a lawsuit.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The defense team wanted to <w>discover</w> the witness before the hearing.",
                            "target_lang_translations": {
                                "ru": "Сторона защиты хотела <w>опросить</w> свидетеля до слушания."
                            }
                        },
                        {
                            "text": "Laws allow parties to <w>discover</w> relevant facts from each other.",
                            "target_lang_translations": {
                                "ru": "Законы позволяют сторонам <w>раскрывать</w> друг другу относящиеся к делу факты."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Запрашивать документы или опрашивать свидетелей для получения информации в рамках судебного процесса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать сведения",
                                "target_lang_sense_clarification": "в рамках судебной процедуры"
                            },
                            {
                                "target_lang_word": "раскрывать",
                                "target_lang_sense_clarification": "доказательства или факты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ffc19aa-6ead-3d6c-9867-196cd7cb0524",
                    "sense_definition": "To remove a cover from something; to uncover or expose physically.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "expose_reveal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The melting snow <w>discovered</w> the rocky path beneath.",
                            "target_lang_translations": {
                                "ru": "Тающий снег <w>обнажил</w> каменистую тропу под ним."
                            }
                        },
                        {
                            "text": "He lifted the heavy cloth to <w>discover</w> the statue hidden underneath.",
                            "target_lang_translations": {
                                "ru": "Он поднял тяжелую ткань, чтобы <w>открыть</w> спрятанную под ней статую."
                            }
                        }
                    ],
                    "synonyms": [
                        "uncover",
                        "expose",
                        "reveal"
                    ],
                    "antonyms": [
                        "cover",
                        "conceal",
                        "hide"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "original literal meaning"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Снимать покров с чего-либо; обнажать или физически открывать спрятанное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнажать",
                                "target_lang_sense_clarification": "делать видимым, снимая покров"
                            },
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "являть взору"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ae8eb9e-f8fe-37c2-b522-fa2fbd924418",
                    "sense_definition": "To reveal or make known information that was previously secret.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "expose_reveal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He promised not to <w>discover</w> the conspiracy to anyone.",
                            "target_lang_translations": {
                                "ru": "Он обещал никому не <w>раскрывать</w> заговор."
                            }
                        },
                        {
                            "text": "Her eyes <w>discovered</w> her true feelings despite her silence.",
                            "target_lang_translations": {
                                "ru": "Её глаза <w>выдавали</w> её истинные чувства, несмотря на молчание."
                            }
                        }
                    ],
                    "synonyms": [
                        "reveal"
                    ],
                    "antonyms": [
                        "conceal",
                        "hide"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "now replaced by reveal or disclose"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскрывать или делать известной информацию, которая ранее была секретной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрывать",
                                "target_lang_sense_clarification": "тайну или секрет"
                            },
                            {
                                "target_lang_word": "выдавать",
                                "target_lang_sense_clarification": "обнаруживать (чувства, секрет)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "47090660-1eb0-32f2-b301-e5b8f65ae2a4",
                    "sense_definition": "A brand of credit card.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "I paid for the groceries using my <w>Discover</w> card.",
                            "target_lang_translations": {
                                "ru": "Я расплатился за продукты своей картой <w>Discover</w>."
                            }
                        },
                        {
                            "text": "Many stores in the region accept <w>Discover</w> for payments.",
                            "target_lang_translations": {
                                "ru": "Многие магазины в регионе принимают <w>Discover</w> к оплате."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бренд кредитных карт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Discover",
                                "target_lang_sense_clarification": "название бренда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b127296-0f1e-3eec-b2ae-9483a7e5383c",
                    "sense_definition": "A general audience science magazine.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "She read an interesting article about space in <w>Discover</w>.",
                            "target_lang_translations": {
                                "ru": "Она прочитала интересную статью о космосе в журнале <w>Discover</w>."
                            }
                        },
                        {
                            "text": "He has a subscription to <w>Discover</w> to keep up with science news.",
                            "target_lang_translations": {
                                "ru": "У него есть подписка на <w>Discover</w>, чтобы быть в курсе новостей науки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Научно-популярный журнал для широкой аудитории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Discover",
                                "target_lang_sense_clarification": "название журнала"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
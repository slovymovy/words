    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dee5cb9a-d8a1-3901-8449-4d9e877d7f2c",
                    "sense_definition": "Выговор, строгое указание на ошибку или нарушение правил поведения.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reprimand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель сделал ученику <w>замечание</w> за громкие разговоры во время урока.",
                            "target_lang_translations": {
                                "en": "The teacher gave the student a <w>reprimand</w> for loud talking during the lesson."
                            }
                        },
                        {
                            "text": "Я стараюсь работать хорошо, чтобы не получить <w>замечание</w> от начальника.",
                            "target_lang_translations": {
                                "en": "I try to work well so as not to receive a <w>reprimand</w> from the boss."
                            }
                        }
                    ],
                    "synonyms": [
                        "выговор",
                        "упрёк",
                        "порицание"
                    ],
                    "antonyms": [
                        "похвала",
                        "одобрение",
                        "поощрение"
                    ],
                    "common_phrases": [
                        "сделать замечание",
                        "получить замечание",
                        "строгое замечание"
                    ],
                    "target_lang_definitions": {
                        "en": "A formal expression of disapproval or a strict indication of a mistake or violation of rules."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reprimand",
                                "target_lang_sense_clarification": "formal disciplinary action"
                            },
                            {
                                "target_lang_word": "rebuke",
                                "target_lang_sense_clarification": "sharp disapproval"
                            },
                            {
                                "target_lang_word": "admonition",
                                "target_lang_sense_clarification": "warning or advice against a fault"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e693dd33-e902-31ad-a4e1-995bdca6ce7b",
                    "sense_definition": "Краткое суждение, мнение или высказывание по поводу чего-либо увиденного, услышанного или прочитанного.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "observation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ваше <w>замечание</w> оказалось очень полезным для решения нашей проблемы.",
                            "target_lang_translations": {
                                "en": "Your <w>remark</w> turned out to be very useful for solving our problem."
                            }
                        },
                        {
                            "text": "Он вставил остроумное <w>замечание</w> в наш разговор и всех рассмешил.",
                            "target_lang_translations": {
                                "en": "He inserted a witty <w>remark</w> into our conversation and made everyone laugh."
                            }
                        }
                    ],
                    "synonyms": [
                        "высказывание",
                        "суждение",
                        "реплика",
                        "комментарий",
                        "наблюдение"
                    ],
                    "common_phrases": [
                        "верное замечание",
                        "ценное замечание",
                        "критическое замечание",
                        "остроумное замечание"
                    ],
                    "target_lang_definitions": {
                        "en": "A brief statement, opinion, or comment regarding something seen, heard, or read."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remark",
                                "target_lang_sense_clarification": "brief statement or comment"
                            },
                            {
                                "target_lang_word": "comment",
                                "target_lang_sense_clarification": "opinion or explanation"
                            },
                            {
                                "target_lang_word": "observation",
                                "target_lang_sense_clarification": "remark based on something noticed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76a82762-3cdf-34e2-9919-b7fca78c5ef1",
                    "sense_definition": "Комментарии, научная критика или пояснения к тексту.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "commentary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автор подготовил подробные <w>замечания</w> к новому изданию своей книги.",
                            "target_lang_translations": {
                                "en": "The author prepared detailed <w>notes</w> for the new edition of his book."
                            }
                        },
                        {
                            "text": "Я внимательно изучил его исторические <w>замечания</w> на полях рукописи.",
                            "target_lang_translations": {
                                "en": "I carefully studied his historical <w>notes</w> in the margins of the manuscript."
                            }
                        }
                    ],
                    "synonyms": [
                        "комментарий",
                        "пояснение",
                        "примечание"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'примечания' or 'комментарии'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Critical notes, explanations, or commentary added to a text."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "note",
                                "target_lang_sense_clarification": "annotation or explanation in text"
                            },
                            {
                                "target_lang_word": "comment",
                                "target_lang_sense_clarification": "critical feedback on a text"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a27e4ea-77b4-3aa8-8b01-8fb09776b14f",
                    "sense_definition": "Действие по значению глагола «замечать»; наблюдение, обнаружение.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По верному <w>замечанию</w> очевидцев, погода в тот день резко испортилась.",
                            "target_lang_translations": {
                                "en": "According to the correct <w>observation</w> of eyewitnesses, the weather deteriorated sharply that day."
                            }
                        },
                        {
                            "text": "Это явление ускользнуло от моего <w>замечания</w> в первый раз.",
                            "target_lang_translations": {
                                "en": "This phenomenon escaped my <w>notice</w> the first time."
                            }
                        }
                    ],
                    "synonyms": [
                        "наблюдение",
                        "обнаружение"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly found in literary texts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The act of noticing, observing, or detecting something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "observation",
                                "target_lang_sense_clarification": "act of observing"
                            },
                            {
                                "target_lang_word": "notice",
                                "target_lang_sense_clarification": "attention or awareness"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
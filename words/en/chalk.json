{
    "word_family": [
        "chalky",
        "chalkboard",
        "chalkiness"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "48a4afff-80ea-3fd4-acf0-59ea3f246365",
                    "sense_definition": "A stick of white or colored material, originally limestone but now often compressed gypsum, used for writing and drawing on a blackboard or other rough surface.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "writing_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher picked up a piece of <w>chalk</w> and wrote the math problem on the board.",
                            "target_lang_translations": {
                                "ru": "Учитель взял кусок <w>мела</w> и написал математическую задачу на доске."
                            }
                        },
                        {
                            "text": "Children love to draw pictures on the sidewalk with colored <w>chalk</w>.",
                            "target_lang_translations": {
                                "ru": "Дети любят рисовать картинки на тротуаре цветным <w>мелом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piece of chalk",
                        "colored chalk",
                        "box of chalk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Палочка из белого или цветного материала, используемая для письма и рисования на доске или другой шероховатой поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мел",
                                "target_lang_sense_clarification": "материал для письма"
                            },
                            {
                                "target_lang_word": "мелок",
                                "target_lang_sense_clarification": "кусочек мела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26dfae44-6cce-34aa-a4f6-04951655adae",
                    "sense_definition": "A soft, white, porous sedimentary rock, a form of limestone composed of the mineral calcite.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The famous white cliffs of the coastline are formed from massive layers of <w>chalk</w>.",
                            "target_lang_translations": {
                                "ru": "Знаменитые белые скалы побережья образованы массивными слоями <w>мела</w>."
                            }
                        },
                        {
                            "text": "Gardeners sometimes add <w>chalk</w> to the soil to reduce its acidity.",
                            "target_lang_translations": {
                                "ru": "Садоводы иногда добавляют <w>мел</w> в почву, чтобы снизить её кислотность."
                            }
                        }
                    ],
                    "synonyms": [
                        "limestone"
                    ],
                    "common_phrases": [
                        "white chalk",
                        "chalk dust",
                        "chalk cliffs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мягкая белая пористая осадочная горная порода, разновидность известняка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мел",
                                "target_lang_sense_clarification": "горная порода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7948b013-009e-365c-9bb2-fd0cd698d56f",
                    "sense_definition": "A powder, often magnesium carbonate, used by athletes and climbers to dry their hands and improve their grip.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_grip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gymnast applied <w>chalk</w> to her hands before approaching the uneven bars.",
                            "target_lang_translations": {
                                "ru": "Гимнастка нанесла <w>магнезию</w> на руки перед подходом к брусьям."
                            }
                        },
                        {
                            "text": "You should keep your <w>chalk</w> in a bag so it does not spill inside your gym kit.",
                            "target_lang_translations": {
                                "ru": "Тебе следует держать <w>магнезию</w> в мешочке, чтобы она не рассыпалась в спортивной сумке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chalk bag",
                        "gym chalk",
                        "liquid chalk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Порошок (часто магнезия), используемый спортсменами для подсушивания рук и улучшения хвата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магнезия",
                                "target_lang_sense_clarification": "спортивный порошок"
                            },
                            {
                                "target_lang_word": "спортивный мел",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98bfd8dd-0eb0-3190-89fe-d4c18e7e9e77",
                    "sense_definition": "A hard, flat piece of chalk used by tailors to mark lines on fabric for cutting or sewing.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tailoring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tailor used blue <w>chalk</w> to mark where the trousers needed to be hemmed.",
                            "target_lang_translations": {
                                "ru": "Портной использовал синий <w>мелок</w>, чтобы отметить, где нужно подшить брюки."
                            }
                        },
                        {
                            "text": "Marks made with tailor's <w>chalk</w> can be easily brushed off the fabric later.",
                            "target_lang_translations": {
                                "ru": "Следы, оставленные портновским <w>мелком</w>, можно легко счистить с ткани позже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tailor's chalk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый плоский кусок мела, используемый портными для нанесения линий на ткань при раскрое или шитье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелок",
                                "target_lang_sense_clarification": "портновский"
                            },
                            {
                                "target_lang_word": "портновский мел",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62a57520-d9b6-32b7-aa54-db3c99c3d330",
                    "sense_definition": "The competitor or team considered most likely to win a contest or sporting event.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "gambling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is safer to bet on the <w>chalk</w>, but the payout is usually much lower.",
                            "target_lang_translations": {
                                "ru": "Безопаснее ставить на <w>фаворита</w>, но выигрыш обычно намного ниже."
                            }
                        },
                        {
                            "text": "The tournament was full of upsets, and the <w>chalk</w> was eliminated in the first round.",
                            "target_lang_translations": {
                                "ru": "Турнир был полон неожиданностей, и <w>фаворит</w> выбыл в первом раунде."
                            }
                        }
                    ],
                    "synonyms": [
                        "favorite"
                    ],
                    "antonyms": [
                        "underdog"
                    ],
                    "common_phrases": [
                        "bet on the chalk"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used in sports betting"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Участник или команда, которые считаются наиболее вероятными победителями в соревновании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фаворит",
                                "target_lang_sense_clarification": "в ставках и спорте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "745dd3cd-8e11-386d-b9e0-36aec1b21a03",
                    "sense_definition": "To rub or cover something with chalk, especially to improve grip or friction.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_grip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>chalked</w> the tip of his billiard cue to prevent a miscue.",
                            "target_lang_translations": {
                                "ru": "Он <w>намелил</w> кончик бильярдного кия, чтобы предотвратить кикс."
                            }
                        },
                        {
                            "text": "Climbers <w>chalk</w> their fingers to hold onto the rock face better.",
                            "target_lang_translations": {
                                "ru": "Скалолазы <w>натирают мелом</w> пальцы, чтобы лучше держаться за скалу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chalk the cue",
                        "chalk up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Натирать или покрывать что-либо мелом, особенно для улучшения хвата или трения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелить",
                                "target_lang_sense_clarification": "натирать мелом"
                            },
                            {
                                "target_lang_word": "натирать мелом",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59039d7b-a7b3-301a-8d2f-b9fd80d30866",
                    "sense_definition": "To write, mark, or draw something with chalk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "writing_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shop owner <w>chalked</w> the daily specials on a board outside the café.",
                            "target_lang_translations": {
                                "ru": "Владелец магазина <w>написал мелом</w> блюда дня на доске у кафе."
                            }
                        },
                        {
                            "text": "Someone had <w>chalked</w> a message on the pavement near the park entrance.",
                            "target_lang_translations": {
                                "ru": "Кто-то <w>написал мелом</w> сообщение на асфальте у входа в парк."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chalk out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Писать, отмечать или рисовать что-либо мелом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "писать мелом",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "рисовать мелом",
                                "target_lang_sense_clarification": "создавать изображение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76444019-0772-3319-8d93-0624b7dc2a41",
                    "sense_definition": "To mark the boundaries or lines of a playing field with white powder.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "marking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The groundskeeper <w>chalked</w> the lines for the tennis match early in the morning.",
                            "target_lang_translations": {
                                "ru": "Смотритель <w>разметил</w> линии для теннисного матча рано утром."
                            }
                        },
                        {
                            "text": "We need to <w>chalk</w> the football pitch before the game starts.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>сделать разметку</w> футбольного поля перед началом игры."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Размечать границы или линии игрового поля белым порошком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размечать",
                                "target_lang_sense_clarification": "наносить разметку"
                            },
                            {
                                "target_lang_word": "делать разметку",
                                "target_lang_sense_clarification": "спортивное поле"
                            }
                        ]
                    }
                }
            ]
        },

                },
                {
                    "sense_id": "a9530db1-fe37-34d0-a227-beacff148b33",
                    "sense_definition": "Uroczyste wydarzenie, ceremonia o charakterze oficjalnym, podkreślająca wagę jakiejś chwili.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ceremony_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczestniczyliśmy w podniosłym <w>akcie</w> nadania sztandaru naszej szkole.",
                            "target_lang_translations": {
                                "ru": "Мы участвовали в торжественной <w>церемонии</w> вручения знамени нашей школе."
                            }
                        },
                        {
                            "text": "Sam <w>akt</w> zaślubin odbył się w małym, zabytkowym kościele.",
                            "target_lang_translations": {
                                "ru": "Сам <w>акт</w> бракосочетания состоялся в маленькой старинной церкви."
                            }
                        }
                    ],
                    "synonyms": [
                        "ceremonia",
                        "uroczystość",
                        "obrzęd",
                        "obchód"
                    ],
                    "common_phrases": [
                        "akt koronacji",
                        "akt erekcyjny",
                        "akt dekoracji",
                        "akt zaślubin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественное событие, официальная церемония, подчеркивающая важность момента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "церемония",
                                "target_lang_sense_clarification": "торжественный обряд"

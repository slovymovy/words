                        "en": [
                            {
                                "target_lang_word": "stock",
                                "target_lang_sense_clarification": "cards"
                            },
                            {
                                "target_lang_word": "draw pile",
                                "target_lang_sense_clarification": "cards"
                            }
                        ]
                    }
                }

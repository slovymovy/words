            "senses": [
                {
                    "sense_id": "98353a86-1df9-3fc5-b61e-f5c7d312071c",
                    "sense_definition": "The last match or game in a competition that decides the winner.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our team played very well and reached the <w>final</w>.",
                            "target_lang_translations": {
                                "ru": "Наша команда играла очень хорошо и вышла в <w>фина́л</w>."
                            }
                        },
                        {
                            "text": "Millions of people watched the World Cup <w>final</w> on television.",
                            "target_lang_translations": {
                                "ru": "Миллионы людей смотрели <w>фина́л</w> Чемпионата мира по телевизору."
                            }

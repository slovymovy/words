    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e49e182b-9d20-3cfb-9a5e-6765c3c55fbb",
                    "sense_definition": "Moczyć lub pokrywać powierzchnię czegoś cieczą rozpadającą się na drobne krople.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "wetting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fryzjer zaczął delikatnie <w>opryskiwać</w> moje włosy wodą przed rozpoczęciem strzyżenia.",
                            "target_lang_translations": {
                                "ru": "Парикмахер начал осторожно <w>сбрызгивать</w> мои волосы водой перед началом стрижки."
                            }
                        },
                        {
                            "text": "Przejeżdżające szybko samochody mogą <w>opryskiwać</w> pieszych brudną wodą z kałuży.",
                            "target_lang_translations": {
                                "ru": "Быстро проезжающие машины могут <w>обрызгивать</w> пешеходов грязной водой из лужи."
                            }
                        }
                    ],
                    "synonyms": [
                        "zraszać",
                        "spryskiwać",
                        "ochlapywać"
                    ],
                    "antonyms": [
                        "osuszać",
                        "wycierać"
                    ],
                    "common_phrases": [
                        "opryskiwać wodą",
                        "opryskiwać farbą",
                        "opryskiwać błotem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мочить или покрывать поверхность чего-либо жидкостью в виде мелких капель или брызг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обрызгивать",
                                "target_lang_sense_clarification": "покрывать каплями или грязью (например, проезжая мимо)"
                            },
                            {
                                "target_lang_word": "опрыскивать",
                                "target_lang_sense_clarification": "покрывать мелкими каплями с помощью распылителя"
                            },
                            {
                                "target_lang_word": "сбрызгивать",
                                "target_lang_sense_clarification": "слегка смачивать водой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c169dd22-406e-3c9d-ae1d-177d8e339e0d",
                    "sense_definition": "Rozpylać środki chemiczne na rośliny w celu zwalczania szkodników lub chorób.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnicy musieli <w>opryskiwać</w> swoje uprawy, aby uchronić je przed inwazją owadów.",
                            "target_lang_translations": {
                                "ru": "Фермеры должны были <w>опрыскивать</w> свои посевы, чтобы защитить их от нашествия насекомых."
                            }
                        },
                        {
                            "text": "Wiosną należy <w>opryskiwać</w> drzewa owocowe specjalnym preparatem przeciwko grzybom.",
                            "target_lang_translations": {
                                "ru": "Весной следует <w>опрыскивать</w> фруктовые деревья специальным препаратом против грибка."
                            }
                        }
                    ],
                    "synonyms": [
                        "pryskać"
                    ],
                    "common_phrases": [
                        "opryskiwać rośliny",
                        "opryskiwać drzewa",
                        "opryskiwać pestycydami",
                        "opryskiwać uprawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распылять химические средства на растения для борьбы с вредителями или болезнями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опрыскивать",
                                "target_lang_sense_clarification": "обрабатывать растения химикатами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e95b637-97a0-382e-a587-2068d2c4e570",
                    "sense_definition": "Moczyć samego siebie lub siebie nawzajem cieczą, która rozpryskuje się na krople.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "wetting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci bawiły się w ogrodzie i zaczęły <w>opryskiwać się</w> wodą z węża ogrodowego.",
                            "target_lang_translations": {
                                "ru": "Дети играли в саду и начали <w>обрызгивать друг друга</w> водой из садового шланга."
                            }
                        },
                        {
                            "text": "W upalne dni lubimy <w>opryskiwać się</w> mgiełką wodną dla ochłody.",
                            "target_lang_translations": {
                                "ru": "В жаркие дни мы любим <w>опрыскивать себя</w> водяной дымкой для прохлады."
                            }
                        }
                    ],
                    "synonyms": [
                        "oblewać się",
                        "chlapać się"
                    ],
                    "common_phrases": [
                        "opryskiwać się wodą",
                        "opryskiwać się nawzajem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мочить самого себя или друг друга жидкостью, которая разлетается на капли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обрызгиваться",
                                "target_lang_sense_clarification": "попадать брызгами на себя или друг на друга"
                            },
                            {
                                "target_lang_word": "брызгаться",
                                "target_lang_sense_clarification": "играть с водой, плескаться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
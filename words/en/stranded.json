                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обесценившийся",
                                "target_lang_sense_clarification": "об активах"
                            },
                            {
                                "target_lang_word": "невозвратный",
                                "target_lang_sense_clarification": "о расходах или издержках"
                            }
                        ]

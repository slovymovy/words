    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fb78e709-7ace-37eb-9b89-4816a69a8a21",
                    "sense_definition": "umiejętność rozpoznawania określonych miejsc i kierunków w terenie",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_awareness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Turysta stracił <orientację> w gęstym lesie i musiał użyć kompasu.",
                            "target_lang_translations": {
                                "ru": "Турист потерял <w>ориентацию</w> в густом лесу и был вынужден воспользоваться компасом."
                            }
                        },
                        {
                            "text": "Mój brat ma świetną <orientację> w terenie i nigdy się nie gubi w nowym mieście.",
                            "target_lang_translations": {
                                "ru": "У моего брата отличная <w>ориентация</w> на местности, и он никогда не теряется в новом городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "orientowanie się"
                    ],
                    "antonyms": [
                        "dezorientacja"
                    ],
                    "common_phrases": [
                        "orientacja w terenie",
                        "zmysł orientacji",
                        "stracić orientację"
                    ],
                    "target_lang_definitions": {
                        "ru": "способность определять своё местоположение и направление движения на местности"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ориентация",
                                "target_lang_sense_clarification": "на местности"
                            },
                            {
                                "target_lang_word": "ориентирование",
                                "target_lang_sense_clarification": "процесс определения местоположения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f79bbdc7-284a-3701-a754-3cf713a394f7",
                    "sense_definition": "określone poglądy polityczne lub preferencje seksualne",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "preferences",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy człowiek zasługuje na szacunek niezależnie od swojej <orientacji> seksualnej.",
                            "target_lang_translations": {
                                "ru": "Каждый человек заслуживает уважения независимо от своей сексуальной <w>ориентации</w>."
                            }
                        },
                        {
                            "text": "W ankiecie nie trzeba było podawać swojej <orientacji> politycznej ani wyznania.",
                            "target_lang_translations": {
                                "ru": "В анкете не нужно было указывать свою политическую <w>ориентацию</w> или вероисповедание."
                            }
                        }
                    ],
                    "synonyms": [
                        "ukierunkowanie",
                        "tendencja",
                        "linia"
                    ],
                    "common_phrases": [
                        "orientacja seksualna",
                        "orientacja polityczna",
                        "orientacja zawodowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "определённые политические взгляды или сексуальные предпочтения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ориентация",
                                "target_lang_sense_clarification": "взгляды, предпочтения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66be0355-13f9-3a78-a3f1-edcda479babe",
                    "sense_definition": "rozeznanie w sytuacji, jakimś zagadnieniu oraz umiejętność ich oceny",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mental_awareness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy pracownik potrzebuje kilku dni na <orientację> w zasadach panujących w firmie.",
                            "target_lang_translations": {
                                "ru": "Новому сотруднику нужно несколько дней на <w>ориентацию</w> в правилах, действующих в компании."
                            }
                        },
                        {
                            "text": "Dobra <orientacja> w przepisach prawnych jest niezbędna w pracy adwokata.",
                            "target_lang_translations": {
                                "ru": "Хорошая <w>ориентация</w> в правовых нормах необходима в работе адвоката."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozeznanie",
                        "znajomość rzeczy",
                        "rozumienie",
                        "orientowanie się"
                    ],
                    "antonyms": [
                        "dezorientacja"
                    ],
                    "common_phrases": [
                        "orientacja w sytuacji",
                        "orientacja w temacie"
                    ],
                    "target_lang_definitions": {
                        "ru": "понимание ситуации, осведомлённость в каком-либо вопросе"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ориентация",
                                "target_lang_sense_clarification": "понимание, осведомлённость"
                            },
                            {
                                "target_lang_word": "осведомлённость",
                                "target_lang_sense_clarification": "знание предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da4563b0-234d-3c05-8beb-49dedf81902e",
                    "sense_definition": "ustawienie, pozycja względem jakiegoś punktu odniesienia",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "positioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Południowa <orientacja> okien zapewnia dużo naturalnego światła w salonie przez cały dzień.",
                            "target_lang_translations": {
                                "ru": "Южная <w>ориентация</w> окон обеспечивает много естественного света в гостиной в течение всего дня."
                            }
                        },
                        {
                            "text": "Prawidłowa <orientacja> mapy względem północy jest kluczowa podczas górskiej wędrówki.",
                            "target_lang_translations": {
                                "ru": "Правильная <w>ориентация</w> карты относительно севера является ключевой во время горного похода."
                            }
                        }
                    ],
                    "synonyms": [
                        "ustawienie",
                        "ukierunkowanie"
                    ],
                    "common_phrases": [
                        "orientacja budynku",
                        "orientacja mapy"
                    ],
                    "target_lang_definitions": {
                        "ru": "расположение или положение относительно какой-либо точки отсчёта"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ориентация",
                                "target_lang_sense_clarification": "расположение"
                            }
                        ]
                    }
                }
            ]
        }

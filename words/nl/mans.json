                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'man'.",
                        "ru": "Множественное число слова 'man'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "men",
                                "target_lang_sense_clarification": "non-standard or dialectal plural"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мужчины",
                                "target_lang_sense_clarification": "нестандартное или диалектное множественное число"
                            }
                        ]
                    }
                }
            ]
        }
    ]

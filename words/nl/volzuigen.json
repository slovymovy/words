{
    "word_family": [
        "zuigen",
        "opzuigen",
        "uitzuigen",
        "afzuigen",
        "inzuigen",
        "wegzuigen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "78662c7d-f22a-34a0-ac25-293aa582668e",
                    "sense_definition": "Zich volledig vullen met een vloeistof door deze op te nemen (absorberen).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "absorption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De droge spons zal zich direct met water <w>volzuigen</w> zodra je hem onderdompelt.",
                            "target_lang_translations": {
                                "en": "The dry sponge will immediately <w>soak up</w> water as soon as you submerge it.",
                                "ru": "Сухая губка сразу же <w>напитается</w> водой, как только вы погрузите её в воду."
                            }
                        },
                        {
                            "text": "Een teek kan zich helemaal <w>volzuigen</w> met het bloed van een gastheer.",
                            "target_lang_translations": {
                                "en": "A tick can completely <w>gorge itself</w> on the blood of a host.",
                                "ru": "Клещ может полностью <w>насосаться</w> крови хозяина."
                            }
                        }
                    ],
                    "synonyms": [
                        "absorberen",
                        "opnemen",
                        "doordrenken"
                    ],
                    "antonyms": [
                        "uitknijpen",
                        "uitwringen"
                    ],
                    "common_phrases": [
                        "zich volzuigen met water",
                        "zich volzuigen met bloed"
                    ],
                    "target_lang_definitions": {
                        "en": "To fill oneself completely with a liquid by absorbing it.",
                        "ru": "Полностью наполниться жидкостью, впитывая её (абсорбировать)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to soak up",
                                "target_lang_sense_clarification": "absorbing liquid like a sponge"
                            },
                            {
                                "target_lang_word": "to gorge oneself",
                                "target_lang_sense_clarification": "filling completely with food or blood (e.g., parasites)"
                            },
                            {
                                "target_lang_word": "to suck full",
                                "target_lang_sense_clarification": "literal translation, filling by suction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "напитаться",
                                "target_lang_sense_clarification": "влагой (о губке, ткани)"
                            },
                            {
                                "target_lang_word": "насосаться",
                                "target_lang_sense_clarification": "кровью (о насекомых)"
                            },
                            {
                                "target_lang_word": "пропитаться",
                                "target_lang_sense_clarification": "жидкостью насквозь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1edc221-3d2e-3ebd-bbd9-acb77200c90e",
                    "sense_definition": "Een ruimte of orgaan volledig vullen door lucht of gas naar binnen te zuigen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "suction_filling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de klim wilde hij zijn longen <w>volzuigen</w> met de frisse berglucht.",
                            "target_lang_translations": {
                                "en": "After the climb, he wanted to <w>fill</w> his lungs with the fresh mountain air.",
                                "ru": "После подъема он хотел <w>наполнить</w> легкие свежим горным воздухом."
                            }
                        },
                        {
                            "text": "De cilinder moet zich eerst <w>volzuigen</w> met het gasmengsel voordat de motor start.",
                            "target_lang_translations": {
                                "en": "The cylinder must first <w>fill up</w> with the gas mixture before the engine starts.",
                                "ru": "Цилиндр должен сначала <w>наполниться</w> газовой смесью перед запуском двигателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "vullen"
                    ],
                    "antonyms": [
                        "leegblazen"
                    ],
                    "common_phrases": [
                        "longen volzuigen"
                    ],
                    "target_lang_definitions": {
                        "en": "To fill a space or organ completely by sucking in air or gas.",
                        "ru": "Заполнить пространство или орган, втягивая внутрь воздух или газ."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to fill",
                                "target_lang_sense_clarification": "filling lungs or a container"
                            },
                            {
                                "target_lang_word": "to suck full",
                                "target_lang_sense_clarification": "mechanical filling via suction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наполнить",
                                "target_lang_sense_clarification": "легкие воздухом"
                            },
                            {
                                "target_lang_word": "наполниться",
                                "target_lang_sense_clarification": "всасыванием (о механизмах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
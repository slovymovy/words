{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "71068a90-164c-3fef-a777-4001b916e5e5",
                    "sense_definition": "In isolation, a request for repetition or clarification of what has just been said.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "request_repetition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "—I didn't hear you clearly. —<w>Eh</w>? Can you say that again?",
                            "target_lang_translations": {
                                "ru": "—Я не расслышал. —<w>А</w>? Можешь повторить?"
                            }
                        },
                        {
                            "text": "—We need to leave now. —<w>Eh</w>? I thought we had more time.",
                            "target_lang_translations": {
                                "ru": "—Нам пора уходить. —<w>Что</w>? Я думал, у нас еще есть время."
                            }
                        }
                    ],
                    "synonyms": [
                        "pardon",
                        "sorry",
                        "what",
                        "huh",
                        "come again"
                    ],
                    "common_phrases": [
                        "eh? what?",
                        "say again, eh?"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как просьба повторить или разъяснить только что сказанное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "вопросительная частица"
                            },
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "вопрос для уточнения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2565dc42-8cfe-39ae-93fb-e74141ffa3b9",
                    "sense_definition": "Used as a tag question, to emphasise what goes before or to request that the listener express an opinion about what has been said.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tag_question",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is pretty cold outside today, <w>eh</w>?",
                            "target_lang_translations": {
                                "ru": "Сегодня на улице довольно холодно, <w>да</w>?"
                            }
                        },
                        {
                            "text": "You really like this movie, <w>eh</w>?",
                            "target_lang_translations": {
                                "ru": "Тебе и правда нравится этот фильм, <w>а</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "y'know",
                        "huh",
                        "what"
                    ],
                    "common_phrases": [
                        "good, eh",
                        "right, eh"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in Canadian and British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Употребляется в конце предложения для подтверждения сказанного или выяснения мнения собеседника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "вопрос-подтверждение"
                            },
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "вопрос-подтверждение"
                            },
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "усилительная частица в вопросе"
                            },
                            {
                                "target_lang_word": "не так ли",
                                "target_lang_sense_clarification": "формальный вопрос-подтверждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e9a4388-5ce0-3641-9cb7-c0e89752cfbd",
                    "sense_definition": "In isolation, expressing surprise or confusion.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "surprise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Eh</w>? I never expected you to be here!",
                            "target_lang_translations": {
                                "ru": "<w>А</w>? Не ожидал тебя здесь увидеть!"
                            }
                        },
                        {
                            "text": "—You won the lottery! —<w>Eh</w>? You must be joking!",
                            "target_lang_translations": {
                                "ru": "—Ты выиграл в лотерею! —<w>Что</w>? Ты, должно быть, шутишь!"
                            }
                        }
                    ],
                    "synonyms": [
                        "huh",
                        "wha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает удивление или недоумение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "возглас удивления"
                            },
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "возглас удивления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "229af6ab-4e8b-390e-9d90-42a9668a4aec",
                    "sense_definition": "Said to get someone's attention.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Eh</w>, look at what I found over here!",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>, смотри, что я тут нашел!"
                            }
                        },
                        {
                            "text": "<w>Eh</w>, come here for a second.",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>, подойди-ка сюда на секунду."
                            }
                        }
                    ],
                    "synonyms": [
                        "hey",
                        "oi",
                        "I say"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас для привлечения внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "оклик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90b4ab1e-2516-3461-9aa0-6be1d7c5080c",
                    "sense_definition": "Expressing apathy or lack of enthusiasm.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "apathy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "—Do you want to go to the cinema? —<w>Eh</w>, I don't really feel like it.",
                            "target_lang_translations": {
                                "ru": "—Хочешь пойти в кино? —<w>Ну</w>, мне не особо хочется."
                            }
                        },
                        {
                            "text": "I looked at the results and felt just, <w>eh</w>.",
                            "target_lang_translations": {
                                "ru": "Я посмотрел на результаты, и ощущение было — <w>так себе</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает безразличие, сомнение или отсутствие энтузиазма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "выражение сомнения"
                            },
                            {
                                "target_lang_word": "м",
                                "target_lang_sense_clarification": "звук раздумья или неопределенности"
                            },
                            {
                                "target_lang_word": "так себе",
                                "target_lang_sense_clarification": "выражение безразличия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "18906fcf-0f10-3370-b8f0-33780a46934e",
                    "sense_definition": "Of mediocre quality; unremarkable.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The food at that restaurant was just <w>eh</w>, nothing special.",
                            "target_lang_translations": {
                                "ru": "Еда в том ресторане была <w>так себе</w>, ничего особенного."
                            }
                        },
                        {
                            "text": "I thought the movie was a bit <w>eh</w> compared to the book.",
                            "target_lang_translations": {
                                "ru": "Мне показалось, что фильм <w>средненький</w> по сравнению с книгой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "just eh",
                        "pretty eh"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Посредственный, ничем не примечательный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так себе",
                                "target_lang_sense_clarification": "посредственный"
                            },
                            {
                                "target_lang_word": "средненький",
                                "target_lang_sense_clarification": "невыдающийся"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "054a261b-1763-3486-a99b-2f7494258e40",
                    "sense_definition": "To use the interjection eh.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He kept <w>ehing</w> throughout the conversation, which was distracting.",
                            "target_lang_translations": {
                                "ru": "Он постоянно <w>экал</w> во время разговора, что отвлекало."
                            }
                        },
                        {
                            "text": "Stop <w>ehing</w> at me and speak clearly.",
                            "target_lang_translations": {
                                "ru": "Перестань <w>экать</w> и говори внятно."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянно употреблять междометие «э» или «а»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экать",
                                "target_lang_sense_clarification": "произносить «э»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4bf4d6b2-dac8-3ca2-ab0f-f6b48c1cfb14",
                    "sense_definition": "Used to negate the following verb; do not.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>eh</w> know what you are talking about.",
                            "target_lang_translations": {
                                "ru": "Я <w>не</w> знаю, о чем ты говоришь."
                            }
                        },
                        {
                            "text": "He <w>eh</w> want to go to the market today.",
                            "target_lang_translations": {
                                "ru": "Он <w>не</w> хочет идти на рынок сегодня."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Caribbean English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрицательная частица (в диалектах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не",
                                "target_lang_sense_clarification": "отрицание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
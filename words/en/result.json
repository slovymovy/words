{
    "word_family": [
        "resultant",
        "resultless"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4124755b-ce87-32b2-a526-68ff75687a88",
                    "sense_definition": "The outcome, consequence, or effect of an action, process, or situation.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accident was the direct <w>result</w> of careless driving.",
                            "target_lang_translations": {
                                "ru": "Авария стала прямым <w>результатом</w> неосторожного вождения."
                            }
                        },
                        {
                            "text": "He lost his job as a <w>result</w> of the scandal.",
                            "target_lang_translations": {
                                "ru": "Он потерял работу в <w>результате</w> скандала."
                            }
                        }
                    ],
                    "common_phrases": [
                        "as a result",
                        "result of",
                        "end result",
                        "direct result"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исход, последствие или эффект действия, процесса или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "результат",
                                "target_lang_sense_clarification": "общее значение: итог действия или события"
                            },
                            {
                                "target_lang_word": "итог",
                                "target_lang_sense_clarification": "то, что получается в конце, вывод"
                            },
                            {
                                "target_lang_word": "исход",
                                "target_lang_sense_clarification": "разрешение ситуации, конец события"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "412de87d-61a6-3563-a8ad-b2e049cfa7d0",
                    "sense_definition": "The final score, mark, or ranking in a game, competition, exam, or election.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "score",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was very happy with her exam <w>results</w>.",
                            "target_lang_translations": {
                                "ru": "Она была очень довольна <w>результатами</w> своих экзаменов."
                            }
                        },
                        {
                            "text": "The football <w>result</w> was a draw.",
                            "target_lang_translations": {
                                "ru": "<w>Результат</w> футбольного матча — ничья."
                            }
                        }
                    ],
                    "common_phrases": [
                        "exam results",
                        "election result",
                        "match result"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окончательный счет, оценка или место в игре, соревновании, экзамене или на выборах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "результат",
                                "target_lang_sense_clarification": "показатель успеваемости или итог матча"
                            },
                            {
                                "target_lang_word": "счёт",
                                "target_lang_sense_clarification": "в спорте: числовое выражение итогов игры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6468c915-c616-3af7-9ffc-cbe7290582e2",
                    "sense_definition": "A successful, beneficial, or tangible effect achieved through effort.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new manager is under pressure to produce <w>results</w> quickly.",
                            "target_lang_translations": {
                                "ru": "На нового менеджера оказывают давление, требуя быстро показать <w>результаты</w>."
                            }
                        },
                        {
                            "text": "We are finally seeing <w>results</w> from our marketing campaign.",
                            "target_lang_translations": {
                                "ru": "Мы наконец видим <w>результаты</w> нашей маркетинговой кампании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get results",
                        "achieve results",
                        "produce results"
                    ],
                    "target_lang_definitions": {
                        "ru": "Успешный, полезный или ощутимый эффект, достигнутый благодаря усилиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "результат",
                                "target_lang_sense_clarification": "достижение, успех"
                            },
                            {
                                "target_lang_word": "плод",
                                "target_lang_sense_clarification": "перен. результат труда или усилий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7f13297d-3dd0-30a7-ade5-45305b3a984c",
                    "sense_definition": "To cause a particular outcome or situation to happen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cause_effect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy rain will likely <w>result</w> in floods across the region.",
                            "target_lang_translations": {
                                "ru": "Сильный дождь, вероятно, <w>приведет</w> к наводнениям по всему региону."
                            }
                        },
                        {
                            "text": "Carelessness often <w>results</w> in serious accidents.",
                            "target_lang_translations": {
                                "ru": "Неосторожность часто <w>приводит</w> к серьезным авариям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "result in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать определенный исход или ситуацию; приводить к чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приводить (к)",
                                "target_lang_sense_clarification": "служить причиной, вызывать последствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfe9d20f-4a65-3644-aabd-b3d234562f62",
                    "sense_definition": "To be caused by or happen because of something else.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "cause_effect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most of his health problems <w>result</w> from poor diet and lack of exercise.",
                            "target_lang_translations": {
                                "ru": "Большинство его проблем со здоровьем <w>проистекают</w> из плохого питания и недостатка физических упражнений."
                            }
                        },
                        {
                            "text": "Nothing good can <w>result</w> from this argument.",
                            "target_lang_translations": {
                                "ru": "Ничего хорошего из этого спора не <w>выйдет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "follow",
                        "arise"
                    ],
                    "common_phrases": [
                        "result from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть вызванным чем-то или происходить вследствие чего-то другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проистекать (из)",
                                "target_lang_sense_clarification": "быть следствием, брать начало"
                            },
                            {
                                "target_lang_word": "являться результатом",
                                "target_lang_sense_clarification": "возникать вследствие чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e1f2cf17-4a48-3710-abfc-5d62b0ae6584",
                    "sense_definition": "An exclamation used to express satisfaction or joy at a favorable outcome.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I managed to get the last ticket for the show—<w>result</w>!",
                            "target_lang_translations": {
                                "ru": "Мне удалось достать последний билет на шоу — <w>победа</w>!"
                            }
                        },
                        {
                            "text": "Found a tenner on the pavement this morning. <w>Result</w>!",
                            "target_lang_translations": {
                                "ru": "Нашел десятку на тротуаре сегодня утром. <w>Удача</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "get in"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used informally to express triumph"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, используемое для выражения удовлетворения или радости от благоприятного исхода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "успех",
                                "target_lang_sense_clarification": "выражение радости от удачи"
                            },
                            {
                                "target_lang_word": "победа",
                                "target_lang_sense_clarification": "разг. триумф, удачное завершение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
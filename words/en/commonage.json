    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c69b8f4-52dd-3396-8a25-34cdb9d3c85a",
                    "sense_definition": "Shared land; a common or commons.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "land_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local farmers allow their sheep to graze freely on the village <w>commonage</w>.",
                            "target_lang_translations": {
                                "ru": "Местные фермеры позволяют своим овцам свободно пастись на деревенском <w>выгоне</w>."
                            }
                        },
                        {
                            "text": "We went for a long walk across the grassy <w>commonage</w> near the hills.",
                            "target_lang_translations": {
                                "ru": "Мы отправились на долгую прогулку по поросшей травой <w>общинной земле</w> возле холмов."
                            }
                        }
                    ],
                    "synonyms": [
                        "common",
                        "commons",
                        "common land"
                    ],
                    "antonyms": [
                        "private property"
                    ],
                    "common_phrases": [
                        "village commonage",
                        "local commonage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Земля, находящаяся в общем пользовании; общинный выгон или пастбище."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общинная земля",
                                "target_lang_sense_clarification": "территория общего пользования"
                            },
                            {
                                "target_lang_word": "выгон",
                                "target_lang_sense_clarification": "пастбище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45445285-5e71-3613-a01f-c73f24fa648a",
                    "sense_definition": "The right to pasture animals on common land.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_right",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He exercised his <w>commonage</w> by bringing his cattle to the open field every summer.",
                            "target_lang_translations": {
                                "ru": "Он пользовался своим <w>правом выпаса</w>, каждое лето выгоняя скот в открытое поле."
                            }
                        },
                        {
                            "text": "Disputes often arose regarding the extent of <w>commonage</w> allowed for each family in the parish.",
                            "target_lang_translations": {
                                "ru": "Часто возникали споры относительно объема <w>прав на выпас</w>, дозволенного каждой семье в приходе."
                            }
                        }
                    ],
                    "synonyms": [
                        "grazing rights",
                        "pasture rights"
                    ],
                    "common_phrases": [
                        "right of commonage",
                        "exercise commonage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Право пасти скот на общинной земле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "право выпаса",
                                "target_lang_sense_clarification": "на общинной земле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ad25677-afb2-3f0a-9ee3-23b17b2f45ff",
                    "sense_definition": "The condition of land that is held in common.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The historical system of <w>commonage</w> ensured that resources were shared among the community.",
                            "target_lang_translations": {
                                "ru": "Историческая система <w>общинного землепользования</w> обеспечивала совместное использование ресурсов общиной."
                            }
                        },
                        {
                            "text": "Scholars debated the economic efficiency of <w>commonage</w> versus private ownership.",
                            "target_lang_translations": {
                                "ru": "Ученые спорили об экономической эффективности <w>общинного владения</w> по сравнению с частной собственностью."
                            }
                        }
                    ],
                    "synonyms": [
                        "common ownership",
                        "joint ownership"
                    ],
                    "antonyms": [
                        "private ownership"
                    ],
                    "common_phrases": [
                        "system of commonage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система или состояние общинного землевладения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общинное владение",
                                "target_lang_sense_clarification": "форма собственности"
                            },
                            {
                                "target_lang_word": "общинное землепользование",
                                "target_lang_sense_clarification": "система использования земли"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
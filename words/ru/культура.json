    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "905c2920-11d4-37d4-a896-c1268bf7a3ec",
                    "sense_definition": "Совокупность производственных, общественных и духовных достижений людей.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "human_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Русская <w>культура</w> подарила миру великих писателей, композиторов и художников.",
                            "target_lang_translations": {
                                "en": "Russian <w>culture</w> gave the world great writers, composers, and artists."
                            }
                        },
                        {
                            "text": "Мы должны беречь и сохранять национальную <w>культуру</w> для будущих поколений.",
                            "target_lang_translations": {
                                "en": "We must protect and preserve national <w>culture</w> for future generations."
                            }
                        }
                    ],
                    "synonyms": [
                        "традиции",
                        "искусство"
                    ],
                    "antonyms": [
                        "бескультурье"
                    ],
                    "common_phrases": [
                        "русская культура",
                        "мировая культура",
                        "история культуры",
                        "памятник культуры"
                    ],
                    "target_lang_definitions": {
                        "en": "The aggregate of productive, social, and spiritual achievements of people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "culture",
                                "target_lang_sense_clarification": "arts, customs, and social achievements"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39b98bf5-d724-3393-bc01-0b11baad399f",
                    "sense_definition": "Уровень нравственного и интеллектуального развития, воспитанность человека.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "human_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот человек высокой <w>культуры</w> всегда вежлив и внимателен к окружающим.",
                            "target_lang_translations": {
                                "en": "This person of high <w>culture</w> is always polite and attentive to others."
                            }
                        },
                        {
                            "text": "Ему явно не хватает внутренней <w>культуры</w>, чтобы вести себя прилично в обществе.",
                            "target_lang_translations": {
                                "en": "He clearly lacks inner <w>culture</w> to behave decently in society."
                            }
                        }
                    ],
                    "synonyms": [
                        "приличие"
                    ],
                    "antonyms": [
                        "бескультурье",
                        "неприличие"
                    ],
                    "common_phrases": [
                        "человек высокой культуры",
                        "культура поведения",
                        "культура речи",
                        "внутренняя культура"
                    ],
                    "target_lang_definitions": {
                        "en": "The level of moral and intellectual development, a person's good breeding."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "culture",
                                "target_lang_sense_clarification": "personal refinement and manners"
                            },
                            {
                                "target_lang_word": "refinement",
                                "target_lang_sense_clarification": "good breeding and sophistication"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc1b12f0-6546-3c48-9ef9-c6a7016ddfc2",
                    "sense_definition": "Сложившиеся традиции, правила и порядок в какой-либо организации или социальной среде.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "human_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашей компании сложилась особая корпоративная <w>культура</w>, основанная на доверии.",
                            "target_lang_translations": {
                                "en": "A special corporate <w>culture</w> based on trust has developed in our company."
                            }
                        },
                        {
                            "text": "Политическая <w>культура</w> граждан играет важную роль в развитии демократического государства.",
                            "target_lang_translations": {
                                "en": "The political <w>culture</w> of citizens plays an important role in the development of a democratic state."
                            }
                        }
                    ],
                    "synonyms": [
                        "традиции"
                    ],
                    "common_phrases": [
                        "корпоративная культура",
                        "организационная культура",
                        "политическая культура"
                    ],
                    "target_lang_definitions": {
                        "en": "Established traditions, rules, and order in an organization or social environment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "culture",
                                "target_lang_sense_clarification": "shared values in a group or organization"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8b5fd19-c345-3166-8be4-11e590baa15c",
                    "sense_definition": "Растение, которое выращивают и разводят для хозяйственных целей.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cultivation_biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пшеница — это самая важная зерновая <w>культура</w> во многих странах мира.",
                            "target_lang_translations": {
                                "en": "Wheat is the most important grain <w>crop</w> in many countries of the world."
                            }
                        },
                        {
                            "text": "Фермеры начали уборку технических <w>культур</w> в конце августа.",
                            "target_lang_translations": {
                                "en": "Farmers began harvesting industrial <w>crops</w> at the end of August."
                            }
                        }
                    ],
                    "antonyms": [
                        "растительность"
                    ],
                    "common_phrases": [
                        "зерновые культуры",
                        "технические культуры",
                        "сельскохозяйственные культуры",
                        "овощные культуры"
                    ],
                    "target_lang_definitions": {
                        "en": "A plant that is grown and bred for economic purposes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crop",
                                "target_lang_sense_clarification": "agricultural plant"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a63dca01-7484-3a86-8aab-1a4d87cd1253",
                    "sense_definition": "Процесс разведения и выращивания растений.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cultivation_biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Культура</w> винограда требует много солнца и специального состава почвы.",
                            "target_lang_translations": {
                                "en": "Grape <w>cultivation</w> requires a lot of sun and a special soil composition."
                            }
                        },
                        {
                            "text": "Правильная <w>культура</w> земледелия позволяет получать высокие урожаи каждый год.",
                            "target_lang_translations": {
                                "en": "Proper farming <w>practices</w> allow for high yields every year."
                            }
                        }
                    ],
                    "synonyms": [
                        "культивация"
                    ],
                    "common_phrases": [
                        "культура земледелия",
                        "введение в культуру"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of breeding and growing plants."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cultivation",
                                "target_lang_sense_clarification": "growing of plants"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0018b257-f08c-36ec-907a-4865a94ce7d2",
                    "sense_definition": "Совокупность микроорганизмов или клеток, выращенных в лабораторных условиях.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cultivation_biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ученые исследовали, как бактериальная <w>культура</w> реагирует на новый антибиотик.",
                            "target_lang_translations": {
                                "en": "Scientists investigated how the bacterial <w>culture</w> reacts to the new antibiotic."
                            }
                        },
                        {
                            "text": "В лаборатории была выращена чистая <w>культура</w> клеток для проведения эксперимента.",
                            "target_lang_translations": {
                                "en": "A pure cell <w>culture</w> was grown in the laboratory for the experiment."
                            }
                        }
                    ],
                    "synonyms": [
                        "штамм"
                    ],
                    "common_phrases": [
                        "чистая культура",
                        "культура клеток",
                        "бактериальная культура"
                    ],
                    "target_lang_definitions": {
                        "en": "A collection of microorganisms or cells grown under laboratory conditions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "culture",
                                "target_lang_sense_clarification": "biological growth in a lab"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
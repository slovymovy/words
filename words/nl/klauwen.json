                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iemand heeft gisteren mijn portemonnee <w>geklauwd</w>.",
                            "target_lang_translations": {

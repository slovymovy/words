                {
                    "sense_id": "bd9f294c-54ab-301c-b75e-e98c20732c8b",
                    "sense_definition": "Pijnlijk getroffen in het gevoel van eigenwaarde.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>geraakte</w> man verliet zwijgend de vergadering na de kritiek.",
                            "target_lang_translations": {
                                "en": "The <w>offended</w> man left the meeting silently after the criticism.",
                                "ru": "<w>Уязвленный</w> мужчина молча покинул собрание после критики."
                            }
                        },
                        {
                            "text": "Ze reageerde op een zichtbaar <w>geraakte</w> toon toen haar integriteit in twijfel werd getrokken.",
                            "target_lang_translations": {
                                "en": "She responded in a visibly <w>hurt</w> tone when her integrity was questioned.",
                                "ru": "Она ответила явно <w>обиженным</w> тоном, когда ее честность поставили под сомнение."
                            }
                        }
                    ],
                    "synonyms": [
                        "beledigd",
                        "gekwetst",
                        "gepikeerd"
                    ],
                    "common_phrases": [
                        "diep geraakt",
                        "zichtbaar geraakt"
                    ],
                    "target_lang_definitions": {
                        "en": "Painfully affected in one's sense of self-worth.",
                        "ru": "Болезненно задетый в чувстве собственного достоинства."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "offended",
                                "target_lang_sense_clarification": "feeling insulted or slighted"
                            },
                            {
                                "target_lang_word": "hurt",
                                "target_lang_sense_clarification": "emotionally pained"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уязвленный",

            "senses": [
                {
                    "sense_id": "64137e12-3d2e-33a9-9bff-b4667f4a354c",
                    "sense_definition": "Tarcza zegara lub zegarka, na której znajdują się cyfry i podziałki, po której poruszają się wskazówki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zegarmistrz ostrożnie wymienił pęknięty <w>cyferblat</w> w moim starym zegarku.",
                            "target_lang_translations": {
                                "ru": "Часовщик осторожно заменил треснувший <w>циферблат</w> в моих старых часах."
                            }
                        },
                        {
                            "text": "Na wieży ratuszowej błyszczał w słońcu wielki, złocony <w>cyferblat</w>.",
                            "target_lang_translations": {
                                "ru": "На башне ратуши блестел на солнце огромный позолоченный <w>циферблат</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tarcza"
                    ],
                    "common_phrases": [
                        "czytelny cyferblat",
                        "cyferblat zegara"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицевая сторона часов или прибора с цифрами и делениями, по которой движутся стрелки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "циферблат",
                                "target_lang_sense_clarification": "часть часов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cbc3340-b23f-3f3b-ba07-6b53c898267b",
                    "sense_definition": "Potoczne, często żartobliwe lub lekceważące określenie ludzkiej twarzy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli nie przestaniesz mnie obrażać, możesz zaraz dostać w <w>cyferblat</w>.",
                            "target_lang_translations": {
                                "ru": "Если не перестанешь меня оскорблять, можешь сейчас же получить по <w>морде</w>."
                            }
                        },
                        {
                            "text": "Jego ogorzały <w>cyferblat</w> zdradzał lata spędzone na morzu.",
                            "target_lang_translations": {
                                "ru": "Его обветренная <w>физиономия</w> выдавала годы, проведенные в море."
                            }
                        }
                    ],
                    "synonyms": [

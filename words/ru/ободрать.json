            "pos": "verb",
            "senses": [
                {
                    "sense_id": "db59c53a-5637-3d39-9622-c90e5f399401",
                    "sense_definition": "Повредить кожу (на руках, ногах и т. п.), сделав на ней царапины или ссадины.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мальчик упал с велосипеда и умудрился <ободрать> коленку.",
                            "target_lang_translations": {
                                "en": "The boy fell off his bike and managed to <graze> his knee."
                            }
                        },
                        {
                            "text": "Я случайно <ободрал> руку о шершавую стену в подъезде.",
                            "target_lang_translations": {
                                "en": "I accidentally <scraped> my arm against the rough wall in the hallway."
                            }
                        }
                    ],
                    "synonyms": [
                        "поцарапать",
                        "оцарапать",
                        "расцарапать"
                    ],
                    "common_phrases": [
                        "ободрать коленку",
                        "ободрать локти",
                        "ободрать руку",
                        "ободрать нос"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "обычно используется в разговорной речи"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To damage the skin (on hands, legs, etc.) by making scratches or abrasions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "graze",
                                "target_lang_sense_clarification": "to scrape the skin"
                            },
                            {
                                "target_lang_word": "scrape",
                                "target_lang_sense_clarification": "to injure by rubbing against something rough"
                            },
                            {
                                "target_lang_word": "skin",
                                "target_lang_sense_clarification": "specifically for knees or elbows"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "667c710c-f548-3524-ac6e-8453e5869715",
                    "sense_definition": "Снять, оторвать верхний слой или покрытие с поверхности чего-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед покраской стен нужно полностью <ободрать> старые обои.",
                            "target_lang_translations": {
                                "en": "Before painting the walls, you need to completely <strip> the old wallpaper."
                            }

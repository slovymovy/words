            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "f0fd102d-f47a-3bf5-ae84-b3e557cedc2f",
                    "sense_definition": "Słowo łączące wyrazy lub zdania równorzędne, wskazujące na to, że wymienione elementy występują wspólnie; i, a także.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym ogrodzie rosną kwiaty <w>oraz</w> warzywa.",
                            "target_lang_translations": {
                                "ru": "В нашем саду растут цветы, <w>а также</w> овощи."
                            }
                        },
                        {
                            "text": "Wikipedia <w>oraz</w> Wikisłownik działają na oprogramowaniu Mediawiki.",
                            "target_lang_translations": {
                                "ru": "Википедия <w>и</w> Викисловарь работают на программном обеспечении Mediawiki."
                            }
                        }
                    ],
                    "synonyms": [
                        "i",
                        "(a) także",
                        "również"
                    ],
                    "antonyms": [
                        "ani"
                    ],
                    "common_phrases": [
                        "wiedza oraz doświadczenie",
                        "szybko oraz sprawnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, соединяющий слова или равноправные предложения; указывает на то, что перечисленные элементы выступают вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а также",
                                "target_lang_sense_clarification": "используется при присоединении"
                            },
                            {
                                "target_lang_word": "и",
                                "target_lang_sense_clarification": "общий соединительный союз"
                            }
                        ]
                    }
                }
            ]
        },

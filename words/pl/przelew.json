    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "918413eb-7a36-3e78-85e4-e6c9c2efdfc6",
                    "sense_definition": "Przeniesienie określonej kwoty pieniędzy z jednego rachunku bankowego na inny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę dzisiaj zrobić <w>przelew</w> za czynsz.",
                            "target_lang_translations": {
                                "ru": "Мне нужно сегодня сделать <w>перевод</w> за аренду."
                            }
                        },
                        {
                            "text": "Wynagrodzenie otrzymuję co miesiąc <w>przelewem</w> na konto.",
                            "target_lang_translations": {
                                "ru": "Я получаю зарплату каждый месяц <w>переводом</w> на счет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przelew bankowy",
                        "zrobić przelew",
                        "potwierdzenie przelewu",
                        "przelew natychmiastowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещение определенной суммы денег с одного банковского счета на другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевод",
                                "target_lang_sense_clarification": "денежный"
                            },
                            {
                                "target_lang_word": "банковский перевод",
                                "target_lang_sense_clarification": "банковская операция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb4b577b-53ee-348e-abe8-daa60c480fed",
                    "sense_definition": "Otwór w urządzeniach sanitarnych, takich jak wanna czy umywalka, który zapobiega przelaniu się wody.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda bezpiecznie odpłynęła przez <w>przelew</w>, gdy zapomniałem zakręcić kran.",
                            "target_lang_translations": {
                                "ru": "Вода безопасно утекла через <w>перелив</w>, когда я забыл закрыть кран."
                            }
                        },
                        {
                            "text": "Kupiliśmy nową umywalkę z estetycznie ukrytym <w>przelewem</w>.",
                            "target_lang_translations": {
                                "ru": "Мы купили новую раковину с эстетично скрытым <w>переливом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wanna z przelewem",
                        "otwór przelewu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отверстие в сантехнических устройствах (ванна, раковина), предотвращающее переполнение водой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перелив",
                                "target_lang_sense_clarification": "отверстие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cd00035-20e9-3127-b0d5-db6ad35932d9",
                    "sense_definition": "Wystąpienie wody lub innej cieczy z brzegów zbiornika lub koryta.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ulewie nastąpił <w>przelew</w> rzeki przez wały przeciwpowodziowe.",
                            "target_lang_translations": {
                                "ru": "После ливня произошел <w>перелив</w> реки через противопаводковые валы."
                            }
                        },
                        {
                            "text": "Tama została uszkodzona przez gwałtowny <w>przelew</w> wody.",
                            "target_lang_translations": {
                                "ru": "Плотина была повреждена бурным <w>переливом</w> воды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przelew rzeki",
                        "przelew wód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выход воды или другой жидкости за пределы емкости или русла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перелив",
                                "target_lang_sense_clarification": "жидкости через край"
                            },
                            {
                                "target_lang_word": "выход из берегов",
                                "target_lang_sense_clarification": "реки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "476bd47d-38dd-3d1e-9c26-864dbf7340e7",
                    "sense_definition": "Przeniesienie wierzytelności lub praw z majątku pierwotnego wierzyciela do majątku osoby trzeciej.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Strony podpisały umowę o <w>przelew</w> praw autorskich.",
                            "target_lang_translations": {
                                "ru": "Стороны подписали договор о <w>передаче</w> авторских прав."
                            }
                        },
                        {
                            "text": "Dłużnik został oficjalnie powiadomiony o <w>przelewie</w> wierzytelności na bank.",
                            "target_lang_translations": {
                                "ru": "Должник был официально уведомлен об <w>уступке</w> требования банку."
                            }
                        }
                    ],
                    "synonyms": [
                        "cesja"
                    ],
                    "common_phrases": [
                        "przelew wierzytelności",
                        "umowa przelewu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передача прав или требований от первоначального кредитора третьему лицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уступка",
                                "target_lang_sense_clarification": "прав или требований"
                            },
                            {
                                "target_lang_word": "цессия",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

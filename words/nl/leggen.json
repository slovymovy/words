            "senses": [
                {
                    "sense_id": "821c4669-f1e2-319a-bd1f-00f7afc575c7",
                    "sense_definition": "Iets of iemand in een liggende (horizontale) positie plaatsen",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "placement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>leg</w> het boek voorzichtig op de tafel.",
                            "target_lang_translations": {
                                "en": "I carefully <w>lay</w> the book on the table.",
                                "ru": "Я осторожно <w>кладу</w> книгу на стол."
                            }
                        },
                        {
                            "text": "De moeder <w>legt</w> haar kind elke avond om acht uur in bed.",
                            "target_lang_translations": {
                                "en": "The mother <w>puts</w> her child in bed every evening at eight o'clock.",
                                "ru": "Мать <w>укладывает</w> своего ребенка в кровать каждый вечер в восемь часов."
                            }
                        },
                        {
                            "text": "Hij <w>legde</w> zijn hand op mijn schouder om me te troosten.",
                            "target_lang_translations": {
                                "en": "He <w>laid</w> his hand on my shoulder to comfort me.",
                                "ru": "Он <w>положил</w> руку мне на плечо, чтобы утешить меня."
                            }
                        }
                    ],
                    "synonyms": [
                        "neerleggen",
                        "plaatsen"
                    ],
                    "antonyms": [
                        "oppakken",
                        "zetten",
                        "rechtop zetten"
                    ],
                    "common_phrases": [
                        "op tafel leggen",
                        "in bed leggen",
                        "de hand leggen op",
                        "naast zich neer leggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To place something or someone in a lying (horizontal) position",
                        "ru": "Помещать что-либо или кого-либо в лежачее (горизонтальное) положение"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lay",
                                "target_lang_sense_clarification": "to place horizontally"
                            },
                            {
                                "target_lang_word": "put",
                                "target_lang_sense_clarification": "general placement"
                            },
                            {
                                "target_lang_word": "place",
                                "target_lang_sense_clarification": "to position carefully"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "класть",
                                "target_lang_sense_clarification": "помещать горизонтально"
                            },
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "совершенный вид"
                            },
                            {
                                "target_lang_word": "укладывать",
                                "target_lang_sense_clarification": "помещать в постель или аккуратно размещать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8955966d-4a3d-3766-aade-7733e522c144",
                    "sense_definition": "Het produceren en uitstoten van eieren (door vogels, insecten of reptielen)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Onze kippen <w>leggen</w> bijna elke dag een vers ei.",
                            "target_lang_translations": {
                                "en": "Our chickens <w>lay</w> a fresh egg almost every day.",
                                "ru": "Наши куры <w>несут</w> почти каждый день по свежему яйцу."
                            }
                        },
                        {
                            "text": "De zeeschildpad komt aan land om haar eieren in het zand te <w>leggen</w>.",
                            "target_lang_translations": {
                                "en": "The sea turtle comes ashore to <w>lay</w> her eggs in the sand.",
                                "ru": "Морская черепаха выходит на сушу, чтобы <w>отложить</w> яйца в песок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een ei leggen",
                        "eieren leggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To produce and expel eggs (by birds, insects, or reptiles)",
                        "ru": "Производить и откладывать яйца (о птицах, насекомых или рептилиях)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lay",
                                "target_lang_sense_clarification": "to produce eggs"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нести",
                                "target_lang_sense_clarification": "о птицах (нести яйца)"
                            },
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "о рептилиях, насекомых или птицах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9767afcf-d385-38a8-a301-efa952efdd60",
                    "sense_definition": "Zich ergens bevinden (foutief of dialectisch gebruik voor <w>liggen</w>)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dialectal_confusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waar <w>leggen</w> mijn sleutels eigenlijk?",
                            "target_lang_translations": {

                },
                {
                    "sense_id": "d66614bc-9fa3-3dfb-bcf6-59e77c5ae3ea",
                    "sense_definition": "Een verdedigende actie bij volleybal waarbij spelers aan het net springen om de bal tegen te houden.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "obstruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met een goed getimed <w>blok</w> scoorde het team een punt.",
                            "target_lang_translations": {
                                "ru": "Благодаря своевременному <w>блоку</w> команда заработала очко."
                            }
                        },
                        {
                            "text": "De aanvallers kwamen niet langs het hoge <w>blok</w> van de tegenstander.",
                            "target_lang_translations": {
                                "ru": "Нападающие не смогли пройти через высокий <w>блок</w> противника."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитное действие в волейболе, когда игроки прыгают у сетки, чтобы остановить мяч."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блок",
                                "target_lang_sense_clarification": "в спорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc0fbc8b-b1cf-3d95-b655-98ba6219c632",
                    "sense_definition": "Een technische maatregel die een gebruiker verhindert om acties uit te voeren op een computer of website.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "obstruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De moderator legde een <w>blok</w> op aan de gebruiker vanwege wangedrag.",
                            "target_lang_translations": {
                                "ru": "Модератор наложил <w>блокировку</w> на пользователя за плохое поведение."
                            }
                        },
                        {
                            "text": "Na drie dagen werd het <w>blok</w> op zijn account opgeheven.",
                            "target_lang_translations": {
                                "ru": "Через три дня <w>блокировка</w> его аккаунта была снята."
                            }
                        }
                    ],
                    "synonyms": [
                        "bewerkingsblokkade"
                    ],
                    "target_lang_definitions": {
                        "ru": "Техническая мера, которая не позволяет пользователю выполнять действия на компьютере или веб-сайте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блокировка",
                                "target_lang_sense_clarification": "ограничение доступа"
                            },
                            {
                                "target_lang_word": "бан",
                                "target_lang_sense_clarification": "сленг, запрет доступа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a235b13c-8406-35e7-93c5-19ef04d0e7a4",
                    "sense_definition": "Een mechanisme met een of meer schijven waar een touw overheen loopt om kracht over te brengen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zeilers gebruikten een <w>blok</w> om het zeil strak te trekken.",
                            "target_lang_translations": {
                                "ru": "Моряки использовали <w>блок</w>, чтобы натянуть парус."
                            }
                        },
                        {
                            "text": "Met behulp van een takel en een <w>blok</w> hesen ze de last omhoog.",
                            "target_lang_translations": {
                                "ru": "С помощью тали и <w>блока</w> они подняли груз."
                            }
                        }
                    ],
                    "synonyms": [
                        "katrol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Механизм с одним или несколькими дисками, через которые проходит веревка для передачи силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блок",
                                "target_lang_sense_clarification": "грузоподъемный механизм"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "045b2393-ce8f-3935-a800-8fd368fca86b",
                    "sense_definition": "*Eerste persoon enkelvoud tegenwoordige tijd van* <w>blokken</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>blok</w> de hele avond voor mijn examen geschiedenis.",
                            "target_lang_translations": {
                                "ru": "Я <w>зубрю</w> весь вечер перед экзаменом по истории."
                            }
                        },
                        {
                            "text": "Ik <w>blok</w> de uitgang zodat niemand naar buiten kan.",
                            "target_lang_translations": {
                                "ru": "Я <w>блокирую</w> выход, чтобы никто не мог выйти."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "*Первое лицо единственного числа настоящего времени от* <w>blokken</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зубрю",
                                "target_lang_sense_clarification": "усердно учу"
                            },
                            {
                                "target_lang_word": "блокирую",
                                "target_lang_sense_clarification": "преграждаю путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "934b02b9-b8e9-3f29-8024-fa9b7b310867",
                    "sense_definition": "*Gebiedende wijs van* <w>blokken</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Blok</w> dat nummer als ze je blijven bellen!",
                            "target_lang_translations": {
                                "ru": "<w>Заблокируй</w> этот номер, если они продолжат звонить!"
                            }
                        },
                        {
                            "text": "<w>Blok</w> hard voor je toets, anders haal je het niet.",
                            "target_lang_translations": {
                                "ru": "<w>Зубри</w> усердно к тесту, иначе не сдашь."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "*Повелительное наклонение от* <w>blokken</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заблокируй",
                                "target_lang_sense_clarification": "ограничь доступ"
                            },
                            {
                                "target_lang_word": "зубри",
                                "target_lang_sense_clarification": "учи усердно"
                            }
                        ]
                    }
                }
            ]

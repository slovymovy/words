                {
                    "sense_id": "ab9af391-26df-3975-8f12-8360b0de7452",
                    "sense_definition": "Podstawowa grupa społeczna składająca się z rodziców i ich dzieci.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kinship_core",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja <w>rodzina</w> składa się z czterech osób: mamy, taty, brata i mnie.",
                            "target_lang_translations": {
                                "ru": "Моя <w>семья</w> состоит из четырех человек: мамы, папы, брата и меня."
                            }
                        },
                        {
                            "text": "Współczesna <w>rodzina</w> często różni się modelem od tej sprzed lat.",
                            "target_lang_translations": {
                                "ru": "Современная <w>семья</w> часто отличается своей моделью от той, что была годы назад."
                            }
                        }
                    ],
                    "synonyms": [
                        "rodzina elementarna"
                    ],
                    "common_phrases": [
                        "głowa rodziny",
                        "zakładać rodzinę",
                        "rodzina wielodzietna",
                        "rodzina zastępcza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная социальная группа, состоящая из родителей и их детей."

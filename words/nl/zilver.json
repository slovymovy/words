    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "80516f08-ee1d-3a4b-a001-23e96c8e45eb",
                    "sense_definition": "Een kostbaar, glanzend, grijs-wit edelmetaal en scheikundig element (Ag).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De antieke kandelaar is gemaakt van massief <w>zilver</w>.",
                            "target_lang_translations": {
                                "ru": "Антикварный подсвечник сделан из массивного <w>серебра</w>."
                            }
                        },
                        {
                            "text": "Mijn grootmoeder bewaart haar sieraden van puur <w>zilver</w> in een kistje.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка хранит свои украшения из чистого <w>серебра</w> в шкатулке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "puur zilver",
                        "oud zilver"
                    ],
                    "target_lang_definitions": {
                        "ru": "Драгоценный блестящий серо-белый металл и химический элемент (Ag)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серебро",
                                "target_lang_sense_clarification": "металл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8c4de98-0a6e-3d13-af97-f73cb6d60858",
                    "sense_definition": "Een glanzende, grijs-witte kleur die lijkt op het metaal.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor het feest droeg zij een opvallende jurk in het <w>zilver</w>.",
                            "target_lang_translations": {
                                "ru": "На вечеринку она надела эффектное платье в <w>серебре</w>."
                            }
                        },
                        {
                            "text": "De nieuwe auto is verkrijgbaar in zwart, wit en <w>zilver</w>.",
                            "target_lang_translations": {
                                "ru": "Новый автомобиль доступен в черном, белом и <w>серебристом</w> цветах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in het zilver"
                    ],
                    "target_lang_definitions": {
                        "ru": "Блестящий серо-белый цвет, напоминающий этот металл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серебристый цвет",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "серебро",
                                "target_lang_sense_clarification": "цвет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1be16f36-7b99-3481-aed6-d55017af88c7",
                    "sense_definition": "De tweede plaats in een wedstrijd of de medaille die daarbij hoort.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De Nederlandse ploeg won goud en <w>zilver</w> tijdens het toernooi.",
                            "target_lang_translations": {
                                "ru": "Голландская команда выиграла золото и <w>серебро</w> на турнире."
                            }
                        },
                        {
                            "text": "Na een spannende finale moest de atleet genoegen nemen met <w>zilver</w>.",
                            "target_lang_translations": {
                                "ru": "После захватывающего финала атлету пришлось довольствоваться <w>серебром</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "goud en zilver",
                        "zilver winnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Второе место в соревновании или медаль за него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серебро",
                                "target_lang_sense_clarification": "медаль/место"
                            },
                            {
                                "target_lang_word": "серебряная медаль",
                                "target_lang_sense_clarification": "награда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1846100-40d6-36b9-ae49-98ff790e237e",
                    "sense_definition": "De kleur wit of zilver zoals gebruikt in de heraldiek (wapenkunde).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het familiewapen toont een leeuw op een veld van <w>zilver</w>.",
                            "target_lang_translations": {
                                "ru": "Фамильный герб изображает льва на поле из <w>серебра</w>."
                            }
                        },
                        {
                            "text": "In de beschrijving van het wapenschild staat <w>zilver</w> voor de kleur wit.",
                            "target_lang_translations": {
                                "ru": "В описании герба <w>серебро</w> означает белый цвет."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Белый или серебряный цвет в геральдике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серебро",
                                "target_lang_sense_clarification": "геральдика"
                            }
                        ]
                    }
                }
            ]
        }
    ]

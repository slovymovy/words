            "senses": [
                {
                    "sense_id": "6378c0c0-0774-372a-a90b-0554cfd6d252",
                    "sense_definition": "Eén enkele keer; niet vaker.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft die vergissing slechts <w>eenmaal</w> gemaakt.",
                            "target_lang_translations": {
                                "en": "He made that mistake only <w>once</w>.",
                                "ru": "Он совершил эту ошибку всего <w>один раз</w>."
                            }
                        },
                        {
                            "text": "Ik ben slechts <w>eenmaal</w> in Parijs geweest.",
                            "target_lang_translations": {
                                "en": "I have been to Paris only <w>once</w>.",
                                "ru": "Я был в Париже всего <w>один раз</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "eenwerf"
                    ],
                    "common_phrases": [
                        "slechts eenmaal",
                        "voor dit eenmaal"
                    ],
                    "target_lang_definitions": {
                        "en": "One single time; not more often.",
                        "ru": "Один единственный раз; не чаще."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "once",
                                "target_lang_sense_clarification": "frequency"
                            },
                            {
                                "target_lang_word": "one time",
                                "target_lang_sense_clarification": "count"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "один раз",
                                "target_lang_sense_clarification": "счёт"
                            },
                            {
                                "target_lang_word": "однажды",
                                "target_lang_sense_clarification": "наречие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d297418-f7c1-3eaf-b0ea-d9c10724d39a",
                    "sense_definition": "Op het moment dat; wanneer een bepaalde toestand is bereikt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als ik <w>eenmaal</w> een nieuwe baan heb, zal ik je zeker trakteren.",
                            "target_lang_translations": {
                                "en": "<w>Once</w> I have a new job, I will definitely treat you.",
                                "ru": "<w>Как только</w> у меня будет новая работа, я тебя обязательно угощу."
                            }
                        },
                        {
                            "text": "Als de trein <w>eenmaal</w> rijdt, kan hij niet meer stoppen.",
                            "target_lang_translations": {
                                "en": "<w>Once</w> the train is moving, it can no longer stop.",
                                "ru": "<w>Как только</w> поезд поедет, он больше не сможет остановиться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "als ... eenmaal",
                        "eenmaal thuis",
                        "eenmaal binnen"
                    ],
                    "target_lang_definitions": {
                        "en": "As soon as; when a certain state is reached.",
                        "ru": "Как только; когда определенное состояние достигнуто."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "once",
                                "target_lang_sense_clarification": "conjunction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "как только",
                                "target_lang_sense_clarification": "союз"
                            },
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "союз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3b54547-f2cd-3977-a2b1-0e30902b88f2",
                    "sense_definition": "Drukt uit dat iets een feit is dat men moet accepteren en niet kan veranderen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sommige mensen zijn nu <w>eenmaal</w> niet zo vriendelijk.",
                            "target_lang_translations": {
                                "en": "Some people are <w>simply</w> not that friendly.",
                                "ru": "Некоторые люди <w>просто</w> не так дружелюбны."
                            }
                        },
                        {
                            "text": "We hebben nu <w>eenmaal</w> minder tijd dan vroeger.",
                            "target_lang_translations": {
                                "en": "We <w>simply</w> have less time than before.",
                                "ru": "У нас <w>просто</w> меньше времени, чем раньше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nu eenmaal",
                        "zo is het eenmaal"
                    ],
                    "target_lang_definitions": {
                        "en": "Expresses an unchangeable fact that must be accepted.",
                        "ru": "Выражает неизменный факт, который нужно принять."
                    },

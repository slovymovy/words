    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "42ab7bb0-62d9-3fff-9c39-bce42387cb0b",
                    "sense_definition": "jadalny, czerwony owoc rośliny warzywnej, powszechnie używany w kuchni",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "roslina_i_owoc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na śniadanie zjadłem kanapkę z żółtym serem i <w>pomidorem</w>.",
                            "target_lang_translations": {
                                "ru": "На завтрак я съел бутерброд с сыром и <w>помидором</w>."
                            }
                        },
                        {
                            "text": "Do przygotowania sosu do spaghetti potrzebne są świeże, dojrzałe <w>pomidory</w>.",
                            "target_lang_translations": {
                                "ru": "Для приготовления соуса для спагетти нужны свежие, спелые <w>помидоры</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) tomata, tómata, tółmata"
                    ],
                    "common_phrases": [
                        "zupa pomidorowa",
                        "sok pomidorowy",
                        "suszone pomidory",
                        "przecier z pomidorów"
                    ],
                    "target_lang_definitions": {
                        "ru": "съедобный красный плод овощного растения, широко используемый в кулинарии"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помидор",
                                "target_lang_sense_clarification": "плод"
                            },
                            {
                                "target_lang_word": "томат",
                                "target_lang_sense_clarification": "плод (в кулинарии/промышленности)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "387cb7bf-5e58-3ebe-af27-a2b64ecb800e",
                    "sense_definition": "jednoroczna roślina uprawna z rodziny psiankowatych, dająca jadalne, czerwone owoce",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "roslina_i_owoc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W szklarni mojego dziadka rosną wysokie <w>pomidory</w>.",
                            "target_lang_translations": {
                                "ru": "В теплице моего дедушки растут высокие <w>помидоры</w>."
                            }
                        },
                        {
                            "text": "Muszę wieczorem podlać <w>pomidory</w> w ogrodzie, bo ziemia jest bardzo sucha.",
                            "target_lang_translations": {
                                "ru": "Вечером я должен полить <w>помидоры</w> в саду, так как земля очень сухая."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) tomata, tómata, tółmata"
                    ],
                    "common_phrases": [
                        "sadzić pomidory",
                        "krzak pomidora",
                        "uprawa pomidorów"
                    ],
                    "target_lang_definitions": {
                        "ru": "однолетнее культурное растение семейства пасленовых, дающее съедобные красные плоды"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помидор",
                                "target_lang_sense_clarification": "растение"
                            },
                            {
                                "target_lang_word": "томат",
                                "target_lang_sense_clarification": "растение (сельскохозяйственное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9f34981-ac5b-3597-a274-562b1c14ae76",
                    "sense_definition": "dziecięca zabawa towarzyska, w której na każde zadane pytanie należy odpowiadać słowem „pomidor”, powstrzymując się od śmiechu",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci na podwórku głośno śmiały się, grając w <w>pomidora</w>.",
                            "target_lang_translations": {
                                "ru": "Дети во дворе громко смеялись, играя в <w>помидора</w>."
                            }
                        },
                        {
                            "text": "Szybko przegrałem w <w>pomidora</w>, bo zacząłem chichotać już przy pierwszym pytaniu.",
                            "target_lang_translations": {
                                "ru": "Я быстро проиграл в <w>помидора</w>, потому что начал хихикать уже на первом вопросе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grać w pomidora",
                        "zabawa w pomidora"
                    ],
                    "target_lang_definitions": {
                        "ru": "детская игра, в которой на каждый вопрос нужно отвечать словом «помидор», сдерживая смех"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помидор",
                                "target_lang_sense_clarification": "игра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50b21b76-0d8a-3183-8674-0ea40914971e",
                    "sense_definition": "osoba, która ma bardzo czerwoną twarz, zazwyczaj z powodu wstydu, zdenerwowania lub wysiłku fizycznego",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy nauczyciel go wywołał do tablicy, stał się <w>pomidorem</w> ze wstydu.",
                            "target_lang_translations": {
                                "ru": "Когда учитель вызвал его к доске, он стал красным как <w>помидор</w> от стыда."
                            }
                        },
                        {
                            "text": "Po wyczerpującym biegu na dziesięć kilometrów wyglądał jak <w>pomidor</w>.",
                            "target_lang_translations": {
                                "ru": "После изнурительного бега на десять километров он выглядел как <w>помидор</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czerwony jak pomidor",
                        "zrobić się jak pomidor"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "określenie osoby zaczerwienionej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "человек с очень красным лицом, обычно от стыда, волнения или физического напряжения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помидор",
                                "target_lang_sense_clarification": "о человеке с красным лицом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "550b960b-a97f-3226-9fb7-e3836e3259e0",
                    "sense_definition": "czerwony, opuchnięty nos, często kojarzony z nadużywaniem alkoholu lub działaniem zimna",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna w płaszczu miał wielkiego, czerwonego <w>pomidora</w> zamiast nosa.",
                            "target_lang_translations": {
                                "ru": "У мужчины в плаще был огромный красный <w>помидор</w> вместо носа."
                            }
                        },
                        {
                            "text": "Na silnym mrozie każdemu szybko robi się <w>pomidor</w> na twarzy.",
                            "target_lang_translations": {
                                "ru": "На сильном морозе у каждого на лице быстро появляется <w>помидор</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "obraźliwie lub żartobliwie o nosie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "красный, опухший нос, часто ассоциируемый со злоупотреблением алкоголем или воздействием холода"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помидор",
                                "target_lang_sense_clarification": "красный нос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d097f3f4-3971-3d26-80b9-566236a39fb4",
                    "sense_definition": "sekcja roślin z rodzaju psianka, obejmująca gatunki takie jak pomidor zwyczajny",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Studenci biologii uczyli się o klasyfikacji botanicznej sekcji <w>pomidor</w>.",
                            "target_lang_translations": {
                                "ru": "Студенты биологии изучали ботаническую классификацию секции <w>томат</w>."
                            }
                        },
                        {
                            "text": "Ten rodzaj obejmuje różne dzikie gatunki zaliczane do grupy <w>pomidor</w>.",
                            "target_lang_translations": {
                                "ru": "Этот род включает различные дикие виды, относящиеся к группе <w>томат</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "секция растений рода паслён, включающая такие виды, как томат обыкновенный"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "томат",
                                "target_lang_sense_clarification": "ботаническая секция"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
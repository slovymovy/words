    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa010688-7cb4-3733-bb8a-8e920601fa1a",
                    "sense_definition": "Proces przyjmowania zgłoszeń od osób, które chcą zostać uczestnikami jakiegoś wydarzenia, zajęć lub pacjentami w przychodni.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "registration_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym tygodniu ruszają <zapisy> na darmowy kurs języka angielskiego.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе начинается <запись> на бесплатный курс английского языка."
                            }
                        },
                        {
                            "text": "Musisz się pospieszyć, bo <zapisy> do lekarza specjalisty kończą się bardzo szybko.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно поторопиться, так как <запись> к врачу-специалисту заканчивается очень быстро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zapisy na zajęcia",
                        "zapisy do lekarza",
                        "trwają zapisy",
                        "internetowe zapisy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "często w liczbie mnogiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс приема заявок от людей, желающих стать участниками какого-либо события, занятий или пациентами в поликлинике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запись",
                                "target_lang_sense_clarification": "регистрация"
                            },
                            {
                                "target_lang_word": "регистрация",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "212028df-eddb-3e48-8257-26de6ed0459c",
                    "sense_definition": "Utrwalona w jakiejś formie informacja, dźwięk lub obraz, możliwa do późniejszego odtworzenia lub odczytania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "record_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja zabezpieczyła <zapis> z kamer przemysłowych jako główny dowód w sprawie.",
                            "target_lang_translations": {
                                "ru": "Полиция изъяла <запись> с камер наблюдения в качестве главного доказательства по делу."
                            }
                        },
                        {
                            "text": "Jakość tego <zapisu> dźwiękowego jest zbyt słaba, by zrozumieć słowa.",
                            "target_lang_translations": {
                                "ru": "Качество этой звуковой <записи> слишком низкое, чтобы разобрать слова."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zapis wideo",
                        "zapis dźwięku",
                        "zapis cyfrowy",
                        "zapis z monitoringu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зафиксированная в какой-либо форме информация, звук или изображение, доступные для последующего воспроизведения или прочтения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запись",
                                "target_lang_sense_clarification": "аудио или видео"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "deb87529-4393-3a38-8009-df2c70b3907a",
                    "sense_definition": "Konkretny punkt, warunek lub postanowienie zawarte w dokumencie prawnym, umowie lub regulaminie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_clause_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed podpisaniem tej umowy musisz dokładnie przeczytać każdy jej <zapis>.",
                            "target_lang_translations": {
                                "ru": "Перед подписанием этого договора ты должен внимательно прочитать каждый его <пункт>."
                            }
                        },
                        {
                            "text": "Nowy <zapis> w regulaminie szkoły zabrania używania telefonów podczas lekcji.",
                            "target_lang_translations": {
                                "ru": "Новый <пункт> в школьном уставе запрещает использование телефонов во время уроков."
                            }
                        }
                    ],
                    "synonyms": [
                        "punkt umowy",
                        "artykuł ustawy",
                        "paragraf"
                    ],
                    "common_phrases": [
                        "zapis w umowie",
                        "zapis ustawowy",
                        "niekorzystny zapis",
                        "martwy zapis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный пункт, условие или положение, содержащееся в юридическом документе, договоре или уставе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пункт",
                                "target_lang_sense_clarification": "договора или правила"
                            },
                            {
                                "target_lang_word": "положение",
                                "target_lang_sense_clarification": "условие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12265fa0-e9a9-3aac-bdea-22b2e5499506",
                    "sense_definition": "System znaków lub symboli służący do przedstawiania określonych treści, na przykład muzyki lub działań matematycznych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "notation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dla początkującego muzyka <zapis> nutowy może wydawać się bardzo skomplikowany.",
                            "target_lang_translations": {
                                "ru": "Для начинающего музыканта нотная <запись> может показаться очень сложной."
                            }
                        },
                        {
                            "text": "W szkole podstawowej dzieci uczą się zamieniać <zapis> rzymski liczb na arabski.",
                            "target_lang_translations": {
                                "ru": "В начальной школе дети учатся переводить римскую <запись> чисел в арабскую."
                            }
                        }
                    ],
                    "synonyms": [
                        "notacja"
                    ],
                    "common_phrases": [
                        "zapis nutowy",
                        "zapis matematyczny",
                        "zapis dziesiętny",
                        "jednolity zapis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система знаков или символов, используемая для представления определенного содержания, например, музыки или математических операций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запись",
                                "target_lang_sense_clarification": "нотация"
                            },
                            {
                                "target_lang_word": "нотация",
                                "target_lang_sense_clarification": "система знаков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "644db9e1-ec6e-3155-a29e-e90ff2176f13",
                    "sense_definition": "Rozporządzenie w testamencie, w którym spadkodawca przekazuje konkretnej osobie określony składnik majątku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legacy_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziadek zrobił w swoim testamencie specjalny <zapis> na rzecz najmłodszego wnuka.",
                            "target_lang_translations": {
                                "ru": "Дедушка сделал в своем завещании особое <распоряжение> в пользу младшего внука."
                            }
                        },
                        {
                            "text": "Adwokat wyjaśnił rodzinie różnicę między powołaniem do spadku a <zapisem>.",
                            "target_lang_translations": {
                                "ru": "Адвокат объяснил семье разницу между призванием к наследованию и <завещательным отказом>."
                            }
                        }
                    ],
                    "synonyms": [
                        "punkt testamentu"
                    ],
                    "common_phrases": [

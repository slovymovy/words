    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "30a9f8c7-e7f8-327e-a7db-c143508d412f",
                    "sense_definition": "The chemical element with atomic number 78 and symbol Pt; a dense, malleable, ductile, highly unreactive, silverish-white transition metal of great value.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "element",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wedding ring was made of pure <w>platinum</w> and cost a fortune.",
                            "target_lang_translations": {
                                "ru": "Обручальное кольцо было сделано из чистой <w>платины</w> и стоило целое состояние."
                            }
                        },
                        {
                            "text": "Because of its resistance to corrosion, <w>platinum</w> is used in many industrial applications.",
                            "target_lang_translations": {
                                "ru": "Благодаря устойчивости к коррозии <w>платина</w> используется во многих промышленных сферах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pure platinum",
                        "platinum group"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химический элемент с атомным номером 78; плотный, ковкий, химически инертный драгоценный металл серебристо-белого цвета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платина",
                                "target_lang_sense_clarification": "химический элемент / металл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b110667-96b3-365c-aa61-8d778a00f005",
                    "sense_definition": "A musical recording that has achieved a specific high volume of sales, typically over one million copies.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "music_award",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band has several <w>platinums</w> on their wall from their early career.",
                            "target_lang_translations": {
                                "ru": "На стене у группы висит несколько <w>платиновых дисков</w> времён начала их карьеры."
                            }
                        },
                        {
                            "text": "Achieving a <w>platinum</w> is much harder in the age of digital streaming.",
                            "target_lang_translations": {
                                "ru": "Получить <w>платину</w> в эпоху цифрового стриминга гораздо сложнее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "double platinum",
                        "multi-platinum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальная запись, достигшая высокого объема продаж (обычно более миллиона копий)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платиновый диск",
                                "target_lang_sense_clarification": "награда"
                            },
                            {
                                "target_lang_word": "платина",
                                "target_lang_sense_clarification": "разг. о статусе альбома"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcefbecf-58c0-363c-ac33-878b02db3608",
                    "sense_definition": "A whitish grey colour, like that of the metal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car was painted in a stunning shade of <w>platinum</w>.",
                            "target_lang_translations": {
                                "ru": "Машина была покрашена в потрясающий <w>платиновый цвет</w>."
                            }
                        },
                        {
                            "text": "She chose a cool <w>platinum</w> for the walls of her modern office.",
                            "target_lang_translations": {
                                "ru": "Она выбрала холодный <w>платиновый цвет</w> для стен своего современного офиса."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Беловато-серый цвет, напоминающий цвет металла платины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платиновый цвет",
                                "target_lang_sense_clarification": "оттенок"
                            },
                            {
                                "target_lang_word": "цвет платины",
                                "target_lang_sense_clarification": "цвет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee5587a1-33f1-3cdc-8d44-d496a3effc37",
                    "sense_definition": "A platinum-based drug used in chemotherapy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor prescribed a <w>platinum</w> to treat the aggressive tumor.",
                            "target_lang_translations": {
                                "ru": "Врач назначил <w>препарат платины</w> для лечения агрессивной опухоли."
                            }
                        },
                        {
                            "text": "Patients often respond well to the new generation of <w>platinums</w>.",
                            "target_lang_translations": {
                                "ru": "Пациенты часто хорошо реагируют на новое поколение <w>препаратов платины</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лекарственный препарат на основе платины, используемый при химиотерапии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "препарат платины",
                                "target_lang_sense_clarification": "медицина"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "119b0ce7-2fa0-3703-8acd-dfb83ebdbd78",
                    "sense_definition": "Very expensive, or of very high quality and status.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to upgrade to the <w>platinum</w> package for our vacation.",
                            "target_lang_translations": {
                                "ru": "Мы решили улучшить наш турпакет до <w>платинового</w> уровня."
                            }
                        },
                        {
                            "text": "As a <w>platinum</w> member, you are entitled to free lounge access.",
                            "target_lang_translations": {
                                "ru": "Как <w>платиновый</w> клиент, вы имеете право на бесплатный доступ в лаунж-зону."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "81914bab-5d91-3754-8028-6175dccb0a79",
                    "sense_definition": "Тот, кто публично произносит речь или исполняет артистический номер перед аудиторией.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_performer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Следующий <w>выступающий</w> подошёл к микрофону и начал свою речь.",
                            "target_lang_translations": {
                                "en": "The next <w>speaker</w> approached the microphone and began his speech."
                            }
                        },
                        {
                            "text": "Организаторы конференции попросили всех соблюдать регламент, чтобы каждый <w>выступающий</w> успел сделать доклад.",
                            "target_lang_translations": {
                                "en": "The conference organizers asked everyone to stick to the schedule so that every <w>speaker</w> would have time to give their report."
                            }
                        }
                    ],
                    "synonyms": [
                        "оратор",
                        "спикер",
                        "артист"
                    ],
                    "antonyms": [
                        "слушатель",
                        "зритель"
                    ],
                    "common_phrases": [
                        "следующий выступающий",
                        "список выступающих",
                        "вопрос выступающему",
                        "первый выступающий"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who gives a speech or performs in front of an audience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speaker",
                                "target_lang_sense_clarification": "person delivering a speech"
                            },
                            {
                                "target_lang_word": "performer",
                                "target_lang_sense_clarification": "person entertaining an audience"
                            },
                            {
                                "target_lang_word": "presenter",
                                "target_lang_sense_clarification": "person giving a presentation or report"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "062950e9-2df0-3515-ad6e-86e17beac10c",
                    "sense_definition": "Действительное причастие настоящего времени от глагола <w>выступать</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент, <w>выступающий</w> с презентацией, заметно волновался перед аудиторией.",
                            "target_lang_translations": {
                                "en": "The student <w>giving</w> the presentation was noticeably nervous in front of the audience."
                            }
                        },
                        {
                            "text": "Артист, <w>выступающий</w> на сцене сегодня вечером, очень популярен в нашей стране.",
                            "target_lang_translations": {
                                "en": "The artist <w>performing</w> on stage tonight is very popular in our country."
                            }
                        }
                    ],
                    "common_phrases": [

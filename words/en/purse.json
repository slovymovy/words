                    "synonyms": [
                        "pocketbook",
                        "coin purse",
                        "change purse"
                    ],
                    "common_phrases": [
                        "coin purse",
                        "change purse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой мешочек или футляр, используемый специально для ношения денег, особенно монет."
                    },
                    "translations": {
                        "ru": [

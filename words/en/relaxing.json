                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ослабление",
                                "target_lang_sense_clarification": "смягчение правил или уменьшение натяжения"
                            },
                            {
                                "target_lang_word": "расслабление",
                                "target_lang_sense_clarification": "снятие мышечного тонуса или напряжения"
                            }

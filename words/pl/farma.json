            "senses": [
                {
                    "sense_id": "97ec0ff9-37ed-304d-b219-3f8e6f577ad6",
                    "sense_definition": "Duże gospodarstwo rolne, typowe dla krajów anglosaskich lub Europy Zachodniej.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "John pracuje na <w>farmie</w> swojego ojca.",
                            "target_lang_translations": {
                                "ru": "Джон работает на <w>ферме</w> своего отца."
                            }
                        },
                        {
                            "text": "Marzę o tym, by kupić małą <w>farmę</w> i hodować konie.",
                            "target_lang_translations": {
                                "ru": "Я мечтаю купить небольшую <w>ферму</w> и разводить лошадей."
                            }
                        }
                    ],
                    "synonyms": [
                        "gospodarstwo",
                        "ranczo"
                    ],
                    "common_phrases": [
                        "praca na farmie",
                        "prowadzić farmę",
                        "życie na farmie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное сельскохозяйственное хозяйство, типичное для англосаксонских стран или Западной Европы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ферма",
                                "target_lang_sense_clarification": "частное хозяйство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb771f44-0440-3882-a803-734935a739b3",
                    "sense_definition": "Specjalistyczny zakład rolny zajmujący się hodowlą zwierząt; to samo co <w>ferma</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszej okolicy powstała nowoczesna <w>farma</w> drobiu.",
                            "target_lang_translations": {
                                "ru": "В нашей округе появилась современная птицеводческая <w>ферма</w>."
                            }
                        },
                        {
                            "text": "Ekolodzy sprawdzają warunki panujące na tej <w>farmie</w> lisów.",
                            "target_lang_translations": {
                                "ru": "Экологи проверяют условия на этой лисьей <w>ферме</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ferma"
                    ],
                    "common_phrases": [
                        "farma drobiu",
                        "farma zwierząt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специализированное сельскохозяйственное предприятие, занимающееся разведением животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ферма",
                                "target_lang_sense_clarification": "животноводческая"

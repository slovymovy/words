    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "ae7c98c9-0610-37b6-91a7-3f69a13a97f0",
                    "sense_definition": "Stolica i największe miasto stanu Massachusetts w Stanach Zjednoczonych, ważny ośrodek akademicki i kulturalny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mój starszy brat studiuje medycynę na uniwersytecie w <w>Bostonie</w>.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат изучает медицину в университете в <w>Бостоне</w>."
                            }
                        },
                        {
                            "text": "W przyszłym roku planujemy rodzinną wycieczkę do <w>Bostonu</w>.",
                            "target_lang_translations": {
                                "ru": "В следующем году мы планируем семейную поездку в <w>Бостон</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "centrum Bostonu",
                        "przedmieścia Bostonu",
                        "maraton w Bostonie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Столица и крупнейший город штата Массачусетс в США, важный академический и культурный центр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бостон",
                                "target_lang_sense_clarification": "город в США"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "868105c4-e641-3ea9-879f-b1df5fb3ec7f",
                    "sense_definition": "Nazwa kilku innych miast i miejscowości w różnych krajach, na przykład w Wielkiej Brytanii.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Angielski <w>Boston</w> jest miastem portowym w hrabstwie Lincolnshire.",
                            "target_lang_translations": {
                                "ru": "Английский <w>Бостон</w> — портовый город в графстве Линкольншир."
                            }
                        },
                        {
                            "text": "Niewiele osób wie, że w Europie też istnieje miasto o nazwie <w>Boston</w>.",
                            "target_lang_translations": {
                                "ru": "Немногие знают, что в Европе тоже есть город под названием <w>Бостон</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название нескольких других городов и населенных пунктов в разных странах, например, в Великобритании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бостон",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "770ca28f-cc58-3f24-b84a-035b20b79e8b",
                    "sense_definition": "Pies rasy terier bostoński; niewielki pies o krótkiej sierści i charakterystycznym umaszczeniu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiadka kupiła sobie małego <w>bostona</w>, który jest bardzo energiczny.",
                            "target_lang_translations": {
                                "ru": "Соседка купила себе маленького <w>бостон-терьера</w>, который очень энергичен."
                            }
                        },
                        {
                            "text": "W parku bawiły się dwa wesołe <w>bostony</w> pod opieką właścicieli.",
                            "target_lang_translations": {
                                "ru": "В парке играли два веселых <w>бостон-терьера</w> под присмотром владельцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "terier bostoński",
                        "boston terier"
                    ],
                    "common_phrases": [
                        "szczeniak bostona",
                        "hodowla bostonów"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "skrócona nazwa rasy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Собака породы бостон-терьер; небольшая собака с короткой шерстью и характерным окрасом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бостон-терьер",
                                "target_lang_sense_clarification": "порода собаки"
                            },
                            {
                                "target_lang_word": "бостон",
                                "target_lang_sense_clarification": "разговорное название породы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10877d1b-b566-3ad6-8743-b938c85d347f",
                    "sense_definition": "Taniec towarzyski pochodzący ze Stanów Zjednoczonych, będący odmianą powolnego walca; także muzyka do tego tańca.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na balu maturalnym para zatańczyła eleganckiego <w>bostona</w>.",
                            "target_lang_translations": {
                                "ru": "На выпускном балу пара станцевала элегантный <w>бостон</w>."
                            }
                        },
                        {
                            "text": "Nasze babki chętnie tańczyły <w>bostona</w> na początku dwudziestego wieku.",
                            "target_lang_translations": {
                                "ru": "Наши бабушки охотно танцевали <w>бостон</w> в начале двадцатого века."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tańczyć bostona",
                        "kroki bostona"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бальный танец родом из США, разновидность медленного вальса; также музыка к этому танцу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бостон",
                                "target_lang_sense_clarification": "танец"
                            },
                            {
                                "target_lang_word": "вальс-бостон",
                                "target_lang_sense_clarification": "разновидность вальса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f867d8a-8c1a-3134-8327-699e545cb261",
                    "sense_definition": "Gra karciana zbliżona do wista, popularna w XVIII i XIX wieku.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczorami dżentelmeni siadali przy stole, aby grać w <w>bostona</w>.",
                            "target_lang_translations": {
                                "ru": "По вечерам джентльмены садились за стол, чтобы играть в <w>бостон</w>."
                            }
                        },
                        {
                            "text": "Karciana gra w <w>bostona</w> została uwieczniona w wielu powieściach z epoki.",
                            "target_lang_translations": {
                                "ru": "Карточная игра в <w>бостон</w> была увековечена во многих романах той эпохи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grać w bostona",
                        "partia bostona"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "gra historyczna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Карточная игра, близкая к висту, популярная в XVIII и XIX веках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бостон",
                                "target_lang_sense_clarification": "карточная игра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49fb2795-ab63-3f50-9bd2-f3b21768f301",
                    "sense_definition": "Tkanina z wełny czesankowej o skośnym splocie, używana dawniej do szycia ubrań.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krawiec uszył ten solidny płaszcz z grubego, ciemnego <w>bostona</w>.",
                            "target_lang_translations": {
                                "ru": "Портной сшил это добротное пальто из толстого темного <w>бостона</w>."
                            }
                        },
                        {
                            "text": "W szafie wisiała stara marynarka wykonana z szarego <w>bostona</w>.",
                            "target_lang_translations": {
                                "ru": "В шкафу висел старый пиджак, сшитый из серого <w>бостона</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "płaszcz z bostona",
                        "wełniany boston"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "rzadko używane współcześnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ткань из гребенной шерсти саржевого переплетения, использовавшаяся ранее для пошива одежды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бостон",
                                "target_lang_sense_clarification": "шерстяная ткань"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            },
                            {
                                "target_lang_word": "рыжик",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "366994ef-57ad-3db0-9b97-cba18ed8e910",

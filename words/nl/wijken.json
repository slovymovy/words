    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ca26d4c6-fe38-3f53-96d2-21b06515be12",
                    "sense_definition": "Plaatsmaken of opzijgaan voor iemand of iets dat sterker of belangrijker is; zich terugtrekken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "yield_retreat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De demonstranten weigerden te <w>wijken</w> voor de politie.",
                            "target_lang_translations": {
                                "ru": "Демонстранты отказались <w>отступать</w> перед полицией.",
                                "en": "The demonstrators refused to <w>yield</w> to the police."
                            }
                        },
                        {
                            "text": "Hij moest uiteindelijk <w>wijken</w> voor een jongere opvolger binnen het bedrijf.",
                            "target_lang_translations": {
                                "ru": "В конце концов ему пришлось <w>уступить место</w> более молодому преемнику в компании.",
                                "en": "He eventually had to <w>give way</w> to a younger successor within the company."
                            }
                        }
                    ],
                    "synonyms": [
                        "plaatsmaken",
                        "terugtrekken",
                        "deinzen",
                        "opzijgaan"
                    ],
                    "antonyms": [
                        "standhouden",
                        "blijven"
                    ],
                    "common_phrases": [
                        "wijken voor",
                        "niet wijken",
                        "geen duimbreed wijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уступать место или отходить в сторону перед кем-то или чем-то более сильным или важным; отступать.",
                        "en": "To make way or step aside for someone or something stronger or more important; to retreat."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уступать",
                                "target_lang_sense_clarification": "сдавать позиции, давать дорогу"
                            },
                            {
                                "target_lang_word": "отступать",
                                "target_lang_sense_clarification": "отходить назад, ретироваться"
                            },
                            {
                                "target_lang_word": "сторониться",
                                "target_lang_sense_clarification": "отходить в сторону"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "yield",
                                "target_lang_sense_clarification": "to surrender or make room"
                            },
                            {
                                "target_lang_word": "give way",
                                "target_lang_sense_clarification": "to step aside or be replaced"
                            },
                            {
                                "target_lang_word": "retreat",
                                "target_lang_sense_clarification": "to withdraw"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a269d28-fb95-3fe7-8f00-dca5ffd57475",
                    "sense_definition": "Langzaam verdwijnen of minder worden in hevigheid (vaak gezegd van gevaar, pijn of gevoelens).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "subside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gelukkig is de hoge koorts na twee dagen eindelijk <w>geweken</w>.",
                            "target_lang_translations": {
                                "ru": "К счастью, высокая температура спустя два дня наконец <w>спала</w>.",
                                "en": "Fortunately, the high fever finally <w>subsided</w> after two days."
                            }
                        },
                        {
                            "text": "Toen het gevaar was <w>geweken</w>, durfden de bewoners weer naar buiten te komen.",
                            "target_lang_translations": {
                                "ru": "Когда опасность <w>отступила</w>, жители снова осмелились выйти на улицу.",
                                "en": "When the danger had <w>passed</w>, the residents dared to come out again."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdwijnen",
                        "afnemen",
                        "wegebben",
                        "luwen"
                    ],
                    "antonyms": [
                        "toenemen",
                        "verergeren",
                        "aanhouden"
                    ],
                    "common_phrases": [
                        "het gevaar is geweken",
                        "de spanning week"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медленно исчезать или ослабевать (часто говорится об опасности, боли или чувствах).",
                        "en": "To slowly disappear or decrease in intensity (often said of danger, pain, or feelings)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отступать",
                                "target_lang_sense_clarification": "об опасности или болезни"
                            },
                            {
                                "target_lang_word": "спадать",
                                "target_lang_sense_clarification": "о температуре или напряжении"
                            },
                            {
                                "target_lang_word": "проходить",
                                "target_lang_sense_clarification": "исчезать"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "subside",
                                "target_lang_sense_clarification": "regarding danger, pain, or emotions"
                            },
                            {
                                "target_lang_word": "recede",
                                "target_lang_sense_clarification": "to move back or diminish"
                            },
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "to go away or end"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c0eca08b-a99e-3533-ad6e-dafda8f66180",
                    "sense_definition": "Meervoud van <w>wijk</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De burgemeester bezocht verschillende <w>wijken</w> om met de bewoners te praten.",
                            "target_lang_translations": {
                                "ru": "Мэр посетил различные <w>районы</w>, чтобы поговорить с жителями.",
                                "en": "The mayor visited various <w>neighborhoods</w> to talk to the residents."
                            }
                        },
                        {
                            "text": "In de nieuwe <w>wijken</w> is er veel ruimte voor groen en speelplaatsen.",
                            "target_lang_translations": {
                                "ru": "В новых <w>кварталах</w> много места для зелени и игровых площадок.",
                                "en": "In the new <w>districts</w> there is a lot of room for greenery and playgrounds."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число слова <w>wijk</w> (район, квартал).",
                        "en": "Plural of <w>wijk</w> (neighborhood, district)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "районы",
                                "target_lang_sense_clarification": "части города"
                            },
                            {
                                "target_lang_word": "кварталы",
                                "target_lang_sense_clarification": "жилые массивы"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "neighborhoods",
                                "target_lang_sense_clarification": "residential areas"
                            },
                            {
                                "target_lang_word": "districts",
                                "target_lang_sense_clarification": "parts of a city"
                            }
                        ]
                    }
                }
            ]
        }
    ]

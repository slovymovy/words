    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fe46ae99-c71a-342a-8f8a-ab1c78d28e84",
                    "sense_definition": "Niet spontaan, maar gemaakt of onnatuurlijk.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zette een <w>geforceerde</w> glimlach op om zijn teleurstelling te verbergen.",
                            "target_lang_translations": {
                                "en": "He put on a <w>forced</w> smile to hide his disappointment."
                            }
                        },
                        {
                            "text": "De sfeer tijdens het diner voelde heel ongemakkelijk en <w>geforceerd</w> aan.",
                            "target_lang_translations": {
                                "en": "The atmosphere during dinner felt very uncomfortable and <w>strained</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gemaakt",
                        "onnatuurlijk",
                        "gekunsteld"
                    ],
                    "antonyms": [
                        "ongeforceerd"
                    ],
                    "common_phrases": [
                        "een geforceerde lach",
                        "geforceerd doen",
                        "geforceerd overkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "Not spontaneous, but artificial or unnatural."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forced",
                                "target_lang_sense_clarification": "unnatural behavior or expression"
                            },
                            {
                                "target_lang_word": "strained",
                                "target_lang_sense_clarification": "lacking ease or spontaneity"
                            },
                            {
                                "target_lang_word": "contrived",
                                "target_lang_sense_clarification": "obviously planned or artificial"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2760042-ab71-3a59-ae04-7d4d9be9c735",
                    "sense_definition": "Met geweld of veel moeite tot stand gebracht of geopend.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie vond sporen van een <w>geforceerde</w> deur aan de achterkant van het huis.",
                            "target_lang_translations": {
                                "en": "The police found traces of a <w>forced</w> door at the back of the house."
                            }
                        },
                        {
                            "text": "Het was duidelijk een <w>geforceerde</w> beslissing waar niemand echt blij mee was.",
                            "target_lang_translations": {
                                "en": "It was clearly a <w>forced</w> decision that no one was really happy with."
                            }
                        }
                    ],
                    "synonyms": [
                        "gedwongen"
                    ],
                    "common_phrases": [
                        "een geforceerde deur",
                        "een geforceerde toegang",
                        "een geforceerd slot"
                    ],
                    "target_lang_definitions": {
                        "en": "Brought about or opened with violence or great effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forced",
                                "target_lang_sense_clarification": "opened or produced by physical force"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93fc8634-bbad-334b-a8ea-bf1ad8148012",
                    "sense_definition": "Van metaal dat in een bepaalde vorm is gedrukt of gedraaid door middel van forceren.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de vitrine stond een prachtige <w>geforceerde</w> zilveren vaas uit de achttiende eeuw.",
                            "target_lang_translations": {
                                "en": "In the display case stood a beautiful <w>spun</w> silver vase from the eighteenth century."
                            }
                        },
                        {
                            "text": "Voor dit onderdeel wordt vaak <w>geforceerde</w> koperplaat gebruikt vanwege de specifieke vorm.",
                            "target_lang_translations": {
                                "en": "<w>Spun</w> copper sheet is often used for this part because of the specific shape."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Made of metal pressed or turned into a specific shape by spinning."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spun",
                                "target_lang_sense_clarification": "metalworking technique"
                            }

{
    "word_family": [
        "prędki",
        "prędko"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "965715fe-ab77-3874-8eb5-4ecfbd444034",
                    "sense_definition": "Tempo, w jakim zachodzi jakieś zjawisko, lub szybkość, z jaką porusza się obiekt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca znacznie przekroczył dopuszczalną <w>prędkość</w> w terenie zabudowanym.",
                            "target_lang_translations": {
                                "ru": "Водитель значительно превысил допустимую <w>скорость</w> в населенном пункте."
                            }
                        },
                        {
                            "text": "Samochód poruszał się z dużą <w>prędkością</w> po autostradzie.",
                            "target_lang_translations": {
                                "ru": "Автомобиль двигался с большой <w>скоростью</w> по автостраде."
                            }
                        }
                    ],
                    "synonyms": [
                        "szybkość",
                        "tempo"
                    ],
                    "common_phrases": [
                        "ograniczenie prędkości",
                        "średnia prędkość",
                        "dopuszczalna prędkość",
                        "nadmierna prędkość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Темп, в котором происходит какое-либо явление, или быстрота, с которой движется объект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скорость",
                                "target_lang_sense_clarification": "движения или действия"
                            },
                            {
                                "target_lang_word": "быстрота",
                                "target_lang_sense_clarification": "темп"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52cfa381-f7be-30b4-abfc-40dcf6d178a7",
                    "sense_definition": "Wielkość fizyczna opisująca zmianę położenia ciała w czasie, posiadająca wartość i kierunek.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Prędkość</w> światła jest najwyższą prędkością przenoszenia informacji we wszechświecie.",
                            "target_lang_translations": {
                                "ru": "<w>Скорость</w> света является самой высокой скоростью передачи информации во Вселенной."
                            }
                        },
                        {
                            "text": "Na lekcji fizyki obliczaliśmy <w>prędkość</w> początkową wystrzelonego pocisku.",
                            "target_lang_translations": {
                                "ru": "На уроке физики мы вычисляли начальную <w>скорость</w> выпущенного снаряда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prędkość światła",
                        "prędkość dźwięku",
                        "prędkość początkowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая величина, описывающая изменение положения тела во времени, имеющая значение и направление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скорость",
                                "target_lang_sense_clarification": "физическая величина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af895398-3f97-32c6-a4ea-5fbffc0f29d3",
                    "sense_definition": "Nadmierny pośpiech lub gwałtowność w działaniu, często wiążąca się z brakiem rozwagi.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Decyzja została podjęta z taką <w>prędkością</w>, że nikt nie zdążył jej przemyśleć.",
                            "target_lang_translations": {
                                "ru": "Решение было принято с такой <w>скоростью</w>, что никто не успел его обдумать."
                            }
                        },
                        {
                            "text": "Jego <w>prędkość</w> w wyciąganiu wniosków często prowadzi do błędnych ocen.",
                            "target_lang_translations": {
                                "ru": "Его <w>поспешность</w> в выводах часто приводит к ошибочным оценкам."
                            }
                        }
                    ],
                    "synonyms": [
                        "pośpiech",
                        "pochopność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезмерная спешка или стремительность в действиях, часто связанная с недостатком рассудительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поспешность",
                                "target_lang_sense_clarification": "торопливость"
                            },
                            {
                                "target_lang_word": "торопливость",
                                "target_lang_sense_clarification": "суета"
                            },
                            {
                                "target_lang_word": "стремительность",
                                "target_lang_sense_clarification": "резкость"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
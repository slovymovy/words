{
    "word_family": [
        "afsprong",
        "springen",
        "sprong"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cae996dd-e8e3-3f7b-a38b-266d0a0ed274",
                    "sense_definition": "Van een hoger gelegen plaats naar beneden springen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dief wilde van het lage dak <w>afspringen</w> om te ontsnappen.",
                            "target_lang_translations": {
                                "en": "The thief wanted to <w>jump off</w> the low roof to escape.",
                                "ru": "Вор хотел <w>спрыгнуть</w> с низкой крыши, чтобы сбежать."
                            }
                        },
                        {
                            "text": "Je moet niet van een rijdende trein <w>afspringen</w>.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>jump off</w> a moving train.",
                                "ru": "Не стоит <w>спрыгивать</w> с движущегося поезда."
                            }
                        }
                    ],
                    "synonyms": [
                        "springen"
                    ],
                    "antonyms": [
                        "opspringen"
                    ],
                    "common_phrases": [
                        "van de fiets afspringen",
                        "van de wagen afspringen"
                    ],
                    "target_lang_definitions": {
                        "en": "To jump down from a higher place.",
                        "ru": "Спрыгивать вниз с более высокого места."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jump off",
                                "target_lang_sense_clarification": "physical movement from a height"
                            },
                            {
                                "target_lang_word": "jump down",
                                "target_lang_sense_clarification": "movement to a lower level"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спрыгивать",
                                "target_lang_sense_clarification": "движение вниз с высоты"
                            },
                            {
                                "target_lang_word": "соскакивать",
                                "target_lang_sense_clarification": "быстро слезать (с транспорта, лошади)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6d88f68-e6ac-3e9d-b29d-5c4962a939d3",
                    "sense_definition": "Niet doorgaan of mislukken van een voorgenomen plan, samenwerking of overleg.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De onderhandelingen over de nieuwe coalitie zijn helaas <w>afgesprongen</w>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, the negotiations about the new coalition have <w>broken down</w>.",
                                "ru": "К сожалению, переговоры о новой коалиции <w>сорвались</w>."
                            }
                        },
                        {
                            "text": "Door een gebrek aan geld dreigt het project <w>af te springen</w>.",
                            "target_lang_translations": {
                                "en": "Due to a lack of money, the project threatens to <w>fall through</w>.",
                                "ru": "Из-за нехватки денег проект может <w>сорваться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mislukken",
                        "stuklopen",
                        "afketsen"
                    ],
                    "common_phrases": [
                        "de deal is afgesprongen",
                        "de onderhandelingen zijn afgesprongen"
                    ],
                    "target_lang_definitions": {
                        "en": "To fail to happen or be completed (of a plan, cooperation, or negotiation).",
                        "ru": "Не состояться или потерпеть неудачу (о планах, сотрудничестве или переговорах)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall through",
                                "target_lang_sense_clarification": "fail to happen (plans/deals)"
                            },
                            {
                                "target_lang_word": "break down",
                                "target_lang_sense_clarification": "fail or cease (negotiations)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "срываться",
                                "target_lang_sense_clarification": "не состояться (о переговорах, сделках)"
                            },
                            {
                                "target_lang_word": "проваливаться",
                                "target_lang_sense_clarification": "терпеть неудачу (о планах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "add08338-8aec-3e54-b71c-c0602ff62d32",
                    "sense_definition": "Plotseling loskomen, breken of wegvliegen van de plaats waar het vastzat.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "detachment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pas op dat de knoop niet van je jas kan <w>afspringen</w>.",
                            "target_lang_translations": {
                                "en": "Be careful that the button doesn't <w>pop off</w> your coat.",
                                "ru": "Следи, чтобы пуговица не <w>отскочила</w> от твоего пальто."
                            }
                        },
                        {
                            "text": "Door de hitte kan het glazuur van de tegels <w>afspringen</w>.",
                            "target_lang_translations": {
                                "en": "Due to the heat, the glaze can <w>chip off</w> the tiles.",
                                "ru": "Из-за жары глазурь на плитке может <w>отколоться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "losspringen",
                        "wegspringen",
                        "afvliegen",
                        "loslaten",
                        "afspatten"
                    ],
                    "common_phrases": [
                        "de verf is afgesprongen"
                    ],
                    "target_lang_definitions": {
                        "en": "To suddenly come loose, break, or fly away from where it was attached.",
                        "ru": "Внезапно отрываться, ломаться или отлетать от места крепления."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pop off",
                                "target_lang_sense_clarification": "detach suddenly (e.g., buttons)"
                            },
                            {
                                "target_lang_word": "chip off",
                                "target_lang_sense_clarification": "break off in small pieces (e.g., paint/enamel)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отскакивать",
                                "target_lang_sense_clarification": "внезапно отделяться (о пуговицах, деталях)"
                            },
                            {
                                "target_lang_word": "откалываться",
                                "target_lang_sense_clarification": "отламываться мелкими кусками (о краске, эмали)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8e72bc3-6df3-3c76-b126-e71166477250",
                    "sense_definition": "Een oppervlak raken en terugkaatsen zonder binnen te dringen (meestal over projectielen).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "ricochet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kogel zal op het kogelvrije glas <w>afspringen</w>.",
                            "target_lang_translations": {
                                "en": "The bullet will <w>ricochet</w> off the bulletproof glass.",
                                "ru": "Пуля <w>срикошетит</w> от пуленепробиваемого стекла."
                            }
                        },
                        {
                            "text": "Het projectiel is op de harde muur <w>afgesprongen</w>.",
                            "target_lang_translations": {
                                "en": "The projectile <w>bounced off</w> the hard wall.",
                                "ru": "Снаряд <w>отскочил</w> от твердой стены."
                            }
                        }
                    ],
                    "synonyms": [
                        "afketsen"
                    ],
                    "target_lang_definitions": {
                        "en": "To hit a surface and bounce back without penetrating (usually regarding projectiles).",
                        "ru": "Ударяться о поверхность и отлетать назад, не пробивая её (обычно о снарядах)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ricochet",
                                "target_lang_sense_clarification": "rebound off a surface"
                            },
                            {
                                "target_lang_word": "bounce off",
                                "target_lang_sense_clarification": "hit and rebound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рикошетить",
                                "target_lang_sense_clarification": "отскакивать при ударе под углом"
                            },
                            {
                                "target_lang_word": "отскакивать",
                                "target_lang_sense_clarification": "ударяться и отлетать назад"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
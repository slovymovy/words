            "pos": "verb",
            "senses": [
                {
                    "sense_id": "20f0e4de-ef60-362a-ab83-381a3f45919f",
                    "sense_definition": "Bekend of vertrouwd zijn met een persoon, zaak of begrip door ervaring of ontmoeting",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "knowing_familiarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>ken</w> mijn buren al heel lang.",
                            "target_lang_translations": {
                                "en": "I have <w>known</w> my neighbors for a very long time."
                            }
                        },
                        {
                            "text": "Zij <w>kent</w> de weg in Amsterdam heel goed.",
                            "target_lang_translations": {
                                "en": "She <w>knows</w> her way around Amsterdam very well."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iemand van gezicht kennen",
                        "de weg kennen",
                        "elkaar leren kennen",
                        "iemand van naam kennen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be familiar or acquainted with a person, thing, or concept through experience or meeting"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "know",
                                "target_lang_sense_clarification": "to be familiar with people, places, or things"
                            },
                            {
                                "target_lang_word": "be acquainted with",
                                "target_lang_sense_clarification": "to know someone socially (formal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9997187b-7b3a-3264-a835-ed0af03bf334",
                    "sense_definition": "Door studie of oefening geleerd hebben; kennis hebben van feiten of een taal",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "knowing_skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leerling <w>kent</w> het gedicht helemaal uit het hoofd.",
                            "target_lang_translations": {
                                "en": "The student <w>knows</w> the poem entirely by heart."
                            }
                        },
                        {
                            "text": "Hij <w>kent</w> de regels van het spel niet.",
                            "target_lang_translations": {
                                "en": "He does not <w>know</w> the rules of the game."
                            }
                        }
                    ],
                    "synonyms": [
                        "kunnen"
                    ],
                    "common_phrases": [
                        "uit het hoofd kennen",
                        "zijn lesje kennen",
                        "zijn talen kennen"
                    ],
                    "target_lang_definitions": {
                        "en": "To have learned through study or practice; to have knowledge of facts or a language"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "know",
                                "target_lang_sense_clarification": "to possess knowledge, facts, or skills"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ded13a38-96fe-33bf-8e23-0889f7145671",
                    "sense_definition": "Iets ondervinden, doormaken of ervaren",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "experiencing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het land <w>kende</w> een lange periode van vrede.",
                            "target_lang_translations": {
                                "en": "The country <w>experienced</w> a long period of peace."
                            }
                        },
                        {
                            "text": "Zijn geduld <w>kent</w> geen grenzen.",
                            "target_lang_translations": {
                                "en": "His patience <w>knows</w> no bounds."
                            }
                        }
                    ],
                    "common_phrases": [
                        "geen grenzen kennen",
                        "armoede kennen",
                        "tijden van crisis kennen"
                    ],
                    "target_lang_definitions": {
                        "en": "To undergo, go through, or experience something"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "experience",
                                "target_lang_sense_clarification": "to undergo a situation or feeling"
                            },
                            {
                                "target_lang_word": "know",
                                "target_lang_sense_clarification": "literary: to have experience of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06dca755-f265-30a2-9bff-eb1b27e768ae",
                    "sense_definition": "In staat zijn iets te doen; een dialectische vorm van <w>kunnen</w>",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dialectal_can",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat <w>ken</w> ik echt niet doen.",
                            "target_lang_translations": {
                                "en": "I really <w>can</w>'t do that."
                            }
                        },
                        {
                            "text": "Hij <w>ken</w> heel hard rennen.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7857bc23-dda8-3722-ab52-7b0527000faf",
                    "sense_definition": "One of the elements, circumstances, or influences that contribute to a particular result or situation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "element_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The weather was a major <w>factor</w> in our decision to cancel the picnic.",
                            "target_lang_translations": {
                                "ru": "Погода стала основным <w>фактором</w> в нашем решении отменить пикник."
                            }
                        },
                        {
                            "text": "Cost is often the deciding <w>factor</w> when people choose a new car.",
                            "target_lang_translations": {
                                "ru": "Стоимость часто является решающим <w>фактором</w> при выборе новой машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "element",
                        "component",
                        "part",
                        "influence",
                        "determinant"
                    ],
                    "common_phrases": [
                        "deciding factor",
                        "key factor",
                        "risk factor",
                        "major factor",
                        "contributing factor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одно из обстоятельств, условий или сил, влияющих на результат или ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фактор",
                                "target_lang_sense_clarification": "движущая сила, причина совершения процесса"
                            },
                            {
                                "target_lang_word": "обстоятельство",
                                "target_lang_sense_clarification": "условие, влияющее на что-либо"
                            },
                            {
                                "target_lang_word": "причина",
                                "target_lang_sense_clarification": "основание или повод для чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "320e4e63-1aff-32c1-9d47-8a5c38d6d80f",
                    "sense_definition": "A number that divides another number evenly without leaving a remainder.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "math_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher asked the students to find every <w>factor</w> of the number twelve.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников найти каждый <w>делитель</w> числа двенадцать."
                            }
                        },
                        {
                            "text": "Three is a common <w>factor</w> of both nine and fifteen.",
                            "target_lang_translations": {
                                "ru": "Три — это общий <w>делитель</w> девяти и пятнадцати."
                            }
                        }
                    ],
                    "synonyms": [
                        "divisor"
                    ],
                    "antonyms": [
                        "multiple"
                    ],
                    "common_phrases": [
                        "common factor",
                        "prime factor",
                        "scale factor",
                        "conversion factor"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Число, на которое делится другое число без остатка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делитель",
                                "target_lang_sense_clarification": "число, на которое делят"
                            },
                            {
                                "target_lang_word": "множитель",
                                "target_lang_sense_clarification": "число, участвующее в умножении (при разложении)"
                            },
                            {
                                "target_lang_word": "коэффициент",
                                "target_lang_sense_clarification": "числовой множитель (scale factor)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09e1be7d-de74-3b65-b3f9-1fe8eee6b31b",
                    "sense_definition": "A resource, such as land, labor, or capital, used in the production of goods or services.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "economics_resource",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Labor is a critical <w>factor</w> of production in any manufacturing industry.",
                            "target_lang_translations": {
                                "ru": "Труд — это критически важный <w>фактор</w> производства в любой обрабатывающей промышленности."
                            }
                        },
                        {
                            "text": "The report analyzed how each economic <w>factor</w> influenced the final price of the product.",
                            "target_lang_translations": {
                                "ru": "В отчете анализировалось, как каждый экономический <w>фактор</w> влиял на конечную цену продукта."
                            }
                        }
                    ],
                    "synonyms": [
                        "resource",
                        "input"
                    ],
                    "antonyms": [
                        "output",
                        "product"
                    ],
                    "common_phrases": [
                        "factors of production"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "economics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ресурс (земля, труд, капитал), используемый в производстве товаров или услуг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фактор",
                                "target_lang_sense_clarification": "экономический ресурс (фактор производства)"
                            },
                            {
                                "target_lang_word": "ресурс",
                                "target_lang_sense_clarification": "источник или запас"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bd26ac7-c996-314d-926d-f16668dce9a1",
                    "sense_definition": "An agent, representative, or steward who manages business or property for another person.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "agent_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wealthy merchant sent his <w>factor</w> to negotiate the trade deal abroad.",
                            "target_lang_translations": {
                                "ru": "Богатый купец отправил своего <w>управляющего</w> для переговоров о торговой сделке за границей."
                            }
                        },
                        {
                            "text": "As the estate <w>factor</w>, he was responsible for collecting rents from the tenants.",
                            "target_lang_translations": {
                                "ru": "Как <w>управляющий</w> поместьем, он отвечал за сбор арендной платы с жильцов."
                            }
                        }
                    ],
                    "synonyms": [
                        "agent",
                        "representative",
                        "steward",
                        "broker"
                    ],
                    "antonyms": [
                        "principal"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical or specific business contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Агент, представитель или управляющий, который ведет дела или управляет имуществом другого лица."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управляющий",
                                "target_lang_sense_clarification": "распорядитель имуществом"
                            },
                            {
                                "target_lang_word": "агент",
                                "target_lang_sense_clarification": "представитель интересов"
                            },
                            {
                                "target_lang_word": "посредник",
                                "target_lang_sense_clarification": "лицо, содействующее сделке"
                            },
                            {
                                "target_lang_word": "фактор",
                                "target_lang_sense_clarification": "устаревшее название поверенного или агента"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d2640c0-d9c4-3594-9265-b4f2f9554b81",
                    "sense_definition": "A business organization that lends money on accounts receivable or buys and collects debts.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "finance_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company hired a <w>factor</w> to handle their unpaid invoices and improve cash flow.",
                            "target_lang_translations": {
                                "ru": "Компания наняла <w>финансового агента</w> для работы с неоплаченными счетами и улучшения денежного потока."
                            }
                        },
                        {
                            "text": "A financial <w>factor</w> can provide immediate funds to a business waiting for customer payments.",
                            "target_lang_translations": {
                                "ru": "Финансовый <w>посредник</w> может предоставить немедленные средства бизнесу, ожидающему платежей от клиентов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "finance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Финансовая организация, которая покупает дебиторскую задолженность или выдает деньги под счета к получению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фактор",
                                "target_lang_sense_clarification": "финансовый агент в факторинге"
                            },
                            {
                                "target_lang_word": "факторинговая компания",
                                "target_lang_sense_clarification": "организация, занимающаяся перекупкой долгов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "868c1406-2d8c-30ce-9632-d7195bd9d164",
                    "sense_definition": "To break a number or mathematical expression down into the numbers that can be multiplied together to produce it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "math_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Students must learn how to <w>factor</w> quadratic equations to solve them.",
                            "target_lang_translations": {
                                "ru": "Студенты должны научиться <w>раскладывать на множители</w> квадратные уравнения, чтобы решать их."
                            }
                        },
                        {
                            "text": "If you <w>factor</w> the number ten, you get two and five.",
                            "target_lang_translations": {
                                "ru": "Если <w>разложить на множители</w> число десять, получится два и пять."
                            }
                        }
                    ],
                    "synonyms": [
                        "factorize"
                    ],
                    "antonyms": [
                        "multiply",
                        "expand"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскладывать число или математическое выражение на множители, произведение которых дает исходное значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскладывать на множители",
                                "target_lang_sense_clarification": "представлять в виде произведения"
                            },
                            {
                                "target_lang_word": "факторизовать",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07be9154-344c-3745-a214-4f93d37c91f8",
                    "sense_definition": "To sell a debt or invoice to a third party agent in order to receive immediate cash.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "finance_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small business decided to <w>factor</w> its debts to pay for new inventory.",
                            "target_lang_translations": {
                                "ru": "Малый бизнес решил <w>продать</w> свои долги, чтобы заплатить за новый товар."
                            }
                        },
                        {
                            "text": "They often <w>factor</w> their accounts receivable to maintain steady operations.",
                            "target_lang_translations": {
                                "ru": "Они часто <w>продают</w> свою дебиторскую задолженность, чтобы поддерживать стабильную работу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "finance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Продавать долг или счет-фактуру третьему лицу для получения немедленных денежных средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продавать долги",
                                "target_lang_sense_clarification": "переуступать задолженность фактору"
                            },
                            {
                                "target_lang_word": "использовать факторинг",
                                "target_lang_sense_clarification": "финансовая операция"
                            }
                        ]
                    }
                }
            ]
        },
        {

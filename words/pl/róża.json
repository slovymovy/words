    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "94c5b0f1-e2cc-3ece-80f6-49a8b9811023",
                    "sense_definition": "ozdobny kwiat o pachnących płatkach i łodydze wyposażonej w kolce, symbolizujący często miłość",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "plant_flower",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na pierwszą randkę Marek kupił jej czerwoną <w>różę</w>.",
                            "target_lang_translations": {
                                "ru": "На первое свидание Марек купил ей красную <w>розу</w>."
                            }
                        },
                        {
                            "text": "W wazonie na stole stały świeże, pachnące <w>róże</w>.",
                            "target_lang_translations": {
                                "ru": "В вазе на столе стояли свежие, ароматные <w>розы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "królowa kwiatów"
                    ],
                    "common_phrases": [
                        "czerwona róża",
                        "bukiet róż",
                        "płatki róży",
                        "nie ma róży bez kolców"
                    ],
                    "target_lang_definitions": {
                        "ru": "декоративный цветок с ароматными лепестками и шипами, часто символизирующий любовь"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роза",
                                "target_lang_sense_clarification": "цветок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c77becac-1e82-331f-a0a5-a3a7b691abfd",
                    "sense_definition": "kolczasty krzew z rodziny różowatych, uprawiany w ogrodach dla kwiatów lub rosnący dziko",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "plant_flower",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym ogrodzie rośnie piękna dzika <w>róża</w>.",
                            "target_lang_translations": {
                                "ru": "В нашем саду растёт красивый <w>шиповник</w>."
                            }
                        },
                        {
                            "text": "Musimy przyciąć krzewy <w>róży</w> przed nadejściem zimy.",
                            "target_lang_translations": {
                                "ru": "Мы должны подрезать кусты <w>роз</w> перед наступлением зимы."
                            }
                        }
                    ],
                    "synonyms": [
                        "szypszyna"
                    ],
                    "common_phrases": [
                        "dzika róża",
                        "krzew róży",
                        "olejek z róży",
                        "sadzić róże"
                    ],
                    "target_lang_definitions": {
                        "ru": "колючий кустарник семейства розовых, выращиваемый в садах или растущий в дикой природе"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роза",
                                "target_lang_sense_clarification": "кустарник"
                            },
                            {
                                "target_lang_word": "шиповник",
                                "target_lang_sense_clarification": "дикая роза"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a8e4f35-c21f-3ad4-ae71-03fce62383f9",
                    "sense_definition": "oddzielona, jadalna część kwiatostanu kalafiora lub brokuła",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do zupy jarzynowej wrzuć kilka <w>róż</w> kalafiora.",
                            "target_lang_translations": {
                                "ru": "В овощной суп брось несколько <w>розочек</w> цветной капусты."
                            }
                        },
                        {
                            "text": "Podziel brokuł na mniejsze <w>róże</w> i gotuj przez pięć minut.",
                            "target_lang_translations": {
                                "ru": "Раздели брокколи на более мелкие <w>розочки</w> и вари пять минут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "róża kalafiora",
                        "róża brokuła"
                    ],
                    "target_lang_definitions": {
                        "ru": "отделённая съедобная часть соцветия цветной капусты или брокколи"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "розочка",
                                "target_lang_sense_clarification": "соцветие капусты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ac32417-523e-30a9-94fb-285b9c57170e",
                    "sense_definition": "zakaźna choroba skóry wywoływana przez bakterie, objawiająca się bolesnym zaczerwienieniem i obrzękiem",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "disease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zdiagnozował u pacjenta <w>różę</w> i przepisał antybiotyki.",
                            "target_lang_translations": {
                                "ru": "Врач диагностировал у пациента <w>рожу</w> и выписал антибиотики."
                            }
                        },
                        {
                            "text": "Nieleczona <w>róża</w> może prowadzić do poważnych powikłań zdrowotnych.",
                            "target_lang_translations": {
                                "ru": "Нелеченая <w>рожа</w> может привести к серьёзным осложнениям здоровья."
                            }
                        }
                    ],
                    "common_phrases": [
                        "róża twarzy"
                    ],
                    "target_lang_definitions": {
                        "ru": "инфекционное заболевание кожи, вызываемое бактериями, проявляющееся болезненным покраснением и отёком"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рожа",
                                "target_lang_sense_clarification": "болезнь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bfcf53a-f584-34f7-865c-2174e7d23d18",
                    "sense_definition": "grupa osób w Kościele katolickim wspólnie odmawiająca różaniec w określonym porządku",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia należy do żywej <w>róży</w> przy naszej parafii.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка принадлежит к живой <w>розе</w> при нашем приходе."
                            }
                        },
                        {
                            "text": "Członkowie <w>róży</w> spotykają się raz w miesiącu na wspólnej modlitwie.",
                            "target_lang_translations": {
                                "ru": "Члены <w>розы</w> встречаются раз в месяц для совместной молитвы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "żywa róża",
                        "róża różańcowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "группа людей в Католической церкви, совместно читающая Розарий в определённом порядке"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роза",
                                "target_lang_sense_clarification": "молитвенная группа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a39ac681-7706-39a7-bc97-42f0ff4b3cfe",
                    "sense_definition": "skupienie kryształów minerału, np. gipsu, przypominające kształtem kwiat",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mineral",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przywiozłem z wycieczki na Saharę piękną <w>różę</w> pustyni.",
                            "target_lang_translations": {
                                "ru": "Я привёз с экскурсии в Сахару красивую <w>розу</w> пустыни."
                            }
                        },
                        {
                            "text": "W muzeum geologicznym oglądaliśmy skamieniałe <w>róże</w> gipsowe.",
                            "target_lang_translations": {
                                "ru": "В геологическом музее мы рассматривали окаменелые гипсовые <w>розы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "skupienie rozetowe"
                    ],
                    "common_phrases": [
                        "róża pustyni",
                        "róża gipsowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "срастание кристаллов минерала, например гипса, напоминающее формой цветок"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роза",
                                "target_lang_sense_clarification": "минерал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a789452f-0720-37a6-990f-3007106f63b0",
                    "sense_definition": "pierścieniowate zgrubienie u nasady poroża zwierząt jeleniowatych",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hunting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony myśliwy ocenił wiek jelenia, patrząc na <w>róże</w> jego poroża.",
                            "target_lang_translations": {
                                "ru": "Опытный охотник оценил возраст оленя, глядя на <w>розетки</w> его рогов."
                            }
                        },
                        {
                            "text": "Poroże tego byka miało wyjątkowo szerokie i guzkowate <w>róże</w>.",
                            "target_lang_translations": {
                                "ru": "Рога этого быка имели исключительно широкие и бугристые <w>розетки</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "кольцевидное утолщение у основания рогов оленевых"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "розетка",
                                "target_lang_sense_clarification": "часть рогов"
                            }
                        ]
                    }
                }
            ]
        },
        {

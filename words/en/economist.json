    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d4100054-759f-384d-a547-15381cf10617",
                    "sense_definition": "An expert in economics, especially one who studies economic data and extracts higher-level information or proposes theories.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chief <w>economist</w> predicts that inflation will decrease by the end of the year.",
                            "target_lang_translations": {
                                "ru": "Главный <w>экономист</w> прогнозирует, что инфляция снизится к концу года."
                            }
                        },
                        {
                            "text": "She works as an <w>economist</w> for the government, analyzing labor market trends.",
                            "target_lang_translations": {
                                "ru": "Она работает <w>экономистом</w> в правительстве, анализируя тенденции рынка труда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chief economist",
                        "senior economist",
                        "leading economist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист в области экономики, эксперт, анализирующий экономические данные и разрабатывающий теории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экономи́ст",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57aee30f-3674-3625-ab67-323e7d93c72b",
                    "sense_definition": "One who economizes, or manages domestic or other concerns with frugality; one who expends money, time, or labor, judiciously, and without waste.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother was a prudent <w>economist</w> who could make a small budget last for weeks.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка была бережливой <w>хозяйкой</w>, которая могла растянуть небольшой бюджет на несколько недель."
                            }
                        },
                        {
                            "text": "He is a poor <w>economist</w> of his own energy, often exhausting himself on trivial tasks.",
                            "target_lang_translations": {
                                "ru": "Он плохой <w>эконом</w> своей собственной энергии, часто истощая себя на пустяковых задачах."
                            }
                        }
                    ],
                    "synonyms": [
                        "economizer",
                        "economiser"
                    ],
                    "common_phrases": [
                        "good economist",
                        "poor economist"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to personal frugality rather than the profession"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который бережливо ведет хозяйство или разумно расходует деньги, время и силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рачи́тельный хозя́ин",
                                "target_lang_sense_clarification": "бережливый человек"
                            },
                            {
                                "target_lang_word": "эконо́м",
                                "target_lang_sense_clarification": "устаревшее, книжное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cbd4f2b-ec54-3d05-a617-351fa3361405",
                    "sense_definition": "One who manages a household.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the 18th century, the head of the family was often referred to as the <w>economist</w> of the estate.",
                            "target_lang_translations": {
                                "ru": "В XVIII веке главу семьи часто называли <w>экономом</w> поместья."
                            }
                        },
                        {
                            "text": "The old manual describes the duties of a skilled <w>economist</w> in maintaining the home.",
                            "target_lang_translations": {
                                "ru": "В старом руководстве описываются обязанности опытного <w>домоправителя</w> по ведению домашнего хозяйства."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "original literal meaning"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Управляющий домашним хозяйством или имением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "домоправи́тель",
                                "target_lang_sense_clarification": "управляющий домом"
                            },
                            {
                                "target_lang_word": "эконо́м",
                                "target_lang_sense_clarification": "историческое"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
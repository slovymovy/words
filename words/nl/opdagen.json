                    "sense_id": "c8303341-8753-3a6e-bc05-2f3f07311f2b",
                    "sense_definition": "Op de verwachte tijd en plaats verschijnen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben lang gewacht, maar hij kwam niet <w>opdagen</w>.",
                            "target_lang_translations": {
                                "en": "We waited a long time, but he didn't <w>show up</w>.",
                                "ru": "Мы долго ждали, но он так и не <w>явился</w>."
                            }
                        },
                        {
                            "text": "Hij was ondanks het noodweer toch <w>opgedaagd</w>.",
                            "target_lang_translations": {
                                "en": "Despite the bad weather, he <w>turned up</w> anyway.",
                                "ru": "Несмотря на непогоду, он все же <w>явился</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verschijnen",
                        "arriveren",
                        "aankomen"
                    ],
                    "antonyms": [
                        "wegblijven"
                    ],
                    "common_phrases": [
                        "niet komen opdagen",
                        "toch opdagen",
                        "te laat opdagen"
                    ],
                    "target_lang_definitions": {

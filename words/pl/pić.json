                            {
                                "target_lang_word": "тереть",
                                "target_lang_sense_clarification": "натирать (об обуви)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "511c365f-8631-3cda-a2c3-6052527e5b68",
                    "sense_definition": "Robić aluzje do kogoś lub czegoś, często w sposób złośliwy lub ukryty.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie rozumiem, do czego <pijesz> w tej dyskusji.",
                            "target_lang_translations": {
                                "ru": "Я не понимаю, к чему ты <клонишь> в этой дискуссии."
                            }
                        },
                        {
                            "text": "Czy ty <pijesz> do mojej wczorajszej pomyłki w raporcie?",
                            "target_lang_translations": {
                                "ru": "Ты <намекаешь> на мою вчерашнюю ошибку в отчете?"
                            }
                        }
                    ],
                    "synonyms": [
                        "przymawiać"
                    ],
                    "common_phrases": [
                        "pić do kogoś",
                        "pić do czegoś"
                    ],
                    "traits": [
                        {

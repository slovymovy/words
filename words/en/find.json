{
    "word_family": [
        "finder",
        "finding"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6f9dedb4-b55d-392c-b19f-158571f46bc2",
                    "sense_definition": "To discover or locate something that one has been looking for.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I finally <w>found</w> my car keys under the couch.",
                            "target_lang_translations": {
                                "ru": "Я наконец-то <w>нашёл</w> ключи от машины под диваном."
                            }
                        },
                        {
                            "text": "Can you help me <w>find</w> a good place to eat?",
                            "target_lang_translations": {
                                "ru": "Ты можешь помочь мне <w>найти</w> хорошее место, где можно поесть?"
                            }
                        }
                    ],
                    "antonyms": [
                        "lose"
                    ],
                    "common_phrases": [
                        "find a way",
                        "find out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнаружить или определить местонахождение того, что искали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "найти",
                                "target_lang_sense_clarification": "отыскать в результате поиска"
                            },
                            {
                                "target_lang_word": "находить",
                                "target_lang_sense_clarification": "процесс поиска"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5da17300-761e-3de3-9d27-ab070481d4a1",
                    "sense_definition": "To encounter or discover something by accident or chance.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>found</w> a beautiful shell on the beach this morning.",
                            "target_lang_translations": {
                                "ru": "Сегодня утром я <w>нашёл</w> на пляже красивую ракушку."
                            }
                        },
                        {
                            "text": "She arrived home to <w>find</w> that the door was unlocked.",
                            "target_lang_translations": {
                                "ru": "Она пришла домой и <w>обнаружила</w>, что дверь не заперта."
                            }
                        }
                    ],
                    "antonyms": [
                        "lose"
                    ],
                    "common_phrases": [
                        "find oneself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случайно наткнуться на что-то или обнаружить что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "найти",
                                "target_lang_sense_clarification": "случайно обнаружить"
                            },
                            {
                                "target_lang_word": "обнаружить",
                                "target_lang_sense_clarification": "заметить, выявить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3af809ac-d86a-3e50-b31b-122cb67a2acb",
                    "sense_definition": "To have a particular feeling or opinion about something; to consider.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>find</w> it difficult to wake up early in the winter.",
                            "target_lang_translations": {
                                "ru": "Я <w>нахожу</w>, что зимой просыпаться рано трудно."
                            }
                        },
                        {
                            "text": "Do you <w>find</w> this book interesting?",
                            "target_lang_translations": {
                                "ru": "Ты <w>находишь</w> эту книгу интересной?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "find it hard",
                        "find it easy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь определенное мнение или ощущение по поводу чего-либо; считать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находить",
                                "target_lang_sense_clarification": "считать, полагать"
                            },
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "иметь мнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "905ad7a1-5da1-3971-8adf-7427679a4801",
                    "sense_definition": "To succeed in getting or obtaining something required or desired, such as time or money.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I hope you can <w>find</w> the time to read this report.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, вы сможете <w>найти</w> время прочитать этот отчет."
                            }
                        },
                        {
                            "text": "He needs to <w>find</w> a job before the end of the month.",
                            "target_lang_translations": {
                                "ru": "Ему нужно <w>найти</w> работу до конца месяца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "find time",
                        "find money"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преуспеть в получении чего-либо необходимого или желаемого, например, времени или денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "найти",
                                "target_lang_sense_clarification": "добыть, изыскать"
                            },
                            {
                                "target_lang_word": "изыскать",
                                "target_lang_sense_clarification": "найти средства или время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "573d5082-f26e-38fb-a030-8dc33f3b6502",
                    "sense_definition": "To discover a fact or truth through study, calculation, or investigation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "study",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The researchers <w>found</w> that the two variables were strongly correlated.",
                            "target_lang_translations": {
                                "ru": "Исследователи <w>установили</w>, что эти две переменные сильно коррелируют."
                            }
                        },
                        {
                            "text": "Water is <w>found</w> to be a compound substance.",
                            "target_lang_translations": {
                                "ru": "<w>Установлено</w>, что вода является сложным веществом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнаружить факт или истину посредством изучения, вычисления или расследования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "установить",
                                "target_lang_sense_clarification": "выяснить в ходе исследования"
                            },
                            {
                                "target_lang_word": "обнаружить",
                                "target_lang_sense_clarification": "выявить факт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5aa2cb44-5bde-3697-8e25-0d29612fa702",
                    "sense_definition": "To make a formal legal decision or verdict.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The jury <w>found</w> for the defendant after a long deliberation.",
                            "target_lang_translations": {
                                "ru": "Присяжные <w>вынесли решение</w> в пользу ответчика после долгих совещаний."
                            }
                        },
                        {
                            "text": "The court <w>found</w> him guilty of the charges.",
                            "target_lang_translations": {
                                "ru": "Суд <w>признал</w> его виновным по предъявленным обвинениям."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вынести официальное судебное решение или вердикт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признать",
                                "target_lang_sense_clarification": "вынести вердикт (напр. виновным)"
                            },
                            {
                                "target_lang_word": "вынести решение",
                                "target_lang_sense_clarification": "судебное постановление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0f8913ce-9439-3e73-b8fa-00ec56fc4119",
                    "sense_definition": "A valuable or interesting thing that has been discovered.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The archaeologist was excited about the new <w>find</w> at the site.",
                            "target_lang_translations": {
                                "ru": "Археолог был в восторге от новой <w>находки</w> на раскопках."
                            }
                        },
                        {
                            "text": "This antique vase was a lucky <w>find</w> at the flea market.",
                            "target_lang_translations": {
                                "ru": "Эта антикварная ваза была удачной <w>находкой</w> на блошином рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "discovery",
                        "catch"
                    ],
                    "common_phrases": [
                        "lucky find",
                        "rare find"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ценная или интересная вещь, которая была обнаружена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "находка",
                                "target_lang_sense_clarification": "найденный предмет"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
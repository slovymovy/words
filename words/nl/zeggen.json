                },
                {
                    "sense_id": "ffbb8ca4-1870-3964-bc03-34b2261a5d08",
                    "sense_definition": "Kritiek uiten of invloed en inspraak hebben.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jij hebt hier helemaal niets te <zeggen>.",
                            "target_lang_translations": {
                                "en": "You have absolutely no <say> here.",
                                "ru": "Ты здесь ничего не <решаешь>."
                            }
                        },
                        {
                            "text": "Hij laat zich door niemand iets <zeggen>.",
                            "target_lang_translations": {
                                "en": "He does not let anyone <tell> him anything.",
                                "ru": "Он никому не позволяет себе <указывать>."

            "senses": [
                {
                    "sense_id": "3b756f96-214a-3d9e-a1e9-96c95e7181ef",
                    "sense_definition": "Een harde, glanzende stof die warmte en elektriciteit goed geleidt, zoals ijzer, goud of koper.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Goud is een zeer kostbaar <w>metaal</w>.",
                            "target_lang_translations": {
                                "ru": "Золото — очень драгоценный <w>металл</w>."
                            }
                        },
                        {
                            "text": "De meeste pannen in de keuken zijn gemaakt van <w>metaal</w>.",
                            "target_lang_translations": {
                                "ru": "Большинство кастрюль на кухне сделаны из <w>металла</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "niet-metaal"
                    ],
                    "common_phrases": [
                        "edel metaal",
                        "zwaar metaal",
                        "van metaal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердое, блестящее вещество, хорошо проводящее тепло и электричество, например железо, золото или медь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "металл",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd85263c-ff60-3af6-9b92-113b6e178d26",
                    "sense_definition": "In de astronomie: elk chemisch element dat zwaarder is dan waterstof en helium.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor astronomen geldt zuurstof technisch gezien als een <w>metaal</w>.",
                            "target_lang_translations": {
                                "ru": "Для астрономов кислород технически считается <w>металлом</w>."
                            }
                        },
                        {
                            "text": "Oude sterren bevatten vaak minder <w>metaal</w> dan jonge sterren.",
                            "target_lang_translations": {
                                "ru": "Старые звезды часто содержат меньше <w>металла</w>, чем молодые звезды."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В астрономии: любой химический элемент тяжелее водорода и гелия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "металл",
                                "target_lang_sense_clarification": "астрономический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f21cffef-624a-34e2-886f-30b6bb8f7167",
                    "sense_definition": "Een lichte kleur (tinctuur) in de heraldiek, specifiek goud (geel) of zilver (wit).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "heraldry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In een familiewapen mag je nooit <w>metaal</w> op <w>metaal</w> plaatsen.",
                            "target_lang_translations": {
                                "ru": "В фамильном гербе никогда нельзя накладывать <w>металл</w> на <w>металл</w>."
                            }
                        },
                        {
                            "text": "Het schild toont een rode leeuw op een achtergrond van <w>metaal</w>.",
                            "target_lang_translations": {
                                "ru": "На щите изображен красный лев на фоне из <w>металла</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Светлый цвет (тинктура) в геральдике, в частности золото (желтый) или серебро (белый)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "металл",
                                "target_lang_sense_clarification": "геральдика"
                            }
                        ]
                    }
                }
            ]
        }
    ]

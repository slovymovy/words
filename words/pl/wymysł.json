                            },
                            {
                                "target_lang_word": "выдумка",
                                "target_lang_sense_clarification": "изобретение ума, небылица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20d3c733-7f8a-33f1-905d-5e6066631765",
                    "sense_definition": "Coś wymyślonego w przesadzie, powstałe z kaprysu, często zbytkowne lub niepotrzebne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mam pieniędzy na twoje kolejne drogie <w>wymysły</w>.",
                            "target_lang_translations": {

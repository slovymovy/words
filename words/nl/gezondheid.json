{
    "word_family": [
        "gezond",
        "ongezond"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fc8327c7-9caa-32aa-9f14-46362c1f9df4",
                    "sense_definition": "De toestand van lichamelijk en geestelijk welzijn; de mate waarin iemand vrij is van ziekte.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Roken is erg slecht voor je <w>gezondheid</w>.",
                            "target_lang_translations": {
                                "ru": "Курение очень вредно для твоего <w>здоровья</w>.",
                                "en": "Smoking is very bad for your <w>health</w>."
                            }
                        },
                        {
                            "text": "Zijn <w>gezondheid</w> was gelukkig niet in gevaar na het ongeluk.",
                            "target_lang_translations": {
                                "ru": "К счастью, его <w>здоровье</w> было вне опасности после аварии.",
                                "en": "Fortunately, his <w>health</w> was not in danger after the accident."
                            }
                        }
                    ],
                    "antonyms": [
                        "ziekte"
                    ],
                    "common_phrases": [
                        "goede gezondheid",
                        "geestelijke gezondheid",
                        "publieke gezondheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние физического и психического благополучия; степень отсутствия болезней.",
                        "en": "The state of physical and mental well-being; the degree to which someone is free from illness."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здоровье",
                                "target_lang_sense_clarification": "общее состояние организма"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "health",
                                "target_lang_sense_clarification": "general physical or mental condition"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "10488d80-256a-3643-8443-a09805d1668b",
                    "sense_definition": "Een uitroep die men doet wanneer iemand niest, om diegene het beste te wensen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Gezondheid</w>! Heb je een kou gevat?",
                            "target_lang_translations": {
                                "ru": "<w>Будь здоров</w>! Ты простудился?",
                                "en": "<w>Bless you</w>! Did you catch a cold?"
                            }
                        },
                        {
                            "text": "Toen hij hard niesde, riep iedereen in de kamer: <w>gezondheid</w>!",
                            "target_lang_translations": {
                                "ru": "Когда он громко чихнул, все в комнате крикнули: «<w>Будь здоров</w>!»",
                                "en": "When he sneezed loudly, everyone in the room shouted: <w>bless you</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "gesundheit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, которое произносят, когда кто-то чихает, чтобы пожелать ему всего наилучшего.",
                        "en": "An exclamation used when someone sneezes to wish them well."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "будьте здоровы",
                                "target_lang_sense_clarification": "вежливое обращение или к группе людей"
                            },
                            {
                                "target_lang_word": "будь здоров",
                                "target_lang_sense_clarification": "неформальное обращение к одному человеку"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "bless you",
                                "target_lang_sense_clarification": "said after someone sneezes"
                            },
                            {
                                "target_lang_word": "gesundheit",
                                "target_lang_sense_clarification": "borrowed from German, used after sneezing"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
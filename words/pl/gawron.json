                {
                    "sense_id": "973be27f-6921-3a26-a8e9-1e2849f7555e",
                    "sense_definition": "Duży, czarny ptak z rodziny krukowatych, często żyjący w stadach w pobliżu osiedli ludzkich i na polach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą w miejskim parku można zobaczyć niejednego <w>gawrona</w> szukającego pożywienia.",
                            "target_lang_translations": {
                                "ru": "Зимой в городском парке можно увидеть не одного <w>грача</w>, ищущего пищу."
                            }
                        },
                        {
                            "text": "Głośne stado <w>gawronów</w> usiadło na wysokim drzewie za domem.",
                            "target_lang_translations": {
                                "ru": "Шумная стая <w>грачей</w> села на высокое дерево за домом."
                            }
                        }
                    ],
                    "synonyms": [
                        "gapa",
                        "reg. łódz. reg. pozn. glapa"
                    ],
                    "common_phrases": [
                        "stado gawronów",
                        "czarny jak gawron"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная черная птица из семейства врановых, часто обитающая стаями вблизи человеческого жилья и на полях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грач",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                }
            ]
        },
        {

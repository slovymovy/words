                    "frequency": "Middle",
                    "semantic_group_id": "participants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obie <w>strony</w> konfliktu zgodziły się na zawieszenie broni.",
                            "target_lang_translations": {
                                "ru": "Обе <w>стороны</w> конфликта согласились на перемирие."
                            }
                        },
                        {
                            "text": "Umowa została podpisana przez każdą ze <w>stron</w>.",

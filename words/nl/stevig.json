{
    "word_family": [
        "stevigheid",
        "verstevigen"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1faf5d9e-63d9-3dc5-aaf0-e7bbef6f3ca8",
                    "sense_definition": "Sterk en solide; bestand tegen druk, stoten of zwaar gebruik.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_strength",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft een <stevig> huis gebouwd dat elke storm kan doorstaan.",
                            "target_lang_translations": {
                                "ru": "Он построил <прочный> дом, который может выдержать любой шторм."
                            }
                        },
                        {
                            "text": "Zorg ervoor dat de knoop <stevig> vastzit voordat je vertrekt.",
                            "target_lang_translations": {
                                "ru": "Убедись, что узел <крепко> завязан, прежде чем уходить."
                            }
                        }
                    ],
                    "synonyms": [
                        "sterk",
                        "solide",
                        "degelijk",
                        "vast"
                    ],
                    "antonyms": [
                        "slap",
                        "wankel",
                        "breekbaar",
                        "fragiel"
                    ],
                    "common_phrases": [
                        "stevige schoenen",
                        "stevig in je schoenen staan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочный и надежный; устойчивый к давлению, ударам или износу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прочный",
                                "target_lang_sense_clarification": "о материалах и конструкциях"
                            },
                            {
                                "target_lang_word": "крепкий",
                                "target_lang_sense_clarification": "сильный, надежный"
                            },
                            {
                                "target_lang_word": "надёжный",
                                "target_lang_sense_clarification": "на который можно положиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32591056-0801-3807-b3ee-67a18ecefdd4",
                    "sense_definition": "Groot van omvang, kracht of intensiteit; aanzienlijk.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We maakten een <stevige> wandeling door het bos om fit te blijven.",
                            "target_lang_translations": {
                                "ru": "Мы совершили <энергичную> прогулку по лесу, чтобы поддерживать форму."
                            }
                        },
                        {
                            "text": "Er was <stevige> kritiek op het nieuwe beleid van de regering.",
                            "target_lang_translations": {
                                "ru": "Была <сильная> критика новой политики правительства."
                            }
                        }
                    ],
                    "synonyms": [
                        "flink",
                        "aanzienlijk",
                        "behoorlijk",
                        "fors"
                    ],
                    "antonyms": [
                        "licht",
                        "gering",
                        "zacht"
                    ],
                    "common_phrases": [
                        "een stevige discussie",
                        "een stevige wandeling",
                        "stevige wind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Значительный по объему, силе или интенсивности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сильный",
                                "target_lang_sense_clarification": "о ветре, критике"
                            },
                            {
                                "target_lang_word": "энергичный",
                                "target_lang_sense_clarification": "о действиях, например, прогулке"
                            },
                            {
                                "target_lang_word": "изрядный",
                                "target_lang_sense_clarification": "значительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15001dab-2712-3715-994e-cde1a6e62c95",
                    "sense_definition": "Groot en sterk gebouwd (van personen); met een vol figuur.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "body_build",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <stevige> man tilde de zware doos zonder enige moeite op.",
                            "target_lang_translations": {
                                "ru": "<Крепкий> мужчина поднял тяжелую коробку без всяких усилий."
                            }
                        },
                        {
                            "text": "Zij heeft een <stevig> postuur en is erg sterk.",
                            "target_lang_translations": {
                                "ru": "У неё <плотное> телосложение, и она очень сильная."
                            }
                        }
                    ],
                    "synonyms": [
                        "fors",
                        "gezet",
                        "robuust"
                    ],
                    "antonyms": [
                        "tenger",
                        "mager",
                        "fijngebouwd"
                    ],
                    "common_phrases": [
                        "een stevige meid",
                        "een stevige kerel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупного и крепкого телосложения (о людях); плотный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плотный",
                                "target_lang_sense_clarification": "о телосложении"
                            },
                            {
                                "target_lang_word": "крепкий",
                                "target_lang_sense_clarification": "физически сильный"
                            },
                            {
                                "target_lang_word": "коренастый",
                                "target_lang_sense_clarification": "широкоплечий и невысокий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31d7a8c5-301c-34c3-8465-3eecf9e71304",
                    "sense_definition": "Voedzaam en vullend (van eten); wat de maag goed vult.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de winter eet ik graag een <stevige> soep met veel groenten.",
                            "target_lang_translations": {
                                "ru": "Зимой я люблю есть <сытный> суп с большим количеством овощей."
                            }
                        },
                        {
                            "text": "Na een <stevig> ontbijt kunnen we er de hele ochtend tegenaan.",
                            "target_lang_translations": {
                                "ru": "После <плотного> завтрака мы можем работать всё утро."
                            }
                        }
                    ],
                    "synonyms": [
                        "voedzaam",
                        "vullend",
                        "zwaar"
                    ],
                    "antonyms": [
                        "licht"
                    ],
                    "common_phrases": [
                        "stevige kost",
                        "een stevig ontbijt",
                        "een stevige maaltijd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Питательный и сытный (о еде); хорошо утоляющий голод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сытный",
                                "target_lang_sense_clarification": "питательный"
                            },
                            {
                                "target_lang_word": "плотный",
                                "target_lang_sense_clarification": "обильный (о приеме пищи)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1995b321-0114-3719-a716-81fc1d705f2e",
                    "sense_definition": "Op een krachtige, vaste of onwrikbare manier.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Houd het stuur <stevig> vast als je over deze hobbelige weg rijdt.",
                            "target_lang_translations": {
                                "ru": "Держи руль <крепко>, когда едешь по этой неровной дороге."
                            }
                        },
                        {
                            "text": "Ze moesten <stevig> doorwerken om het project op tijd af te krijgen.",
                            "target_lang_translations": {
                                "ru": "Им пришлось <упорно> работать, чтобы закончить проект вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "krachtig",
                        "vast",
                        "hard"
                    ],
                    "antonyms": [
                        "zachtjes",
                        "losjes",
                        "voorzichtig"
                    ],
                    "common_phrases": [
                        "stevig vasthouden",
                        "stevig aanpakken",
                        "stevig doorwerken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно, крепко или решительно (наречие)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крепко",
                                "target_lang_sense_clarification": "держать, хватать"
                            },
                            {
                                "target_lang_word": "упорно",
                                "target_lang_sense_clarification": "работать, трудиться"
                            },
                            {
                                "target_lang_word": "сильно",
                                "target_lang_sense_clarification": "интенсивно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7a5e3dfe-f5cf-3823-89e6-b20ad966970e",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>stevigen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <stevig> de constructie met extra balken.",
                            "target_lang_translations": {
                                "ru": "Я <укрепляю> конструкцию дополнительными балками."
                            }
                        },
                        {
                            "text": "Elke dag <stevig> ik mijn positie in het bedrijf.",
                            "target_lang_translations": {
                                "ru": "Каждый день я <укрепляю> свою позицию в компании."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма 1-го лица единственного числа настоящего времени от глагола 'stevigen' (укреплять)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрепляю",
                                "target_lang_sense_clarification": "я укрепляю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfef8642-4b6f-367c-97d7-7876cd5dcd92",
                    "sense_definition": "Gebiedende wijs van <w>stevigen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Stevig> de dijken voordat het water stijgt!",
                            "target_lang_translations": {
                                "ru": "<Укрепите> дамбы, пока вода не поднялась!"
                            }
                        },
                        {
                            "text": "<Stevig> je greep, anders valt het.",
                            "target_lang_translations": {
                                "ru": "<Укрепи> хватку, иначе оно упадет."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола 'stevigen' (укреплять)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрепи",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "024c84c4-ccf8-3f00-a903-f5d49c622d1b",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>stevigen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Stevig> je de tent wel goed voor de storm?",
                            "target_lang_translations": {
                                "ru": "Хорошо ли ты <укрепляешь> палатку перед штормом?"
                            }
                        },
                        {
                            "text": "Waarom <stevig> je die muur niet met cement?",
                            "target_lang_translations": {
                                "ru": "Почему ты не <укрепляешь> эту стену цементом?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма 2-го лица единственного числа настоящего времени от глагола 'stevigen' (укреплять)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрепляешь",
                                "target_lang_sense_clarification": "ты укрепляешь"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "bb5fdad9-be5d-39f4-9e80-c065c822f93b",
                    "sense_definition": "Een situatie of probleem laten voortduren zonder in te grijpen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De onvrede onder het personeel bleef nog lange tijd <w>doorsudderen</w>.",
                            "target_lang_translations": {
                                "en": "The dissatisfaction among the staff continued to <w>fester</w> for a long time.",
                                "ru": "Недовольство среди персонала продолжало <w>тлеть</w> еще долгое время."
                            }
                        },
                        {
                            "text": "De regering laat de problemen in het onderwijs te lang <w>doorsudderen</w>.",
                            "target_lang_translations": {
                                "en": "The government is letting the problems in education <w>drag on</w> for too long.",
                                "ru": "Правительство позволяет проблемам в образовании слишком долго <w>затягиваться</w>."
                            }
                        }
                    ],
                    "synonyms": [

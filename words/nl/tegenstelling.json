{
    "word_family": [
        "tegenstellen",
        "tegengesteld"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0351c1fa-5928-3ed3-9651-2b519825ee83",
                    "sense_definition": "Een duidelijk en groot verschil tussen twee dingen of personen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "contrast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In <w>tegenstelling</w> tot gisteren schijnt vandaag de zon volop.",
                            "target_lang_translations": {
                                "en": "In <w>contrast</w> to yesterday, the sun is shining brightly today.",
                                "ru": "В <w>отличие</w> от вчерашнего дня, сегодня вовсю светит солнце."
                            }
                        },
                        {
                            "text": "De <w>tegenstelling</w> tussen arm en rijk is in dit land erg groot.",
                            "target_lang_translations": {
                                "en": "The <w>contrast</w> between rich and poor is very great in this country.",
                                "ru": "<w>Контраст</w> между бедными и богатыми в этой стране очень велик."
                            }
                        }
                    ],
                    "synonyms": [
                        "contrast"
                    ],
                    "common_phrases": [
                        "in tegenstelling tot",
                        "scherpe tegenstelling",
                        "grote tegenstelling"
                    ],
                    "target_lang_definitions": {
                        "en": "A clear and large difference between two things or people.",
                        "ru": "Явное и значительное различие между двумя предметами или людьми."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "contrast",
                                "target_lang_sense_clarification": "difference between things"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "контраст",
                                "target_lang_sense_clarification": "резкое различие"
                            },
                            {
                                "target_lang_word": "противопоставление",
                                "target_lang_sense_clarification": "сравнение для выявления различий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "645dc059-6c6d-322a-a4d5-7d84655e31a4",
                    "sense_definition": "Iets wat precies het omgekeerde is van iets anders.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "opposite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zwart wordt vaak gezien als de <w>tegenstelling</w> van wit.",
                            "target_lang_translations": {
                                "en": "Black is often seen as the <w>opposite</w> of white.",
                                "ru": "Черный часто рассматривается как <w>противоположность</w> белому."
                            }
                        },
                        {
                            "text": "Zijn vrolijke gedrag was een complete <w>tegenstelling</w> van hoe hij zich werkelijk voelde.",
                            "target_lang_translations": {
                                "en": "His cheerful behavior was a complete <w>contrast</w> to how he really felt.",
                                "ru": "Его веселое поведение было полной <w>противоположностью</w> тому, как он себя чувствовал на самом деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "antithese",
                        "antoniem"
                    ],
                    "common_phrases": [
                        "complete tegenstelling",
                        "directe tegenstelling"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that is exactly the reverse of something else.",
                        "ru": "Что-то, что является полной противоположностью чему-то другому."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opposite",
                                "target_lang_sense_clarification": "reverse counterpart"
                            },
                            {
                                "target_lang_word": "antithesis",
                                "target_lang_sense_clarification": "direct opposite"
                            },
                            {
                                "target_lang_word": "contrast",
                                "target_lang_sense_clarification": "striking difference"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "противоположность",
                                "target_lang_sense_clarification": "обратное явление или качество"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
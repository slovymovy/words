                {
                    "sense_id": "cef0b10d-a1d1-3556-ab02-3157d5c0cad0",
                    "sense_definition": "To transfer money to someone using the PayPal system.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>PayPal</w> me the money for the dinner bill?",

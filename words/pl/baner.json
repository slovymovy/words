    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8e34e768-43fd-3b72-ba02-4652d1b82fad",
                    "sense_definition": "Duża płachta materiału z nadrukowaną treścią reklamową lub informacyjną, przeznaczona do zawieszenia w widocznym miejscu, np. na budynku lub ogrodzeniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "advertising",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma zamówiła ogromny <w>baner</w>, aby promować swoje nowe usługi.",
                            "target_lang_translations": {
                                "ru": "Фирма заказала огромный <w>баннер</w>, чтобы продвигать свои новые услуги."
                            }
                        },
                        {
                            "text": "Na płocie sąsiada wisi stary <w>baner</w> wyborczy, który zasłania widok.",
                            "target_lang_translations": {
                                "ru": "На заборе соседа висит старый предвыборный <w>баннер</w>, который загораживает вид."
                            }
                        }
                    ],
                    "synonyms": [
                        "banner",
                        "banner"
                    ],
                    "common_phrases": [
                        "baner reklamowy",
                        "powiesić baner",
                        "wielki baner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое полотно с напечатанной рекламой или информацией, предназначенное для размещения на видном месте, например, на здании или заборе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баннер",
                                "target_lang_sense_clarification": "рекламное полотно"
                            },
                            {
                                "target_lang_word": "растяжка",
                                "target_lang_sense_clarification": "вид рекламного носителя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96d04030-3f10-3558-bbec-0e008e16b7b2",
                    "sense_definition": "Graficzna forma reklamy wyświetlana na stronach internetowych, często będąca odnośnikiem do innej witryny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "advertising",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten migający <w>baner</w> na górze strony jest bardzo irytujący.",
                            "target_lang_translations": {
                                "ru": "Этот мигающий <w>баннер</w> вверху страницы очень раздражает."
                            }
                        },
                        {
                            "text": "Przypadkowo kliknąłem w <w>baner</w> i otworzyła mi się strona sklepu.",
                            "target_lang_translations": {
                                "ru": "Я случайно нажал на <w>баннер</w>, и открылась страница магазина."
                            }
                        }
                    ],
                    "synonyms": [
                        "banner"
                    ],
                    "common_phrases": [
                        "baner internetowy",
                        "kliknąć w baner",
                        "zablokować banery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Графическая форма рекламы на веб-сайтах, часто содержащая ссылку на другой сайт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баннер",
                                "target_lang_sense_clarification": "интернет-реклама"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66b608e5-d68c-3cc3-8e34-2d2c7cd1e311",
                    "sense_definition": "Ganek, altanka lub pomieszczenie na strychu (znaczenie regionalne lub dawne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W letnie wieczory dziadek lubił odpoczywać na drewnianym <w>banerze</w> przed domem.",
                            "target_lang_translations": {
                                "ru": "Летними вечерами дедушка любил отдыхать на деревянной <w>веранде</w> перед домом."
                            }
                        },
                        {
                            "text": "Wszystkie niepotrzebne rzeczy wynieśliśmy na <w>baner</w>, żeby nie przeszkadzały w domu.",
                            "target_lang_translations": {
                                "ru": "Все ненужные вещи мы вынесли на <w>чердак</w>, чтобы они не мешали в доме."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "użycie regionalne lub rzadkie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крыльцо, беседка или помещение на чердаке (региональное или устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "веранда",
                                "target_lang_sense_clarification": "пристройка к дому"
                            },
                            {
                                "target_lang_word": "чердак",
                                "target_lang_sense_clarification": "помещение под крышей"
                            }
                        ]
                    }
                }
            ]
        }
    ]

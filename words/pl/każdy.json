    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "93b8189d-6742-3e9f-8e64-80a67cd535b2",
                    "sense_definition": "Odnoszący się do wszystkich osób, rzeczy lub zjawisk w danym zbiorze, bez wyjątku; oznaczający pojedynczy element z całości.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Każda</w> kobieta jest piękna i wyjątkowa na swój sposób.",
                            "target_lang_translations": {
                                "ru": "<w>Каждая</w> женщина красива и уникальна по-своему."
                            }
                        },
                        {
                            "text": "<w>Każde</w> zwierzę zasługuje na dobrego i odpowiedzialnego właściciela.",
                            "target_lang_translations": {
                                "ru": "<w>Каждое</w> животное заслуживает хорошего и ответственного хозяина."
                            }
                        },
                        {
                            "text": "Muszę brać to lekarstwo <w>każdego</w> dnia rano.",
                            "target_lang_translations": {
                                "ru": "Я должен принимать это лекарство <w>каждый</w> день утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "wszelki"
                    ],
                    "antonyms": [
                        "żaden",
                        "niektóry"
                    ],
                    "common_phrases": [
                        "w każdym razie",
                        "na każdym kroku",
                        "każdego dnia",
                        "o każdej porze",
                        "pod każdym względem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся ко всем лицам, предметам или явлениям в данной группе без исключения; обозначающий отдельный элемент целого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каждый",
                                "target_lang_sense_clarification": "один из всех"
                            },
                            {
                                "target_lang_word": "всякий",
                                "target_lang_sense_clarification": "любой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcdd4890-084e-388a-ad71-af91a7cd61f6",
                    "sense_definition": "Wszyscy ludzie; każda osoba z osobna.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Każdy</w> ma prawo do życia, wolności i bezpieczeństwa swej osoby.",
                            "target_lang_translations": {
                                "ru": "<w>Каждый</w> имеет право на жизнь, свободу и личную неприкосновенность."
                            }
                        },
                        {
                            "text": "Prawie <w>każdy</w> z nas lubi lody i słodycze.",
                            "target_lang_translations": {
                                "ru": "Почти <w>каждый</w> из нас любит мороженое и сладости."
                            }
                        }
                    ],
                    "synonyms": [
                        "wszystek"
                    ],
                    "antonyms": [
                        "żaden",
                        "niektóry"
                    ],
                    "common_phrases": [
                        "każdy wie",
                        "każdy z nas",
                        "jak każdy inny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Все люди; любой человек в отдельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каждый",
                                "target_lang_sense_clarification": "человек"
                            },
                            {
                                "target_lang_word": "любой",
                                "target_lang_sense_clarification": "кто угодно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

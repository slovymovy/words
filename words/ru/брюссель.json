    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "766267c7-65e9-3114-9669-1997c04c04a9",
                    "sense_definition": "Столица Бельгии, а также город, в котором расположены штаб-квартиры Евросоюза и НАТО.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "city_literal",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Мы прилетели в <w>Брюссель</w> рано утром и сразу поехали в отель.",
                            "target_lang_translations": {
                                "en": "We flew to <w>Brussels</w> early in the morning and went straight to the hotel."
                            }
                        },
                        {
                            "text": "Исторический центр <w>Брюсселя</w> привлекает тысячи туристов каждый год.",
                            "target_lang_translations": {
                                "en": "The historic center of <w>Brussels</w> attracts thousands of tourists every year."
                            }
                        }
                    ],
                    "common_phrases": [
                        "центр Брюсселя",
                        "поехать в Брюссель",
                        "карта Брюсселя",
                        "мэр Брюсселя"
                    ],
                    "target_lang_definitions": {
                        "en": "The capital of Belgium, as well as the city where the headquarters of the European Union and NATO are located."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Brussels",
                                "target_lang_sense_clarification": "capital of Belgium"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17d636a6-2804-3fe6-9937-e2246fb2aedd",
                    "sense_definition": "Руководство Европейского союза (политическая метонимия).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "political_metonymy",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "Официальный <w>Брюссель</w> выразил обеспокоенность экономической ситуацией в регионе.",
                            "target_lang_translations": {
                                "en": "Official <w>Brussels</w> expressed concern about the economic situation in the region."
                            }
                        },
                        {
                            "text": "Новые правила торговли были жестко раскритикованы <w>Брюсселем</w>.",
                            "target_lang_translations": {
                                "en": "The new trade rules were harshly criticized by <w>Brussels</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Европейский союз"
                    ],
                    "common_phrases": [
                        "официальный Брюссель",
                        "решение Брюсселя",
                        "позиция Брюсселя",
                        "диалог с Брюсселем"
                    ],
                    "target_lang_definitions": {
                        "en": "The leadership of the European Union (political metonymy)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Brussels",
                                "target_lang_sense_clarification": "European Union administration"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2334a208-6901-3c1d-a129-86109349bec5",
                    "sense_definition": "Руководство НАТО (политическая метонимия).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "political_metonymy",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "<w>Брюссель</w> объявил о начале масштабных военных учений в Европе.",
                            "target_lang_translations": {
                                "en": "<w>Brussels</w> announced the start of large-scale military exercises in Europe."
                            }
                        },
                        {
                            "text": "Генерал отправился в <w>Брюссель</w> для согласования стратегии обороны.",
                            "target_lang_translations": {
                                "en": "The general went to <w>Brussels</w> to coordinate the defense strategy."
                            }
                        }
                    ],
                    "synonyms": [
                        "НАТО"
                    ],
                    "common_phrases": [
                        "планы Брюсселя",
                        "заявление Брюсселя"
                    ],
                    "target_lang_definitions": {
                        "en": "The leadership of NATO (political metonymy)."

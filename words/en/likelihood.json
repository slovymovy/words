    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "606082e3-2fd3-39ca-873b-f2ce7a2bd292",
                    "sense_definition": "The chance of something happening; the state or degree of being probable.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "probability_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In all <w>likelihood</w> the meeting will be cancelled due to the storm.",
                            "target_lang_translations": {
                                "ru": "По всей <w>вероятности</w>, встреча будет отменена из-за шторма."
                            }
                        },
                        {
                            "text": "There is very little <w>likelihood</w> that he will agree to the new terms.",
                            "target_lang_translations": {
                                "ru": "Существует очень малая <w>вероятность</w> того, что он согласится на новые условия."
                            }
                        }
                    ],
                    "synonyms": [
                        "odds",
                        "likeliness"
                    ],
                    "antonyms": [
                        "unlikelihood"
                    ],
                    "common_phrases": [
                        "in all likelihood",
                        "likelihood of",
                        "high likelihood",
                        "low likelihood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шанс того, что что-то произойдет; степень вероятности события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вероятность",
                                "target_lang_sense_clarification": "степень возможности наступления события"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0c90184-ed9e-30a1-ae00-39e91869be51",
                    "sense_definition": "The probability that a specific outcome was generated by a random distribution with a specific parameter.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "probability_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The statistician calculated the maximum <w>likelihood</w> to estimate the model parameters.",
                            "target_lang_translations": {
                                "ru": "Статистик рассчитал максимальное <w>правдоподобие</w> для оценки параметров модели."
                            }
                        },
                        {
                            "text": "This function represents the <w>likelihood</w> of the observed data given the hypothesis.",
                            "target_lang_translations": {
                                "ru": "Эта функция представляет собой <w>правдоподобие</w> наблюдаемых данных при данной гипотезе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "maximum likelihood"
                    ],
                    "target_lang_definitions": {
                        "ru": "В статистике: вероятность того, что конкретный исход был получен при определенных параметрах распределения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правдоподобие",
                                "target_lang_sense_clarification": "статистический термин (функция правдоподобия)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6600937d-2128-3f01-b2b1-95652dc68707",
                    "sense_definition": "Likeness or resemblance to something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The portrait bears a strong <w>likelihood</w> to the king in his youth.",
                            "target_lang_translations": {
                                "ru": "Портрет имеет сильное <w>сходство</w> с королем в молодости."
                            }
                        },
                        {
                            "text": "There is no <w>likelihood</w> between the two distinct theories.",
                            "target_lang_translations": {
                                "ru": "Между двумя различными теориями нет никакого <w>сходства</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "likeliness"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used to mean resemblance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подобие или внешнее сходство с кем-либо или чем-либо (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сходство",
                                "target_lang_sense_clarification": "наличие общих черт"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
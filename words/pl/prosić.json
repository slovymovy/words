                {
                    "sense_id": "78bb39e3-4e45-3d21-8f0d-bf42336937e8",
                    "sense_definition": "Zwracać się do kogoś z życzeniem, aby ta osoba coś zrobiła lub coś dała.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy mogę cię <w>prosić</w> o szklankę wody?",
                            "target_lang_translations": {
                                "ru": "Могу я <w>попросить</w> у тебя стакан воды?"
                            }
                        },
                        {
                            "text": "Student <w>prosił</w> profesora o wyjaśnienie trudnego tematu.",
                            "target_lang_translations": {
                                "ru": "Студент <w>просил</w> профессора объяснить трудную тему."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwracać się z prośbą",
                        "wnosić prośbę",
                        "apelować"
                    ],
                    "antonyms": [
                        "kazać",
                        "rozkazywać",
                        "nakazywać",
                        "zmuszać",
                        "polecić",
                        "zlecać",
                        "zarządzać"
                    ],
                    "common_phrases": [
                        "prosić o pomoc",
                        "prosić o radę",
                        "prosić o rękę",
                        "prosić o głos",
                        "prosić o wybaczenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться к кому-либо с просьбой сделать что-либо или дать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просить",
                                "target_lang_sense_clarification": "обращаться с просьбой"
                            },
                            {
                                "target_lang_word": "попросить",
                                "target_lang_sense_clarification": "выразить просьбу (совершенный вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e97e210b-3864-3826-b558-91ba0ce5536d",
                    "sense_definition": "Zachęcać kogoś do wejścia do środka, zajęcia miejsca lub wzięcia udziału w jakimś wydarzeniu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz uprzejmie <w>prosił</w> wszystkich gości do stołu.",
                            "target_lang_translations": {
                                "ru": "Хозяин вежливо <w>приглашал</w> всех гостей к столу."
                            }
                        },
                        {
                            "text": "Chciałbym cię <w>prosić</w> do tańca, jeśli się zgodzisz.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>пригласить</w> тебя на танец, если ты согласишься."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapraszać",
                        "inwitować"
                    ],
                    "antonyms": [
                        "kazać",
                        "rozkazywać",
                        "nakazywać",
                        "zmuszać",
                        "polecić",
                        "zlecać",
                        "zarządzać"

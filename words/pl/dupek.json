            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0841f9b3-bf18-3118-91ee-648edfa5099d",
                    "sense_definition": "Wulgarnie o mężczyźnie, który zachowuje się w sposób arogancki, złośliwy lub chamski.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "insult_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy kierownik to straszny <w>dupek</w>, nikt go nie lubi.",
                            "target_lang_translations": {
                                "ru": "Этот новый руководитель — ужасный <w>мудак</w>, никто его не любит."
                            }
                        },
                        {
                            "text": "Nie bądź takim <w>dupkiem</w> i przeproś ją za to, co powiedziałeś.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>мудаком</w> и извинись перед ней за то, что сказал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skończony dupek",
                        "kawał dupka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "offensive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарно о мужчине, который ведет себя высокомерно, злобно или по-хамски."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мудак",
                                "target_lang_sense_clarification": "грубое оскорбление"
                            },
                            {
                                "target_lang_word": "засранец",
                                "target_lang_sense_clarification": "мерзкий человек"
                            },
                            {
                                "target_lang_word": "козёл",
                                "target_lang_sense_clarification": "неприятный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "067a5f36-eedd-31ed-bf56-f828d1c31594",
                    "sense_definition": "Wulgarnie o osobie niezaradnej, naiwnej lub głupiej, którą się lekceważy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "insult_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ale ze mnie <w>dupek</w>, zapomniałem zabrać dokumenty z domu.",
                            "target_lang_translations": {
                                "ru": "Ну я и <w>дурак</w>, забыл забрать документы из дома."
                            }
                        },
                        {
                            "text": "Tylko taki <w>dupek</w> jak on mógł uwierzyć w te kłamstwa.",
                            "target_lang_translations": {
                                "ru": "Только такой <w>придурок</w>, как он, мог поверить в эту ложь."
                            }
                        }
                    ],
                    "synonyms": [
                        "oferma",
                        "fujara",
                        "zero"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "depreciative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарно о беспомощном, наивном или глупом человеке, которым пренебрегают."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дурак",
                                "target_lang_sense_clarification": "глупый человек"
                            },
                            {
                                "target_lang_word": "чмо",
                                "target_lang_sense_clarification": "ничтожество"
                            },
                            {
                                "target_lang_word": "придурок",
                                "target_lang_sense_clarification": "недотёпа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0f5c65e-1a8c-3c81-99b1-f81dcdd80a04",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>dupka</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym sklepie nie ma już tych chrupiących <w>dupek</w> od chleba.",
                            "target_lang_translations": {
                                "ru": "В этом магазине уже нет тех хрустящих <w>горбушек</w> хлеба."
                            }
                        },
                        {
                            "text": "Na plaży było pełno ładnych <w>dupek</w> opalających się na słońcu.",
                            "target_lang_translations": {
                                "ru": "На пляже было полно симпатичных <w>попок</w>, загорающих на солнце."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного «dupka» (попка, горбушка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попок",
                                "target_lang_sense_clarification": "уменьш. от ягодицы"
                            },
                            {
                                "target_lang_word": "горбушек",
                                "target_lang_sense_clarification": "край хлеба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "260e6f5e-0186-3f9b-b319-857a68ccaaab",
                    "sense_definition": "Potoczne określenie waleta w kartach do gry.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zostały mu na ręce same dziewiątki i <w>dupki</w>.",
                            "target_lang_translations": {
                                "ru": "У него на руках остались одни девятки и <w>валеты</w>."
                            }
                        },
                        {
                            "text": "Rzucił na stół <w>dupka</w> żołędnego, licząc na wygraną.",
                            "target_lang_translations": {
                                "ru": "Он бросил на стол трефового <w>валета</w>, рассчитывая на выигрыш."
                            }
                        }
                    ],
                    "synonyms": [

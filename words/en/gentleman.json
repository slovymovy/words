    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d77ac7e-240c-3dec-ba00-bbd89f213451",
                    "sense_definition": "A polite word used to refer to or address a man.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ladies and <w>gentlemen</w>, please take your seats as the show is about to begin.",
                            "target_lang_translations": {
                                "ru": "Дамы и <w>господа</w>, пожалуйста, займите свои места, так как представление вот-вот начнется."
                            }
                        },
                        {
                            "text": "Please escort this <w>gentleman</w> to the waiting room so he can speak with the manager.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проводите этого <w>господина</w> в комнату ожидания, чтобы он мог поговорить с управляющим."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ladies and gentlemen",
                        "elderly gentleman",
                        "this gentleman"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливое слово, используемое для обозначения мужчины или обращения к нему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "господин",
                                "target_lang_sense_clarification": "вежливое обращение или упоминание"
                            },
                            {
                                "target_lang_word": "джентльмен",
                                "target_lang_sense_clarification": "вежливое обозначение мужчины"
                            },
                            {
                                "target_lang_word": "господа",
                                "target_lang_sense_clarification": "обращение к группе людей (дамы и господа)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4eded55f-50f2-363f-b57f-5f17babac59e",
                    "sense_definition": "A man who is polite, behaves well, and treats others with respect.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He opened the door for her and acted like a perfect <w>gentleman</w> all evening.",
                            "target_lang_translations": {
                                "ru": "Он открыл ей дверь и весь вечер вел себя как истинный <w>джентльмен</w>."
                            }
                        },
                        {
                            "text": "Being a <w>gentleman</w>, he offered his coat to the woman who was shivering in the cold.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>джентльменом</w>, он предложил свое пальто женщине, которая дрожала от холода."
                            }
                        }
                    ],
                    "common_phrases": [
                        "perfect gentleman",
                        "true gentleman",
                        "being a gentleman",
                        "act like a gentleman"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, который вежлив, хорошо воспитан и относится к другим с уважением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "джентльмен",
                                "target_lang_sense_clarification": "воспитанный, благородный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d885a51-2f67-3c31-90c9-105cbaf37f2b",
                    "sense_definition": "A man of high social standing or wealth, especially in the past, who did not have to work for a living.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The large estate was owned by a wealthy <w>gentleman</w> who rarely visited the local village.",
                            "target_lang_translations": {
                                "ru": "Большим поместьем владел богатый <w>господин</w>, который редко посещал местную деревню."
                            }
                        },
                        {
                            "text": "In the nineteenth century, a <w>gentleman</w> of independent means often spent his time traveling.",
                            "target_lang_translations": {
                                "ru": "В девятнадцатом веке <w>джентльмен</w> с независимым доходом часто проводил время в путешествиях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gentleman of leisure",
                        "country gentleman"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to historical social class"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек высокого социального положения или богатый человек, особенно в прошлом, которому не нужно было работать ради средств к существованию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "джентльмен",
                                "target_lang_sense_clarification": "человек благородного происхождения"
                            },
                            {
                                "target_lang_word": "аристократ",
                                "target_lang_sense_clarification": "представитель высшего сословия"
                            },
                            {
                                "target_lang_word": "господин",
                                "target_lang_sense_clarification": "состоятельный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8981621-578c-33c8-8788-2b68c3932fcb",
                    "sense_definition": "An amateur who participates in a sport or activity for pleasure rather than for money.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was a <w>gentleman</w> scientist who funded his own research experiments during the Victorian era.",
                            "target_lang_translations": {
                                "ru": "Он был ученым-<w>любителем</w>, который финансировал свои собственные научные эксперименты в викторианскую эпоху."
                            }
                        },
                        {
                            "text": "The cricket match was played between the professional players and the <w>gentlemen</w>.",
                            "target_lang_translations": {
                                "ru": "Матч по крикету был сыгран между профессиональными игроками и <w>любителями</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dilettante"
                    ],
                    "common_phrases": [
                        "gentleman scientist",
                        "gentleman farmer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любитель, занимающийся спортом или какой-либо деятельностью ради удовольствия, а не ради денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любитель",
                                "target_lang_sense_clarification": "непрофессионал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "703d393c-f0fb-3c0a-a137-170652f82a0c",
                    "sense_definition": "An effeminate or overly sophisticated man.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rough sailors mocked him as a delicate <w>gentleman</w> who could not handle hard labor.",
                            "target_lang_translations": {
                                "ru": "Грубые моряки насмехались над ним как над изнеженным <w>белоручкой</w>, который не мог справиться с тяжелой работой."
                            }
                        },
                        {
                            "text": "He was teased for being a <w>gentleman</w> because he preferred reading poetry to playing sports.",
                            "target_lang_translations": {
                                "ru": "Его дразнили <w>неженкой</w>, потому что он предпочитал чтение поэзии спорту."
                            }
                        }
                    ],
                    "synonyms": [
                        "sissy",
                        "pansy",
                        "pretty boy"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often derogatory or ironic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женоподобный или чрезмерно утонченный мужчина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неженка",
                                "target_lang_sense_clarification": "изнеженный мужчина"
                            },
                            {
                                "target_lang_word": "белоручка",
                                "target_lang_sense_clarification": "человек, избегающий физического труда"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "puncher",
        "punchy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d6d0ed05-8974-3587-837a-58784716d7bc",
                    "sense_definition": "To strike someone or something with one's fist.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_strike",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was so angry that he wanted to <w>punch</w> the wall.",
                            "target_lang_translations": {
                                "ru": "Он был так зол, что хотел <w>ударить</w> кулаком в стену."
                            }
                        },
                        {
                            "text": "The boxer knows how to <w>punch</w> hard and fast.",
                            "target_lang_translations": {
                                "ru": "Боксер знает, как <w>бить</w> сильно и быстро."
                            }
                        }
                    ],
                    "synonyms": [
                        "box",
                        "slug"
                    ],
                    "common_phrases": [
                        "punch hard",
                        "punch in the face"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ударить кого-то или что-то кулаком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ударить",
                                "target_lang_sense_clarification": "нанести удар (обычно кулаком)"
                            },
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "наносить удары"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc424023-0ba5-3dce-aab4-42f274e4d84e",
                    "sense_definition": "To make a hole in something, often using a special tool.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "perforation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>punch</w> a hole in the corner of the document.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>пробейте</w> отверстие в углу документа."
                            }
                        },
                        {
                            "text": "The conductor will <w>punch</w> your ticket when he comes by.",
                            "target_lang_translations": {
                                "ru": "Кондуктор <w>прокомпостирует</w> ваш билет, когда подойдет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "punch a hole",
                        "punch a ticket"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать отверстие в чем-либо, часто используя специальный инструмент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробивать",
                                "target_lang_sense_clarification": "делать отверстие"
                            },
                            {
                                "target_lang_word": "компостировать",
                                "target_lang_sense_clarification": "пробивать билет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "154031f3-90a1-3ef5-b585-0f444a6b9f55",
                    "sense_definition": "To press buttons or keys on a device to operate it or enter information.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "operate_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>punch</w> your code into the keypad to open the door.",
                            "target_lang_translations": {
                                "ru": "Вам нужно <w>ввести</w> свой код на клавиатуре, чтобы открыть дверь."
                            }
                        },
                        {
                            "text": "He forgot to <w>punch</w> the clock when he arrived at work.",
                            "target_lang_translations": {
                                "ru": "Он забыл <w>отметиться</w> на часах, когда пришел на работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "punch in",
                        "punch the clock",
                        "punch a button"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нажимать кнопки или клавиши на устройстве для управления им или ввода информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набирать",
                                "target_lang_sense_clarification": "вводить данные нажатием кнопок"
                            },
                            {
                                "target_lang_word": "вводить",
                                "target_lang_sense_clarification": "вносить информацию"
                            },
                            {
                                "target_lang_word": "отмечаться",
                                "target_lang_sense_clarification": "фиксировать время прихода/ухода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c94501ce-f4de-32da-a98d-8cd923e629d9",
                    "sense_definition": "To herd cattle.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "herding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cowboys used to <w>punch</w> cattle across the great plains.",
                            "target_lang_translations": {
                                "ru": "Ковбои раньше <w>гнали</w> скот через великие равнины."
                            }
                        },
                        {
                            "text": "He found a job helping to <w>punch</w> cows on a ranch in Texas.",
                            "target_lang_translations": {
                                "ru": "Он нашел работу, помогая <w>пасти</w> коров на ранчо в Техасе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "punch cattle",
                        "punch cows"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Western US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сгонять скот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сгонять",
                                "target_lang_sense_clarification": "собирать скот в стадо"
                            },
                            {
                                "target_lang_word": "пасти",
                                "target_lang_sense_clarification": "присматривать за скотом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4b841d70-a6b2-3093-acbb-820d8fe69b68",
                    "sense_definition": "A hit or strike made with the fist.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_strike",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He threw a powerful <w>punch</w> at his opponent.",
                            "target_lang_translations": {
                                "ru": "Он нанес мощный <w>удар</w> своему противнику."
                            }
                        },
                        {
                            "text": "The boxer landed a solid <w>punch</w> to the jaw.",
                            "target_lang_translations": {
                                "ru": "Боксер нанес сильный <w>удар</w> в челюсть."
                            }
                        }
                    ],
                    "synonyms": [
                        "blow"
                    ],
                    "common_phrases": [
                        "throw a punch",
                        "land a punch",
                        "pack a punch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удар, нанесенный кулаком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удар",
                                "target_lang_sense_clarification": "резкое физическое воздействие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2dba3735-bbae-3d82-8ef5-86808efd7fbb",
                    "sense_definition": "A drink made by mixing fruit juices, water, spices, and often wine or liquor.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "beverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We served a large bowl of fruit <w>punch</w> at the party.",
                            "target_lang_translations": {
                                "ru": "На вечеринке мы подали большую чашу фруктового <w>пунша</w>."
                            }
                        },
                        {
                            "text": "She poured herself a glass of the sweet red <w>punch</w>.",
                            "target_lang_translations": {
                                "ru": "Она налила себе стакан сладкого красного <w>пунша</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fruit punch",
                        "rum punch",
                        "bowl of punch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напиток, приготовленный путем смешивания фруктовых соков, воды, специй и часто вина или ликера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пунш",
                                "target_lang_sense_clarification": "алкогольный или фруктовый напиток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f082e753-14a2-3086-96a1-a15f58050e10",
                    "sense_definition": "Power, strength, or effective force.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "energy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new engine has a lot more <w>punch</w> than the old one.",
                            "target_lang_translations": {
                                "ru": "У нового двигателя гораздо больше <w>мощи</w>, чем у старого."
                            }
                        },
                        {
                            "text": "Her speech lacked <w>punch</w> and failed to convince the audience.",
                            "target_lang_translations": {
                                "ru": "Её речи не хватало <w>остроты</w>, и она не смогла убедить аудиторию."
                            }
                        }
                    ],
                    "synonyms": [
                        "oomph",
                        "pep"
                    ],
                    "common_phrases": [
                        "pack a punch",
                        "lose its punch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мощь, сила или действенная сила."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сила",
                                "target_lang_sense_clarification": "мощь воздействия"
                            },
                            {
                                "target_lang_word": "энергия",
                                "target_lang_sense_clarification": "активность, напор"
                            },
                            {
                                "target_lang_word": "острота",
                                "target_lang_sense_clarification": "выразительность (о речи)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0f27ba3-8b98-3e8a-b740-e4ee167e235c",
                    "sense_definition": "A tool used for making holes in paper, leather, or metal.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "perforation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need a hole <w>punch</w> to organize these papers in the binder.",
                            "target_lang_translations": {
                                "ru": "Мне нужен <w>дырокол</w>, чтобы разложить эти бумаги в папке."
                            }
                        },
                        {
                            "text": "He used a leather <w>punch</w> to add an extra hole to his belt.",
                            "target_lang_translations": {
                                "ru": "Он использовал <w>пробойник</w> для кожи, чтобы сделать дополнительную дырку в ремне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hole punch",
                        "leather punch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструмент, используемый для проделывания отверстий в бумаге, коже или металле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дырокол",
                                "target_lang_sense_clarification": "для бумаги"
                            },
                            {
                                "target_lang_word": "пробойник",
                                "target_lang_sense_clarification": "для кожи или металла"
                            },
                            {
                                "target_lang_word": "кернер",
                                "target_lang_sense_clarification": "слесарный инструмент для разметки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7ee857e5-e00c-3989-a3b2-99c175505a82",
                    "sense_definition": "A traditional glove puppet character, typically violent and hunchbacked, who appears in shows with his wife Judy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "puppet_character",
                    "name_type": "fictional_name",
                    "examples": [
                        {
                            "text": "The children laughed as <w>Punch</w> chased the other puppets around the stage.",
                            "target_lang_translations": {
                                "ru": "Дети смеялись, когда <w>Панч</w> гонялся за другими куклами по сцене."
                            }
                        },
                        {
                            "text": "We watched a traditional <w>Punch</w> and Judy show on the beach.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели традиционное представление \"<w>Панч</w> и Джуди\" на пляже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Punch and Judy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционный персонаж перчаточных кукол, обычно жестокий и горбатый, выступающий в шоу со своей женой Джуди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Панч",
                                "target_lang_sense_clarification": "имя персонажа"
                            },
                            {
                                "target_lang_word": "Петрушка",
                                "target_lang_sense_clarification": "русский аналог персонажа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e3aee70-b4a5-318e-9ad1-a8f29e4a3e65",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Punch</w> has lived in this town for over forty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Панч</w> живет в этом городе уже более сорока лет."
                            }
                        },
                        {
                            "text": "I am looking for the contact information of a Sarah <w>Punch</w>.",
                            "target_lang_translations": {
                                "ru": "Я ищу контактную информацию Сары <w>Панч</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Панч",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7bf38052-8805-388c-81fe-c0149dca5c7f",
                    "sense_definition": "Short and thickset in physical build.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_description",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer rode a small, <w>punch</w> horse across the field.",
                            "target_lang_translations": {
                                "ru": "Фермер ехал через поле на маленькой, <w>коренастой</w> лошади."
                            }
                        },
                        {
                            "text": "He was described as a <w>punch</w> man with broad shoulders.",
                            "target_lang_translations": {
                                "ru": "Его описывали как <w>приземистого</w> мужчину с широкими плечами."
                            }

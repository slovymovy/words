                    "sense_definition": "Meervoud van <w>laars</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik trek mijn <laarzen> aan omdat het buiten regent.",
                            "target_lang_translations": {
                                "ru": "Я надеваю свои <w>сапоги</w>, так как на улице идет дождь."
                            }
                        },
                        {
                            "text": "Zijn nieuwe leren <laarzen> waren erg duur.",
                            "target_lang_translations": {
                                "ru": "Его новые кожаные <w>сапоги</w> были очень дорогими."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hoge laarzen",
                        "rubberen laarzen",
                        "vieze laarzen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова 'сапог'; высокая обувь, закрывающая стопу и голень."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сапоги",
                                "target_lang_sense_clarification": "обувь"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c6ba03bd-162f-33a7-b7ac-9658ff743b6b",
                    "sense_definition": "Iemand slaan of geselen met een touw, vooral als straf op schepen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De matroos werd voor straf <gelaarsd> door de bootsman.",
                            "target_lang_translations": {
                                "ru": "Матрос был <w>выпорот</w> боцманом в наказание."
                            }
                        },
                        {
                            "text": "In die tijd was het normaal om muiters te <laarzen>.",
                            "target_lang_translations": {
                                "ru": "В то время было в порядке вещей <w>пороть</w> мятежников."
                            }
                        }
                    ],
                    "synonyms": [
                        "geselen",
                        "afranselen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бить или пороть кого-либо веревкой, особенно в качестве наказания на кораблях (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пороть",
                                "target_lang_sense_clarification": "наказывать ударами"
                            },
                            {
                                "target_lang_word": "сечь",
                                "target_lang_sense_clarification": "бить плетью или розгами"
                            }
                        ]
                    }
                }
            ]
        }

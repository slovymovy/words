    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b4194201-5893-3a5e-9acf-29a82d2baa5e",
                    "sense_definition": "Used at the end of a list to indicate that further, similar items are included; an alternative form of <w>etc.</w>",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please bring your own pens, pencils, paper, <w>etc</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, принесите свои ручки, карандаши, бумагу <w>и т. д.</w>"
                            }
                        },
                        {
                            "text": "The festival features music, dance, food, <w>etc</w>.",
                            "target_lang_translations": {
                                "ru": "На фестивале представлены музыка, танцы, еда <w>и т. д.</w>"
                            }
                        }
                    ],
                    "synonyms": [
                        "and so on",
                        "and so forth"
                    ],
                    "common_phrases": [
                        "books, pens, etc",
                        "apples, oranges, etc"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "alternative spelling of etc."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в конце перечисления для указания на то, что список можно продолжить другими подобными элементами; сокращение от «и так далее»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и так далее",
                                "target_lang_sense_clarification": "стандартное завершение перечисления"
                            },
                            {
                                "target_lang_word": "и тому подобное",
                                "target_lang_sense_clarification": "для перечисления схожих предметов или явлений"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "drinker",
        "drinkable",
        "undrinkable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2a55ae17-7933-3e76-9744-d5baae7b61b0",
                    "sense_definition": "To consume a liquid by swallowing it through the mouth.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>drank</w> the water I gave him.",
                            "target_lang_translations": {
                                "ru": "Он <w>выпил</w> воду, которую я ему дал."
                            }
                        },
                        {
                            "text": "You should <w>drink</w> plenty of fluids when you are sick.",
                            "target_lang_translations": {
                                "ru": "Тебе следует <w>пить</w> много жидкости, когда ты болеешь."
                            }
                        }
                    ],
                    "synonyms": [
                        "imbibe",
                        "quaff",
                        "hydrate",
                        "sip",
                        "gulp",
                        "swig"
                    ],
                    "common_phrases": [
                        "drink water",
                        "eat and drink",
                        "drink up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поглощать жидкость, глотая её ртом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пить",
                                "target_lang_sense_clarification": "процесс поглощения жидкости"
                            },
                            {
                                "target_lang_word": "выпить",
                                "target_lang_sense_clarification": "совершенный вид: выпить определенное количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a171a2a2-b2dd-3b73-8667-82b258e5eed1",
                    "sense_definition": "To consume alcoholic beverages, often socially or habitually.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "alcohol_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He does not <w>drink</w> because he needs to drive home.",
                            "target_lang_translations": {
                                "ru": "Он не <w>пьёт</w>, потому что ему нужно вести машину домой."
                            }
                        },
                        {
                            "text": "They went to the bar to <w>drink</w> and celebrate.",
                            "target_lang_translations": {
                                "ru": "Они пошли в бар, чтобы <w>выпить</w> и отпраздновать."
                            }
                        }
                    ],
                    "synonyms": [
                        "imbibe",
                        "drink alcohol"
                    ],
                    "common_phrases": [
                        "drink heavily",
                        "drink and drive",
                        "don't drink"
                    ],
                    "target_lang_definitions": {
                        "ru": "Употреблять алкогольные напитки, часто в компании или по привычке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пить",
                                "target_lang_sense_clarification": "употреблять алкоголь (вообще или в данный момент)"
                            },
                            {
                                "target_lang_word": "выпивать",
                                "target_lang_sense_clarification": "периодически употреблять спиртное"
                            },
                            {
                                "target_lang_word": "выпить",
                                "target_lang_sense_clarification": "принять алкоголь (однократно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ffdfa15-1092-344c-90e7-ec5866afb35b",
                    "sense_definition": "To wish someone well or honour them by raising a glass and consuming liquid.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "toasting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let us <w>drink</w> to the health of the bride and groom.",
                            "target_lang_translations": {
                                "ru": "Давайте <w>выпьем</w> за здоровье жениха и невесты."
                            }
                        },
                        {
                            "text": "We raised our glasses to <w>drink</w> to his future success.",
                            "target_lang_translations": {
                                "ru": "Мы подняли бокалы, чтобы <w>выпить</w> за его будущий успех."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drink to someone",
                        "drink a toast"
                    ],
                    "target_lang_definitions": {
                        "ru": "Желать кому-либо добра или чествовать, поднимая бокал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпить",
                                "target_lang_sense_clarification": "за кого-то или что-то (тост)"
                            },
                            {
                                "target_lang_word": "пить",
                                "target_lang_sense_clarification": "поднимать тост"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4004d56b-b122-332c-b235-2220be0df381",

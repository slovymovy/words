{
    "word_family": [
        "rajdowy",
        "rajdowiec"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f9594d1b-5b37-3b4a-8432-af28225c89b8",
                    "sense_definition": "Wyścig samochodowy lub motocyklowy, często odbywający się etapami na drogach publicznych lub w trudnym terenie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "organized_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca przygotowuje się do startu w trudnym <rajdzie> terenowym.",
                            "target_lang_translations": {
                                "ru": "Водитель готовится к старту в сложном <ралли> по бездорожью."
                            }
                        },
                        {
                            "text": "Ten słynny <rajd> samochodowy przyciąga co roku tysiące kibiców.",
                            "target_lang_translations": {
                                "ru": "Это знаменитое автомобильное <ралли> ежегодно привлекает тысячи болельщиков."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyścig"
                    ],
                    "common_phrases": [
                        "rajd samochodowy",
                        "rajd Dakar",
                        "kierowca rajdowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Автомобильная или мотоциклетная гонка, часто проходящая этапами по дорогам общего пользования или бездорожью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ралли",
                                "target_lang_sense_clarification": "автоспорт"
                            },
                            {
                                "target_lang_word": "гонка",
                                "target_lang_sense_clarification": "соревнование на скорость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c79e56b1-779b-31a6-9e1d-8d72a27c483c",
                    "sense_definition": "Zorganizowana, wielodniowa wycieczka turystyczna (piesza, rowerowa lub inna), polegająca na przebyciu określonej trasy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "organized_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczniowie pojechali na trzydniowy <rajd> w góry.",
                            "target_lang_translations": {
                                "ru": "Ученики отправились в трехдневный <поход> в горы."
                            }
                        },
                        {
                            "text": "Organizujemy <rajd> rowerowy dookoła jeziora dla wszystkich chętnych.",
                            "target_lang_translations": {
                                "ru": "Мы организуем <велопробег> вокруг озера для всех желающих."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyprawa",
                        "wycieczka",
                        "wędrówka"
                    ],
                    "common_phrases": [
                        "rajd pieszy",
                        "rajd rowerowy",
                        "rajd górski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованный многодневный туристический поход (пеший, велосипедный) по определенному маршруту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поход",
                                "target_lang_sense_clarification": "пеший туризм"
                            },
                            {
                                "target_lang_word": "пробег",
                                "target_lang_sense_clarification": "велосипедный или автомобильный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df64b8d2-2de4-35d1-b890-651d52ddc46b",
                    "sense_definition": "Szybki, indywidualny bieg zawodnika z piłką w kierunku bramki przeciwnika, mający na celu minięcie obrońców.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "offensive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napastnik przeprowadził wspaniały <rajd> przez połowę boiska.",
                            "target_lang_translations": {
                                "ru": "Нападающий совершил великолепный <проход> через половину поля."
                            }
                        },
                        {
                            "text": "Jego samotny <rajd> lewym skrzydłem zakończył się golem.",
                            "target_lang_translations": {
                                "ru": "Его одиночный <рейд> по левому флангу завершился голом."
                            }
                        }
                    ],
                    "synonyms": [
                        "szarża",
                        "akcja"
                    ],
                    "common_phrases": [
                        "samotny rajd",
                        "rajd z piłką"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "sport"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стремительный проход игрока с мячом к воротам соперника, с целью обойти защитников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проход",
                                "target_lang_sense_clarification": "спорт"
                            },
                            {
                                "target_lang_word": "рейд",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6715c8a6-5bdd-3c68-a921-ce9c34f055cb",
                    "sense_definition": "Nagły, szybki atak wojskowy na terytorium wroga, obliczony na wyrządzenie szkód i szybki powrót, a nie na zajęcie terenu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "offensive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oddział specjalny wykonał nocny <rajd> na tyły wroga.",
                            "target_lang_translations": {
                                "ru": "Спецотряд совершил ночной <рейд> в тыл врага."
                            }
                        },
                        {
                            "text": "Generał zaplanował <rajd> kawalerii, aby zniszczyć zapasy przeciwnika.",
                            "target_lang_translations": {
                                "ru": "Генерал спланировал кавалерийский <рейд>, чтобы уничтожить припасы противника."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypad",
                        "napaść",
                        "atak"
                    ],
                    "common_phrases": [
                        "rajd lotniczy",
                        "rajd kawaleryjski"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wojskowość"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное военное нападение на территорию врага с целью нанесения урона и быстрого отхода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рейд",
                                "target_lang_sense_clarification": "военная операция"
                            },
                            {
                                "target_lang_word": "набег",
                                "target_lang_sense_clarification": "внезапное нападение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "343cf236-880c-3df8-b289-ba321b48de0d",
                    "sense_definition": "Impreza harcerska polegająca na przebyciu trasy w terenie i wykonywaniu zadań sprawdzających umiejętności.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "organized_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza drużyna wygrała <rajd> dzięki świetnej orientacji w lesie.",
                            "target_lang_translations": {
                                "ru": "Наша команда выиграла <рейд> благодаря отличному ориентированию в лесу."
                            }
                        },
                        {
                            "text": "Podczas <rajdu> harcerze musieli zbudować szałas.",
                            "target_lang_translations": {
                                "ru": "Во время <похода> скауты должны были построить шалаш."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rajd harcerski"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "harcerstwo"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скаутское мероприятие, состоящее в прохождении маршрута на местности и выполнении различных заданий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поход",
                                "target_lang_sense_clarification": "туристический"
                            },
                            {
                                "target_lang_word": "рейд",
                                "target_lang_sense_clarification": "соревнование"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
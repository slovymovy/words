            "senses": [
                {
                    "sense_id": "cb4658d4-d263-3c10-a3e0-abc87bdc884b",
                    "sense_definition": "Of a pale yellow to brownish-yellow color.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "color_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has beautiful <w>honey</w> hair that shines in the sun.",
                            "target_lang_translations": {
                                "ru": "У неё красивые <w>медовые</w> волосы, которые сияют на солнце."
                            }
                        },
                        {
                            "text": "The evening light cast a <w>honey</w> glow over the landscape.",
                            "target_lang_translations": {
                                "ru": "Вечерний свет отбрасывал <w>медовый</w> отблеск на пейзаж."
                            }
                        }
                    ],
                    "common_phrases": [
                        "honey blonde"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бледно-желтого или коричневато-желтого цвета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медовый",
                                "target_lang_sense_clarification": "о цвете"
                            },
                            {
                                "target_lang_word": "золотисто-желтый",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8bc6344-1cfd-3052-9257-057cccaccb8c",
                    "sense_definition": "Sweet or delightful, often referring to a voice or smile.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sweet_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to persuade her with his <w>honeyest</w> voice.",
                            "target_lang_translations": {
                                "ru": "Он пытался убедить её своим самым <w>медовым</w> голосом."
                            }
                        },
                        {
                            "text": "She gave him a <w>honey</w> smile before asking for a favor.",
                            "target_lang_translations": {
                                "ru": "Она одарила его <w>сладкой</w> улыбкой, прежде чем попросить об одолжении."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкий или восхитительный, часто относится к голосу или улыбке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медовый",
                                "target_lang_sense_clarification": "о голосе или улыбке"
                            },
                            {
                                "target_lang_word": "сладкий",
                                "target_lang_sense_clarification": "льстивый или приятный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f7406d31-651b-3951-bb5c-5fea6a732122",
                    "sense_definition": "To sweeten or make something agreeable.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_sweeten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chef <w>honeyed</w> the glaze to balance the tartness of the fruit.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар <w>подсластил мёдом</w> глазурь, чтобы сбалансировать кислоту фруктов."
                            }
                        },
                        {
                            "text": "He <w>honeyed</w> his words to avoid offending the guests.",
                            "target_lang_translations": {
                                "ru": "Он <w>подсластил</w> свои слова, чтобы не обидеть гостей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подсластить или сделать что-то приятным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подсластить",
                                "target_lang_sense_clarification": "сделать сладким"
                            },
                            {
                                "target_lang_word": "сдобрить мёдом",
                                "target_lang_sense_clarification": "добавить мёд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e167cb97-df4a-3601-83f3-81a658997d27",
                    "sense_definition": "To talk fondly or coaxingly; to use endearments.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_talk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They were <w>honeying</w> and whispering in the corner of the room.",
                            "target_lang_translations": {
                                "ru": "Они <w>ворковали</w> и шептались в углу комнаты."
                            }
                        },
                        {
                            "text": "She spent the evening <w>honeying</w> to get what she wanted.",
                            "target_lang_translations": {
                                "ru": "Она провела вечер, <w>лебезя</w>, чтобы получить желаемое."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить нежно или льстиво; использовать ласковые слова."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ворковать",
                                "target_lang_sense_clarification": "говорить нежно"
                            },
                            {
                                "target_lang_word": "лебезить",
                                "target_lang_sense_clarification": "говорить льстиво"
                            },
                            {
                                "target_lang_word": "умасливать",
                                "target_lang_sense_clarification": "добиваться своего лестью"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [

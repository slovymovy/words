                            {
                                "target_lang_word": "купольный",
                                "target_lang_sense_clarification": "относящийся к архитектуре"
                            },
                            {
                                "target_lang_word": "с куполом",
                                "target_lang_sense_clarification": "описывающий наличие купола"
                            }

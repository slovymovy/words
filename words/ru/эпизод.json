    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f04aef7f-4cfe-3226-b730-f503027e1889",
                    "sense_definition": "Отдельное событие, случай или происшествие, являющееся частью жизни или ряда событий.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В книге описан забавный <эпизод> из жизни известного писателя.",
                            "target_lang_translations": {
                                "en": "The book describes a funny <episode> from the life of a famous writer."
                            }
                        },
                        {
                            "text": "Этот неприятный <эпизод> надолго испортил наши отношения с соседями.",
                            "target_lang_translations": {
                                "en": "This unpleasant <incident> ruined our relationship with the neighbors for a long time."
                            }
                        }
                    ],
                    "synonyms": [
                        "случай",
                        "событие",
                        "происшествие"
                    ],
                    "common_phrases": [
                        "эпизод из жизни",
                        "неприятный эпизод",
                        "отдельный эпизод"
                    ],
                    "target_lang_definitions": {
                        "en": "A separate event, case, or incident that is part of life or a series of events."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "episode",
                                "target_lang_sense_clarification": "event in life"
                            },
                            {
                                "target_lang_word": "incident",
                                "target_lang_sense_clarification": "occurrence, often negative"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04b25dc6-1e41-30af-9506-f20751eb8928",
                    "sense_definition": "Отдельная часть сериала, многосерийного фильма или программы.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "series_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы с нетерпением ждали, когда выйдет новый <эпизод> нашего любимого сериала.",
                            "target_lang_translations": {
                                "en": "We were eagerly waiting for the new <episode> of our favorite series to be released."
                            }
                        },
                        {
                            "text": "Первый <эпизод> подкаста оказался очень интересным и познавательным.",
                            "target_lang_translations": {
                                "en": "The first <episode> of the podcast turned out to be very interesting and informative."
                            }
                        }
                    ],
                    "synonyms": [
                        "случай",
                        "событие",
                        "происшествие"
                    ],
                    "common_phrases": [
                        "новый эпизод",
                        "финальный эпизод",
                        "следующий эпизод"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used for TV series or podcasts, synonymous with 'серия'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A separate part of a TV series, multi-part film, or program."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "episode",
                                "target_lang_sense_clarification": "part of a series"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9de489c2-2443-3ebf-b4d3-88c6cb649fa3",
                    "sense_definition": "Часть литературного или художественного произведения, обладающая относительной самостоятельностью и законченностью.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "art_fragment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Режиссёр решил вырезать этот жестокий <эпизод> из финальной версии фильма.",
                            "target_lang_translations": {
                                "en": "The director decided to cut this cruel <scene> from the final version of the movie."
                            }
                        },
                        {
                            "text": "Центральный <эпизод> романа раскрывает истинные мотивы главного героя.",
                            "target_lang_translations": {
                                "en": "The central <episode> of the novel reveals the true motives of the main character."
                            }
                        }
                    ],
                    "synonyms": [
                        "случай",
                        "событие",
                        "происшествие"
                    ],
                    "common_phrases": [
                        "эпизод фильма",
                        "эпизод романа",
                        "яркий эпизод"
                    ],
                    "target_lang_definitions": {
                        "en": "A part of a literary or artistic work that is relatively independent and complete."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scene",
                                "target_lang_sense_clarification": "part of a film or play"
                            },
                            {
                                "target_lang_word": "episode",
                                "target_lang_sense_clarification": "part of a narrative"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
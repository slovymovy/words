            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "ba3ccfe6-1491-3404-9bd5-28017f58f0b4",
                    "sense_definition": "Указывает на нахождение в непосредственной близости от чего-либо или кого-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У нас есть небольшой сад <w>при</w> доме."
                        },
                        {
                            "text": "Она ждала его <w>при</w> входе в парк."
                        }
                    ],
                    "synonyms": [
                        "у",
                        "возле",
                        "около",
                        "рядом c"
                    ],
                    "common_phrases": [
                        "при дороге",
                        "при входе"
                    ]
                },
                {
                    "sense_id": "f34db1a0-eafd-346c-b8e6-0f37b0beeca3",
                    "sense_definition": "Указывает на время совершения действия или период, связанный с каким-либо событием или жизнью определенного лица.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>При</w> встрече мы обсудим все детали."
                        },
                        {
                            "text": "Этот красивый дом был построен ещё <w>при</w> царе."
                        }
                    ],
                    "synonyms": [
                        "во время",
                        "во времена"
                    ],
                    "common_phrases": [
                        "при встрече",
                        "при жизни",
                        "при этом"
                    ]
                },
                {
                    "sense_id": "261b5019-58e4-3a14-9104-f56a733efbef",
                    "sense_definition": "Указывает на обстоятельство или условие, при котором происходит действие.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>При</w> желании можно добиться больших успехов в учебе."
                        },
                        {
                            "text": "Звоните в скорую помощь <w>при</w> высокой температуре."
                        }
                    ],
                    "antonyms": [
                        "без"
                    ],
                    "common_phrases": [
                        "при условии",
                        "при необходимости",
                        "при помощи",
                        "при желании"
                    ]
                },
                {
                    "sense_id": "4bdbb723-7dd9-30b7-ab86-18ee4779f9d7",
                    "sense_definition": "Указывает на лицо, в присутствии которого происходит действие.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "presence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, не ругайтесь и не спорьте <w>при</w> детях."
                        },
                        {
                            "text": "Он стеснялся выступать <w>при</w> большом количестве незнакомых людей."
                        }
                    ],
                    "common_phrases": [
                        "при свидетелях",
                        "при мне",
                        "при всех"
                    ]
                },
                {
                    "sense_id": "4a4bdfad-1017-3b42-97f7-f9349223b722",
                    "sense_definition": "Указывает на наличие чего-либо у лица в данный момент (обычно в одежде или в руках).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для входа в здание у вас должен быть <w>при</w> себе паспорт."
                        },
                        {
                            "text": "К сожалению, <w>при</w> мне сейчас нет наличных денег."
                        }
                    ],
                    "common_phrases": [
                        "при себе",
                        "иметь при себе"
                    ]
                },
                {
                    "sense_id": "116fecb8-4058-3340-ad7f-8ae1b653e5c3",
                    "sense_definition": "Указывает на учреждение или организацию, в составе которой находится или функционирует данный объект.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "affiliation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач принимает пациентов в поликлинике <w>при</w> университете."
                        },
                        {
                            "text": "Столовая <w>при</w> заводе работает круглосуточно для всех сотрудников."
                        }
                    ]
                }
            ]
        },

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0b581a68-f370-3516-91be-141f23de77e1",
                    "sense_definition": "Proces zmniejszania się wartości, liczby, natężenia lub poziomu czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ostatnich miesiącach obserwujemy wyraźny <w>spadek</w> cen paliw na stacjach.",
                            "target_lang_translations": {
                                "ru": "В последние месяцы мы наблюдаем заметное <w>снижение</w> цен на топливо на заправках."
                            }
                        },
                        {
                            "text": "Nagły <w>spadek</w> temperatury spowodował gołoledź na drogach.",
                            "target_lang_translations": {
                                "ru": "Внезапное <w>падение</w> температуры привело к гололеду на дорогах."
                            }
                        }
                    ],
                    "antonyms": [
                        "wzrost"
                    ],
                    "common_phrases": [
                        "spadek cen",
                        "spadek wartości",
                        "gwałtowny spadek",
                        "spadek formy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс уменьшения стоимости, количества, интенсивности или уровня чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спад",
                                "target_lang_sense_clarification": "уменьшение, ослабление процесса"
                            },
                            {
                                "target_lang_word": "падение",
                                "target_lang_sense_clarification": "резкое снижение"
                            },
                            {
                                "target_lang_word": "снижение",
                                "target_lang_sense_clarification": "постепенное уменьшение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a14d63cd-4c21-3402-a118-b307c4fd9589",
                    "sense_definition": "Majątek, prawa i obowiązki przechodzące na inną osobę po śmierci właściciela.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <w>spadku</w> po dziadku otrzymał dużą kolekcję znaczków.",
                            "target_lang_translations": {
                                "ru": "В <w>наследство</w> от дедушки он получил большую коллекцию марок."
                            }
                        },
                        {
                            "text": "Rodzina spotkała się u notariusza, aby formalnie przyjąć <w>spadek</w>.",
                            "target_lang_translations": {
                                "ru": "Семья встретилась у нотариуса, чтобы официально принять <w>наследство</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "otrzymać spadek",
                        "odrzucić spadek",
                        "nabycie spadku",
                        "prawo do spadku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имущество, права и обязанности, переходящие к другому лицу после смерти владельца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наследство",
                                "target_lang_sense_clarification": "имущество от умершего"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57527050-9088-34eb-b44f-9c1191c7ecea",
                    "sense_definition": "Nachylenie powierzchni terenu lub konstrukcji w dół.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rowerzysta nabrał dużej prędkości na stromym <w>spadku</w> drogi.",
                            "target_lang_translations": {
                                "ru": "Велосипедист набрал большую скорость на крутом <w>спуске</w> дороги."
                            }
                        },
                        {
                            "text": "Dach musi mieć odpowiedni <w>spadek</w>, żeby woda deszczowa mogła swobodnie spływać.",
                            "target_lang_translations": {
                                "ru": "Крыша должна иметь соответствующий <w>уклон</w>, чтобы дождевая вода могла свободно стекать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spadek terenu",
                        "stromy spadek",
                        "spadek dachu",
                        "lekki spadek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наклон поверхности местности или конструкции вниз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спуск",
                                "target_lang_sense_clarification": "наклонная поверхность для движения вниз"
                            },
                            {
                                "target_lang_word": "уклон",
                                "target_lang_sense_clarification": "крутизна ската или поверхности"
                            },
                            {
                                "target_lang_word": "скат",
                                "target_lang_sense_clarification": "наклонная поверхность крыши"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ef08164-5ebf-3d4b-b1e4-779a869f41bf",
                    "sense_definition": "Przesunięcie do niższej klasy rozgrywkowej w sporcie lub utrata pozycji w hierarchii.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po serii przegranych meczów drużynie grozi <w>spadek</w> do drugiej ligi.",
                            "target_lang_translations": {
                                "ru": "После серии проигранных матчей команде грозит <w>вылет</w> во вторую лигу."
                            }
                        },
                        {
                            "text": "Dla ambitnego zawodnika <w>spadek</w> w rankingu był bolesnym doświadczeniem.",
                            "target_lang_translations": {
                                "ru": "Для амбициозного игрока <w>падение</w> в рейтинге стало болезненным опытом."
                            }
                        }
                    ],
                    "synonyms": [
                        "degradacja",
                        "deklasacja"
                    ],
                    "antonyms": [
                        "awans"
                    ],
                    "common_phrases": [
                        "spadek z ligi",
                        "spadek do niższej klasy",
                        "zagrożenie spadkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переход в низшую лигу в спорте или потеря позиции в иерархии."

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "890fe1f1-8560-332c-8ef6-c8aec2576514",
                    "sense_definition": "A person who sets up a business or businesses, taking on financial risks in the hope of profit.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ambitious <w>entrepreneur</w> pitched her innovative idea to a group of wealthy investors.",
                            "target_lang_translations": {
                                "ru": "Амбициозная <w>предпринимательница</w> представила свою инновационную идею группе богатых инвесторов."
                            }
                        },
                        {
                            "text": "He became a successful <w>entrepreneur</w> after launching a popular online clothing store.",
                            "target_lang_translations": {
                                "ru": "Он стал успешным <w>предпринимателем</w> после запуска популярного интернет-магазина одежды."
                            }
                        }
                    ],
                    "synonyms": [
                        "businessperson",
                        "founder",
                        "industrialist"
                    ],
                    "antonyms": [
                        "employee"
                    ],
                    "common_phrases": [
                        "successful entrepreneur",
                        "social entrepreneur",
                        "tech entrepreneur",
                        "budding entrepreneur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который создает бизнес или предприятия, принимая на себя финансовые риски в надежде на получение прибыли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предприниматель",
                                "target_lang_sense_clarification": "владелец бизнеса, организатор дела"
                            },
                            {
                                "target_lang_word": "бизнесмен",
                                "target_lang_sense_clarification": "деловой человек, коммерсант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef59ada0-a00b-3b80-b8bb-6b30b95ef385",
                    "sense_definition": "A person who organizes and manages public entertainments, especially musical or theatrical performances.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The famous <w>entrepreneur</w> arranged for the opera singer to perform at the grand opening.",
                            "target_lang_translations": {
                                "ru": "Известный <w>антрепренёр</w> договорился о выступлении оперного певца на торжественном открытии."
                            }
                        },
                        {
                            "text": "As a theatrical <w>entrepreneur</w>, he was responsible for booking acts and managing the venue.",
                            "target_lang_translations": {
                                "ru": "Как театральный <w>антрепренёр</w>, он отвечал за наем артистов и управление площадкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "impresario",
                        "promoter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, организующий и управляющий публичными развлечениями, особенно музыкальными или театральными представлениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "антрепренёр",
                                "target_lang_sense_clarification": "частный театральный предприниматель"
                            },
                            {
                                "target_lang_word": "импресарио",

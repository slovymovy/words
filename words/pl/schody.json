    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c67baad4-ea69-339f-8d98-5d10b9680f89",
                    "sense_definition": "Konstrukcja złożona ze stopni, służąca do przemieszczania się między różnymi poziomami lub piętrami budynku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_stairs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy wejść na trzecie piętro po <w>schodach</w>, ponieważ winda jest zepsuta.",
                            "target_lang_translations": {
                                "ru": "Нам нужно подняться на третий этаж по <w>лестнице</w>, так как лифт сломан."
                            }
                        },
                        {
                            "text": "Te drewniane <w>schody</w> są bardzo strome i niebezpieczne dla dzieci.",
                            "target_lang_translations": {
                                "ru": "Эта деревянная <w>лестница</w> очень крутая и опасная для детей."
                            }
                        }
                    ],
                    "synonyms": [
                        "stopnie"
                    ],
                    "antonyms": [
                        "winda"
                    ],
                    "common_phrases": [
                        "ruchome schody",
                        "kręte schody",
                        "wejść po schodach",
                        "spaść ze schodów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конструкция, состоящая из ступеней, служащая для перемещения между различными уровнями или этажами здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лестница",
                                "target_lang_sense_clarification": "сооружение для подъема и спуска"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "150badce-4255-32cf-b212-a503fddaa8ee",
                    "sense_definition": "Forma liczby mnogiej rzeczownika <w>schód</w>, oznaczająca poszczególne stopnie.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "group_stairs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naprawił dwa pęknięte <w>schody</w> prowadzące do wejścia.",
                            "target_lang_translations": {
                                "ru": "Он починил две треснувшие <w>ступеньки</w>, ведущие ко входу."
                            }
                        },
                        {
                            "text": "Policzył wszystkie <w>schody</w>, wchodząc na wysoką wieżę.",
                            "target_lang_translations": {
                                "ru": "Он пересчитал все <w>ступени</w>, поднимаясь на высокую башню."
                            }
                        }
                    ],
                    "synonyms": [
                        "stopnie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "liczba mnoga"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма множественного числа, обозначающая отдельные ступени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ступени",
                                "target_lang_sense_clarification": "отдельные элементы лестницы"
                            },
                            {
                                "target_lang_word": "ступеньки",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50999675-5722-3b4f-9fac-8127c32410b3",
                    "sense_definition": "Układ elementów lub kształt przypominający stopnie.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej włosy były modnie ścięte w <w>schody</w>.",
                            "target_lang_translations": {
                                "ru": "Её волосы были модно подстрижены <w>лесенкой</w>."
                            }
                        },
                        {
                            "text": "Książki na półce były ułożone w <w>schody</w>, od największej do najmniejszej.",
                            "target_lang_translations": {
                                "ru": "Книги на полке были сложены <w>лесенкой</w>, от самой большой к самой маленькой."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположение элементов или форма, напоминающая ступени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лесенка",
                                "target_lang_sense_clarification": "вид стрижки или расположение предметов"
                            },
                            {
                                "target_lang_word": "уступы",
                                "target_lang_sense_clarification": "ступенчатая форма"
                            }
                        ]
                    }
                }
            ]
        }

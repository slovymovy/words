{
    "word_family": [
        "geheel",
        "heel"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "932a673a-a85d-3bd2-b42f-6dd6d68fa144",
                    "sense_definition": "Volledig; in alle opzichten, zonder dat er iets ontbreekt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben het <w>helemaal</w> met je eens over dat plan.",
                            "target_lang_translations": {
                                "ru": "Я <w>полностью</w> согласен с тобой насчет этого плана."
                            }
                        },
                        {
                            "text": "Het is <w>helemaal</w> niet erg als je wat later komt.",
                            "target_lang_translations": {
                                "ru": "Это <w>совсем</w> не страшно, если ты придешь немного позже."
                            }
                        },
                        {
                            "text": "Ze heeft de taart <w>helemaal</w> zelf gebakken voor haar verjaardag.",
                            "target_lang_translations": {
                                "ru": "Она испекла торт <w>полностью</w> сама на свой день рождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "volledig",
                        "totaal",
                        "compleet",
                        "volkomen",
                        "volstrekt"
                    ],
                    "common_phrases": [
                        "helemaal niet",
                        "helemaal niets",
                        "helemaal alleen",
                        "helemaal goed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью; во всех отношениях, без каких-либо недостающих частей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полностью",
                                "target_lang_sense_clarification": "целиком, без остатка"
                            },
                            {
                                "target_lang_word": "совсем",
                                "target_lang_sense_clarification": "совершенно (часто используется с отрицанием)"
                            },
                            {
                                "target_lang_word": "совершенно",
                                "target_lang_sense_clarification": "абсолютно"
                            },
                            {
                                "target_lang_word": "абсолютно",
                                "target_lang_sense_clarification": "безусловно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03f7cc2d-7a11-3f7e-895d-cb71d30153b6",
                    "sense_definition": "De hele afstand afleggend tot aan een bepaald eindpunt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zijn <w>helemaal</w> naar het zuiden van Frankrijk gereden.",
                            "target_lang_translations": {
                                "ru": "Мы ехали <w>до самого</w> юга Франции."
                            }
                        },
                        {
                            "text": "Ik ben <w>helemaal</w> naar huis gelopen omdat de bus niet kwam.",
                            "target_lang_translations": {
                                "ru": "Я шел <w>до самого</w> дома пешком, потому что автобус не пришел."
                            }
                        }
                    ],
                    "common_phrases": [
                        "helemaal naar",
                        "helemaal tot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Преодолевая всё расстояние до определенного конечного пункта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до самого",
                                "target_lang_sense_clarification": "указание на достижение отдаленного предела"
                            },
                            {
                                "target_lang_word": "аж",
                                "target_lang_sense_clarification": "усилительная частица, подчеркивающая дальность (разг.)"
                            },
                            {
                                "target_lang_word": "весь путь",
                                "target_lang_sense_clarification": "вся дистанция целиком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8753956b-a391-3664-ab43-a03613c77306",
                    "sense_definition": "In het bijzonder; in nog sterkere mate (vaak gebruikt om iets te benadrukken).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik hou niet van wachten, en al <w>helemaal</w> niet in de regen.",
                            "target_lang_translations": {
                                "ru": "Я не люблю ждать, и <w>тем более</w> под дождем."
                            }
                        },
                        {
                            "text": "Kauwgom is slecht voor je tanden, al <w>helemaal</w> als er suiker in zit.",
                            "target_lang_translations": {
                                "ru": "Жвачка вредна для зубов, <w>особенно</w> если в ней есть сахар."
                            }
                        }
                    ],
                    "common_phrases": [
                        "al helemaal"
                    ],
                    "target_lang_definitions": {
                        "ru": "В особенности; в еще большей степени (используется для усиления)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тем более",
                                "target_lang_sense_clarification": "усиление утверждения или отрицания"
                            },
                            {
                                "target_lang_word": "особенно",
                                "target_lang_sense_clarification": "преимущественно"
                            },
                            {
                                "target_lang_word": "подавно",
                                "target_lang_sense_clarification": "тем паче (обычно при отрицании)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e7260a2-6a9b-3288-a42f-7a7be19a525a",
                    "sense_definition": "Eigenlijk (gebruikt om iets te relativeren of in retorische vragen).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rhetorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wie ben jij nu <w>helemaal</w> om mij de les te lezen?",
                            "target_lang_translations": {
                                "ru": "Кто ты <w>вообще</w> такой, чтобы читать мне нотации?"
                            }
                        },
                        {
                            "text": "Wat stelt die hele ruzie nou eigenlijk <w>helemaal</w> voor?",
                            "target_lang_translations": {
                                "ru": "Что эта ссора <w>вообще</w> собой представляет?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Собственно говоря (используется для релятивизации или в риторических вопросах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вообще",
                                "target_lang_sense_clarification": "в риторических вопросах или для обобщения"
                            },
                            {
                                "target_lang_word": "собственно",
                                "target_lang_sense_clarification": "по сути дела"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c29ac95b-f2a4-3abc-ad9b-bac9262076d8",
                    "sense_definition": "С чувством волнения, беспокойства или страха.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мать <w>тревожно</w> смотрела в окно, ожидая возвращения сына.",
                            "target_lang_translations": {
                                "en": "The mother looked <w>anxiously</w> out the window, waiting for her son's return."
                            }
                        },
                        {
                            "text": "Птицы <w>тревожно</w> кричали, чувствуя приближение грозы.",
                            "target_lang_translations": {
                                "en": "The birds cried out <w>uneasily</w>, sensing the approaching thunderstorm."
                            }
                        }
                    ],
                    "synonyms": [
                        "беспокойно",
                        "взволнованно"
                    ],
                    "antonyms": [
                        "спокойно",
                        "безмятежно"
                    ],
                    "common_phrases": [
                        "тревожно смотреть",
                        "тревожно биться",
                        "тревожно звучать"
                    ],
                    "target_lang_definitions": {
                        "en": "With a feeling of agitation, anxiety, or fear."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "anxiously",
                                "target_lang_sense_clarification": "in a worried or nervous manner"
                            },
                            {
                                "target_lang_word": "uneasily",
                                "target_lang_sense_clarification": "with a sense of discomfort or apprehension"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d72779ee-8eff-3e53-a4e8-4d325c2b5093",
                    "sense_definition": "О психологическом состоянии беспокойства или страха, которое испытывает человек (используется как сказуемое).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне стало <w>тревожно</w> на душе после этого странного разговора.",
                            "target_lang_translations": {
                                "en": "I began to feel <w>anxious</w> at heart after that strange conversation."
                            }
                        },
                        {
                            "text": "В пустом доме было тихо и <w>тревожно</w>.",
                            "target_lang_translations": {
                                "en": "It was quiet and <w>unsettling</w> in the empty house."
                            }
                        }
                    ],
                    "synonyms": [
                        "беспокойно",
                        "неспокойно"
                    ],
                    "antonyms": [
                        "спокойно"
                    ],
                    "common_phrases": [
                        "мне тревожно",
                        "на душе тревожно",
                        "стало тревожно"
                    ],
                    "target_lang_definitions": {
                        "en": "Describing a psychological state of anxiety or fear experienced by a person (used as a predicate)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "anxious",
                                "target_lang_sense_clarification": "feeling worried or nervous (e.g., I feel anxious)"
                            },
                            {
                                "target_lang_word": "uneasy",
                                "target_lang_sense_clarification": "feeling uncomfortable or disturbed"
                            },
                            {
                                "target_lang_word": "unsettling",
                                "target_lang_sense_clarification": "causing a feeling of anxiety (referring to an atmosphere)"
                            }
                        ]
                    }
                }
            ]
        },
        {

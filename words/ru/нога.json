            "senses": [
                {
                    "sense_id": "daf07548-4098-34ad-ac11-ad83dca3c374",
                    "sense_definition": "Одна из двух нижних конечностей человека, которая служит для опоры и передвижения.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "human_anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгой пробежки у спортсмена заболела левая <w>нога</w>.",
                            "target_lang_translations": {
                                "en": "After a long run, the athlete's left <w>leg</w> began to hurt."
                            }
                        },
                        {
                            "text": "Мальчик упал с велосипеда и сильно ушиб <w>ногу</w>.",
                            "target_lang_translations": {
                                "en": "The boy fell off his bike and badly bruised his <w>leg</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "рука"
                    ],
                    "common_phrases": [
                        "правая нога",
                        "левая нога",
                        "сломать ногу",
                        "стоять на ногах"
                    ],
                    "target_lang_definitions": {
                        "en": "One of the two lower limbs of a human, used for support and movement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leg",
                                "target_lang_sense_clarification": "human limb"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0e277ab-4de0-375b-9342-a7a793c7b286",
                    "sense_definition": "Ступня, самая нижняя часть конечности человека, на которую надевают обувь.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "human_anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эти новые ботинки мне жмут, поэтому <w>ногам</w> в них очень тесно.",
                            "target_lang_translations": {
                                "en": "These new boots pinch me, so my <w>feet</w> feel very tight in them."
                            }
                        },
                        {
                            "text": "В переполненном автобусе кто-то случайно наступил мне на <w>ногу</w>.",
                            "target_lang_translations": {
                                "en": "In the crowded bus, someone accidentally stepped on my <w>foot</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "наступить на ногу",
                        "размер ноги",
                        "обувь по ноге"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used instead of 'ступня'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The foot, the lowest part of a human limb, on which shoes are worn."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "foot",
                                "target_lang_sense_clarification": "body part below the ankle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f76426af-90e2-3a45-8deb-117a55c8c616",
                    "sense_definition": "Конечность животного или птицы.",

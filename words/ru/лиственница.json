    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e0fa5f72-4973-3f81-a350-a9eea9b4f0eb",
                    "sense_definition": "Крупное хвойное дерево семейства сосновых с мягкой хвоей, которая опадает на зиму.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tree_species",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем парке растёт высокая и старая <w>лиственница</w>.",
                            "target_lang_translations": {
                                "en": "A tall and old <w>larch</w> grows in our park."
                            }
                        },
                        {
                            "text": "Осенью <w>лиственница</w> желтеет и сбрасывает свои иголки, в отличие от ели.",
                            "target_lang_translations": {
                                "en": "In autumn, the <w>larch</w> turns yellow and sheds its needles, unlike the spruce."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сибирская лиственница",
                        "хвоя лиственницы",
                        "посадить лиственницу"
                    ],
                    "target_lang_definitions": {
                        "en": "A large coniferous tree of the pine family with soft needles that fall off in winter."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "larch",
                                "target_lang_sense_clarification": "tree"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1640047c-e391-3378-ace4-393b9e4e1440",
                    "sense_definition": "Древесина этого дерева, используемая как прочный строительный материал.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пол в деревенском доме был сделан из прочной <w>лиственницы</w>.",
                            "target_lang_translations": {
                                "en": "The floor in the village house was made of durable <w>larch</w>."
                            }
                        },
                        {
                            "text": "<w>Лиственница</w> очень ценится в строительстве, так как она не гниёт в воде.",
                            "target_lang_translations": {
                                "en": "<w>Larch</w> is highly valued in construction because it does not rot in water."
                            }
                        }
                    ],
                    "common_phrases": [
                        "дом из лиственницы",
                        "доска из лиственницы",
                        "сруб из лиственницы"
                    ],
                    "target_lang_definitions": {
                        "en": "The wood of this tree, used as a durable building material."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "larch",
                                "target_lang_sense_clarification": "wood/timber"
                            }
                        ]
                    }
                }

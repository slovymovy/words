    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4cf96ad7-07ee-3cbc-ab56-56681bc24841",
                    "sense_definition": "Uczucie silnej tęsknoty za przeszłością, za tym, co minęło lub zostało utracone.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oglądanie starych, czarno-białych fotografii zawsze budzi we mnie głęboką <w>nostalgię</w>.",
                            "target_lang_translations": {
                                "ru": "Просмотр старых черно-белых фотографий всегда пробуждает во мне глубокую <w>ностальгию</w>."
                            }
                        },
                        {
                            "text": "Z wielką <w>nostalgią</w> wracam myślami do beztroskich lat mojego dzieciństwa.",
                            "target_lang_translations": {
                                "ru": "С большой <w>ностальгией</w> я возвращаюсь мыслями к беззаботным годам моего детства."
                            }
                        }
                    ],
                    "synonyms": [
                        "tęsknota",
                        "sentyment",
                        "rzewność"
                    ],
                    "antonyms": [
                        "obojętność"
                    ],
                    "common_phrases": [
                        "czuć nostalgię",
                        "fala nostalgii",
                        "wspominać z nostalgią",
                        "nuta nostalgii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство сильной тоски по прошлому, по тому, что ушло или было утрачено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ностальгия",
                                "target_lang_sense_clarification": "тоска по прошлому"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "938c7b22-ed60-31b8-9353-c881e9b3809e",
                    "sense_definition": "Bolesna tęsknota za krajem ojczystym odczuwana przez osoby przebywające na obczyźnie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu polskich poetów na emigracji przelewało swoją <w>nostalgię</w> na papier.",
                            "target_lang_translations": {
                                "ru": "Многие польские поэты в эмиграции изливали свою <w>ностальгию</w> на бумагу."
                            }
                        },
                        {
                            "text": "Po wielu latach życia za granicą ogarnęła go silna <w>nostalgia</w> i chęć powrotu.",
                            "target_lang_translations": {
                                "ru": "После многих лет жизни за границей его охватила сильная <w>ностальгия</w> и желание вернуться."
                            }
                        }
                    ],
                    "synonyms": [
                        "tęsknota za krajem"
                    ],
                    "common_phrases": [
                        "cierpieć na nostalgię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Болезненная тоска по родине, которую испытывают люди, находящиеся вдали от дома."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ностальгия",
                                "target_lang_sense_clarification": "тоска по родине"
                            },
                            {
                                "target_lang_word": "тоска по родине",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "735ff7f3-daf4-3675-8376-cff7534e20f1",
                    "sense_definition": "The quality of behaving in a sensible, adult way, or the state of being fully developed.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "development_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Taking responsibility for your own mistakes is a clear sign of <w>maturity</w>.",
                            "target_lang_translations": {
                                "ru": "Принятие ответственности за свои ошибки — явный признак <w>зре́лости</w>."
                            }
                        },
                        {
                            "text": "She handled the difficult situation with a level of <w>maturity</w> beyond her years.",
                            "target_lang_translations": {
                                "ru": "Она справилась с трудной ситуацией с уровнем <w>зре́лости</w> не по годам."
                            }
                        }
                    ],
                    "synonyms": [
                        "matureness"
                    ],
                    "antonyms": [
                        "immaturity",
                        "naïveté"
                    ],
                    "common_phrases": [
                        "sign of maturity",
                        "emotional maturity",
                        "lack of maturity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество разумного, взрослого поведения или состояние полного развития."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зре́лость",
                                "target_lang_sense_clarification": "психологическая или эмоциональная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cf3144f-3f31-398a-b725-6f9cf1263d1f",
                    "sense_definition": "The stage of development when an organism is fully grown or capable of reproduction.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "development_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most animals reach sexual <w>maturity</w> within the first few years of life.",
                            "target_lang_translations": {
                                "ru": "Большинство животных достигают половой <w>зре́лости</w> в первые несколько лет жизни."
                            }
                        },
                        {
                            "text": "The trees in this forest will take decades to reach their full <w>maturity</w>.",
                            "target_lang_translations": {
                                "ru": "Деревьям в этом лесу потребуются десятилетия, чтобы достичь полной <w>зре́лости</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "adulthood",
                        "ripeness"
                    ],
                    "antonyms": [
                        "immaturity"
                    ],
                    "common_phrases": [
                        "sexual maturity",
                        "reach maturity",
                        "full maturity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стадия развития, когда организм полностью сформирован или способен к размножению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зре́лость",
                                "target_lang_sense_clarification": "биологическая или половая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b6ed0af-d0c1-30f2-bd44-5b3d391fc463",
                    "sense_definition": "The date when a financial agreement ends and payment must be made.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The investor decided to hold the bond until <w>maturity</w> to ensure a fixed return.",
                            "target_lang_translations": {
                                "ru": "Инвестор решил держать облигацию до <w>срока погаше́ния</w>, чтобы гарантировать фиксированный доход."
                            }
                        },
                        {
                            "text": "The note was cashed at <w>maturity</w>.",
                            "target_lang_translations": {
                                "ru": "Вексель был обналичен при наступлении <w>срока платежа́</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "due date"
                    ],
                    "common_phrases": [
                        "at maturity",
                        "date of maturity",
                        "yield to maturity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дата окончания действия финансового соглашения, когда должна быть произведена выплата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срок погаше́ния",
                                "target_lang_sense_clarification": "облигаций, долга"
                            },
                            {
                                "target_lang_word": "срок платежа́",
                                "target_lang_sense_clarification": "дата выплаты"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
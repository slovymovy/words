{
    "word_family": [
        "message",
        "messaging"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc6e3e88-5a78-39c5-ad48-6b2c81253c36",
                    "sense_definition": "A person who carries and delivers messages, packages, or news.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "agent_of_delivery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king sent a <w>messenger</w> to deliver the urgent news to the general.",
                            "target_lang_translations": {
                                "ru": "Король отправил <w>гонца</w>, чтобы передать генералу срочные новости."
                            }
                        },
                        {
                            "text": "Please hand this important envelope to the <w>messenger</w> waiting in the lobby.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, передайте этот важный конверт <w>курьеру</w>, ожидающему в вестибюле."
                            }
                        }
                    ],
                    "synonyms": [
                        "courier",
                        "bearer",
                        "carrier",
                        "envoy"
                    ],
                    "common_phrases": [
                        "don't shoot the messenger",
                        "send a messenger",
                        "special messenger"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который доставляет сообщения, посылки или новости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курьер",
                                "target_lang_sense_clarification": "доставщик документов или грузов"
                            },
                            {
                                "target_lang_word": "посыльный",
                                "target_lang_sense_clarification": "человек, выполняющий поручения"
                            },
                            {
                                "target_lang_word": "гонец",
                                "target_lang_sense_clarification": "доставщик срочных известий (часто ист.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ce703f0-ccc4-3f39-ba6e-f72545c16bb5",
                    "sense_definition": "A computer program or mobile application used for instant text communication.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I use an instant <w>messenger</w> to chat with my friends who live abroad.",
                            "target_lang_translations": {
                                "ru": "Я использую <w>мессенджер</w> для общения с друзьями, живущими за границей."
                            }
                        },
                        {
                            "text": "You can send me the file directly through the <w>messenger</w> on your phone.",
                            "target_lang_translations": {
                                "ru": "Ты можешь отправить мне файл прямо через <w>мессенджер</w> на своем телефоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "chat app"
                    ],
                    "common_phrases": [
                        "instant messenger",
                        "online messenger"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа или мобильное приложение для мгновенного обмена сообщениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мессенджер",
                                "target_lang_sense_clarification": "программа для общения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e406d03f-b6e4-3d5f-a830-f5aba37bc2cd",
                    "sense_definition": "A person or thing that signals the approach of something else; a forerunner.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agent_of_delivery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The first blooming crocus is often seen as a <w>messenger</w> of spring.",
                            "target_lang_translations": {
                                "ru": "Первый распустившийся крокус часто считают <w>вестником</w> весны."
                            }
                        },
                        {
                            "text": "Dark clouds gathered on the horizon, acting as a <w>messenger</w> of the approaching storm.",
                            "target_lang_translations": {
                                "ru": "На горизонте собрались темные тучи, выступая <w>вестником</w> приближающейся бури."
                            }
                        }
                    ],
                    "synonyms": [
                        "harbinger",
                        "herald",
                        "precursor"
                    ],
                    "common_phrases": [
                        "messenger of doom",
                        "messenger of peace",
                        "messenger of the gods"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary or poetic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или явление, предвещающее приближение чего-либо; предвестник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вестник",
                                "target_lang_sense_clarification": "предвестник события"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cd087e36-500b-3247-a01e-9e1eb62dab65",
                    "sense_definition": "To send documents or items using a courier service.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>messenger</w> the signed contract to your office immediately.",
                            "target_lang_translations": {
                                "ru": "Я немедленно <w>отправлю курьером</w> подписанный договор в ваш офис."
                            }
                        },
                        {
                            "text": "The law firm <w>messengered</w> the files to the court this morning.",
                            "target_lang_translations": {
                                "ru": "Юридическая фирма сегодня утром <w>отправила курьером</w> документы в суд."
                            }
                        }
                    ],
                    "synonyms": [
                        "courier"
                    ],
                    "common_phrases": [
                        "messenger over"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in business contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправлять документы или предметы с помощью курьерской службы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправить курьером",
                                "target_lang_sense_clarification": "послать через службу доставки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f7fcd884-4524-38f4-8422-f96ef1d6887b",
                    "sense_definition": "A surname originating as an occupation for a messenger.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We invited Mr. <w>Messenger</w> to the neighborhood party.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили мистера <w>Мессенджера</w> на вечеринку соседей."
                            }
                        },
                        {
                            "text": "Dr. <w>Messenger</w> wrote a comprehensive book on historical linguistics.",
                            "target_lang_translations": {
                                "ru": "Доктор <w>Мессенджер</w> написал исчерпывающую книгу по исторической лингвистике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, происходящая от названия профессии посыльного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мессенджер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
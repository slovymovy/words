    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5559fbe4-906e-3de0-a422-8b7961e4c4f1",
                    "sense_definition": "Wyrażający wielką radość, szczęście i optymizm, zwykle widoczny w wyrazie twarzy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotions_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej <w>promienny</w> uśmiech sprawił, że wszyscy w biurze poczuli się lepiej.",
                            "target_lang_translations": {
                                "ru": "Её <w>лучезарная</w> улыбка заставила всех в офисе почувствовать себя лучше."
                            }
                        },
                        {
                            "text": "Mimo zmęczenia zachował <w>promienny</w> nastrój do samego końca spotkania.",
                            "target_lang_translations": {
                                "ru": "Несмотря на усталость, он сохранял <w>солнечное</w> настроение до самого конца встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpromieniony",
                        "wesoły",
                        "rozpromieniony",
                        "jasny"
                    ],
                    "antonyms": [
                        "smutny",
                        "ponury",
                        "posępny"
                    ],
                    "common_phrases": [
                        "promienny uśmiech",
                        "promienna twarz",
                        "promienny nastrój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражающий огромную радость, счастье и оптимизм, обычно заметный в выражении лица."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лучезарный",
                                "target_lang_sense_clarification": "о улыбке, взгляде или лице"
                            },
                            {
                                "target_lang_word": "сияющий",
                                "target_lang_sense_clarification": "светящийся от счастья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e48478bb-81ee-3210-86ee-0a22dd5636e6",
                    "sense_definition": "Pełen jasnego światła, blasku lub słońca; intensywnie świecący.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "light_brightness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obudził nas <w>promienny</w> poranek i głośny śpiew ptaków za oknem.",
                            "target_lang_translations": {
                                "ru": "Нас разбудило <w>солнечное</w> утро и громкое пение птиц за окном."
                            }
                        },
                        {
                            "text": "Słońce rzucało <w>promienny</w> blask na spokojną taflę jeziora.",
                            "target_lang_translations": {
                                "ru": "Солнце бросало <w>лучистый</w> отблеск на спокойную гладь озера."
                            }
                        }
                    ],
                    "synonyms": [
                        "jasny",
                        "lśniący",
                        "rozpromieniony",
                        "wesoły"
                    ],
                    "antonyms": [
                        "ciemny",
                        "mroczny",
                        "pochmurny"
                    ],
                    "common_phrases": [
                        "promienny dzień",
                        "promienny blask",
                        "promienny poranek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный яркого света, блеска или солнца; интенсивно светящийся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солнечный",
                                "target_lang_sense_clarification": "о погоде или дне"
                            },
                            {
                                "target_lang_word": "лучистый",
                                "target_lang_sense_clarification": "испускающий лучи, яркий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "332d2d8e-8ff1-3f8b-a44c-63a3d06adf03",
                    "sense_definition": "Związany z fizycznym zjawiskiem emisji energii w postaci fal lub cząstek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Energia <w>promienna</w> słońca jest niezbędna do podtrzymania życia na Ziemi.",
                            "target_lang_translations": {
                                "ru": "<w>Лучистая</w> энергия солнца необходима для поддержания жизни на Земле."
                            }
                        },
                        {
                            "text": "Naukowcy badali, jak ciepło <w>promienne</w> wpływa na nowoczesne materiały izolacyjne.",
                            "target_lang_translations": {
                                "ru": "Ученые исследовали, как <w>лучистое</w> тепло влияет на современные изоляционные материалы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "energia promienna",
                        "ciepło promienne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с физическим явлением излучения энергии в виде волн или частиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лучистый",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

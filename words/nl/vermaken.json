                    "sense_definition": "Meervoud van <w>vermaak</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de oude stad waren er diverse <w>vermaken</w> te vinden voor de toeristen.",
                            "target_lang_translations": {
                                "en": "In the old city, various <w>amusements</w> could be found for the tourists."
                            }
                        },
                        {
                            "text": "De traditionele <w>vermaken</w> op de kermis trekken nog steeds veel bekijks.",
                            "target_lang_translations": {
                                "en": "The traditional <w>entertainments</w> at the fair still attract a lot of attention."
                            }
                        }
                    ],

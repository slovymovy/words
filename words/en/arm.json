                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "heraldry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family <w>arms</w> were displayed proudly above the fireplace.",
                            "target_lang_translations": {
                                "ru": "Фамильный <w>герб</w> с гордостью демонстрировался над камином."
                            }

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d7544330-d3c6-331f-83b8-963bc63151c8",
                    "sense_definition": "Возглас, выражающий сильные чувства: радость, испуг, удивление, досаду или нетерпение.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "exclamation_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "О <w>господи</w>, я совсем забыл, что мы договорились встретиться сегодня!",
                            "target_lang_translations": {
                                "en": "Oh <w>God</w>, I completely forgot that we agreed to meet today!"
                            }
                        },
                        {
                            "text": "Да <w>господи</w>, сколько можно повторять одно и то же?",
                            "target_lang_translations": {
                                "en": "<w>Lord</w>, how many times do I have to repeat the same thing?"
                            }
                        }
                    ],
                    "synonyms": [
                        "боже",
                        "боже",
                        "боже",
                        "о бо́же!",
                        "бо́же мой!"
                    ],
                    "common_phrases": [
                        "прости господи",
                        "господи помилуй",
                        "слава тебе господи"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation expressing strong feelings: joy, fright, surprise, annoyance, or impatience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "God",
                                "target_lang_sense_clarification": "exclamation of surprise or annoyance"
                            },
                            {
                                "target_lang_word": "Lord",
                                "target_lang_sense_clarification": "exclamation of exasperation or surprise"
                            },
                            {
                                "target_lang_word": "good gracious",
                                "target_lang_sense_clarification": "expression of surprise (dated)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb06da68-59b7-34c2-aea1-7280a43350d5",
                    "sense_definition": "Звательная форма слова <w>Господь</w>, используемая при обращении к Богу.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Услышь мою молитву, <w>Господи</w>, и пошли мне душевный покой.",
                            "target_lang_translations": {
                                "en": "Hear my prayer, <w>Lord</w>, and send me peace of mind."
                            }
                        },
                        {
                            "text": "Прости меня, <w>Господи</w>, за все мои вольные и невольные прегрешения.",
                            "target_lang_translations": {
                                "en": "Forgive me, <w>Lord</w>, for all my voluntary and involuntary sins."
                            }
                        }
                    ],
                    "synonyms": [
                        "боже"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "vocative singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The vocative form of the word 'Lord', used when addressing God."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Lord",
                                "target_lang_sense_clarification": "address to God"
                            },
                            {
                                "target_lang_word": "God",
                                "target_lang_sense_clarification": "address to the Deity"
                            }
                        ]
                    }
                }
            ]
        }
    ]

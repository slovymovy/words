    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "98a60bf3-bfb0-3a10-a7aa-1c2c1ba66baa",
                    "sense_definition": "Музыкально-драматический жанр, в котором артисты поют в сопровождении оркестра.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "music_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя бабушка очень любит классическую <оперу> и часто слушает её по радио.",
                            "target_lang_translations": {
                                "en": "My grandmother really loves classical <opera> and often listens to it on the radio."
                            }
                        },
                        {
                            "text": "Итальянская <опера> известна во всём мире своими красивыми мелодиями.",
                            "target_lang_translations": {
                                "en": "Italian <opera> is known all over the world for its beautiful melodies."
                            }
                        }
                    ],
                    "common_phrases": [
                        "итальянская опера",
                        "любить оперу",
                        "слушать оперу"
                    ],
                    "target_lang_definitions": {
                        "en": "A musical-dramatic genre in which artists sing accompanied by an orchestra."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opera",
                                "target_lang_sense_clarification": "music genre"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f333dbc1-8958-3995-b703-7439d65511d7",
                    "sense_definition": "Отдельное музыкальное произведение или сценическая постановка в этом жанре.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "music_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Композитор писал эту <оперу> более трёх лет.",
                            "target_lang_translations": {
                                "en": "The composer wrote this <opera> for more than three years."
                            }
                        },
                        {
                            "text": "Мы купили билеты на новую <оперу> в Большом театре.",
                            "target_lang_translations": {
                                "en": "We bought tickets for a new <opera> at the Bolshoi Theatre."
                            }
                        }
                    ],
                    "common_phrases": [
                        "билет на оперу",
                        "герой оперы",
                        "ария из оперы"
                    ],
                    "target_lang_definitions": {
                        "en": "A separate musical work or stage production in this genre."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opera",
                                "target_lang_sense_clarification": "musical work"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efc7109b-077f-3a61-b42d-e583c99388da",
                    "sense_definition": "Здание театра, в котором ставятся такие музыкальные представления.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы встретились возле здания старой <оперы> в центре города.",
                            "target_lang_translations": {
                                "en": "We met near the old <opera house> building in the city center."
                            }
                        },
                        {
                            "text": "Венская <опера> поражает туристов своей архитектурой и величием.",
                            "target_lang_translations": {
                                "en": "The Vienna <Opera> amazes tourists with its architecture and grandeur."
                            }
                        }
                    ],
                    "synonyms": [
                        "оперный театр"
                    ],
                    "common_phrases": [
                        "здание оперы",
                        "пойти в оперу"
                    ],
                    "target_lang_definitions": {
                        "en": "A theater building where such musical performances are staged."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opera house",
                                "target_lang_sense_clarification": "building"
                            },
                            {
                                "target_lang_word": "opera",
                                "target_lang_sense_clarification": "building (short form)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5d78a57-e53e-31f0-8955-407234e9af9f",
                    "sense_definition": "Совершенно иная ситуация, тема или категория, не относящаяся к текущему обсуждению (употребляется в устойчивых выражениях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "idiom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Твои проблемы с машиной — это совсем из другой <оперы>.",
                            "target_lang_translations": {
                                "en": "Your car problems are a completely different <matter>."
                            }
                        },
                        {
                            "text": "Мы обсуждаем литературу, а твоё замечание из другой <оперы>.",
                            "target_lang_translations": {
                                "en": "We are discussing literature, and your remark is a different <story>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "из другой оперы",
                        "совсем из другой оперы"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in idiomatic phrases"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A completely different situation, topic, or category not related to the current discussion (used in set phrases)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "different matter",
                                "target_lang_sense_clarification": "idiomatic"
                            },
                            {
                                "target_lang_word": "another story",
                                "target_lang_sense_clarification": "idiomatic"
                            },
                            {
                                "target_lang_word": "different kettle of fish",
                                "target_lang_sense_clarification": "idiomatic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6ea2acf-e5ea-3327-bdcc-4c778bc73a0a",
                    "sense_definition": "Форма родительного или винительного падежа единственного числа существительного <w>опер</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Преступник сразу узнал опытного <опера> в толпе прохожих.",
                            "target_lang_translations": {
                                "en": "The criminal immediately recognized the experienced <detective> in the crowd of passersby."
                            }
                        },
                        {
                            "text": "У этого молодого <опера> ещё совсем мало опыта работы.",
                            "target_lang_translations": {
                                "en": "This young <detective> has very little work experience yet."
                            }
                        }
                    ],

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "20c04a9d-a369-3777-aef8-8fcf8ff6360f",
                    "sense_definition": "Okres kilku tygodni bezpośrednio po porodzie, w czasie którego organizm kobiety regeneruje się i powraca do stanu sprzed ciąży.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz wyjaśnił pacjentce, jak dbać o higienę przez cały <w>połóg</w>.",
                            "target_lang_translations": {
                                "ru": "Врач объяснил пациентке, как соблюдать гигиену весь <w>послеродовой период</w>."
                            }
                        },
                        {
                            "text": "Prawidłowy przebieg <w>połogu</w> jest kluczowy dla szybkiego powrotu do zdrowia.",
                            "target_lang_translations": {
                                "ru": "Правильное течение <w>послеродового периода</w> имеет ключевое значение для быстрого выздоровления."
                            }
                        }
                    ],
                    "synonyms": [
                        "okres poporodowy"
                    ],
                    "common_phrases": [
                        "okres połogu",
                        "gorączka połogowa",
                        "zakażenie połogowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период в несколько недель непосредственно после родов, в течение которого организм женщины восстанавливается и возвращается к состоянию до беременности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "послеродовой период",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eee29f86-bd20-3e7f-bfd1-44fa0fb24ab1",
                    "sense_definition": "Dawne lub literackie określenie na sam akt wydawania dziecka na świat.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Królowa zmarła w <w>połogu</w>, pozostawiając królestwo w żałobie.",
                            "target_lang_translations": {
                                "ru": "Королева умерла во время <w>родов</w>, оставив королевство в трауре."
                            }
                        },
                        {
                            "text": "Ciężki <w>połóg</w> nadwątlił siły młodej matki na długie miesiące.",
                            "target_lang_translations": {
                                "ru": "Тяжелые <w>роды</w> подорвали силы молодой матери на долгие месяцы."
                            }
                        }
                    ],
                    "synonyms": [
                        "poród",
                        "rozwiązanie"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie słowo to oznacza okres po porodzie, a nie sam poród"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее или литературное название самого акта рождения ребенка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роды",
                                "target_lang_sense_clarification": "процесс рождения"
                            }
                        ]
                    }
                }
            ]
        },

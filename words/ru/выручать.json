                            },
                            {
                                "target_lang_word": "retrieve",
                                "target_lang_sense_clarification": "get or bring back"
                            },
                            {
                                "target_lang_word": "get back",
                                "target_lang_sense_clarification": "recover possession of"
                            }
                        ]
                    }

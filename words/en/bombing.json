    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec608d69-9466-3121-952b-fee9d9531cb8",
                    "sense_definition": "The action of attacking a target by dropping bombs from aircraft.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city suffered severe damage during the heavy <w>bombing</w>.",
                            "target_lang_translations": {
                                "ru": "Город сильно пострадал во время тяжелой <w>бомбардировки</w>."
                            }
                        },
                        {
                            "text": "They ordered the <w>bombing</w> of strategic military targets.",
                            "target_lang_translations": {
                                "ru": "Они приказали начать <w>бомбардировку</w> стратегических военных целей."
                            }
                        }
                    ],
                    "synonyms": [
                        "bombardment",
                        "air raid",
                        "shelling",
                        "air strike"
                    ],
                    "common_phrases": [
                        "aerial bombing",
                        "heavy bombing",
                        "bombing raid",
                        "strategic bombing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по атаке цели путем сбрасывания бомб с самолета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бомбардиро́вка",
                                "target_lang_sense_clarification": "военная операция"
                            },
                            {
                                "target_lang_word": "бомбёжка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cc3b669-3a21-3126-8a47-bf647fd6cd79",
                    "sense_definition": "The act of placing and detonating an explosive device, often for terrorist or criminal purposes.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Police are investigating the <w>bombing</w> at the central train station.",
                            "target_lang_translations": {
                                "ru": "Полиция расследует <w>взрыв</w> на центральном вокзале."
                            }
                        },
                        {
                            "text": "The suicide <w>bombing</w> killed several people and injured many more.",
                            "target_lang_translations": {
                                "ru": "<w>Взрыв</w>, устроенный смертником, убил несколько человек и ранил многих других."
                            }
                        }
                    ],
                    "synonyms": [
                        "explosion",
                        "blast",
                        "attack"
                    ],
                    "common_phrases": [
                        "suicide bombing",
                        "car bombing",
                        "terrorist bombing",
                        "roadside bombing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт закладки и подрыва взрывного устройства, часто в террористических или преступных целях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрыв",
                                "target_lang_sense_clarification": "детонация устройства"
                            },
                            {
                                "target_lang_word": "тера́кт",
                                "target_lang_sense_clarification": "террористический акт"
                            },
                            {
                                "target_lang_word": "подры́в",
                                "target_lang_sense_clarification": "намеренное разрушение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f8d207a-67a0-3837-b96a-58f0b945a7c6",
                    "sense_definition": "The act of swallowing a drug wrapped in a covering, such as cigarette paper.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He suggested <w>bombing</w> the powder to avoid the bitter taste.",
                            "target_lang_translations": {
                                "ru": "Он предложил <w>проглотить</w> порошок в обертке, чтобы избежать горького вкуса."
                            }
                        },
                        {
                            "text": "The method of <w>bombing</w> allows the substance to be ingested quickly.",
                            "target_lang_translations": {
                                "ru": "Метод <w>проглатывания в обертке</w> позволяет быстро принять вещество."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "drug culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проглатывание наркотика, завернутого в оболочку (например, в сигаретную бумагу)."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },

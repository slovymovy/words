    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb571854-412c-3002-807a-a63966400279",
                    "sense_definition": "Приступать к осуществлению чего-либо, начинать делать что-либо (обычно о важных действиях, мерах или попытках).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство обещало <предпринимать> меры по борьбе с безработицей.",
                            "target_lang_translations": {
                                "en": "The government promised to <take> measures to combat unemployment."
                            }
                        },
                        {
                            "text": "Мы не будем <предпринимать> никаких действий без вашего согласия.",
                            "target_lang_translations": {
                                "en": "We will not <take> any action without your consent."
                            }
                        },
                        {
                            "text": "Врачи <предпринимали> отчаянные попытки спасти пациента.",
                            "target_lang_translations": {
                                "en": "The doctors <made> desperate attempts to save the patient."
                            }
                        }
                    ],
                    "synonyms": [
                        "начинать",
                        "осуществлять",
                        "затевать"
                    ],
                    "antonyms": [
                        "бездействовать"
                    ],
                    "common_phrases": [
                        "предпринимать меры",
                        "предпринимать попытки",
                        "предпринимать шаги",
                        "предпринимать действия",
                        "предпринимать усилия"
                    ],
                    "target_lang_definitions": {
                        "en": "To start carrying out something, to begin doing something (usually regarding important actions, measures, or attempts)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "used with measures, steps, or actions"
                            },
                            {
                                "target_lang_word": "undertake",
                                "target_lang_sense_clarification": "formal; to commit oneself to and begin an enterprise or responsibility"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "used specifically with attempts or efforts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54c996ca-4b4d-38c7-99c4-53fbcb121c9c",
                    "sense_definition": "Задумывать, замышлять или планировать что-либо.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В молодости он часто <предпринимал> смелые путешествия в своих мечтах.",
                            "target_lang_translations": {
                                "en": "In his youth, he often <undertook> bold journeys in his dreams."
                            }
                        },
                        {
                            "text": "Автор <предпринимал> написать этот роман ещё десять лет назад.",
                            "target_lang_translations": {
                                "en": "The author <intended> to write this novel ten years ago."
                            }
                        }
                    ],
                    "synonyms": [
                        "задумывать",
                        "замышлять",
                        "планировать"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To conceive, devise, or plan something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "intend",
                                "target_lang_sense_clarification": "to have in mind as a purpose"
                            },
                            {
                                "target_lang_word": "plan",
                                "target_lang_sense_clarification": "to formulate a scheme or program"
                            }
                        ]
                    }
                }

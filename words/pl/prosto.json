    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8ab6e2ae-56c4-3aac-886c-c9b8329f00a3",
                    "sense_definition": "W kierunku, który nie skręca ani nie zbacza na boki; w linii prostej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Idź cały czas <w>prosto</w> tą ulicą, a na końcu zobaczysz dworzec.",
                            "target_lang_translations": {
                                "ru": "Иди всё время <w>прямо</w> по этой улице, и в конце увидишь вокзал."
                            }
                        },
                        {
                            "text": "Kierowca patrzył <w>prosto</w> przed siebie, skupiając się na drodze.",
                            "target_lang_translations": {
                                "ru": "Водитель смотрел <w>прямо</w> перед собой, сосредоточившись на дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "prościutko",
                        "prościuteńko"
                    ],
                    "antonyms": [
                        "krzywo"
                    ],
                    "common_phrases": [
                        "iść prosto",
                        "jechać prosto",
                        "patrzeć prosto"
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении, которое не поворачивает и не отклоняется в сторону; по прямой линии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямо",
                                "target_lang_sense_clarification": "направление движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c86d2fbc-bbbd-3c9f-ac7b-d53cc7f9e99e",
                    "sense_definition": "W sposób łatwy, nieskomplikowany i zrozumiały; bez trudności.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel wytłumaczył nam ten problem bardzo <w>prosto</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель объяснил нам эту проблему очень <w>просто</w>."
                            }
                        },
                        {
                            "text": "Chcę żyć <w>prosto</w> i cieszyć się każdą chwilą bez zbędnych problemów.",
                            "target_lang_translations": {
                                "ru": "Я хочу жить <w>просто</w> и наслаждаться каждым моментом без лишних проблем."
                            }
                        }
                    ],
                    "antonyms": [
                        "trudno"
                    ],
                    "common_phrases": [
                        "wyjaśnić prosto",
                        "żyć prosto",
                        "pisać prosto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легким, несложным и понятным способом; без трудностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просто",
                                "target_lang_sense_clarification": "легко, несложно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63338bf0-a6cc-3c62-96c3-4d59acc554fc",
                    "sense_definition": "W pozycji wyprostowanej, pionowej, bez garbienia się.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "posture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy nauczyciel wszedł do klasy, wszyscy uczniowie siedzieli <w>prosto</w>.",
                            "target_lang_translations": {
                                "ru": "Когда учитель вошел в класс, все ученики сидели <w>прямо</w>."
                            }
                        },
                        {
                            "text": "Musisz stać <w>prosto</w>, żeby lekarz mógł dokładnie zbadać twój kręgosłup.",
                            "target_lang_translations": {
                                "ru": "Ты должен стоять <w>прямо</w>, чтобы врач мог тщательно осмотреть твой позвоночник."
                            }
                        }
                    ],
                    "antonyms": [
                        "krzywo"
                    ],
                    "common_phrases": [
                        "siedzieć prosto",
                        "stać prosto",
                        "trzymać się prosto"
                    ],
                    "target_lang_definitions": {
                        "ru": "В выпрямленном, вертикальном положении, не сутулясь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямо",
                                "target_lang_sense_clarification": "осанка"
                            },
                            {
                                "target_lang_word": "ровно",
                                "target_lang_sense_clarification": "положение тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e785761b-206e-3202-a190-28534df44fa4",
                    "sense_definition": "Bezpośrednio, bez zatrzymywania się po drodze lub bez pośredników.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "directness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po pracy poszedłem <w>prosto</w> do domu, bo byłem bardzo zmęczony.",
                            "target_lang_translations": {
                                "ru": "После работы я пошел <w>прямо</w> домой, так как очень устал."
                            }
                        },
                        {
                            "text": "Ten pociąg jedzie <w>prosto</w> do Warszawy, bez żadnych przesiadek.",
                            "target_lang_translations": {
                                "ru": "Этот поезд едет <w>прямо</w> в Варшаву, без каких-либо пересадок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prosto do celu",
                        "prosto z mostu",
                        "prosto w oczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непосредственно, без остановок в пути или без посредников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямо",
                                "target_lang_sense_clarification": "непосредственно"
                            },
                            {
                                "target_lang_word": "сразу",
                                "target_lang_sense_clarification": "без промедления"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
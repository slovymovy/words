                },
                {
                    "sense_id": "96640d2f-c8ab-374b-b267-e8e9627b8d57",
                    "sense_definition": "A guest or guests who visit someone socially.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to clean the house because we have <w>company</w> coming for dinner.",

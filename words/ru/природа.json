    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e8f45461-42b5-39a3-8bf2-cdefeb3af3c6",
                    "sense_definition": "Естественный мир, всё, что не создано человеком (растения, животные, ландшафты, климат).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_world",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы должны беречь и охранять родную <природу>.",
                            "target_lang_translations": {
                                "en": "We must cherish and protect our native <nature>."
                            }
                        },
                        {
                            "text": "Весной вся <природа> просыпается после долгой зимы.",
                            "target_lang_translations": {
                                "en": "In spring, all of <nature> wakes up after the long winter."
                            }
                        }
                    ],
                    "synonyms": [
                        "натура",
                        "естество"
                    ],
                    "common_phrases": [
                        "живая природа",
                        "охрана природы",
                        "законы природы",
                        "явления природы",
                        "красота природы"
                    ],
                    "target_lang_definitions": {
                        "en": "The natural world, everything not created by humans (plants, animals, landscapes, climate)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nature",
                                "target_lang_sense_clarification": "the physical world and living things"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e776a5d3-85f3-309e-a608-35278c6a66cc",
                    "sense_definition": "Местность за пределами города (лес, поле, река), куда люди едут для отдыха.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_world",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В выходные мы с друзьями планируем выехать на <природу>.",
                            "target_lang_translations": {
                                "en": "On the weekend, my friends and I plan to go out into <nature>."
                            }
                        },
                        {
                            "text": "Летом очень полезно отдыхать на <природе>.",
                            "target_lang_translations": {
                                "en": "In summer, it is very healthy to rest in <nature>."
                            }
                        }
                    ],
                    "synonyms": [
                        "загород"
                    ],
                    "antonyms": [
                        "город"
                    ],
                    "common_phrases": [
                        "выехать на природу",
                        "отдых на природе",
                        "пикник на природе",
                        "быть на природе"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An area outside the city (forest, fields, rivers) where people go for recreation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the outdoors",
                                "target_lang_sense_clarification": "open air, area away from buildings"
                            },
                            {
                                "target_lang_word": "nature",
                                "target_lang_sense_clarification": "natural environment as a place for rest"
                            },
                            {
                                "target_lang_word": "the countryside",
                                "target_lang_sense_clarification": "rural area outside the city"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8eaa489a-dcb0-3c8a-9fc3-cd40f482e3c9",
                    "sense_definition": "Внутренняя сущность, основное свойство или характер кого-либо или чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Такова уж <природа> человека, что он всегда хочет большего.",
                            "target_lang_translations": {
                                "en": "Such is human <nature> that one always wants more."
                            }
                        },
                        {
                            "text": "Учёные долго изучали <природу> этого странного явления.",
                            "target_lang_translations": {
                                "en": "Scientists studied the <nature> of this strange phenomenon for a long time."
                            }
                        }
                    ],
                    "synonyms": [

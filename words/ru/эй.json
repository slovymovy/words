{
    "word_family": [
        "эйкать"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "244d015f-8e39-3458-a5f5-dbcef8282922",
                    "sense_definition": "Возглас для привлечения внимания, оклика или обращения к кому-либо, часто в фамильярной или грубой форме.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "call_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Эй</w>, подождите меня, я тоже иду!",
                            "target_lang_translations": {
                                "en": "<w>Hey</w>, wait for me, I'm coming too!"
                            }
                        },
                        {
                            "text": "<w>Эй</w>! Вы меня будете слушать или нет?",
                            "target_lang_translations": {
                                "en": "<w>Hey</w>! Are you going to listen to me or not?"
                            }
                        }
                    ],
                    "synonyms": [
                        "эгей"
                    ],
                    "common_phrases": [
                        "эй, ты",
                        "эй, послушай",
                        "эй, человек"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation used to attract attention, call out, or address someone, often in a familiar or rude way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hey",
                                "target_lang_sense_clarification": "used to call attention or express interest"
                            },
                            {
                                "target_lang_word": "oi",
                                "target_lang_sense_clarification": "informal, often rude call for attention"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14393ae6-cfd0-347a-bde2-f0718e57fc2f",
                    "sense_definition": "Возглас, выражающий предостережение, угрозу или недовольство чьими-либо действиями.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "warning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Эй</w>, не трогай эту кнопку!",
                            "target_lang_translations": {
                                "en": "<w>Hey</w>, don't touch that button!"
                            }
                        },
                        {
                            "text": "<w>Эй</w>, куда ты лезешь без очереди?",
                            "target_lang_translations": {
                                "en": "<w>Hey</w>, where are you going out of turn?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "эй, осторожнее",
                        "эй, полегче"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation expressing a warning, threat, or dissatisfaction with someone's actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hey",
                                "target_lang_sense_clarification": "used to express warning or dissatisfaction"
                            },
                            {
                                "target_lang_word": "oi",
                                "target_lang_sense_clarification": "used to protest or give a warning"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
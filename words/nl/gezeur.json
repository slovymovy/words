            "senses": [
                {
                    "sense_id": "783acf2b-99e3-36a3-825c-327f6a835556",
                    "sense_definition": "Het voortdurend en op een vervelende manier klagen of vragen om iets.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hou toch eens op met dat eeuwige <w>gezeur</w>.",
                            "target_lang_translations": {
                                "en": "Just stop with that constant <w>nagging</w>.",
                                "ru": "Прекрати уже это вечное <w>нытьё</w>."
                            }
                        },
                        {
                            "text": "Ik heb vandaag echt geen zin in jouw <w>gezeur</w>.",
                            "target_lang_translations": {
                                "en": "I really don't feel like dealing with your <w>whining</w> today.",
                                "ru": "У меня сегодня совсем нет настроения слушать твоё <w>нытьё</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gezeik"
                    ],
                    "common_phrases": [
                        "wat een gezeur",
                        "geen gezeur",
                        "ophouden met gezeur"
                    ],
                    "target_lang_definitions": {
                        "en": "Constant and annoying complaining or asking for something.",
                        "ru": "Постоянные и раздражающие жалобы или просьбы о чём-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nagging",
                                "target_lang_sense_clarification": "persistent complaining"
                            },
                            {
                                "target_lang_word": "whining",
                                "target_lang_sense_clarification": "complaining in an annoying tone"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нытьё",
                                "target_lang_sense_clarification": "жалобы"
                            },
                            {
                                "target_lang_word": "ворчание",
                                "target_lang_sense_clarification": "недовольство"
                            },
                            {
                                "target_lang_word": "занудство",
                                "target_lang_sense_clarification": "надоедливость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b41e6f6-2fdf-36cc-abd3-bd95ebcf4834",
                    "sense_definition": "Een vervelende situatie die veel moeite kost en ergernis veroorzaakt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was een heel <w>gezeur</w> om de juiste papieren te krijgen.",
                            "target_lang_translations": {
                                "en": "It was a real <w>hassle</w> to get the right papers.",
                                "ru": "Получить нужные документы было целой <w>морокой</w>."
                            }
                        },
                        {
                            "text": "We willen de klus klaren zonder <w>gezeur</w> achteraf.",
                            "target_lang_translations": {
                                "en": "We want to get the job done without any <w>fuss</w> afterwards.",
                                "ru": "Мы хотим закончить дело без лишних <w>проблем</w> потом."
                            }
                        }
                    ],
                    "synonyms": [
                        "gedoe",
                        "toestand"
                    ],
                    "common_phrases": [
                        "zonder gezeur",
                        "een hoop gezeur"
                    ],
                    "target_lang_definitions": {
                        "en": "An annoying situation that takes a lot of effort and causes irritation.",
                        "ru": "Неприятная ситуация, требующая больших усилий и вызывающая раздражение; хлопоты."
                    },

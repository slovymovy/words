                {
                    "sense_id": "028adfb4-2d0d-3c3f-94fa-c254baac4922",
                    "sense_definition": "Dzień o szczególnym znaczeniu, upamiętniający ważne wydarzenie religijne, państwowe lub osobiste, często wolny od pracy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W niedziele i <w>święta</w> autobusy kursują rzadziej.",
                            "target_lang_translations": {
                                "ru": "По воскресеньям и <w>праздникам</w> автобусы ходят реже."
                            }
                        },
                        {
                            "text": "Boże Narodzenie to najważniejsze <w>święto</w> dla mojej rodziny.",
                            "target_lang_translations": {
                                "ru": "Рождество — это самый важный <w>праздник</w> для моей семьи."
                            }
                        }
                    ],
                    "antonyms": [
                        "dzień powszedni",
                        "zwykły dzień"
                    ],
                    "common_phrases": [
                        "święto narodowe",
                        "święto państwowe",
                        "Wesołych Świąt",
                        "święta wielkanocne"
                    ],
                    "target_lang_definitions": {
                        "ru": "День особого значения, отмечающий важное религиозное, государственное или личное событие, часто нерабочий день."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "праздник",
                                "target_lang_sense_clarification": "день торжества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e7309ec-ec68-318c-8af9-90a4633ca3fb",
                    "sense_definition": "Wyjątkowe, radosne wydarzenie lub dzień, który jest traktowany jako niezwykły i bardzo przyjemny.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każde spotkanie z dawnym przyjacielem to dla mnie prawdziwe <w>święto</w>.",
                            "target_lang_translations": {
                                "ru": "Каждая встреча со старым другом — для меня настоящий <w>праздник</w>."
                            }
                        },
                        {
                            "text": "Wyjście do teatru było dla nich małym <w>świętem</w>.",
                            "target_lang_translations": {
                                "ru": "Поход в театр был для них маленьким <w>праздником</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prawdziwe święto",
                        "robić sobie święto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исключительное, радостное событие или день, воспринимаемый как необычный и очень приятный."
                    },
                    "translations": {
                        "ru": [

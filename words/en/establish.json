{
    "word_family": [
        "establishment",
        "reestablish",
        "disestablish",
        "anti-establishment",
        "establisher"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a38a4d75-8f27-3a2b-bb89-b8d5ead2dd4c",
                    "sense_definition": "To start a company, organization, system, or relationship that is intended to continue for a long time.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university was <w>established</w> in the nineteenth century to promote science and arts.",
                            "target_lang_translations": {
                                "ru": "Университет был <w>основан</w> в девятнадцатом веке для развития науки и искусства."
                            }
                        },
                        {
                            "text": "We are trying to <w>establish</w> a relationship of trust with our new clients.",
                            "target_lang_translations": {
                                "ru": "Мы пытаемся <w>установить</w> доверительные отношения с нашими новыми клиентами."
                            }
                        },
                        {
                            "text": "The committee voted to <w>establish</w> a new system for tracking expenses.",
                            "target_lang_translations": {
                                "ru": "Комитет проголосовал за то, чтобы <w>создать</w> новую систему отслеживания расходов."
                            }
                        }
                    ],
                    "synonyms": [
                        "found",
                        "set up",
                        "institute",
                        "create",
                        "launch"
                    ],
                    "antonyms": [
                        "abolish",
                        "dissolve",
                        "terminate"
                    ],
                    "common_phrases": [
                        "establish a company",
                        "establish a relationship",
                        "establish a system",
                        "establish contact"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основывать компанию, организацию или систему; налаживать отношения, рассчитанные на долгий срок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основать",
                                "target_lang_sense_clarification": "компанию, город, династию"
                            },
                            {
                                "target_lang_word": "учредить",
                                "target_lang_sense_clarification": "организацию, фонд, звание"
                            },
                            {
                                "target_lang_word": "установить",
                                "target_lang_sense_clarification": "отношения, контакты, правила"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e229b660-41d3-32fb-a1be-b2c0aa3ce267",
                    "sense_definition": "To discover or prove that something is true or accepted.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "verification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police investigation failed to <w>establish</w> a link between the two suspects.",
                            "target_lang_translations": {
                                "ru": "Полицейскому расследованию не удалось <w>установить</w> связь между двумя подозреваемыми."
                            }
                        },
                        {
                            "text": "Scientists are working hard to <w>establish</w> the cause of the sudden outbreak.",
                            "target_lang_translations": {
                                "ru": "Ученые усердно работают, чтобы <w>установить</w> причину внезапной вспышки заболевания."
                            }
                        },
                        {
                            "text": "We must <w>establish</w> the truth before we make any public accusations.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>установить</w> истину, прежде чем выдвигать какие-либо публичные обвинения."
                            }
                        }
                    ],
                    "synonyms": [
                        "prove",
                        "demonstrate",
                        "verify",
                        "confirm",
                        "substantiate"
                    ],
                    "antonyms": [
                        "disprove",
                        "refute"
                    ],
                    "common_phrases": [
                        "establish the truth",
                        "establish a fact",
                        "establish a connection",
                        "establish cause"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выяснять или доказывать, что нечто является истинным или признанным фактом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "установить",
                                "target_lang_sense_clarification": "факт, личность, причину"
                            },
                            {
                                "target_lang_word": "выяснить",
                                "target_lang_sense_clarification": "обстоятельства, правду"
                            },
                            {
                                "target_lang_word": "доказать",
                                "target_lang_sense_clarification": "вину, теорию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e414caf-0f51-3124-b59e-63d1b4bf126e",
                    "sense_definition": "To make something stable or firm, or to place someone in a secure and respected position.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "stabilization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She worked for many years to <w>establish</w> herself as a leading expert in the field.",
                            "target_lang_translations": {
                                "ru": "Она работала много лет, чтобы <w>утвердиться</w> в качестве ведущего эксперта в этой области."
                            }
                        },
                        {
                            "text": "Once the plants <w>establish</w> in the soil, they will require much less water.",
                            "target_lang_translations": {
                                "ru": "Как только растения <w>укоренятся</w> в почве, им потребуется гораздо меньше воды."
                            }
                        },
                        {
                            "text": "The team managed to <w>establish</w> a reputation for excellence very quickly.",
                            "target_lang_translations": {
                                "ru": "Команде удалось очень быстро <w>завоевать</w> отличную репутацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "secure",
                        "solidify",
                        "strengthen",
                        "entrench"
                    ],
                    "antonyms": [
                        "destabilize",
                        "weaken"
                    ],
                    "common_phrases": [
                        "establish oneself",
                        "become established",
                        "establish a reputation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Укреплять положение, добиваться признания и авторитета или укореняться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утвердиться",
                                "target_lang_sense_clarification": "в статусе, в мнении"
                            },
                            {
                                "target_lang_word": "зарекомендовать себя",
                                "target_lang_sense_clarification": "как специалист, с лучшей стороны"
                            },
                            {
                                "target_lang_word": "укорениться",
                                "target_lang_sense_clarification": "о растениях, привычках"
                            },
                            {
                                "target_lang_word": "завоевать",
                                "target_lang_sense_clarification": "репутацию, авторитет"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
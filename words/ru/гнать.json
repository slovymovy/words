    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7cc2f385-34d7-34dd-b41c-c7ba9ca26a8c",
                    "sense_definition": "Заставлять перемещаться в определённом направлении (животных, людей или предметы).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пастух каждый день <гнал> стадо коров на зелёный луг.",
                            "target_lang_translations": {
                                "en": "The shepherd <drove> the herd of cows to the green meadow every day."
                            }
                        },
                        {
                            "text": "Сильный ветер <гнал> по небу тяжёлые серые тучи.",
                            "target_lang_translations": {
                                "en": "A strong wind <drove> heavy grey clouds across the sky."
                            }
                        }
                    ],
                    "antonyms": [
                        "останавливать"
                    ],
                    "common_phrases": [
                        "гнать стадо",
                        "гнать мяч",
                        "ветер гонит"
                    ],
                    "target_lang_definitions": {
                        "en": "To force people or animals to move in a specific direction; to cause objects (like clouds) to move rapidly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "general term for forcing movement"
                            },
                            {
                                "target_lang_word": "herd",
                                "target_lang_sense_clarification": "specifically for animals"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93617054-01e0-3179-9ff4-97a38bbe3d42",
                    "sense_definition": "Быстро ехать, управлять транспортным средством на высокой скорости.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "motion_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель такси <гнал> машину в аэропорт, чтобы мы не опоздали.",
                            "target_lang_translations": {
                                "en": "The taxi driver <drove> the car <fast> to the airport so we wouldn't be late."
                            }
                        },
                        {
                            "text": "Не нужно так <гнать>, дорога очень скользкая и опасная.",
                            "target_lang_translations": {
                                "en": "You shouldn't <speed> like that, the road is very slippery and dangerous."
                            }
                        }
                    ],
                    "antonyms": [
                        "останавливать"
                    ],
                    "common_phrases": [
                        "гнать машину",
                        "гнать лошадей",
                        "гнать во весь опор"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To drive a vehicle at a very high speed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speed",
                                "target_lang_sense_clarification": "to drive faster than allowed or safe"
                            },
                            {
                                "target_lang_word": "drive fast",
                                "target_lang_sense_clarification": "general description of high speed"
                            },
                            {
                                "target_lang_word": "race",
                                "target_lang_sense_clarification": "to move at full speed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adb3eac4-5652-3976-9ed4-2240689e811c",
                    "sense_definition": "Грубо заставлять уйти, выгонять кого-либо прочь.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "pressure_expulsion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если этот человек снова придёт, смело <гони> его отсюда.",
                            "target_lang_translations": {
                                "en": "If this man comes again, feel free to <kick> him <out> of here."
                            }
                        },
                        {
                            "text": "Хозяин дома <гнал> непрошеных гостей со своего двора.",
                            "target_lang_translations": {
                                "en": "The host <chased> the uninvited guests <away> from his yard."
                            }
                        }
                    ],
                    "antonyms": [
                        "приглашать",
                        "призывать"
                    ],
                    "common_phrases": [
                        "гнать прочь",
                        "гнать в шею",
                        "гнать из дома"
                    ],
                    "target_lang_definitions": {
                        "en": "To rudely force someone to leave; to expel or kick out."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chase away",
                                "target_lang_sense_clarification": "to force someone to leave"
                            },
                            {
                                "target_lang_word": "kick out",
                                "target_lang_sense_clarification": "informal, to expel someone"
                            },
                            {
                                "target_lang_word": "drive away",
                                "target_lang_sense_clarification": "to force to leave"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07b9ddeb-1a36-351b-ad66-22654facd824",
                    "sense_definition": "Говорить неправду, выдумывать, рассказывать небылицы или говорить глупости.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "speech_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не слушай его, он просто <гонит> и ничего не знает.",
                            "target_lang_translations": {
                                "en": "Don't listen to him, he's just <talking nonsense> and knows nothing."
                            }
                        },
                        {
                            "text": "Хватит <гнать>, расскажи нам, как всё было на самом деле.",
                            "target_lang_translations": {
                                "en": "Stop <making things up>, tell us how it really was."
                            }
                        }
                    ],
                    "synonyms": [
                        "врать",
                        "лгать",
                        "выдумывать",
                        "обманывать",
                        "фантазировать",
                        "нести",
                        "городить",
                        "молоть"
                    ],
                    "common_phrases": [
                        "ты гонишь",
                        "гнать пургу",
                        "гнать ерунду"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "(Slang) To tell lies, make things up, or talk nonsense."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "talk nonsense",
                                "target_lang_sense_clarification": "to say things that are not true or silly"
                            },
                            {
                                "target_lang_word": "make things up",
                                "target_lang_sense_clarification": "to fabricate stories"
                            },
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "to say something untrue"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17606dc1-bd12-3080-9a50-8ab4ce45f75d",
                    "sense_definition": "Торопить кого-либо, принуждать делать что-то быстрее.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pressure_expulsion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник постоянно <гнал> нас с отчётом, требуя закончить сегодня.",
                            "target_lang_translations": {
                                "en": "The boss constantly <rushed> us with the report, demanding we finish today."
                            }
                        },
                        {
                            "text": "Не <гони> меня, я стараюсь работать так быстро, как могу.",
                            "target_lang_translations": {
                                "en": "Don't <rush> me, I'm trying to work as fast as I can."
                            }
                        }
                    ],
                    "synonyms": [
                        "торопить",
                        "понукать"
                    ],
                    "common_phrases": [
                        "гнать с работой",
                        "не гони меня"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To pressure someone to do something faster; to hurry someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rush",
                                "target_lang_sense_clarification": "to pressure someone to act faster"
                            },
                            {
                                "target_lang_word": "push",
                                "target_lang_sense_clarification": "to urge or pressure someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23cf5f68-172f-334f-9081-aa4951b60bf4",
                    "sense_definition": "Высказывать претензии, необоснованно обвинять или наезжать на кого-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "speech_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зачем ты <гонишь> на друга, если он ни в чём не виноват?",
                            "target_lang_translations": {
                                "en": "Why are you <attacking> your friend if he's not to blame for anything?"
                            }
                        },
                        {
                            "text": "Не надо на меня <гнать>, я сделал всё правильно.",
                            "target_lang_translations": {
                                "en": "Don't <come at> me, I did everything right."
                            }
                        }
                    ],
                    "synonyms": [
                        "упрекать",
                        "обвинять"
                    ],
                    "common_phrases": [
                        "гнать на человека",
                        "не гони на меня"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "(Slang) To make unfounded accusations or verbally attack someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come at",
                                "target_lang_sense_clarification": "slang, to verbally attack or accuse"
                            },
                            {
                                "target_lang_word": "attack",
                                "target_lang_sense_clarification": "to criticize or blame someone"
                            },
                            {
                                "target_lang_word": "diss",
                                "target_lang_sense_clarification": "slang, to speak disrespectfully"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1348d269-6b20-3166-943a-ce3f1199a8c9",
                    "sense_definition": "Преследовать зверя во время охоты, загонять добычу.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "motion_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охотники с собаками <гнали> волка через густой лес.",
                            "target_lang_translations": {
                                "en": "Hunters with dogs <chased> the wolf through the dense forest."
                            }
                        },
                        {
                            "text": "Егерь умел правильно <гнать> зверя прямо на стрелков.",
                            "target_lang_translations": {
                                "en": "The gamekeeper knew how to properly <drive> the beast right towards the shooters."
                            }
                        }
                    ],
                    "synonyms": [
                        "охотиться",
                        "преследовать"
                    ],
                    "common_phrases": [
                        "гнать зверя",
                        "гнать добычу"
                    ],
                    "target_lang_definitions": {
                        "en": "To pursue or chase a wild animal during a hunt."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chase",
                                "target_lang_sense_clarification": "to pursue prey"
                            },
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "to force game towards hunters"
                            },
                            {
                                "target_lang_word": "hunt",
                                "target_lang_sense_clarification": "general term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5303855c-04b6-35a9-8662-a95723eb1f90",
                    "sense_definition": "Изготавливать спиртные напитки или другие вещества методом перегонки.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В деревне многие умеют <гнать> качественный самогон из яблок.",
                            "target_lang_translations": {
                                "en": "In the village, many know how to <distill> high-quality moonshine from apples."
                            }
                        },
                        {
                            "text": "Для получения эфирного масла нужно <гнать> пар через лепестки роз.",
                            "target_lang_translations": {
                                "en": "To obtain essential oil, you need to <drive> steam through rose petals."
                            }
                        }
                    ],
                    "synonyms": [
                        "перегонять"
                    ],
                    "common_phrases": [
                        "гнать самогон",
                        "гнать спирт"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often implies illicit production"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To produce alcoholic beverages (like moonshine) or substances by distillation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "distill",
                                "target_lang_sense_clarification": "technical term for the process"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "general term, often used with moonshine"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2897b95-4dd5-39af-a04b-8018ef215801",
                    "sense_definition": "Подвергать гонениям, жестоко притеснять или преследовать за убеждения.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "pressure_expulsion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В те времена власть жестоко <гнала> всех инакомыслящих поэтов.",
                            "target_lang_translations": {
                                "en": "In those times, the government cruelly <persecuted> all dissident poets."
                            }
                        },
                        {
                            "text": "Судьба <гнала> его по свету, не давая нигде найти приют.",
                            "target_lang_translations": {
                                "en": "Fate <drove> him around the world, not letting him find shelter anywhere."
                            }
                        }
                    ],
                    "synonyms": [
                        "притеснять",
                        "преследовать"
                    ],
                    "common_phrases": [
                        "гнать со света",
                        "судьба гнала"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "or literary style"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To persecute, oppress, or harass someone, often for their beliefs; to be driven by fate."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "persecute",
                                "target_lang_sense_clarification": "to oppress or harass persistently"
                            },
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "used metaphorically, e.g., by fate"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
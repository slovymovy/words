    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "24ae96bd-bc55-352c-bc9d-7aa39e240640",
                    "sense_definition": "Предосудительный недостаток характера, дурная привычка или безнравственное поведение.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "defect_flaw",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лень — это большой <w>порок</w>, который мешает человеку достичь успеха в жизни.",
                            "target_lang_translations": {
                                "en": "Laziness is a great <w>vice</w> that prevents a person from achieving success in life."
                            }
                        },
                        {
                            "text": "Герой романа пытается побороть свой главный <w>порок</w> и стать лучше.",
                            "target_lang_translations": {
                                "en": "The hero of the novel tries to overcome his main <w>vice</w> and become better."
                            }
                        }
                    ],
                    "synonyms": [
                        "недостаток",
                        "грех",
                        "разврат",
                        "греховность"
                    ],
                    "antonyms": [
                        "добродетель",
                        "достоинство",
                        "праведность",
                        "безгрешность"
                    ],
                    "common_phrases": [
                        "предаваться пороку",
                        "людские пороки",
                        "искоренять пороки",
                        "порок общества"
                    ],
                    "target_lang_definitions": {
                        "en": "A reprehensible character flaw, bad habit, or immoral behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vice",
                                "target_lang_sense_clarification": "immoral behavior or habit"
                            },
                            {
                                "target_lang_word": "flaw",
                                "target_lang_sense_clarification": "character defect"
                            },
                            {
                                "target_lang_word": "depravity",
                                "target_lang_sense_clarification": "moral corruption"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f963bbe-6618-3e7d-815a-6e771de03bcd",
                    "sense_definition": "Серьёзный физический недостаток или аномалия в развитии организма.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "defect_flaw",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи обнаружили у новорождённого врождённый <w>порок</w> сердца и назначили операцию.",
                            "target_lang_translations": {
                                "en": "Doctors discovered a congenital heart <w>defect</w> in the newborn and scheduled surgery."
                            }
                        },
                        {
                            "text": "Несмотря на физический <w>порок</w>, он вёл активный образ жизни и занимался творчеством.",
                            "target_lang_translations": {
                                "en": "Despite his physical <w>defect</w>, he led an active lifestyle and engaged in creative work."
                            }
                        }
                    ],
                    "synonyms": [
                        "недуг",
                        "дефект",
                        "уродство"
                    ],
                    "common_phrases": [
                        "порок сердца",
                        "врождённый порок",
                        "физический порок"
                    ],
                    "target_lang_definitions": {
                        "en": "A serious physical defect or anomaly in the development of an organism."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defect",
                                "target_lang_sense_clarification": "medical or physical abnormality"
                            },
                            {

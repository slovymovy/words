                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Худ",
                                "target_lang_sense_clarification": "Фамилия"
                            },
                            {
                                "target_lang_word": "Гуд",
                                "target_lang_sense_clarification": "Традиционно для Робин Гуда"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0b666012-462f-30fc-991f-e08ffb63a233",
                    "sense_definition": "Klimat psychiczny panujący w jakimś środowisku, miejscu lub grupie ludzi; nastrój.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym biurze panuje bardzo przyjazna i rodzinna <w>atmosfera</w>.",
                            "target_lang_translations": {
                                "ru": "В нашем офисе царит очень дружелюбная и семейная <w>атмосфера</w>."
                            }
                        },
                        {
                            "text": "Przez kłótnię rodziców przy stole zapanowała gęsta <w>atmosfera</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за ссоры родителей за столом воцарилась напряжённая <w>атмосфера</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "klimat",
                        "nastrój",
                        "aura",
                        "duch"
                    ],
                    "common_phrases": [
                        "miła atmosfera",
                        "gęsta atmosfera",
                        "luźna atmosfera",
                        "psuć atmosferę",
                        "rodzinna atmosfera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психологический климат в какой-либо среде или группе людей; настроение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атмосфера",
                                "target_lang_sense_clarification": "обстановка, дух"
                            },
                            {
                                "target_lang_word": "обстановка",
                                "target_lang_sense_clarification": "ситуация, условия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ab4a050-9a07-36a3-b1af-e5104a7809a8",
                    "sense_definition": "Powłoka gazowa otaczająca Ziemię lub inne ciało niebieskie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Większość meteorów spala się całkowicie, wchodząc w ziemską <w>atmosferę</w>.",
                            "target_lang_translations": {
                                "ru": "Большинство метеоров полностью сгорает, входя в земную <w>атмосферу</w>."
                            }
                        },
                        {
                            "text": "Księżyc nie posiada gęstej <w>atmosfery</w>, która chroniłaby go przed promieniowaniem.",
                            "target_lang_translations": {
                                "ru": "У Луны нет плотной <w>атмосферы</w>, которая защищала бы её от излучения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "atmosfera ziemska",
                        "zanieczyszczenie atmosfery",
                        "górne warstwy atmosfery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Газовая оболочка, окружающая Землю или другое небесное тело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атмосфера",
                                "target_lang_sense_clarification": "газовая оболочка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af4ec1cc-c365-3833-a124-282fe54c36d8",
                    "sense_definition": "Powietrze wypełniające jakieś pomieszczenie, często oceniane jako duszne lub nieświeże.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W małej sali konferencyjnej panowała duszna <w>atmosfera</w>, więc otworzyłem okno.",
                            "target_lang_translations": {
                                "ru": "В маленьком конференц-зале была душная <w>атмосфера</w>, поэтому я открыл окно."
                            }
                        },
                        {
                            "text": "Dym papierosowy sprawił, że <w>atmosfera</w> w barze była ciężka i nieprzyjemna.",
                            "target_lang_translations": {
                                "ru": "Из-за сигаретного дыма <w>атмосфера</w> в баре была тяжёлой и неприятной."
                            }
                        }
                    ],
                    "synonyms": [
                        "powietrze"
                    ],
                    "common_phrases": [
                        "duszna atmosfera",
                        "ciężka atmosfera",
                        "oczyścić atmosferę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздух, наполняющий помещение, часто оцениваемый как душный или несвежий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воздух",
                                "target_lang_sense_clarification": "физическая среда"
                            },
                            {
                                "target_lang_word": "атмосфера",
                                "target_lang_sense_clarification": "состояние воздуха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd4c0902-30b5-3c84-bb52-36090bd1c626",
                    "sense_definition": "Jednostka miary ciśnienia, odpowiadająca średniemu ciśnieniu atmosferycznemu na poziomie morza.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten profesjonalny zegarek jest wodoszczelny do ciśnienia dziesięciu <w>atmosfer</w>.",
                            "target_lang_translations": {
                                "ru": "Эти профессиональные часы водонепроницаемы при давлении до десяти <w>атмосфер</w>."
                            }
                        },
                        {
                            "text": "W oponach samochodowych zazwyczaj utrzymuje się ciśnienie około dwóch <w>atmosfer</w>.",
                            "target_lang_translations": {
                                "ru": "В автомобильных шинах обычно поддерживается давление около двух <w>атмосфер</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "atmosfera techniczna",
                        "atmosfera fizyczna",
                        "ciśnienie kilku atmosfer"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения давления, соответствующая среднему атмосферному давлению на уровне моря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атмосфера",
                                "target_lang_sense_clarification": "единица измерения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

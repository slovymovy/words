            "pos": "noun",
            "senses": [
                {
                    "sense_id": "828fa509-4842-38fd-a198-5ef1aba81435",
                    "sense_definition": "Chaotyczne ruchy powietrza w atmosferze, często odczuwalne przez pasażerów samolotu jako wstrząsy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samolot wpadł w silne <w>turbulencje</w> podczas przelotu nad oceanem.",
                            "target_lang_translations": {
                                "ru": "Самолет попал в сильную <w>турбулентность</w> во время перелета над океаном."
                            }
                        },
                        {
                            "text": "Pilot ostrzegł pasażerów o zbliżającej się strefie <w>turbulencji</w>.",
                            "target_lang_translations": {
                                "ru": "Пилот предупредил пассажиров о приближении к зоне <w>турбулентности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wstrząs",
                        "zawirowanie"
                    ],
                    "common_phrases": [
                        "wpaść w turbulencje",
                        "silne turbulencje",
                        "strefa turbulencji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хаотичные движения воздуха в атмосфере, часто ощущаемые пассажирами самолета как тряска."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "турбулентность",
                                "target_lang_sense_clarification": "атмосферное явление"
                            },
                            {
                                "target_lang_word": "болтанка",
                                "target_lang_sense_clarification": "разговорное, тряска в полете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3497fa7-a7ee-37c5-9e2a-630c739bf051",
                    "sense_definition": "Nieregularny, chaotyczny ruch cieczy lub gazu, w którym cząsteczki mieszają się ze sobą w nieuporządkowany sposób.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "motion_physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Inżynierowie badają <w>turbulencję</w> powietrza wokół skrzydeł nowego modelu samolotu.",
                            "target_lang_translations": {
                                "ru": "Инженеры изучают <w>турбулентность</w> воздуха вокруг крыльев новой модели самолета."
                            }
                        },
                        {
                            "text": "Szybki przepływ wody w rurze może powodować niepożądane <w>turbulencje</w>.",
                            "target_lang_translations": {
                                "ru": "Быстрый поток воды в трубе может вызывать нежелательную <w>турбулентность</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawirowanie",
                        "wirowanie"
                    ],
                    "common_phrases": [
                        "przepływ i turbulencja",
                        "model turbulencji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нерегулярное, хаотичное движение жидкости или газа, при котором частицы смешиваются беспорядочным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "турбулентность",

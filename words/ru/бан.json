    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ae2e06d1-9d0b-3121-843c-1db2cf3c6ada",
                    "sense_definition": "Запрет на пользование интернет-ресурсом, форумом или игрой, накладываемый администратором за нарушение правил.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "internet_restriction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За использование нецензурных выражений в чате игрок получил <w>бан</w> на неделю.",
                            "target_lang_translations": {
                                "en": "For using profanity in the chat, the player received a one-week <w>ban</w>."
                            }
                        },
                        {
                            "text": "Если ты продолжишь рассылать спам, модератор отправит тебя в вечный <w>бан</w>.",
                            "target_lang_translations": {
                                "en": "If you continue sending spam, the moderator will issue a permanent <w>ban</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "блокировка",
                        "запрет"
                    ],
                    "antonyms": [
                        "разбан"
                    ],
                    "common_phrases": [
                        "отправить в бан",
                        "получить бан",
                        "вечный бан",
                        "вылететь в бан"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A prohibition on using an internet resource, forum, or game, imposed by an administrator for rule violations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ban",
                                "target_lang_sense_clarification": "internet restriction"
                            },
                            {
                                "target_lang_word": "block",
                                "target_lang_sense_clarification": "access restriction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebeb993e-ddb2-34c7-9ec6-31abbc5dadc0",

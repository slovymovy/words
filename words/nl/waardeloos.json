                            {
                                "target_lang_word": "паршивый",
                                "target_lang_sense_clarification": "разг. очень плохой"
                            },
                            {
                                "target_lang_word": "никчемный",
                                "target_lang_sense_clarification": "о человеке или чувстве"
                            }
                        ],

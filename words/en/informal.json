    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "87b90d0d-2c6d-339f-bcac-5e619980b5a1",
                    "sense_definition": "Having a relaxed, friendly, or unofficial style or manner; not following strict rules of behavior.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager held an <w>informal</w> meeting to hear our ideas.",
                            "target_lang_translations": {
                                "ru": "Менеджер провёл <w>неформальную</w> встречу, чтобы выслушать наши идеи."
                            }
                        },
                        {
                            "text": "We enjoyed a relaxed and <w>informal</w> dinner with our neighbors.",
                            "target_lang_translations": {
                                "ru": "Мы наслаждались спокойным и <w>неофициальным</w> ужином с нашими соседями."
                            }
                        }
                    ],
                    "synonyms": [
                        "not formal",
                        "colloquial"
                    ],
                    "antonyms": [
                        "formal"
                    ],
                    "common_phrases": [
                        "informal meeting",
                        "informal gathering",
                        "informal atmosphere"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий расслабленный, дружеский или неофициальный стиль; не следующий строгим правилам поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неформальный",
                                "target_lang_sense_clarification": "общий смысл; не следующий правилам"
                            },
                            {
                                "target_lang_word": "неофициальный",
                                "target_lang_sense_clarification": "не служебный, дружеский"
                            },
                            {
                                "target_lang_word": "непринуждённый",
                                "target_lang_sense_clarification": "свободный, лишенный натянутости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da2b2eff-4044-37a9-801b-b709f7050942",
                    "sense_definition": "Suitable for everyday use rather than for special or official occasions.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most people prefer to wear <w>informal</w> clothes on the weekend.",
                            "target_lang_translations": {
                                "ru": "Большинство людей предпочитают носить <w>неформальную</w> одежду в выходные."
                            }
                        },
                        {
                            "text": "The invitation says the dress code for the party is <w>informal</w>.",
                            "target_lang_translations": {
                                "ru": "В приглашении сказано, что дресс-код для вечеринки — <w>неофициальный</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "suited for everyday use"
                    ],
                    "antonyms": [
                        "formal"
                    ],
                    "common_phrases": [
                        "informal clothes",
                        "informal wear",
                        "informal dress"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходящий для повседневного использования, а не для особых или официальных случаев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неформальный",
                                "target_lang_sense_clarification": "о стиле одежды"

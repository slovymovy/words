{
    "word_family": [
        "declaration",
        "declarative",
        "declaratory",
        "declarer"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6d8d3580-7457-3c89-ba34-98f82a0f0c81",
                    "sense_definition": "To assert or announce something formally, officially, explicitly, or emphatically.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_announce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government decided to <w>declare</w> a state of emergency after the earthquake."
                        },
                        {
                            "text": "She stood up to <w>declare</w> her intention to run for office in the next election."
                        }
                    ],
                    "synonyms": [
                        "announce",
                        "proclaim",
                        "state",
                        "assert",
                        "make known"
                    ],
                    "common_phrases": [
                        "declare war",
                        "declare independence",
                        "declare bankruptcy",
                        "declare victory"
                    ]
                },
                {
                    "sense_id": "304705bc-d3c7-36a8-91cb-383e2b2ced25",
                    "sense_definition": "To inform government customs or taxation officials of goods one is importing or of income and expenses.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_announce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Travelers must <w>declare</w> any alcohol or tobacco products when entering the country."
                        },
                        {
                            "text": "He failed to <w>declare</w> his additional income on his annual tax return."
                        }
                    ],
                    "synonyms": [
                        "disclose",
                        "make known"
                    ],
                    "common_phrases": [
                        "nothing to declare",
                        "goods to declare",
                        "items to declare"
                    ]
                },
                {
                    "sense_id": "8a81d5cd-e9c9-3a65-8f1b-67762464792b",
                    "sense_definition": "To explicitly establish the existence of a variable, function, or object in a computer program without necessarily describing its content.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>declare</w> the variable as an integer before you can use it in the calculation."
                        },
                        {
                            "text": "The compiler threw an error because I forgot to <w>declare</w> the function at the top of the script."
                        }
                    ],
                    "synonyms": [
                        "define",
                        "initialize"
                    ],
                    "antonyms": [
                        "deinitialize"
                    ],
                    "common_phrases": [
                        "declare a variable",
                        "declare a function"
                    ]
                },
                {
                    "sense_id": "b1220772-e587-31ce-b627-36e35bcf8088",
                    "sense_definition": "To voluntarily end an innings in cricket before ten wickets have fallen, usually to save time.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain decided to <w>declare</w> at 500 runs to give his bowlers enough time to dismiss the opposition."
                        },
                        {
                            "text": "They <w>declared</w> just before tea, hoping to take a few quick wickets in the evening session."
                        }
                    ],
                    "common_phrases": [
                        "declare the innings"
                    ]
                },
                {
                    "sense_id": "8b80d2e5-8e2c-3fbf-b6c4-b91f1b73e8fd",
                    "sense_definition": "To show one's cards in a game in order to score points or claim a win.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the final round of betting, the players had to <w>declare</w> their hands."
                        },
                        {
                            "text": "She <w>declared</w> a full house and collected the chips from the center of the table."
                        }
                    ]
                },
                {
                    "sense_id": "2b691aff-d999-3b49-8ca4-bd4cbd112161",
                    "sense_definition": "To make clear, explain, or interpret something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_announce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient prophet stood before the crowd to <w>declare</w> the meaning of the vision."
                        },
                        {
                            "text": "The heavens <w>declare</w> the glory of the creator in this poetic verse."
                        }
                    ],
                    "synonyms": [
                        "make known"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "often biblical or literary"
                        }
                    ]
                }
            ]
        }
    ]
}
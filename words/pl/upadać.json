{
    "word_family": [
        "upadek",
        "upadłość",
        "upadły"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "751b6ffa-3844-3ec1-9e07-4841c23b2566",
                    "sense_definition": "Przewracać się na ziemię lub podłogę, tracąc równowagę z pozycji stojącej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe dziecko często <w>upada</w>, kiedy uczy się stawiać pierwsze kroki.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок часто <w>падает</w>, когда учится делать первые шаги."
                            }
                        },
                        {
                            "text": "Biegacz był tak zmęczony, że niemal <w>upadał</w> na mecie wyścigu.",
                            "target_lang_translations": {
                                "ru": "Бегун так устал, что едва не <w>падал</w> на финише гонки."
                            }
                        }
                    ],
                    "synonyms": [
                        "przewracać się",
                        "wywracać się"
                    ],
                    "common_phrases": [
                        "upadać na twarz",
                        "upadać na kolana",
                        "ciężko upadać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Падать на землю или пол, теряя равновесие из положения стоя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "терять равновесие"
                            },
                            {
                                "target_lang_word": "валиться",
                                "target_lang_sense_clarification": "падать от усталости или слабости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bad4ef1-2788-3338-9abe-b388abe052d3",
                    "sense_definition": "Przestawać istnieć, tracić znaczenie lub kończyć się niepowodzeniem (o państwach, firmach, planach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ich rodzinna firma niestety <w>upada</w> z powodu braku nowych klientów.",
                            "target_lang_translations": {
                                "ru": "Их семейная фирма, к сожалению, <w>разоряется</w> из-за нехватки новых клиентов."
                            }
                        },
                        {
                            "text": "Starożytne imperium zaczęło powoli <w>upadać</w> przez liczne wojny i korupcję.",
                            "target_lang_translations": {
                                "ru": "Древняя империя начала медленно <w>приходить в упадок</w> из-за многочисленных войн и коррупции."
                            }
                        }
                    ],
                    "synonyms": [
                        "bankrutować",
                        "załamywać się",
                        "rozpadać się",
                        "runąć"
                    ],
                    "common_phrases": [
                        "rząd upada",
                        "firma upada",
                        "mit upada"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать существовать, терять значение или терпеть неудачу (о государствах, фирмах, планах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рушиться",
                                "target_lang_sense_clarification": "о планах, империях"
                            },
                            {
                                "target_lang_word": "приходить в упадок",
                                "target_lang_sense_clarification": "слабеть, терять значение"
                            },
                            {
                                "target_lang_word": "разоряться",
                                "target_lang_sense_clarification": "о фирмах, банкротство"
                            },
                            {
                                "target_lang_word": "разваливаться",
                                "target_lang_sense_clarification": "распадаться на части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ae809e8-9e5d-3c39-a28d-ab83c3084309",
                    "sense_definition": "Przemieszczać się w dół w powietrzu pod wpływem grawitacji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jesienią każdy suchy liść powoli <w>upada</w> na wilgotną trawę w parku.",
                            "target_lang_translations": {
                                "ru": "Осенью каждый сухой лист медленно <w>падает</w> на влажную траву в парке."
                            }
                        },
                        {
                            "text": "Widzieliśmy, jak kamień rzucony ze skały <w>upadał</w> w głęboką przepaść.",
                            "target_lang_translations": {
                                "ru": "Мы видели, как брошенный со скалы камень <w>падал</w> в глубокую пропасть."
                            }
                        }
                    ],
                    "synonyms": [
                        "spadać",
                        "opadać",
                        "zlatywać"
                    ],
                    "common_phrases": [
                        "upadać na ziemię",
                        "upadać nisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться вниз по воздуху под действием гравитации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "перемещаться вниз"
                            },
                            {
                                "target_lang_word": "спускаться",
                                "target_lang_sense_clarification": "двигаться вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06f5833b-979d-3aab-aa79-1ce0820e545e",
                    "sense_definition": "Tracić moralność, godność lub dobre imię; postępować w sposób nieetyczny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Człowiek, który kłamie i oszukuje przyjaciół, powoli <w>upada</w> moralnie.",
                            "target_lang_translations": {
                                "ru": "Человек, который лжет и обманывает друзей, медленно <w>опускается</w> нравственно."
                            }
                        },
                        {
                            "text": "Bohater tej powieści <w>upadał</w> coraz niżej, pogrążając się w nałogach.",
                            "target_lang_translations": {
                                "ru": "Герой этого романа <w>опускался</w> всё ниже, погружаясь в пороки."
                            }
                        }
                    ],
                    "synonyms": [
                        "staczać się"
                    ],
                    "common_phrases": [
                        "upadać moralnie",
                        "nisko upadać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Терять нравственность, достоинство или доброе имя; поступать неэтично."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опускаться",
                                "target_lang_sense_clarification": "терять достоинство"
                            },
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "нравственно"
                            },
                            {
                                "target_lang_word": "деградировать",
                                "target_lang_sense_clarification": "ухудшаться морально"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
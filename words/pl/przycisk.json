    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "593ac8ff-b0ec-3f82-9691-a4090118158c",
                    "sense_definition": "Ruchomy element mechanizmu lub urządzenia, który naciska się palcem, aby włączyć, wyłączyć lub sterować jakąś funkcją.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "device_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naciśnij czerwony <w>przycisk</w>, aby natychmiast zatrzymać maszynę.",
                            "target_lang_translations": {
                                "ru": "Нажми красную <w>кнопку</w>, чтобы немедленно остановить машину."
                            }
                        },
                        {
                            "text": "Ten mały <w>przycisk</w> służy do włączania światła na klatce schodowej.",
                            "target_lang_translations": {
                                "ru": "Эта маленькая <w>кнопка</w> служит для включения света на лестничной клетке."
                            }
                        }
                    ],
                    "synonyms": [
                        "guzik",
                        "pstryczek",
                        "pipsztyk"
                    ],
                    "common_phrases": [
                        "przycisk zasilania",
                        "wcisnąć przycisk",
                        "przycisk alarmowy",
                        "nacisnąć przycisk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижный элемент механизма или устройства, на который нажимают пальцем для включения, выключения или управления какой-либо функцией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кнопка",
                                "target_lang_sense_clarification": "механическое устройство"
                            },
                            {
                                "target_lang_word": "клавиша",
                                "target_lang_sense_clarification": "переключатель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4365428c-ce40-3a7b-bfce-d4ea0f590e0b",
                    "sense_definition": "Element graficzny na ekranie komputera lub telefonu, który uruchamia określoną akcję po kliknięciu lub dotknięciu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "interface_element",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kliknij <w>przycisk</w> \"Dalej\", aby przejść do następnego etapu instalacji.",
                            "target_lang_translations": {
                                "ru": "Нажми <w>кнопку</w> «Далее», чтобы перейти к следующему этапу установки."
                            }
                        },
                        {
                            "text": "Na stronie internetowej znajduje się <w>przycisk</w> umożliwiający szybki kontakt z obsługą.",
                            "target_lang_translations": {
                                "ru": "На веб-сайте находится <w>кнопка</w>, позволяющая быстро связаться с поддержкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przycisk myszy",
                        "kliknąć przycisk",
                        "lewy przycisk",
                        "prawy przycisk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Графический элемент на экране компьютера или телефона, который запускает определенное действие при клике или касании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кнопка",
                                "target_lang_sense_clarification": "элемент интерфейса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cfcfd03-dada-3f7c-873b-e0b80cbd4dd8",
                    "sense_definition": "Czynność mocnego naciskania na coś lub siła tego nacisku.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "action_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciężkie drzwi otworzyły się dopiero pod silnym <w>przyciskiem</w> jego ramienia.",
                            "target_lang_translations": {
                                "ru": "Тяжелая дверь открылась только под сильным <w>нажимом</w> его плеча."
                            }
                        },
                        {
                            "text": "Ten czuły mechanizm reaguje nawet na bardzo lekki <w>przycisk</w>.",
                            "target_lang_translations": {
                                "ru": "Этот чувствительный механизм реагирует даже на очень легкое <w>нажатие</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nacisk",
                        "przyciśnięcie",
                        "przyciskanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие сильного нажатия на что-либо или сила этого нажатия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нажим",
                                "target_lang_sense_clarification": "сила давления"
                            },
                            {
                                "target_lang_word": "нажатие",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "давление",
                                "target_lang_sense_clarification": "физическая сила"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d90237a0-2163-3869-b64f-a6c2f34ee4d9",
                    "sense_definition": "Ciężki przedmiot, często ozdobny, kładziony na dokumentach, aby zapobiec ich przesuwaniu się lub zwianiu przez wiatr.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "office_item",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na dębowym biurku prezesa leżał elegancki, szklany <w>przycisk</w> do papieru.",
                            "target_lang_translations": {
                                "ru": "На дубовом столе президента лежало элегантное стеклянное <w>пресс-папье</w>."
                            }
                        },
                        {
                            "text": "Kupiłam ciężki <w>przycisk</w> w kształcie kuli, żeby wiatr nie porywał moich notatek.",
                            "target_lang_translations": {
                                "ru": "Я купила тяжелое <w>пресс-папье</w> в форме шара, чтобы ветер не уносил мои заметки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przycisk do papieru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжелый предмет, часто декоративный, который кладут на документы, чтобы предотвратить их смещение или сдувание ветром."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пресс-папье",
                                "target_lang_sense_clarification": "груз для бумаг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d839667d-a986-3018-9fa5-53212df38eb9",
                    "sense_definition": "Wyróżnienie sylaby lub wyrazu w mowie poprzez silniejszą artykulację i zwiększenie siły głosu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W języku polskim <w>przycisk</w> pada zazwyczaj na przedostatnią sylabę wyrazu.",
                            "target_lang_translations": {
                                "ru": "В польском языке <w>ударение</w> падает обычно на предпоследний слог слова."
                            }
                        },
                        {
                            "text": "Aktor wypowiadał swoje kwestie z wyraźnym <w>przyciskiem</w> na słowa oznaczające emocje.",
                            "target_lang_translations": {
                                "ru": "Актер произносил свои реплики с явным <w>ударением</w> на словах, обозначающих эмоции."
                            }
                        }
                    ],
                    "synonyms": [
                        "akcent",
                        "nacisk",
                        "akcent dynamiczny",
                        "akcent przyciskowy"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical / linguistic context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделение слога или слова в речи посредством более сильной артикуляции и увеличения силы голоса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ударение",
                                "target_lang_sense_clarification": "выделение слога"
                            },
                            {
                                "target_lang_word": "акцент",
                                "target_lang_sense_clarification": "выделение слова"
                            }
                        ]
                    }
                }
            ]
        }

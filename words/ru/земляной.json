            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "395c2dce-2196-3aeb-9033-5bf925c09085",
                    "sense_definition": "Состоящий из земли, сделанный из грунта или почвы.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старой деревенской избе был утрамбованный <w>земляной</w> пол.",
                            "target_lang_translations": {
                                "en": "The old village hut had a packed <w>earthen</w> floor."
                            }
                        },
                        {
                            "text": "Вокруг лагеря солдаты возвели высокий <w>земляной</w> вал для защиты от врага.",
                            "target_lang_translations": {
                                "en": "Around the camp, the soldiers erected a high <w>earthen</w> rampart for protection against the enemy."
                            }
                        }
                    ],
                    "synonyms": [
                        "грунтовый"
                    ],
                    "antonyms": [
                        "каменный",
                        "бетонный",
                        "деревянный"
                    ],
                    "common_phrases": [
                        "земляной пол",
                        "земляной вал",
                        "земляная плотина",
                        "земляная насыпь"
                    ],
                    "target_lang_definitions": {
                        "en": "Consisting of earth, made of ground or soil."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earthen",
                                "target_lang_sense_clarification": "made of compressed earth or clay"
                            },
                            {
                                "target_lang_word": "earth",
                                "target_lang_sense_clarification": "used attributively to describe construction materials"
                            },
                            {
                                "target_lang_word": "dirt",
                                "target_lang_sense_clarification": "informal; made of packed soil"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6b51f0f-eeb9-355e-8f10-750be0e6c4e0",
                    "sense_definition": "Живущий, растущий или находящийся в верхнем слое почвы; связанный с обработкой или выемкой грунта.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После сильного дождя на асфальт выполз длинный <w>земляной</w> червь.",
                            "target_lang_translations": {
                                "en": "After a heavy rain, a long <w>earth</w>worm crawled out onto the asphalt."
                            }
                        },
                        {
                            "text": "Строители начали масштабные <w>земляные</w> работы для прокладки новых труб.",
                            "target_lang_translations": {
                                "en": "The builders began large-scale <w>earth</w>works to lay new pipes."
                            }
                        }
                    ],
                    "synonyms": [
                        "подземный"
                    ],
                    "antonyms": [
                        "надземный"
                    ],
                    "common_phrases": [
                        "земляной червь",
                        "земляной орех",
                        "земляные работы",
                        "земляной ком"
                    ],
                    "target_lang_definitions": {
                        "en": "Living, growing, or located in the top layer of soil; related to soil cultivation or excavation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earth",
                                "target_lang_sense_clarification": "used in compounds denoting connection to the soil (e.g., earthworm, earthworks)"
                            },
                            {
                                "target_lang_word": "ground",
                                "target_lang_sense_clarification": "related to the ground or soil"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be10887e-2e67-36a7-a3f1-662a98b22040",
                    "sense_definition": "Напоминающий землю (почву) по запаху или другим свойствам.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "characteristic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из открытого подвала тянуло сыростью и тяжёлым <w>земляным</w> запахом.",
                            "target_lang_translations": {
                                "en": "Dampness and a heavy <w>earthy</w> smell drifted from the open basement."
                            }
                        },
                        {
                            "text": "На руках у садовника остался стойкий <w>земляной</w> налёт после работы на грядках.",
                            "target_lang_translations": {
                                "en": "A persistent <w>earthy</w> residue remained on the gardener's hands after working in the beds."
                            }
                        }
                    ],
                    "common_phrases": [
                        "земляной запах",
                        "земляной дух"
                    ],
                    "target_lang_definitions": {
                        "en": "Resembling earth (soil) in smell or other properties."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earthy",
                                "target_lang_sense_clarification": "having the smell, taste, or texture of soil"
                            }
                        ]
                    }
                }
            ]
        }

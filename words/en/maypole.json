    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "057c94be-96b8-3d1b-adcf-058f252fc6fb",
                    "sense_definition": "A tall wooden pole decorated with flowers, greenery, and ribbons, around which people dance during traditional May Day celebrations.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "may_day_pole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children gathered on the village green to dance around the <w>maypole</w>.",
                            "target_lang_translations": {
                                "ru": "Дети собрались на деревенской лужайке, чтобы потанцевать вокруг <w>майского дерева</w>."
                            }
                        },
                        {
                            "text": "Colorful ribbons were woven into complex patterns as the dancers circled the <w>maypole</w>.",
                            "target_lang_translations": {
                                "ru": "Разноцветные ленты сплетались в сложные узоры, пока танцоры кружили вокруг <w>майского дерева</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dance around the maypole",
                        "maypole ribbons",
                        "maypole dancing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокий деревянный столб, украшенный цветами, зеленью и лентами, вокруг которого танцуют во время традиционных празднований Первого мая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "майское дерево",
                                "target_lang_sense_clarification": "праздничный атрибут"
                            },
                            {
                                "target_lang_word": "майский шест",
                                "target_lang_sense_clarification": "столб для танцев"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "418577c7-e0d0-3862-bf2e-b144131bc3a6",
                    "sense_definition": "A very tall and thin person, typically a young woman.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "metaphor_tall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She felt like a <w>maypole</w> standing next to her much shorter friends.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала себя <w>каланчой</w>, стоя рядом со своими гораздо более низкими подругами."
                            }
                        },
                        {
                            "text": "He teased his sister for growing into a <w>maypole</w> over the summer.",
                            "target_lang_translations": {
                                "ru": "Он дразнил сестру за то, что за лето она вымахала в <w>жердь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "beanpole"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень высокий и худой человек, обычно молодая женщина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каланча",
                                "target_lang_sense_clarification": "разговорное, о высоком человеке"
                            },
                            {
                                "target_lang_word": "жердь",
                                "target_lang_sense_clarification": "разговорное, о худом и высоком человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11c96986-5e79-3eba-855f-c57b7fb022d7",
                    "sense_definition": "A structure made of sticks built around a sapling by certain species of bowerbirds to attract a mate.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The male golden bowerbird constructs an elaborate <w>maypole</w> to impress potential mates.",
                            "target_lang_translations": {
                                "ru": "Самец золотого шалашника строит сложное сооружение типа <w>«майское дерево»</w>, чтобы впечатлить потенциальных партнерш."
                            }
                        },
                        {
                            "text": "Researchers observed the bird decorating its <w>maypole</w> with lichen and flowers.",
                            "target_lang_translations": {
                                "ru": "Исследователи наблюдали, как птица украшала свое <w>«майское дерево»</w> лишайником и цветами."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сооружение из палочек, построенное вокруг молодого деревца некоторыми видами птиц-шалашников для привлечения самки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "«майское дерево»",
                                "target_lang_sense_clarification": "тип гнезда-шалаша"
                            },
                            {
                                "target_lang_word": "шалаш",
                                "target_lang_sense_clarification": "сооружение птиц"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "740519c9-32d8-304d-a9ef-b3fd6ac39831",
                    "sense_definition": "A penis, especially one of large size.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "metaphor_tall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bawdy novel used the term <w>maypole</w> to describe the character's anatomy.",
                            "target_lang_translations": {
                                "ru": "В непристойном романе термин <w>«майский шест»</w> использовался для описания анатомии персонажа."
                            }
                        },
                        {
                            "text": "He made a crude joke comparing himself to a <w>maypole</w>.",
                            "target_lang_translations": {
                                "ru": "Он отпустил грубую шутку, сравнив себя с <w>майским шестом</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пенис, особенно большого размера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "елда",
                                "target_lang_sense_clarification": "вульгарное"
                            },
                            {
                                "target_lang_word": "дрын",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0590e109-c360-3ce0-bfef-25dd269f2ab0",
                    "sense_definition": "To move, dance, or spin in a circle around a central point or object.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The excited puppy <w>maypoled</w> around my legs when I got home.",
                            "target_lang_translations": {
                                "ru": "Взволнованный щенок <w>вился</w> вокруг моих ног, когда я вернулся домой."
                            }
                        },
                        {
                            "text": "The fishing line <w>maypoled</w> itself around the rod, creating a tangled mess.",
                            "target_lang_translations": {
                                "ru": "Леска <w>намоталась</w> на удилище, создав запутанный клубок."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться, танцевать или вращаться по кругу вокруг центральной точки или объекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружиться",
                                "target_lang_sense_clarification": "вокруг чего-либо"
                            },
                            {
                                "target_lang_word": "обвиваться",
                                "target_lang_sense_clarification": "наматываться вокруг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7531ac68-30a0-3db1-a098-e6c6096a5fa0",
                    "sense_definition": "A name used for several settlements in England and Wales, such as in Kent, Greater London, or Monmouthshire.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They visited a small village called <w>Maypole</w> during their trip to Kent.",
                            "target_lang_translations": {
                                "ru": "Во время поездки в Кент они посетили маленькую деревню под названием <w>Мейпол</w>."
                            }
                        },
                        {
                            "text": "The map shows a settlement named <w>Maypole</w> near the coast.",
                            "target_lang_translations": {
                                "ru": "На карте показано поселение под названием <w>Мейпол</w> недалеко от побережья."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название нескольких поселений в Англии и Уэльсе, например, в Кенте, Большом Лондоне или Монмутшире."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мейпол",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df928db9-1923-32c6-b295-8bca07116a1f",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Maypole</w> was known for his bakery in the town square.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Мейпол</w> был известен своей пекарней на городской площади."
                            }
                        },
                        {
                            "text": "We looked up the <w>Maypole</w> family history in the local archives.",
                            "target_lang_translations": {
                                "ru": "Мы искали историю семьи <w>Мейпол</w> в местных архивах."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мейпол",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }

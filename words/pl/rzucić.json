            "pos": "verb",
            "senses": [
                {
                    "sense_id": "52447db2-8f2c-3513-ac64-0c379f1f1bdc",
                    "sense_definition": "Gwałtownie przemieścić przedmiot w powietrzu, używając siły rąk; sprawić, że obiekt leci.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawodnik mocno <rzucił> piłkę w stronę bramki.",
                            "target_lang_translations": {
                                "ru": "Игрок сильно <бросил> мяч в сторону ворот."
                            }
                        },
                        {
                            "text": "Nie wolno <rzucić> kamieniem w okno sąsiada.",
                            "target_lang_translations": {
                                "ru": "Нельзя <бросать> камни в окно соседа."
                            }
                        }
                    ],
                    "synonyms": [
                        "pobiec",
                        "przelać",
                        "potrząsnąć",
                        "wyprawić",
                        "wypuścić",
                        "napaść",
                        "popędzić",
                        "zwymiotować",
                        "grabić",
                        "zagarniać"
                    ],
                    "antonyms": [
                        "złapać",
                        "chwycić"
                    ],
                    "common_phrases": [
                        "rzucić piłkę",
                        "rzucić kamieniem",
                        "rzucić monetą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко переместить предмет в воздухе, используя силу рук; заставить объект лететь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросить",
                                "target_lang_sense_clarification": "предмет"
                            },
                            {
                                "target_lang_word": "кинуть",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b19eed9-2df7-3fa6-ac4c-1bba1835ff95",
                    "sense_definition": "Zrezygnować z czegoś, przestać coś robić (np. pracę, szkołę lub nałóg).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "termination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój tata postanowił <rzucić> palenie dla zdrowia.",
                            "target_lang_translations": {
                                "ru": "Мой папа решил <бросить> курить ради здоровья."
                            }
                        },
                        {
                            "text": "Ona chce <rzucić> pracę i wyjechać w Bieszczady.",
                            "target_lang_translations": {
                                "ru": "Она хочет <бросить> работу и уехать в Бещады."
                            }
                        }
                    ],
                    "synonyms": [
                        "porzucić",
                        "zwymiotować",
                        "grabić",
                        "zagarniać"
                    ],
                    "antonyms": [
                        "zacząć",
                        "kontynuować"
                    ],
                    "common_phrases": [
                        "rzucić palenie",
                        "rzucić pracę",
                        "rzucić szkołę",
                        "rzucić studia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказаться от чего-либо, перестать что-то делать (например, работу, учебу или вредную привычку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросить",
                                "target_lang_sense_clarification": "привычку, дело"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab4b9827-6c32-3bdb-8485-7d6a10f715c1",
                    "sense_definition": "Skierować wzrok, światło lub cień na jakiś obiekt (często używane w wyrażeniach metaforycznych).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "projection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <rzucić> okiem na ten raport i sprawdzić błędy.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <бросьте> взгляд на этот отчет и проверьте ошибки."
                            }
                        },
                        {
                            "text": "Wielkie drzewo <rzuciło> cień na nasz ogród.",
                            "target_lang_translations": {
                                "ru": "Большое дерево <отбросило> тень на наш сад."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzucić okiem",
                        "rzucić spojrzenie",
                        "rzucić cień",
                        "rzucić światło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направить взгляд, свет или тень на какой-либо объект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросить",
                                "target_lang_sense_clarification": "взгляд, свет"
                            },
                            {
                                "target_lang_word": "отбросить",
                                "target_lang_sense_clarification": "тень"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2abe239-239b-3229-93c8-b0b5cd186e80",
                    "sense_definition": "Gwałtownie zaatakować kogoś fizycznie lub ruszyć w czyimś kierunku (używane z zaimkiem zwrotnym 'się').",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pies <rzucił> się na listonosza, gdy tylko otworzyłem furtkę.",
                            "target_lang_translations": {
                                "ru": "Собака <бросилась> на почтальона, как только я открыл калитку."
                            }
                        },
                        {
                            "text": "Złodziej <rzucił> się do ucieczki na widok policji.",
                            "target_lang_translations": {
                                "ru": "Вор <бросился> бежать при виде полиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "napaść",
                        "pobiec",
                        "popędzić"
                    ],
                    "common_phrases": [
                        "rzucić się na kogoś",
                        "rzucić się do ucieczki",
                        "rzucić się w wir"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко атаковать кого-то физически или устремиться в чьем-то направлении (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "броситься",
                                "target_lang_sense_clarification": "в атаку, бежать"
                            },
                            {
                                "target_lang_word": "кинуться",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "575e0fa1-c150-3f57-8331-59d50d5e7146",
                    "sense_definition": "Zakończyć z kimś związek romantyczny; zerwać z partnerem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "termination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek <rzucił> swoją dziewczynę tuż przed walentynkami.",
                            "target_lang_translations": {
                                "ru": "Марек <бросил> свою девушку прямо перед Днем святого Валентина."
                            }
                        },
                        {
                            "text": "Bała się, że mąż ją <rzuci> dla innej kobiety.",
                            "target_lang_translations": {
                                "ru": "Она боялась, что муж её <бросит> ради другой женщины."
                            }
                        }
                    ],
                    "synonyms": [
                        "opuścić",
                        "porzucić"
                    ],
                    "common_phrases": [
                        "rzucić chłopaka",
                        "rzucić dziewczynę",
                        "rzucić męża"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Закончить с кем-то романтические отношения; расстаться с партнером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросить",
                                "target_lang_sense_clarification": "кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38d34663-5862-3f42-b5ed-9fcbec871022",
                    "sense_definition": "Szybko ubrać coś na siebie; nałożyć ubranie w pośpiechu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zrobiło się zimno, więc <rzuciła> szal na ramiona.",
                            "target_lang_translations": {
                                "ru": "Стало холодно, поэтому она <накинула> шаль на плечи."
                            }
                        },
                        {
                            "text": "Muszę tylko <rzucić> coś na siebie i możemy wychodzić.",
                            "target_lang_translations": {
                                "ru": "Мне нужно только <накинуть> что-то на себя, и можем выходить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzucić coś na siebie",
                        "rzucić na grzbiet"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро надеть что-то на себя; накинуть одежду в спешке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накинуть",
                                "target_lang_sense_clarification": "одежду"
                            },
                            {
                                "target_lang_word": "набросить",
                                "target_lang_sense_clarification": "на плечи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d394f60e-8acd-3bc2-9db5-bd3c6b7dd265",
                    "sense_definition": "Wypowiedzieć oskarżenie, przekleństwo lub urok w czyimś kierunku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "projection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zła czarownica <rzuciła> klątwę na całe królestwo.",
                            "target_lang_translations": {
                                "ru": "Злая ведьма <наложила> проклятие на всё королевство."
                            }
                        },
                        {
                            "text": "Nie możesz bezpodstawnie <rzucić> oskarżenia na niewinną osobę.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь безосновательно <бросить> обвинение в адрес невиновного человека."
                            }
                        }
                    ],
                    "antonyms": [
                        "zdjąć"
                    ],
                    "common_phrases": [
                        "rzucić urok",
                        "rzucić klątwę",
                        "rzucić oskarżenie",
                        "rzucić hasło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высказать обвинение, проклятие или наложить чары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наложить",
                                "target_lang_sense_clarification": "заклятие"
                            },
                            {
                                "target_lang_word": "бросить",
                                "target_lang_sense_clarification": "обвинение, слова"
                            }
                        ]
                    }
                }
            ]

                    ],
                    "antonyms": [
                        "bescheiden"
                    ],
                    "common_phrases": [
                        "grootse plannen",
                        "groots aanpakken",
                        "groots onthaal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Впечатляющий, возвышенный или организованный с большим размахом и амбициями."

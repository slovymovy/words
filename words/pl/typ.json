                    "sense_id": "a24f32d0-cdd2-3470-9de7-454d97540ef2",
                    "sense_definition": "Zespół cech charakterystycznych dla danej grupy przedmiotów, zjawisk lub osób; także grupa posiadająca te cechy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "kind_category",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie lubię filmów tego <w>typu</w>, ponieważ zazwyczaj są dla mnie zbyt brutalne.",
                            "target_lang_translations": {
                                "ru": "Я не люблю фильмы этого <w>типа</w>, так как они обычно слишком жестоки для меня."
                            }
                        },
                        {
                            "text": "W tym sklepie można znaleźć różnego <w>typu</w> akcesoria do telefonów komórkowych.",
                            "target_lang_translations": {
                                "ru": "В этом магазине можно найти различного <w>типа</w> аксессуары для мобильных телефонов."
                            }
                        }
                    ],
                    "synonyms": [
                        "rodzaj",
                        "autorament",
                        "model"
                    ],
                    "common_phrases": [
                        "tego typu",
                        "różnego typu",
                        "w swoim typie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность черт, характерных для определенной группы предметов или явлений; разновидность."
                    },

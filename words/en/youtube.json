                            {
                                "target_lang_word": "ролик на YouTube",
                                "target_lang_sense_clarification": "контекстное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d4904b5-3fc6-3451-96b3-196d7b73a456",
                    "sense_definition": "A personal channel or profile on the YouTube website.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "content_channel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Check out my <w>YouTube</w> for more weekly tutorials.",
                            "target_lang_translations": {
                                "ru": "Загляни на мой <w>YouTube-канал</w> за новыми еженедельными уроками."
                            }
                        },
                        {
                            "text": "The theater company streams plays directly on its <w>YouTube</w> every Thursday.",
                            "target_lang_translations": {
                                "ru": "Театральная труппа транслирует спектакли прямо на своем <w>YouTube-канале</w> каждый четверг."
                            }
                        }
                    ],
                    "synonyms": [
                        "channel"
                    ],
                    "traits": [
                        {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c739718b-182e-3d31-8794-0df2a96701f2",
                    "sense_definition": "Наносить на своё лицо косметическое средство (румяна) для придания коже розового или красноватого оттенка.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "makeup",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед выходом на сцену актриса начала густо <нарумяниваться>.",
                            "target_lang_translations": {
                                "en": "Before going on stage, the actress began to heavily <w>put on rouge</w>."
                            }
                        },
                        {
                            "text": "Она любила <нарумяниваться> перед зеркалом, чтобы выглядеть свежее.",
                            "target_lang_translations": {
                                "en": "She loved to <w>apply blush</w> in front of the mirror to look fresher."
                            }
                        }
                    ],
                    "synonyms": [
                        "румяниться",
                        "краситься",
                        "подкрашиваться"
                    ],
                    "common_phrases": [
                        "сильно нарумяниваться",
                        "нарумяниваться перед зеркалом"
                    ],
                    "target_lang_definitions": {
                        "en": "To apply a cosmetic product (rouge or blush) to one's face to give the skin a pink or reddish tint."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put on rouge",
                                "target_lang_sense_clarification": "apply makeup"
                            },
                            {
                                "target_lang_word": "apply blush",
                                "target_lang_sense_clarification": "apply makeup"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "959c1950-e88f-3ccd-823b-85d6730bf342",
                    "sense_definition": "Становиться розовым или красным, приобретать здоровый цвет лица (обычно от мороза, волнения или здоровья).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "color_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На свежем воздухе бледные щёки детей стали быстро <нарумяниваться>.",
                            "target_lang_translations": {
                                "en": "In the fresh air, the children's pale cheeks quickly began to <w>get rosy</w>."
                            }
                        },
                        {
                            "text": "От быстрой ходьбы её лицо начало слегка <нарумяниваться>.",
                            "target_lang_translations": {
                                "en": "From walking fast, her face began to slightly <w>flush</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "краснеть",
                        "розоветь",
                        "румяниться"
                    ],
                    "antonyms": [
                        "бледнеть"
                    ],
                    "common_phrases": [
                        "слегка нарумяниваться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become pink or red, to acquire a healthy complexion (usually from frost, excitement, or health)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get rosy",
                                "target_lang_sense_clarification": "become pink"
                            },
                            {
                                "target_lang_word": "flush",
                                "target_lang_sense_clarification": "turn red"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "barbarzyńca",
        "barbarzyński",
        "barbarzyńsko"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ef199117-5099-35d9-a9b4-089aa48055db",
                    "sense_definition": "Okrutne, nieludzkie zachowanie, gwałt zadawany prawom ludzkim i moralnym; czyn świadczący o bestialstwie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wojna to czyste <w>barbarzyństwo</w>, które niszczy wszystko na swojej drodze.",
                            "target_lang_translations": {
                                "ru": "Война — это чистое <w>варварство</w>, которое уничтожает всё на своем пути."
                            }
                        },
                        {
                            "text": "To, jak potraktowano bezbronnych jeńców, było jawnym <w>barbarzyństwem</w>.",
                            "target_lang_translations": {
                                "ru": "То, как обошлись с беззащитными пленными, было явным <w>варварством</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "barbaria",
                        "prymitywizm",
                        "prostactwo"
                    ],
                    "antonyms": [
                        "humanitaryzm",
                        "ludzkość"
                    ],
                    "common_phrases": [
                        "akt barbarzyństwa",
                        "czyste barbarzyństwo",
                        "dopuścić się barbarzyństwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жестокое, бесчеловечное поведение, нарушение человеческих и моральных законов; поступок, свидетельствующий о зверстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "варварство",
                                "target_lang_sense_clarification": "жестокость"
                            },
                            {
                                "target_lang_word": "зверство",
                                "target_lang_sense_clarification": "крайняя жестокость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5cbb904-50d2-3971-9cfb-23e2d432d9ba",
                    "sense_definition": "Postępowanie świadczące o braku kultury, ogłady i szacunku dla sztuki, tradycji lub dorobku cywilizacyjnego.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zburzenie tego zabytkowego kościoła pod budowę biurowca to prawdziwe <w>barbarzyństwo</w>.",
                            "target_lang_translations": {
                                "ru": "Снос этой старинной церкви ради строительства офисного здания — это настоящее <w>варварство</w>."
                            }
                        },
                        {
                            "text": "Wyrzucanie starych książek na śmietnik uważam za <w>barbarzyństwo</w>.",
                            "target_lang_translations": {
                                "ru": "Я считаю выбрасывание старых книг на помойку <w>варварством</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "prostactwo",
                        "prymitywizm",
                        "barbaria"
                    ],
                    "antonyms": [
                        "kultura",
                        "cywilizacja"
                    ],
                    "common_phrases": [
                        "szczyt barbarzyństwa",
                        "przejaw barbarzyństwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поведение, свидетельствующее об отсутствии культуры, воспитания и уважения к искусству или традициям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "варварство",
                                "target_lang_sense_clarification": "бескультурье"
                            },
                            {
                                "target_lang_word": "дикость",
                                "target_lang_sense_clarification": "невежество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76798684-e5db-353e-b736-3e175d9f80da",
                    "sense_definition": "Stan braku cywilizacji, dzikość lub okres w historii ludzkości poprzedzający rozwój cywilizacyjny.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po upadku wielkiego imperium region ten ponownie popadł w <w>barbarzyństwo</w>.",
                            "target_lang_translations": {
                                "ru": "После падения великой империи этот регион снова впал в <w>варварство</w>."
                            }
                        },
                        {
                            "text": "Dawne plemiona żyły w stanie <w>barbarzyństwa</w>, zanim nauczyły się uprawiać rolę.",
                            "target_lang_translations": {
                                "ru": "Древние племена жили в состоянии <w>варварства</w>, прежде чем научились возделывать землю."
                            }
                        }
                    ],
                    "synonyms": [
                        "barbaria",
                        "prymitywizm"
                    ],
                    "antonyms": [
                        "cywilizacja",
                        "rozwój"
                    ],
                    "common_phrases": [
                        "popaść w barbarzyństwo",
                        "wyjść z barbarzyństwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние отсутствия цивилизации, дикость или исторический период, предшествующий цивилизационному развитию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "варварство",
                                "target_lang_sense_clarification": "историческая стадия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
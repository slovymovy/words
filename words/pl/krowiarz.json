            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e66473c9-7140-3283-b617-5221edde85c2",
                    "sense_definition": "Osoba zajmująca się wypasaniem krów i pilnowaniem ich na pastwisku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "occupation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody <w>krowiarz</w> zaganiał bydło do zagrody przed nadchodzącą burzą.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>пастух</w> загонял скот в загон перед надвигающейся грозой."
                            }
                        },
                        {
                            "text": "Na skraju lasu odpoczywał zmęczony <w>krowiarz</w>, obserwując pasące się zwierzęta.",
                            "target_lang_translations": {
                                "ru": "На опушке леса отдыхал уставший <w>пастух</w>, наблюдая за пасущимися животными."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasterz",
                        "pastuch"
                    ],
                    "common_phrases": [
                        "stary krowiarz",
                        "praca krowiarza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, занимающийся выпасом коров и присмотром за ними на пастбище."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пастух",
                                "target_lang_sense_clarification": "человек, пасущий скот"
                            },
                            {
                                "target_lang_word": "пастух коров",
                                "target_lang_sense_clarification": "уточнение специализации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62d3dad9-c80f-3905-bff4-4d5b951720d7",
                    "sense_definition": "Rolnik lub przedsiębiorca zajmujący się hodowlą bydła.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "occupation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony <w>krowiarz</w> doskonale wie, jak dbać o zdrowie swojego stada.",
                            "target_lang_translations": {
                                "ru": "Опытный <w>скотовод</w> прекрасно знает, как заботиться о здоровье своего стада."
                            }
                        },
                        {
                            "text": "Wielu lokalnych <w>krowiarzy</w> spotkało się na targach rolniczych, by omówić ceny mleka.",
                            "target_lang_translations": {
                                "ru": "Многие местные <w>скотоводы</w> встретились на сельскохозяйственной ярмарке, чтобы обсудить цены на молоко."
                            }
                        }
                    ],
                    "synonyms": [
                        "hodowca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фермер или предприниматель, занимающийся разведением крупного рогатого скота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скотовод",
                                "target_lang_sense_clarification": "специалист по разведению скота"
                            },
                            {
                                "target_lang_word": "животновод",
                                "target_lang_sense_clarification": "более широкий термин"
                            }
                        ]
                    }
                }
            ]
        },
        {

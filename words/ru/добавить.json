{
    "word_family": [
        "добавка",
        "добавление",
        "добавочный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3946a077-20f4-3f55-87e2-f4e81c5e0801",
                    "sense_definition": "Увеличить количество чего-либо или присоединить одно к другому.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "add_increase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Повар решил <w>добавить</w> в суп немного соли и перца.",
                            "target_lang_translations": {
                                "en": "The cook decided to <w>add</w> some salt and pepper to the soup."
                            }
                        },
                        {
                            "text": "Не забудьте <w>добавить</w> этот сайт в закладки браузера.",
                            "target_lang_translations": {
                                "en": "Don't forget to <w>add</w> this site to your browser bookmarks."
                            }
                        }
                    ],
                    "synonyms": [
                        "прибавить",
                        "присоединить"
                    ],
                    "antonyms": [
                        "убавить",
                        "уменьшить"
                    ],
                    "common_phrases": [
                        "добавить воды",
                        "добавить в друзья",
                        "добавить звук"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase the quantity of something or join one thing to another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "add",
                                "target_lang_sense_clarification": "to put something with something else"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "435bfc04-5c1f-3797-8883-a4c45e89b5f8",
                    "sense_definition": "Сказать или написать что-либо в дополнение к уже сказанному.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "say_additionally",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Разрешите мне <w>добавить</w> несколько слов к вашему выступлению.",
                            "target_lang_translations": {
                                "en": "Allow me to <w>add</w> a few words to your speech."
                            }
                        },
                        {
                            "text": "К этому мне больше нечего <w>добавить</w>, всё уже сказано.",
                            "target_lang_translations": {
                                "en": "I have nothing more to <w>add</w> to this; everything has already been said."
                            }
                        }
                    ],
                    "synonyms": [
                        "дополнить"
                    ],
                    "common_phrases": [
                        "хочу добавить",
                        "нечего добавить",
                        "добавить к сказанному"
                    ],
                    "target_lang_definitions": {
                        "en": "To say or write something in addition to what has already been said."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "add",
                                "target_lang_sense_clarification": "to say further information"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "04ef993b-3222-341c-ba20-b1b8b5f98738",
                    "sense_definition": "In a truthful, sincere, or fair manner; without lying or cheating.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He answered the questions <w>honestly</w> during the interview.",
                            "target_lang_translations": {
                                "ru": "Во время интервью он отвечал на вопросы <w>че́стно</w>."
                            }
                        },
                        {
                            "text": "If you speak <w>honestly</w> about your feelings, people will trust you more.",
                            "target_lang_translations": {
                                "ru": "Если ты будешь говорить о своих чувствах <w>и́скренне</w>, люди будут больше тебе доверять."
                            }
                        }
                    ],
                    "synonyms": [
                        "truthfully",
                        "sincerely",
                        "candidly",
                        "openly"
                    ],
                    "antonyms": [
                        "dishonestly",
                        "falsely"
                    ],
                    "common_phrases": [
                        "answer honestly",
                        "speak honestly",
                        "come by honestly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Честно, правдиво или искренне; без лжи и обмана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "че́стно",
                                "target_lang_sense_clarification": "правдиво, без обмана"
                            },
                            {
                                "target_lang_word": "и́скренне",
                                "target_lang_sense_clarification": "откровенно, от чистого сердца"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75406bb5-24d9-3028-8fe2-bf93e549265f",
                    "sense_definition": "Used to emphasize that what is being said is the truth, or to express a frank opinion.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Honestly</w>, I didn't believe a word she said.",
                            "target_lang_translations": {
                                "ru": "<w>Че́стно говоря</w>, я не поверил ни единому её слову."
                            }
                        },
                        {
                            "text": "I <w>honestly</w> don't know where he went after the meeting.",
                            "target_lang_translations": {
                                "ru": "Я <w>че́стно</w> не знаю, куда он пошел после собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "frankly",
                        "actually",
                        "really",
                        "truly",
                        "in fact"
                    ],
                    "common_phrases": [
                        "quite honestly",
                        "honestly speaking",
                        "I honestly think"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для подчеркивания правдивости сказанного или для выражения откровенного мнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "че́стно говоря",
                                "target_lang_sense_clarification": "вводное слово для выражения откровенности"
                            },
                            {
                                "target_lang_word": "че́стно",
                                "target_lang_sense_clarification": "на самом деле, действительно"
                            },
                            {
                                "target_lang_word": "и́скренне",
                                "target_lang_sense_clarification": "по-настоящему"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "ec9a2e5d-bd9e-35f1-adf4-cbc214c61626",
                    "sense_definition": "Used to express annoyance, exasperation, or dismay at a situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Honestly</w>! I want to finish this work and you keep interrupting.",
                            "target_lang_translations": {
                                "ru": "<w>Че́стное сло́во</w>! Я хочу закончить эту работу, а ты постоянно перебиваешь."
                            }
                        },
                        {
                            "text": "Oh <w>honestly</w>, do we have to go through this argument again?",
                            "target_lang_translations": {
                                "ru": "О, <w>ну пра́вда</w>, нам обязательно снова спорить об этом?"
                            }
                        }
                    ],
                    "synonyms": [
                        "really",
                        "seriously"
                    ],
                    "common_phrases": [
                        "oh honestly"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения раздражения, негодования или разочарования ситуацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "че́стное сло́во",
                                "target_lang_sense_clarification": "восклицание при возмущении"
                            },
                            {
                                "target_lang_word": "ну пра́вда",
                                "target_lang_sense_clarification": "выражение нетерпения или досады"
                            }
                        ]
                    }
                }
            ]
        }

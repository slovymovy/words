            "senses": [
                {
                    "sense_id": "af784ed5-4aaa-3af2-9db9-af1de9f232f5",
                    "sense_definition": "Plural of <w>liability</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The aging players became <w>liabilities</w> to the team rather than assets.",
                            "target_lang_translations": {
                                "ru": "Стареющие игроки стали для команды <w>обузой</w>, а не ценным активом."
                            }
                        },
                        {
                            "text": "Strict new laws impose heavy <w>liabilities</w> on companies that pollute the environment.",
                            "target_lang_translations": {
                                "ru": "Новые строгие законы налагают серьезную <w>ответственность</w> на компании, загрязняющие окружающую среду."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от \"liability\". Может означать помехи, недостатки или юридическую ответственность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обязательства",
                                "target_lang_sense_clarification": "юридические или договорные"
                            },
                            {
                                "target_lang_word": "ответственность",
                                "target_lang_sense_clarification": "перед законом или другими лицами"
                            },
                            {
                                "target_lang_word": "обуза",
                                "target_lang_sense_clarification": "помеха, бремя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d604b9da-9487-39ca-b471-38dd540f6ea4",
                    "sense_definition": "Financial debts or pecuniary obligations that a company or individual owes to others, which must be paid in the future.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "financial_obligations",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The corporation reported a significant increase in its total <w>liabilities</w> this quarter.",
                            "target_lang_translations": {
                                "ru": "В этом квартале корпорация сообщила о значительном увеличении своих общих <w>пассивов</w>."
                            }
                        },
                        {
                            "text": "Investors were worried because the firm's <w>liabilities</w> exceeded its assets.",
                            "target_lang_translations": {
                                "ru": "Инвесторы были обеспокоены, так как <w>пассивы</w> фирмы превышали ее активы."
                            }
                        }
                    ],
                    "synonyms": [
                        "debts",
                        "obligations",
                        "arrears"
                    ],
                    "antonyms": [
                        "assets"
                    ],
                    "common_phrases": [
                        "current liabilities",
                        "long-term liabilities",
                        "tax liabilities",
                        "total liabilities",
                        "assets and liabilities"
                    ],
                    "target_lang_definitions": {
                        "ru": "Финансовые долги или денежные обязательства компании или частного лица перед другими, подлежащие выплате."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пассивы",
                                "target_lang_sense_clarification": "бухгалтерский термин, противоположность активам"
                            },
                            {
                                "target_lang_word": "долговые обязательства",
                                "target_lang_sense_clarification": "финансовые долги"
                            },
                            {
                                "target_lang_word": "задолженность",
                                "target_lang_sense_clarification": "сумма долга"
                            }
                        ]
                    }
                }
            ]
        }
    ]

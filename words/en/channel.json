{
    "word_family": [
        "channeler",
        "multichannel",
        "omnichannel",
        "interchannel"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a0d55c5a-be8b-3966-b543-e89e5fc15a74",
                    "sense_definition": "A specific television or radio station and the programs it broadcasts.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "broadcasting_digital",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I want to watch the news, so please change the <w>channel</w>.",
                            "target_lang_translations": {
                                "ru": "Я хочу посмотреть новости, поэтому, пожалуйста, переключи <w>кана́л</w>."
                            }
                        },
                        {
                            "text": "My favorite <w>channel</w> plays cartoons all day long.",
                            "target_lang_translations": {
                                "ru": "Мой любимый <w>кана́л</w> показывает мультики весь день."
                            }
                        }
                    ],
                    "synonyms": [
                        "station"
                    ],
                    "common_phrases": [
                        "change the channel",
                        "news channel",
                        "sports channel",
                        "movie channel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная телевизионная или радиостанция и программы, которые она транслирует."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кана́л",
                                "target_lang_sense_clarification": "телевидение, радио"
                            },
                            {
                                "target_lang_word": "телекана́л",
                                "target_lang_sense_clarification": "телевидение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4467b7e-93cb-3b55-a44e-0f78ec5bb9e9",
                    "sense_definition": "A specific area or account on an internet platform used for sharing videos, content, or communicating with others.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "broadcasting_digital",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She started a cooking <w>channel</w> on the internet last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году она создала кулинарный <w>кана́л</w> в интернете."
                            }
                        },
                        {
                            "text": "Don't forget to subscribe to my <w>channel</w> for more updates.",
                            "target_lang_translations": {
                                "ru": "Не забудьте подписаться на мой <w>кана́л</w>, чтобы получать новости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "subscribe to a channel",
                        "video channel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специальная область или аккаунт на интернет-платформе, используемые для обмена видео, контентом или общения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кана́л",
                                "target_lang_sense_clarification": "в интернете (YouTube, Telegram)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85bc91c3-afdc-37a9-9f05-a8dd1e2c8c77",
                    "sense_definition": "A system, method, or route used for sending or receiving information, goods, or complaints.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "means_of_conveyance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We must follow the proper <w>channels</w> to file a formal complaint.",
                            "target_lang_translations": {
                                "ru": "Мы должны использовать официальные <w>кана́лы</w>, чтобы подать формальную жалобу."
                            }
                        },
                        {
                            "text": "Social media is a powerful marketing <w>channel</w> for new businesses.",
                            "target_lang_translations": {
                                "ru": "Социальные сети — это мощный маркетинговый <w>кана́л</w> для нового бизнеса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "official channels",
                        "communication channel",
                        "distribution channel",
                        "through proper channels"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система, метод или маршрут, используемые для отправки или получения информации, товаров или жалоб."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кана́л",
                                "target_lang_sense_clarification": "способ связи, путь распространения"
                            },
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "способ достижения цели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9be2bd51-81a1-3090-af70-4f740f6ca6db",
                    "sense_definition": "A passage of water that connects two larger bodies of water, or the bed where a river flows.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "water_geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ship moved slowly through the narrow <w>channel</w> to reach the harbor.",
                            "target_lang_translations": {
                                "ru": "Корабль медленно двигался по узкому <w>фарва́теру</w>, чтобы добраться до гавани."
                            }
                        },
                        {
                            "text": "Heavy rain caused the river to overflow its <w>channel</w> and flood the fields.",
                            "target_lang_translations": {
                                "ru": "Из-за сильного дождя река вышла из <w>ру́сла</w> и затопила поля."
                            }
                        }
                    ],
                    "synonyms": [
                        "strait",
                        "passage"
                    ],
                    "common_phrases": [
                        "deep channel",
                        "river channel",
                        "narrow channel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Водный проход, соединяющий два больших водоема, или углубление, по которому течет река."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ру́сло",
                                "target_lang_sense_clarification": "углубление, по которому течет река"
                            },
                            {
                                "target_lang_word": "проли́в",
                                "target_lang_sense_clarification": "соединяет два водоема"
                            },
                            {
                                "target_lang_word": "прото́к",
                                "target_lang_sense_clarification": "узкая река, соединяющая водоемы"
                            },
                            {
                                "target_lang_word": "фарва́тер",
                                "target_lang_sense_clarification": "судоходный путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9facf4b8-b05c-3d13-ac54-035662928aca",
                    "sense_definition": "A long, narrow groove or cut in a surface that allows water or other things to flow along it.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Water runs down the <w>channel</w> on the side of the roof.",
                            "target_lang_translations": {
                                "ru": "Вода стекает по <w>жёлобу</w> сбоку крыши."
                            }
                        },
                        {
                            "text": "The carpenter cut a small <w>channel</w> in the wood to hide the wire.",
                            "target_lang_translations": {
                                "ru": "Плотник прорезал в дереве небольшую <w>кана́вку</w>, чтобы спрятать провод."
                            }
                        }
                    ],
                    "synonyms": [
                        "groove",
                        "gutter"
                    ],
                    "common_phrases": [
                        "cut a channel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинное узкое углубление или вырез в поверхности, позволяющий воде или другим предметам течь по нему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жёлоб",
                                "target_lang_sense_clarification": "для стока воды"
                            },
                            {
                                "target_lang_word": "кана́вка",
                                "target_lang_sense_clarification": "небольшое углубление"
                            },
                            {
                                "target_lang_word": "паз",
                                "target_lang_sense_clarification": "вырез в материале"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "571cf42b-6c68-32b5-9fa9-d9609e00736a",
                    "sense_definition": "To direct money, energy, resources, or emotions toward a specific purpose or direction.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "means_of_conveyance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tries to <w>channel</w> his anger into his boxing training.",
                            "target_lang_translations": {
                                "ru": "Он пытается <w>напра́вить</w> свой гнев на тренировки по боксу."
                            }
                        },
                        {
                            "text": "The charity will <w>channel</w> the donations to build new schools in the area.",
                            "target_lang_translations": {
                                "ru": "Благотворительная организация <w>напра́вит</w> пожертвования на строительство новых школ в этом районе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "channel energy",
                        "channel funds",
                        "channel resources"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять деньги, энергию, ресурсы или эмоции на определенную цель или в определенном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "направля́ть",
                                "target_lang_sense_clarification": "устремлять на что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5548133c-5545-386e-b12e-cdb808463880",
                    "sense_definition": "To act as a medium for a spirit, or to imitate the style and behavior of another person.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "imitation_medium",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actor seemed to <w>channel</w> the famous politician perfectly in the movie.",
                            "target_lang_translations": {
                                "ru": "Казалось, что актер идеально <w>изобража́ет</w> этого известного политика в фильме."
                            }
                        },
                        {
                            "text": "She claimed to <w>channel</w> spirits from the past during the session.",
                            "target_lang_translations": {
                                "ru": "Она утверждала, что <w>явля́ется проводнико́м</w> для духов из прошлого во время сеанса."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступать в качестве медиума для духа или подражать стилю и поведению другого человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изобража́ть",
                                "target_lang_sense_clarification": "подражать кому-либо"
                            },
                            {
                                "target_lang_word": "быть проводнико́м",
                                "target_lang_sense_clarification": "для духов, энергии"
                            },
                            {
                                "target_lang_word": "воплоща́ть",
                                "target_lang_sense_clarification": "образ, роль"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f1cf0fac-20be-3266-a3f7-59dbe3ba3527",
                    "sense_definition": "The English Channel; the body of water separating Southern England from Northern France.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "water_geography",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They took a ferry across the <w>Channel</w> to visit France for the weekend.",
                            "target_lang_translations": {
                                "ru": "Они переправились на пароме через <w>Ла-Ма́нш</w>, чтобы посетить Францию на выходных."
                            }
                        },
                        {
                            "text": "Swimming across the <w>Channel</w> is a difficult challenge for any athlete.",
                            "target_lang_translations": {
                                "ru": "Переплыть <w>Ла-Ма́нш</w> — сложная задача для любого спортсмена."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cross the Channel",
                        "the Channel Tunnel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ла-Манш; пролив, отделяющий Южную Англию от Северной Франции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ла-Ма́нш",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
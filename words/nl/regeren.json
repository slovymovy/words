    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0978fa35-7904-3f82-a602-8079eea9a7bf",
                    "sense_definition": "Het besturen van een land, stad of organisatie door politieke macht uit te oefenen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rule_govern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe coalitie zal het land de komende vier jaar <w>regeren</w>.",
                            "target_lang_translations": {
                                "en": "The new coalition will <w>govern</w> the country for the next four years.",
                                "ru": "Новая коалиция будет <w>управлять</w> страной в ближайшие четыре года."
                            }
                        },
                        {
                            "text": "Het land werd tientallen jaren door een dictator <w>geregeerd</w>.",
                            "target_lang_translations": {
                                "en": "The country was <w>ruled</w> by a dictator for decades.",
                                "ru": "Страной десятилетиями <w>правил</w> диктатор."
                            }
                        },
                        {
                            "text": "Ze willen de stad op een rechtvaardige manier <w>regeren</w>.",
                            "target_lang_translations": {
                                "en": "They want to <w>govern</w> the city in a just way.",
                                "ru": "Они хотят справедливо <w>управлять</w> городом."
                            }
                        }
                    ],
                    "synonyms": [
                        "besturen",
                        "leiden"
                    ],
                    "common_phrases": [
                        "regeren is vooruitzien",
                        "het land regeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To govern a country, city, or organization by exercising political power.",
                        "ru": "Управлять страной, городом или организацией, осуществляя политическую власть."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "govern",
                                "target_lang_sense_clarification": "to administer public policy and affairs"
                            },
                            {
                                "target_lang_word": "rule",
                                "target_lang_sense_clarification": "to exercise ultimate authority or dominion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "править",
                                "target_lang_sense_clarification": "осуществлять верховную власть"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "руководить административно или политически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f5d497e-9a9e-3f56-a38c-3f0523f172ac",
                    "sense_definition": "Als vorst of vorstin op de troon zitten en de functie van staatshoofd vervullen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rule_govern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Koningin Victoria <w>regeerde</w> over een enorm rijk.",
                            "target_lang_translations": {
                                "en": "Queen Victoria <w>reigned</w> over a huge empire.",
                                "ru": "Королева Виктория <w>правила</w> огромной империей."
                            }
                        },
                        {
                            "text": "Hij <w>regeerde</w> als koning zonder enige echte politieke macht.",
                            "target_lang_translations": {
                                "en": "He <w>reigned</w> as king without any real political power.",
                                "ru": "Он <w>царствовал</w> как король, не имея никакой реальной политической власти."
                            }
                        }
                    ],
                    "synonyms": [
                        "heersen"
                    ],
                    "common_phrases": [
                        "over een rijk regeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To sit on the throne as a monarch and fulfill the function of head of state.",
                        "ru": "Находиться на троне в качестве монарха, выполнять функции главы государства."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reign",
                                "target_lang_sense_clarification": "to hold royal office as a monarch"
                            },
                            {
                                "target_lang_word": "rule",
                                "target_lang_sense_clarification": "to exercise sovereign power"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "царствовать",
                                "target_lang_sense_clarification": "быть царем или монархом"
                            },
                            {
                                "target_lang_word": "править",
                                "target_lang_sense_clarification": "быть правителем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "434dd5f2-482a-3f05-96d6-57f2a565c022",
                    "sense_definition": "Leiden, sturen of onder controle houden.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het oude volkslied vraagt de dichter of God hem wil <w>regeren</w>.",
                            "target_lang_translations": {
                                "en": "In the old anthem, the poet asks if God will <w>guide</w> him.",
                                "ru": "В старинном гимне поэт просит Бога <w>направлять</w> его."
                            }
                        },
                        {
                            "text": "Je moet je niet door je angsten laten <w>regeren</w>.",
                            "target_lang_translations": {
                                "en": "You shouldn't let yourself be <w>ruled</w> by your fears.",
                                "ru": "Ты не должен позволять своим страхам <w>управлять</w> тобой."
                            }
                        }
                    ],
                    "synonyms": [
                        "leiden",
                        "beheersen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "vooral in religieuze of plechtige context"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "in de betekenis van sturen of leiden"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To lead, guide, or keep under control.",
                        "ru": "Направлять, вести или держать под контролем (часто в переносном или устаревшем смысле)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "control",
                                "target_lang_sense_clarification": "to exercise restraint or direction over"
                            },
                            {
                                "target_lang_word": "govern",
                                "target_lang_sense_clarification": "to influence, determine, or restrain"
                            },
                            {
                                "target_lang_word": "guide",
                                "target_lang_sense_clarification": "to lead or direct (often spiritual/archaic)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "контролировать (напр. эмоции)"
                            },
                            {
                                "target_lang_word": "направлять",
                                "target_lang_sense_clarification": "вести, указывать путь (устар./религ.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
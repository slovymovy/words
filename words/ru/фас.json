            "senses": [
                {
                    "sense_id": "3cc6bfe0-032c-3f32-92d4-937bf890ff2c",
                    "sense_definition": "Федеральная антимонопольная служба (государственный орган в России).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "organization",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "<w>ФАС</w> начала проверку цен на бензин в нескольких регионах.",
                            "target_lang_translations": {
                                "en": "<w>FAS</w> began checking gasoline prices in several regions."
                            }
                        },
                        {
                            "text": "Компания направила жалобу в <w>ФАС</w> из-за недобросовестной рекламы.",
                            "target_lang_translations": {
                                "en": "The company filed a complaint with <w>FAS</w> due to unfair advertising."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Federal Antimonopoly Service (state body in Russia)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "FAS",
                                "target_lang_sense_clarification": "acronym"
                            },
                            {
                                "target_lang_word": "Federal Antimonopoly Service",
                                "target_lang_sense_clarification": "full name"
                            }
                        ]
                    }
                }
            ]
        }
    ]

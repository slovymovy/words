    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6e14ad10-19d2-3ce9-96d5-ce7a60c8c316",
                    "sense_definition": "Вздуваться, болезненно увеличиваться в объёме вследствие воспаления или травмы (о частях тела).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "swelling_medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После укуса осы палец начал быстро <w>распухать</w> и краснеть.",
                            "target_lang_translations": {
                                "en": "After the wasp sting, the finger began to rapidly <w>swell</w> and turn red."
                            }
                        },
                        {
                            "text": "Если не приложить лёд, ушибленное колено будет <w>распухать</w>.",
                            "target_lang_translations": {
                                "en": "If you don't apply ice, the bruised knee will <w>swell</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "опухать",
                        "отекать"
                    ],
                    "common_phrases": [
                        "сильно распухать",
                        "начать распухать",
                        "распухать от укуса",
                        "распухать от удара"
                    ],
                    "target_lang_definitions": {
                        "en": "To become larger and rounder than normal, usually due to injury, inflammation, or illness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swell",
                                "target_lang_sense_clarification": "of a body part; to increase in size due to injury"
                            },
                            {
                                "target_lang_word": "swell up",
                                "target_lang_sense_clarification": "to become visibly swollen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cd1afa7-28d8-37a7-9394-255113795a40",
                    "sense_definition": "Увеличиваться в объёме, становиться толще из-за впитывания влаги или наполнения чем-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "swelling_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От постоянной сырости деревянная дверь стала <w>распухать</w> и плохо закрываться.",
                            "target_lang_translations": {
                                "en": "Due to the constant dampness, the wooden door began to <w>swell</w> and close poorly."
                            }
                        },
                        {
                            "text": "Портфель начал <w>распухать</w> от огромного количества бумаг и книг.",
                            "target_lang_translations": {
                                "en": "The briefcase began to <w>swell</w> from the huge amount of papers and books."
                            }
                        }
                    ],
                    "synonyms": [
                        "разбухать"
                    ],
                    "antonyms": [
                        "ссыхаться"
                    ],
                    "common_phrases": [
                        "распухать от влаги",
                        "распухать от воды",
                        "дерево распухает"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase in volume or thickness, often by absorbing moisture or being filled with something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swell",
                                "target_lang_sense_clarification": "to expand due to moisture or internal pressure"
                            },
                            {
                                "target_lang_word": "expand",
                                "target_lang_sense_clarification": "to become larger in size"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
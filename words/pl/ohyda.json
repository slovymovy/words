{
    "word_family": [
        "ohydny",
        "ohydnie",
        "zohydzić"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b909ba78-8391-3520-b732-31f1c49b58f7",
                    "sense_definition": "Coś bardzo brzydkiego, niesmacznego lub odrażającego; często używane w odniesieniu do jedzenia, przedmiotów lub sytuacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "disgust_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przecież ta brukselka to <w>ohyda</w>, jak można to w ogóle jeść?",
                            "target_lang_translations": {
                                "ru": "Ведь эта брюссельская капуста — <w>гадость</w>, как это вообще можно есть?"
                            }
                        },
                        {
                            "text": "Wyrzuć tę <w>ohydę</w> do kosza, bo strasznie śmierdzi.",
                            "target_lang_translations": {
                                "ru": "Выбрось эту <w>гадость</w> в мусорку, она ужасно воняет."
                            }
                        }
                    ],
                    "synonyms": [
                        "paskudztwo",
                        "ohydztwo"
                    ],
                    "common_phrases": [
                        "straszna ohyda",
                        "sama ohyda",
                        "co za ohyda"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used expressively"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то очень неприятное, невкусное или отвратительное; часто используется по отношению к еде, предметам или ситуациям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гадость",
                                "target_lang_sense_clarification": "о чем-то неприятном, невкусном"
                            },
                            {
                                "target_lang_word": "мерзость",
                                "target_lang_sense_clarification": "сильнее, чем гадость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f329ace-b9e1-3ab3-afe8-f58da58eadf1",
                    "sense_definition": "Silne uczucie wstrętu, niechęci i odrazy wobec kogoś lub czegoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disgust_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyna spojrzała na brudną podłogę z wyraźną <w>ohydą</w>.",
                            "target_lang_translations": {
                                "ru": "Девушка посмотрела на грязный пол с явным <w>отвращением</w>."
                            }
                        },
                        {
                            "text": "Jego kłamstwa budziły w nas jedynie <w>ohydę</w> i gniew.",
                            "target_lang_translations": {
                                "ru": "Его ложь вызывала у нас лишь <w>омерзение</w> и гнев."
                            }
                        }
                    ],
                    "synonyms": [
                        "wstręt",
                        "odraza",
                        "obrzydzenie"
                    ],
                    "common_phrases": [
                        "z ohydą",
                        "budzić ohydę",
                        "czuć ohydę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное чувство неприязни и отвращения к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвращение",
                                "target_lang_sense_clarification": "чувство неприязни"
                            },
                            {
                                "target_lang_word": "омерзение",
                                "target_lang_sense_clarification": "сильное чувство гадливости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a23b1a8a-99c9-3320-aaee-da922428d4e0",
                    "sense_definition": "Właściwość tego, co jest wstrętne, brzydkie lub moralnie odrażające.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "disgust_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trudno opisać słowami <w>ohydę</w> tej okrutnej zbrodni.",
                            "target_lang_translations": {
                                "ru": "Трудно описать словами <w>гнусность</w> этого жестокого преступления."
                            }
                        },
                        {
                            "text": "<w>Ohyda</w> tego miejsca sprawiała, że nikt nie chciał tam przebywać.",
                            "target_lang_translations": {
                                "ru": "<w>Мерзость</w> этого места отбивала у всех желание там находиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "ohydztwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство того, что является отвратительным, уродливым или морально неприемлемым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнусность",
                                "target_lang_sense_clarification": "моральная низость"
                            },
                            {
                                "target_lang_word": "мерзость",
                                "target_lang_sense_clarification": "отвратительное свойство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4246948d-7416-3804-b36f-3245c4a0f45a",
                    "sense_definition": "Wstyd, hańba lub niesława.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "shame",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sowizdrzał, powiedzą, przed dworem <w>ohyda</w>!",
                            "target_lang_translations": {
                                "ru": "Скажут, сорвиголова, перед двором — <w>позор</w>!"
                            }
                        },
                        {
                            "text": "Taka zdrada to wieczna <w>ohyda</w> dla całego rodu.",
                            "target_lang_translations": {
                                "ru": "Такое предательство — вечный <w>позор</w> для всего рода."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical or literary usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стыд, позор или дурная слава."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позор",
                                "target_lang_sense_clarification": "бесчестие"
                            },
                            {
                                "target_lang_word": "срам",
                                "target_lang_sense_clarification": "стыд"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
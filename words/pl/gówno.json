    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "62d27d1a-b64c-3fc7-b50d-ceaadd708377",
                    "sense_definition": "Kał, odchody wydalane przez ludzi lub zwierzęta.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sem_excrement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj, żeby nie wdepnąć w psie <gówno> na trawniku.",
                            "target_lang_translations": {
                                "ru": "Смотри, не наступи в собачье <w>говно</w> на газоне."
                            }
                        },
                        {
                            "text": "Wszędzie w tym parku leżą psie <gówna>, to naprawdę obrzydliwe.",
                            "target_lang_translations": {
                                "ru": "В этом парке повсюду лежит собачье <w>говно</w>, это просто отвратительно."
                            }
                        }
                    ],
                    "synonyms": [
                        "kał",
                        "kupa",
                        "stolec"
                    ],
                    "common_phrases": [
                        "psie gówno",
                        "wdepnąć w gówno"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кал, испражнения, выделяемые людьми или животными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говно",
                                "target_lang_sense_clarification": "прямое значение"
                            },
                            {
                                "target_lang_word": "дерьмо",
                                "target_lang_sense_clarification": "прямое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "477bce70-ede8-37f0-9896-e83a2f73b14d",
                    "sense_definition": "Przedmiot bardzo niskiej jakości, bezwartościowy, zepsuty lub brzydki.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sem_trash",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy telefon to straszne <gówno>, ciągle się zawiesza.",
                            "target_lang_translations": {
                                "ru": "Этот новый телефон — жуткое <w>говно</w>, постоянно зависает."
                            }
                        },
                        {
                            "text": "Nie chcę mieć z tym tanim <gównem> absolutnie nic wspólnego.",
                            "target_lang_translations": {
                                "ru": "Я не хочу иметь с этим дешёвым <w>дерьмом</w> абсолютно ничего общего."
                            }
                        }
                    ],
                    "synonyms": [
                        "badziewie",
                        "chłam",
                        "szajs",
                        "tandeta",
                        "bubel",
                        "syf",
                        "złom",
                        "dziadostwo",
                        "barachło"
                    ],
                    "antonyms": [
                        "cacko"
                    ],
                    "common_phrases": [
                        "totalne gówno",
                        "straszne gówno"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pospolite określenie rzeczy marnej jakości"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет очень низкого качества, бесполезный, сломанный или уродливый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говно",
                                "target_lang_sense_clarification": "о качестве"
                            },
                            {
                                "target_lang_word": "дерьмо",
                                "target_lang_sense_clarification": "о плохой вещи"
                            },
                            {
                                "target_lang_word": "барахло",
                                "target_lang_sense_clarification": "хлам"
                            },
                            {
                                "target_lang_word": "фуфло",
                                "target_lang_sense_clarification": "подделка, дрянь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64f7cbd5-d1fa-3aab-b241-63a7ccfce205",
                    "sense_definition": "Zupełnie nic; słowo używane dla podkreślenia braku rezultatu, wiedzy lub korzyści.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "sem_nothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały dzień ciężko pracowałem, a <gówno> z tego mam.",
                            "target_lang_translations": {
                                "ru": "Я весь день пахал, и <w>ни хрена</w> с этого не имею."
                            }
                        },
                        {
                            "text": "Była taka gęsta mgła, że <gówno> widzieliśmy na drodze.",
                            "target_lang_translations": {
                                "ru": "Был такой густой туман, что мы <w>ни хрена</w> не видели на дороге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gówno prawda",
                        "gówno widać",
                        "gówno wiedzieć"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w emocjonalnych wypowiedziach"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершенно ничего; используется для подчеркивания отсутствия результата, знаний или выгоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ни хрена",
                                "target_lang_sense_clarification": "вульг. ничего"
                            },
                            {
                                "target_lang_word": "ничего",
                                "target_lang_sense_clarification": "нейтрально"
                            },
                            {
                                "target_lang_word": "шиш",
                                "target_lang_sense_clarification": "разг. ничего"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "b1fc553c-e122-35ad-b2ac-2b7055ecb0a4",
                    "sense_definition": "Wulgarny wyraz sprzeciwu, oznaczający kategoryczną odmowę, niedowierzanie lub stwierdzenie nieprawdy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sem_denial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mówili, że to będzie proste, a <gówno>, wcale tak nie jest.",
                            "target_lang_translations": {
                                "ru": "Говорили, что это будет просто, а <w>хрен там</w>, вовсе не так."
                            }
                        },
                        {
                            "text": "<Gówno>! Nie zgadzam się na takie niesprawiedliwe warunki.",
                            "target_lang_translations": {
                                "ru": "<w>Ни хрена</w>! Я не согласен на такие несправедливые условия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gówno tam"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "wyraża silne emocje i sprzeciw"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное выражение протеста, означающее категорический отказ, недоверие или констатацию неправды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хрен там",
                                "target_lang_sense_clarification": "отказ"
                            },
                            {
                                "target_lang_word": "брехня",
                                "target_lang_sense_clarification": "ложь"
                            },
                            {
                                "target_lang_word": "чёрта с два",
                                "target_lang_sense_clarification": "отрицание"
                            }
                        ]
                    }
                }
            ]
        }

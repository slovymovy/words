{
    "word_family": [
        "roaring",
        "roarer",
        "uproarious",
        "uproar"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d65819e-5d69-3b0d-b910-bb3d8e2a4bb0",
                    "sense_definition": "To make a very loud, deep sound, typical of large animals like lions.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lion <w>roared</w> loudly to warn the other animals.",
                            "target_lang_translations": {
                                "ru": "Лев громко <w>зарычал</w>, чтобы предупредить других животных."
                            }
                        },
                        {
                            "text": "We could hear the tigers <w>roar</w> from the other side of the zoo.",
                            "target_lang_translations": {
                                "ru": "Мы слышали, как тигры <w>рычат</w> на другом конце зоопарка."
                            }
                        }
                    ],
                    "synonyms": [
                        "bellow",
                        "growl"
                    ],
                    "common_phrases": [
                        "roar like a lion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать очень громкий, низкий звук, характерный для крупных животных, таких как львы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рычать",
                                "target_lang_sense_clarification": "издавать рык (о львах, тиграх, собаках)"
                            },
                            {
                                "target_lang_word": "реветь",
                                "target_lang_sense_clarification": "издавать громкий, протяжный звук (о медведях, быках)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a04ba4d2-45b6-3c8a-8cf6-7b1496005450",
                    "sense_definition": "To make a loud, deep, continuous noise, often used for engines, fire, or the ocean.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inanimate_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car engine <w>roared</w> to life when he turned the key.",
                            "target_lang_translations": {
                                "ru": "Двигатель машины <w>заревел</w>, когда он повернул ключ."
                            }
                        },
                        {
                            "text": "Huge waves <w>roared</w> against the rocky cliffs during the storm.",
                            "target_lang_translations": {
                                "ru": "Огромные волны <w>ревели</w>, разбиваясь о скалистые утёсы во время шторма."
                            }
                        }
                    ],
                    "synonyms": [
                        "rumble",
                        "thunder",
                        "boom"
                    ],
                    "common_phrases": [
                        "fire roared",
                        "engines roar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать громкий, низкий, непрерывный шум (часто о двигателях, огне или океане)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реветь",
                                "target_lang_sense_clarification": "о моторах, пламени или стихии"
                            },
                            {
                                "target_lang_word": "гудеть",
                                "target_lang_sense_clarification": "издавать ровный, протяжный шум"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06d06d92-76fb-3550-b12d-46b3e7a813a6",
                    "sense_definition": "To shout something in a very loud, deep voice, usually because of anger or pain.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "human_vocalization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain <w>roared</w> orders at the crew over the noise of the storm.",
                            "target_lang_translations": {
                                "ru": "Капитан <w>прорычал</w> приказы команде сквозь шум шторма."
                            }
                        },
                        {
                            "text": "He <w>roared</w> in pain when the heavy box fell on his foot.",
                            "target_lang_translations": {
                                "ru": "Он <w>заревел</w> от боли, когда тяжелая коробка упала ему на ногу."
                            }
                        }
                    ],
                    "synonyms": [
                        "bellow",
                        "yell",
                        "shout"
                    ],
                    "antonyms": [
                        "whisper",
                        "murmur"
                    ],
                    "common_phrases": [
                        "roar in pain",
                        "roar with anger"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кричать что-либо очень громким, низким голосом, обычно от гнева или боли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рычать",
                                "target_lang_sense_clarification": "говорить грубо, со злостью"
                            },
                            {
                                "target_lang_word": "орать",
                                "target_lang_sense_clarification": "громко кричать"
                            },
                            {
                                "target_lang_word": "реветь",
                                "target_lang_sense_clarification": "громко плакать или кричать басом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f7a2cc6-968c-3dc7-9031-558c33bae30f",
                    "sense_definition": "To laugh in a very loud and energetic manner.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "human_vocalization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The entire audience <w>roared</w> at the comedian's jokes.",
                            "target_lang_translations": {
                                "ru": "Вся публика <w>хохотала</w> над шутками комика."
                            }
                        },
                        {
                            "text": "She threw her head back and <w>roared</w> with laughter.",
                            "target_lang_translations": {
                                "ru": "Она запрокинула голову и <w>рассмеялась</w> во весь голос."
                            }
                        }
                    ],
                    "synonyms": [
                        "guffaw",
                        "howl"
                    ],
                    "common_phrases": [
                        "roar with laughter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смеяться очень громко и энергично."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хохотать",
                                "target_lang_sense_clarification": "громко смеяться"
                            },
                            {
                                "target_lang_word": "грохотать",
                                "target_lang_sense_clarification": "о громком смехе толпы или зала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63f7f567-9b55-38d2-a80c-c7d5971b8b98",
                    "sense_definition": "To move at high speed while making a loud noise.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sports car <w>roared</w> past us on the highway.",
                            "target_lang_translations": {
                                "ru": "Спортивная машина <w>пронеслась с рёвом</w> мимо нас по шоссе."
                            }
                        },
                        {
                            "text": "The train <w>roared</w> through the station without stopping.",
                            "target_lang_translations": {
                                "ru": "Поезд <w>прогрохотал</w> через станцию без остановки."
                            }
                        }
                    ],
                    "synonyms": [
                        "zoom",
                        "speed"
                    ],
                    "common_phrases": [
                        "roar past",
                        "roar off"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться на высокой скорости, издавая громкий шум."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проноситься с рёвом",
                                "target_lang_sense_clarification": "быстро проезжать, издавая шум"
                            },
                            {
                                "target_lang_word": "мчаться",
                                "target_lang_sense_clarification": "быстро ехать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b2fa9df5-8b1e-310c-986a-9c71e2da88ef",
                    "sense_definition": "A loud, deep sound made by an animal, especially a lion.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>roar</w> of the lion frightened the smaller animals.",
                            "target_lang_translations": {
                                "ru": "<w>Рык</w> льва напугал мелких животных."
                            }
                        },
                        {
                            "text": "We heard a terrifying <w>roar</w> coming from the dark forest.",
                            "target_lang_translations": {
                                "ru": "Мы услышали ужасающий <w>рёв</w>, доносящийся из темного леса."
                            }
                        }
                    ],
                    "synonyms": [
                        "growl",
                        "bellow"
                    ],
                    "common_phrases": [
                        "let out a roar",
                        "mighty roar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громкий, низкий звук, издаваемый животным, особенно львом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рык",
                                "target_lang_sense_clarification": "резкий, отрывистый звук"
                            },
                            {
                                "target_lang_word": "рёв",
                                "target_lang_sense_clarification": "протяжный, мощный звук"
                            },
                            {
                                "target_lang_word": "рычание",
                                "target_lang_sense_clarification": "процесс издания звука"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cc7dc68-86a8-3c8d-af0a-9d68dcc90397",
                    "sense_definition": "A loud, deep, continuous noise made by things like traffic, the sea, or machinery.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inanimate_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>roar</w> of the waterfall was so loud we could not hear each other speak.",
                            "target_lang_translations": {
                                "ru": "<w>Рёв</w> водопада был настолько громким, что мы не слышали друг друга."
                            }
                        },
                        {
                            "text": "I woke up to the <w>roar</w> of a motorcycle engine outside my window.",
                            "target_lang_translations": {
                                "ru": "Я проснулся от <w>рёва</w> мотоциклетного двигателя за окном."
                            }
                        }
                    ],
                    "synonyms": [
                        "rumble",
                        "thunder",
                        "din"
                    ],
                    "antonyms": [
                        "silence"
                    ],
                    "common_phrases": [
                        "dull roar",
                        "deafening roar",
                        "roar of traffic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громкий, низкий, непрерывный шум, издаваемый транспортом, морем или механизмами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рёв",
                                "target_lang_sense_clarification": "мотора, турбин, водопада"
                            },
                            {
                                "target_lang_word": "гул",
                                "target_lang_sense_clarification": "общий фоновый шум"
                            },
                            {
                                "target_lang_word": "грохот",
                                "target_lang_sense_clarification": "сильный шум с ударами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7edc508-9b17-3e30-9177-02a40e7778fb",
                    "sense_definition": "A long, loud, deep shout expressing strong emotion like anger or amusement.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "human_vocalization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd erupted in a <w>roar</w> of approval when the band walked on stage.",
                            "target_lang_translations": {
                                "ru": "Толпа разразилась одобрительным <w>гулом</w>, когда группа вышла на сцену."
                            }
                        },
                        {
                            "text": "His joke was met with a <w>roar</w> of laughter from the guests.",
                            "target_lang_translations": {
                                "ru": "Его шутка была встречена <w>хохотом</w> гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "shout",
                        "yell",
                        "bellow"
                    ],
                    "antonyms": [
                        "whisper"
                    ],
                    "common_phrases": [
                        "roar of laughter",
                        "roar of approval",
                        "roar of rage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Долгий, громкий крик, выражающий сильные эмоции, такие как гнев или веселье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рёв",
                                "target_lang_sense_clarification": "толпы, одобрения"
                            },
                            {
                                "target_lang_word": "хохот",
                                "target_lang_sense_clarification": "громкий смех"
                            },
                            {
                                "target_lang_word": "вопль",
                                "target_lang_sense_clarification": "пронзительный крик"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
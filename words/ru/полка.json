                    "frequency": "Low",
                    "semantic_group_id": "bath_bench",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В парной было так жарко, что я не смог забраться на верхнюю <полку>.",
                            "target_lang_translations": {
                                "en": "It was so hot in the steam room that I couldn't climb onto the top <bench>."
                            }
                        },
                        {
                            "text": "Старик лежал на деревянной <полке> и бил себя берёзовым веником.",
                            "target_lang_translations": {
                                "en": "The old man lay on the wooden <bench> and beat himself with a birch broom."
                            }
                        }
                    ],
                    "common_phrases": [
                        "банная полка",
                        "залезть на полку"
                    ],
                    "target_lang_definitions": {
                        "en": "A wooden bench or platform in a steam bath or sauna."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bench",
                                "target_lang_sense_clarification": "in a sauna"
                            },
                            {
                                "target_lang_word": "platform",
                                "target_lang_sense_clarification": "in a steam room"
                            }
                        ]

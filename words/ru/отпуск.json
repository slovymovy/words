    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ed251d9d-6d93-3fef-9c7b-c49ac7238c17",
                    "sense_definition": "Временное освобождение от работы или службы на определённый срок для отдыха и личных целей",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом мы всей семьёй планируем поехать в <w>отпуск</w> на море.",
                            "target_lang_translations": {
                                "en": "In the summer, our whole family plans to go on <w>vacation</w> to the sea."
                            }
                        },
                        {
                            "text": "Мой коллега сейчас находится в <w>отпуске</w>, поэтому я выполняю его обязанности.",
                            "target_lang_translations": {
                                "en": "My colleague is currently on <w>leave</w>, so I am fulfilling his duties."
                            }
                        },
                        {
                            "text": "Директор подписал заявление и разрешил мне уйти в <w>отпуск</w> на две недели.",
                            "target_lang_translations": {
                                "en": "The director signed the application and allowed me to go on <w>leave</w> for two weeks."
                            }
                        }
                    ],
                    "synonyms": [
                        "отдых",
                        "каникулы"
                    ],
                    "antonyms": [
                        "работа",
                        "служба"
                    ],
                    "common_phrases": [
                        "уйти в отпуск",
                        "декретный отпуск",
                        "ежегодный отпуск",
                        "быть в отпуске",
                        "отпуск за свой счёт"
                    ],
                    "target_lang_definitions": {
                        "en": "Temporary release from work or service for a specific period for rest and personal purposes"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vacation",
                                "target_lang_sense_clarification": "standard term for time off (US)"
                            },
                            {
                                "target_lang_word": "holiday",
                                "target_lang_sense_clarification": "standard term for time off (UK)"
                            },
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "official permission to be absent from work"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19cd9f11-e89b-3832-adb9-1b4f8d96937f",
                    "sense_definition": "Выдача товара, продукции, лекарств или материалов получателю",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой аптеке производится <w>отпуск</w> лекарств только по рецепту врача.",
                            "target_lang_translations": {
                                "en": "This pharmacy carries out the <w>dispensing</w> of medicines only by doctor's prescription."
                            }
                        },
                        {
                            "text": "Завод временно приостановил <w>отпуск</w> готовой продукции со склада из-за инвентаризации.",
                            "target_lang_translations": {
                                "en": "The factory temporarily suspended the <w>release</w> of finished products from the warehouse due to inventory."
                            }
                        }
                    ],
                    "synonyms": [
                        "выдача",
                        "отгрузка"
                    ],
                    "antonyms": [
                        "приём",
                        "получение"
                    ],
                    "common_phrases": [
                        "отпуск товаров",
                        "отпуск лекарств",
                        "цена отпуска"
                    ],
                    "target_lang_definitions": {
                        "en": "The issuing of goods, products, medicines, or materials to a recipient"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dispensing",
                                "target_lang_sense_clarification": "specifically for medicines"
                            },
                            {
                                "target_lang_word": "issue",
                                "target_lang_sense_clarification": "act of supplying or distributing items"
                            },
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "act of allowing goods to leave a warehouse"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c75ef178-63be-3528-9f83-09df12dba3a7",
                    "sense_definition": "Термическая обработка металла, заключающаяся в нагреве и последующем охлаждении для уменьшения хрупкости",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После закалки детали необходим <w>отпуск</w> для снятия внутренних напряжений в металле.",
                            "target_lang_translations": {
                                "en": "After hardening, the part requires <w>tempering</w> to relieve internal stresses in the metal."
                            }
                        },
                        {
                            "text": "Правильно проведённый <w>отпуск</w> делает сталь более прочной и вязкой.",
                            "target_lang_translations": {
                                "en": "Correctly performed <w>tempering</w> makes steel stronger and tougher."
                            }
                        }
                    ],
                    "synonyms": [
                        "термообработка"
                    ],
                    "antonyms": [
                        "закалка"
                    ],
                    "common_phrases": [
                        "высокий отпуск",
                        "низкий отпуск",
                        "отпуск стали"
                    ],
                    "target_lang_definitions": {
                        "en": "Heat treatment of metal consisting of heating and subsequent cooling to reduce brittleness"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tempering",
                                "target_lang_sense_clarification": "metallurgy term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2f53933-8f05-38ab-8735-1df47f650ef6",
                    "sense_definition": "Заключительная молитва церковной службы; обряд отпущения грехов",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В конце литургии священник вышел к прихожанам и прочитал <w>отпуск</w>.",
                            "target_lang_translations": {
                                "en": "At the end of the liturgy, the priest came out to the parishioners and read the <w>dismissal</w>."
                            }
                        },
                        {
                            "text": "Верующие склонили головы, слушая праздничный <w>отпуск</w> и получая благословение.",
                            "target_lang_translations": {
                                "en": "The believers bowed their heads, listening to the festive <w>dismissal</w> and receiving the blessing."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3d470349-33b6-39f5-95c2-4ee129f2099a",
                    "sense_definition": "Относящийся к прошлому, уже закончившийся, миновавший (о времени или событиях).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_past",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За <w>прошедший</w> год в нашем городе построили новый парк.",
                            "target_lang_translations": {
                                "en": "Over the <w>past</w> year, a new park was built in our city."
                            }
                        },
                        {
                            "text": "<w>Прошедший</w> дождь напоил землю влагой и освежил воздух.",
                            "target_lang_translations": {
                                "en": "The rain that <w>passed</w> soaked the earth with moisture and refreshed the air."
                            }
                        }
                    ],
                    "synonyms": [
                        "минувший",
                        "прошлый",
                        "миновавший"
                    ],
                    "antonyms": [
                        "грядущий"
                    ],
                    "common_phrases": [
                        "прошедший год",
                        "прошедшее время",
                        "прошедший сезон",
                        "прошедшая неделя"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the past, already finished, or gone by (referring to time or events)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "past",
                                "target_lang_sense_clarification": "Gone by in time and no longer existing."
                            },
                            {
                                "target_lang_word": "last",
                                "target_lang_sense_clarification": "Most recent; occurring immediately before the present."
                            },
                            {
                                "target_lang_word": "previous",
                                "target_lang_sense_clarification": "Existing or occurring before in time or order."
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "255f99e1-bb37-36a8-9166-2e45ec01c52e",
                    "sense_definition": "Действительное причастие прошедшего времени от глагола <w>пройти</w>; обозначает того, кто переместился мимо чего-либо или преодолел какое-то расстояние.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_move",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Турист, <w>прошедший</w> весь маршрут до конца, очень устал.",
                            "target_lang_translations": {
                                "en": "The tourist, having <w>completed</w> the entire route to the end, was very tired."
                            }
                        },
                        {
                            "text": "Мы не узнали человека, быстро <w>прошедшего</w> мимо нас на улице.",
                            "target_lang_translations": {
                                "en": "We did not recognize the person who quickly <w>passed</w> by us on the street."
                            }
                        }
                    ],
                    "synonyms": [
                        "прошествовавший"
                    ],
                    "antonyms": [
                        "приближающийся"
                    ],
                    "common_phrases": [
                        "прошедший мимо",
                        "прошедший через испытания",
                        "успешно прошедший"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Past active participle of the verb 'to pass'; denotes one who has moved past something or covered a certain distance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "passed",
                                "target_lang_sense_clarification": "Having moved past or beyond something."
                            }
                        ]
                    }
                }
            ]
        }
    ]

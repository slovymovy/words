{
    "word_family": [
        "spoglądać",
        "spojrzenie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8c3ffda9-411e-3a90-84ea-30eb89b7ee50",
                    "sense_definition": "Skierować wzrok na kogoś lub na coś przez krótką chwilę, aby to zobaczyć.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <w>spojrzeć</w> na tablicę i przepisać zdania.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>посмотрите</w> на доску и перепишите предложения."
                            }
                        },
                        {
                            "text": "Chłopiec <w>spojrzał</w> na matkę i uśmiechnął się szeroko.",
                            "target_lang_translations": {
                                "ru": "Мальчик <w>взглянул</w> на маму и широко улыбнулся."
                            }
                        },
                        {
                            "text": "Musisz <w>spojrzeć</w> w lustro, żeby zobaczyć tę plamę.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>посмотреть</w> в зеркало, чтобы увидеть это пятно."
                            }
                        }
                    ],
                    "synonyms": [
                        "popatrzeć",
                        "zerknąć",
                        "spoglądnąć"
                    ],
                    "antonyms": [
                        "odwrócić wzrok"
                    ],
                    "common_phrases": [
                        "spojrzeć w oczy",
                        "spojrzeć na zegarek",
                        "spojrzeć za siebie",
                        "spojrzeć w niebo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направить взгляд на кого-либо или что-либо на короткое время, чтобы увидеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взглянуть",
                                "target_lang_sense_clarification": "бросить быстрый взгляд"
                            },
                            {
                                "target_lang_word": "посмотреть",
                                "target_lang_sense_clarification": "направить взгляд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50bc80a4-fc09-3636-bf20-b20f61a7d79b",
                    "sense_definition": "Ocenić sytuację, problem lub sprawę w określony sposób; zastanowić się nad czymś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <w>spojrzeć</w> na ten problem z innej perspektywy.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>взглянуть</w> на эту проблему с другой точки зрения."
                            }
                        },
                        {
                            "text": "Trzeba <w>spojrzeć</w> prawdzie w oczy i przyznać się do błędu.",
                            "target_lang_translations": {
                                "ru": "Нужно <w>посмотреть</w> правде в глаза и признать ошибку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spojrzeć prawdzie w oczy",
                        "spojrzeć z innej strony",
                        "spojrzeć na sprawę",
                        "spojrzeć obiektywnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценить ситуацию, проблему или дело определенным образом; обдумать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взглянуть",
                                "target_lang_sense_clarification": "оценить, проанализировать"
                            },
                            {
                                "target_lang_word": "посмотреть",
                                "target_lang_sense_clarification": "отнестись к чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3352b55-8eb6-3110-944f-37e44019ad25",
                    "sense_definition": "Skierować wzrok na kogoś lub na coś (użycie z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dlaczego tak dziwnie się na mnie <w>spojrzałeś</w>?",
                            "target_lang_translations": {
                                "ru": "Почему ты так странно на меня <w>посмотрел</w>?"
                            }
                        },
                        {
                            "text": "Dziewczyna <w>spojrzała się</w> za siebie, słysząc hałas.",
                            "target_lang_translations": {
                                "ru": "Девушка <w>оглянулась</w> назад, услышав шум."
                            }
                        }
                    ],
                    "synonyms": [
                        "spoglądnąć się"
                    ],
                    "common_phrases": [
                        "spojrzeć się na kogoś",
                        "spojrzeć się w lustro"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "forma z zaimkiem „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Направить взгляд на кого-то или что-то (разговорное употребление с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посмотреть",
                                "target_lang_sense_clarification": "на кого-либо"
                            },
                            {
                                "target_lang_word": "оглянуться",
                                "target_lang_sense_clarification": "посмотреть назад"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
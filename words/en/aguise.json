    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cbb4c20e-8928-3928-bdac-3172da09b346",
                    "sense_definition": "To dress, adorn, or array someone or something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The servants rushed to <w>aguise</w> the princess for her wedding day.",
                            "target_lang_translations": {
                                "ru": "Слуги поспешили <w>нарядить</w> принцессу к дню её свадьбы."
                            }
                        },
                        {
                            "text": "She did <w>aguise</w> herself with flowers gathered from the meadow.",
                            "target_lang_translations": {
                                "ru": "Она <w>украсила</w> себя цветами, собранными на лугу."
                            }
                        }
                    ],
                    "synonyms": [
                        "attire",
                        "adorn",
                        "deck",
                        "array"
                    ],
                    "antonyms": [
                        "strip",
                        "undress"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Used in older literature, such as the works of Spenser."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одевать, наряжать или украшать кого-либо или что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наряжать",
                                "target_lang_sense_clarification": "одевать в красивую одежду"
                            },
                            {
                                "target_lang_word": "облачать",
                                "target_lang_sense_clarification": "одевать торжественно (устар./книжн.)"
                            },
                            {
                                "target_lang_word": "украшать",
                                "target_lang_sense_clarification": "делать красивым, добавляя детали"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c7463f00-a258-3e13-9b3b-4383bcba81f0",
                    "sense_definition": "Clothing, dress, or attire.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The noblewoman appeared in a splendid <w>aguise</w> of silk and pearls.",
                            "target_lang_translations": {
                                "ru": "Дворянка предстала в великолепном <w>одеянии</w> из шелка и жемчуга."
                            }
                        },
                        {
                            "text": "He cast off his warrior's <w>aguise</w> and dressed in simple robes.",
                            "target_lang_translations": {
                                "ru": "Он сбросил свое воинское <w>облачение</w> и оделся в простые одежды."
                            }
                        }
                    ],
                    "synonyms": [
                        "attire",
                        "garb",
                        "apparel",
                        "costume"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Used in older literature, such as the works of Spenser."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одежда, наряд или одеяние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одеяние",
                                "target_lang_sense_clarification": "торжественная или длинная одежда (книжн.)"
                            },
                            {
                                "target_lang_word": "наряд",
                                "target_lang_sense_clarification": "красивая, праздничная одежда"
                            },
                            {
                                "target_lang_word": "убранство",
                                "target_lang_sense_clarification": "внешнее украшение или одежда"
                            }
                        ]
                    }
                }
            ]
        }
    ]

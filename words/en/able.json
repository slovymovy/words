    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "06f0c496-8877-3150-b267-a99138e18fea",
                    "sense_definition": "Having the necessary power, skill, time, opportunity, or resources to do something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "capability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am sorry, but I am not <w>able</w> to come to the party tonight.",
                            "target_lang_translations": {
                                "ru": "Извини, но я не <w>смогу</w> прийти сегодня на вечеринку."
                            }
                        },
                        {
                            "text": "Will you be <w>able</w> to finish the report by tomorrow morning?",
                            "target_lang_translations": {
                                "ru": "Ты <w>сможешь</w> закончить отчет к завтрашнему утру?"
                            }
                        },
                        {
                            "text": "She is <w>able</w> to speak three languages fluently.",
                            "target_lang_translations": {
                                "ru": "Она <w>способна</w> свободно говорить на трех языках."
                            }
                        }
                    ],
                    "synonyms": [
                        "Alfa"
                    ],
                    "antonyms": [
                        "unable"
                    ],
                    "common_phrases": [
                        "be able to",
                        "will be able",
                        "not able to",
                        "better able to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий возможность, навык, время или средства сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спосо́бный",
                                "target_lang_sense_clarification": "обладающий возможностью или свойством"
                            },
                            {
                                "target_lang_word": "в состоя́нии",
                                "target_lang_sense_clarification": "имеющий физическую или моральную возможность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89787efc-72f6-38a8-b9fa-43916b171e30",
                    "sense_definition": "Intelligent, skilled, or effective at doing a job.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "competence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company hired an <w>able</w> manager to fix the financial problems.",
                            "target_lang_translations": {
                                "ru": "Компания наняла <w>компетентного</w> менеджера для решения финансовых проблем."
                            }
                        },
                        {
                            "text": "He is an <w>able</w> student who always gets high grades.",
                            "target_lang_translations": {
                                "ru": "Он <w>способный</w> ученик, который всегда получает высокие оценки."
                            }
                        }
                    ],
                    "synonyms": [
                        "Alfa"
                    ],
                    "antonyms": [
                        "incompetent",
                        "inept"
                    ],
                    "common_phrases": [
                        "able student",
                        "able leader",
                        "able seaman"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умный, талантливый или квалифицированный в каком-либо деле."

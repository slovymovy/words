            "senses": [
                {
                    "sense_id": "cbc6daf6-45d7-38fe-9f2c-295ff63bcf49",
                    "sense_definition": "Zespół budynków mieszkalnych (bloków lub domów jednorodzinnych) wraz z infrastrukturą usługową i drogami, stanowiący wyodrębnioną część miasta lub wsi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "housing_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkam na tym <w>osiedlu</w> od dziesięciu lat.",
                            "target_lang_translations": {
                                "ru": "Я живу в этом <w>микрорайоне</w> уже десять лет."
                            }
                        },
                        {
                            "text": "Na naszym <w>osiedlu</w> wybudowano niedawno nowy plac zabaw dla dzieci.",
                            "target_lang_translations": {
                                "ru": "В нашем <w>микрорайоне</w> недавно построили новую детскую площадку."
                            }
                        }
                    ],
                    "synonyms": [
                        "osiedle mieszkaniowe"
                    ],
                    "common_phrases": [
                        "mieszkać na osiedlu",
                        "nowe osiedle",
                        "osiedle zamknięte",
                        "osiedle domków jednorodzinnych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплекс жилых зданий (многоквартирных или частных домов) вместе с инфраструктурой обслуживания и дорогами, составляющий обособленную часть города или села."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "микрорайон",
                                "target_lang_sense_clarification": "административная единица жилой застройки в городе"
                            },
                            {
                                "target_lang_word": "жилой комплекс",
                                "target_lang_sense_clarification": "группа жилых домов с единой территорией"
                            },
                            {
                                "target_lang_word": "жилмассив",
                                "target_lang_sense_clarification": "крупный жилой район"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "699f3d70-e236-3c4e-8ecd-a0109a95d956",
                    "sense_definition": "Niewielka jednostka osadnicza; miejsce, gdzie ludzie osiedlili się i mieszkają, często o charakterze historycznym lub administracyjnym.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "settlement_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Archeolodzy odkryli w lesie ślady starego <w>osiedla</w> z epoki brązu.",
                            "target_lang_translations": {
                                "ru": "Археологи обнаружили в лесу следы древнего <w>поселения</w> бронзового века."
                            }
                        },
                        {
                            "text": "To małe <w>osiedle</w> powstało w dziewiętnastym wieku wokół fabryki włókienniczej.",
                            "target_lang_translations": {
                                "ru": "Этот небольшой <w>посёлок</w> возник в девятнадцатом веке вокруг текстильной фабрики."
                            }
                        }
                    ],
                    "synonyms": [
                        "osada"
                    ],
                    "common_phrases": [
                        "osiedle robotnicze",
                        "osiedle górnicze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой населённый пункт; место, где люди поселились и живут, часто исторического или административного характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поселение",
                                "target_lang_sense_clarification": "место жительства людей, часто историческое"
                            },
                            {
                                "target_lang_word": "посёлок",
                                "target_lang_sense_clarification": "небольшой населённый пункт, часто рабочий"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "hook",
        "unhook",
        "fishhook"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "25d9c1b6-6406-3a96-bd1a-44ec1693297a",
                    "sense_definition": "Addicted to something or unable to stop doing it.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I got <w>hooked</w> on that TV show after just one episode.",
                            "target_lang_translations": {
                                "ru": "Я <w>подсел</w> на этот сериал всего после одной серии."
                            }
                        },
                        {
                            "text": "He is completely <w>hooked</w> on video games and plays every day.",
                            "target_lang_translations": {
                                "ru": "Он полностью <w>помешан</w> на видеоиграх и играет каждый день."
                            }
                        }
                    ],
                    "synonyms": [
                        "addicted"
                    ],
                    "common_phrases": [
                        "hooked on",
                        "get hooked"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий сильное пристрастие к чему-либо или неспособный остановиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подсевший",
                                "target_lang_sense_clarification": "разг., об увлечении сериалом, игрой и т.п."
                            },
                            {
                                "target_lang_word": "зависимый",
                                "target_lang_sense_clarification": "имеющий пагубную привычку"
                            },
                            {
                                "target_lang_word": "помешанный",
                                "target_lang_sense_clarification": "сильно увлеченный чем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c21b142-bb9a-3d80-b514-cdf9676aeb12",
                    "sense_definition": "Having a sharp curve at the end; resembling a hook.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The eagle used its sharp, <w>hooked</w> beak to tear the food.",
                            "target_lang_translations": {
                                "ru": "Орел использовал свой острый <w>крючковатый</w> клюв, чтобы рвать пищу."
                            }
                        },
                        {
                            "text": "The old man had a prominent, <w>hooked</w> nose.",
                            "target_lang_translations": {
                                "ru": "У старика был выдающийся <w>крючковатый</w> нос."
                            }
                        }
                    ],
                    "synonyms": [
                        "bent",
                        "crooked",
                        "curved"
                    ],
                    "antonyms": [
                        "straight"
                    ],
                    "common_phrases": [
                        "hooked nose",
                        "hooked beak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий резкий изгиб на конце; напоминающий крюк."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крючковатый",
                                "target_lang_sense_clarification": "о носе или клюве"
                            },
                            {
                                "target_lang_word": "изогнутый",
                                "target_lang_sense_clarification": "кривой, в форме крюка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "88973510-e07a-3c97-bb28-f6f055b0a1e5",
                    "sense_definition": "Simple past and past participle of <w>hook</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>hooked</w> the fish and quickly reeled it in.",
                            "target_lang_translations": {
                                "ru": "Он <w>подцепил</w> рыбу и быстро смотал леску."
                            }
                        },
                        {
                            "text": "She <w>hooked</w> her arm through his as they walked.",
                            "target_lang_translations": {
                                "ru": "Она <w>продела</w> свою руку под его, когда они шли."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола hook (цеплять)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зацепил",
                                "target_lang_sense_clarification": "закрепил или поймал крюком"
                            },
                            {
                                "target_lang_word": "подцепил",
                                "target_lang_sense_clarification": "захватил или поймал"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "322d5ae2-aba3-3e77-9046-6dc2833b1f8e",
                    "sense_definition": "Живой организм, существо, обладающее способностью чувствовать и двигаться, в отличие от растений.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня дома живёт очень ласковое и пушистое <w>животное</w>.",
                            "target_lang_translations": {
                                "en": "A very affectionate and fluffy <w>animal</w> lives at my house."
                            }
                        },
                        {
                            "text": "В зоопарке мы видели, как кормят огромное дикое <w>животное</w>.",
                            "target_lang_translations": {
                                "en": "At the zoo, we saw them feeding a huge wild <w>animal</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "зверь"
                    ],
                    "antonyms": [
                        "растение",
                        "человек"
                    ],
                    "common_phrases": [
                        "домашнее животное",
                        "дикое животное",
                        "мир животных",
                        "бедное животное"
                    ],
                    "target_lang_definitions": {
                        "en": "A living organism that feeds on organic matter, typically having sense organs and a nervous system, distinguished from plants and fungi."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "animal",
                                "target_lang_sense_clarification": "general term for living creatures"
                            },
                            {
                                "target_lang_word": "beast",
                                "target_lang_sense_clarification": "often refers to a large or wild animal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31379662-3d4c-3837-a9ca-a75b18d0a482",
                    "sense_definition": "Грубый, невоспитанный человек, который руководствуется инстинктами, а не разумом и нормами морали.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После того, что он сделал, он для меня просто <w>животное</w>.",
                            "target_lang_translations": {
                                "en": "After what he did, he is just an <w>animal</w> to me."
                            }
                        },
                        {
                            "text": "Не веди себя как <w>животное</w>, сядь за стол и ешь аккуратно.",
                            "target_lang_translations": {
                                "en": "Don't behave like an <w>animal</w>; sit at the table and eat neatly."
                            }
                        }
                    ],
                    "synonyms": [
                        "скотина",
                        "тварь",
                        "зверь"
                    ],
                    "antonyms": [
                        "человек"
                    ],
                    "common_phrases": [
                        "грязное животное",
                        "вести себя как животное"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "пренебрежительное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person whose behavior is regarded as uncivilized, coarse, violent, or governed by instincts rather than reason."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "animal",
                                "target_lang_sense_clarification": "figurative: uncivilized or wild person"
                            },
                            {
                                "target_lang_word": "brute",
                                "target_lang_sense_clarification": "emphasizes roughness, violence, or lack of intelligence"
                            },
                            {
                                "target_lang_word": "beast",
                                "target_lang_sense_clarification": "emphasizes cruelty or wild nature"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bceaccdc-608b-3ea6-8d74-dd78c6914915",
                    "sense_definition": "Ироничное или шутливое обращение к кому-либо.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вставай, ленивое <w>животное</w>, нам пора собираться на работу!",
                            "target_lang_translations": {
                                "en": "Get up, you lazy <w>animal</w>, it's time for us to get ready for work!"
                            }
                        },
                        {
                            "text": "Ну ты и <w>животное</w>, опять съел всё печенье без меня.",
                            "target_lang_translations": {
                                "en": "You really are an <w>animal</w>; you ate all the cookies without me again."
                            }
                        }
                    ],
                    "common_phrases": [
                        "эй животное"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "ироничное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A playful or ironic term of address for someone, often implying laziness, greed, or lack of self-control."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "animal",
                                "target_lang_sense_clarification": "playful or ironic address"
                            },
                            {
                                "target_lang_word": "beast",
                                "target_lang_sense_clarification": "often used in phrases like 'lazy beast'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "37f03c4d-567b-3cfe-b49b-343cf0edc30a",
                    "sense_definition": "Форма именительного или винительного падежа среднего рода единственного числа прилагательного <w>животный</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его охватило странное <w>животное</w> беспокойство, когда наступила полная темнота.",
                            "target_lang_translations": {
                                "en": "A strange <w>animal</w> anxiety seized him when total darkness fell."
                            }
                        },
                        {
                            "text": "Это лекарство имеет <w>животное</w> происхождение и очень помогает людям.",
                            "target_lang_translations": {
                                "en": "This medicine is of <w>animal</w> origin and helps people a lot."
                            }
                        }
                    ],
                    "common_phrases": [
                        "животное происхождение",
                        "животное чувство",
                        "животное начало"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to or characteristic of animals; pertaining to physical instincts or sensations rather than intellectual or spiritual qualities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "animal",
                                "target_lang_sense_clarification": "adjective: instinctive, physical, or visceral"
                            }
                        ]
                    }
                }
            ]
        }
    ]

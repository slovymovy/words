                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_jump",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The deer cleared the high fence in a single <w>bound</w>.",
                            "target_lang_translations": {
                                "ru": "Олень перемахнул через высокий забор одним <w>прыжком</w>."
                            }
                        },
                        {
                            "text": "Technology is improving by leaps and <w>bounds</w> every year.",
                            "target_lang_translations": {
                                "ru": "Технологии с каждым годом совершенствуются гигантскими <w>скачками</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "leap",
                        "jump",
                        "spring"
                    ],
                    "common_phrases": [
                        "by leaps and bounds",
                        "in a single bound"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный или высокий прыжок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прыжо́к",
                                "target_lang_sense_clarification": "перемещение тела в воздухе отталкиванием"
                            },
                            {
                                "target_lang_word": "скачо́к",
                                "target_lang_sense_clarification": "резкое перемещение прыжком"
                            }
                        ]
                    }
                }
            ]
        },

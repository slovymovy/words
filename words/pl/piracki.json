            "senses": [
                {
                    "sense_id": "1d472665-67e3-3a6b-a599-35ea7ca953f6",
                    "sense_definition": "odnoszący się do piratów morskich, należący do nich lub dla nich charakterystyczny",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na najwyższym maszcie statku wisiała czarna <w>piracka</w> flaga.",
                            "target_lang_translations": {
                                "ru": "На самой высокой мачте корабля висел черный <w>пиратский</w> флаг."
                            }
                        },
                        {
                            "text": "W tej jaskini ukryto podobno legendarny <w>piracki</w> skarb.",
                            "target_lang_translations": {
                                "ru": "Говорят, в этой пещере спрятано легендарное <w>пиратское</w> сокровище."
                            }
                        }
                    ],
                    "common_phrases": [
                        "statek piracki",
                        "flaga piracka",
                        "bandera piracka",
                        "skarb piracki",
                        "atak piracki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к морским пиратам, принадлежащий им или характерный для них"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пиратский",
                                "target_lang_sense_clarification": "о морских разбойниках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6912085d-d888-375b-8130-9a2e268d3c1a",
                    "sense_definition": "naruszający prawa autorskie, wyprodukowany, skopiowany lub rozpowszechniany nielegalnie",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "On ciągle ogląda w internecie nielegalne <w>pirackie</w> filmy.",
                            "target_lang_translations": {
                                "ru": "Он постоянно смотрит в интернете нелегальные <w>пиратские</w> фильмы."
                            }
                        },
                        {
                            "text": "Zainstalowanie <w>pirackiego</w> oprogramowania może narazić komputer na wirusy.",
                            "target_lang_translations": {
                                "ru": "Установка <w>пиратского</w> программного обеспечения может подвергнуть компьютер воздействию вирусов."
                            }
                        }
                    ],
                    "synonyms": [
                        "nielegalny"
                    ],
                    "antonyms": [
                        "legalny",
                        "oryginalny",
                        "antypiracki"
                    ],
                    "common_phrases": [
                        "oprogramowanie pirackie",
                        "wersja piracka",
                        "kopia piracka",
                        "nagranie pirackie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушающий авторские права, произведенный, скопированный или распространяемый незаконно"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пиратский",
                                "target_lang_sense_clarification": "нелегальный, контрафактный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4287c2fb-5afc-376c-8e00-de9a2c7963db",
                    "sense_definition": "dotyczący brawurowej, niebezpiecznej jazdy samochodem, niezgodnej z przepisami ruchu drogowego",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja zatrzymała kierowcę za szaleńczy i <w>piracki</w> rajd ulicami miasta.",
                            "target_lang_translations": {
                                "ru": "Полиция задержала водителя за безумную и <w>лихаческую</w> гонку по улицам города."
                            }
                        },
                        {
                            "text": "Jego <w>piracki</w> styl jazdy stwarza ogromne zagrożenie dla innych kierowców.",
                            "target_lang_translations": {
                                "ru": "Его <w>лихаческий</w> стиль вождения создает огромную угрозу для других водителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "brawurowy",
                        "niebezpieczny"
                    ],
                    "common_phrases": [
                        "piracki rajd",
                        "piracki styl jazdy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с лихачеством, опасной ездой на автомобиле, нарушающей правила дорожного движения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лихаческий",
                                "target_lang_sense_clarification": "о стиле вождения"
                            },
                            {
                                "target_lang_word": "безрассудный",
                                "target_lang_sense_clarification": "опасный"
                            }
                        ]
                    }
                }
            ]
        }

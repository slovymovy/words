{
    "word_family": [
        "coagulatie",
        "coagulaat"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c28e2912-a0ef-3954-83a9-a9ff1735a24d",
                    "sense_definition": "Samenklonteren of stollen tot een vaste massa of vlokken, meestal gezegd van vloeistoffen of colloïdale oplossingen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stollen_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doordat de ionsterkte te hoog was, is dit colloïd <w>gecoaguleerd</w>.",
                            "target_lang_translations": {
                                "en": "Because the ionic strength was too high, this colloid <w>coagulated</w>.",
                                "ru": "Из-за слишком высокой ионной силы этот коллоид <w>коагулировал</w>."
                            }
                        },
                        {
                            "text": "Het bloed zal snel <w>coaguleren</w> zodra het in contact komt met de lucht.",
                            "target_lang_translations": {
                                "en": "The blood will quickly <w>coagulate</w> as soon as it comes into contact with the air.",
                                "ru": "Кровь быстро <w>свернется</w>, как только вступит в контакт с воздухом."
                            }
                        }
                    ],
                    "synonyms": [
                        "stollen",
                        "klonteren",
                        "uitvlokken",
                        "stremmen"
                    ],
                    "common_phrases": [
                        "bloed coaguleert",
                        "snel coaguleren"
                    ],
                    "target_lang_definitions": {
                        "en": "To clump together or solidify into a solid mass or flakes, usually referring to liquids or colloidal solutions.",
                        "ru": "Слипаться или затвердевать в твердую массу или хлопья, обычно говорится о жидкостях или коллоидных растворах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "coagulate",
                                "target_lang_sense_clarification": "general / scientific"
                            },
                            {
                                "target_lang_word": "clot",
                                "target_lang_sense_clarification": "specifically for blood"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "коагулировать",
                                "target_lang_sense_clarification": "научный термин"
                            },
                            {
                                "target_lang_word": "сворачиваться",
                                "target_lang_sense_clarification": "о крови или молоке"
                            },
                            {
                                "target_lang_word": "свертываться",
                                "target_lang_sense_clarification": "синоним сворачиваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7e280b6-03bc-3d66-9e1e-7d24b46b5763",
                    "sense_definition": "Een vloeistof of stof doen samenklonteren of laten stollen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stollen_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De toevoeging van stremsel <w>coaguleerde</w> het eiwit in de melk.",
                            "target_lang_translations": {
                                "en": "The addition of rennet <w>coagulated</w> the protein in the milk.",
                                "ru": "Добавление сычужного фермента <w>свернуло</w> белок в молоке."
                            }
                        },
                        {
                            "text": "De chirurg gebruikt hitte om het bloedvat te <w>coaguleren</w> en de bloeding te stoppen.",
                            "target_lang_translations": {
                                "en": "The surgeon uses heat to <w>coagulate</w> the blood vessel and stop the bleeding.",
                                "ru": "Хирург использует тепло, чтобы <w>коагулировать</w> кровеносный сосуд и остановить кровотечение."
                            }
                        }
                    ],
                    "synonyms": [
                        "stremmen",
                        "laten stollen"
                    ],
                    "common_phrases": [
                        "eiwitten coaguleren"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a liquid or substance to clump together or solidify.",
                        "ru": "Заставлять жидкость или вещество слипаться или затвердевать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "coagulate",
                                "target_lang_sense_clarification": "transitive use"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "коагулировать",
                                "target_lang_sense_clarification": "вызывать коагуляцию (напр. сосудов)"
                            },
                            {
                                "target_lang_word": "сворачивать",
                                "target_lang_sense_clarification": "заставлять свернуться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
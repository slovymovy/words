                    "synonyms": [
                        "aankomen",
                        "verschijnen"
                    ],
                    "antonyms": [
                        "vertrekken"
                    ],
                    "common_phrases": [
                        "komen aanzetten"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often implies annoyance or surprise"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To arrive or appear, often unexpectedly or bringing something along.",
                        "ru": "Приходить или появляться, часто неожиданно или с чем-то при себе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show up",
                                "target_lang_sense_clarification": "to appear, often unexpectedly"
                            },
                            {
                                "target_lang_word": "come up with",
                                "target_lang_sense_clarification": "when bringing a story or excuse"
                            },
                            {
                                "target_lang_word": "turn up",
                                "target_lang_sense_clarification": "to arrive"
                            }

{
    "word_family": [
        "tapetować",
        "tapetowanie",
        "tapeciarz"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6bf634f9-3926-3178-bb55-e7bbfeb11d9d",
                    "sense_definition": "Papier lub inny materiał, często wzorzysty, naklejany na ściany wewnątrz pomieszczeń w celach dekoracyjnych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj cały dzień zrywaliśmy starą <tapetę> w sypialni rodziców.",
                            "target_lang_translations": {
                                "ru": "Вчера мы весь день сдирали старые <обои> в спальне родителей."
                            }
                        },
                        {
                            "text": "Ta wzorzysta <tapeta> zupełnie nie pasuje do nowoczesnych mebli w salonie.",
                            "target_lang_translations": {
                                "ru": "Эти узорчатые <обои> совершенно не подходят к современной мебели в гостиной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "klej do tapet",
                        "kłaść tapetę",
                        "zrywać tapetę",
                        "rolka tapety"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бумага или другой материал, часто с узором, наклеиваемый на стены внутри помещений для украшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обои",
                                "target_lang_sense_clarification": "для стен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94dc73fe-c3fb-311b-b813-f5f1438fa097",
                    "sense_definition": "Obraz lub grafika wyświetlana jako tło na pulpicie komputera lub ekranie telefonu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako <tapetę> na pulpicie mam ustawione zdjęcie mojego psa.",
                            "target_lang_translations": {
                                "ru": "В качестве <обоев> на рабочем столе у меня стоит фотография моей собаки."
                            }
                        },
                        {
                            "text": "Ściągnąłem nową <tapetę> z internetu, żeby odświeżyć wygląd ekranu.",
                            "target_lang_translations": {
                                "ru": "Я скачал новые <обои> из интернета, чтобы освежить вид экрана."
                            }
                        }
                    ],
                    "synonyms": [
                        "tło"
                    ],
                    "common_phrases": [
                        "tapeta na pulpit",
                        "ustawić jako tapetę",
                        "tapeta w telefonie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображение или графика, используемые в качестве фона на рабочем столе компьютера или экране телефона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обои",
                                "target_lang_sense_clarification": "на рабочий стол"
                            },
                            {
                                "target_lang_word": "фоновый рисунок",
                                "target_lang_sense_clarification": "фон экрана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3ce8bb7-13db-3881-a0c9-8d64a9e9b1e5",
                    "sense_definition": "Gruba warstwa makijażu nałożona na twarz, często wyglądająca nienaturalnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmyj z twarzy tę <tapetę>, wyglądasz o wiele ładniej bez niej.",
                            "target_lang_translations": {
                                "ru": "Смой с лица эту <штукатурку>, ты выглядишь намного лучше без неё."
                            }
                        },
                        {
                            "text": "Dziewczyna miała na sobie tyle <tapety>, że ledwo ją poznałem.",
                            "target_lang_translations": {
                                "ru": "На девушке было столько <штукатурки>, что я едва её узнал."
                            }
                        }
                    ],
                    "synonyms": [
                        "makijaż"
                    ],
                    "common_phrases": [
                        "mieć na sobie tapetę",
                        "zmyć tapetę",
                        "tona tapety"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potoczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Толстый слой макияжа на лице, часто выглядящий неестественно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штукатурка",
                                "target_lang_sense_clarification": "макияж (разг.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
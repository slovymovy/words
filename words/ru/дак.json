    "entries": [
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "112424dc-9fc5-3386-b995-e2263a00ac76",
                    "sense_definition": "Разговорный или диалектный вариант слова «так»; используется для связи слов, выражения следствия или усиления.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "particle_variant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Не хватит, <дак> останется.",
                            "target_lang_translations": {
                                "en": "— If it's not enough, <w>then</w> it will remain."
                            }
                        },
                        {
                            "text": "Ты не хочешь идти в кино? <Дак> давай останемся дома.",
                            "target_lang_translations": {
                                "en": "You don't want to go to the cinema? <w>Then</w> let's stay home."
                            }
                        }
                    ],
                    "synonyms": [
                        "дак-бунгало"
                    ],
                    "common_phrases": [
                        "ну дак",
                        "а дак"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "просторечие"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "распространено в северных говорах"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A colloquial or dialectal variant of the word \"tak\" (so, then); used to link words, express consequence, or add emphasis."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "then",
                                "target_lang_sense_clarification": "used to express consequence"
                            },
                            {
                                "target_lang_word": "so",
                                "target_lang_sense_clarification": "used as a conjunction or for emphasis"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1426b8a-7354-32dd-9610-7a5b24143e54",
                    "sense_definition": "Представитель древних северофракийских племён, живших к северу от Дуная до отрогов Карпат (современная Румыния и Молдова).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Римский император Траян вёл долгие войны, чтобы покорить свободолюбивых <даков>.",
                            "target_lang_translations": {
                                "en": "The Roman Emperor Trajan waged long wars to conquer the freedom-loving <Dacians>."
                            }
                        },
                        {
                            "text": "На старинном барельефе изображён <дак>, защищающий свою крепость от легионеров.",
                            "target_lang_translations": {
                                "en": "The ancient bas-relief depicts a <Dacian> defending his fortress against legionaries."
                            }
                        }
                    ],
                    "common_phrases": [
                        "царь даков",
                        "золото даков"
                    ],
                    "target_lang_definitions": {
                        "en": "A member of the ancient North Thracian tribes living north of the Danube to the Carpathian Mountains (modern Romania and Moldova)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Dacian",
                                "target_lang_sense_clarification": "historical people"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3ff2c5d-4679-3fb3-913c-0b5a853830d7",
                    "sense_definition": "Придорожная гостиница или постоялый двор для путешественников в Индии.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Уставшие путники надеялись, что ближайший <дак> окажется свободным и чистым.",
                            "target_lang_translations": {
                                "en": "The tired travelers hoped that the nearest <w>dak</w> would be free and clean."
                            }
                        },
                        {
                            "text": "Наконец <дак> мой заказан к завтрашнему вечеру.",
                            "target_lang_translations": {
                                "en": "Finally, my <w>dak</w> is booked for tomorrow evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "дак-бунгало"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "в Индии"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A roadside inn or rest house for travelers in India."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dak",
                                "target_lang_sense_clarification": "transport or post system in India"
                            },
                            {
                                "target_lang_word": "dak bungalow",
                                "target_lang_sense_clarification": "traveler's rest house"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [
        "plac",
        "placowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a0dc18cf-ca10-3d70-8173-de20d713f4b0",
                    "sense_definition": "Instytucja, zakład lub oddział pełniący określone funkcje publiczne, usługowe lub handlowe (np. szkoła, szpital, bank).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każda <placówka> oświatowa musi zapewniać uczniom odpowiednie warunki do nauki.",
                            "target_lang_translations": {
                                "ru": "Каждое образовательное <учреждение> должно обеспечивать ученикам соответствующие условия для учебы."
                            }
                        },
                        {
                            "text": "Ta <placówka> medyczna słynie z doskonałych specjalistów i nowoczesnego sprzętu.",
                            "target_lang_translations": {
                                "ru": "Это медицинское <учреждение> славится отличными специалистами и современным оборудованием."
                            }
                        }
                    ],
                    "synonyms": [
                        "instytucja",
                        "ośrodek"
                    ],
                    "common_phrases": [
                        "placówka oświatowa",
                        "placówka medyczna",
                        "placówka handlowa",
                        "kierownik placówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение, заведение или отделение, выполняющее определенные общественные, служебные или торговые функции (например, школа, больница, банк)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учреждение",
                                "target_lang_sense_clarification": "организация"
                            },
                            {
                                "target_lang_word": "отделение",
                                "target_lang_sense_clarification": "филиал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c74ba220-b989-329b-bb0f-8078cd0eaa46",
                    "sense_definition": "Urząd będący oficjalnym przedstawicielstwem państwa za granicą, np. ambasada lub konsulat.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "diplomacy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polska <placówka> dyplomatyczna w Londynie udziela pomocy naszym obywatelom.",
                            "target_lang_translations": {
                                "ru": "Польское дипломатическое <представительство> в Лондоне оказывает помощь нашим гражданам."
                            }
                        },
                        {
                            "text": "Rząd podjął decyzję, aby zamknąć tę <placówkę> ze względów politycznych.",
                            "target_lang_translations": {
                                "ru": "Правительство приняло решение закрыть это <представительство> по политическим причинам."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedstawicielstwo"
                    ],
                    "common_phrases": [
                        "placówka dyplomatyczna",
                        "placówka zagraniczna",
                        "pracownik placówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное представительство государства за границей, например, посольство или консульство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представительство",
                                "target_lang_sense_clarification": "дипломатическое"
                            },
                            {
                                "target_lang_word": "миссия",
                                "target_lang_sense_clarification": "дипломатическая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d130311-6ff3-3270-8842-05f9009b0235",
                    "sense_definition": "Wysunięty punkt obronny lub mały oddział wojska wyznaczony do obserwacji i ochrony terenu.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze obsadzili <placówkę> na wzgórzu, aby obserwować ruchy nieprzyjaciela.",
                            "target_lang_translations": {
                                "ru": "Солдаты заняли <пост> на холме, чтобы наблюдать за передвижениями противника."
                            }
                        },
                        {
                            "text": "Nocna <placówka> zameldowała o podejrzanych odgłosach dochodzących z lasu.",
                            "target_lang_translations": {
                                "ru": "Ночной <пост> доложил о подозрительных звуках, доносящихся из леса."
                            }
                        }
                    ],
                    "synonyms": [
                        "posterunek"
                    ],
                    "common_phrases": [
                        "objąć placówkę",
                        "dowódca placówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передовой оборонительный пункт или небольшой отряд войск для наблюдения и охраны местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пост",
                                "target_lang_sense_clarification": "сторожевой"
                            },
                            {
                                "target_lang_word": "застава",
                                "target_lang_sense_clarification": "военная"
                            },
                            {
                                "target_lang_word": "форпост",
                                "target_lang_sense_clarification": "передовой пункт"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "3810dee1-5a2d-3dd6-b462-652be4c256c2",
                    "sense_definition": "Jajo wszy, pasożyta bytującego zazwyczaj na owłosionej skórze człowieka lub zwierzęcia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas kontroli higieny pielęgniarka znalazła u ucznia jedną <gnidę>.",
                            "target_lang_translations": {
                                "ru": "Во время проверки гигиены медсестра нашла у ученика одну <гниду>."
                            }
                        },
                        {
                            "text": "Aby skutecznie pozbyć się pasożytów, trzeba wyczesać wszystkie <gnidy> z włosów.",
                            "target_lang_translations": {
                                "ru": "Чтобы эффективно избавиться от паразитов, нужно вычесать все <гниды> из волос."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Яйцо вши, паразита, обитающего обычно на волосистой части кожи человека или животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнида",
                                "target_lang_sense_clarification": "яйцо вши"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3f19f5d-a0dd-34bb-a450-39a10b885446",
                    "sense_definition": "Osoba podła, fałszywa, złośliwa i godna pogardy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie ufaj mu, to straszna <gnida>, która oszukała już wielu ludzi.",
                            "target_lang_translations": {
                                "ru": "Не доверяй ему, это страшная <гнида>, которая обманула уже многих людей."
                            }
                        },
                        {
                            "text": "Tylko wyjątkowa <gnida> mogłaby zrobić coś tak okrutnego własnej rodzinie.",
                            "target_lang_translations": {
                                "ru": "Только исключительная <гнида> могла бы сделать что-то столь жестокое собственной семье."
                            }
                        }
                    ],
                    "synonyms": [
                        "szuja"
                    ],
                    "common_phrases": [
                        "wredna gnida",
                        "fałszywa gnida"
                    ],
                    "traits": [
                        {

                },
                {
                    "sense_id": "0c8e2135-36bd-3594-addc-873dd09cc4cf",
                    "sense_definition": "An organization that checks that certain laws or rules are obeyed.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

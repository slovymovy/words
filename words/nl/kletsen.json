                            {
                                "target_lang_word": "трепаться",
                                "target_lang_sense_clarification": "разг. говорить пустое или лгать"
                            },
                            {
                                "target_lang_word": "молоть вздор",
                                "target_lang_sense_clarification": "говорить бессмыслицу"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "talk nonsense",
                                "target_lang_sense_clarification": "say things that make no sense or are untrue"

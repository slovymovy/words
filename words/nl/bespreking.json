                {
                    "sense_id": "0b528778-601a-3891-887d-2da1134a1e26",
                    "sense_definition": "Een mondeling overleg of vergadering waarbij mensen samenkomen om een onderwerp te behandelen of besluiten te nemen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>bespreking</w> met de nieuwe klanten verliep erg soepel.",
                            "target_lang_translations": {
                                "ru": "<w>Обсуждение</w> с новыми клиентами прошло очень гладко.",
                                "en": "The <w>meeting</w> with the new clients went very smoothly."
                            }
                        },
                        {
                            "text": "Tijdens de <w>bespreking</w> hebben we besloten om het project uit te stellen.",
                            "target_lang_translations": {
                                "ru": "В ходе <w>обсуждения</w> мы решили отложить проект.",
                                "en": "During the <w>meeting</w>, we decided to postpone the project."
                            }
                        }
                    ],
                    "synonyms": [
                        "overleg",
                        "vergadering",
                        "zitting",
                        "discussie"
                    ],
                    "common_phrases": [

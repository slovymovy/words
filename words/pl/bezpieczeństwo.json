            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1f134002-b0a2-3d94-97cb-1f492d15d71b",
                    "sense_definition": "Stan, w którym nie występuje zagrożenie ani ryzyko utraty zdrowia, życia lub mienia; sytuacja bycia chronionym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_of_safety",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy pasażerowie muszą zapiąć pasy <w>bezpieczeństwa</w> przed startem samolotu.",
                            "target_lang_translations": {
                                "ru": "Все пассажиры должны пристегнуть ремни <w>безопасности</w> перед взлетом самолета."
                            }
                        },
                        {
                            "text": "Nowe przepisy mają na celu poprawę <w>bezpieczeństwa</w> na drogach w całym kraju.",
                            "target_lang_translations": {
                                "ru": "Новые правила направлены на повышение <w>безопасности</w> на дорогах по всей стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "ochrona",
                        "pewność"
                    ],
                    "antonyms": [
                        "niebezpieczeństwo",
                        "zagrożenie"
                    ],
                    "common_phrases": [
                        "pasy bezpieczeństwa",
                        "bezpieczeństwo narodowe",
                        "zasady bezpieczeństwa",
                        "bezpieczeństwo publiczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, при котором отсутствует угроза или риск для здоровья, жизни или имущества; положение защищенности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безопасность",
                                "target_lang_sense_clarification": "отсутствие опасности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "587b6a24-1bb7-3da5-8036-69fb833a6141",
                    "sense_definition": "Psychiczny stan spokoju, braku lęku i obaw; subiektywne odczucie bycia bezpiecznym.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "feeling_of_safety",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obecność rodziców dawała dziecku ogromne poczucie <w>bezpieczeństwa</w> w nowym miejscu.",
                            "target_lang_translations": {
                                "ru": "Присутствие родителей давало ребенку огромное чувство <w>безопасности</w> в новом месте."
                            }
                        },
                        {
                            "text": "Stabilna praca wpływa na nasze finansowe <w>bezpieczeństwo</w> i spokój ducha.",
                            "target_lang_translations": {
                                "ru": "Стабильная работа влияет на нашу финансовую <w>безопасность</w> и душевное спокойствие."
                            }
                        }
                    ],
                    "synonyms": [
                        "pewność"
                    ],
                    "antonyms": [
                        "zagrożenie"
                    ],
                    "common_phrases": [
                        "poczucie bezpieczeństwa",
                        "fałszywe bezpieczeństwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психическое состояние спокойствия, отсутствия страха и тревоги; субъективное ощущение защищенности."

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "706ed5d5-903b-3c46-9414-a05f240469db",
                    "sense_definition": "Betrekking hebbend op Nederland of de Nederlandse cultuur.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "meaning_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We eten vanavond een typisch <w>Hollandse</w> maaltijd met aardappelen en groente.",
                            "target_lang_translations": {
                                "ru": "Сегодня вечером мы едим типичное <w>голландское</w> блюдо с картофелем и овощами."
                            }
                        },
                        {
                            "text": "Veel buitenlandse toeristen komen kijken naar de oude <w>Hollandse</w> molens.",
                            "target_lang_translations": {
                                "ru": "Многие иностранные туристы приезжают посмотреть на старинные <w>голландские</w> мельницы."
                            }
                        }
                    ],
                    "synonyms": [
                        "Hollander"
                    ],
                    "common_phrases": [
                        "op zijn Hollands",
                        "Hollandse pot",
                        "Hollandse nieuwe"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "vaak gebruikt als synoniem voor Nederlands"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Нидерландам или нидерландской культуре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голландский",
                                "target_lang_sense_clarification": "относящийся к Нидерландам"
                            },
                            {
                                "target_lang_word": "нидерландский",
                                "target_lang_sense_clarification": "официальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "007f247e-7581-311e-8e1e-a2d8bc534931",
                    "sense_definition": "Afkomstig uit of behorend tot de provincies Noord- en Zuid-Holland.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "meaning_regional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij spreekt met een zwaar <w>Hollands</w> accent dat direct opvalt.",
                            "target_lang_translations": {
                                "ru": "Он говорит с сильным <w>голландским</w> акцентом, который сразу бросается в глаза."
                            }
                        },
                        {
                            "text": "De <w>Hollandse</w> duinen zijn belangrijk voor de bescherming van het land.",
                            "target_lang_translations": {
                                "ru": "<w>Голландские</w> дюны важны для защиты страны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Hollands glorie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий из провинций Северная или Южная Голландия или относящийся к ним."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голландский",
                                "target_lang_sense_clarification": "относящийся к региону Голландия"
                            }
                        ]
                    }
                }
            ]
        },
        {

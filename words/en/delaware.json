                {
                    "sense_id": "28c46163-79fb-3d22-b27c-a4b27ff03181",
                    "sense_definition": "A state in the northeastern United States, located on the Atlantic coast.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "place_us",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Dover is the capital city of <w>Delaware</w>.",
                            "target_lang_translations": {
                                "ru": "Довер — столица <w>Делавэра</w>."
                            }
                        },
                        {
                            "text": "Many companies choose to incorporate in the state of <w>Delaware</w>.",
                            "target_lang_translations": {
                                "ru": "Многие компании предпочитают регистрироваться в штате <w>Делавэр</w>."
                            }

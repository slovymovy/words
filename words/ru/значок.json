    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4000bc5f-ea1d-3249-958e-ad9da17371ed",
                    "sense_definition": "Маленькая пластинка из металла, пластика или другого материала с рисунком или надписью, которую носят на одежде как украшение или знак принадлежности к организации.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "badge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На лацкане его пиджака блестел золотой <значок> университета.",
                            "target_lang_translations": {
                                "en": "A gold university <w>badge</w> shone on the lapel of his jacket."
                            }
                        },
                        {
                            "text": "Туристы часто покупают сувенирные <значки> на память о поездке.",
                            "target_lang_translations": {
                                "en": "Tourists often buy souvenir <w>pins</w> as a memento of the trip."
                            }
                        }
                    ],
                    "synonyms": [
                        "нагрудный знак"
                    ],
                    "common_phrases": [
                        "носить значок",
                        "приколоть значок",
                        "коллекционировать значки",
                        "почётный значок"
                    ],
                    "target_lang_definitions": {
                        "en": "A small plate made of metal, plastic, or other material with a design or inscription, worn on clothing as an ornament or sign of affiliation with an organization."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "badge",
                                "target_lang_sense_clarification": "sign of membership or authority"
                            },
                            {
                                "target_lang_word": "pin",
                                "target_lang_sense_clarification": "decorative item worn on clothes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37ee0318-3c06-3326-a73b-7f5534fb2903",
                    "sense_definition": "Небольшое условное изображение или символ, часто используемый в компьютерных интерфейсах для обозначения программы или файла.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чтобы открыть программу, нужно дважды кликнуть на этот <значок>.",
                            "target_lang_translations": {
                                "en": "To open the program, you need to double-click this <w>icon</w>."
                            }
                        },
                        {
                            "text": "На карте города каждый <значок> обозначает достопримечательность или музей.",
                            "target_lang_translations": {
                                "en": "On the city map, each <w>icon</w> marks a landmark or a museum."
                            }
                        }
                    ],
                    "synonyms": [
                        "иконка",
                        "символ",
                        "эмблема"
                    ],
                    "common_phrases": [
                        "значок приложения",
                        "нажать на значок",
                        "значок на экране"
                    ],
                    "target_lang_definitions": {
                        "en": "A small conventional image or symbol, often used in computer interfaces to represent a program or file."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "icon",
                                "target_lang_sense_clarification": "computer interface element"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80e0cd67-159c-35e3-b451-ae87f0a5ef3a",
                    "sense_definition": "Уменьшительная форма слова <w>знак</w>; небольшая пометка, сделанная пишущим предметом.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель поставил <значок> на полях тетради, указывая на ошибку.",
                            "target_lang_translations": {
                                "en": "The teacher put a <w>mark</w> in the notebook margin, pointing out a mistake."
                            }
                        },
                        {
                            "text": "Заполняя анкету, поставьте любой <значок> в нужном квадрате.",
                            "target_lang_translations": {
                                "en": "When filling out the form, put any <w>mark</w> in the required square."
                            }
                        }
                    ],
                    "synonyms": [
                        "пометка",
                        "галочка"
                    ],
                    "common_phrases": [
                        "поставить значок"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A diminutive form of the word \"sign\"; a small mark made with a writing instrument."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mark",
                                "target_lang_sense_clarification": "written symbol"
                            },
                            {
                                "target_lang_word": "check mark",
                                "target_lang_sense_clarification": "tick used for selection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1def6f03-507f-3780-8a3f-ca454663944f",
                    "sense_definition": "Небольшой флаг, служивший в старину опознавательным знаком для воинской части или начальника.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Впереди отряда ехал всадник, державший в руке полковой <значок>.",
                            "target_lang_translations": {
                                "en": "Ahead of the troop rode a horseman holding the regimental <w>pennant</w>."
                            }
                        },
                        {
                            "text": "Историки изучали старинные военные <значки>, найденные на месте битвы.",
                            "target_lang_translations": {
                                "en": "Historians studied ancient military <w>pennants</w> found at the battle site."
                            }
                        }
                    ],
                    "synonyms": [
                        "флажок",
                        "вымпел"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A small flag that served in olden times as an identifying sign for a military unit or commander."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pennant",
                                "target_lang_sense_clarification": "historical military flag"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
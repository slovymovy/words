    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "42ac46bc-2893-3864-96db-4e3eabab7f5b",
                    "sense_definition": "Принять окончательное решение сделать что-либо, преодолев сомнения или страх",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я долго думал и наконец <решился> сменить работу.",
                            "target_lang_translations": {
                                "en": "I thought for a long time and finally <decided> to change jobs."
                            }
                        },
                        {
                            "text": "Она не <решилась> войти в тёмную комнату одна.",
                            "target_lang_translations": {
                                "en": "She did not <dare> to enter the dark room alone."
                            }
                        }
                    ],
                    "synonyms": [
                        "осмелиться",
                        "отважиться"
                    ],
                    "antonyms": [
                        "передумать"
                    ],
                    "common_phrases": [
                        "решиться на шаг",
                        "наконец решиться",
                        "решиться сказать правду",
                        "не решиться сделать"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a final decision to do something, often after overcoming doubt or fear"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decide",
                                "target_lang_sense_clarification": "to make a choice to do something"
                            },
                            {
                                "target_lang_word": "make up one's mind",
                                "target_lang_sense_clarification": "to reach a firm decision"
                            },
                            {
                                "target_lang_word": "dare",
                                "target_lang_sense_clarification": "to have the courage to do something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d020e5e-2d32-3856-b0d1-fe48f3580804",
                    "sense_definition": "Получить определённый итог или завершение (о вопросе, проблеме, судьбе)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта сложная проблема <решилась> сама собой.",
                            "target_lang_translations": {
                                "en": "This complex problem <resolved> itself."
                            }
                        },
                        {
                            "text": "Завтра <решится> моя судьба на экзамене.",
                            "target_lang_translations": {
                                "en": "Tomorrow my fate <will be decided> at the exam."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрешиться",
                        "уладиться"
                    ],
                    "common_phrases": [
                        "вопрос решился",
                        "судьба решилась",
                        "всё решится",
                        "дело решилось"
                    ],
                    "target_lang_definitions": {
                        "en": "To reach a conclusion, outcome, or settlement (regarding a problem, question, or fate)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be resolved",
                                "target_lang_sense_clarification": "of a problem or issue"
                            },
                            {
                                "target_lang_word": "be decided",
                                "target_lang_sense_clarification": "of fate or an outcome"
                            },
                            {
                                "target_lang_word": "be settled",
                                "target_lang_sense_clarification": "of a matter or dispute"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
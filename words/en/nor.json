    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "574db076-1e1b-316b-a7da-779b43102df4",
                    "sense_definition": "Used after 'neither' to introduce the second or subsequent negative option in a series, indicating that none of the options are true.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "negation_series",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am neither hungry <w>nor</w> thirsty at the moment."
                        },
                        {
                            "text": "The result was neither here <w>nor</w> there in the final analysis."
                        }
                    ],
                    "common_phrases": [
                        "neither here nor there",
                        "neither fish nor fowl",
                        "neither rhyme nor reason"
                    ]
                },
                {
                    "sense_id": "f7ba54f0-3e6d-31b0-bac5-f1df549db3c8",
                    "sense_definition": "Used to introduce a further negative statement following a previous negative one, typically requiring the verb to be placed before the subject.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "negation_continuation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The struggle did not end, <w>nor</w> was it diminished by time."
                        },
                        {
                            "text": "I have never visited that city, <w>nor</w> do I intend to go in the future."
                        }
                    ]
                },
                {
                    "sense_id": "0c810959-3219-3a18-9216-b14d37f4d7c0",
                    "sense_definition": "And not; used to introduce a negative statement that adds to what has been said, often in a formal or literary context.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "negation_continuation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They are happy with the decision, <w>nor</w> need we worry about the outcome."
                        },
                        {
                            "text": "He did not stop to think, <w>nor</w> did he hesitate for a moment."
                        }
                    ]
                },
                {
                    "sense_id": "5a1d964f-8af8-3664-944f-e57e91c5bd62",
                    "sense_definition": "Used in the place of 'neither' to introduce the first of two negative alternatives.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "negation_series",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Nor</w> you <w>nor</w> your house were spoken of during the meeting."
                        },
                        {
                            "text": "<w>Nor</w> silver <w>nor</w> gold could buy his loyalty."
                        }
                    ],
                    "synonyms": [
                        "neither"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "poetic or obsolete usage"
                        }
                    ]
                },
                {
                    "sense_id": "868561e1-0dd4-3c1d-84bf-0106c858157d",
                    "sense_definition": "Used to indicate difference or comparison; equivalent to 'than'.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is no better <w>nor</w> you are at this game."
                        },
                        {
                            "text": "I would sooner go home <w>nor</w> stay here another minute."
                        }
                    ],
                    "synonyms": [
                        "than"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ]
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a5fc6971-6480-3760-bb74-f911dcf0b62f",
                    "sense_definition": "A logic gate or boolean operator that produces a true output only when all inputs are false (Not OR).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "logic_gate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The circuit design utilizes a <w>NOR</w> gate to invert the signal when both inputs are low."
                        },
                        {
                            "text": "In boolean logic, a <w>NOR</w> operation is the negation of an OR operation."
                        }
                    ],
                    "antonyms": [
                        "OR"
                    ],
                    "common_phrases": [
                        "NOR gate"
                    ]
                },
                {
                    "sense_id": "b5769975-b60e-3c20-bb41-33f528852bf7",
                    "sense_definition": "Acronym for nucleolus organiser region, a part of a chromosome.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biology_acronym",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist stained the chromosomes to identify the location of the <w>NOR</w>."
                        },
                        {
                            "text": "Silver staining is a common technique used to visualize active <w>NORs</w> in cells."
                        }
                    ]
                }
            ]
        }

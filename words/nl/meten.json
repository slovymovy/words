    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5368a80c-38f2-3fd0-bf37-a03b329a6426",
                    "sense_definition": "De afmeting, grootte, hoeveelheid of waarde van iets bepalen, vaak met behulp van een instrument.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "measure_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>mat</w> de lengte van de kamer met een meetlat.",
                            "target_lang_translations": {
                                "ru": "Он <w>измерил</w> длину комнаты с помощью линейки."
                            }
                        },
                        {
                            "text": "De verpleegkundige <w>meet</w> elk uur mijn temperatuur en bloeddruk.",
                            "target_lang_translations": {
                                "ru": "Медсестра каждый час <w>измеряет</w> мою температуру и артериальное давление."
                            }
                        }
                    ],
                    "synonyms": [
                        "opmeten",
                        "bepalen"
                    ],
                    "antonyms": [
                        "schatten",
                        "gokken"
                    ],
                    "common_phrases": [
                        "de temperatuur meten",
                        "met twee maten meten",
                        "afstand meten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определять размер, величину, количество или значение чего-либо, часто с помощью инструмента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "измерять",
                                "target_lang_sense_clarification": "определять величину"
                            },
                            {
                                "target_lang_word": "мерить",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d21805ed-2ade-3a91-bf7d-60caa443ca49",
                    "sense_definition": "Zich vergelijken met iemand anders of de strijd aangaan om te bepalen wie beter of sterker is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "measure_compete",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze wedstrijd kan de jonge speler zich <w>meten</w> met de wereldkampioen.",
                            "target_lang_translations": {
                                "ru": "В этом матче молодой игрок может <w>помериться силами</w> с чемпионом мира."
                            }
                        },
                        {
                            "text": "Onze lokale keuken kan zich gemakkelijk <w>meten</w> met die van andere landen.",
                            "target_lang_translations": {
                                "ru": "Наша местная кухня может легко <w>сравниться</w> с кухней других стран."
                            }
                        }
                    ],
                    "synonyms": [
                        "wedijveren",
                        "concurreren"
                    ],
                    "common_phrases": [
                        "zich meten met",
                        "zich kunnen meten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сравнивать себя с кем-то другим или вступать в борьбу, чтобы определить, кто лучше или сильнее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состязаться",
                                "target_lang_sense_clarification": "мериться силами"
                            },
                            {
                                "target_lang_word": "сравниться",
                                "target_lang_sense_clarification": "быть равным по качеству"
                            },
                            {
                                "target_lang_word": "тягаться",
                                "target_lang_sense_clarification": "соперничать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b1565285-a2f3-3e7e-98c9-19ee0b4c39f6",
                    "sense_definition": "Meervoud van <w>meet</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De scheidsrechter controleerde of de lopers achter de <w>meten</w> stonden.",
                            "target_lang_translations": {
                                "ru": "Судья проверил, стоят ли бегуны за <w>стартовыми чертами</w>."
                            }
                        },
                        {
                            "text": "Op het oude sportveld waren de <w>meten</w> nauwelijks meer zichtbaar.",
                            "target_lang_translations": {
                                "ru": "На старом спортивном поле <w>линии разметки</w> были едва видны."
                            }
                        }
                    ],

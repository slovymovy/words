            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c5a83735-b389-38ad-8cbc-c7f8272842f8",
                    "sense_definition": "Наполненный доверху, содержащий в себе много кого-либо или чего-либо; занятый целиком.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "filled_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В комнату вошёл официант, неся <w>полную</w> тарелку супа.",
                            "target_lang_translations": {
                                "en": "The waiter entered the room carrying a <w>full</w> plate of soup."
                            }
                        },
                        {
                            "text": "Автобус был <w>полон</w> людей, и нам пришлось стоять.",
                            "target_lang_translations": {
                                "en": "The bus was <w>full</w> of people, and we had to stand."
                            }
                        },
                        {
                            "text": "Я набрал <w>полные</w> карманы яблок в нашем саду.",
                            "target_lang_translations": {
                                "en": "I gathered <w>full</w> pockets of apples in our garden."
                            }
                        }
                    ],
                    "synonyms": [
                        "наполненный",
                        "заполненный",
                        "набитый"
                    ],
                    "antonyms": [
                        "пустой"
                    ],
                    "common_phrases": [
                        "полный стакан",
                        "полная сумка",
                        "полный автобус",
                        "полный зал"
                    ],
                    "target_lang_definitions": {
                        "en": "Containing as much or as many as possible; having no empty space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "full",
                                "target_lang_sense_clarification": "containing the maximum possible amount"
                            },
                            {
                                "target_lang_word": "packed",
                                "target_lang_sense_clarification": "filled to capacity (often with people)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fb1c3d9-e76d-385f-bd59-2fe30181eb73",
                    "sense_definition": "Абсолютный, проявляющийся в высшей степени (часто о тишине, темноте, победе или поражении).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "absolute_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В зале наступила <w>полная</w> тишина перед началом концерта.",
                            "target_lang_translations": {
                                "en": "There was <w>complete</w> silence in the hall before the concert started."
                            }
                        },
                        {
                            "text": "Мы одержали <w>полную</w> победу в этом сложном матче.",
                            "target_lang_translations": {
                                "en": "We won a <w>complete</w> victory in this difficult match."
                            }
                        },
                        {
                            "text": "В комнате была <w>полная</w> темнота, и я ничего не видел.",
                            "target_lang_translations": {
                                "en": "There was <w>total</w> darkness in the room, and I saw nothing."
                            }
                        }
                    ],
                    "synonyms": [
                        "законченный",
                        "целый"
                    ],
                    "antonyms": [
                        "частичный",
                        "неполный"
                    ],
                    "common_phrases": [
                        "полная тишина",
                        "полная темнота",
                        "полный восторг",
                        "полный провал",
                        "в полном порядке"
                    ],
                    "target_lang_definitions": {
                        "en": "Total and absolute; used to emphasize the degree of a state or quality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "complete",
                                "target_lang_sense_clarification": "total; with nothing missing"
                            },
                            {
                                "target_lang_word": "total",
                                "target_lang_sense_clarification": "entire; absolute"
                            },
                            {
                                "target_lang_word": "absolute",
                                "target_lang_sense_clarification": "complete; utter"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db30f5f8-326c-33af-ad65-86eb710364eb",
                    "sense_definition": "Упитанный, имеющий лишний вес (о человеке или частях тела).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "body_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя тётя — добрая и немного <w>полная</w> женщина.",
                            "target_lang_translations": {
                                "en": "My aunt is a kind and slightly <w>stout</w> woman."
                            }
                        },
                        {
                            "text": "Врач посоветовал этому <w>полному</w> мужчине больше двигаться.",
                            "target_lang_translations": {
                                "en": "The doctor advised this <w>overweight</w> man to move more."
                            }
                        }
                    ],
                    "synonyms": [
                        "упитанный",
                        "тучный"
                    ],
                    "antonyms": [
                        "худой",
                        "тонкий",
                        "тощий"
                    ],
                    "common_phrases": [
                        "полная женщина",
                        "полный мужчина",
                        "полные руки",
                        "полное лицо"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a large body size; overweight or plump."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stout",
                                "target_lang_sense_clarification": "somewhat fat or of heavy build"
                            },
                            {
                                "target_lang_word": "full",
                                "target_lang_sense_clarification": "plump or rounded in shape"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2640d169-36f0-3aa4-8886-7bf0a372f8a6",
                    "sense_definition": "Включающий все части, исчерпывающий; целый, весь.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "complete_set",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам нужен <w>полный</w> список всех участников конференции.",
                            "target_lang_translations": {
                                "en": "We need a <w>full</w> list of all conference participants."
                            }
                        },
                        {
                            "text": "В библиотеке есть <w>полное</w> собрание сочинений Пушкина.",
                            "target_lang_translations": {
                                "en": "The library has a <w>complete</w> collection of Pushkin's works."
                            }
                        },
                        {
                            "text": "Сегодня у меня <w>полный</w> рабочий день.",
                            "target_lang_translations": {
                                "en": "I have a <w>full</w> workday today."
                            }
                        }
                    ],
                    "synonyms": [
                        "целый",
                        "весь"
                    ],
                    "antonyms": [
                        "неполный",
                        "частичный"
                    ],
                    "common_phrases": [
                        "полный список",
                        "полное собрание",
                        "полный комплект",
                        "полный рабочий день"
                    ],
                    "target_lang_definitions": {
                        "en": "Including all necessary parts or details; exhaustive."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "complete",
                                "target_lang_sense_clarification": "having all parts or elements"
                            },
                            {
                                "target_lang_word": "full",
                                "target_lang_sense_clarification": "containing all details; with nothing omitted"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e5f3a94-9091-3012-a0d1-7c5514a86106",
                    "sense_definition": "Целиком охваченный каким-либо сильным чувством, мыслью или качеством.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "filled_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её взгляд был <w>полон</w> любви и нежности.",
                            "target_lang_translations": {
                                "en": "Her gaze was <w>full</w> of love and tenderness."
                            }
                        },
                        {
                            "text": "Мы начали этот проект, <w>полные</w> энтузиазма и новых идей.",
                            "target_lang_translations": {
                                "en": "We started this project <w>full</w> of enthusiasm and new ideas."
                            }
                        }
                    ],
                    "synonyms": [
                        "захваченный",
                        "увлечённый"
                    ],
                    "common_phrases": [
                        "полон сил",
                        "полон надежд",
                        "полон решимости",
                        "взгляд полный ужаса"
                    ],
                    "target_lang_definitions": {
                        "en": "Filled with a specific emotion or quality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "full",
                                "target_lang_sense_clarification": "abounding with; filled with emotions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b679eff-2a18-3914-af72-3660ff103fe9",
                    "sense_definition": "Наивысший, предельный или ничем не ограниченный (о правах, власти, скорости).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "maximum_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор дал мне <w>полную</w> свободу действий в этом вопросе.",
                            "target_lang_translations": {
                                "en": "The director gave me <w>complete</w> freedom of action in this matter."
                            }
                        },
                        {
                            "text": "Корабль шёл <w>полным</w> ходом к берегу.",
                            "target_lang_translations": {
                                "en": "The ship was moving at <w>full</w> speed towards the shore."
                            }
                        },
                        {
                            "text": "У него есть <w>полное</w> право требовать компенсацию.",
                            "target_lang_translations": {
                                "en": "He has the <w>full</w> right to demand compensation."
                            }
                        }
                    ],
                    "synonyms": [
                        "наивысший",
                        "максимальный",
                        "законченный"
                    ],
                    "antonyms": [
                        "ограниченный",
                        "стеснённый"
                    ],
                    "common_phrases": [
                        "полная свобода",
                        "полное право",
                        "полная власть",
                        "полный ход"
                    ],
                    "target_lang_definitions": {
                        "en": "Maximum in degree; not limited by anything."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "full",
                                "target_lang_sense_clarification": "maximum; not limited"
                            },

{
    "word_family": [
        "kogelstoten",
        "kogelstoter"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6cbea5ad-1b38-3601-9fcc-8926b7e4f289",
                    "sense_definition": "De handeling van het wegstoten van een zware metalen bal (kogel) als onderdeel van een atletiekwedstrijd.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met een krachtige <w>kogelstoot</w> wist de atleet zijn persoonlijke record te verbreken.",
                            "target_lang_translations": {
                                "ru": "Благодаря мощному <w>толчку ядра</w> атлет смог побить свой личный рекорд."
                            }
                        },
                        {
                            "text": "De jury keurde de <w>kogelstoot</w> af omdat de deelnemer buiten de ring stapte.",
                            "target_lang_translations": {
                                "ru": "Судьи не засчитали <w>толчок ядра</w>, так как участник вышел за пределы круга."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een verre kogelstoot",
                        "geldige kogelstoot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по выталкиванию тяжелого металлического шара (ядра) в рамках соревнований по легкой атлетике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толчок ядра",
                                "target_lang_sense_clarification": "спортивное действие"
                            },
                            {
                                "target_lang_word": "попытка в толкании ядра",
                                "target_lang_sense_clarification": "в контексте соревнований"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "446c75ec-bac4-3105-87a5-3eb3662a0383",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd, gebiedende wijs of inversie van <w>kogelstoten</w>.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kogelstoot</w> al jaren bij de lokale atletiekvereniging.",
                            "target_lang_translations": {
                                "ru": "Я <w>занимаюсь толканием ядра</w> уже много лет в местном легкоатлетическом клубе."
                            }
                        },
                        {
                            "text": "<w>Kogelstoot</w> jij morgen ook tijdens de wedstrijd?",
                            "target_lang_translations": {
                                "ru": "Ты завтра тоже <w>толкаешь ядро</w> на соревнованиях?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени, повелительное наклонение или инверсия глагола 'kogelstoten' (толкать ядро)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толкаю ядро",
                                "target_lang_sense_clarification": "действие от первого лица"
                            },
                            {
                                "target_lang_word": "толкать ядро",
                                "target_lang_sense_clarification": "инфинитив"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            },
                            {
                                "target_lang_word": "кипа",
                                "target_lang_sense_clarification": "хлопка, бумаги или ткани"
                            },
                            {
                                "target_lang_word": "мешок",
                                "target_lang_sense_clarification": "общий термин для большой упаковки"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "bale",
                                "target_lang_sense_clarification": "of hay, cotton, or paper"
                            }

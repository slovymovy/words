{
    "word_family": [
        "verwijzen",
        "verwijzer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d814b68e-8772-3a61-8526-8571d1bcac1c",
                    "sense_definition": "Een vermelding van een bron of een ander deel van de tekst waar meer informatie te vinden is.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb helaas de bron in deze <w>verwijzing</w> niet terug kunnen vinden.",
                            "target_lang_translations": {
                                "ru": "К сожалению, я не смог найти источник по этой <w>ссылке</w>."
                            }
                        },
                        {
                            "text": "Het artikel bevat een duidelijke <w>verwijzing</w> naar eerder onderzoek over dit onderwerp.",
                            "target_lang_translations": {
                                "ru": "Статья содержит ясную <w>ссылку</w> на предыдущее исследование по этой теме."
                            }
                        }
                    ],
                    "synonyms": [
                        "referentie"
                    ],
                    "common_phrases": [
                        "een verwijzing naar",
                        "bibliografische verwijzing",
                        "een verwijzing maken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упоминание источника или другой части текста, где можно найти дополнительную информацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ссылка",
                                "target_lang_sense_clarification": "на источник или литературу"
                            },
                            {
                                "target_lang_word": "отсылка",
                                "target_lang_sense_clarification": "упоминание чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05d722f5-08ce-3b90-8cc3-cf178c0f8d8c",
                    "sense_definition": "Het doorsturen van een persoon naar een specialist of een andere instantie voor hulp of behandeling.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De huisarts gaf me een <w>verwijzing</w> voor de dermatoloog in het ziekenhuis.",
                            "target_lang_translations": {
                                "ru": "Семейный врач дал мне <w>направление</w> к дерматологу в больницу."
                            }
                        },
                        {
                            "text": "Zonder geldige <w>verwijzing</w> worden de kosten van de specialist niet vergoed.",
                            "target_lang_translations": {
                                "ru": "Без действительного <w>направления</w> расходы на специалиста не возмещаются."
                            }
                        }
                    ],
                    "common_phrases": [
                        "verwijzing naar de specialist",
                        "medische verwijzing",
                        "een verwijzing krijgen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направление человека к специалисту или в другое учреждение для получения помощи или лечения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "направление",
                                "target_lang_sense_clarification": "медицинский документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c389393-f456-356f-8ea0-1a8928537d69",
                    "sense_definition": "Een koppeling in een digitale tekst die leidt naar een andere webpagina of bestand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klik op de blauwe <w>verwijzing</w> om direct naar de website te gaan.",
                            "target_lang_translations": {
                                "ru": "Нажмите на синюю <w>ссылку</w>, чтобы перейти прямо на сайт."
                            }
                        },
                        {
                            "text": "De <w>verwijzing</w> in de e-mail werkte niet meer omdat de pagina was verwijderd.",
                            "target_lang_translations": {
                                "ru": "<w>Ссылка</w> в письме больше не работала, так как страница была удалена."
                            }
                        }
                    ],
                    "common_phrases": [
                        "klikken op de verwijzing",
                        "externe verwijzing",
                        "digitale verwijzing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связь в цифровом тексте, ведущая на другую веб-страницу или файл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ссылка",
                                "target_lang_sense_clarification": "гиперссылка в интернете"
                            },
                            {
                                "target_lang_word": "гиперссылка",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            {
                                "target_lang_word": "непокрытый",
                                "target_lang_sense_clarification": "о расходах или долгах"
                            },
                            {
                                "target_lang_word": "необеспеченный",
                                "target_lang_sense_clarification": "не имеющий гарантий или средств"
                            }
                        ]

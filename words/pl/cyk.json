    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "82701aa0-fc13-3164-986d-e0b2ea65e65b",
                    "sense_definition": "Słowo naśladujące miarowy, krótki dźwięk, na przykład wydawany przez mechanizm zegara.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_imitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary zegar na ścianie robił głośne <w>cyk</w> w kompletnej ciszy.",
                            "target_lang_translations": {
                                "ru": "Старые часы на стене издавали громкое <w>тик</w> в полной тишине."
                            }
                        },
                        {
                            "text": "Mechanizm wydał ciche <w>cyk</w> i nagle przestał działać.",
                            "target_lang_translations": {
                                "ru": "Механизм издал тихое <w>щёлк</w> и внезапно перестал работать."
                            }
                        }
                    ],
                    "synonyms": [
                        "tyk",
                        "tik-tak"
                    ],
                    "common_phrases": [
                        "cyk-cyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, имитирующее ритмичный короткий звук, например, звук часового механизма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тик",
                                "target_lang_sense_clarification": "звук часов"
                            },
                            {
                                "target_lang_word": "щёлк",
                                "target_lang_sense_clarification": "звук механизма"
                            },
                            {
                                "target_lang_word": "чик",
                                "target_lang_sense_clarification": "короткий резкий звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7722a43-58e9-378d-a420-ceaaef3a2e7a",
                    "sense_definition": "Słowo naśladujące charakterystyczny odgłos wydawany przez niektóre owady, takie jak świerszcze.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sound_imitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wysokiej trawie rozległo się nagle wyraźne <w>cyk</w> świerszcza.",
                            "target_lang_translations": {
                                "ru": "В высокой траве внезапно раздалось отчётливое <w>цык</w> сверчка."
                            }
                        },
                        {
                            "text": "Wieczorem na łące słychać było tylko szum wiatru i rytmiczne <w>cyk</w> owadów.",
                            "target_lang_translations": {
                                "ru": "Вечером на лугу был слышен только шум ветра и ритмичное <w>цык</w> насекомых."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, имитирующее характерный звук, издаваемый некоторыми насекомыми, такими как сверчки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цык",
                                "target_lang_sense_clarification": "звук насекомого"
                            },
                            {
                                "target_lang_word": "стрек",
                                "target_lang_sense_clarification": "стрекотание"
                            }
                        ]
                    }
                }
            ]

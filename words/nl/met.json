    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "f1ced21e-829e-3da8-9c76-3f2a8a9816dc",
                    "sense_definition": "In gezelschap van of samen met iemand of iets.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "accompaniment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga vanavond <met> mijn vrienden naar de bioscoop.",
                            "target_lang_translations": {
                                "en": "I am going to the cinema <with> my friends tonight."
                            }
                        },
                        {
                            "text": "Wil je koffie <met> melk en suiker of drink je het zwart?",
                            "target_lang_translations": {
                                "en": "Do you want coffee <with> milk and sugar or do you drink it black?"
                            }
                        }
                    ],
                    "antonyms": [
                        "zonder"
                    ],
                    "common_phrases": [
                        "samen met",
                        "met elkaar",
                        "met z'n tweeÃ«n"
                    ],
                    "target_lang_definitions": {
                        "en": "In the company of or together with someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "with",
                                "target_lang_sense_clarification": "accompaniment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5b01c95-3b94-3fa5-8aeb-f1a12889114a",
                    "sense_definition": "Door middel van; gebruikmakend van een voorwerp, instrument of vervoermiddel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Morgen ga ik <met> de trein naar mijn werk in Amsterdam.",
                            "target_lang_translations": {
                                "en": "Tomorrow I am going to work in Amsterdam <by> train."
                            }
                        },
                        {
                            "text": "Je kunt de soep het beste <met> een lepel eten.",
                            "target_lang_translations": {
                                "en": "It is best to eat the soup <with> a spoon."
                            }
                        }
                    ],
                    "antonyms": [
                        "zonder"
                    ],
                    "common_phrases": [
                        "met de auto",
                        "met de hand",
                        "met behulp van"
                    ],
                    "target_lang_definitions": {
                        "en": "By means of; using an object, instrument, or mode of transport."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "transport or means"
                            },
                            {
                                "target_lang_word": "with",
                                "target_lang_sense_clarification": "instrument"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d0cf52f-1121-3bc8-ac62-88a797d61541",
                    "sense_definition": "Op een bepaalde manier; gekenmerkt door een bepaald gevoel of een bepaalde houding.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij luisterde <met> grote aandacht naar het spannende verhaal.",
                            "target_lang_translations": {
                                "en": "He listened to the exciting story <with> great attention."
                            }
                        },
                        {
                            "text": "Zij doet haar werk altijd <met> veel plezier en inzet.",
                            "target_lang_translations": {
                                "en": "She always does her work <with> great pleasure and dedication."
                            }
                        }
                    ],
                    "antonyms": [
                        "zonder"
                    ],
                    "common_phrases": [
                        "met plezier",
                        "met rust",
                        "met opzet"
                    ],
                    "target_lang_definitions": {
                        "en": "In a certain way; characterized by a certain feeling or attitude."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "with",
                                "target_lang_sense_clarification": "manner or attitude"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ad2d992-256f-31a9-a7ce-fa32a30443a7",
                    "sense_definition": "Gebruikt als introductie van de eigen naam bij het opnemen van de telefoon.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "telephone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hallo, <met> Jan de Vries, kan ik iemand spreken?",
                            "target_lang_translations": {
                                "en": "Hello, <this is> Jan de Vries, can I speak to someone?"
                            }
                        },
                        {
                            "text": "Goedemiddag, u spreekt <met> de receptie van het hotel.",
                            "target_lang_translations": {
                                "en": "Good afternoon, you are speaking <with> the hotel reception."
                            }
                        }
                    ],
                    "common_phrases": [
                        "u spreekt met"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to introduce one's own name when answering the telephone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "this is",
                                "target_lang_sense_clarification": "telephone introduction"
                            },
                            {
                                "target_lang_word": "speaking",
                                "target_lang_sense_clarification": "telephone introduction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12a7e055-be85-3de4-9752-4d3fe1ef72fd",
                    "sense_definition": "Tijdens; gedurende een periode of ter gelegenheid van een gebeurtenis.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Met> kerst zijn de meeste winkels in de stad gesloten.",
                            "target_lang_translations": {
                                "en": "<At> Christmas, most shops in the city are closed."
                            }
                        },
                        {
                            "text": "Het wordt buiten <met> de dag kouder en donkerder.",
                            "target_lang_translations": {
                                "en": "It is getting colder and darker outside <by> the day."
                            }
                        }
                    ],
                    "common_phrases": [
                        "met de tijd",
                        "met kerst",
                        "met vakantie"
                    ],
                    "target_lang_definitions": {
                        "en": "During; throughout a period or on the occasion of an event."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "occasion (e.g., Christmas)"
                            },
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "progression (e.g., by the day)"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4acabdef-910c-3685-ba4c-bd2d7d3c341f",
                    "sense_definition": "Artysta cyrkowy w charakterystycznym, kolorowym stroju i makijażu, którego zadaniem jest rozśmieszanie publiczności; inna pisownia słowa <w>klaun</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci głośno śmiały się, gdy <clown> potknął się o własne buty.",
                            "target_lang_translations": {
                                "ru": "Дети громко смеялись, когда <w>клоун</w> споткнулся о собственные ботинки."
                            }
                        },
                        {
                            "text": "Po występie niemieckiego <clowna> na arenie pojawił się żongler.",
                            "target_lang_translations": {
                                "ru": "После выступления немецкого <w>клоуна</w> на арене появился жонглер."
                            }
                        }
                    ],
                    "synonyms": [
                        "klaun",
                        "pajac"
                    ],
                    "common_phrases": [
                        "smutny clown",
                        "strój clowna"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "pisownia alternatywna, rzadsza niż „klaun”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Цирковой артист в характерном цветном костюме и гриме, задача которого — смешить публику."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клоун",
                                "target_lang_sense_clarification": "артист цирка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0407dd36-efe4-320d-88d6-7c25bf61603e",
                    "sense_definition": "Osoba zachowująca się niepoważnie, błaznująca, często w celu zwrócenia na siebie uwagi.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestań zachowywać się jak <clown> i weź się wreszcie do pracy.",
                            "target_lang_translations": {
                                "ru": "Перестань вести себя как <w>клоун</w> и возьмись наконец за работу."
                            }
                        },
                        {
                            "text": "Nie zamierzam robić z siebie <clowna> przed całą klasą.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь делать из себя <w>клоуна</w> перед всем классом."
                            }
                        }
                    ],
                    "synonyms": [
                        "błazen",
                        "pajac",
                        "wesołek"
                    ],
                    "antonyms": [
                        "ponurak"
                    ],
                    "common_phrases": [
                        "robić z siebie clowna"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "określenie nacechowane emocjonalnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, ведущий себя несерьезно, паясничающий, часто с целью привлечь к себе внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клоун",
                                "target_lang_sense_clarification": "несерьезный человек"
                            },
                            {
                                "target_lang_word": "шут",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "паяц",
                                "target_lang_sense_clarification": "кривляка"
                            }
                        ]
                    }
                }
            ]
        }
    ]

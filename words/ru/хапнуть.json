    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0ed85f80-f4f2-384f-9704-906ffe7bea50",
                    "sense_definition": "Быстро, резко схватить или укусить кого-либо или что-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_grab",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Злая собака попыталась <хапнуть> прохожего за ногу.",
                            "target_lang_translations": {
                                "en": "The angry dog tried to <snap at> the passerby's leg."
                            }
                        },
                        {
                            "text": "Кот ловко <хапнул> кусок колбасы со стола и убежал.",
                            "target_lang_translations": {
                                "en": "The cat deftly <snatched> a piece of sausage from the table and ran away."
                            }
                        }
                    ],
                    "synonyms": [
                        "цапнуть",
                        "хватануть",
                        "куснуть"
                    ],
                    "antonyms": [
                        "отпустить"
                    ],
                    "common_phrases": [
                        "хапнуть за палец",
                        "хапнуть зубами"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "просторечное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To grab or bite someone or something quickly and roughly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "snatch",
                                "target_lang_sense_clarification": "to seize quickly"
                            },
                            {
                                "target_lang_word": "snap at",
                                "target_lang_sense_clarification": "to try to bite suddenly"
                            },
                            {
                                "target_lang_word": "grab",
                                "target_lang_sense_clarification": "to take hold of roughly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cbfc332-7d24-3763-8423-8bed8ba72ef4",
                    "sense_definition": "Присвоить себе что-либо незаконным образом или из жадности; украсть.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_steal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чиновник успел <хапнуть> огромную взятку перед проверкой.",
                            "target_lang_translations": {
                                "en": "The official managed to <grab> a huge bribe before the inspection."
                            }
                        },
                        {
                            "text": "Они хотели по-быстрому <хапнуть> денег и скрыться за границей.",
                            "target_lang_translations": {
                                "en": "They wanted to quickly <grab> some money and hide abroad."
                            }
                        }
                    ],
                    "synonyms": [
                        "хватануть",
                        "цапнуть",
                        "куснуть"
                    ],
                    "antonyms": [
                        "вернуть",
                        "отдать"
                    ],
                    "common_phrases": [
                        "хапнуть денег",
                        "хапнуть взятку",
                        "хапнуть лишнего"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "просторечное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To appropriate something illegally or out of greed; to steal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grab",
                                "target_lang_sense_clarification": "to take greedily"
                            },
                            {
                                "target_lang_word": "snatch",
                                "target_lang_sense_clarification": "to steal quickly"
                            },
                            {
                                "target_lang_word": "pocket",
                                "target_lang_sense_clarification": "to take for oneself dishonestly"
                            },
                            {
                                "target_lang_word": "swipe",
                                "target_lang_sense_clarification": "to steal (slang)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a59d2c21-9e62-3d7b-a988-9cabc130770d",
                    "sense_definition": "Послать компьютерному процессу сигнал HUP (SIGHUP) для перезагрузки конфигурации.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tech_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Системный администратор решил <хапнуть> зависший процесс.",
                            "target_lang_translations": {
                                "en": "The system administrator decided to <HUP> the hung process."
                            }
                        },
                        {
                            "text": "Тебе нужно <хапнуть> демон, чтобы он перечитал настройки.",
                            "target_lang_translations": {
                                "en": "You need to <HUP> the daemon so it rereads the settings."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "жаргон"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To send a SIGHUP signal to a computer process to reload configuration."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "HUP",
                                "target_lang_sense_clarification": "IT slang: to send a SIGHUP signal"
                            }
                        ]
                    }
                }
            ]
        }
    ]

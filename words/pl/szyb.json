    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e11823a0-a645-31af-a453-fa31a4b23920",
                    "sense_definition": "*dopełniacz liczby mnogiej od słowa* <w>szyba</w>",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W oknach tego starego domu nie ma już <w>szyb</w>.",
                            "target_lang_translations": {
                                "ru": "В окнах этого старого дома уже нет <w>стёкол</w>."
                            }
                        },
                        {
                            "text": "Musimy zamówić wymianę pękniętych <w>szyb</w> w samochodzie.",
                            "target_lang_translations": {
                                "ru": "Мы должны заказать замену треснувших <w>стёкол</w> в машине."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural of szyba"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "*родительный падеж множественного числа от слова* <w>szyba</w> (оконное стекло)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стёкол",
                                "target_lang_sense_clarification": "форма слова 'стекло'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c454c6b-193e-367c-9a1e-91306dd64bbb",
                    "sense_definition": "Pionowy kanał wewnątrz budynku, w którym porusza się kabina windy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vertical_channels",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Technicy weszli do <w>szybu</w> windy, aby naprawić zerwaną linę.",
                            "target_lang_translations": {
                                "ru": "Техники вошли в <w>шахту</w> лифта, чтобы починить оборванный трос."
                            }
                        },
                        {
                            "text": "Przez szparę w drzwiach zajrzał do ciemnego <w>szybu</w>.",
                            "target_lang_translations": {
                                "ru": "Через щель в дверях он заглянул в темную <w>шахту</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szyb windy",
                        "szyb dźwigowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальный канал внутри здания, в котором движется кабина лифта"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шахта",
                                "target_lang_sense_clarification": "лифта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ab400f6-c8ec-3bc1-a5f6-399c0c8e2791",
                    "sense_definition": "Głęboki, pionowy lub pochyły korytarz drążony w ziemi, służący do wydobywania kopalin, transportu ludzi lub wentylacji kopalni.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "vertical_channels",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Górnicy zjeżdżają głównym <w>szybem</w> na poziom wydobywczy.",
                            "target_lang_translations": {
                                "ru": "Шахтеры спускаются по главному <w>стволу</w> на уровень добычи."
                            }
                        },
                        {
                            "text": "Nad <w>szybem</w> kopalnianym wznosi się charakterystyczna wieża wyciągowa.",
                            "target_lang_translations": {
                                "ru": "Над шахтным <w>стволом</w> возвышается характерная подъемная башня."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szyb kopalniany",
                        "szyb wydobywczy",
                        "szyb wentylacyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубокий вертикальный или наклонный коридор в земле, служащий для добычи ископаемых, транспорта людей или вентиляции шахты"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ствол",
                                "target_lang_sense_clarification": "шахтный"
                            },
                            {
                                "target_lang_word": "шахта",
                                "target_lang_sense_clarification": "горная выработка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6d17db5-7e15-3a30-8e87-c8176f6aa7f3",
                    "sense_definition": "Pionowy przewód w konstrukcji budynku służący do wymiany powietrza lub prowadzenia instalacji technicznych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "vertical_channels",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gęsty dym wydostawał się na dach przez <w>szyb</w> wentylacyjny.",
                            "target_lang_translations": {
                                "ru": "Густой дым выходил на крышу через вентиляционную <w>шахту</w>."
                            }
                        },
                        {
                            "text": "Wszystkie rury wodociągowe poprowadzono specjalnym <w>szybem</w> instalacyjnym.",
                            "target_lang_translations": {
                                "ru": "Все водопроводные трубы были проложены через специальную монтажную <w>шахту</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "szacht"
                    ],
                    "common_phrases": [
                        "szyb wentylacyjny",
                        "szyb kominowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальный канал в конструкции здания, служащий для воздухообмена или прокладки технических коммуникаций"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шахта",
                                "target_lang_sense_clarification": "вентиляционная или коммуникационная"
                            },
                            {
                                "target_lang_word": "канал",
                                "target_lang_sense_clarification": "технический"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
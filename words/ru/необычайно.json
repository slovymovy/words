    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "55fca5e9-3280-3b14-b061-3dc53a720277",
                    "sense_definition": "В высшей степени, предельно в своём проявлении; очень, чрезвычайно.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "degree_intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот фильм оказался <w>необычайно</w> интересным и захватывающим для всех зрителей.",
                            "target_lang_translations": {
                                "en": "This movie turned out to be <w>extraordinarily</w> interesting and exciting for all viewers."
                            }
                        },
                        {
                            "text": "Мы были <w>необычайно</w> рады встретить старых друзей после стольких лет разлуки.",
                            "target_lang_translations": {
                                "en": "We were <w>exceptionally</w> happy to meet old friends after so many years of separation."
                            }
                        }
                    ],
                    "synonyms": [
                        "очень",
                        "чрезвычайно",
                        "крайне",
                        "весьма",
                        "в высшей степени",
                        "исключительно"
                    ],
                    "common_phrases": [
                        "необычайно красивый",
                        "необычайно важно",
                        "необычайно трудно",
                        "необычайно быстро"
                    ],
                    "target_lang_definitions": {
                        "en": "To a very high degree; extremely or remarkably."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "extraordinarily",
                                "target_lang_sense_clarification": "indicating a degree far beyond the ordinary"
                            },
                            {
                                "target_lang_word": "remarkably",
                                "target_lang_sense_clarification": "in a way that is worthy of attention"
                            },
                            {
                                "target_lang_word": "exceptionally",
                                "target_lang_sense_clarification": "unusually well or to a high degree"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bd09752-65e2-3372-8f8b-4ba3e1ead637",
                    "sense_definition": "Исключительно, совершенно не так, как всегда или обычно; поразительно.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "manner_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Всё в этом старинном доме выглядело <w>необычайно</w> и таинственно.",
                            "target_lang_translations": {
                                "en": "Everything in this old house looked <w>extraordinary</w> and mysterious."
                            }
                        },
                        {
                            "text": "Событие, о котором все говорили, закончилось <w>необычайно</w> для участников.",
                            "target_lang_translations": {
                                "en": "The event everyone was talking about ended <w>unusually</w> for the participants."
                            }
                        }
                    ],
                    "synonyms": [
                        "необычно",
                        "исключительно"
                    ],
                    "antonyms": [
                        "обычно",
                        "обыденно"
                    ],
                    "common_phrases": [
                        "выглядеть необычайно",
                        "звучать необычайно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that is not common or ordinary; strikingly or strangely."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unusually",
                                "target_lang_sense_clarification": "in a manner different from what is normal"
                            },
                            {
                                "target_lang_word": "singularly",
                                "target_lang_sense_clarification": "in a unique or peculiar way"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "19d86781-221d-35ce-89b5-84161b5f5b76",
                    "sense_definition": "John Milton (1608–1674), a celebrated English poet and intellectual best known for his epic poem 'Paradise Lost'.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We studied the works of <w>Milton</w> in our English literature class.",
                            "target_lang_translations": {
                                "ru": "Мы изучали произведения <w>Мильтона</w> на уроке английской литературы."
                            }
                        },
                        {
                            "text": "Many scholars consider <w>Milton</w> to be one of the greatest writers in history.",
                            "target_lang_translations": {
                                "ru": "Многие ученые считают <w>Мильтона</w> одним из величайших писателей в истории."
                            }
                        }
                    ],
                    "common_phrases": [
                        "John Milton",
                        "reading Milton"
                    ],
                    "traits": [

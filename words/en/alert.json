{
    "word_family": [
        "alertness",
        "alertly"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2fea9e75-55ea-3d09-9658-52e72324329c",
                    "sense_definition": "Quick to see, understand, and act in a particular situation; attentive and watchful.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adj_attentive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drivers must be <w>alert</w> to any signs of fatigue on the road.",
                            "target_lang_translations": {
                                "ru": "Водители должны быть <w>внимательны</w> к любым признакам усталости на дороге."
                            }
                        },
                        {
                            "text": "The security guard stayed <w>alert</w> throughout the long night shift.",
                            "target_lang_translations": {
                                "ru": "Охранник оставался <w>бдительным</w> в течение всей долгой ночной смены."
                            }
                        }
                    ],
                    "synonyms": [
                        "wakeful"
                    ],
                    "common_phrases": [
                        "stay alert",
                        "be alert",
                        "mentally alert"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро замечающий, понимающий и реагирующий; внимательный и бдительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бдительный",
                                "target_lang_sense_clarification": "осторожный и неослабно внимательный"
                            },
                            {
                                "target_lang_word": "внимательный",
                                "target_lang_sense_clarification": "сосредоточенный на чем-либо"
                            },
                            {
                                "target_lang_word": "настороженный",
                                "target_lang_sense_clarification": "находящийся в ожидании чего-то неожиданного или опасного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a725d8d-ae94-364a-946e-5b0c341fc0c7",
                    "sense_definition": "Quick and energetic in movement; brisk and nimble.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "adj_nimble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old man walked with a surprisingly <w>alert</w> step for his age.",
                            "target_lang_translations": {
                                "ru": "Старик шел удивительно <w>бодрым</w> шагом для своего возраста."
                            }
                        },
                        {
                            "text": "An <w>alert</w> dog chased the ball across the field.",
                            "target_lang_translations": {
                                "ru": "<w>Шустрая</w> собака погналась за мячом через поле."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "usage regarding physical agility is less common in modern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстрый и энергичный в движениях; проворный и живой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бодрый",
                                "target_lang_sense_clarification": "полный сил и энергии"
                            },
                            {
                                "target_lang_word": "живой",
                                "target_lang_sense_clarification": "подвижный, энергичный"
                            },
                            {
                                "target_lang_word": "шустрый",
                                "target_lang_sense_clarification": "проворный, быстрый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dc009f52-b94b-3710-a8af-c792f8495380",
                    "sense_definition": "To warn someone of a possibly dangerous situation or a problem.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "verb_warn",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We must <w>alert</w> the police immediately about the accident.",
                            "target_lang_translations": {
                                "ru": "Мы должны немедленно <w>оповестить</w> полицию об аварии."
                            }
                        },
                        {
                            "text": "The bank <w>alerted</w> him to suspicious activity on his account.",
                            "target_lang_translations": {
                                "ru": "Банк <w>предупредил</w> его о подозрительной активности на счете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "alert someone to",
                        "alert the authorities",
                        "alert the public"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предупредить кого-либо о возможной опасной ситуации или проблеме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предупреждать",
                                "target_lang_sense_clarification": "заранее извещать о чем-либо"
                            },
                            {
                                "target_lang_word": "оповещать",
                                "target_lang_sense_clarification": "сообщать всем, информировать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5975a1a0-7b75-3ed5-b1bd-5f8952336d3b",
                    "sense_definition": "A warning signal, alarm, or message indicating danger or a specific event.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_warning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I received an <w>alert</w> on my phone about the severe weather.",
                            "target_lang_translations": {
                                "ru": "Я получил <w>оповещение</w> на телефон о суровой погоде."
                            }
                        },
                        {
                            "text": "The fire <w>alert</w> sounded loudly throughout the building.",
                            "target_lang_translations": {
                                "ru": "Пожарная <w>тревога</w> громко звучала по всему зданию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "security alert",
                        "email alert",
                        "emergency alert",
                        "send an alert"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предупреждающий сигнал или сообщение, указывающее на опасность или определенное событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оповещение",
                                "target_lang_sense_clarification": "уведомление или сообщение"
                            },
                            {
                                "target_lang_word": "сигнал тревоги",
                                "target_lang_sense_clarification": "звуковой или визуальный знак опасности"
                            },
                            {
                                "target_lang_word": "предупреждение",
                                "target_lang_sense_clarification": "предостережение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b3bf14a-c631-318e-860e-c55502d2dac3",
                    "sense_definition": "A state of being ready to deal with a sudden danger, emergency, or attack.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_readiness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The troops were placed on high <w>alert</w> due to the border tensions.",
                            "target_lang_translations": {
                                "ru": "Войска были приведены в состояние повышенной <w>боевой готовности</w> из-за напряженности на границе."
                            }
                        },
                        {
                            "text": "Hospitals are on <w>alert</w> for a potential outbreak of the virus.",
                            "target_lang_translations": {
                                "ru": "Больницы находятся в состоянии <w>готовности</w> к возможной вспышке вируса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on high alert",
                        "on full alert",
                        "red alert"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние готовности к внезапной опасности, чрезвычайной ситуации или нападению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовность",
                                "target_lang_sense_clarification": "состояние, при котором все готово к действию"
                            },
                            {
                                "target_lang_word": "боевая готовность",
                                "target_lang_sense_clarification": "военный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edc22409-d928-3c98-9681-8fe59a3fc006",
                    "sense_definition": "A control character in computing that triggers an audible signal or beep.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The script sends an <w>alert</w> to the terminal when the process finishes.",
                            "target_lang_translations": {
                                "ru": "Скрипт отправляет <w>звуковой сигнал</w> на терминал, когда процесс завершается."
                            }
                        },
                        {
                            "text": "Programmers sometimes use the <w>alert</w> character to debug code audibly.",
                            "target_lang_translations": {
                                "ru": "Программисты иногда используют символ <w>звукового сигнала</w> для отладки кода на слух."
                            }
                        }
                    ],
                    "synonyms": [
                        "bell"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical computing context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Управляющий символ в информатике, вызывающий звуковой сигнал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звуковой сигнал",
                                "target_lang_sense_clarification": "компьютерный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4d324cfc-202b-37e0-b759-5a9701ea737e",
                    "sense_definition": "A place name, notably the northernmost permanently inhabited place in the world, located in Nunavut, Canada.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Scientists conduct climate research at the station in <w>Alert</w>.",
                            "target_lang_translations": {
                                "ru": "Ученые проводят климатические исследования на станции в <w>Алерте</w>."
                            }
                        },
                        {
                            "text": "The military base in <w>Alert</w> is extremely isolated.",
                            "target_lang_translations": {
                                "ru": "Военная база в <w>Алерте</w> чрезвычайно изолирована."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Географическое название; самое северное постоянно населенное место в мире, расположенное в Нунавуте, Канада."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Алерт",
                                "target_lang_sense_clarification": "название поселения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
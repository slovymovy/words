    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dff94682-8989-3db5-8ad4-44d147f8c14e",
                    "sense_definition": "Nadawać przedmiotowi ostrość, czyniąc jego krawędź tnącą bardziej skuteczną, aby lepiej cięła.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_sharpening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kucharz codziennie rano <w>ostrzy</w> swoje noże, aby łatwiej kroić mięso.",
                            "target_lang_translations": {
                                "ru": "Повар каждое утро <w>точит</w> свои ножи, чтобы было легче резать мясо."
                            }
                        },
                        {
                            "text": "Drwal usiadł na pniu i zaczął <w>ostrzyć</w> stępioną siekierę.",
                            "target_lang_translations": {
                                "ru": "Дровосек сел на пень и начал <w>точить</w> затупившийся топор."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyostrzać",
                        "zaostrzać"
                    ],
                    "antonyms": [
                        "tępić"
                    ],
                    "common_phrases": [
                        "ostrzyć nóż",
                        "ostrzyć nożyczki",
                        "ostrzyć siekierę",
                        "ostrzyć kosę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать лезвие инструмента более острым для лучшей резки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точить",
                                "target_lang_sense_clarification": "нож, ножницы, инструменты"
                            },
                            {
                                "target_lang_word": "затачивать",
                                "target_lang_sense_clarification": "делать острым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "004689ed-86bc-3987-96cc-f439b96c0bee",
                    "sense_definition": "Nadawać czemuś ostry kształt na wierzchołku, zwykle poprzez struganie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_sharpening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed egzaminem z matematyki uczeń starannie <w>ostrzył</w> ołówek.",
                            "target_lang_translations": {
                                "ru": "Перед экзаменом по математике ученик старательно <w>точил</w> карандаш."
                            }
                        },
                        {
                            "text": "Dzieci lubią <w>ostrzyć</w> kolorowe kredki do rysowania.",
                            "target_lang_translations": {
                                "ru": "Дети любят <w>точить</w> цветные карандаши для рисования."
                            }
                        }
                    ],
                    "synonyms": [
                        "temperować",
                        "strugać"
                    ],
                    "common_phrases": [
                        "ostrzyć ołówek",
                        "ostrzyć kredki",
                        "ostrzyć patyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать острым кончик предмета (обычно карандаша) путем срезания верхнего слоя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точить",
                                "target_lang_sense_clarification": "карандаш"
                            },
                            {
                                "target_lang_word": "затачивать",
                                "target_lang_sense_clarification": "острие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fac8ccc-b66f-3362-8b59-15f771181d33",
                    "sense_definition": "Zmieniać kurs jednostki pływającej tak, aby płynąć pod mniejszym kątem do wiatru.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sternik kazał załodze <w>ostrzyć</w>, aby ominąć przeszkodę na wodzie.",
                            "target_lang_translations": {
                                "ru": "Рулевой приказал команде <w>приводиться</w>, чтобы обойти препятствие на воде."
                            }
                        },
                        {
                            "text": "Jeśli będziesz zbyt mocno <w>ostrzyć</w>, żagle zaczną łopotać i stracimy prędkość.",
                            "target_lang_translations": {
                                "ru": "Если будешь слишком сильно <w>приводиться</w>, паруса начнут хлопать, и мы потеряем скорость."
                            }
                        }
                    ],
                    "antonyms": [
                        "odpadać"
                    ],
                    "common_phrases": [
                        "ostrzyć do wiatru",
                        "mocno ostrzyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять судно ближе к линии ветра (уменьшать угол между курсом и ветром)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приводиться",
                                "target_lang_sense_clarification": "морской термин"
                            },
                            {
                                "target_lang_word": "идти круче",
                                "target_lang_sense_clarification": "к ветру"
                            }
                        ]
                    }
                }
            ]
        }
    ]

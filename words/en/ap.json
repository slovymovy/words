                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точка доступа",
                                "target_lang_sense_clarification": "компьютерные сети"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48326cb9-4ef0-33fc-88df-cf8acff09ce3",
                    "sense_definition": "Initialism of <w>Advanced</w> <w>Placement</w>: a program in the US and Canada offering college-level curriculum and exams to high school students.",

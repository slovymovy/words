    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "74dade88-79a2-3e74-b705-9016f8708d43",
                    "sense_definition": "A wild mammal belonging to the dog family, typically having a pointed muzzle, upright ears, and a bushy tail.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The quick brown <w>fox</w> jumps over the lazy dog.",
                            "target_lang_translations": {
                                "ru": "Шустрая бурая <w>лиса</w> перепрыгивает через ленивую собаку."
                            }
                        },
                        {
                            "text": "We saw a red <w>fox</w> running across the snowy field yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера мы видели рыжую <w>лису</w>, бегущую по заснеженному полю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "red fox",
                        "sly as a fox",
                        "crazy like a fox",
                        "quick brown fox"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дикое млекопитающее семейства псовых, обычно с острой мордой, стоячими ушами и пушистым хвостом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лиса",
                                "target_lang_sense_clarification": "животное"
                            },
                            {
                                "target_lang_word": "лисица",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb19201a-7869-397d-ad8d-320f160d31f9",
                    "sense_definition": "The fur of a fox, often used to make clothing or accessories.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a coat made of expensive <w>fox</w> to the winter gala.",
                            "target_lang_translations": {
                                "ru": "Она надела шубу из дорогой <w>лисы</w> на зимний гала-концерт."
                            }
                        },
                        {
                            "text": "The collar of the jacket was trimmed with soft white <w>fox</w>.",
                            "target_lang_translations": {
                                "ru": "Воротник куртки был оторочен мягким белым <w>лисьим мехом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fox fur",
                        "silver fox"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мех лисицы, часто используемый для изготовления одежды или аксессуаров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лиса",
                                "target_lang_sense_clarification": "мех"
                            },
                            {
                                "target_lang_word": "лисий мех",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45d2595f-e0d9-3e67-b517-e246b5cadbe4",
                    "sense_definition": "A person who is clever, cunning, and often deceptive.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't trust him with your business plans; he is a sly <w>fox</w>.",
                            "target_lang_translations": {
                                "ru": "Не доверяй ему свои бизнес-планы; он хитрый <w>лис</w>."
                            }
                        },
                        {
                            "text": "The old <w>fox</w> managed to outsmart all his competitors in the deal.",
                            "target_lang_translations": {
                                "ru": "Старый <w>лис</w> сумел перехитрить всех своих конкурентов в этой сделке."
                            }
                        }
                    ],
                    "synonyms": [
                        "trickster",
                        "schemer"
                    ],
                    "common_phrases": [
                        "sly fox",
                        "old fox"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used metaphorically"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который умен, хитер и часто склонен к обману."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хитрец",

                        "omgang"
                    ],
                    "antonyms": [
                        "eenzaamheid",
                        "afzondering"
                    ],
                    "common_phrases": [
                        "gezelschap houden",
                        "in gezelschap van"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние пребывания вместе с другими или присутствие кого-либо.",

                    "sense_id": "0d9e5a4d-049d-3aa6-9b68-ba0345da18f8",
                    "sense_definition": "Het geldbedrag dat men periodiek betaalt om iets te mogen gebruiken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>huur</w> van mijn appartement is deze maand verhoogd.",
                            "target_lang_translations": {
                                "ru": "<w>Арендная плата</w> за мою квартиру в этом месяце повысилась.",
                                "en": "The <w>rent</w> of my apartment has been increased this month."
                            }
                        },
                        {
                            "text": "Wij betalen elke maand netjes de <w>huur</w> aan de huisbaas.",
                            "target_lang_translations": {
                                "ru": "Мы каждый месяц исправно платим <w>аренду</w> хозяину.",
                                "en": "We pay the <w>rent</w> to the landlord punctually every month."
                            }
                        }
                    ],
                    "common_phrases": [
                        "huur betalen",
                        "hoge huur",
                        "inclusief huur",
                        "exclusief huur"
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "zgłoszenie",
        "zgłaszający"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "226261c7-55e6-3104-a366-2f02495a2279",
                    "sense_definition": "Wyrażać chęć uczestnictwa w czymś lub podjęcia się jakiegoś zadania (używane z zaimkiem \"się\").",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek zawsze chętnie <zgłasza> się do odpowiedzi na lekcjach matematyki.",
                            "target_lang_translations": {
                                "ru": "Марек всегда охотно <вызывается> отвечать на уроках математики."
                            }
                        },
                        {
                            "text": "Wielu studentów zaczęło <zgłaszać> się na ochotnika do pomocy przy organizacji festiwalu.",
                            "target_lang_translations": {
                                "ru": "Многие студенты начали <записываться> добровольцами для помощи в организации фестиваля."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawiadamiać",
                        "zdawać sprawę"
                    ],
                    "common_phrases": [
                        "zgłaszać się na ochotnika",
                        "zgłaszać się do odpowiedzi",
                        "zgłaszać się do pomocy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать желание участвовать в чем-либо или взять на себя задачу (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызываться",
                                "target_lang_sense_clarification": "предлагать свою кандидатуру"
                            },
                            {
                                "target_lang_word": "записываться",
                                "target_lang_sense_clarification": "регистрироваться для участия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10c657fa-8afb-38df-90db-9287f6cb8cfa",
                    "sense_definition": "Reagować na sygnał telefonu, dzwonka lub wywołanie, nawiązując połączenie (używane z zaimkiem \"się\").",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzwonię do biura obsługi klienta, ale nikt się tam nie <zgłasza>.",
                            "target_lang_translations": {
                                "ru": "Я звоню в бюро обслуживания клиентов, но никто там не <отвечает>."
                            }
                        },
                        {
                            "text": "Kiedy usłyszał dzwonek do drzwi, natychmiast <zgłosił> się przez domofon.",
                            "target_lang_translations": {
                                "ru": "Услышав звонок в дверь, он немедленно <ответил> по домофону."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nikt się nie zgłasza",
                        "zgłaszać się na wezwanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Реагировать на звонок, сигнал или вызов, устанавливая связь (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвечать",
                                "target_lang_sense_clarification": "на звонок или сигнал"
                            },
                            {
                                "target_lang_word": "отзываться",
                                "target_lang_sense_clarification": "реагировать на зов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a0ee9ef-4ecc-3f16-b104-7e17349bf5da",
                    "sense_definition": "Informować odpowiednie osoby lub instytucje o jakimś zdarzeniu, problemie, fakcie lub sytuacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy pracownik powinien niezwłocznie <zgłaszać> wszelkie awarie sprzętu swojemu kierownikowi.",
                            "target_lang_translations": {
                                "ru": "Каждый сотрудник должен немедленно <сообщать> о любых поломках оборудования своему руководителю."
                            }
                        },
                        {
                            "text": "Sąsiedzi postanowili <zgłaszać> na policję każde zakłócanie ciszy nocnej.",
                            "target_lang_translations": {
                                "ru": "Соседи решили <заявлять> в полицию о каждом нарушении ночной тишины."
                            }
                        }
                    ],
                    "synonyms": [
                        "informować",
                        "zawiadamiać",
                        "meldować",
                        "raportować"
                    ],
                    "antonyms": [
                        "zatajać",
                        "ukrywać"
                    ],
                    "common_phrases": [
                        "zgłaszać problem",
                        "zgłaszać przestępstwo",
                        "zgłaszać reklamację",
                        "zgłaszać nieobecność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Информировать соответствующие лица или учреждения о событии, проблеме или факте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообщать",
                                "target_lang_sense_clarification": "информировать о чем-либо"
                            },
                            {
                                "target_lang_word": "заявлять",
                                "target_lang_sense_clarification": "официально докладывать (напр. в полицию)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7aad12bc-c9f3-393c-bfbe-6382af271538",
                    "sense_definition": "Przychodzić w wyznaczone miejsce w określonym celu, aby potwierdzić swoją obecność (używane z zaimkiem \"się\").",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy pasażerowie muszą <zgłaszać> się do odprawy na godzinę przed odlotem.",
                            "target_lang_translations": {
                                "ru": "Все пассажиры должны <являться> на регистрацию за час до вылета."
                            }
                        },
                        {
                            "text": "Pacjenci powinni <zgłaszać> się do rejestracji zaraz po wejściu do przychodni.",
                            "target_lang_translations": {
                                "ru": "Пациенты должны <обращаться> в регистратуру сразу после входа в поликлинику."
                            }
                        }
                    ],
                    "synonyms": [
                        "meldować",
                        "zawiadamiać"
                    ],
                    "common_phrases": [
                        "zgłaszać się do kontroli",
                        "zgłaszać się na policję",
                        "zgłaszać się do lekarza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить в назначенное место с определенной целью, чтобы подтвердить свое присутствие (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "являться",
                                "target_lang_sense_clarification": "приходить официально"
                            },
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "подходить к кому-либо (напр. к врачу)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "924289b6-f1d3-3035-83f9-feafc6f5383f",
                    "sense_definition": "Przedstawiać oficjalnie propozycję, wniosek, projekt lub uwagi do rozpatrzenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas zebrania mieszkańcy mogli <zgłaszać> swoje uwagi do nowego planu zagospodarowania.",
                            "target_lang_translations": {
                                "ru": "Во время собрания жители могли <высказывать> свои замечания к новому плану застройки."
                            }
                        },
                        {
                            "text": "Opozycja zapowiedziała, że będzie <zgłaszać> poprawki do procedowanej ustawy.",
                            "target_lang_translations": {
                                "ru": "Оппозиция заявила, что будет <вносить> поправки в рассматриваемый закон."
                            }
                        }
                    ],
                    "synonyms": [
                        "komunikować",
                        "sygnalizować"
                    ],
                    "common_phrases": [
                        "zgłaszać wniosek",
                        "zgłaszać sprzeciw",
                        "zgłaszać uwagi",
                        "zgłaszać pretensje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально представлять предложение, заявление, проект или замечания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вносить",
                                "target_lang_sense_clarification": "предложение или поправку"
                            },
                            {
                                "target_lang_word": "подавать",
                                "target_lang_sense_clarification": "заявление или жалобу"
                            },
                            {
                                "target_lang_word": "высказывать",
                                "target_lang_sense_clarification": "замечания или мнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "158e48bb-c8a8-361b-bef0-d3be380d3463",
                    "sense_definition": "Deklarować oficjalnie posiadanie towarów podlegających ocleniu lub dochodów do opodatkowania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podróżni przekraczający granicę mają obowiązek <zgłaszać> przewóz dużych sum gotówki.",
                            "target_lang_translations": {
                                "ru": "Путешественники, пересекающие границу, обязаны <декларировать> перевозку крупных сумм наличных."
                            }
                        },
                        {
                            "text": "Przedsiębiorcy muszą terminowo <zgłaszać> wszystkie osiągnięte przychody do urzędu skarbowego.",
                            "target_lang_translations": {
                                "ru": "Предприниматели должны своевременно <декларировать> все полученные доходы в налоговую инспекцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawiadamiać"
                    ],
                    "antonyms": [
                        "przemycać"
                    ],
                    "common_phrases": [
                        "zgłaszać do oclenia",
                        "zgłaszać do opodatkowania"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "official"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально объявлять о наличии товаров, подлежащих пошлине, или доходов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "декларировать",
                                "target_lang_sense_clarification": "таможня или налоги"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
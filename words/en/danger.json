    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ce69ee09-e647-3213-aee8-68c784db96a1",
                    "sense_definition": "The possibility of suffering harm, injury, or death.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "danger_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hikers were in <w>danger</w> of getting lost in the mountains.",
                            "target_lang_translations": {
                                "ru": "Туристам грозила <w>опасность</w> заблудиться в горах."
                            }
                        },
                        {
                            "text": "Doctors said the patient was finally out of <w>danger</w>.",
                            "target_lang_translations": {
                                "ru": "Врачи сказали, что пациент наконец вне <w>опасности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "risk",
                        "peril",
                        "hazard",
                        "jeopardy",
                        "harm's way"
                    ],
                    "antonyms": [
                        "safety",
                        "security"
                    ],
                    "common_phrases": [
                        "in danger",
                        "out of danger",
                        "in danger of",
                        "danger zone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность пострадать, получить травму или погибнуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опа́сность",
                                "target_lang_sense_clarification": "риск, возможность беды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f96eb12b-5b70-333d-985d-16babb646f3c",
                    "sense_definition": "A person or thing that is likely to cause harm or injury.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "danger_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That aggressive dog is a <w>danger</w> to the neighborhood children.",
                            "target_lang_translations": {
                                "ru": "Эта агрессивная собака представляет <w>опасность</w> для соседских детей."
                            }
                        },
                        {
                            "text": "Icy roads are a serious <w>danger</w> to drivers in winter.",
                            "target_lang_translations": {
                                "ru": "Обледенелые дороги представляют серьезную <w>опасность</w> для водителей зимой."
                            }
                        }
                    ],
                    "synonyms": [
                        "threat",
                        "menace",
                        "hazard",
                        "risk"
                    ],
                    "common_phrases": [
                        "danger to",
                        "public danger",
                        "hidden danger"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или предмет, способный причинить вред или травму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опа́сность",
                                "target_lang_sense_clarification": "источник вреда"
                            },
                            {
                                "target_lang_word": "угро́за",
                                "target_lang_sense_clarification": "потенциальное зло"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bb03fcb-fd13-382b-827f-355ce3abd843",
                    "sense_definition": "A railway signal indicating that a train must stop.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train driver stopped immediately because the signal was at <w>danger</w>.",
                            "target_lang_translations": {
                                "ru": "Машинист немедленно остановился, так как сигнал предупреждал об <w>опасности</w>."
                            }
                        },
                        {
                            "text": "Passing a signal at <w>danger</w> is a major safety violation.",
                            "target_lang_translations": {
                                "ru": "Проезд сигнала, предупреждающего об <w>опасности</w>, является серьезным нарушением правил безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "at danger",
                        "signal at danger"
                    ],
                    "target_lang_definitions": {
                        "ru": "Железнодорожный сигнал, указывающий на необходимость остановки поезда."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]
}
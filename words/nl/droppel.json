{
    "word_family": [
        "droppelen",
        "druppel"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6ca035b7-5cb6-335f-9230-fd1bf86619b1",
                    "sense_definition": "Een kleine hoeveelheid vloeistof, meestal bolvormig; een minder gebruikelijke variant van <w>druppel</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een heldere <w>droppel</w> dauw schitterde in het vroege ochtendlicht op het blad.",
                            "target_lang_translations": {
                                "ru": "Прозрачная <w>капля</w> росы сверкала на листе в раннем утреннем свете."
                            }
                        },
                        {
                            "text": "Hij voelde een koude <w>droppel</w> regen in zijn nek vallen toen hij naar buiten liep.",
                            "target_lang_translations": {
                                "ru": "Выходя на улицу, он почувствовал, как холодная <w>капля</w> дождя упала ему на шею."
                            }
                        }
                    ],
                    "synonyms": [
                        "druppel"
                    ],
                    "common_phrases": [
                        "laatste droppel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькое количество жидкости, обычно сферической формы; менее распространенный вариант слова \"druppel\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капля",
                                "target_lang_sense_clarification": "частица жидкости"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d95f90b-ba8f-34ae-9a7c-3f0cd6552a42",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>droppelen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>droppel</w> voorzichtig wat gesmolten was op de brief om hem te verzegelen.",
                            "target_lang_translations": {
                                "ru": "Я осторожно <w>капаю</w> немного расплавленного воска на письмо, чтобы запечатать его."
                            }
                        },
                        {
                            "text": "Met een pipet <w>droppel</w> ik de juiste hoeveelheid medicijn in het waterglas.",
                            "target_lang_translations": {
                                "ru": "С помощью пипетки я <w>капаю</w> нужное количество лекарства в стакан с водой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "vervoeging"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени глагола \"droppelen\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капаю",
                                "target_lang_sense_clarification": "действие от первого лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56b5b412-bc3d-39ab-a82d-a5789ff33990",
                    "sense_definition": "Gebiedende wijs van <w>droppelen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Droppel</w> de saus langzaam over het gerecht voor een mooie presentatie.",
                            "target_lang_translations": {
                                "ru": "Медленно <w>покапай</w> соусом на блюдо для красивой подачи."
                            }
                        },
                        {
                            "text": "<w>Droppel</w> niet te veel olie in de pan, anders wordt het te vet.",
                            "target_lang_translations": {
                                "ru": "Не <w>капай</w> слишком много масла на сковороду, иначе будет слишком жирно."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "vervoeging"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола \"droppelen\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капай",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ec7a64f-e053-3a9a-ab9c-099248c20c26",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>droppelen</w> (bij inversie).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waarom <w>droppel</w> je die verf op de vloer in plaats van op het doek?",
                            "target_lang_translations": {
                                "ru": "Почему ты <w>капаешь</w> этой краской на пол, а не на холст?"
                            }
                        },
                        {
                            "text": "<w>Droppel</w> je de vloeistof er wel voorzichtig genoeg bij?",
                            "target_lang_translations": {
                                "ru": "Ты достаточно осторожно <w>капаешь</w> туда жидкость?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "vervoeging"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Второе лицо единственного числа настоящего времени глагола \"droppelen\" (при инверсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капаешь",
                                "target_lang_sense_clarification": "действие второго лица"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
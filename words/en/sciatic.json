                            {
                                "target_lang_word": "больной ишиасом",
                                "target_lang_sense_clarification": "субстантивированное"
                            },
                            {
                                "target_lang_word": "страдающий ишиасом",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        }

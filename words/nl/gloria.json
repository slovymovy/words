    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d751287c-cc60-3e11-9d3e-b67aae1cda44",
                    "sense_definition": "Glansrijke roem of een toestand van grote vreugde en eer.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij was helemaal in de <w>gloria</w> toen ze hoorde dat ze geslaagd was.",
                            "target_lang_translations": {
                                "en": "She was in high <w>spirits</w> when she heard she had passed.",
                                "ru": "Она была в полном <w>восторге</w>, когда услышала, что сдала экзамен."
                            }
                        },
                        {
                            "text": "De oude <w>gloria</w> van het vervallen kasteel is nog steeds zichtbaar.",
                            "target_lang_translations": {
                                "en": "The old <w>glory</w> of the dilapidated castle is still visible.",
                                "ru": "Былая <w>слава</w> ветхого замка всё ещё видна."
                            }
                        }
                    ],
                    "synonyms": [
                        "glorie"
                    ],
                    "common_phrases": [
                        "in de gloria"
                    ],
                    "target_lang_definitions": {
                        "en": "Radiant fame or a state of great joy and honor.",
                        "ru": "Блестящая слава или состояние огромной радости и почета."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "glory",
                                "target_lang_sense_clarification": "fame or honor"
                            },
                            {
                                "target_lang_word": "high spirits",
                                "target_lang_sense_clarification": "state of great joy (idiomatic)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "слава",
                                "target_lang_sense_clarification": "известность, почет"
                            },
                            {
                                "target_lang_word": "восторг",
                                "target_lang_sense_clarification": "состояние радости (в выражении 'in de gloria')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ffb46dc-55b6-39b4-82fe-7eebc78678f7",
                    "sense_definition": "Een lofzang van de engelen of een vast onderdeel van de katholieke mis.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de nachtmis zong het koor een prachtig <w>gloria</w>.",
                            "target_lang_translations": {
                                "en": "During the midnight mass, the choir sang a beautiful <w>Gloria</w>.",
                                "ru": "Во время ночной мессы хор пел прекрасную <w>Глорию</w>."
                            }
                        },
                        {
                            "text": "De priester hief het <w>gloria</w> aan en de gelovigen zongen mee.",
                            "target_lang_translations": {
                                "en": "The priest started the <w>Gloria</w> and the faithful sang along.",
                                "ru": "Священник запел <w>Глорию</w>, и верующие подхватили."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het gloria zingen"
                    ],
                    "target_lang_definitions": {
                        "en": "A hymn of praise by angels or a fixed part of the Catholic Mass.",
                        "ru": "Хвалебный гимн ангелов или часть католической мессы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Gloria",
                                "target_lang_sense_clarification": "liturgical hymn"
                            },
                            {
                                "target_lang_word": "doxology",
                                "target_lang_sense_clarification": "hymn of praise"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Глория",
                                "target_lang_sense_clarification": "литургический гимн"
                            },
                            {
                                "target_lang_word": "славословие",
                                "target_lang_sense_clarification": "церковное песнопение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "677d1cea-8804-3525-bfd3-2ef9977183a6",
                    "sense_definition": "Een lichtkrans of stralenkrans om het hoofd van een heilige.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "art_religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op het schilderij is de heilige afgebeeld met een gouden <w>gloria</w>.",
                            "target_lang_translations": {
                                "en": "In the painting, the saint is depicted with a golden <w>halo</w>.",
                                "ru": "На картине святой изображен с золотым <w>нимбом</w>."
                            }
                        },
                        {
                            "text": "De engel werd getekend met een stralende <w>gloria</w> rond het hoofd.",
                            "target_lang_translations": {
                                "en": "The angel was drawn with a radiant <w>halo</w> around the head.",
                                "ru": "Ангел был нарисован с сияющим <w>ореолом</w> вокруг головы."
                            }
                        }
                    ],
                    "synonyms": [
                        "aureool",
                        "mandorla"
                    ],
                    "target_lang_definitions": {
                        "en": "A circle of light or radiance around the head of a saint.",
                        "ru": "Светящийся круг или сияние вокруг головы святого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "halo",
                                "target_lang_sense_clarification": "circle of light"
                            },
                            {
                                "target_lang_word": "aureole",
                                "target_lang_sense_clarification": "radiant light"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нимб",
                                "target_lang_sense_clarification": "над головой святого"
                            },
                            {
                                "target_lang_word": "ореол",
                                "target_lang_sense_clarification": "сияние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d80fefb6-fabb-317c-b6de-d8875c1384f6",
                    "sense_definition": "Een warme alcoholische drank, meestal bestaande uit koffie met brandewijn en suiker.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "food_drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de koude winterwandeling bestelden we een warme <w>gloria</w> in het café.",
                            "target_lang_translations": {
                                "en": "After the cold winter walk, we ordered a warm <w>coffee with brandy</w> in the café.",
                                "ru": "После холодной зимней прогулки мы заказали в кафе горячую <w>глорию</w>."
                            }
                        },
                        {
                            "text": "Mijn grootvader dronk 's avonds graag een <w>gloria</w> met veel suiker.",
                            "target_lang_translations": {
                                "en": "My grandfather liked to drink a <w>coffee with brandy</w> with lots of sugar in the evening.",
                                "ru": "Мой дедушка любил вечером выпить <w>глорию</w> с большим количеством сахара."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A warm alcoholic drink, usually consisting of coffee with brandy and sugar.",
                        "ru": "Горячий алкогольный напиток, обычно состоящий из кофе с бренди и сахаром."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "coffee with brandy",
                                "target_lang_sense_clarification": "alcoholic beverage"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "глория",
                                "target_lang_sense_clarification": "кофейный напиток с алкоголем"
                            }
                        ]
                    }
                }
            ]
        }
    ]

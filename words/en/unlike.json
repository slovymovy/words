                        {
                            "text": "We have very <w>unlike</w> opinions on this political matter.",
                            "target_lang_translations": {
                                "ru": "У нас очень <w>разные</w> мнения по этому политическому вопросу."
                            }
                        }
                    ],
                    "synonyms": [
                        "different",
                        "dissimilar",
                        "unalike"
                    ],
                    "antonyms": [
                        "like",
                        "similar",
                        "alike"
                    ],
                    "common_phrases": [

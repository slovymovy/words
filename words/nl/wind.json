            "senses": [
                {
                    "sense_id": "128be655-12e2-3996-a738-f9e36c3a6963",
                    "sense_definition": "De stroming van lucht veroorzaakt door luchtdrukverschillen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "weather_phenomenon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>wind</w> waait vandaag erg hard door de bomen.",
                            "target_lang_translations": {
                                "en": "The <w>wind</w> is blowing very hard through the trees today.",
                                "ru": "Сегодня <w>ветер</w> очень сильно дует сквозь деревья."
                            }
                        },
                        {
                            "text": "We fietsen met de <w>wind</w> in de rug naar huis.",
                            "target_lang_translations": {
                                "en": "We are cycling home with the <w>wind</w> at our backs.",
                                "ru": "Мы едем домой на велосипедах с попутным <w>ветром</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "luchtstroom"
                    ],
                    "antonyms": [
                        "luwte"
                    ],
                    "common_phrases": [
                        "in de wind",
                        "tegen de wind in",
                        "de wind mee hebben",
                        "waaien als de wind"
                    ],
                    "target_lang_definitions": {
                        "en": "The flow of air caused by differences in air pressure.",
                        "ru": "Поток воздуха, вызванный разницей атмосферного давления."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wind",
                                "target_lang_sense_clarification": "movement of air"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ветер",
                                "target_lang_sense_clarification": "атмосферное явление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6af9c091-d24d-3e12-a1dd-2a2ccd2a07da",
                    "sense_definition": "Gasvormige ontlasting die uit de darmen ontsnapt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "bodily_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij liet per ongeluk een harde <w>wind</w> tijdens de vergadering.",
                            "target_lang_translations": {
                                "en": "He accidentally passed a loud <w>wind</w> during the meeting.",
                                "ru": "Он случайно громко пустил <w>ветры</w> во время совещания."
                            }
                        },
                        {
                            "text": "Het inhouden van een <w>wind</w> kan soms buikpijn veroorzaken.",
                            "target_lang_translations": {
                                "en": "Holding in <w>wind</w> can sometimes cause a stomach ache.",
                                "ru": "Сдерживание <w>газов</w> может иногда вызывать боль в животе."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheet",
                        "ruft",
                        "buikwind"
                    ],
                    "common_phrases": [
                        "een wind laten"
                    ],
                    "target_lang_definitions": {
                        "en": "Gaseous waste that escapes from the intestines.",
                        "ru": "Газы, выходящие из кишечника."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wind",
                                "target_lang_sense_clarification": "intestinal gas"

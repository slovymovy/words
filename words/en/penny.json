                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Penny</w> was known for his generous contributions to the library.",
                            "target_lang_translations": {

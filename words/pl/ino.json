{
    "word_family": [],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "be0c7973-9437-3e56-884d-d2fd91fbc3aa",
                    "sense_definition": "Potoczne lub gwarowe słowo oznaczające ograniczenie lub wyłączność; to samo, co <w>tylko</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "restriction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zostało mi w kieszeni <ino> kilka drobnych monet.",
                            "target_lang_translations": {
                                "ru": "У меня в кармане осталось <w>только</w> несколько мелких монет."
                            }
                        },
                        {
                            "text": "Nie gadaj tyle, <ino> bierz się od razu do pracy.",
                            "target_lang_translations": {
                                "ru": "Не болтай столько, <w>а</w> сразу берись за работу."
                            }
                        },
                        {
                            "text": "Patrz <ino>, jak szybko ten samochód przejechał!",
                            "target_lang_translations": {
                                "ru": "<w>Только</w> посмотри, как быстро проехала эта машина!"
                            }
                        }
                    ],
                    "synonyms": [
                        "tylko",
                        "jeno"
                    ],
                    "common_phrases": [
                        "patrz ino",
                        "czekaj ino",
                        "słuchaj ino"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "częste w gwarach (np. śląskiej, góralskiej)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное или диалектное слово, означающее ограничение или исключительность; то же, что «только»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "ограничение"
                            },
                            {
                                "target_lang_word": "лишь",
                                "target_lang_sense_clarification": "ограничение"
                            },
                            {
                                "target_lang_word": "просто",
                                "target_lang_sense_clarification": "в значении «всего лишь»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "72a8af9e-b275-3d68-bc1f-5a21b483fb1b",
                    "sense_definition": "Gwarowe słowo wyrażające potwierdzenie; to samo, co <w>tak</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "– Czy to na pewno twoja książka? – <Ino>!",
                            "target_lang_translations": {
                                "ru": "– Это точно твоя книга? – <w>Да</w>!"
                            }
                        },
                        {
                            "text": "– Wybierasz się z nami jutro do miasta? – <Ino>.",
                            "target_lang_translations": {
                                "ru": "– Ты пойдешь с нами завтра в город? – <w>Ага</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tak"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "występuje w niektórych gwarach"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Диалектное слово, выражающее подтверждение; то же, что «да»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "подтверждение"
                            },
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "разговорное подтверждение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
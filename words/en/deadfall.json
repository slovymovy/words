    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c30c9478-8972-3dc8-a2bf-8776491158c6",
                    "sense_definition": "A fallen tree or a tangled mass of fallen wood and branches on the forest floor.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hikers struggled to climb over the massive <w>deadfall</w> blocking the trail.",
                            "target_lang_translations": {
                                "ru": "Туристы с трудом перебирались через огромный <w>бурелом</w>, преграждавший тропу."
                            }
                        },
                        {
                            "text": "We gathered some dry <w>deadfall</w> to build a campfire for the night.",
                            "target_lang_translations": {
                                "ru": "Мы собрали сухой <w>валежник</w>, чтобы развести костёр на ночь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clear the deadfall",
                        "forest deadfall",
                        "dry deadfall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поваленное дерево или нагромождение упавших стволов и веток на земле в лесу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "валежник",
                                "target_lang_sense_clarification": "сухие сучья и деревья, лежащие на земле"
                            },
                            {
                                "target_lang_word": "бурелом",
                                "target_lang_sense_clarification": "лес, поваленный бурей; труднопроходимый завал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ece1b0c-7663-3797-8477-8fda012d6791",
                    "sense_definition": "A trap for animals consisting of a heavy weight, such as a log, that falls onto the prey when triggered.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The survivalist demonstrated how to set a <w>deadfall</w> to catch small game.",
                            "target_lang_translations": {
                                "ru": "Специалист по выживанию показал, как установить <w>плашку</w> для ловли мелкой дичи."
                            }
                        },
                        {
                            "text": "A simple <w>deadfall</w> can be effective, but it requires careful placement to work.",
                            "target_lang_translations": {
                                "ru": "Простая <w>ловушка</w> может быть эффективной, но для работы требуется тщательная установка."
                            }
                        }
                    ],
                    "synonyms": [
                        "deadfall trap"
                    ],
                    "common_phrases": [
                        "set a deadfall",
                        "build a deadfall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ловушка для животных, состоящая из тяжелого предмета (например, бревна), который падает на добычу при срабатывании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плашка",
                                "target_lang_sense_clarification": "охотничья ловушка давящего типа"
                            },
                            {
                                "target_lang_word": "ловушка",
                                "target_lang_sense_clarification": "общее название устройства для поимки зверей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a126a33a-9b5d-3cb5-b9cc-585c443f711e",
                    "sense_definition": "A cheap, rough, and often disreputable bar or saloon.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent his evenings gambling in a smoky <w>deadfall</w> on the edge of town.",
                            "target_lang_translations": {
                                "ru": "Он проводил вечера за азартными играми в прокуренном <w>притоне</w> на окраине города."
                            }
                        },
                        {
                            "text": "The old cowboy warned us to stay away from that notorious <w>deadfall</w>.",
                            "target_lang_translations": {
                                "ru": "Старый ковбой предупредил нас держаться подальше от этого пользующегося дурной славой <w>кабака</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dive"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "typically refers to historical saloons or gambling dens"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дешёвое, грубое и часто пользующееся дурной славой питейное заведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "притон",
                                "target_lang_sense_clarification": "место с сомнительной репутацией"
                            },
                            {
                                "target_lang_word": "кабак",
                                "target_lang_sense_clarification": "разг. питейное заведение, трактир"
                            },
                            {
                                "target_lang_word": "дыра",
                                "target_lang_sense_clarification": "разг. захолустное или убогое заведение"
                            }
                        ]

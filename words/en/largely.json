{
    "word_family": [
        "large",
        "largeness",
        "enlarge",
        "enlargement"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "bd2d37d8-8cf7-37e0-a15b-3fc601da1152",
                    "sense_definition": "For the most part; mainly or chiefly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "main_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The success of the project was <w>largely</w> due to the hard work of the team.",
                            "target_lang_translations": {
                                "ru": "Успех проекта был <w>в значительной степени</w> обусловлен упорной работой команды."
                            }
                        },
                        {
                            "text": "The old traditions have been <w>largely</w> forgotten by the younger generation.",
                            "target_lang_translations": {
                                "ru": "Старые традиции были <w>по большей части</w> забыты молодым поколением."
                            }
                        }
                    ],
                    "synonyms": [
                        "mostly",
                        "for the most part",
                        "by and large",
                        "in the main"
                    ],
                    "common_phrases": [
                        "largely due to",
                        "largely because",
                        "largely responsible",
                        "largely ignored"
                    ],
                    "target_lang_definitions": {
                        "ru": "В основном; по большей части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в значительной степени",
                                "target_lang_sense_clarification": "указывает на высокую степень влияния или участия"
                            },
                            {
                                "target_lang_word": "во многом",
                                "target_lang_sense_clarification": "по многим причинам или в существенной мере"
                            },
                            {
                                "target_lang_word": "по большей части",
                                "target_lang_sense_clarification": "преимущественно, в большинстве случаев"
                            },
                            {
                                "target_lang_word": "главным образом",
                                "target_lang_sense_clarification": "в первую очередь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f93c2a93-bcbf-3403-b51d-5044fc060767",
                    "sense_definition": "On a large scale; amply.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quantity_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company contributed <w>largely</w> to the relief fund.",
                            "target_lang_translations": {
                                "ru": "Компания <w>щедро</w> пожертвовала в фонд помощи."
                            }
                        },
                        {
                            "text": "He drank <w>largely</w> from the cup to quench his thirst.",
                            "target_lang_translations": {
                                "ru": "Он <w>много</w> отпил из чашки, чтобы утолить жажду."
                            }
                        }
                    ],
                    "synonyms": [
                        "abundantly",
                        "plentifully",
                        "fully"
                    ],
                    "common_phrases": [
                        "contribute largely"
                    ],
                    "target_lang_definitions": {
                        "ru": "В большом масштабе; обильно или щедро."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щедро",
                                "target_lang_sense_clarification": "богато, не скупясь (о пожертвованиях)"
                            },
                            {
                                "target_lang_word": "обильно",
                                "target_lang_sense_clarification": "в большом количестве"
                            },
                            {
                                "target_lang_word": "в значительной мере",
                                "target_lang_sense_clarification": "существенно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c3fd162-4915-300c-ab1e-dfc71cae9b3c",
                    "sense_definition": "In a widespread or large manner.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "manner_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She smiled <w>largely</w> at the guests as they arrived.",
                            "target_lang_translations": {
                                "ru": "Она <w>широко</w> улыбалась гостям, когда они прибывали."
                            }
                        },
                        {
                            "text": "The host gestured <w>largely</w> to welcome everyone into the hall.",
                            "target_lang_translations": {
                                "ru": "Хозяин <w>размашисто</w> жестикулировал, приглашая всех в зал."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Широко; с размахом (о жестах или мимике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широко",
                                "target_lang_sense_clarification": "об улыбке или открытости"
                            },
                            {
                                "target_lang_word": "размашисто",
                                "target_lang_sense_clarification": "о жестах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "281cb87c-0867-3f6e-9a5c-3b75dc0a75f7",
                    "sense_definition": "Fully, at great length.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "length_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author wrote <w>largely</w> on the history of the region.",
                            "target_lang_translations": {
                                "ru": "Автор <w>подробно</w> писал об истории региона."
                            }
                        },
                        {
                            "text": "I cannot speak <w>largely</w> on this subject right now.",
                            "target_lang_translations": {
                                "ru": "Я не могу сейчас <w>пространно</w> рассуждать на эту тему."
                            }
                        }
                    ],
                    "synonyms": [
                        "at length"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Used in older texts to mean 'at length'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подробно; пространно (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подробно",
                                "target_lang_sense_clarification": "в деталях"
                            },
                            {
                                "target_lang_word": "пространно",
                                "target_lang_sense_clarification": "многословно, обширно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "экономика",
        "экономический",
        "экономный",
        "экономия",
        "экономист",
        "экономичный",
        "сэкономить"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "829e6897-7ee8-3caf-9b44-07b220b7d53b",
                    "sense_definition": "Расходовать что-либо (деньги, время, ресурсы) бережно, стараясь потратить как можно меньше.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "saving_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы стараемся <экономить> электричество и всегда выключаем свет.",
                            "target_lang_translations": {
                                "en": "We try to <save> electricity and always turn off the light."
                            }
                        },
                        {
                            "text": "Этот новый прибор помогает <экономить> время на кухне.",
                            "target_lang_translations": {
                                "en": "This new device helps to <save> time in the kitchen."
                            }
                        }
                    ],
                    "synonyms": [
                        "беречь"
                    ],
                    "antonyms": [
                        "транжирить"
                    ],
                    "common_phrases": [
                        "экономить деньги",
                        "экономить время",
                        "экономить силы",
                        "экономить электроэнергию"
                    ],
                    "target_lang_definitions": {
                        "en": "To use resources (such as money, time, or energy) carefully in order to avoid waste."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "save",
                                "target_lang_sense_clarification": "to use less of a resource, such as money, time, or electricity"
                            },
                            {
                                "target_lang_word": "conserve",
                                "target_lang_sense_clarification": "to protect resources from being wasted or used up"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "433a4317-9f3a-3487-b7ec-45257dc31965",
                    "sense_definition": "Избегать лишних трат, ограничивать себя в расходах на что-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "saving_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студентам часто приходится <экономить> на развлечениях.",
                            "target_lang_translations": {
                                "en": "Students often have to <save> on entertainment."
                            }
                        },
                        {
                            "text": "Нельзя <экономить> на здоровье и качественных продуктах.",
                            "target_lang_translations": {
                                "en": "One shouldn't <economize> on health and quality products."
                            }
                        }
                    ],
                    "common_phrases": [
                        "экономить на всём",
                        "экономить на еде",
                        "экономить на качестве"
                    ],
                    "target_lang_definitions": {
                        "en": "To restrict one's spending or avoid unnecessary expenses."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "save",
                                "target_lang_sense_clarification": "to spend less money, often used with 'on' to indicate the area of reduction"
                            },
                            {
                                "target_lang_word": "economize",
                                "target_lang_sense_clarification": "to reduce expenses or practice frugality"
                            },
                            {
                                "target_lang_word": "cut back",
                                "target_lang_sense_clarification": "to reduce the amount of money spent on something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
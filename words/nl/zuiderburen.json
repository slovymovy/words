                {
                    "sense_id": "09142481-4342-3f82-aa92-0da6acadddbb",
                    "sense_definition": "Meervoud van <w>zuiderbuur</w>; de buren die ten zuiden wonen, door Nederlanders specifiek gebruikt om de inwoners van België aan te duiden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel Nederlanders gaan graag een weekendje weg bij de <w>zuiderburen</w>.",
                            "target_lang_translations": {
                                "ru": "Многие нидерландцы любят ездить на выходные к <w>южным соседям</w>."
                            }
                        },
                        {
                            "text": "Onze <w>zuiderburen</w> staan bekend om hun heerlijke chocolade en speciaalbieren.",
                            "target_lang_translations": {
                                "ru": "Наши <w>южные соседи</w> славятся своим восхитительным шоколадом и особыми сортами пива."
                            }
                        }
                    ],
                    "synonyms": [
                        "Belgen"
                    ],
                    "antonyms": [
                        "noorderburen"
                    ],
                    "common_phrases": [
                        "onze zuiderburen",
                        "bij de zuiderburen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соседи, живущие к югу; термин, используемый нидерландцами для обозначения жителей Бельгии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "южные соседи",
                                "target_lang_sense_clarification": "о бельгийцах"
                            },
                            {
                                "target_lang_word": "бельгийцы",
                                "target_lang_sense_clarification": "значение слова"
                            }
                        ]
                    }
                }
            ]
        },

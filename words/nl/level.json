                    ],
                    "common_phrases": [
                        "op hetzelfde level",
                        "naar een hoger level"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as an anglicism for 'niveau'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень качества, высоты или развития; синоним слова «уровень»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "степень качества или развития"
                            }
                        ]
                    }
                }
            ]
        },

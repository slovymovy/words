    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b71cf9c-140c-32ee-8db0-753292c0158a",
                    "sense_definition": "Официальный осмотр помещения, вещей или человека представителями власти с целью найти что-либо спрятанное или запрещённое.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "search_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция предъявила ордер на <w>обыск</w> и вошла в квартиру.",
                            "target_lang_translations": {
                                "en": "The police presented a <w>search</w> warrant and entered the apartment."
                            }
                        },
                        {
                            "text": "Все шкафы и столы в комнате носили следы недавнего <w>обыска</w>.",
                            "target_lang_translations": {
                                "en": "All the cupboards and tables in the room bore traces of a recent <w>search</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "шмон"
                    ],
                    "common_phrases": [
                        "ордер на обыск",
                        "провести обыск",
                        "личный обыск",
                        "тщательный обыск"
                    ],
                    "target_lang_definitions": {
                        "en": "An official inspection of premises, belongings, or a person by authorities to find concealed or prohibited items."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "search",
                                "target_lang_sense_clarification": "official inspection by authorities"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afe153cd-7d01-3f43-bf46-43160a24c6fe",
                    "sense_definition": "Письменное церковное свидетельство об отсутствии родства между людьми, вступающими в брак (в дореволюционной России).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Священник занёс брачный <w>обыск</w> в церковную книгу перед венчанием.",
                            "target_lang_translations": {
                                "en": "The priest entered the pre-nuptial <w>inquiry</w> into the church register before the wedding."
                            }
                        },
                        {
                            "text": "Без подписанного <w>обыска</w> обряд бракосочетания не проводился.",
                            "target_lang_translations": {
                                "en": "The marriage ceremony was not performed without a signed <w>inquiry</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "свидетельство"
                    ],
                    "common_phrases": [
                        "брачный обыск"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical religious term"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A written church certificate confirming the absence of kinship between people intending to marry (historical)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pre-nuptial inquiry",
                                "target_lang_sense_clarification": "historical religious document"
                            }
                        ]
                    }
                }
            ]
        }
    ]

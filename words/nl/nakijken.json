                {
                    "sense_id": "d2279f32-9407-351d-a077-2e0af62c373c",
                    "sense_definition": "Het beoordelen en corrigeren van gemaakt werk, zoals toetsen of huiswerk.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "check_verify",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar heeft het hele weekend besteed aan het <w>nakijken</w> van de examens.",
                            "target_lang_translations": {

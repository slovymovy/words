{
    "word_family": [
        "intervallic"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a1603d7c-a169-3259-a9c6-de5542bb80a7",
                    "sense_definition": "A period of time between two events or situations.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>interval</w> between the two meetings was very short.",
                            "target_lang_translations": {
                                "ru": "<w>Промежуток</w> между двумя встречами был очень коротким."
                            }
                        },
                        {
                            "text": "Buses leave the station at regular <w>intervals</w> throughout the day.",
                            "target_lang_translations": {
                                "ru": "Автобусы отправляются со станции через равные <w>интервалы</w> в течение дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "period",
                        "time",
                        "gap",
                        "pause",
                        "spell"
                    ],
                    "common_phrases": [
                        "at regular intervals",
                        "in the interval",
                        "brief interval"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени между двумя событиями или ситуациями"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "промежу́ток",
                                "target_lang_sense_clarification": "времени"
                            },
                            {
                                "target_lang_word": "интерва́л",
                                "target_lang_sense_clarification": "временной отрезок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92cbe195-f3d4-35f2-bd4f-d3ed965f7abb",
                    "sense_definition": "A short break between parts of a play, concert, or sports event.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bought some drinks and snacks during the <w>interval</w>.",
                            "target_lang_translations": {
                                "ru": "Мы купили напитки и закуски во время <w>антракта</w>."
                            }
                        },
                        {
                            "text": "The play includes a twenty-minute <w>interval</w> after the first act.",
                            "target_lang_translations": {
                                "ru": "В пьесе предусмотрен двадцатиминутный <w>антракт</w> после первого акта."
                            }
                        }
                    ],
                    "synonyms": [
                        "intermission",
                        "break",
                        "half-time",
                        "pause"
                    ],
                    "common_phrases": [
                        "during the interval",
                        "at the interval"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий перерыв между частями спектакля, концерта или спортивного мероприятия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "антра́кт",
                                "target_lang_sense_clarification": "в театре или на концерте"
                            },
                            {
                                "target_lang_word": "переры́в",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8607f5b6-0a47-38d6-b31b-c054acb55986",
                    "sense_definition": "A distance or empty space between two objects or points.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The trees were planted at an <w>interval</w> of three meters.",
                            "target_lang_translations": {
                                "ru": "Деревья были посажены с <w>интервалом</w> в три метра."
                            }
                        },
                        {
                            "text": "There is a wide <w>interval</w> between the inner and outer fences.",
                            "target_lang_translations": {
                                "ru": "Между внутренним и внешним заборами есть широкий <w>промежуток</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gap",
                        "space",
                        "distance"
                    ],
                    "common_phrases": [
                        "at intervals"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расстояние или свободное пространство между двумя объектами или точками"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расстоя́ние",
                                "target_lang_sense_clarification": "дистанция"
                            },
                            {
                                "target_lang_word": "промежу́ток",
                                "target_lang_sense_clarification": "свободное пространство"
                            },
                            {
                                "target_lang_word": "интерва́л",
                                "target_lang_sense_clarification": "техническое или военное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da0a4882-e056-3559-8c06-e70cc88b4c41",
                    "sense_definition": "The difference in pitch between two musical notes.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>interval</w> between C and E is called a major third.",
                            "target_lang_translations": {
                                "ru": "<w>Интервал</w> между нотами до и ми называется большой терцией."
                            }
                        },
                        {
                            "text": "She learned to recognize every musical <w>interval</w> by ear.",
                            "target_lang_translations": {
                                "ru": "Она научилась распознавать каждый музыкальный <w>интервал</w> на слух."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разница в высоте между двумя музыкальными нотами"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интерва́л",
                                "target_lang_sense_clarification": "музыкальный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aafafb8b-8394-3899-9b86-ef5740746d5e",
                    "sense_definition": "A set of numbers consisting of all the numbers between two given numbers.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The function is defined on the <w>interval</w> from zero to one.",
                            "target_lang_translations": {
                                "ru": "Функция определена на <w>интервале</w> от нуля до единицы."
                            }
                        },
                        {
                            "text": "Students calculated the value within the closed <w>interval</w>.",
                            "target_lang_translations": {
                                "ru": "Студенты вычислили значение в пределах замкнутого <w>интервала</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множество чисел, включающее все числа между двумя заданными числами"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интерва́л",
                                "target_lang_sense_clarification": "математический"
                            },
                            {
                                "target_lang_word": "промежу́ток",
                                "target_lang_sense_clarification": "числовой"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "mrugnięcie",
        "mrugnąć"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d23dc14d-b423-3780-8232-9b0effa76f46",
                    "sense_definition": "Wykonywać szybkie ruchy powiekami, chwilowo zamykając i otwierając oczy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "body_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słońce świeciło tak mocno, że musiałem ciągle <w>mrugać</w>.",
                            "target_lang_translations": {
                                "ru": "Солнце светило так ярко, что мне приходилось постоянно <w>моргать</w>."
                            }
                        },
                        {
                            "text": "Dziewczynka zaczęła szybko <w>mrugać</w> oczami ze zdziwienia.",
                            "target_lang_translations": {
                                "ru": "Девочка начала быстро <w>моргать</w> глазами от удивления."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mrugać oczami",
                        "mrugać powiekami",
                        "mrugać nerwowo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать быстрые движения веками, на мгновение закрывая и открывая глаза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моргать",
                                "target_lang_sense_clarification": "глазами"
                            },
                            {
                                "target_lang_word": "мигать",
                                "target_lang_sense_clarification": "веками"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c7fe8ef-6e75-3940-bb08-2a52746e71fe",
                    "sense_definition": "Świecić nieregularnym, przerywanym lub pulsującym światłem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "light_emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na ciemnym niebie wesoło <w>mrugały</w> tysiące jasnych gwiazd.",
                            "target_lang_translations": {
                                "ru": "На темном небе весело <w>мерцали</w> тысячи ярких звезд."
                            }
                        },
                        {
                            "text": "Stara latarnia uliczna zaczęła niebezpiecznie <w>mrugać</w> i w końcu zgasła.",
                            "target_lang_translations": {
                                "ru": "Старый уличный фонарь начал опасно <w>мигать</w> и в конце концов погас."
                            }
                        }
                    ],
                    "synonyms": [
                        "migotać",
                        "błyskać",
                        "migać"
                    ],
                    "common_phrases": [
                        "gwiazdy mrugają",
                        "światło mruga",
                        "lampka mruga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Светить неровным, прерывистым или пульсирующим светом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мерцать",
                                "target_lang_sense_clarification": "о звездах, слабом свете"
                            },
                            {
                                "target_lang_word": "мигать",
                                "target_lang_sense_clarification": "о лампе, индикаторе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3182ee7f-9630-318f-97bd-24c87e98dc65",
                    "sense_definition": "Dawać sygnały świetlne innym uczestnikom ruchu, włączając i wyłączając światła pojazdu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "light_emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca z naprzeciwka zaczął mi <w>mrugać</w>, ostrzegając przed kontrolą drogową.",
                            "target_lang_translations": {
                                "ru": "Водитель встречной машины начал мне <w>моргать</w>, предупреждая о дорожном контроле."
                            }
                        },
                        {
                            "text": "Nie powinieneś bez powodu <w>mrugać</w> światłami na autostradzie.",
                            "target_lang_translations": {
                                "ru": "Ты не должен без причины <w>мигать</w> фарами на автостраде."
                            }
                        }
                    ],
                    "synonyms": [
                        "migać"
                    ],
                    "common_phrases": [
                        "mrugać światłami",
                        "mrugać długimi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подавать световые сигналы другим участникам движения, включая и выключая фары автомобиля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моргать",
                                "target_lang_sense_clarification": "фарами"
                            },
                            {
                                "target_lang_word": "мигать",
                                "target_lang_sense_clarification": "сигналить светом"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
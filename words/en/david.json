            "senses": [
                {
                    "sense_id": "38514242-36f6-3352-9745-e6c2280f1441",
                    "sense_definition": "A common male given name of Hebrew origin.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My brother <w>David</w> is coming to visit us tomorrow.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>Дэвид</w> приедет навестить нас завтра."
                            }
                        },
                        {
                            "text": "Please tell <w>David</w> that I will call him back later.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, передай <w>Дэвиду</w>, что я перезвоню ему позже."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенное мужское имя еврейского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дэвид",
                                "target_lang_sense_clarification": "английское имя"
                            },
                            {
                                "target_lang_word": "Давид",
                                "target_lang_sense_clarification": "общее имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc73218d-360f-3203-a92e-224083785db4",
                    "sense_definition": "The second king of ancient Israel and Judah, a central biblical figure known for killing the giant Goliath and writing many psalms.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "The statue by Michelangelo depicts the biblical hero <w>David</w> before his battle.",
                            "target_lang_translations": {
                                "ru": "Статуя Микеланджело изображает библейского героя <w>Давида</w> перед битвой."
                            }
                        },
                        {
                            "text": "King <w>David</w> is traditionally credited with composing many of the Psalms.",
                            "target_lang_translations": {
                                "ru": "Царю <w>Давиду</w> традиционно приписывают авторство многих псалмов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Star of David",
                        "City of David"
                    ],
                    "target_lang_definitions": {
                        "ru": "Второй царь древнего Израиля и Иудеи, центральная библейская фигура, известная победой над великаном Голиафом и написанием множества псалмов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Давид",
                                "target_lang_sense_clarification": "библейский царь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce56bf0a-83e7-355a-854d-da25e2a1f54b",
                    "sense_definition": "A surname derived from the given name.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>David</w> has been working at the firm for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Дэвид</w> работает в фирме уже двадцать лет."
                            }
                        },
                        {
                            "text": "The letter was addressed to the <w>David</w> family.",
                            "target_lang_translations": {
                                "ru": "Письмо было адресовано семье <w>Дэвид</w>."

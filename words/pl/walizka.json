            "senses": [
                {
                    "sense_id": "b56da4d3-cfbe-3935-8edd-662db030f0a3",
                    "sense_definition": "Rodzaj torby podróżnej w formie sztywnego prostopadłościanu z uchwytem, służącej do przewożenia ubrań i rzeczy osobistych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "travel_bag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Udało mi się spakować wszystkie rzeczy tylko w jedną małą <w>walizkę</w>.",
                            "target_lang_translations": {
                                "ru": "Мне удалось упаковать все вещи только в один маленький <w>чемодан</w>."
                            }
                        },
                        {
                            "text": "Moja <w>walizka</w> jest zbyt ciężka, żeby wnieść ją po schodach na trzecie piętro.",
                            "target_lang_translations": {
                                "ru": "Мой <w>чемодан</w> слишком тяжёлый, чтобы поднять его по лестнице на третий этаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "waliza",
                        "bagaż"
                    ],
                    "common_phrases": [
                        "spakować walizkę",
                        "rozpakować walizkę",
                        "walizka na kółkach",
                        "ciężka walizka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дорожная сумка с жёсткими стенками и ручкой, предназначенная для перевозки одежды и личных вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чемодан",
                                "target_lang_sense_clarification": "дорожная сумка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3dc8e36-21d4-34b4-88de-a25ef0f7e22d",
                    "sense_definition": "Duży brzuch.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "body_part_slang",
                    "name_type": "no",
                    "examples": [
                        {

            "senses": [
                {
                    "sense_id": "339257fd-26b9-373d-a99f-01f56ced2e55",
                    "sense_definition": "Een kunstwerk of ander product dat getuigt van uitzonderlijk vakmanschap en kwaliteit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit schilderij wordt door velen beschouwd als een waar <w>meesterwerk</w>.",
                            "target_lang_translations": {
                                "en": "This painting is considered by many to be a true <w>masterpiece</w>.",
                                "ru": "Многие считают эту картину настоящим <w>шедевром</w>."
                            }
                        },
                        {
                            "text": "De schrijver leverde een literair <w>meesterwerk</w> af met zijn debuutroman.",
                            "target_lang_translations": {
                                "en": "The writer delivered a literary <w>masterpiece</w> with his debut novel.",
                                "ru": "Своим дебютным романом писатель создал литературный <w>шедевр</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "meesterstuk",
                        "topwerk"
                    ],
                    "antonyms": [
                        "broddelwerk",
                        "prul"
                    ],
                    "common_phrases": [
                        "een waar meesterwerk",
                        "literair meesterwerk",
                        "schilderkundig meesterwerk"
                    ],
                    "target_lang_definitions": {
                        "en": "A work of art or other product that demonstrates exceptional craftsmanship and quality.",
                        "ru": "Произведение искусства или иное изделие, свидетельствующее об исключительном мастерстве и качестве."
                    },
                    "translations": {

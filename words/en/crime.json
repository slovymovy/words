                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "punish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sergeant <w>crimed</w> the soldier for having a messy uniform.",
                            "target_lang_translations": {
                                "ru": "Сержант <w>наказал</w> солдата за неопрятную форму."
                            }
                        },
                        {
                            "text": "He was <w>crimed</w> immediately after causing a disturbance in the barracks.",
                            "target_lang_translations": {
                                "ru": "Ему <w>объявили взыскание</w> сразу после того, как он устроил беспорядок в казарме."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "military context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обвинять кого-либо в дисциплинарном проступке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "налагать взыскание",
                                "target_lang_sense_clarification": "военный термин"
                            },
                            {
                                "target_lang_word": "наказывать",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        },

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cbbbb185-b912-3b22-934c-302d63b2f81f",
                    "sense_definition": "Verdergaan met een activiteit of handeling; niet stoppen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "continue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten blijven <w>doorgaan</w> tot het werk helemaal af is.",
                            "target_lang_translations": {
                                "en": "We must <w>keep going</w> until the work is completely finished.",
                                "ru": "Мы должны <w>продолжать</w>, пока работа не будет полностью закончена."
                            }
                        },
                        {
                            "text": "Ondanks de vermoeidheid wilde de loper toch <w>doorgaan</w>.",
                            "target_lang_translations": {
                                "en": "Despite the fatigue, the runner still wanted to <w>continue</w>.",
                                "ru": "Несмотря на усталость, бегун все равно хотел <w>продолжать</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "volhouden"
                    ],
                    "antonyms": [
                        "stoppen"
                    ],
                    "common_phrases": [
                        "ga zo door",
                        "doorgaan met"
                    ],
                    "target_lang_definitions": {
                        "en": "To continue with an activity or action; not to stop.",
                        "ru": "Продолжать действие или занятие; не останавливаться."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "continue",
                                "target_lang_sense_clarification": "to proceed with an activity"
                            },
                            {
                                "target_lang_word": "go on",
                                "target_lang_sense_clarification": "to continue an action"
                            },
                            {
                                "target_lang_word": "keep going",
                                "target_lang_sense_clarification": "to persist without stopping"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "продолжать",
                                "target_lang_sense_clarification": "делать дальше"
                            },
                            {
                                "target_lang_word": "продолжаться",
                                "target_lang_sense_clarification": "длиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9fa2396-c411-310e-8973-58e5f74359f6",
                    "sense_definition": "Plaatsvinden volgens de planning, vaak ondanks eerdere onzekerheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "happen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De voetbalwedstrijd zal ondanks de regen gewoon <w>doorgaan</w>.",
                            "target_lang_translations": {
                                "en": "The football match will just <w>go ahead</w> despite the rain.",
                                "ru": "Футбольный матч <w>состоится</w>, несмотря на дождь."
                            }
                        },
                        {
                            "text": "Het is nog niet zeker of het feest morgen <w>gaat door</w>.",
                            "target_lang_translations": {
                                "en": "It is not yet certain if the party <w>will go ahead</w> tomorrow.",
                                "ru": "Еще не точно, <w>состоится</w> ли завтра вечеринка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niet doorgaan",
                        "toch doorgaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To take place according to plan, often despite uncertainty.",
                        "ru": "Происходить согласно плану, часто вопреки неопределенности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go ahead",
                                "target_lang_sense_clarification": "to happen or take place as planned"
                            },
                            {
                                "target_lang_word": "take place",
                                "target_lang_sense_clarification": "to happen"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "состояться",
                                "target_lang_sense_clarification": "о событии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "675310ca-71f1-363a-b4ae-4f8108a8c74f",
                    "sense_definition": "Na een overwinning of succes naar de volgende ronde van een competitie gaan.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "advance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als ze deze wedstrijd winnen, mogen ze <w>doorgaan</w> naar de finale.",
                            "target_lang_translations": {
                                "en": "If they win this match, they can <w>advance</w> to the final.",
                                "ru": "Если они выиграют этот матч, они смогут <w>пройти</w> в финал."
                            }
                        },
                        {
                            "text": "Slechts twee kandidaten zullen <w>doorgaan</w> naar de volgende ronde.",
                            "target_lang_translations": {
                                "en": "Only two candidates will <w>go through</w> to the next round.",
                                "ru": "Только два кандидата <w>пройдут</w> в следующий раунд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "doorgaan naar"
                    ],
                    "target_lang_definitions": {
                        "en": "To move to the next round of a competition after a victory or success.",
                        "ru": "Переходить в следующий этап соревнования после победы или успеха."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "advance",
                                "target_lang_sense_clarification": "to move forward in a competition"
                            },
                            {
                                "target_lang_word": "go through",
                                "target_lang_sense_clarification": "to qualify for the next stage"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "проходить",
                                "target_lang_sense_clarification": "в следующий этап"
                            },
                            {
                                "target_lang_word": "выходить",
                                "target_lang_sense_clarification": "в финал/следующий круг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f044a62c-8375-3e5d-80a2-2ed1e7d0e879",
                    "sense_definition": "Geaccepteerd of aangezien worden voor iets of iemand anders.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "pass_for",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met die jas aan kan hij makkelijk <w>doorgaan</w> voor een politieagent.",
                            "target_lang_translations": {

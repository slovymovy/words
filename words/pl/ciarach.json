            "senses": [
                {
                    "sense_id": "1c1f9bbd-88c1-34db-af8a-03cd211c2174",
                    "sense_definition": "Ubogi lub drobny szlachcic; dawne określenie członka niższej warstwy szlachty.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "social_class_historical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo że był to tylko ubogi <ciarach>, nosił się z wielką dumą i godnością.",
                            "target_lang_translations": {
                                "ru": "Хотя это был всего лишь бедный <мелкопоместный шляхтич>, он держался с большой гордостью и достоинством."
                            }
                        },
                        {
                            "text": "Miejscowy <ciarach> często wspominał dawne czasy świetności swojego rodu.",
                            "target_lang_translations": {
                                "ru": "Местный <бедный дворянин> часто вспоминал былые времена величия своего рода."
                            }
                        }
                    ],
                    "synonyms": [
                        "szlachcic"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "regional",
                            "comment": "dawniej używane głównie na Mazowszu i Podlasiu"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бедный или мелкопоместный шляхтич; старое название представителя низшего слоя шляхты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелкопоместный шляхтич",
                                "target_lang_sense_clarification": "исторический термин"
                            },
                            {
                                "target_lang_word": "бедный дворянин",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02495926-cac4-3d24-bbad-c8ca6073df48",
                    "sense_definition": "Chłop, który ubiera się i zachowuje jak mieszczanin, próbując uchodzić za kogoś z wyższego stanu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "social_class_historical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiedzi śmiali się, że wygląda jak <ciarach>, który pierwszy raz przyjechał do miasta.",
                            "target_lang_translations": {
                                "ru": "Соседи смеялись, что он выглядит как <деревенщина>, который первый раз приехал в город."
                            }
                        },
                        {
                            "text": "Ten <ciarach> założył modny surdut, ale słoma wciąż wystawała mu z butów.",
                            "target_lang_translations": {
                                "ru": "Этот <мужик> надел модный сюртук, но солома всё ещё торчала у него из ботинок."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крестьянин, который одевается и ведет себя как мещанин, пытаясь сойти за человека высшего сословия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мужик",
                                "target_lang_sense_clarification": "пренебрежительно, о человеке низкого происхождения"
                            },
                            {
                                "target_lang_word": "деревенщина",
                                "target_lang_sense_clarification": "о неотесанном человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea0f79dd-b224-3ee2-b30e-fb28c404cd30",
                    "sense_definition": "Człowiek nędzny, niewiele wart lub przeciętny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "character_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie zadawaj się z nim, to zwykły <ciarach> bez honoru i pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Не водись с ним, это обычный <голодранец> без чести и денег."
                            }
                        },
                        {
                            "text": "Jakiś <ciarach> próbował namówić nas na podejrzany interes.",
                            "target_lang_translations": {
                                "ru": "Какой-то <оборванец> пытался подбить нас на подозрительное дело."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жалкий, никчемный или заурядный человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голодранец",
                                "target_lang_sense_clarification": "о бедном, оборванном человеке"
                            },
                            {
                                "target_lang_word": "оборванец",
                                "target_lang_sense_clarification": "о человеке в плохой одежде"
                            },
                            {
                                "target_lang_word": "ничтожество",
                                "target_lang_sense_clarification": "о человеке, не представляющем ценности"
                            }
                        ]
                    }
                }
            ]
        },

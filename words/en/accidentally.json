    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "286befc9-3d60-3ccf-b703-bc73931ed4a9",
                    "sense_definition": "Without intention; by mistake.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "unintentional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>accidentally</w> dropped my phone into the water.",
                            "target_lang_translations": {
                                "ru": "Я <w>случайно</w> уронил свой телефон в воду."
                            }
                        },
                        {
                            "text": "She <w>accidentally</w> sent the email to the wrong person.",
                            "target_lang_translations": {
                                "ru": "Она <w>по ошибке</w> отправила электронное письмо не тому человеку."
                            }
                        }
                    ],
                    "synonyms": [
                        "unintentionally",
                        "by accident",
                        "inadvertently",
                        "unwittingly",
                        "involuntarily",
                        "on accident"
                    ],
                    "antonyms": [
                        "on purpose",
                        "intentionally",
                        "deliberately",
                        "purposely",
                        "knowingly"
                    ],
                    "common_phrases": [
                        "accidentally deleted",
                        "accidentally dropped",
                        "accidentally broke"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без умысла; по ошибке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "случайно",
                                "target_lang_sense_clarification": "без намерения"
                            },
                            {
                                "target_lang_word": "нечаянно",
                                "target_lang_sense_clarification": "по неосторожности, без умысла"
                            },
                            {
                                "target_lang_word": "ненамеренно",
                                "target_lang_sense_clarification": "не имея намерения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95f70e60-e5fc-363f-9bef-95bee6365684",
                    "sense_definition": "In a way that happens by chance or unexpectedly, rather than being planned.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "chance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist discovered the new medicine <w>accidentally</w> while studying something else.",
                            "target_lang_translations": {
                                "ru": "Учёный открыл новое лекарство <w>случайно</w>, изучая что-то другое."
                            }
                        },
                        {
                            "text": "We met <w>accidentally</w> at the train station this morning.",
                            "target_lang_translations": {
                                "ru": "Мы <w>случайно</w> встретились на вокзале сегодня утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "by chance",
                        "unexpectedly",
                        "fortuitously"
                    ],
                    "antonyms": [
                        "intentionally",
                        "deliberately",
                        "by design"
                    ],
                    "common_phrases": [
                        "discovered accidentally",
                        "met accidentally"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неожиданно, по воле случая, а не по плану."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "случайно",
                                "target_lang_sense_clarification": "по стечению обстоятельств"

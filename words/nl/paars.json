{
    "word_family": [
        "paarsig"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3e25f1ab-a635-3677-b303-6bfa16e68eab",
                    "sense_definition": "Met een kleur die een mengsel is van rood en blauw, zoals die van viooltjes of lavendel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze heeft een <w>paarse</w> blouse in haar kast hangen.",
                            "target_lang_translations": {
                                "ru": "У неё в шкафу висит <w>фиолетовая</w> блузка."
                            }
                        },
                        {
                            "text": "De lucht kleurde <w>paars</w> tijdens de zonsondergang.",
                            "target_lang_translations": {
                                "ru": "Во время заката небо окрасилось в <w>фиолетовый</w> цвет."
                            }
                        }
                    ],
                    "synonyms": [
                        "purper"
                    ],
                    "common_phrases": [
                        "paars aanlopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Цвет, представляющий собой смесь красного и синего, как у фиалок или лаванды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фиолетовый",
                                "target_lang_sense_clarification": "основной цвет"
                            },
                            {
                                "target_lang_word": "лиловый",
                                "target_lang_sense_clarification": "светло-фиолетовый, сиреневый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb9538c1-a723-3aa4-86aa-80f58cd5786f",
                    "sense_definition": "Verwijzend naar een politieke samenwerking tussen liberalen (blauw) en sociaaldemocraten (rood).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een <w>paars</w> kabinet bestaat uit zowel linkse als rechtse partijen.",
                            "target_lang_translations": {
                                "ru": "<w>Фиолетовый</w> кабинет состоит как из левых, так и из правых партий."
                            }
                        },
                        {
                            "text": "De <w>paarse</w> coalitie heeft lang over het nieuwe beleid vergaderd.",
                            "target_lang_translations": {
                                "ru": "<w>Фиолетовая</w> коалиция долго совещалась по поводу новой политики."
                            }
                        }
                    ],
                    "common_phrases": [
                        "paars kabinet",
                        "paarse coalitie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к политическому сотрудничеству между либералами (синий) и социал-демократами (красный)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фиолетовый",
                                "target_lang_sense_clarification": "политический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e0c83002-e2a4-3c84-aa96-b1c42fc57a0b",
                    "sense_definition": "De kleur die zich in het spectrum tussen rood en blauw bevindt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de katholieke kerk is <w>paars</w> de kleur van boetedoening en rouw.",
                            "target_lang_translations": {
                                "ru": "В католической церкви <w>фиолетовый</w> — это цвет покаяния и траура."
                            }
                        },
                        {
                            "text": "Hij koos voor een diep <w>paars</w> als kleur voor de gordijnen.",
                            "target_lang_translations": {
                                "ru": "Он выбрал глубокий <w>фиолетовый</w> в качестве цвета для штор."
                            }
                        }
                    ],
                    "synonyms": [
                        "purper"
                    ],
                    "common_phrases": [
                        "in het paars"
                    ],
                    "target_lang_definitions": {
                        "ru": "Цвет, находящийся в спектре между красным и синим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фиолетовый цвет",
                                "target_lang_sense_clarification": "название цвета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a013205f-6f0d-3c84-bb86-9f7abce4bda6",
                    "sense_definition": "Een politieke coalitie van liberalen en sociaaldemocraten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De invloed van <w>paars</w> op de economie is nog steeds merkbaar.",
                            "target_lang_translations": {
                                "ru": "Влияние <w>фиолетовой коалиции</w> на экономику всё ещё ощутимо."
                            }
                        },
                        {
                            "text": "Tijdens de formatie werd <w>paars</w> als een serieuze optie overwogen.",
                            "target_lang_translations": {
                                "ru": "Во время формирования правительства <w>фиолетовая коалиция</w> рассматривалась как серьёзный вариант."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Политическая коалиция либералов и социал-демократов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фиолетовая коалиция",
                                "target_lang_sense_clarification": "политический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "journalist",
        "journalism",
        "journalistic"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f42bc16c-0e17-3fa3-be37-5e71a4c0de6d",
                    "sense_definition": "A daily record of personal events, thoughts, feelings, or observations.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "record_keeping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She writes in her <w>journal</w> every night before going to sleep.",
                            "target_lang_translations": {
                                "ru": "Она пишет в своём <w>дневнике</w> каждый вечер перед сном."
                            }
                        },
                        {
                            "text": "I decided to keep a <w>journal</w> to document my travels through Europe.",
                            "target_lang_translations": {
                                "ru": "Я решил вести <w>дневник</w>, чтобы документировать своё путешествие по Европе."
                            }
                        }
                    ],
                    "synonyms": [
                        "diary"
                    ],
                    "common_phrases": [
                        "keep a journal",
                        "travel journal",
                        "personal journal",
                        "write in a journal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ежедневные записи о личных событиях, мыслях, чувствах или наблюдениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дневник",
                                "target_lang_sense_clarification": "личные записи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a0cf669-93b4-395e-8acd-e7d0e76f2d9c",
                    "sense_definition": "A newspaper or magazine that deals with a particular subject or professional activity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "publication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist published her findings in a prestigious medical <w>journal</w>.",
                            "target_lang_translations": {
                                "ru": "Учёный опубликовала свои открытия в престижном медицинском <w>журнале</w>."
                            }
                        },
                        {
                            "text": "Our library subscribes to several academic <w>journals</w> regarding history and politics.",
                            "target_lang_translations": {
                                "ru": "Наша библиотека выписывает несколько академических <w>журналов</w> по истории и политике."
                            }
                        }
                    ],
                    "common_phrases": [
                        "academic journal",
                        "scientific journal",
                        "medical journal",
                        "trade journal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Периодическое издание (газета или журнал), посвященное определенной теме или профессиональной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "журнал",
                                "target_lang_sense_clarification": "периодическое издание"
                            },
                            {
                                "target_lang_word": "научный журнал",
                                "target_lang_sense_clarification": "академическое издание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "435751f0-b7bc-3d66-8dc6-31573167254d",
                    "sense_definition": "A book in which financial transactions are recorded in chronological order before being posted to the ledger.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "record_keeping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accountant made an entry in the <w>journal</w> for the new equipment purchase.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер сделал запись в <w>журнале</w> о покупке нового оборудования."
                            }
                        },
                        {
                            "text": "Errors in the <w>journal</w> can lead to significant problems in the final balance sheet.",
                            "target_lang_translations": {
                                "ru": "Ошибки в <w>журнале</w> могут привести к значительным проблемам в итоговом балансе."
                            }
                        }
                    ],
                    "synonyms": [
                        "daybook"
                    ],
                    "common_phrases": [
                        "general journal",
                        "journal entry"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "accounting context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Книга, в которой финансовые операции записываются в хронологическом порядке перед переносом в главную книгу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "журнал",
                                "target_lang_sense_clarification": "бухгалтерский"
                            },
                            {
                                "target_lang_word": "книга учёта",
                                "target_lang_sense_clarification": "финансовая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "056cc1ca-d91b-3fe3-8915-407c2495e716",
                    "sense_definition": "A chronological record of changes made to a database or system, used for recovery or auditing.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "record_keeping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The system administrator checked the <w>journal</w> to see when the file was deleted.",
                            "target_lang_translations": {
                                "ru": "Системный администратор проверил <w>журнал</w>, чтобы узнать, когда был удалён файл."
                            }
                        },
                        {
                            "text": "Database recovery relies on the transaction <w>journal</w> to restore lost data.",
                            "target_lang_translations": {
                                "ru": "Восстановление базы данных зависит от <w>журнала</w> транзакций для возврата утерянных данных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "system journal"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хронологическая запись изменений, внесенных в базу данных или систему, используемая для восстановления или аудита."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "журнал",
                                "target_lang_sense_clarification": "системный лог"
                            },
                            {
                                "target_lang_word": "лог",
                                "target_lang_sense_clarification": "компьютерный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "449240e8-0176-32a7-9cab-0925d14b9660",
                    "sense_definition": "The part of a machine shaft or axle that rests on or rotates within a bearing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mechanics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic applied grease to the <w>journal</w> to reduce friction.",
                            "target_lang_translations": {
                                "ru": "Механик нанёс смазку на <w>цапфу</w>, чтобы уменьшить трение."
                            }
                        },
                        {
                            "text": "Excessive wear on the <w>journal</w> caused the machine to vibrate dangerously.",
                            "target_lang_translations": {
                                "ru": "Чрезмерный износ <w>цапфы</w> заставил машину опасно вибрировать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "journal bearing"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "mechanical engineering"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть вала или оси машины, которая опирается на подшипник или вращается в нем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цапфа",
                                "target_lang_sense_clarification": "деталь механизма"
                            },
                            {
                                "target_lang_word": "шейка вала",
                                "target_lang_sense_clarification": "опорная часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e44fa50-04ec-3203-8f23-c985bf71725b",
                    "sense_definition": "A historical measure of land representing the amount that could be worked in a single day.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The estate was described in old documents as covering fifty <w>journaux</w> of fertile soil.",
                            "target_lang_translations": {
                                "ru": "В старых документах поместье описывалось как занимающее пятьдесят <w>журналей</w> плодородной земли."
                            }
                        },
                        {
                            "text": "Farmers in that region used to calculate their property size by the <w>journal</w>.",
                            "target_lang_translations": {
                                "ru": "Фермеры в том регионе раньше рассчитывали размер своих владений в <w>журналях</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical measurement"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Историческая мера земельной площади, соответствующая участку, который можно обработать за один день."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "журналь",
                                "target_lang_sense_clarification": "старинная мера площади"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9068e417-6046-3673-965f-d18b3ebc56f1",
                    "sense_definition": "To write in a journal or diary; to record daily experiences, thoughts, or feelings.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>journals</w> every morning to clear his mind before work.",
                            "target_lang_translations": {
                                "ru": "Он <w>ведёт дневник</w> каждое утро, чтобы очистить разум перед работой."
                            }
                        },
                        {
                            "text": "She has been <w>journaling</w> about her recovery process for several months.",
                            "target_lang_translations": {
                                "ru": "Она <w>ведёт дневник</w> о процессе своего выздоровления уже несколько месяцев."
                            }
                        }
                    ],
                    "common_phrases": [
                        "start journaling",
                        "journal about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести дневник; записывать ежедневные события, мысли или чувства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести дневник",
                                "target_lang_sense_clarification": "делать записи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2235cb8f-e053-3068-923d-7c3307759ece",
                    "sense_definition": "To insert a shaft or axle into a bearing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_mechanics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The main axle was <w>journalled</w> securely to support the heavy wheel.",
                            "target_lang_translations": {
                                "ru": "Главная ось была надёжно <w>закреплена на цапфах</w> для поддержки тяжёлого колеса."
                            }
                        },
                        {
                            "text": "Engineers designed the mechanism so the shaft is <w>journalled</w> at both ends.",
                            "target_lang_translations": {
                                "ru": "Инженеры спроектировали механизм так, что вал <w>опирается на цапфы</w> с обоих концов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "mechanical engineering"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вставлять вал или ось в подшипник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устанавливать на цапфы",
                                "target_lang_sense_clarification": "механика"
                            },
                            {
                                "target_lang_word": "опирать на подшипник",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "71c3d8cb-4d3b-3764-a2dd-1531235e3a83",
                    "sense_definition": "Occurring every day; daily.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poet described the sun's <w>journal</w> course across the sky.",
                            "target_lang_translations": {
                                "ru": "Поэт описал <w>дневной</w> путь солнца по небу."
                            }
                        },
                        {
                            "text": "They rested from their <w>journal</w> labors when the sun went down.",
                            "target_lang_translations": {
                                "ru": "Они отдыхали от своих <w>ежедневных</w> трудов, когда солнце село."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий каждый день; ежедневный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дневной",
                                "target_lang_sense_clarification": "относящийся к дню"
                            },
                            {
                                "target_lang_word": "ежедневный",
                                "target_lang_sense_clarification": "повторяющийся каждый день"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
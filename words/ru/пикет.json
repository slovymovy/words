    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "92e82e8f-fb55-3995-93da-797d772ca1ab",
                    "sense_definition": "Группа людей или один человек, стоящие в общественном месте для выражения публичного протеста или поддержки каких-либо требований.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "protest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Активисты организовали <пикет> у здания городской администрации, чтобы привлечь внимание к проблеме.",
                            "target_lang_translations": {
                                "en": "Activists organized a <picket> near the city administration building to draw attention to the problem."
                            }
                        },
                        {
                            "text": "Мой знакомый решил выйти на одиночный <пикет> в центре города.",
                            "target_lang_translations": {
                                "en": "My acquaintance decided to go out for a one-man <picket> in the city center."
                            }
                        }
                    ],
                    "common_phrases": [
                        "одиночный пикет",
                        "выйти на пикет",
                        "стоять в пикете",
                        "устроить пикет"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people or a single person standing in a public place to express protest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "picket",
                                "target_lang_sense_clarification": "protest"
                            },
                            {
                                "target_lang_word": "picket line",
                                "target_lang_sense_clarification": "group of protesters"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5813e1f-39df-38cc-ab97-e5f006220b20",
                    "sense_definition": "Небольшой военный сторожевой отряд, выставляемый для охраны или наблюдения за противником.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Генерал приказал выставить усиленный <пикет> на окраине лагеря для безопасности.",
                            "target_lang_translations": {
                                "en": "The general ordered a reinforced <picket> to be posted at the edge of the camp for security."
                            }
                        },
                        {
                            "text": "Ночью наш <пикет> заметил подозрительное движение в лесу и поднял тревогу.",
                            "target_lang_translations": {
                                "en": "At night, our <picket> noticed suspicious movement in the forest and raised the alarm."
                            }

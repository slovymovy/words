{
    "word_family": [
        "motorisch",
        "motoriseren",
        "motoriek",
        "motorblok",
        "motorboot",
        "motorrijder",
        "motorclub"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c262d1f5-d583-3c99-a490-69090f9ebffc",
                    "sense_definition": "Een machine die energie omzet in beweging om een voertuig of apparaat aan te drijven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "engine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze <w>motor</w> loopt op elektriciteit en is erg stil.",
                            "target_lang_translations": {
                                "ru": "Этот <w>мотор</w> работает на электричестве и очень тихий."
                            }
                        },
                        {
                            "text": "Je moet de <w>motor</w> afzetten als je lang moet wachten.",
                            "target_lang_translations": {
                                "ru": "Нужно заглушить <w>двигатель</w>, если приходится долго ждать."
                            }
                        }
                    ],
                    "synonyms": [
                        "krachtbron"
                    ],
                    "common_phrases": [
                        "de motor starten",
                        "de motor afzetten",
                        "draaiende motor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство, преобразующее энергию в механическое движение для привода машин или механизмов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двигатель",
                                "target_lang_sense_clarification": "механизм"
                            },
                            {
                                "target_lang_word": "мотор",
                                "target_lang_sense_clarification": "разговорное или электрический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7865d67-471a-35eb-95f6-a27d0d2e2c40",
                    "sense_definition": "Een voertuig op twee wielen dat wordt aangedreven door een motor.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft net een nieuwe <w>motor</w> gekocht om tochten te maken.",
                            "target_lang_translations": {
                                "ru": "Он только что купил новый <w>мотоцикл</w>, чтобы ездить в поездки."
                            }
                        },
                        {
                            "text": "In de zomer gaan we vaak met de <w>motor</w> op vakantie.",
                            "target_lang_translations": {
                                "ru": "Летом мы часто ездим в отпуск на <w>мотоцикле</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "motorfiets",
                        "motorrijwiel"
                    ],
                    "common_phrases": [
                        "op de motor",
                        "motor rijden"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verkorting van motorfiets"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Двухколесное транспортное средство, приводимое в движение двигателем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мотоцикл",
                                "target_lang_sense_clarification": "транспортное средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75e79c7f-02b7-30a2-b91e-f5cd0daee562",
                    "sense_definition": "Iemand of iets dat de drijvende kracht is achter een organisatie of ontwikkeling.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is de <w>motor</w> van ons bedrijf en inspireert iedereen.",
                            "target_lang_translations": {
                                "ru": "Он — <w>двигатель</w> нашей компании и вдохновляет всех."
                            }
                        },
                        {
                            "text": "Innovatie is de <w>motor</w> van economische vooruitgang in deze regio.",
                            "target_lang_translations": {
                                "ru": "Инновации — это <w>двигатель</w> экономического прогресса в этом регионе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de motor achter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или фактор, являющийся главной движущей силой развития или деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двигатель",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "движущая сила",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
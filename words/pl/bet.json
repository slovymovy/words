            "senses": [
                {
                    "sense_id": "5488c101-0196-3de6-b401-151173bc1f36",
                    "sense_definition": "Gruba kołdra, najczęściej wypełniona pierzem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą u babci zawsze spałem pod ciężkim, puchowym <w>betem</w>.",
                            "target_lang_translations": {
                                "ru": "Зимой у бабушки я всегда спал под тяжелым пуховым <w>одеялом</w>."
                            }
                        },
                        {
                            "text": "Gospodyni wywietrzyła <w>bety</w> na mrozie, żeby pozbyć się roztoczy.",
                            "target_lang_translations": {
                                "ru": "Хозяйка проветрила <w>пуховые одеяла</w> на морозе, чтобы избавиться от клещей."
                            }
                        }
                    ],
                    "synonyms": [
                        "pierzyna"
                    ],
                    "common_phrases": [
                        "ciepły bet",
                        "puchowy bet"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "słowo rzadziej używane współcześnie, wypierane przez 'kołdra'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Толстое одеяло, чаще всего наполненное перьями или пухом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пуховое одеяло",
                                "target_lang_sense_clarification": "толстое одеяло с наполнителем из пуха или пера"
                            },
                            {
                                "target_lang_word": "перина",
                                "target_lang_sense_clarification": "разг. тяжелые перьевые постельные принадлежности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd30cb04-b9ec-3594-a690-6a392381a46f",
                    "sense_definition": "Okrycie dla niemowlęcia w formie rożka lub śpiworka, służące do owijania dziecka.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "bedding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda matka ostrożnie ułożyła niemowlę w ciepłym <w>becie</w>.",
                            "target_lang_translations": {
                                "ru": "Молодая мать осторожно уложила младенца в теплый <w>конверт</w>."
                            }
                        },
                        {
                            "text": "Dziecko spało spokojnie, szczelnie owinięte w biały <w>bet</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок спокойно спал, плотно завернутый в белый <w>конверт</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "częściej używa się formy zdrobniałej 'becik'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одеяло или чехол для младенца в форме конверта или спального мешка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конверт для новорожденного",
                                "target_lang_sense_clarification": "утепленный чехол для младенца"
                            },
                            {
                                "target_lang_word": "детский конверт",
                                "target_lang_sense_clarification": "одежда-одеяло для прогулок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3414da33-9897-32f4-9135-417028885e23",
                    "sense_definition": "Druga litera alfabetów semickich, między innymi hebrajskiego, aramejskiego i fenickiego.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hebrajska litera <w>bet</w> ma wartość liczbową dwa.",
                            "target_lang_translations": {
                                "ru": "Еврейская буква <w>бет</w> имеет числовое значение два."
                            }
                        },
                        {
                            "text": "Student teologii uczył się pisać literę <w>bet</w> w różnych stylach kaligraficznych.",
                            "target_lang_translations": {
                                "ru": "Студент-теолог учился писать букву <w>бет</w> в разных каллиграфических стилях."

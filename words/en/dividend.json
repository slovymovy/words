                        "reward",
                        "payoff"
                    ],
                    "common_phrases": [
                        "pay dividends"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полезные результаты или выгода, полученные в результате прошлых усилий."
                    },
                    "translations": {
                        "ru": [

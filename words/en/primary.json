    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bb0a69fe-bae3-3780-9757-dfc839a28f06",
                    "sense_definition": "Most important; main or principal.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our <w>primary</w> concern is the safety of the passengers.",
                            "target_lang_translations": {
                                "ru": "Наша <w>главная</w> забота — безопасность пассажиров."
                            }
                        },
                        {
                            "text": "The <w>primary</w> reason for the delay was the bad weather.",
                            "target_lang_translations": {
                                "ru": "<w>Основной</w> причиной задержки была плохая погода."
                            }
                        }
                    ],
                    "synonyms": [
                        "main",
                        "chief",
                        "principal",
                        "key"
                    ],
                    "antonyms": [
                        "secondary",
                        "minor"
                    ],
                    "common_phrases": [
                        "primary concern",
                        "primary goal",
                        "primary reason",
                        "primary focus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый важный; главный или основной."

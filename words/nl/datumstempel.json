            "senses": [
                {
                    "sense_id": "98dc97ca-1a16-355f-804a-3f6fb084dd9f",
                    "sense_definition": "Een stempel die wordt gebruikt om een datum op papier, documenten of poststukken af te drukken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brief was voorzien van een <w>datumstempel</w> van het postkantoor.",
                            "target_lang_translations": {
                                "ru": "На письме стоял <w>штемпель с датой</w> почтового отделения."
                            }
                        },
                        {
                            "text": "De ambtenaar zette een <w>datumstempel</w> op het officiële formulier.",
                            "target_lang_translations": {
                                "ru": "Чиновник поставил <w>штамп с датой</w> на официальный бланк."
                            }
                        }
                    ],
                    "synonyms": [
                        "poststempel"
                    ],
                    "common_phrases": [
                        "datumstempel zetten",
                        "voorzien van datumstempel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Штамп, используемый для проставления даты на бумаге, документах или почтовых отправлениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штемпель с датой",
                                "target_lang_sense_clarification": "на почте или документах"
                            },
                            {
                                "target_lang_word": "датер",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d06dfdb-c265-3305-a9a8-2db9ab737844",
                    "sense_definition": "Een digitale markering in de metadata van een bestand die aangeeft wanneer het is gemaakt of gewijzigd.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aan de <w>datumstempel</w> kun je zien dat de foto gisteren is genomen.",
                            "target_lang_translations": {
                                "ru": "По <w>метке даты</w> можно увидеть, что фото было сделано вчера."
                            }
                        },
                        {
                            "text": "Het systeem voegt automatisch een <w>datumstempel</w> toe aan elk nieuw bestand.",
                            "target_lang_translations": {
                                "ru": "Система автоматически добавляет <w>метку даты</w> к каждому новому файлу."
                            }
                        }
                    ],
                    "synonyms": [
                        "tijdstempel"
                    ],
                    "common_phrases": [
                        "datumstempel controleren",
                        "digitale datumstempel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Цифровая метка в метаданных файла, указывающая время его создания или изменения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "метка даты",
                                "target_lang_sense_clarification": "в файлах"
                            },
                            {
                                "target_lang_word": "штамп даты",
                                "target_lang_sense_clarification": "цифровой"
                            }
                        ]
                    }
                }
            ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7b392560-3cd0-3afe-b856-6a145c04b7e7",
                    "sense_definition": "Charakteryzujący się niechęcią do pracy, wysiłku fizycznego lub umysłowego; lubiący próżnować.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest bardzo <w>leniwy</w> i nigdy nie sprząta swojego pokoju.",
                            "target_lang_translations": {
                                "ru": "Мой брат очень <w>ленивый</w> и никогда не убирает свою комнату."
                            }
                        },
                        {
                            "text": "Kot był tak <w>leniwy</w>, że Ewa musiała mu przynosić miskę z karmą.",
                            "target_lang_translations": {
                                "ru": "Кот был таким <w>ленивым</w>, что Еве приходилось приносить ему миску с кормом."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezczynny",
                        "gnuśny",
                        "leniuchowaty"
                    ],
                    "antonyms": [
                        "pracowity"
                    ],
                    "common_phrases": [
                        "leniwy uczeń",
                        "strasznie leniwy",
                        "zbyt leniwy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся нежеланием работать, прилагать физические или умственные усилия; любящий бездельничать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ленивый",
                                "target_lang_sense_clarification": "склонный к лени, избегающий труда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7a3256b-7a48-327f-8a05-5805ba155378",
                    "sense_definition": "Odbywający się powoli, bez pośpiechu; płynący lub poruszający się wolno.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzeka ma w tym miejscu szeroki i <w>leniwy</w> nurt.",
                            "target_lang_translations": {
                                "ru": "В этом месте у реки широкое и <w>ленивое</w> течение."
                            }
                        },
                        {
                            "text": "Spędziliśmy długie, <w>leniwe</w> popołudnie na plaży, odpoczywając od pracy.",
                            "target_lang_translations": {
                                "ru": "Мы провели долгий <w>ленивый</w> день на пляже, отдыхая от работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "powolny",
                        "opieszały",
                        "nieśpieszny",
                        "ospały",
                        "wolny",
                        "ślimaczy"
                    ],
                    "antonyms": [
                        "szybki"
                    ],
                    "common_phrases": [
                        "leniwy nurt",
                        "leniwe popołudnie",
                        "leniwy krok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий медленно, без спешки; текущий или движущийся медленно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ленивый",
                                "target_lang_sense_clarification": "медленный, неторопливый"
                            },
                            {
                                "target_lang_word": "неспешный",
                                "target_lang_sense_clarification": "происходящий без суеты"
                            },
                            {
                                "target_lang_word": "медлительный",
                                "target_lang_sense_clarification": "вялый, лишенный энергии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
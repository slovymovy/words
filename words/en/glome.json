    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "278dbc25-53fe-340f-a43e-34f3a56f8b8b",
                    "sense_definition": "A hypersphere in four-dimensional Euclidean space, defined as the set of points equidistant from a center point; a 3-sphere.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geometry_senses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mathematician attempted to visualize the properties of a <w>glome</w>.",
                            "target_lang_translations": {
                                "ru": "Математик пытался визуализировать свойства <w>3-сферы</w>."
                            }
                        },
                        {
                            "text": "In four-dimensional geometry, a <w>glome</w> is the analogue of a sphere.",
                            "target_lang_translations": {
                                "ru": "В четырехмерной геометрии <w>3-сфера</w> является аналогом сферы."
                            }
                        }
                    ],
                    "synonyms": [
                        "3-sphere",
                        "hypersphere"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гиперсфера в четырехмерном евклидовом пространстве; 3-сфера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "3-сфера",
                                "target_lang_sense_clarification": "математический термин"
                            },
                            {
                                "target_lang_word": "гиперсфера",
                                "target_lang_sense_clarification": "геометрия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de85b9dc-dba4-3c68-982c-37e8abb98f87",
                    "sense_definition": "A rounded or globular head of flowers.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany_senses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The botanist examined the dense <w>glome</w> of purple blossoms.",
                            "target_lang_translations": {
                                "ru": "Ботаник изучал плотный <w>клубочек</w> пурпурных цветов."
                            }
                        },
                        {
                            "text": "Each <w>glome</w> on the stem contains dozens of tiny individual flowers.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>клубочек</w> на стебле содержит десятки крошечных отдельных цветков."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Округлое или шаровидное соцветие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клубочек",
                                "target_lang_sense_clarification": "ботаника"
                            },
                            {
                                "target_lang_word": "головка",
                                "target_lang_sense_clarification": "форма соцветия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a18290b2-e7d3-3120-9c30-8c8758e9fbd6",
                    "sense_definition": "One of the two prominences at the back part of the frog in a horse's foot.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "veterinary_senses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farrier cleaned the area around the <w>glome</w> to prevent infection.",
                            "target_lang_translations": {
                                "ru": "Кузнец очистил область вокруг <w>луковицы пятки</w>, чтобы предотвратить инфекцию."
                            }
                        },
                        {
                            "text": "An injury to the <w>glome</w> can cause significant lameness in the animal.",
                            "target_lang_translations": {
                                "ru": "Травма <w>луковицы пятки</w> может вызвать сильную хромоту у животного."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из двух выпуклостей в задней части стрелки копыта лошади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "луковица пятки",
                                "target_lang_sense_clarification": "анатомия копыта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca1e4a7b-d5c9-3775-8838-60697ad23694",
                    "sense_definition": "A state of gloom or darkness.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "gloom_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A heavy <w>glome</w> settled over the forest as night approached.",
                            "target_lang_translations": {
                                "ru": "Тяжелый <w>мрак</w> опустился на лес с приближением ночи."
                            }
                        },
                        {
                            "text": "He sat in the <w>glome</w> of the library, barely able to read the text.",
                            "target_lang_translations": {
                                "ru": "Он сидел во <w>мраке</w> библиотеки, едва имея возможность читать текст."
                            }
                        }
                    ],
                    "synonyms": [
                        "gloom",
                        "darkness"
                    ],
                    "antonyms": [
                        "brightness"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние мрака или темноты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мрак",
                                "target_lang_sense_clarification": "темнота"
                            },
                            {
                                "target_lang_word": "уныние",
                                "target_lang_sense_clarification": "настроение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5f94fe32-adbc-36b0-a07c-94d8aec5e751",
                    "sense_definition": "To look gloomy, morose, or sullen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "gloom_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do not <w>glome</w> at the festivities while everyone else is celebrating.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>хмуриться</w> на празднике, пока все остальные веселятся."
                            }
                        },
                        {
                            "text": "She ceased to <w>glome</w> once the cheerful music began to play.",
                            "target_lang_translations": {
                                "ru": "Она перестала <w>смотреть угрюмо</w>, как только заиграла веселая музыка."
                            }
                        }
                    ],
                    "synonyms": [
                        "frown",
                        "scowl",
                        "glower"
                    ],
                    "antonyms": [
                        "smile",
                        "beam"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выглядеть мрачным, угрюмым или сердитым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хмуриться",
                                "target_lang_sense_clarification": "выражение лица"
                            },
                            {
                                "target_lang_word": "смотреть угрюмо",
                                "target_lang_sense_clarification": "взгляд"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "klezmer",
        "klezmerski"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5e6a91fd-48d7-331b-919a-29dc8fa6803b",
                    "sense_definition": "Kobieta, która zarobkowo gra na instrumentach na weselach, w restauracjach lub na zabawach (żeńska forma słowa <w>klezmer</w>).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "musicians",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta utalentowana <w>klezmerka</w> potrafiła rozbawić gości weselnych do białego rana.",
                            "target_lang_translations": {
                                "ru": "Эта талантливая <w>музыкантша</w> умела развлекать свадебных гостей до самого утра."
                            }
                        },
                        {
                            "text": "Jako skrzypaczka i <w>klezmerka</w>, znała na pamięć setki popularnych melodii.",
                            "target_lang_translations": {
                                "ru": "Как скрипачка и <w>ресторанный музыкант</w>, она знала наизусть сотни популярных мелодий."
                            }
                        }
                    ],
                    "synonyms": [
                        "muzykantka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, которая зарабатывает игрой на музыкальных инструментах на свадьбах, в ресторанах или на вечеринках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "музыкантша",
                                "target_lang_sense_clarification": "играющая на мероприятиях (разг.)"
                            },
                            {
                                "target_lang_word": "ресторанная певица",
                                "target_lang_sense_clarification": "или музыкант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5721a995-a622-30e2-a33b-4549a30e5195",
                    "sense_definition": "Potoczne określenie pracy zarobkowej polegającej na graniu muzyki na imprezach okolicznościowych, takich jak wesela czy bankiety.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "music_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lecie muzyk często trudnił się <w>klezmerką</w> na wiejskich weselach, by dorobić do pensji.",
                            "target_lang_translations": {
                                "ru": "Летом музыкант часто занимался <w>халтурой</w> на деревенских свадьбах, чтобы подзаработать к зарплате."
                            }
                        },
                        {
                            "text": "Choć studiował muzykę klasyczną, w weekendy żył z <w>klezmerki</w>.",
                            "target_lang_translations": {
                                "ru": "Хотя он изучал классическую музыку, по выходным он жил за счет <w>игры на свадьбах</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "chałtura",
                        "granie"
                    ],
                    "common_phrases": [
                        "trudnić się klezmerką",
                        "żyć z klezmerki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название работы, заключающейся в игре музыки на торжественных мероприятиях, таких как свадьбы или банкеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "халтура",
                                "target_lang_sense_clarification": "музыкальная подработка (разг.)"
                            },
                            {
                                "target_lang_word": "игра на свадьбах",
                                "target_lang_sense_clarification": "вид заработка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
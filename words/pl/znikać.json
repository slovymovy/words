    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "24c6dbd1-d2b7-3704-96ac-b422f26bec0e",
                    "sense_definition": "Stawać się niewidocznym dla oka; przestawać być dostrzegalnym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "disappearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słońce powoli <znikało> za horyzontem, kończąc długi dzień.",
                            "target_lang_translations": {
                                "ru": "Солнце медленно <исчезало> за горизонтом, завершая долгий день."
                            }
                        },
                        {
                            "text": "Statek zaczął <znikać> we mgle, aż w końcu całkiem go straciliśmy z oczu.",
                            "target_lang_translations": {
                                "ru": "Корабль начал <исчезать> в тумане, пока мы окончательно не потеряли его из виду."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpływać się"
                    ],
                    "antonyms": [
                        "pojawiać się"
                    ],
                    "common_phrases": [
                        "znikać z oczu",
                        "znikać we mgle",
                        "znikać w tłumie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться невидимым для глаз; переставать быть заметным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исчезать",
                                "target_lang_sense_clarification": "становиться невидимым"
                            },
                            {
                                "target_lang_word": "скрываться",
                                "target_lang_sense_clarification": "уходить из вида"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f02d1e0-b15b-380b-b50b-69ad23537052",
                    "sense_definition": "Przestawać być obecnym w danym miejscu, gubić się lub przestawać istnieć.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "disappearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój portfel ciągle gdzieś <znika> i nie mogę go znaleźć.",
                            "target_lang_translations": {
                                "ru": "Мой кошелек постоянно где-то <пропадает>, и я не могу его найти."
                            }
                        },
                        {
                            "text": "Ten gatunek zwierząt powoli <znika> z powierzchni ziemi z powodu zmian klimatu.",
                            "target_lang_translations": {
                                "ru": "Этот вид животных медленно <исчезает> с лица земли из-за изменения климата."
                            }
                        }
                    ],
                    "synonyms": [
                        "przepadać",
                        "gubić się"
                    ],
                    "antonyms": [
                        "pojawiać się"
                    ],
                    "common_phrases": [
                        "znikać bez śladu",
                        "nagle znikać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать присутствовать в данном месте, теряться или прекращать существование."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропадать",
                                "target_lang_sense_clarification": "теряться"
                            },
                            {
                                "target_lang_word": "исчезать",
                                "target_lang_sense_clarification": "прекращать существование"
                            }
                        ]
                    }
                }
            ]
        }
    ]

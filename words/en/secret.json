                {
                    "sense_id": "88413749-236c-3a3d-b7af-49d8f165e628",
                    "sense_definition": "Being kept hidden from knowledge or view; not known by others.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hidden_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We found a <w>secret</w> passage behind the bookshelf.",

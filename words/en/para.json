                {
                    "sense_id": "0eb65c7b-3dee-3f66-b2fc-0b681a8cfa21",
                    "sense_definition": "A soldier trained to be dropped from an aircraft by parachute; a clipping of <w>paratrooper</w>.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "military_personnel",
                    "name_type": "no",

                {
                    "sense_id": "22759127-c22e-318e-a6ac-19b51ae9ece6",
                    "sense_definition": "Verdergaan met zwemmen zonder te stoppen, vaak om een bepaald doel te bereiken of gedurende een bepaalde tijd.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "continue_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als we nog een klein stukje <w>doorzwemmen</w>, zijn we bij de overkant.",
                            "target_lang_translations": {
                                "en": "If we <w>swim on</w> a little further, we'll be at the other side.",
                                "ru": "Если мы <w>проплывем</w> еще немного, то окажемся на другом берегу."
                            }
                        },
                        {
                            "text": "Ondanks de kou is hij gewoon <w>doorgezwommen</w> tot het einde.",
                            "target_lang_translations": {
                                "en": "Despite the cold, he just <w>kept swimming</w> until the end.",
                                "ru": "Несмотря на холод, он просто <w>продолжал плыть</w> до самого конца."
                            }
                        }
                    ],
                    "synonyms": [
                        "verder zwemmen"
                    ],
                    "antonyms": [
                        "stoppen",
                        "ophouden"
                    ],
                    "common_phrases": [
                        "gewoon doorzwemmen",
                        "stug doorzwemmen",
                        "even doorzwemmen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "separable verb (door + zwemmen)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To continue swimming without stopping, often to reach a goal or for a certain duration.",
                        "ru": "Продолжать плыть, не останавливаясь, часто для достижения определенной цели или в течение определенного времени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swim on",
                                "target_lang_sense_clarification": "continue swimming"
                            },
                            {
                                "target_lang_word": "keep swimming",
                                "target_lang_sense_clarification": "continue action"
                            },
                            {
                                "target_lang_word": "continue swimming",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {

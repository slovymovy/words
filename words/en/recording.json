{
    "word_family": [
        "record",
        "recorder"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ccdb358-b1d9-3d98-a15d-1efa9ae207a9",
                    "sense_definition": "A piece of music, speech, or video that has been stored on a tape, disc, or digital file.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I listened to a <w>recording</w> of the speech to understand the details.",
                            "target_lang_translations": {
                                "ru": "Я прослушал <w>запись</w> речи, чтобы понять детали."
                            }
                        },
                        {
                            "text": "The band released a new <w>recording</w> of their most popular song.",
                            "target_lang_translations": {
                                "ru": "Группа выпустила новую <w>запись</w> своей самой популярной песни."
                            }
                        }
                    ],
                    "synonyms": [
                        "tape",
                        "track",
                        "video",
                        "copy"
                    ],
                    "common_phrases": [
                        "make a recording",
                        "audio recording",
                        "digital recording",
                        "live recording"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальное произведение, речь или видео, сохраненные на пленке, диске или в цифровом файле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запись",
                                "target_lang_sense_clarification": "результат: трек, видео, файл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ca97bf6-a0e9-3372-82d2-af9f5bb4281a",
                    "sense_definition": "The process or action of capturing sound, images, or data to store them permanently.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>recording</w> of the concert took several hours to complete.",
                            "target_lang_translations": {
                                "ru": "<w>Запись</w> концерта заняла несколько часов."
                            }
                        },
                        {
                            "text": "Illegal <w>recording</w> of movies in the cinema is strictly prohibited.",
                            "target_lang_translations": {
                                "ru": "Незаконная <w>запись</w> фильмов в кинотеатре строго запрещена."
                            }
                        }
                    ],
                    "synonyms": [
                        "capturing",
                        "taping",
                        "filming"
                    ],
                    "common_phrases": [
                        "start recording",
                        "stop recording",
                        "during the recording"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс или действие по захвату звука, изображений или данных для их постоянного хранения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запись",
                                "target_lang_sense_clarification": "процесс сохранения"
                            },
                            {
                                "target_lang_word": "съёмка",
                                "target_lang_sense_clarification": "видеозапись"
                            },
                            {
                                "target_lang_word": "звукозапись",
                                "target_lang_sense_clarification": "аудио"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c5bdfaa-5b1e-3539-ad4c-a1631eaf8c88",
                    "sense_definition": "The act of officially writing down information or entering it into a system or register.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Accurate <w>recording</w> of financial data is essential for the company.",
                            "target_lang_translations": {
                                "ru": "Точная <w>фиксация</w> финансовых данных необходима для компании."
                            }
                        },
                        {
                            "text": "The <w>recording</w> of votes continued late into the night.",
                            "target_lang_translations": {
                                "ru": "<w>Регистрация</w> голосов продолжалась до поздней ночи."
                            }
                        }
                    ],
                    "synonyms": [
                        "registration",
                        "logging",
                        "entry",
                        "documentation"
                    ],
                    "common_phrases": [
                        "data recording",
                        "recording of deeds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по официальному письменному фиксированию информации или внесению её в систему или реестр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регистрация",
                                "target_lang_sense_clarification": "внесение в реестр"
                            },
                            {
                                "target_lang_word": "фиксация",
                                "target_lang_sense_clarification": "документирование данных"
                            },
                            {
                                "target_lang_word": "учёт",
                                "target_lang_sense_clarification": "ведение записей"
                            }
                        ]
                    }
                }
            ]
        },
        {

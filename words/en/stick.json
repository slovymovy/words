    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6b4c244e-d07c-3d18-a740-1e5f77bddacd",
                    "sense_definition": "To become or remain attached to a surface or object.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "adhere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tape will not <w>stick</w> if the surface is wet.",
                            "target_lang_translations": {
                                "ru": "Лента не <w>приклеится</w>, если поверхность влажная."
                            }
                        },
                        {
                            "text": "Mud tends to <w>stick</w> to your boots when you walk in the rain.",
                            "target_lang_translations": {
                                "ru": "Грязь имеет обыкновение <w>налипать</w> на ботинки, когда гуляешь под дождем."
                            }
                        }
                    ],
                    "synonyms": [
                        "adhere",
                        "cling",
                        "attach",
                        "cleave"
                    ],
                    "antonyms": [
                        "unstick"
                    ],
                    "common_phrases": [
                        "stick together",
                        "stick like glue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прилипать или оставаться прикрепленным к поверхности или объекту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прилипать",
                                "target_lang_sense_clarification": "приставать к поверхности"
                            },
                            {
                                "target_lang_word": "приклеиваться",
                                "target_lang_sense_clarification": "скрепляться клеем или липким веществом"
                            },
                            {
                                "target_lang_word": "липнуть",
                                "target_lang_sense_clarification": "быть липким, приставать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cddc5080-a1fb-3963-a3c7-4ce5992f932e",
                    "sense_definition": "To press a sharp pointed object into something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pierce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to <w>stick</w> your finger with the needle.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, не <w>уколи</w> палец иглой."
                            }
                        },
                        {
                            "text": "He <w>stuck</w> the fork into the potato to check if it was cooked.",
                            "target_lang_translations": {
                                "ru": "Он <w>воткнул</w> вилку в картофелину, чтобы проверить, сварилась ли она."
                            }
                        }
                    ],
                    "synonyms": [
                        "pierce",
                        "stab",
                        "puncture",
                        "prick"
                    ],
                    "common_phrases": [
                        "stick a pin in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Втыкать острый предмет во что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "втыкать",
                                "target_lang_sense_clarification": "помещать внутрь с силой"
                            },
                            {
                                "target_lang_word": "колоть",
                                "target_lang_sense_clarification": "повреждать острием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "177116f6-76e3-33b0-bee1-bb2bc05f9a4f",
                    "sense_definition": "To put something in a particular place, often quickly or carelessly.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Just <w>stick</w> your bag over there in the corner.",
                            "target_lang_translations": {
                                "ru": "Просто <w>брось</w> свою сумку вон там в углу."
                            }
                        },
                        {
                            "text": "She <w>stuck</w> the note in her pocket and walked away.",
                            "target_lang_translations": {
                                "ru": "Она <w>сунула</w> записку в карман и ушла."
                            }
                        }
                    ],
                    "synonyms": [
                        "put",
                        "place",
                        "shove",
                        "set down"
                    ],
                    "common_phrases": [
                        "stick it there"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Common in informal speech."

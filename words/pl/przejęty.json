                    "sense_id": "6ae844b8-d70d-34ca-8a7d-2a9aa62dee22",
                    "sense_definition": "odczuwający silne emocje, takie jak troska, lęk, wzruszenie lub głębokie zainteresowanie; mocno skupiony na czymś",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat był bardzo <w>przejęty</w> swoją nową pracą w dużej firmie.",
                            "target_lang_translations": {
                                "ru": "Мой брат был очень <w>взволнован</w> своей новой работой в крупной фирме."
                            }
                        },
                        {
                            "text": "Wszyscy byliśmy głęboko <w>przejęci</w> losem bezdomnych zwierząt w schronisku.",
                            "target_lang_translations": {
                                "ru": "Мы все были глубоко <w>обеспокоены</w> судьбой бездомных животных в приюте."
                            }
                        }
                    ],
                    "synonyms": [
                        "poruszony",
                        "wzruszony",
                        "zaniepokojony",
                        "zaabsorbowany",
                        "pochłonięty"
                    ],
                    "antonyms": [
                        "obojętny",
                        "niewzruszony"
                    ],
                    "common_phrases": [

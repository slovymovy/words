    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "dea5a60b-72a0-356e-a776-4fd380af9d90",
                    "sense_definition": "Ready to do something, particularly something that requires change or effort; not objecting.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>willing</w> to help us with the project if we ask her nicely.",
                            "target_lang_translations": {
                                "ru": "Она <w>готова</w> помочь нам с проектом, если мы её вежливо попросим."
                            }
                        },
                        {
                            "text": "Are you <w>willing</w> to travel abroad for this job?",
                            "target_lang_translations": {
                                "ru": "Вы <w>согласны</w> поехать за границу ради этой работы?"
                            }
                        }
                    ],
                    "synonyms": [
                        "agreeable",
                        "consenting",
                        "voluntary"
                    ],
                    "antonyms": [
                        "unwilling",
                        "reluctant"
                    ],
                    "common_phrases": [
                        "willing to help",
                        "willing to learn",
                        "willing and able",
                        "God willing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовый сделать что-либо, особенно если это требует усилий; не возражающий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовый",
                                "target_lang_sense_clarification": "имеющий намерение или желание сделать что-то"
                            },
                            {
                                "target_lang_word": "согласный",
                                "target_lang_sense_clarification": "выражающий согласие"
                            },
                            {
                                "target_lang_word": "охотный",
                                "target_lang_sense_clarification": "делаемый с желанием (обычно об участии или помощи)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2101505c-26b2-355b-8df8-9fd497049170",
                    "sense_definition": "Present participle and gerund of <w>will</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is currently <w>willing</w> his entire estate to his favorite charity.",
                            "target_lang_translations": {
                                "ru": "В данный момент он <w>завещает</w> всё своё состояние любимой благотворительной организации."
                            }
                        },
                        {
                            "text": "She sat there silently, <w>willing</w> the phone to ring.",
                            "target_lang_translations": {
                                "ru": "Она сидела молча, <w>заклиная</w> телефон зазвонить."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола will (завещать; заставлять силой воли)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завещающий",
                                "target_lang_sense_clarification": "оставляющий наследство"
                            },
                            {
                                "target_lang_word": "заставляющий силой воли",
                                "target_lang_sense_clarification": "использующий ментальные усилия для влияния на события"
                            }
                        ]
                    }
                }
            ]
        },
        {

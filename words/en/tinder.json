                {
                    "sense_id": "86717d4d-f44a-3919-b35e-db9a9730fbde",
                    "sense_definition": "Small dry sticks, fibers, or other flammable materials used to help light a fire.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gathered some dry <w>tinder</w> from the forest floor to start our campfire.",
                            "target_lang_translations": {
                                "ru": "Мы собрали в лесу сухую <w>растопку</w>, чтобы разжечь костер."
                            }
                        },
                        {
                            "text": "Without proper <w>tinder</w>, the sparks from the flint failed to catch.",
                            "target_lang_translations": {
                                "ru": "Без хорошего <w>трута</w> искры от кремня не могли разгореться."
                            }
                        }
                    ],
                    "synonyms": [
                        "kindling"
                    ],
                    "common_phrases": [
                        "dry tinder",
                        "tinder and flint"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легковоспламеняющийся материал (сухие ветки, трут), используемый для разжигания огня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трут",
                                "target_lang_sense_clarification": "материал для высекания огня"
                            },
                            {
                                "target_lang_word": "растопка",
                                "target_lang_sense_clarification": "материал для разжигания костра"
                            }
                        ]

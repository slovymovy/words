                {
                    "sense_id": "b59d0362-1835-38c4-8b5b-d906973f87bc",
                    "sense_definition": "A country in Southeast Europe. Capital and largest city: Bucharest.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We are planning a trip to <w>Romania</w> next summer.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку в <w>Румынию</w> следующим летом."
                            }
                        },
                        {
                            "text": "Bucharest is the capital and largest city of <w>Romania</w>.",
                            "target_lang_translations": {
                                "ru": "Бухарест — столица и крупнейший город <w>Румынии</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "visit Romania",
                        "capital of Romania",
                        "history of Romania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страна в Юго-Восточной Европе. Столица и крупнейший город: Бухарест."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Румыния",
                                "target_lang_sense_clarification": "страна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2df14aa0-3227-3d31-9bf5-7eb8c0b83640",
                    "sense_definition": "A historical name for the Byzantine Empire or its territories.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The Latin conquest resulted in the political division of <w>Romania</w>.",
                            "target_lang_translations": {
                                "ru": "Латинское завоевание привело к политическому разделу <w>Романии</w>."
                            }
                        },
                        {
                            "text": "Medieval historians often referred to the Eastern Empire as <w>Romania</w>.",
                            "target_lang_translations": {
                                "ru": "Средневековые историки часто называли Восточную империю <w>Романией</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Byzantine Empire"
                    ],
                    "common_phrases": [
                        "Empire of Romania"
                    ],
                    "target_lang_definitions": {

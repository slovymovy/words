                    "sense_id": "457931e2-1698-3054-8d9e-5e72b164f36e",
                    "sense_definition": "Uitwerpselen van mensen of dieren; de vaste stof die het lichaam verlaat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "excrement",
                    "name_type": "no",

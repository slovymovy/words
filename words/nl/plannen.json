    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4788e003-41b6-3fee-a527-94a50d919013",
                    "sense_definition": "Het van tevoren bedenken en organiseren hoe iets moet gebeuren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten onze vakantie goed <w>plannen</w> om alles te kunnen zien.",
                            "target_lang_translations": {
                                "en": "We need to <w>plan</w> our vacation well to be able to see everything.",
                                "ru": "Нам нужно хорошо <w>спланировать</w> наш отпуск, чтобы всё увидеть."
                            }
                        },
                        {
                            "text": "Zij heeft het hele evenement tot in de puntjes <w>gepland</w>.",
                            "target_lang_translations": {
                                "en": "She has <w>planned</w> the entire event down to the last detail.",
                                "ru": "Она <w>спланировала</w> всё мероприятие до мельчайших деталей."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorbereiden",
                        "uitstippelen"
                    ],
                    "common_phrases": [
                        "een reis plannen",
                        "alles zorgvuldig plannen",
                        "vooruit plannen"
                    ],
                    "target_lang_definitions": {
                        "en": "To think about and organize in advance how something should happen.",
                        "ru": "Заранее обдумывать и организовывать, как что-то должно произойти."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to plan",
                                "target_lang_sense_clarification": "general action of organizing"
                            },
                            {
                                "target_lang_word": "to organize",
                                "target_lang_sense_clarification": "to arrange or structure"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "планировать",
                                "target_lang_sense_clarification": "общее действие по составлению плана"
                            },
                            {
                                "target_lang_word": "спланировать",
                                "target_lang_sense_clarification": "завершённое действие"
                            },
                            {
                                "target_lang_word": "организовывать",
                                "target_lang_sense_clarification": "устраивать или подготавливать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73903516-dd20-3715-9838-549441fde826",
                    "sense_definition": "Een tijdstip vastleggen voor een activiteit of afspraak in een agenda of rooster.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kunnen we volgende week een moment <w>plannen</w> om de verhuizing te bespreken?",
                            "target_lang_translations": {
                                "en": "Can we <w>schedule</w> a time next week to discuss the move?",
                                "ru": "Можем ли мы <w>запланировать</w> время на следующей неделе, чтобы обсудить переезд?"
                            }
                        },
                        {
                            "text": "De tandarts moet een nieuwe afspraak voor mij <w>plannen</w> in zijn agenda.",
                            "target_lang_translations": {
                                "en": "The dentist needs to <w>schedule</w> a new appointment for me in his calendar.",
                                "ru": "Стоматолог должен <w>запланировать</w> для меня новый прием в своем расписании."
                            }
                        }
                    ],
                    "synonyms": [
                        "inroosteren"
                    ],
                    "common_phrases": [
                        "een afspraak plannen",
                        "een vergadering plannen"
                    ],
                    "target_lang_definitions": {
                        "en": "To fix a time for an activity or appointment in a calendar or schedule.",
                        "ru": "Назначать время для мероприятия или встречи в календаре или расписании."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to schedule",
                                "target_lang_sense_clarification": "to set a specific time"
                            },
                            {
                                "target_lang_word": "to book",
                                "target_lang_sense_clarification": "to reserve a time slot"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "запланировать",
                                "target_lang_sense_clarification": "внести в планы или график"
                            },
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "устанавливать время встречи"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

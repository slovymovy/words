    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a4486d71-91af-3d26-8c41-a0e7176ad2cf",
                    "sense_definition": "Распространённое млекопитающее отряда грызунов с длинным хвостом, часто считающееся вредителем.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кошка поймала в подвале большую серую <w>крысу</w>.",
                            "target_lang_translations": {
                                "en": "The cat caught a big grey <w>rat</w> in the basement."
                            }
                        },
                        {
                            "text": "Учёные часто используют белых <w>крыс</w> для проведения лабораторных опытов.",
                            "target_lang_translations": {
                                "en": "Scientists often use white <w>rats</w> for conducting laboratory experiments."
                            }
                        }
                    ],
                    "common_phrases": [
                        "водяная крыса",
                        "белая крыса"
                    ],
                    "target_lang_definitions": {
                        "en": "A common rodent mammal with a long tail, often considered a pest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rat",
                                "target_lang_sense_clarification": "animal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f12d873-98aa-3fe5-a68b-0ef390b1f795",
                    "sense_definition": "Неприятный, мелочный или вредный человек (часто употребляется по отношению к бюрократам).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphor_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот чиновник — типичная канцелярская <w>крыса</w>, которая требует бесконечные справки.",
                            "target_lang_translations": {
                                "en": "This official is a typical office <w>rat</w> who demands endless certificates."
                            }
                        },
                        {
                            "text": "Не будь такой <w>крысой</w>, одолжи мне немного денег до зарплаты.",
                            "target_lang_translations": {
                                "en": "Don't be such a <w>rat</w>, lend me some money until payday."
                            }
                        }
                    ],
                    "common_phrases": [
                        "канцелярская крыса"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "презрительное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An unpleasant, petty, or harmful person (often used regarding bureaucrats)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rat",
                                "target_lang_sense_clarification": "unpleasant person"
                            },
                            {
                                "target_lang_word": "scoundrel",
                                "target_lang_sense_clarification": "dishonest person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ce39673-24d0-3362-b4ed-0f1514bacd62",
                    "sense_definition": "Человек, который крадёт у своих, предатель или доносчик.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphor_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем коллективе завелась <w>крыса</w>, которая доносит начальству обо всех опозданиях.",
                            "target_lang_translations": {
                                "en": "A <w>rat</w> has appeared in our team who reports all lateness to the management."
                            }
                        },
                        {
                            "text": "Никто не хотел общаться с ним, так как все считали его <w>крысой</w>.",
                            "target_lang_translations": {
                                "en": "No one wanted to talk to him, as everyone considered him a <w>rat</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ты крыса"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "криминальное или молодежное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who steals from their own group, a traitor, or an informer."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rat",
                                "target_lang_sense_clarification": "traitor"
                            },
                            {
                                "target_lang_word": "snitch",
                                "target_lang_sense_clarification": "informer"
                            },
                            {
                                "target_lang_word": "traitor",
                                "target_lang_sense_clarification": "betrayer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98e52690-d5b2-3801-8517-3901742be063",
                    "sense_definition": "Игрок в компьютерных играх, который прячется в укрытии и поджидает противника, избегая открытого боя.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не люблю играть с ним, потому что он вечно сидит в углу как <w>крыса</w>.",
                            "target_lang_translations": {
                                "en": "I don't like playing with him because he always sits in the corner like a <w>rat</w>."
                            }
                        },
                        {
                            "text": "В этой игре много <w>крыс</w>, которые стреляют в спину из засады.",
                            "target_lang_translations": {
                                "en": "There are many <w>rats</w> in this game who shoot in the back from ambush."
                            }
                        }
                    ],
                    "synonyms": [
                        "кемпер"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "компьютерное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A player in computer games who hides in cover and waits for an enemy, avoiding open combat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "camper",
                                "target_lang_sense_clarification": "gaming strategy"
                            },
                            {
                                "target_lang_word": "rat",
                                "target_lang_sense_clarification": "gaming slang"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8c69615-3431-354c-bf20-61d8e5c9f2f5",
                    "sense_definition": "Компьютерная мышь (устройство ввода).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "tech_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня сломалась <w>крыса</w>, курсор перестал двигаться по экрану.",
                            "target_lang_translations": {
                                "en": "My <w>mouse</w> broke, the cursor stopped moving on the screen."
                            }
                        },
                        {
                            "text": "Для игр лучше купить хорошую проводную <w>крысу</w> с высокой чувствительностью.",
                            "target_lang_translations": {
                                "en": "For games, it is better to buy a good wired <w>mouse</w> with high sensitivity."
                            }
                        }
                    ],
                    "synonyms": [
                        "мышь"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "компьютерное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A computer mouse (input device)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mouse",
                                "target_lang_sense_clarification": "computer device"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37876d1e-4070-3413-9c0e-5e573ecedb3a",
                    "sense_definition": "Электрический удлинитель с несколькими розетками.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tech_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Под столом лежит <w>крыса</w>, в которую включены компьютер и монитор.",
                            "target_lang_translations": {
                                "en": "There is a <w>power strip</w> lying under the table, into which the computer and monitor are plugged."
                            }
                        },
                        {
                            "text": "Нам не хватает розеток, нужно принести из соседней комнаты ещё одну <w>крысу</w>.",
                            "target_lang_translations": {
                                "en": "We don't have enough sockets, we need to bring another <w>power strip</w> from the next room."
                            }
                        }
                    ],
                    "synonyms": [
                        "пилот"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "техническое"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An electrical extension cord with multiple sockets."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "power strip",
                                "target_lang_sense_clarification": "electrical device"
                            },
                            {
                                "target_lang_word": "extension cord",
                                "target_lang_sense_clarification": "cabling"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "narrow",
        "narrowness"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e1da6940-7530-3f1e-ba59-ab7f21407d7d",
                    "sense_definition": "By only a very small amount or margin; barely.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The driver <w>narrowly</w> escaped injury when the car skidded off the road.",
                            "target_lang_translations": {
                                "ru": "Водитель <w>чудом</w> избежал травм, когда машина вылетела с дороги."
                            }
                        },
                        {
                            "text": "The visiting team <w>narrowly</w> missed scoring a goal in the final minute.",
                            "target_lang_translations": {
                                "ru": "Команда гостей <w>едва</w> не забила гол на последней минуте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "narrowly escaped",
                        "narrowly missed",
                        "narrowly avoided",
                        "narrowly defeated",
                        "narrowly won"
                    ],
                    "target_lang_definitions": {
                        "ru": "С очень небольшим отрывом или перевесом; едва, на волосок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "едва",
                                "target_lang_sense_clarification": "с трудом, еле-еле"
                            },
                            {
                                "target_lang_word": "чудом",
                                "target_lang_sense_clarification": "удивительным образом (часто об избежании опасности)"
                            },
                            {
                                "target_lang_word": "чуть",
                                "target_lang_sense_clarification": "немного (часто в значении «чуть не»)"
                            },
                            {
                                "target_lang_word": "с небольшим перевесом",
                                "target_lang_sense_clarification": "о победе или поражении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76e62c90-d668-3fbe-ac82-a29cc08b3d83",
                    "sense_definition": "In a limited way that lacks breadth, flexibility, or scope.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new regulations were <w>narrowly</w> defined to apply only to large corporations.",
                            "target_lang_translations": {
                                "ru": "Новые правила были <w>узко</w> сформулированы и применялись только к крупным корпорациям."
                            }
                        },
                        {
                            "text": "He <w>narrowly</w> focused his research on the economic impact of the war.",
                            "target_lang_translations": {
                                "ru": "Он <w>узко</w> сфокусировал свое исследование на экономических последствиях войны."
                            }
                        }
                    ],
                    "antonyms": [
                        "widely"
                    ],
                    "common_phrases": [
                        "narrowly defined",
                        "narrowly focused",
                        "narrowly interpreted",
                        "narrowly based"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограниченно; в узком смысле, без широкого охвата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узко",
                                "target_lang_sense_clarification": "в ограниченном смысле"
                            },
                            {
                                "target_lang_word": "ограниченно",
                                "target_lang_sense_clarification": "в узких рамках"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
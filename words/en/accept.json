{
    "word_family": [
        "acceptance",
        "acceptable",
        "unacceptable",
        "acceptability",
        "acceptor"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dedccb3e-1ebd-3003-bd90-400ab05e2a87",
                    "sense_definition": "To willingly take or receive something that is offered.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "receive_take",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to <w>accept</w> the gift with a warm smile.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>принять</w> подарок с теплой улыбкой."
                            }
                        },
                        {
                            "text": "Please <w>accept</w> this small token of our appreciation for your hard work.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>примите</w> этот небольшой знак нашей благодарности за ваш усердный труд."
                            }
                        }
                    ],
                    "synonyms": [
                        "receive",
                        "take"
                    ],
                    "antonyms": [
                        "reject",
                        "decline"
                    ],
                    "common_phrases": [
                        "accept a gift",
                        "accept an award",
                        "accept an apology"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать или получать то, что предлагают (подарок, помощь и т.д.) добровольно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "брать то, что дают"
                            },
                            {
                                "target_lang_word": "принять",
                                "target_lang_sense_clarification": "совершенный вид к «принимать»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8dce10b-fe0b-3a0d-994f-ca4ebb390e61",
                    "sense_definition": "To say yes to an offer, invitation, or proposal.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "agree_consent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was very happy to <w>accept</w> the job offer immediately.",
                            "target_lang_translations": {
                                "ru": "Я был очень рад немедленно <w>принять</w> предложение о работе."
                            }
                        },
                        {
                            "text": "They did not <w>accept</w> the invitation to the wedding because they were busy.",
                            "target_lang_translations": {
                                "ru": "Они не <w>приняли</w> приглашение на свадьбу, потому что были заняты."
                            }
                        }
                    ],
                    "antonyms": [
                        "decline",
                        "reject"
                    ],
                    "common_phrases": [
                        "accept an offer",
                        "accept an invitation",
                        "accept a challenge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать согласие на предложение, приглашение или просьбу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "соглашаться на предложение"
                            },
                            {
                                "target_lang_word": "соглашаться",
                                "target_lang_sense_clarification": "отвечать утвердительно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f09a09a1-8fc9-36d6-922f-5129026384fb",
                    "sense_definition": "To consider something as true, valid, or proper.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "believe_valid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>accept</w> responsibility for your own actions and mistakes.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>принять</w> ответственность за свои действия и ошибки."
                            }
                        },
                        {
                            "text": "The police refused to <w>accept</w> his version of the story without proof.",
                            "target_lang_translations": {
                                "ru": "Полиция отказалась <w>принять</w> его версию событий без доказательств."
                            }
                        }
                    ],
                    "synonyms": [
                        "believe",
                        "acknowledge"
                    ],
                    "antonyms": [
                        "reject"
                    ],
                    "common_phrases": [
                        "accept the truth",
                        "accept responsibility",
                        "generally accepted"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать что-либо истинным, действительным или правильным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признавать",
                                "target_lang_sense_clarification": "считать действительным или верным"
                            },
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "соглашаться с чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b38ae5c-e7ae-3c9f-8261-1a210c73c7e5",
                    "sense_definition": "To agree to take a specific form of payment.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "receive_take",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most shops in the city center <w>accept</w> credit cards.",
                            "target_lang_translations": {
                                "ru": "Большинство магазинов в центре города <w>принимают</w> кредитные карты."
                            }
                        },
                        {
                            "text": "We do not <w>accept</w> checks at this store, only cash.",
                            "target_lang_translations": {
                                "ru": "Мы не <w>принимаем</w> чеки в этом магазине, только наличные."
                            }
                        }
                    ],
                    "common_phrases": [
                        "accept credit cards",
                        "accept cash",
                        "accept payment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соглашаться брать определенный вид оплаты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "брать в качестве оплаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83ee4590-9559-3a6c-9860-e4e5c5e63c01",
                    "sense_definition": "To allow someone to become a member of a group or institution.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "admit_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university will <w>accept</w> students from all over the world next year.",
                            "target_lang_translations": {
                                "ru": "В следующем году университет будет <w>принимать</w> студентов со всего мира."
                            }
                        },
                        {
                            "text": "She was proud to be <w>accepted</w> into the prestigious club.",
                            "target_lang_translations": {
                                "ru": "Она гордилась тем, что её <w>приняли</w> в престижный клуб."
                            }
                        }
                    ],
                    "synonyms": [
                        "admit"
                    ],
                    "antonyms": [
                        "reject"
                    ],
                    "common_phrases": [
                        "accept into college",
                        "accept as a member"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зачислять кого-либо в состав группы, клуба или учебного заведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "включать в состав"
                            },
                            {
                                "target_lang_word": "зачислять",
                                "target_lang_sense_clarification": "официально включать в списки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bef04f42-33da-3978-b244-f920cafb18da",
                    "sense_definition": "To endure a difficult situation without attempting to change it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "endure_tolerate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had to <w>accept</w> the fact that he could not win the race.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось <w>смириться</w> с тем фактом, что он не сможет выиграть гонку."
                            }
                        },
                        {
                            "text": "It is hard to <w>accept</w> change when you are used to the old ways.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>принять</w> перемены, когда привык к старому укладу."
                            }
                        }
                    ],
                    "synonyms": [
                        "tolerate",
                        "endure"
                    ],
                    "common_phrases": [
                        "accept one's fate",
                        "accept the consequences",
                        "accept the fact"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мириться с трудной ситуацией, не пытаясь её изменить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смиряться",
                                "target_lang_sense_clarification": "перестать сопротивляться неизбежному"
                            },
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "признавать реальность ситуации"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c9ba8702-99b7-3ac0-9088-5967c4a92e8b",
                    "sense_definition": "Something that is accepted, often used in computing or technical contexts to denote a valid input.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The system logs all <w>accepts</w> and rejects for security analysis.",
                            "target_lang_translations": {
                                "ru": "Система регистрирует все <w>подтверждения</w> и отказы для анализа безопасности."
                            }
                        },
                        {
                            "text": "Almost all line segments will be trivial <w>accepts</w> in this algorithm.",
                            "target_lang_translations": {
                                "ru": "Почти все отрезки будут тривиальными <w>принятиями</w> в этом алгоритме."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принятый запрос или подтверждение (в техническом контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принятие",
                                "target_lang_sense_clarification": "действие по значению гл. принимать"
                            },
                            {
                                "target_lang_word": "подтверждение",
                                "target_lang_sense_clarification": "одобрение запроса"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1f9d8242-4d79-3de5-b535-19ec7988adb9",
                    "sense_definition": "Accepted.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pass our <w>accept</w> and peremptory answer to the messenger.",
                            "target_lang_translations": {
                                "ru": "Передайте наш <w>принятый</w> и безапелляционный ответ гонцу."
                            }
                        },
                        {
                            "text": "The document was considered <w>accept</w> by the council.",
                            "target_lang_translations": {
                                "ru": "Документ считался <w>принятым</w> советом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "archaic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принятый (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принятый",
                                "target_lang_sense_clarification": "согласованный или одобренный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
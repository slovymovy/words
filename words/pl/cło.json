                },
                {
                    "sense_id": "30cee607-bbf5-31da-9150-251ecccff2e0",
                    "sense_definition": "Dawna opłata pobierana za przejazd drogą, mostem lub za wjazd do miasta.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

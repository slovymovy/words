{
    "word_family": [
        "blooming",
        "bloomer",
        "bloomy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dec19267-22ea-3419-bfab-513e886dfcd4",
                    "sense_definition": "A flower, especially one on a fruit tree or plant.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "botany_flower",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The garden was filled with colorful <w>blooms</w> of every shape and size.",
                            "target_lang_translations": {
                                "ru": "Сад был наполнен разноцветными <w>цветами</w> всех форм и размеров."
                            }
                        },
                        {
                            "text": "She picked a single white <w>bloom</w> to put in her hair.",
                            "target_lang_translations": {
                                "ru": "Она сорвала один белый <w>цветок</w>, чтобы вставить его в волосы."
                            }
                        }
                    ],
                    "synonyms": [
                        "blossom",
                        "flower"
                    ],
                    "target_lang_definitions": {
                        "ru": "Цветок, особенно на фруктовом дереве или растении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цветок",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "цвет",
                                "target_lang_sense_clarification": "на фруктовых деревьях, собирательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d78904fa-3fed-3f63-b518-30aa9e6ed9d7",
                    "sense_definition": "The state or period when flowers are open.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "botany_flower",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cherry trees are finally in <w>bloom</w> after the long winter.",
                            "target_lang_translations": {
                                "ru": "Вишневые деревья наконец в <w>цвету</w> после долгой зимы."
                            }
                        },
                        {
                            "text": "We visited the park when the roses were in full <w>bloom</w>.",
                            "target_lang_translations": {
                                "ru": "Мы посетили парк, когда розы были в полном <w>цвету</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "blossom"
                    ],
                    "common_phrases": [
                        "in bloom",
                        "full bloom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние или период, когда цветы распущены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цветение",
                                "target_lang_sense_clarification": "период или процесс"
                            },
                            {
                                "target_lang_word": "цвет",
                                "target_lang_sense_clarification": "состояние (быть в цвету)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "081644fd-b61d-3523-90fe-cc0f0e5e08ec",
                    "sense_definition": "A state or time of high beauty, freshness, and vigor; a healthy glow.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was in the <w>bloom</w> of youth and full of energy.",
                            "target_lang_translations": {
                                "ru": "Она была в <w>расцвете</w> юности и полна энергии."
                            }
                        },
                        {
                            "text": "The fresh sea air brought a healthy <w>bloom</w> to his cheeks.",
                            "target_lang_translations": {
                                "ru": "Свежий морской воздух вызвал здоровый <w>румянец</w> на его щеках."
                            }
                        }
                    ],
                    "synonyms": [
                        "flush",
                        "glow"
                    ],
                    "common_phrases": [
                        "bloom of youth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние или время наивысшей красоты, свежести и силы; здоровый румянец."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расцвет",
                                "target_lang_sense_clarification": "наивысшая точка развития"
                            },
                            {
                                "target_lang_word": "румянец",
                                "target_lang_sense_clarification": "здоровый цвет лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb1913aa-d932-3759-bff9-631ffb9baaab",
                    "sense_definition": "A rapid accumulation of algae in an aquatic system.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ecology_algae",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The toxic algal <w>bloom</w> forced the closure of the local beach.",
                            "target_lang_translations": {
                                "ru": "Токсичное <w>цветение</w> водорослей вынудило закрыть местный пляж."
                            }
                        },
                        {
                            "text": "Nutrient runoff from farms can cause a massive <w>bloom</w> in the lake.",
                            "target_lang_translations": {
                                "ru": "Сток питательных веществ с ферм может вызвать массовое <w>цветение</w> в озере."
                            }
                        }
                    ],
                    "common_phrases": [
                        "algal bloom"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical (ecology)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстрое скопление водорослей в водной системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цветение",
                                "target_lang_sense_clarification": "воды, водорослей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1277faf4-ec7d-3804-bdc0-71f763aab6d6",
                    "sense_definition": "A delicate, powdery coating found on certain fruits, leaves, or chocolate.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surface_coating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Freshly picked blueberries often have a silvery <w>bloom</w> on their skin.",
                            "target_lang_translations": {
                                "ru": "Свежесобранная черника часто имеет серебристый <w>налёт</w> на кожице."
                            }
                        },
                        {
                            "text": "The old chocolate bar had developed a white <w>bloom</w> on the surface.",
                            "target_lang_translations": {
                                "ru": "На старой плитке шоколада появился белый <w>налёт</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий порошкообразный налёт на некоторых фруктах, листьях или шоколаде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "налёт",
                                "target_lang_sense_clarification": "на поверхности фруктов или шоколада"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2612bec9-d747-3336-8949-c2b2c8e22011",
                    "sense_definition": "To produce flowers; to open into blossoms.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "botany_flower",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "These rare orchids only <w>bloom</w> once every few years.",
                            "target_lang_translations": {
                                "ru": "Эти редкие орхидеи <w>цветут</w> только раз в несколько лет."
                            }
                        },
                        {
                            "text": "The garden will <w>bloom</w> with tulips in the spring.",
                            "target_lang_translations": {
                                "ru": "Сад <w>зацветет</w> тюльпанами весной."
                            }
                        }
                    ],
                    "synonyms": [
                        "blossom",
                        "flower"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить цветы; распускаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цвести",
                                "target_lang_sense_clarification": "быть в цвету"
                            },
                            {
                                "target_lang_word": "расцветать",
                                "target_lang_sense_clarification": "распускаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c560646b-1003-358c-8a4d-21428f5fb8c7",
                    "sense_definition": "To grow or develop in a healthy, vigorous way; to flourish.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Their small business began to <w>bloom</w> after they launched the new website.",
                            "target_lang_translations": {
                                "ru": "Их малый бизнес начал <w>процветать</w> после запуска нового веб-сайта."
                            }
                        },
                        {
                            "text": "Under her grandmother's care, the shy child <w>bloomed</w> into a confident girl.",
                            "target_lang_translations": {
                                "ru": "Под опекой бабушки застенчивый ребенок <w>расцвел</w> и превратился в уверенную девочку."
                            }
                        }
                    ],
                    "synonyms": [
                        "flourish",
                        "thrive",
                        "blossom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расти или развиваться здоровым, энергичным образом; процветать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расцветать",
                                "target_lang_sense_clarification": "становиться краше, развиваться"
                            },
                            {
                                "target_lang_word": "процветать",
                                "target_lang_sense_clarification": "об успехе дела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d301d9df-b7e8-3098-815e-ec214f6f6b8b",
                    "sense_definition": "To spread out or expand slowly and softly, often used for liquids, light, or feelings.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "motion_spread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A drop of red ink <w>bloomed</w> in the glass of water.",
                            "target_lang_translations": {
                                "ru": "Капля красных чернил <w>расплылась</w> в стакане с водой."
                            }
                        },
                        {
                            "text": "A feeling of warmth <w>bloomed</w> in his chest when he saw her smile.",
                            "target_lang_translations": {
                                "ru": "Чувство тепла <w>разлилось</w> в его груди, когда он увидел ее улыбку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Медленно и мягко распространяться или расширяться, часто используется для жидкостей, света или чувств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расплываться",
                                "target_lang_sense_clarification": "о пятне, чернилах"
                            },
                            {
                                "target_lang_word": "разливаться",
                                "target_lang_sense_clarification": "о чувствах, тепле"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d52cacab-ad3b-37ce-bff5-f000b25bc577",
                    "sense_definition": "A surname.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Bloom</w> will see you in his office now.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Блум</w> примет вас в своем кабинете сейчас."
                            }
                        },
                        {
                            "text": "The novel features a protagonist named Leopold <w>Bloom</w>.",
                            "target_lang_translations": {
                                "ru": "В романе фигурирует главный герой по имени Леопольд <w>Блум</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Блум",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            },
                            {
                                "target_lang_word": "suspicion",
                                "target_lang_sense_clarification": "feeling that something is wrong or someone is guilty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "555a83f0-cccc-3d9d-b8ce-5449b8215f56",

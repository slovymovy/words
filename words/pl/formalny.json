    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "007ae0a1-33a9-3e9e-8a04-3c68a4b28685",
                    "sense_definition": "Zgodny z ustalonymi przepisami, normami lub etykietą; mający charakter urzędowy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Republika Sacha to <w>formalna</w> nazwa Jakucji.",
                            "target_lang_translations": {
                                "ru": "Республика Саха — это <w>официальное</w> название Якутии."
                            }
                        },
                        {
                            "text": "Musisz złożyć <w>formalny</w> wniosek, aby otrzymać dofinansowanie z urzędu.",
                            "target_lang_translations": {
                                "ru": "Ты должен подать <w>официальное</w> заявление, чтобы получить финансирование от ведомства."
                            }
                        }
                    ],
                    "synonyms": [
                        "urzędowy",
                        "oficjalny",
                        "legalny",
                        "obowiązujący"
                    ],
                    "antonyms": [
                        "nieformalny"
                    ],
                    "common_phrases": [
                        "strój formalny",
                        "wymogi formalne",
                        "względy formalne",
                        "pismo formalne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствующий установленным правилам, нормам или этикету; имеющий официальный характер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "официальный",
                                "target_lang_sense_clarification": "связанный с правительством или учреждением"
                            },
                            {
                                "target_lang_word": "формальный",
                                "target_lang_sense_clarification": "соблюдающий правила и процедуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30cc6250-68c6-38a2-ae22-307957adabca",
                    "sense_definition": "Istniejący tylko z nazwy lub dla zachowania pozorów; nieszczery lub wymuszony.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ich małżeństwo to tylko <w>formalny</w> związek, w rzeczywistości żyją osobno.",
                            "target_lang_translations": {
                                "ru": "Их брак — это лишь <w>формальный</w> союз, на самом деле они живут раздельно."
                            }
                        },
                        {
                            "text": "Uścisk dłoni był chłodny i czysto <w>formalny</w>, bez cienia sympatii.",
                            "target_lang_translations": {
                                "ru": "Рукопожатие было холодным и чисто <w>формальным</w>, без тени симпатии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czysto formalny",
                        "charakter formalny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий только на словах или для видимости; неискренний или вынужденный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формальный",
                                "target_lang_sense_clarification": "сделанный только ради формы, для галочки"
                            },
                            {
                                "target_lang_word": "номинальный",
                                "target_lang_sense_clarification": "существующий только по названию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2dbb61f-4106-32ba-9c7e-42de6d4315dd",
                    "sense_definition": "Dotyczący zewnętrznej formy, kształtu lub konstrukcji czegoś, a nie jego treści.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biegłość <w>formalna</w> tego twórcy budziła szacunek krytyków sztuki.",
                            "target_lang_translations": {
                                "ru": "<w>Формальное</w> мастерство этого творца вызывало уважение искусствоведов."
                            }
                        },
                        {
                            "text": "Wiersz jest doskonały pod względem <w>formalnym</w>, ale trudny w odbiorze.",
                            "target_lang_translations": {
                                "ru": "Стихотворение совершенно в <w>формальном</w> отношении, но сложно для восприятия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "błąd formalny",
                        "analiza formalna",
                        "poprawność formalna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к внешней форме, очертаниям или конструкции чего-либо, а не к содержанию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формальный",
                                "target_lang_sense_clarification": "касающийся формы и структуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6194df5c-14e1-3f85-a3ad-5b5616f44aad",
                    "sense_definition": "Ścisły, oparty na definicjach i regułach logicznych (używane głównie w nauce i logice).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Logika <w>formalna</w> jest podstawą współczesnej informatyki i matematyki.",
                            "target_lang_translations": {
                                "ru": "<w>Формальная</w> логика является основой современной информатики и математики."
                            }
                        },
                        {
                            "text": "Student przedstawił <w>formalny</w> dowód tego skomplikowanego twierdzenia.",
                            "target_lang_translations": {
                                "ru": "Студент представил <w>формальное</w> доказательство этой сложной теоремы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "logika formalna",
                        "dowód formalny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строгий, основанный на определениях и логических правилах (используется в науке и логике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формальный",

        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "77b647ad-e5a3-3348-a1b0-f019f6d2a45e",
                    "sense_definition": "To shine brightly or provide guidance like a signal light.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The warm lights of the cabin <w>beaconed</w> to the weary travelers through the snow.",
                            "target_lang_translations": {
                                "ru": "Теплые огни хижины <w>светили</w> уставшим путникам сквозь снег."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "41f5aaf1-d225-3632-82c1-33f7a230cdb7",
                    "sense_definition": "Небольшой тонкий кусок твёрдого материала; уменьшительная форма от слова <пластина>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На двери кабинета висела медная <пластинка> с именем врача.",
                            "target_lang_translations": {
                                "en": "A copper <plate> with the doctor's name hung on the office door."
                            }
                        },
                        {
                            "text": "Стоматолог сказал, что сыну нужно носить <пластинку> для выравнивания зубов.",
                            "target_lang_translations": {
                                "en": "The dentist said that my son needs to wear a <retainer> to straighten his teeth."
                            }
                        }
                    ],
                    "synonyms": [
                        "пластина"
                    ],
                    "common_phrases": [
                        "металлическая пластинка",
                        "тонкая пластинка",
                        "зубная пластинка"
                    ],
                    "target_lang_definitions": {
                        "en": "A small thin piece of hard material; diminutive form of the word \"plate\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plate",
                                "target_lang_sense_clarification": "small flat piece of material"
                            },
                            {
                                "target_lang_word": "plaque",
                                "target_lang_sense_clarification": "sign with a name or inscription"
                            },
                            {
                                "target_lang_word": "retainer",
                                "target_lang_sense_clarification": "orthodontic device"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cea24de8-b1f1-3ab1-aad5-f1ed3726adf4",
                    "sense_definition": "Диск, на котором записан звук (музыка или речь) для прослушивания на специальном устройстве.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "audio_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В коллекции дедушки нашлась редкая виниловая <пластинка>.",
                            "target_lang_translations": {
                                "en": "A rare vinyl <record> was found in grandfather's collection."
                            }
                        },
                        {
                            "text": "Ты повторяешь одно и то же, как заезженная <пластинка>.",
                            "target_lang_translations": {
                                "en": "You keep repeating the same thing like a broken <record>."
                            }
                        }
                    ],
                    "synonyms": [
                        "грампластинка",
                        "диск",
                        "винил"
                    ],
                    "common_phrases": [
                        "заезженная пластинка",
                        "сменить пластинку",
                        "виниловая пластинка",
                        "долгоиграющая пластинка"
                    ],
                    "target_lang_definitions": {
                        "en": "A disc on which sound (music or speech) is recorded for listening on a special device."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "record",
                                "target_lang_sense_clarification": "music disc"
                            },
                            {
                                "target_lang_word": "vinyl",
                                "target_lang_sense_clarification": "format/material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f789b32f-7c55-3852-b278-c17b85c19c99",
                    "sense_definition": "Плоская расширенная часть листа растения или нижняя часть шляпки гриба.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biology_anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Листовая <пластинка> у этого дерева имеет широкую форму.",
                            "target_lang_translations": {
                                "en": "The leaf <blade> of this tree has a broad shape."
                            }
                        },
                        {
                            "text": "Под шляпкой гриба расположены частые тонкие <пластинки>.",
                            "target_lang_translations": {
                                "en": "Under the mushroom cap, there are frequent thin <gills>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "листовая пластинка"
                    ],
                    "target_lang_definitions": {
                        "en": "The flat expanded part of a plant leaf or the lower part of a mushroom cap."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blade",
                                "target_lang_sense_clarification": "botany - leaf part"
                            },
                            {
                                "target_lang_word": "gill",
                                "target_lang_sense_clarification": "mycology - mushroom part"
                            }
                        ]
                    }
                }
            ]
        }
    ]

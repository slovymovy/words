{
    "word_family": [
        "rough",
        "roughness",
        "roughen"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "23fa476f-7c73-39f6-948e-a410002b604b",
                    "sense_definition": "Without precision or exactness; imprecisely but close to in quantity or amount; approximately.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There were <w>roughly</w> fifty people waiting outside the store.",
                            "target_lang_translations": {
                                "ru": "Снаружи магазина ждало <w>примерно</w> пятьдесят человек."
                            }
                        },
                        {
                            "text": "The two cities are <w>roughly</w> equal in population size.",
                            "target_lang_translations": {
                                "ru": "Эти два города <w>приблизительно</w> равны по численности населения."
                            }
                        }
                    ],
                    "synonyms": [
                        "approximately",
                        "about",
                        "around",
                        "nearly",
                        "more or less"
                    ],
                    "antonyms": [
                        "exactly",
                        "precisely"
                    ],
                    "common_phrases": [
                        "roughly speaking",
                        "roughly equal",
                        "roughly half"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не точно; приблизительно по количеству или объему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "примерно",
                                "target_lang_sense_clarification": "о количестве, времени или размере"
                            },
                            {
                                "target_lang_word": "приблизительно",
                                "target_lang_sense_clarification": "с некоторой погрешностью, не точно"
                            },
                            {
                                "target_lang_word": "грубо",
                                "target_lang_sense_clarification": "без деталей (напр. «грубо говоря»)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f71c3c5-476c-3046-a762-140d6acfac7d",
                    "sense_definition": "In a rough manner; without kindness, softness, or gentleness.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He grabbed my arm <w>roughly</w> and pulled me away from the door.",
                            "target_lang_translations": {
                                "ru": "Он <w>грубо</w> схватил меня за руку и оттащил от двери."
                            }
                        },
                        {
                            "text": "The baggage handlers were criticized for treating the luggage <w>roughly</w>.",
                            "target_lang_translations": {
                                "ru": "Грузчиков критиковали за то, что они <w>грубо</w> обращались с багажом."
                            }
                        }
                    ],
                    "synonyms": [
                        "harshly",
                        "severely",
                        "violently",
                        "forcefully"
                    ],
                    "antonyms": [
                        "gently",
                        "softly",
                        "carefully"
                    ],
                    "common_phrases": [
                        "treat roughly",
                        "handle roughly",
                        "play roughly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жестко, без мягкости или осторожности; с применением физической силы или невежливо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грубо",
                                "target_lang_sense_clarification": "с применением силы или без уважения"
                            },
                            {
                                "target_lang_word": "резко",
                                "target_lang_sense_clarification": "внезапно и с силой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bec66e0-f6aa-3a06-a6e4-b7b957dd422f",
                    "sense_definition": "Unevenly or irregularly; in a way that creates a coarse surface or texture.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The vegetables should be <w>roughly</w> chopped before adding them to the soup.",
                            "target_lang_translations": {
                                "ru": "Овощи следует <w>крупно</w> нарезать перед добавлением в суп."
                            }
                        },
                        {
                            "text": "They lived in a <w>roughly</w> built shack near the river.",
                            "target_lang_translations": {
                                "ru": "Они жили в <w>грубо</w> сколоченной хижине у реки."
                            }
                        }
                    ],
                    "synonyms": [
                        "unevenly",
                        "coarsely",
                        "irregularly"
                    ],
                    "antonyms": [
                        "smoothly",
                        "evenly",
                        "finely"
                    ],
                    "common_phrases": [
                        "roughly hewn",
                        "roughly chopped",
                        "roughly woven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неровно, неаккуратно; создавая шероховатую поверхность или текстуру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грубо",
                                "target_lang_sense_clarification": "о качестве обработки, текстуре или постройке"
                            },
                            {
                                "target_lang_word": "неровно",
                                "target_lang_sense_clarification": "без гладкости, криво"
                            },
                            {
                                "target_lang_word": "крупно",
                                "target_lang_sense_clarification": "о нарезке продуктов (напр. крупно нарезанный)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
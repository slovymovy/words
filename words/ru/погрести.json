    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "918f197b-f048-3efd-a3d7-5ce699f7e23f",
                    "sense_definition": "Поместить тело умершего в могилу и засыпать землёй; совершить обряд похорон.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "burial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родственники решили <погрести> усопшего на старом городском кладбище.",
                            "target_lang_translations": {
                                "en": "The relatives decided to <bury> the deceased in the old city cemetery."
                            }
                        },
                        {
                            "text": "Древних царей часто стремились <погрести> вместе с их сокровищами.",
                            "target_lang_translations": {
                                "en": "They often sought to <bury> ancient kings together with their treasures."
                            }
                        }
                    ],
                    "synonyms": [
                        "похоронить",
                        "предать земле"
                    ],
                    "antonyms": [
                        "эксгумировать",
                        "откопать"
                    ],
                    "common_phrases": [
                        "погрести заживо",
                        "погрести останки"
                    ],
                    "target_lang_definitions": {
                        "en": "To place a deceased person's body in a grave and cover it with earth; to perform a funeral rite."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bury",
                                "target_lang_sense_clarification": "general term for placing a body in the ground"
                            },
                            {
                                "target_lang_word": "inter",
                                "target_lang_sense_clarification": "formal term for burial"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5ad9f24-4a76-3c57-b074-96e4b06ee49a",
                    "sense_definition": "Полностью засыпать, накрыть или завалить чем-либо тяжёлым или сыпучим (снегом, землёй, обломками).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "burial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильная лавина может мгновенно <погрести> под снегом целый посёлок.",
                            "target_lang_translations": {
                                "en": "A strong avalanche can instantly <bury> a whole village under snow."
                            }
                        },
                        {
                            "text": "Землетрясение грозило <погрести> город под руинами зданий.",
                            "target_lang_translations": {
                                "en": "The earthquake threatened to <bury> the city under the ruins of buildings."
                            }
                        }
                    ],
                    "synonyms": [
                        "завалить",
                        "засыпать",
                        "накрыть"
                    ],
                    "antonyms": [
                        "откопать"
                    ],
                    "common_phrases": [
                        "погрести под обломками",
                        "погрести под снегом"
                    ],
                    "target_lang_definitions": {
                        "en": "To completely cover or overwhelm with something heavy or loose (such as snow, earth, or debris)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bury",
                                "target_lang_sense_clarification": "to cover completely under something heavy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c867b9aa-c0df-30cb-8fb5-7ec12822a2e5",
                    "sense_definition": "Предать забвению; заставить исчезнуть или скрыть (мысли, надежды, талант, прошлое).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "burial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему пришлось <погрести> свои мечты о карьере художника ради семьи.",
                            "target_lang_translations": {
                                "en": "He had to <bury> his dreams of a career as an artist for the sake of his family."
                            }
                        },
                        {
                            "text": "Эта тайна была навеки <погребена> в архивах истории.",
                            "target_lang_translations": {
                                "en": "This secret was forever <buried> in the archives of history."
                            }
                        }
                    ],
                    "synonyms": [
                        "похоронить",
                        "сгубить"
                    ],
                    "antonyms": [
                        "возродить"
                    ],
                    "common_phrases": [
                        "погрести надежды",
                        "погрести талант",
                        "погрести прошлое"
                    ],
                    "target_lang_definitions": {
                        "en": "To consign to oblivion; to make disappear or hide (thoughts, hopes, talent, past)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bury",
                                "target_lang_sense_clarification": "figurative; to suppress or give up on something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55698903-c427-3dfc-9aaf-a5eb764274ba",
                    "sense_definition": "Работать вёслами (на лодке) или граблями в течение некоторого времени.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "rowing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили немного <погрести> против течения, чтобы размяться.",
                            "target_lang_translations": {
                                "en": "We decided to <row> against the current for a bit to warm up."
                            }
                        },
                        {
                            "text": "Отец попросил сына <погрести> сено на дальнем лугу.",
                            "target_lang_translations": {
                                "en": "The father asked his son to <rake> the hay in the far meadow."
                            }
                        }
                    ],
                    "common_phrases": [
                        "погрести вёслами",
                        "немного погрести"
                    ],
                    "target_lang_definitions": {
                        "en": "To use oars (on a boat) or a rake for a certain period of time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "row",
                                "target_lang_sense_clarification": "to use oars for a while"
                            },
                            {
                                "target_lang_word": "rake",
                                "target_lang_sense_clarification": "to use a rake for a while"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad61fced-d2d4-3d7b-a756-0cfb22dedb8a",
                    "sense_definition": "Начать совершать движения вёслами; направиться куда-либо на лодке.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "rowing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рыбак сел в лодку, оттолкнулся от берега и быстро <погрёб> к острову.",
                            "target_lang_translations": {
                                "en": "The fisherman sat in the boat, pushed off from the shore, and quickly <rowed> toward the island."
                            }
                        },
                        {
                            "text": "Он молча <погрёб> прочь от тонущего корабля.",
                            "target_lang_translations": {
                                "en": "He silently <rowed> away from the sinking ship."
                            }
                        }
                    ],
                    "common_phrases": [
                        "погрести к берегу"
                    ],
                    "target_lang_definitions": {
                        "en": "To start moving the oars; to head somewhere in a boat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "row",
                                "target_lang_sense_clarification": "to move in a specific direction by rowing"
                            },
                            {
                                "target_lang_word": "start rowing",
                                "target_lang_sense_clarification": "to begin the action of rowing"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "24d3dfca-c7c6-3e4f-96f2-92c5f1a41d0d",
                    "sense_definition": "Kosmetyk o sypkiej lub sprasowanej konsystencji, nakładany na twarz w celu zmatowienia cery i wyrównania jej kolorytu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cosmetics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Używa pani zbyt dużo <w>pudru</w>, co powoduje nienaturalny wygląd skóry.",
                            "target_lang_translations": {
                                "ru": "Вы используете слишком много <w>пудры</w>, из-за чего кожа выглядит неестественно."
                            }
                        },
                        {
                            "text": "Muszę kupić nowy <w>puder</w>, bo stary mi się pokruszył w torebce.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую <w>пудру</w>, потому что старая раскрошилась в сумочке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "puder do twarzy",
                        "puder w kamieniu",
                        "puder sypki",
                        "nałożyć puder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Косметическое средство рассыпчатой или компактной текстуры, наносимое на лицо для матирования и выравнивания тона кожи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пудра",
                                "target_lang_sense_clarification": "косметическая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9793de79-a8c4-3772-b8f6-590c402229bb",
                    "sense_definition": "Bardzo drobno zmielony cukier, używany w cukiernictwie do dekoracji wypieków i przygotowywania lukru.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To ciasto będzie jeszcze lepsze, jeśli posypiemy je <w>pudrem</w>.",
                            "target_lang_translations": {
                                "ru": "Этот пирог будет еще лучше, если мы посыплем его <w>сахарной пудрой</w>."
                            }
                        },
                        {
                            "text": "Do ubicia śmietany najlepiej dodać cukier <w>puder</w>.",
                            "target_lang_translations": {
                                "ru": "Для взбивания сливок лучше всего добавить сахарную <w>пудру</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cukier puder",
                        "posypać pudrem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень мелкий сахарный песок, используемый в кондитерских изделиях для украшения выпечки и приготовления глазури."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сахарная пудра",
                                "target_lang_sense_clarification": "кулинария"
                            },
                            {
                                "target_lang_word": "пудра",
                                "target_lang_sense_clarification": "разг. о сахаре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e0d2950-b0da-3c37-85ff-648f2f62ce5b",
                    "sense_definition": "Lek w postaci proszku lub zawiesiny stosowany zewnętrznie na skórę, działający wysuszająco i łagodząco.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "medicine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz przepisał dziecku specjalny <w>puder</w> na wysypkę.",
                            "target_lang_translations": {
                                "ru": "Врач выписал ребенку специальную <w>присыпку</w> от сыпи."
                            }
                        },
                        {
                            "text": "Ten leczniczy <w>puder</w> pomaga łagodzić swędzenie skóry.",
                            "target_lang_translations": {
                                "ru": "Эта лечебная <w>присыпка</w> помогает снять зуд кожи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "puder leczniczy",
                        "puder w płynie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лекарство в виде порошка или суспензии для наружного применения на кожу, оказывающее подсушивающее и успокаивающее действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присыпка",
                                "target_lang_sense_clarification": "порошок для кожи"
                            },
                            {
                                "target_lang_word": "болтушка",
                                "target_lang_sense_clarification": "жидкая пудра"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "22f84f7a-c34c-3fdf-983b-cf4ac83070ae",
                    "sense_definition": "Met een dun, scherp voorwerp (zoals een naald of doorn) in iets steken of een gaatje maken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pas op dat je niet in je vinger <w>prikt</w> met de naald.",
                            "target_lang_translations": {
                                "en": "Be careful not to <w>prick</w> your finger with the needle.",
                                "ru": "Осторожно, не <w>уколись</w> иголкой в палец."
                            }
                        },
                        {
                            "text": "Hij <w>prikte</w> met zijn vork in de gekookte aardappel.",
                            "target_lang_translations": {
                                "en": "He <w>poked</w> the boiled potato with his fork.",
                                "ru": "Он <w>ткнул</w> вилкой в вареную картофелину."
                            }
                        }
                    ],
                    "synonyms": [
                        "steken"
                    ],
                    "common_phrases": [
                        "gaatjes prikken",
                        "in je vinger prikken"
                    ],
                    "target_lang_definitions": {
                        "en": "To stick into something or make a small hole with a thin, sharp object (like a needle or thorn).",
                        "ru": "Колоть или протыкать что-либо тонким острым предметом (например, иглой или шипом)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prick",
                                "target_lang_sense_clarification": "to make a small hole with something sharp"
                            },
                            {
                                "target_lang_word": "poke",
                                "target_lang_sense_clarification": "to push with a sharp object"
                            },
                            {
                                "target_lang_word": "pierce",
                                "target_lang_sense_clarification": "to penetrate or go through"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "колоть",
                                "target_lang_sense_clarification": "делать укол или ранить чем-то острым"
                            },
                            {
                                "target_lang_word": "протыкать",
                                "target_lang_sense_clarification": "делать отверстие насквозь"
                            },
                            {
                                "target_lang_word": "тыкать",
                                "target_lang_sense_clarification": "касаться острием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d079f0fa-8302-3cdf-bbd2-5b42ceccb8b8",
                    "sense_definition": "Een injectie geven of bloed afnemen met een naald.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Morgenochtend moet ik nuchter zijn om bloed te laten <w>prikken</w>.",
                            "target_lang_translations": {
                                "en": "Tomorrow morning I must be fasting to have blood <w>drawn</w>.",
                                "ru": "Завтра утром мне нужно быть натощак, чтобы <w>сдать</w> кровь."
                            }
                        },
                        {
                            "text": "De verpleegkundige <w>prikte</w> behendig in zijn arm voor de vaccinatie.",
                            "target_lang_translations": {
                                "en": "The nurse skillfully <w>stuck</w> the needle into his arm for the vaccination.",
                                "ru": "Медсестра ловко <w>уколола</w> его в руку для вакцинации."
                            }
                        }
                    ],
                    "synonyms": [
                        "steken"
                    ],
                    "common_phrases": [
                        "bloed prikken"
                    ],
                    "target_lang_definitions": {
                        "en": "To give an injection or draw blood with a needle.",
                        "ru": "Делать укол или брать кровь с помощью иглы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "specifically for taking blood"
                            },
                            {
                                "target_lang_word": "stick",
                                "target_lang_sense_clarification": "to insert a needle"
                            },
                            {
                                "target_lang_word": "inject",
                                "target_lang_sense_clarification": "to give medicine via needle"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "делать укол",
                                "target_lang_sense_clarification": "вводить лекарство"
                            },
                            {
                                "target_lang_word": "брать кровь",
                                "target_lang_sense_clarification": "медицинская процедура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85081088-39f8-3dd0-a5dd-10133870fd3e",
                    "sense_definition": "Een pijnlijk, stekend of irriterend gevoel veroorzaken op de huid of in de ogen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn ogen <w>prikken</w> door de rook in de kamer.",
                            "target_lang_translations": {
                                "en": "My eyes <w>sting</w> because of the smoke in the room.",
                                "ru": "У меня <w>щиплет</w> глаза от дыма в комнате."
                            }
                        },
                        {
                            "text": "Deze nieuwe wollen trui <w>prikt</w> verschrikkelijk op mijn huid.",
                            "target_lang_translations": {
                                "en": "This new wool sweater <w>itches</w> terribly on my skin.",
                                "ru": "Этот новый шерстяной свитер ужасно <w>колется</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prikkende ogen"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a painful, stinging, or irritating sensation on the skin or in the eyes.",
                        "ru": "Вызывать болезненное, колющее или раздражающее ощущение на коже или в глазах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sting",
                                "target_lang_sense_clarification": "sharp burning pain (e.g., eyes, wound)"
                            },
                            {
                                "target_lang_word": "itch",
                                "target_lang_sense_clarification": "irritating sensation (e.g., wool)"
                            },
                            {
                                "target_lang_word": "prickle",
                                "target_lang_sense_clarification": "tingling sensation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "щипать",
                                "target_lang_sense_clarification": "о глазах или коже"
                            },
                            {
                                "target_lang_word": "колоться",
                                "target_lang_sense_clarification": "о шерстяной одежде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "061037d0-f0e0-34bb-8d59-d11007e2efd6",
                    "sense_definition": "Een datum of tijdstip vastleggen voor een afspraak.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "planning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zullen we binnenkort een datum <w>prikken</w> voor ons etentje?",
                            "target_lang_translations": {
                                "en": "Shall we <w>pick</w> a date for our dinner soon?",
                                "ru": "Может, скоро <w>назначим</w> дату для нашего ужина?"
                            }
                        },
                        {
                            "text": "Het is lastig om een moment te <w>prikken</w> waarop iedereen aanwezig kan zijn.",
                            "target_lang_translations": {
                                "en": "It is difficult to <w>set</w> a time when everyone can be present.",
                                "ru": "Трудно <w>выбрать</w> момент, когда все смогут присутствовать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een datum prikken"
                    ],
                    "target_lang_definitions": {
                        "en": "To set or fix a date or time for an appointment.",
                        "ru": "Назначать дату или время для встречи."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pick",
                                "target_lang_sense_clarification": "to choose or select a date"
                            },
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "to fix or establish"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "устанавливать дату"
                            },
                            {
                                "target_lang_word": "выбирать",
                                "target_lang_sense_clarification": "подбирать подходящее время"
                            }
                        ]
                    }
                }
            ]
        },

                        "kobiecy wdzięk",
                        "kobieca sylwetka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к женщине, характерный для её натуры, внешности или поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "женский",

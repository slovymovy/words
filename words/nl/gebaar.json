        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f60f6be2-ee73-3f58-b152-79e1f1ed384b",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>gebaren</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>gebaar</w> naar mijn vriend dat hij stil moet zijn.",
                            "target_lang_translations": {
                                "en": "I <w>gesture</w> to my friend that he needs to be quiet.",
                                "ru": "Я <w>делаю знак</w> другу, чтобы он замолчал."
                            }
                        },
                        {
                            "text": "Als ik niet uit mijn woorden kom, <w>gebaar</w> ik vaak met mijn handen.",
                            "target_lang_translations": {
                                "en": "When I can't find my words, I often <w>gesture</w> with my hands.",
                                "ru": "Если я не могу подобрать слова, я часто <w>жестикулирую</w> руками."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "conjugation"
                        }
                    ],
                    "target_lang_definitions": {

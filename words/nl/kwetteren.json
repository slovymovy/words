    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c343ab1c-289f-3f3e-b0b8-356079baff05",
                    "sense_definition": "Het maken van snelle, korte en vrolijke geluiden door vogels.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_birds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de vroege ochtend <w>kwetteren</w> de mussen luid in de heg.",
                            "target_lang_translations": {
                                "en": "In the early morning, the sparrows <w>twitter</w> loudly in the hedge.",
                                "ru": "Ранним утром воробьи громко <w>чирикают</w> в живой изгороди."
                            }
                        },
                        {
                            "text": "Plots hoor je de vogels <w>kwetteren</w> en voelt de zon extra warm aan.",
                            "target_lang_translations": {
                                "en": "Suddenly you hear the birds <w>twittering</w> and the sun feels extra warm.",
                                "ru": "Вдруг слышишь, как <w>щебечут</w> птицы, и солнце кажется особенно теплым."
                            }
                        }
                    ],
                    "synonyms": [
                        "tsjilpen",
                        "kwinkeleren"
                    ],
                    "common_phrases": [
                        "vrolijk kwetteren",
                        "vogels kwetteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To make quick, short, and cheerful sounds (typically of birds).",
                        "ru": "Издавать быстрые, короткие и веселые звуки (о птицах)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "twitter",
                                "target_lang_sense_clarification": "to make a succession of light, chirping sounds"
                            },
                            {
                                "target_lang_word": "chirp",
                                "target_lang_sense_clarification": "to make short, high-pitched sounds"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "щебетать",
                                "target_lang_sense_clarification": "о пении птиц"
                            },
                            {
                                "target_lang_word": "чирикать",
                                "target_lang_sense_clarification": "о воробьях и мелких птицах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db25d9ef-52f7-31b2-a68d-a21bc04faa67",
                    "sense_definition": "Druk, snel en vrolijk praten, vaak over alledaagse dingen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen <w>kwetterden</w> honderduit over hun avonturen op het schoolplein.",
                            "target_lang_translations": {
                                "en": "The children <w>chattered</w> away about their adventures on the school playground.",
                                "ru": "Дети без умолку <w>щебетали</w> о своих приключениях на школьном дворе."
                            }
                        },
                        {
                            "text": "De twee vriendinnen zaten gezellig te <w>kwetteren</w> op een bankje in het park.",
                            "target_lang_translations": {
                                "en": "The two friends were sitting on a bench in the park, <w>chatting</w> pleasantly.",
                                "ru": "Две подруги сидели на скамейке в парке и мило <w>болтали</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "babbelen",
                        "kwekken"
                    ],
                    "common_phrases": [
                        "druk kwetteren",
                        "gezellig kwetteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To talk busily, quickly, and cheerfully, often about trivial matters.",
                        "ru": "Оживленно, быстро и весело разговаривать, часто о повседневных вещах"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chatter",
                                "target_lang_sense_clarification": "to talk rapidly and continuously about unimportant matters"
                            },
                            {
                                "target_lang_word": "chat",
                                "target_lang_sense_clarification": "to talk in a friendly, informal way"
                            },
                            {
                                "target_lang_word": "twitter",
                                "target_lang_sense_clarification": "to talk in an excited or nervous manner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "щебетать",
                                "target_lang_sense_clarification": "перен. оживленно говорить"
                            },
                            {
                                "target_lang_word": "болтать",
                                "target_lang_sense_clarification": "разговаривать"
                            },
                            {
                                "target_lang_word": "тараторить",
                                "target_lang_sense_clarification": "очень быстро говорить"
                            }
                        ]
                    }
                }
            ]
        }

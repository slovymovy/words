                {
                    "sense_id": "ad74510c-6442-3722-a3ee-28e0d2ed9aad",
                    "sense_definition": "Naar de overkant gaan van een weg, water of open ruimte.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk goed naar links en rechts voordat je de straat <w>oversteekt</w>.",
                            "target_lang_translations": {
                                "en": "Look carefully left and right before you <w>cross</w> the street.",
                                "ru": "Посмотри внимательно налево и направо, прежде чем <w>переходить</w> улицу."
                            }
                        },
                        {
                            "text": "We zijn gisteren met een kleine boot de rivier <w>overgestoken</w>.",
                            "target_lang_translations": {
                                "en": "We <w>crossed</w> the river yesterday with a small boat.",
                                "ru": "Вчера мы <w>переправились</w> через реку на маленькой лодке."
                            }
                        },
                        {
                            "text": "Zij <w>staken</w> de drukke weg <w>over</w> bij het stoplicht.",
                            "target_lang_translations": {
                                "en": "They <w>crossed</w> the busy road at the traffic light.",
                                "ru": "Они <w>перешли</w> оживленную дорогу на светофоре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de straat oversteken",
                        "het zebrapad oversteken",
                        "veilig oversteken",
                        "een grens oversteken"
                    ],
                    "target_lang_definitions": {
                        "en": "To go to the other side of a road, body of water, or open space.",
                        "ru": "Перемещаться на другую сторону дороги, водоема или открытого пространства."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cross",
                                "target_lang_sense_clarification": "to go to the other side"
                            },
                            {
                                "target_lang_word": "cross over",
                                "target_lang_sense_clarification": "movement to the other side"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "переходить",
                                "target_lang_sense_clarification": "идти на другую сторону (обычно пешком)"
                            },
                            {
                                "target_lang_word": "пересекать",
                                "target_lang_sense_clarification": "проходить через пространство или линию"
                            },
                            {
                                "target_lang_word": "переправляться",
                                "target_lang_sense_clarification": "перебираться через реку или препятствие"
                            }

            "senses": [
                {
                    "sense_id": "811affec-c4be-3140-9319-e236f6fb4f48",
                    "sense_definition": "Часть войск или флота, находящаяся впереди главных сил для разведки и охранения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Военный <w>авангард</w> выдвинулся на разведку местности перед наступлением.",
                            "target_lang_translations": {
                                "en": "The military <w>vanguard</w> moved out to scout the terrain before the offensive."
                            }
                        },
                        {
                            "text": "Наш <w>авангард</w> вступил в бой с противником на рассвете.",
                            "target_lang_translations": {
                                "en": "Our <w>vanguard</w> engaged the enemy at dawn."
                            }
                        }
                    ],

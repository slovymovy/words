                            {
                                "target_lang_word": "человек настроения",
                                "target_lang_sense_clarification": "тот, чье поведение зависит от текущего настроения"
                            },
                            {
                                "target_lang_word": "с переменчивым настроением",
                                "target_lang_sense_clarification": "неустойчивый эмоционально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efb4f82f-6d8c-3717-91b1-cd8d37b4fd06",

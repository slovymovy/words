                    "sense_definition": "Gebiedende wijs van <w>schaduwen</w>.",
                    "learner_level": "B1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Schaduw</w> die man en rapporteer wat hij doet.",
                            "target_lang_translations": {
                                "en": "<w>Shadow</w> that man and report what he does.",
                                "ru": "<w>Следи</w> за тем мужчиной и докладывай, что он делает."
                            }

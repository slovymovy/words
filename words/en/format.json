    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a63bb395-8e1c-322e-8ec6-7524ef710133",
                    "sense_definition": "The layout or arrangement of a publication, document, or event.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "layout_arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please submit your report in the correct <w>format</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отправьте ваш отчет в правильном <w>формате</w>."
                            }
                        },
                        {
                            "text": "The <w>format</w> of the meeting allows everyone to speak.",
                            "target_lang_translations": {
                                "ru": "<w>Формат</w> встречи позволяет высказаться каждому."
                            }
                        }
                    ],
                    "common_phrases": [
                        "standard format",
                        "large format",
                        "meeting format",
                        "document format"
                    ],
                    "target_lang_definitions": {
                        "ru": "Макет или способ организации публикации, документа или мероприятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формат",
                                "target_lang_sense_clarification": "общий вид, структура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cb748e5-e39a-3a86-bebb-2dfcdcfab43f",
                    "sense_definition": "A specific structure used to store data in a computer file.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot open this image because the <w>format</w> is not supported.",
                            "target_lang_translations": {
                                "ru": "Я не могу открыть это изображение, потому что этот <w>формат</w> не поддерживается."
                            }
                        },
                        {
                            "text": "Save the document in a compatible <w>format</w> so we can edit it.",
                            "target_lang_translations": {
                                "ru": "Сохраните документ в совместимом <w>формате</w>, чтобы мы могли его редактировать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "file format",
                        "digital format",
                        "PDF format",
                        "audio format"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфическая структура, используемая для хранения данных в компьютерном файле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формат",
                                "target_lang_sense_clarification": "файла, данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a7f9e8a-5a41-370e-a50e-837b605605d3",
                    "sense_definition": "The general style or genre of a television show or radio programme.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The radio station changed its <w>format</w> to play only jazz music.",
                            "target_lang_translations": {
                                "ru": "Радиостанция сменила свой <w>формат</w>, чтобы играть только джаз."
                            }
                        },
                        {
                            "text": "Viewers did not like the new <w>format</w> of the evening news program.",
                            "target_lang_translations": {
                                "ru": "Зрителям не понравился новый <w>формат</w> вечерней новостной программы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "radio format",
                        "show format",
                        "program format"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общий стиль или жанр телепередачи или радиопрограммы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формат",
                                "target_lang_sense_clarification": "вещания, шоу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3f3dd890-9303-39df-9423-d88d8d69c15f",
                    "sense_definition": "To arrange the layout and appearance of text or data in a document.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "layout_arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>format</w> the headings in bold to make them stand out.",
                            "target_lang_translations": {
                                "ru": "Вам нужно <w>отформатировать</w> заголовки жирным шрифтом, чтобы выделить их."
                            }
                        },
                        {
                            "text": "She learned how to <w>format</w> the spreadsheet correctly for the presentation.",
                            "target_lang_translations": {
                                "ru": "Она научилась правильно <w>форматировать</w> таблицу для презентации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "format the text",
                        "format a document",
                        "poorly formatted"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организовывать разметку и внешний вид текста или данных в документе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "форматировать",
                                "target_lang_sense_clarification": "текст, документ"
                            },
                            {
                                "target_lang_word": "оформлять",
                                "target_lang_sense_clarification": "придавать вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cdd010a-b9d4-39f3-a2ff-fcc5f1ff1aed",
                    "sense_definition": "To prepare a mass storage medium for initial use, often erasing any existing data in the process.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I lost weeks of work when I inadvertently <w>formatted</w> my hard drive.",
                            "target_lang_translations": {
                                "ru": "Я потерял результаты недельной работы, когда случайно <w>отформатировал</w> свой жесткий диск."
                            }
                        },
                        {
                            "text": "You must <w>format</w> the memory card before you can use it in the camera.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>отформатировать</w> карту памяти, прежде чем сможете использовать её в камере."
                            }
                        }
                    ],
                    "synonyms": [
                        "initialize"
                    ],
                    "common_phrases": [
                        "format the drive",
                        "format a disk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подготавливать носитель информации к использованию, часто удаляя при этом существующие данные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "форматировать",
                                "target_lang_sense_clarification": "диск, память"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "27e4cf6d-d1d8-3160-9c75-d706fefc94a2",
                    "sense_definition": "Wegstromen of naar een lager gelegen punt stromen, meestal gezegd van vloeistoffen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het regenwater kan via deze geul makkelijk <w>afvloeien</w>.",
                            "target_lang_translations": {
                                "ru": "Дождевая вода может легко <w>стекать</w> по этому желобу."
                            }
                        },
                        {
                            "text": "Zorg dat het overtollige water kan <w>afvloeien</w> naar de sloot.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что излишки воды могут <w>стекать</w> в канаву."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegstromen",
                        "weglopen"
                    ],
                    "antonyms": [
                        "toestromen"
                    ],
                    "common_phrases": [
                        "water laten afvloeien",
                        "snel afvloeien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утекать или течь в более низкое место, обычно говорится о жидкостях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стекать",
                                "target_lang_sense_clarification": "двигаться вниз (о жидкости)"
                            },
                            {
                                "target_lang_word": "утекать",
                                "target_lang_sense_clarification": "покидать место (о жидкости)"
                            },
                            {
                                "target_lang_word": "сливаться",
                                "target_lang_sense_clarification": "удаляться через сток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "126e2029-fa1c-327a-ba34-1a91a757a8b8",
                    "sense_definition": "Het bedrijf moeten verlaten vanwege ontslag, vaak in het kader van een reorganisatie of inkrimping.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dismissal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de reorganisatie moesten honderd werknemers gedwongen <w>afvloeien</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за реорганизации сто сотрудников были вынуждены <w>уйти</w>."
                            }
                        },
                        {
                            "text": "Het bedrijf laat personeel via een sociaal plan <w>afvloeien</w>.",
                            "target_lang_translations": {
                                "ru": "Компания позволяет персоналу <w>уйти</w> по социальному плану."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontslagen worden",
                        "vertrekken"
                    ],
                    "antonyms": [
                        "aangenomen worden"
                    ],
                    "common_phrases": [
                        "natuurlijk afvloeien",
                        "gedwongen afvloeien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть вынужденным покинуть компанию из-за увольнения, часто в рамках реорганизации или сокращения штатов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попадать под сокращение",
                                "target_lang_sense_clarification": "быть уволенным из-за реорганизации"
                            },
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "покидать место работы (эвфемизм)"
                            },
                            {
                                "target_lang_word": "быть уволенным",
                                "target_lang_sense_clarification": "потерять работу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "239bfb0c-3502-3670-aa15-78e883d0ec5f",
                    "sense_definition": "Natte inkt drogen door er vloeipapier op te drukken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij moest de brief voorzichtig <w>afvloeien</w> voordat hij hem in de envelop deed.",
                            "target_lang_translations": {
                                "ru": "Ему нужно было осторожно <w>промокнуть</w> письмо, прежде чем положить его в конверт."
                            }

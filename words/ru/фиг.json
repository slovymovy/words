    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dd257d44-4e05-34c0-8505-8ed192aad2cc",
                    "sense_definition": "Грубый жест (кукиш), при котором большой палец просовывается между указательным и средним; используется для выражения отказа или насмешки.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gesture_refusal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я попросил у брата денег, а он показал мне <фиг>.",
                            "target_lang_translations": {
                                "en": "I asked my brother for money, but he showed me a <fig>."
                            }
                        },
                        {
                            "text": "Вместо обещанной помощи мы получили <фиг> с маслом.",
                            "target_lang_translations": {
                                "en": "Instead of the promised help, we got <zilch>."
                            }
                        }
                    ],
                    "synonyms": [
                        "кукиш",
                        "шиш",
                        "дуля",
                        "фига"
                    ],
                    "common_phrases": [
                        "фиг вам",
                        "фиг с маслом",
                        "фиг тебе"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "вульгарный жест"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A rude gesture (thumb tucked between index and middle fingers) used to express refusal or mockery; figuratively means getting absolutely nothing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fig",
                                "target_lang_sense_clarification": "the specific hand gesture"
                            },
                            {
                                "target_lang_word": "zilch",
                                "target_lang_sense_clarification": "slang for nothing"
                            },
                            {
                                "target_lang_word": "squat",
                                "target_lang_sense_clarification": "slang for nothing (e.g., diddly-squat)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4164c03-d771-3280-8d88-59b059544f61",
                    "sense_definition": "Эвфемизм (замена грубого слова), используемый для обозначения чего-то неопределённого, отсутствия чего-либо или безразличия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "euphemism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <фиг> знает, как починить этот старый телевизор.",
                            "target_lang_translations": {
                                "en": "<Heck> knows how to fix this old TV."
                            }
                        },
                        {
                            "text": "Мы весь день искали грибы, но не нашли ни <фига>.",
                            "target_lang_translations": {
                                "en": "We looked for mushrooms all day but didn't find a <damn thing>."
                            }
                        }
                    ],
                    "synonyms": [
                        "хер",
                        "шиш"
                    ],
                    "common_phrases": [
                        "фиг знает",
                        "ни фига",
                        "иди на фиг",
                        "фиг с ним"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "эвфемизм"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A euphemism (substitute for a ruder word) used to denote uncertainty, absence, or indifference."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heck",
                                "target_lang_sense_clarification": "euphemism in expressions like 'who knows'"
                            },
                            {
                                "target_lang_word": "damn thing",
                                "target_lang_sense_clarification": "emphatic negative (not a damn thing)"
                            },
                            {
                                "target_lang_word": "jack",
                                "target_lang_sense_clarification": "slang for nothing (jack squat)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6c3b20a-7c37-3425-9398-36ff5baf1541",
                    "sense_definition": "Разговорное выражение, означающее категорическое несогласие, отрицание или уверенность в том, что событие не произойдёт.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "denial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Фиг> ты меня теперь обманешь, я знаю всю правду.",
                            "target_lang_translations": {
                                "en": "<Like hell> you will trick me now, I know the whole truth."
                            }
                        },
                        {
                            "text": "Они думали, что мы сдадимся без боя, но <фиг> там!",
                            "target_lang_translations": {
                                "en": "They thought we would give up without a fight, but <fat chance>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "дудки"
                    ],
                    "common_phrases": [
                        "фиг там",
                        "фиг два"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "выражает протест или отрицание"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A colloquial expression indicating categorical disagreement, denial, or certainty that an event will not happen."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "like hell",
                                "target_lang_sense_clarification": "strong refusal or disbelief"
                            },
                            {
                                "target_lang_word": "no way",
                                "target_lang_sense_clarification": "categorical denial"
                            },
                            {
                                "target_lang_word": "fat chance",
                                "target_lang_sense_clarification": "sarcastic denial"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e77399c-f509-3562-a3d8-037743f3a71c",
                    "sense_definition": "Форма родительного падежа множественного числа существительного <w>фига</w> (плод инжира).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fruit_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На рынке продавали много спелых и сладких <фиг>.",
                            "target_lang_translations": {
                                "en": "They were selling many ripe and sweet <figs> at the market."
                            }
                        },
                        {
                            "text": "В этом рецепте варенья совсем нет <фиг>, только сахар и яблоки.",
                            "target_lang_translations": {
                                "en": "There are absolutely no <figs> in this jam recipe, only sugar and apples."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "родительный падеж, множественное число"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive plural form of the noun referring to the fig fruit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "figs",
                                "target_lang_sense_clarification": "fruit (plural)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "062e1a12-87e6-37f6-9d36-b7cc7dff16ed",
                    "sense_definition": "Causing or feeling embarrassment, discomfort, or inconvenience.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That was an extremely <w>awkward</w> moment for everyone involved.",
                            "target_lang_translations": {
                                "ru": "Это был крайне <w>неловкий</w> момент для всех участников."
                            }
                        },
                        {
                            "text": "An <w>awkward</w> silence fell over the room after the joke failed.",
                            "target_lang_translations": {
                                "ru": "После неудачной шутки в комнате повисла <w>неловкая</w> тишина."
                            }
                        }

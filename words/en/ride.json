    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f1aed48d-8749-33d0-95b3-5d88960ad518",
                    "sense_definition": "To transport oneself by sitting on and directing a horse, bicycle, or motorcycle.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>ride</w> my bike to school every morning.",
                            "target_lang_translations": {
                                "ru": "Я <w>езжу</w> в школу на велосипеде каждое утро."
                            }
                        },
                        {
                            "text": "She learned to <w>ride</w> a horse when she was a child.",
                            "target_lang_translations": {
                                "ru": "Она научилась <w>ездить</w> верхом на лошади, когда была ребёнком."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ride a bike",
                        "ride a horse",
                        "ride a motorcycle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться верхом на лошади, велосипеде или мотоцикле, управляя ими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "е́здить",
                                "target_lang_sense_clarification": "регулярно или уметь управлять"
                            },
                            {
                                "target_lang_word": "ката́ться",
                                "target_lang_sense_clarification": "для удовольствия"
                            },
                            {
                                "target_lang_word": "е́хать",
                                "target_lang_sense_clarification": "двигаться в конкретный момент"
                            },
                            {
                                "target_lang_word": "скака́ть",
                                "target_lang_sense_clarification": "быстро ехать верхом на лошади"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aebd29a7-cd2f-302c-a54f-640d60fddefd",
                    "sense_definition": "To travel as a passenger in a vehicle.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>rode</w> the bus to the city center yesterday.",
                            "target_lang_translations": {
                                "ru": "Мы <w>ехали</w> на автобусе в центр города вчера."
                            }
                        },
                        {
                            "text": "He likes to <w>ride</w> in the front seat of the car.",
                            "target_lang_translations": {
                                "ru": "Ему нравится <w>ездить</w> на переднем сиденье машины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ride the bus",
                        "ride in a car",
                        "ride the train"
                    ],
                    "target_lang_definitions": {

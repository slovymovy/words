    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "82d1f760-16ae-38f2-98a9-f4e5dbaf68c7",
                    "sense_definition": "Parallel to the ground or to the horizon; flat and level.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please draw a <w>horizontal</w> line across the center of the page.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проведите <w>горизонтальную</w> линию через центр страницы."
                            }
                        },
                        {
                            "text": "She felt dizzy and needed to lie in a <w>horizontal</w> position.",
                            "target_lang_translations": {
                                "ru": "У неё закружилась голова, и ей нужно было принять <w>горизонтальное</w> положение."
                            }
                        }
                    ],
                    "synonyms": [
                        "linear",
                        "melodic"
                    ],
                    "antonyms": [
                        "vertical"
                    ],
                    "common_phrases": [
                        "horizontal line",
                        "horizontal position",
                        "horizontal surface",
                        "horizontal axis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Параллельный земле или горизонту; плоский и ровный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горизонтальный",
                                "target_lang_sense_clarification": "параллельный горизонту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27d39722-0639-3350-97b2-30c2c46b6770",
                    "sense_definition": "Relating to companies, products, or systems at the same stage of production or across different industries.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The corporation pursued a strategy of <w>horizontal</w> integration to reduce competition.",
                            "target_lang_translations": {
                                "ru": "Корпорация придерживалась стратегии <w>горизонтальной</w> интеграции, чтобы снизить конкуренцию."
                            }
                        },
                        {
                            "text": "Effective <w>horizontal</w> communication between departments improves efficiency.",
                            "target_lang_translations": {
                                "ru": "Эффективная <w>горизонтальная</w> коммуникация между отделами повышает производительность."
                            }

            "senses": [
                {
                    "sense_id": "d78db1ca-9b92-3c59-a1b1-65b98d379c8d",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>kartelen</w>.",
                    "learner_level": "B1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kartel</w> het papier met een speciale schaar voor mijn knutselwerk.",
                            "target_lang_translations": {
                                "ru": "Я <w>нарезаю зубчиками</w> бумагу специальными ножницами для поделок."
                            }
                        },
                        {
                            "text": "Als ik de randen van de stof <w>kartel</w>, gaat het minder snel rafelen.",
                            "target_lang_translations": {
                                "ru": "Если я <w>обработаю зубчиками</w> края ткани, она будет меньше осыпаться."
                            }
                        }
                    ],

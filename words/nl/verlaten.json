{
    "word_family": [
        "verlating",
        "verlaat"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4745dda0-4899-34eb-ae91-b2eb321187d6",
                    "sense_definition": "Weggaan van een plaats, uit een ruimte of bij een persoon vandaan gaan.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "leave_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>verlaat</w> het huis elke ochtend om zeven uur om naar zijn werk te gaan.",
                            "target_lang_translations": {
                                "ru": "Он <w>выходит</w> из дома каждое утро в семь часов, чтобы пойти на работу."
                            }
                        },
                        {
                            "text": "De boze klant <w>verliet</w> de winkel zonder iets te kopen.",
                            "target_lang_translations": {
                                "ru": "Сердитый покупатель <w>покинул</w> магазин, ничего не купив."
                            }
                        }
                    ],
                    "synonyms": [
                        "weggaan",
                        "vertrekken",
                        "uitgaan"
                    ],
                    "antonyms": [
                        "blijven",
                        "arriveren",
                        "aankomen",
                        "binnenkomen"
                    ],
                    "common_phrases": [
                        "het huis verlaten",
                        "het land verlaten",
                        "de zaal verlaten",
                        "het pand verlaten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уходить из какого-либо места, помещения или от человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покидать",
                                "target_lang_sense_clarification": "уходить из места или оставлять кого-то"
                            },
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "удаляться от чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8b65810-e643-39e6-9c2c-64f22803f493",
                    "sense_definition": "Iemand of iets definitief achterlaten zonder de intentie terug te keren of voor diegene te zorgen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abandon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De man heeft zijn vrouw en kinderen jaren geleden <w>verlaten</w>.",
                            "target_lang_translations": {
                                "ru": "Мужчина <w>бросил</w> свою жену и детей много лет назад."
                            }
                        },
                        {
                            "text": "Veel huisdieren worden tijdens de vakantieperiode door hun eigenaars <w>verlaten</w>.",
                            "target_lang_translations": {
                                "ru": "Многих домашних животных хозяева <w>бросают</w> во время отпусков."
                            }
                        }
                    ],
                    "synonyms": [
                        "in de steek laten",
                        "achterlaten",
                        "laten vallen"
                    ],
                    "antonyms": [
                        "zorgen voor",
                        "bijstaan",
                        "steunen"
                    ],
                    "common_phrases": [
                        "zijn gezin verlaten",
                        "zijn post verlaten",
                        "iemand verlaten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставлять кого-либо или что-либо окончательно, без намерения вернуться или заботиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросать",
                                "target_lang_sense_clarification": "оставлять на произвол судьбы (семью, питомцев)"
                            },
                            {
                                "target_lang_word": "оставлять",
                                "target_lang_sense_clarification": "покидать навсегда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca57ddf6-21d9-3d21-b144-c19eef238f47",
                    "sense_definition": "(wederkerend) Vertrouwen hebben in of rekenen op iemand of iets.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rely_on",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt je in moeilijke tijden altijd op je beste vrienden <w>verlaten</w>.",
                            "target_lang_translations": {
                                "ru": "В трудные времена ты всегда можешь <w>положиться</w> на своих лучших друзей."
                            }
                        },
                        {
                            "text": "Hij <w>verlaat</w> zich volledig op zijn intuïtie bij het nemen van belangrijke beslissingen.",
                            "target_lang_translations": {
                                "ru": "Он полностью <w>полагается</w> на свою интуицию при принятии важных решений."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertrouwen op",
                        "rekenen op",
                        "bouwen op"
                    ],
                    "antonyms": [
                        "wantrouwen",
                        "twijfelen aan"
                    ],
                    "common_phrases": [
                        "zich op iemand verlaten",
                        "zich op iets verlaten"
                    ],
                    "target_lang_definitions": {
                        "ru": "(возвратный глагол) Доверять кому-либо или рассчитывать на кого-то/что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полагаться",
                                "target_lang_sense_clarification": "рассчитывать на кого-либо (с предлогом 'на')"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "08c1a4a7-2758-30a3-a1c0-8f83bb751371",
                    "sense_definition": "Waar niemand (meer) is; leeg, stil en eenzaam.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "deserted",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We liepen 's nachts door de donkere en <w>verlaten</w> straten van de stad.",
                            "target_lang_translations": {
                                "ru": "Мы шли ночью по темным и <w>пустынным</w> улицам города."
                            }
                        },
                        {
                            "text": "Het oude fabrieksterrein ligt er al jaren <w>verlaten</w> en vervallen bij.",
                            "target_lang_translations": {
                                "ru": "Территория старой фабрики уже много лет стоит <w>заброшенной</w> и ветхой."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitgestorven",
                        "onbewoond",
                        "leeg",
                        "eenzaam"
                    ],
                    "antonyms": [
                        "druk",
                        "bevolkt",
                        "levendig"
                    ],
                    "common_phrases": [
                        "een verlaten plaats",
                        "een verlaten eiland",
                        "een verlaten straat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где никого нет; пустое, тихое и одинокое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пустынный",
                                "target_lang_sense_clarification": "безлюдный (об улицах, местах)"
                            },
                            {
                                "target_lang_word": "заброшенный",
                                "target_lang_sense_clarification": "оставленный людьми, ветхий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07c2b170-9a64-3669-b7aa-1b2d92d42143",
                    "sense_definition": "Naar een later tijdstip verschuiven; vertragen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "delay",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een technische storing <w>verlaatte</w> de lancering van de raket met enkele uren.",
                            "target_lang_translations": {
                                "ru": "Технический сбой <w>задержал</w> запуск ракеты на несколько часов."
                            }
                        },
                        {
                            "text": "De onverwachte file heeft onze aankomst aanzienlijk <w>verlaat</w>.",
                            "target_lang_translations": {
                                "ru": "Неожиданная пробка значительно <w>задержала</w> наше прибытие."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitstellen",
                        "vertragen",
                        "ophouden"
                    ],
                    "antonyms": [
                        "vervroegen",
                        "bespoedigen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This usage is rare in modern Dutch; 'vertragen' or 'uitstellen' is preferred."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Переносить на более позднее время; задерживать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задерживать",
                                "target_lang_sense_clarification": "замедлять или откладывать (устар.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fe706290-d671-31ae-8f81-5852aaaab8ec",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>verlaat</w> (een kleine sluis).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sluices",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude <w>verlaten</w> in de gracht worden gerestaureerd door de gemeente.",
                            "target_lang_translations": {
                                "ru": "Старые <w>шлюзы</w> на канале реставрируются муниципалитетом."
                            }
                        },
                        {
                            "text": "Het waterpeil wordt geregeld door een systeem van stuwen en <w>verlaten</w>.",
                            "target_lang_translations": {
                                "ru": "Уровень воды регулируется системой плотин и <w>шлюзов</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sluizen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число существительного 'verlaat' (небольшой шлюз)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлюзы",
                                "target_lang_sense_clarification": "гидротехнические сооружения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
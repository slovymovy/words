            "senses": [
                {
                    "sense_id": "5d140d6c-633f-3eb9-9129-7e98f75792fa",
                    "sense_definition": "A surname originating as an occupation for a player of the harp.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "names_derived_from_occupation",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Harper</w> has been teaching history at the local school for twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Харпер</w> преподает историю в местной школе уже двадцать лет."
                            }
                        },
                        {
                            "text": "The <w>Harper</w> family invited all their neighbors to the summer barbecue.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Харпер</w> пригласила всех соседей на летнее барбекю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Miss Harper",
                        "Doctor Harper"
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "occupational origin"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, происходящая от названия профессии (игрок на арфе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Харпер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c8a7790-dbf3-3cfc-a902-53b74c8c8954",
                    "sense_definition": "A male or female given name transferred from the surname.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "names_derived_from_occupation",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "They decided to name their newborn daughter <w>Harper</w> after the famous author.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать свою новорожденную дочь <w>Харпер</w> в честь известной писательницы."
                            }
                        },
                        {
                            "text": "<w>Harper</w> was excited to start his first day of kindergarten.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4293a4cf-0c7b-3753-8523-302492572356",
                    "sense_definition": "Не соблюдать, преступать (закон, правило, договор, обязательство).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "null",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Никто не имеет права безнаказанно <нарушать> закон.",
                            "target_lang_translations": {
                                "en": "No one has the right to <violate> the law with impunity."
                            }
                        },
                        {
                            "text": "Водители не должны <нарушать> правила дорожного движения.",
                            "target_lang_translations": {
                                "en": "Drivers must not <break> traffic rules."
                            }
                        }
                    ],
                    "synonyms": [
                        "преступать"
                    ],
                    "antonyms": [
                        "соблюдать"
                    ],
                    "common_phrases": [
                        "нарушать закон",
                        "нарушать правила",
                        "нарушать слово",
                        "нарушать договор"
                    ],
                    "target_lang_definitions": {
                        "en": "To fail to comply with or transgress (a law, rule, contract, or obligation)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "violate",
                                "target_lang_sense_clarification": "a law, rule, or agreement"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "a law or rule"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ef57ee1-277c-3b0b-93d9-eca84f296923",
                    "sense_definition": "Прерывать или расстраивать нормальное состояние, течение чего-либо (тишину, покой, порядок).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "null",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Громкие звуки могут <нарушать> ночную тишину в спальном районе.",
                            "target_lang_translations": {
                                "en": "Loud sounds can <disturb> the night silence in a residential area."
                            }
                        },
                        {
                            "text": "Мы старались не <нарушать> покой уставших путешественников.",
                            "target_lang_translations": {
                                "en": "We tried not to <disturb> the peace of the tired travelers."
                            }
                        }
                    ],
                    "synonyms": [
                        "прерывать"
                    ],
                    "common_phrases": [
                        "нарушать тишину",
                        "нарушать покой",
                        "нарушать равновесие",
                        "нарушать молчание"
                    ],
                    "target_lang_definitions": {
                        "en": "To interrupt or upset the normal state or course of something (silence, peace, order)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disturb",
                                "target_lang_sense_clarification": "peace, silence, or order"
                            },
                            {
                                "target_lang_word": "disrupt",
                                "target_lang_sense_clarification": "a process or state"
                            }
                        ]
                    }
                }
            ]
        }
    ]

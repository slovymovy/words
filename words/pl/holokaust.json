    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4564b1e4-01cb-3980-b058-294904b772de",
                    "sense_definition": "Zagłada Żydów europejskich dokonana przez nazistowskie Niemcy w czasie II wojny światowej.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "historical_event",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "Muzeum w Oświęcimiu upamiętnia ofiary, które pochłonął <w>Holokaust</w>.",
                            "target_lang_translations": {
                                "ru": "Музей в Освенциме чтит память жертв, которых унес <w>Холокост</w>."
                            }
                        },
                        {
                            "text": "Wielu ocalałych przez całe życie nosiło w sobie traumę, którą spowodował <w>Holokaust</w>.",
                            "target_lang_translations": {
                                "ru": "Многие выжившие всю жизнь носили в себе травму, которую нанес <w>Холокост</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Shoah",
                        "Szoa",
                        "Szoah"
                    ],
                    "common_phrases": [
                        "ofiary Holokaustu",
                        "przeżyć Holokaust",
                        "pamięć o Holokauście"
                    ],
                    "target_lang_definitions": {
                        "ru": "Геноцид европейских евреев, осуществленный нацистской Германией во время Второй мировой войны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Холокост",
                                "target_lang_sense_clarification": "историческое событие"
                            },
                            {
                                "target_lang_word": "Шоа",
                                "target_lang_sense_clarification": "термин на иврите"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a5554429-3f53-3450-ba25-c92b844c7542",
                    "sense_definition": "Masowa zagłada, ludobójstwo lub całkowite zniszczenie jakiejś grupy istot, często używane w sensie metaforycznym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "general_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wizja wojny nuklearnej niesie ze sobą ryzyko, że nastąpi globalny <w>holokaust</w>.",
                            "target_lang_translations": {
                                "ru": "Перспектива ядерной войны несет в себе риск того, что наступит глобальный <w>холокост</w>."
                            }
                        },
                        {
                            "text": "Niektórzy aktywiści określają masową hodowlę przemysłową jako zwierzęcy <w>holokaust</w>.",
                            "target_lang_translations": {
                                "ru": "Некоторые активисты называют массовое промышленное животноводство <w>холокостом</w> животных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nuklearny holokaust",
                        "zwierzęcy holokaust"
                    ],
                    "target_lang_definitions": {
                        "ru": "Массовое уничтожение, геноцид или полная гибель какой-либо группы существ, часто используется в переносном смысле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "холокост",
                                "target_lang_sense_clarification": "массовое уничтожение"
                            },
                            {
                                "target_lang_word": "геноцид",
                                "target_lang_sense_clarification": "истребление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd1739cf-2987-3843-8f64-65cf818adab3",
                    "sense_definition": "W starożytności ofiara religijna, w której zwierzę było całkowicie spalane na ołtarzu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religious_sacrifice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kapłan przygotował ołtarz, na którym miał zostać złożony rytualny <w>holokaust</w>.",
                            "target_lang_translations": {
                                "ru": "Жрец подготовил алтарь, на котором должно было быть совершено ритуальное <w>всесожжение</w>."
                            }
                        },
                        {
                            "text": "W dawnych kulturach wierzono, że <w>holokaust</w> jest najdoskonalszą formą ofiary dla bóstwa.",
                            "target_lang_translations": {
                                "ru": "В древних культурах верили, что <w>всесожжение</w> является самой совершенной формой жертвы божеству."
                            }
                        }
                    ],
                    "synonyms": [
                        "całopalenie"
                    ],
                    "common_phrases": [
                        "ofiara holokaustu"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В древности религиозное жертвоприношение, при котором животное полностью сжигалось на алтаре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всесожжение",
                                "target_lang_sense_clarification": "религиозный обряд"
                            }
                        ]
                    }
                }
            ]
        }
    ]

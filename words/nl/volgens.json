    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "27e7dcd1-fbe0-3d5c-840d-13597070f468",
                    "sense_definition": "Geeft aan wie of wat de bron is van een bewering, mening of informatie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Alles is precies <w>volgens</w> plan verlopen.",
                            "target_lang_translations": {
                                "en": "Everything went exactly <w>according to</w> plan.",
                                "ru": "Всё прошло точно <w>по</w> плану."
                            }
                        },
                        {
                            "text": "<w>Volgens</w> de weersverwachting gaat het morgen regenen.",
                            "target_lang_translations": {
                                "en": "<w>According to</w> the weather forecast, it is going to rain tomorrow.",
                                "ru": "<w>Согласно</w> прогнозу погоды, завтра будет дождь."
                            }
                        },
                        {
                            "text": "Dat is <w>volgens</w> mij niet de juiste oplossing.",
                            "target_lang_translations": {
                                "en": "That is not the right solution <w>according to</w> me.",
                                "ru": "Это, <w>по</w> моему мнению, неправильное решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "luidens"
                    ],
                    "common_phrases": [
                        "volgens mij",
                        "volgens plan",
                        "volgens de regels",
                        "volgens afspraak"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates who or what is the source of a statement, opinion, or information.",
                        "ru": "Указывает, кто или что является источником утверждения, мнения или информации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "according to",
                                "target_lang_sense_clarification": "indicating source or conformity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "согласно",
                                "target_lang_sense_clarification": "в соответствии с чем-либо (правилами, планом, прогнозом)"
                            },
                            {
                                "target_lang_word": "по",
                                "target_lang_sense_clarification": "указывает на источник сведений или соответствие (по мнению, по плану)"
                            }
                        ]
                    }
                }
            ]
        },

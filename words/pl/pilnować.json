                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присматривать друг за другом",
                                "target_lang_sense_clarification": "взаимная забота и контроль"
                            },
                            {
                                "target_lang_word": "держаться вместе",

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9e48a2b5-0b41-3f4b-a02e-7f65baf81347",
                    "sense_definition": "Op deze of die manier; op de aangeduide wijze.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet het <w>zo</w> doen en niet anders.",
                            "target_lang_translations": {
                                "en": "You have to do it <w>like this</w> and not otherwise.",
                                "ru": "Ты должен сделать это <w>так</w>, а не иначе."
                            }
                        },
                        {
                            "text": "Is het <w>zo</w> goed, of wil je nog meer?",
                            "target_lang_translations": {
                                "en": "Is it good <w>like this</w>, or do you want more?",
                                "ru": "<w>Так</w> хорошо, или ты хочешь ещё?"
                            }
                        }
                    ],
                    "synonyms": [
                        "aldus",
                        "zodanig"
                    ],
                    "antonyms": [
                        "anders"
                    ],
                    "common_phrases": [
                        "zo is het",
                        "zo en zo",
                        "of zo"
                    ],
                    "target_lang_definitions": {
                        "en": "In this or that way; in the indicated manner.",
                        "ru": "Таким образом или так; указанным способом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "like this",
                                "target_lang_sense_clarification": "referring to a specific manner shown"
                            },
                            {
                                "target_lang_word": "like that",
                                "target_lang_sense_clarification": "referring to a manner previously mentioned"
                            },
                            {
                                "target_lang_word": "this way",
                                "target_lang_sense_clarification": "in this manner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "общим образом"
                            },
                            {
                                "target_lang_word": "таким образом",
                                "target_lang_sense_clarification": "более формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25c22c1d-4ef3-39e1-b1d7-3cbc19d245ea",
                    "sense_definition": "In die mate; heel erg.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is hier <w>zo</w> koud dat ik sta te beven.",
                            "target_lang_translations": {
                                "en": "It is <w>so</w> cold here that I'm shivering.",
                                "ru": "Здесь <w>так</w> холодно, что я дрожу."
                            }
                        },
                        {
                            "text": "Ik ben <w>zo</w> blij dat je er bent.",
                            "target_lang_translations": {
                                "en": "I am <w>so</w> glad you are here.",
                                "ru": "Я <w>так</w> рад, что ты здесь."
                            }
                        }
                    ],
                    "synonyms": [
                        "heel",
                        "erg",
                        "zeer",
                        "dermate"
                    ],
                    "common_phrases": [
                        "zo goed als",
                        "net zo"
                    ],
                    "target_lang_definitions": {
                        "en": "To that extent; very much.",
                        "ru": "В такой степени; очень."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "so",
                                "target_lang_sense_clarification": "intensifier"
                            },
                            {
                                "target_lang_word": "that",
                                "target_lang_sense_clarification": "to such a degree (e.g., not that good)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "усилительная частица"
                            },
                            {
                                "target_lang_word": "настолько",
                                "target_lang_sense_clarification": "до такой степени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bacfa1aa-779d-30e8-9e48-3c1772183e9f",
                    "sense_definition": "In gelijke mate (gebruikt in vergelijkingen).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is net <w>zo</w> groot als een huis.",
                            "target_lang_translations": {
                                "en": "He is just <w>as</w> big as a house.",
                                "ru": "Он <w>такой</w> же большой, как дом."
                            }
                        },
                        {
                            "text": "Het is niet <w>zo</w> moeilijk als het lijkt.",
                            "target_lang_translations": {
                                "en": "It is not <w>as</w> difficult as it seems.",
                                "ru": "Это не <w>так</w> сложно, как кажется."
                            }
                        }
                    ],
                    "synonyms": [
                        "even"
                    ],
                    "common_phrases": [
                        "zo ... als",
                        "net zo"
                    ],
                    "target_lang_definitions": {
                        "en": "To an equal extent (used in comparisons).",
                        "ru": "В равной степени (используется в сравнениях)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "as",
                                "target_lang_sense_clarification": "used in 'as...as' comparisons"
                            },
                            {
                                "target_lang_word": "so",
                                "target_lang_sense_clarification": "used in negative comparisons (not so...as)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "в сравнениях (так же... как)"
                            },
                            {
                                "target_lang_word": "такой",
                                "target_lang_sense_clarification": "в сравнениях с прилагательными (такой же... как)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44787726-49b4-3b0a-9e0a-161654cfe6a9",
                    "sense_definition": "Binnen korte tijd; onmiddellijk.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal je <w>zo</w> komen helpen, ik ben nu even bezig.",
                            "target_lang_translations": {
                                "en": "I will come help you <w>in a minute</w>, I'm busy right now.",
                                "ru": "Я приду помочь тебе <w>сейчас</w>, я пока занят."
                            }
                        },
                        {
                            "text": "Wacht even, ik ben <w>zo</w> terug.",
                            "target_lang_translations": {
                                "en": "Wait a moment, I'll be back <w>soon</w>.",
                                "ru": "Подожди, я <w>сейчас</w> вернусь."
                            }
                        }
                    ],
                    "synonyms": [
                        "straks",
                        "dadelijk",
                        "gauw",
                        "binnenkort"
                    ],
                    "common_phrases": [
                        "tot zo",
                        "zo meteen"
                    ],
                    "target_lang_definitions": {
                        "en": "Within a short time; immediately.",
                        "ru": "В течение короткого времени; немедленно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "soon",
                                "target_lang_sense_clarification": "in the near future"
                            },
                            {
                                "target_lang_word": "in a minute",
                                "target_lang_sense_clarification": "very shortly"
                            },
                            {
                                "target_lang_word": "in a moment",
                                "target_lang_sense_clarification": "very shortly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сейчас",
                                "target_lang_sense_clarification": "очень скоро, с минуты на минуту"
                            },
                            {
                                "target_lang_word": "скоро",
                                "target_lang_sense_clarification": "в ближайшем будущем"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "c9c829cf-9f42-30be-ba2f-2a7c2093abed",
                    "sense_definition": "Uiting van verbazing, instemming of om een handeling af te sluiten.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zo</w>, ben je daar eindelijk, waar bleef je nou?",
                            "target_lang_translations": {
                                "en": "<w>Well</w>, you're finally here, where were you?",
                                "ru": "<w>Ну</w>, наконец-то ты здесь, где ты был?"
                            }
                        },
                        {
                            "text": "<w>Zo</w>, dat klusje is ook weer geklaard.",
                            "target_lang_translations": {
                                "en": "<w>Right</w>, that job is done too.",
                                "ru": "<w>Так</w>, с этой работой тоже покончено."
                            }
                        }
                    ],
                    "synonyms": [
                        "hèhè",
                        "nou"
                    ],
                    "common_phrases": [
                        "zo zo"
                    ],
                    "target_lang_definitions": {
                        "en": "Expression of surprise, agreement, or to conclude an action.",
                        "ru": "Выражение удивления, согласия или завершения действия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "right",
                                "target_lang_sense_clarification": "concluding an action"
                            },
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "expressing surprise or transition"
                            },
                            {
                                "target_lang_word": "there",
                                "target_lang_sense_clarification": "satisfaction at completion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "при завершении действия или переходе"
                            },
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "выражение удивления или вступление"
                            },
                            {
                                "target_lang_word": "вот",
                                "target_lang_sense_clarification": "удовлетворение от сделанного"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "99ed9bdf-79fa-3827-b075-a4c27e153c42",
                    "sense_definition": "Indien; in het geval dat.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zo</w> mogelijk zou ik eerst nog even naar de wc willen.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f75b80ca-c2f4-3571-acdf-592d117817c7",
                    "sense_definition": "Pleasing to the eye or mind, especially regarding physical appearance.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pleasing_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a very <w>attractive</w> dress to the party.",
                            "target_lang_translations": {
                                "ru": "Она надела очень <w>привлекательное</w> платье на вечеринку."
                            }
                        },
                        {
                            "text": "He is an <w>attractive</w> man with a kind smile.",
                            "target_lang_translations": {
                                "ru": "Он <w>привлекательный</w> мужчина с доброй улыбкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "beautiful",
                        "appealing",
                        "charming",
                        "alluring",
                        "fetching"
                    ],
                    "antonyms": [
                        "ugly",
                        "unattractive",
                        "repulsive"
                    ],
                    "common_phrases": [
                        "very attractive",
                        "attractive person",
                        "find someone attractive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приятный для глаз или ума, особенно в отношении внешности; вызывающий симпатию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привлекательный",
                                "target_lang_sense_clarification": "общий термин для описания приятной внешности"
                            },
                            {
                                "target_lang_word": "симпатичный",
                                "target_lang_sense_clarification": "миловидный, располагающий к себе (часто о людях)"
                            },
                            {
                                "target_lang_word": "притягательный",
                                "target_lang_sense_clarification": "манящий, обладающий особым шармом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4aab97d-e5cb-3f81-89b3-49cad08a8cd3",
                    "sense_definition": "Interesting or worth having because of agreeable qualities.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pleasing_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company made a very <w>attractive</w> offer for the house.",
                            "target_lang_translations": {
                                "ru": "Компания сделала очень <w>привлекательное</w> предложение по покупке дома."
                            }
                        },
                        {
                            "text": "We found an <w>attractive</w> price for the flight tickets.",
                            "target_lang_translations": {
                                "ru": "Мы нашли <w>привлекательную</w> цену на авиабилеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "desirable",
                        "tempting",
                        "inviting",
                        "enticing",
                        "appealing"
                    ],
                    "antonyms": [
                        "unattractive",
                        "unenticing"
                    ],
                    "common_phrases": [
                        "attractive offer",
                        "attractive price",
                        "attractive option"
                    ],
                    "target_lang_definitions": {
                        "ru": "Интересный или желаемый благодаря своим положительным качествам или выгоде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привлекательный",
                                "target_lang_sense_clarification": "о предложении, цене или перспективе"
                            },
                            {
                                "target_lang_word": "заманчивый",
                                "target_lang_sense_clarification": "обещающий удовольствие или выгоду"
                            },
                            {
                                "target_lang_word": "выгодный",
                                "target_lang_sense_clarification": "приносящий пользу или прибыль (о цене/сделке)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ca9170a-bd2c-3b03-a5bf-96b31ba89205",
                    "sense_definition": "Having the power to pull or draw things closer by physical force.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gravity is an <w>attractive</w> force between two objects.",
                            "target_lang_translations": {
                                "ru": "Гравитация — это <w>притягивающая</w> сила между двумя объектами."
                            }

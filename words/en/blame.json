                {
                    "sense_id": "bd38b496-84f5-3e4c-88fc-a57dcfa8ea9e",
                    "sense_definition": "To say or think that someone is responsible for something bad happening.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "assign_responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The driver was <w>blamed</w> for the accident because he was texting."
                        },
                        {
                            "text": "I do not <w>blame</w> you for being angry about the delay."
                        }
                    ],
                    "synonyms": [

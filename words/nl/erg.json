                            {
                                "target_lang_word": "severe",
                                "target_lang_sense_clarification": "e.g. severe pain"
                            },
                            {
                                "target_lang_word": "serious",
                                "target_lang_sense_clarification": "e.g. a serious problem"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "серьёзный",
                                "target_lang_sense_clarification": "ошибки, проблемы"

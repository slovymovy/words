                {
                    "sense_id": "9450fbca-3927-3652-994c-d74c5deb0db5",
                    "sense_definition": "Ciąg zdarzeń w utworze fabularnym (powieści, filmie) dotyczący określonych postaci lub problemów, składający się na akcję.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "story_thread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten film ma bardzo ciekawy <w>wątek</w> kryminalny, który trzyma w napięciu do samego końca.",
                            "target_lang_translations": {
                                "ru": "В этом фильме очень интересная криминальная <w>сюжетная линия</w>, которая держит в напряжении до самого конца."
                            }
                        },
                        {
                            "text": "Autor wprowadził do powieści <w>wątek</w> miłosny, aby urozmaicić fabułę.",
                            "target_lang_translations": {
                                "ru": "Автор ввел в роман любовную <w>линию</w>, чтобы разнообразить сюжет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wątek główny",
                        "wątek poboczny",
                        "wątek miłosny",
                        "zgubić wątek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последовательность событий в художественном произведении (романе, фильме), касающаяся определенных персонажей или проблем и составляющая сюжет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сюжетная линия",
                                "target_lang_sense_clarification": "развитие событий"
                            },
                            {
                                "target_lang_word": "линия",
                                "target_lang_sense_clarification": "часть сюжета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14763b09-3ff1-3299-a3fd-75a8ff8ed847",
                    "sense_definition": "Ciąg powiązanych tematycznie wiadomości na forum internetowym lub w dyskusji online.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "internet_thread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę założyć nowy <w>wątek</w> na forum, żeby uzyskać pomoc w rozwiązaniu problemu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно создать новую <w>тему</w> на форуме, чтобы получить помощь в решении проблемы."
                            }
                        },
                        {
                            "text": "Przeczytałem cały <w>wątek</w> dyskusji, ale nie znalazłem odpowiedzi na moje pytanie.",
                            "target_lang_translations": {
                                "ru": "Я прочитал всю <w>ветку</w> обсуждения, но не нашел ответа на свой вопрос."
                            }
                        }

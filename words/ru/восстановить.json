    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "531f0db8-9ff3-3df6-9a5e-31a2710d12c7",
                    "sense_definition": "Привести в прежнее состояние, вернуть целость, работоспособность или статус.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи смогли полностью <восстановить> здоровье пациента после тяжёлой болезни.",
                            "target_lang_translations": {
                                "en": "The doctors were able to fully <restore> the patient's health after a severe illness."
                            }
                        },
                        {
                            "text": "Городские власти обещали <восстановить> разрушенный мост к началу осени.",
                            "target_lang_translations": {
                                "en": "The city authorities promised to <restore> the destroyed bridge by the beginning of autumn."
                            }
                        }
                    ],
                    "synonyms": [
                        "возобновить",
                        "вернуть",
                        "реставрировать",
                        "воссоздать"
                    ],
                    "antonyms": [
                        "разрушить"
                    ],
                    "common_phrases": [
                        "восстановить здоровье",
                        "восстановить силы",
                        "восстановить документы",
                        "восстановить справедливость",
                        "восстановить пароль"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring back to a former state, return integrity, functionality, or status."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "restore",
                                "target_lang_sense_clarification": "general meaning: health, buildings, justice"
                            },
                            {
                                "target_lang_word": "recover",
                                "target_lang_sense_clarification": "data, documents, or health"
                            },
                            {
                                "target_lang_word": "reestablish",
                                "target_lang_sense_clarification": "relations, order, or connection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd1ae2d6-40f9-3236-b96b-c5a75549e204",
                    "sense_definition": "Враждебно настроить кого-либо по отношению к кому-либо или чему-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Своими резкими словами он <восстановил> против себя всех коллег в офисе.",
                            "target_lang_translations": {
                                "en": "With his harsh words, he <turned> all his colleagues in the office against himself."
                            }
                        },
                        {
                            "text": "Интриги помогли <восстановить> общественное мнение против нового закона.",
                            "target_lang_translations": {
                                "en": "Intrigues helped <turn> public opinion against the new law."
                            }
                        }
                    ],
                    "synonyms": [
                        "поссорить"
                    ],
                    "antonyms": [
                        "примирить"
                    ],
                    "common_phrases": [
                        "восстановить против себя"
                    ],
                    "target_lang_definitions": {
                        "en": "To make someone hostile towards someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn against",
                                "target_lang_sense_clarification": "to cause hostility"
                            },
                            {
                                "target_lang_word": "set against",
                                "target_lang_sense_clarification": "to provoke conflict"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cf24a22-51a3-33a2-afe1-eb5283f3e64e",
                    "sense_definition": "В химии: изменить степень окисления атомов, присоединив к ним электроны.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чтобы получить чистый металл, необходимо <восстановить> его из руды при высокой температуре.",
                            "target_lang_translations": {
                                "en": "To obtain pure metal, it is necessary to <reduce> it from ore at high temperature."
                            }
                        },
                        {
                            "text": "Студент должен был <восстановить> оксид меди в ходе лабораторной работы.",
                            "target_lang_translations": {
                                "en": "The student had to <reduce> copper oxide during the lab work."
                            }
                        }
                    ],
                    "antonyms": [
                        "окислить"
                    ],
                    "common_phrases": [
                        "восстановить металл",
                        "восстановить оксид"
                    ],
                    "target_lang_definitions": {
                        "en": "In chemistry: to change the oxidation state of atoms by adding electrons to them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reduce",
                                "target_lang_sense_clarification": "chemical reaction"
                            }
                        ]
                    }
                }
            ]
        }
    ]

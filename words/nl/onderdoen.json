                {
                    "sense_id": "9ce37a70-058d-3c94-a871-7d03ead66eb5",
                    "sense_definition": "Niet zo goed zijn als iemand of iets anders; van mindere kwaliteit zijn of minder presteren in een vergelijking.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "comparison_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij wil qua prestaties beslist niet voor zijn broer <w>onderdoen</w>.",
                            "target_lang_translations": {
                                "ru": "Он определенно не хочет <w>уступать</w> своему брату в результатах."
                            }
                        },
                        {
                            "text": "De kwaliteit van dit huismerk <w>doet</w> niet <w>onder</w> voor die van de bekende merken.",
                            "target_lang_translations": {
                                "ru": "Качество этой собственной марки не <w>уступает</w> известным брендам."
                            }
                        }
                    ],
                    "synonyms": [
                        "achterblijven",
                        "tekortschieten"
                    ],
                    "antonyms": [
                        "overtreffen",
                        "uitblinken"
                    ],
                    "common_phrases": [
                        "niet onderdoen voor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть хуже кого-то или чего-то другого; уступать в качестве или результатах при сравнении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уступать",
                                "target_lang_sense_clarification": "быть хуже по качеству или способностям"
                            },
                            {
                                "target_lang_word": "быть хуже",
                                "target_lang_sense_clarification": "общее значение сравнения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2530e4b7-81a6-3b00-906b-c7d22da2ef8e",
                    "sense_definition": "Iets aan de onderkant van de voeten bevestigen, specifiek gebruikt voor schaatsen of ski's.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_attachment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zodra het ijs dik genoeg is, kunnen we de schaatsen <w>onderdoen</w>.",
                            "target_lang_translations": {
                                "ru": "Как только лед станет достаточно толстым, мы сможем <w>надеть</w> коньки."
                            }
                        },
                        {
                            "text": "Je moet je ski's goed <w>onderdoen</w> voordat je de berg afgaat.",
                            "target_lang_translations": {
                                "ru": "Нужно хорошо <w>пристегнуть</w> лыжи, прежде чем спускаться с горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderbinden"
                    ],
                    "antonyms": [
                        "afdoen",
                        "uitdoen"
                    ],
                    "common_phrases": [
                        "schaatsen onderdoen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикреплять что-либо к ногам снизу, обычно используется в отношении коньков или лыж."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надевать",
                                "target_lang_sense_clarification": "о коньках или лыжах"
                            },
                            {
                                "target_lang_word": "пристегивать",
                                "target_lang_sense_clarification": "закреплять на ногах"

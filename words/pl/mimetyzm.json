    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "31274b13-0a2b-313f-9eba-ad9aef732ef9",
                    "sense_definition": "Upodabnianie się organizmu do otoczenia, na przykład barwą lub kształtem, w celu ochrony przed drapieżnikami.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "biology_adaptation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten owad stosuje doskonały <w>mimetyzm</w>, dzięki czemu jest niewidoczny na liściach.",
                            "target_lang_translations": {
                                "ru": "Это насекомое использует идеальную <w>мимикрию</w>, благодаря чему оно невидимо на листьях."
                            }
                        },
                        {
                            "text": "Dzięki <w>mimetyzmowi</w> wiele zwierząt unika ataku ze strony drapieżników.",
                            "target_lang_translations": {
                                "ru": "Благодаря <w>мимикрии</w> многие животные избегают нападения хищников."
                            }
                        }
                    ],
                    "synonyms": [
                        "kamuflaż",
                        "maskowanie"
                    ],
                    "common_phrases": [
                        "mimetyzm barwny",
                        "zjawisko mimetyzmu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сходство организма с окружающей средой, например, цветом или формой, для защиты от хищников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мимикрия",
                                "target_lang_sense_clarification": "биология"
                            },
                            {
                                "target_lang_word": "миметизм",
                                "target_lang_sense_clarification": "научный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e40ce57-77a6-3d46-a007-27ee3ac0a161",
                    "sense_definition": "Naśladowanie lub wierne kopiowanie rzeczywistości w dziele sztuki lub literaturze.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "art_aesthetics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej powieści widoczny jest silny <w>mimetyzm</w> w opisie życia codziennego bohaterów.",
                            "target_lang_translations": {
                                "ru": "В этом романе заметен сильный <w>мимесис</w> в описании повседневной жизни героев."
                            }
                        },
                        {
                            "text": "Współczesna sztuka często odrzuca klasyczny <w>mimetyzm</w> na rzecz abstrakcji.",
                            "target_lang_translations": {
                                "ru": "Современное искусство часто отвергает классический <w>мимесис</w> в пользу абстракции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mimetyzm w sztuce",
                        "zasada mimetyzmu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подражание или точное воспроизведение действительности в произведении искусства или литературе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мимесис",
                                "target_lang_sense_clarification": "искусство, литература"
                            },
                            {
                                "target_lang_word": "подражание",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eccc5dcb-62b3-3709-914d-2f544c4c7924",
                    "sense_definition": "Nieświadoma skłonność do naśladowania zachowań, gestów lub sposobu bycia osób z otoczenia.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "psychology_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego zachowanie to typowy <w>mimetyzm</w>, gdyż bezwiednie powtarza gesty swojego szefa.",
                            "target_lang_translations": {
                                "ru": "Его поведение — это типичное <w>подражательство</w>, так как он бессознательно повторяет жесты своего начальника."
                            }
                        },
                        {
                            "text": "Dzieci często wykazują <w>mimetyzm</w>, naśladując postawy dorosłych.",
                            "target_lang_translations": {
                                "ru": "Дети часто проявляют <w>подражательство</w>, копируя поведение взрослых."
                            }
                        }
                    ],
                    "synonyms": [
                        "efekt kameleona"
                    ],
                    "common_phrases": [
                        "mimetyzm społeczny",
                        "mimetyzm zachowań"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бессознательная склонность подражать поведению, жестам или манерам окружающих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подражательство",
                                "target_lang_sense_clarification": "поведение"
                            },
                            {
                                "target_lang_word": "имитация",
                                "target_lang_sense_clarification": "психология"
                            }
                        ]
                    }
                }
            ]
        }

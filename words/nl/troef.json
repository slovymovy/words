                    "sense_id": "8d2a728c-c1f9-3530-b734-36832aa09360",
                    "sense_definition": "Een speelkaart van een kleur die in een spelronde een hogere waarde heeft dan de andere kleuren.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "card_game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Harten is deze ronde <w>troef</w>.",
                            "target_lang_translations": {
                                "en": "Hearts is <w>trump</w> this round.",
                                "ru": "Черви в этом раунде — <w>козырь</w>."
                            }
                        },
                        {
                            "text": "Hij speelde zijn laatste <w>troef</w> en won de slag.",
                            "target_lang_translations": {
                                "en": "He played his last <w>trump</w> and won the trick.",
                                "ru": "Он сыграл своим последним <w>козырем</w> и выиграл взятку."
                            }
                        }
                    ],
                    "synonyms": [
                        "troefkaart"
                    ],
                    "common_phrases": [
                        "troef zijn",
                        "troef bekennen",
                        "troef maken"
                    ],
                    "target_lang_definitions": {
                        "en": "A playing card of a suit that ranks higher than other suits in a game round.",
                        "ru": "Игральная карта масти, которая в данном раунде игры старше других мастей."
                    },
                    "translations": {

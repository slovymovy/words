                },
                {
                    "sense_id": "8ad69ca9-9cdc-3754-980f-61c306d86976",
                    "sense_definition": "Forma słowa <w>rano</w> (dopełniacz liczby pojedynczej).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czekam na telefon od samego <rana>.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dd1ee1c0-469b-30b3-9e6b-985e20315fd1",
                    "sense_definition": "The quality of being very beautiful, impressive, and large in scale.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "magnificence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The palace was restored to its former <w>grandeur</w> after the war.",
                            "target_lang_translations": {
                                "ru": "Дворцу вернули его былое <w>великолепие</w> после войны."
                            }
                        },
                        {
                            "text": "He suffered from delusions of <w>grandeur</w>, believing he was the king of the world.",
                            "target_lang_translations": {
                                "ru": "Он страдал манией <w>величия</w>, считая себя королем мира."
                            }
                        },
                        {
                            "text": "We stood in silence, admiring the scenic <w>grandeur</w> of the canyon.",
                            "target_lang_translations": {
                                "ru": "Мы стояли в тишине, любуясь живописным <w>величием</w> каньона."
                            }
                        }
                    ],
                    "synonyms": [
                        "magnificence",
                        "splendor",
                        "majesty",
                        "opulence",
                        "glory"
                    ],
                    "antonyms": [
                        "insignificance",
                        "modesty",
                        "simplicity",
                        "ordinariness"
                    ],
                    "common_phrases": [
                        "delusions of grandeur",
                        "scenic grandeur",
                        "faded grandeur",
                        "restore grandeur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Впечатляющая красота, масштабность и великолепие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "великолепие",
                                "target_lang_sense_clarification": "роскошь, пышная красота"
                            },
                            {
                                "target_lang_word": "величие",
                                "target_lang_sense_clarification": "торжественная красота, мощь"
                            },
                            {
                                "target_lang_word": "величественность",
                                "target_lang_sense_clarification": "впечатляющий внешний вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a38a491-dc4b-3902-9c99-d6f3f6a1153b",
                    "sense_definition": "High rank, social importance, or noble character.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nobility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Despite his poverty, the old man maintained a certain <w>grandeur</w> of manner.",
                            "target_lang_translations": {
                                "ru": "Несмотря на бедность, старик сохранял определенную <w>величественность</w> манер."
                            }
                        },
                        {
                            "text": "She faced her difficult situation with quiet <w>grandeur</w> and dignity.",
                            "target_lang_translations": {
                                "ru": "Она встретила свою тяжелую ситуацию с тихим <w>благородством</w> и достоинством."
                            }
                        }
                    ],
                    "synonyms": [
                        "nobility",
                        "dignity",
                        "stateliness",
                        "eminence"
                    ],
                    "antonyms": [
                        "baseness",
                        "humbleness"
                    ],
                    "common_phrases": [
                        "moral grandeur",
                        "grandeur of soul"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокое положение, важность или благородство характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благородство",
                                "target_lang_sense_clarification": "высокие моральные качества"
                            },
                            {
                                "target_lang_word": "знатность",
                                "target_lang_sense_clarification": "высокое происхождение"
                            },
                            {
                                "target_lang_word": "величие",
                                "target_lang_sense_clarification": "достоинство в поведении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
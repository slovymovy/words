{
    "word_family": [
        "stealer",
        "stealth",
        "stealthy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "de036b6e-1315-33ac-8f36-d864cd828ba4",
                    "sense_definition": "To take something that belongs to someone else illegally or without permission, without intending to return it.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "theft_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Someone <w>stole</w> my bicycle while I was inside the shop.",
                            "target_lang_translations": {
                                "ru": "Кто-то <w>украл</w> мой велосипед, пока я был в магазине."
                            }
                        },
                        {
                            "text": "It is wrong to <w>steal</w> money from your parents.",
                            "target_lang_translations": {
                                "ru": "<w>Воровать</w> деньги у родителей — это плохо."
                            }
                        },
                        {
                            "text": "The thieves <w>stole</w> several valuable paintings from the gallery.",
                            "target_lang_translations": {
                                "ru": "Воры <w>украли</w> несколько ценных картин из галереи."
                            }
                        }
                    ],
                    "synonyms": [
                        "thieve",
                        "swipe",
                        "shoplift",
                        "filch",
                        "pilfer",
                        "purloin"
                    ],
                    "antonyms": [
                        "buy",
                        "purchase",
                        "return",
                        "donate"
                    ],
                    "common_phrases": [
                        "steal money",
                        "steal a car",
                        "steal from someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать чужое имущество незаконно или без разрешения, не намереваясь его возвращать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красть",
                                "target_lang_sense_clarification": "несовершенный вид, общее название действия"
                            },
                            {
                                "target_lang_word": "украсть",
                                "target_lang_sense_clarification": "совершенный вид, завершенное действие"
                            },
                            {
                                "target_lang_word": "воровать",
                                "target_lang_sense_clarification": "заниматься кражами систематически"
                            },
                            {
                                "target_lang_word": "стащить",
                                "target_lang_sense_clarification": "разговорное, совершить кражу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4e8903c-df8d-33aa-a4de-43fc3a2487a6",
                    "sense_definition": "To borrow something for a very short time, usually in a casual or friendly context.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "borrow_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can I <w>steal</w> your pen for a second to write this down?",
                            "target_lang_translations": {
                                "ru": "Могу я <w>стащить</w> твою ручку на секунду, чтобы записать это?"
                            }
                        },
                        {
                            "text": "I need to <w>steal</w> a few minutes of your time to discuss the project.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>украсть</w> у вас несколько минут, чтобы обсудить проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "borrow"
                    ],
                    "common_phrases": [
                        "steal a minute",
                        "steal a pen",
                        "steal a chair"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in casual conversation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Взять что-то на очень короткое время, обычно в дружеской или непринужденной обстановке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взять",
                                "target_lang_sense_clarification": "самый распространенный вариант"
                            },
                            {
                                "target_lang_word": "стащить",
                                "target_lang_sense_clarification": "шутливое, взять без спроса"
                            },
                            {
                                "target_lang_word": "одолжить",
                                "target_lang_sense_clarification": "взять во временное пользование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68cbab4e-dd93-39dd-8e35-ff546d88f397",
                    "sense_definition": "To attract the most attention and praise in a performance or event, often unexpectedly.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attention_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The supporting actor managed to <w>steal</w> the show with his hilarious performance.",
                            "target_lang_translations": {
                                "ru": "Актёру второго плана удалось <w>затмить всех</w> своим уморительным выступлением."
                            }
                        },
                        {
                            "text": "She didn't mean to <w>steal</w> the spotlight from the bride on her wedding day.",
                            "target_lang_translations": {
                                "ru": "Она не хотела <w>перетягивать внимание</w> на себя от невесты в день её свадьбы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "steal the show",
                        "steal the spotlight",
                        "steal the scene"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привлечь к себе всё внимание и похвалу на выступлении или мероприятии, часто неожиданно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затмить всех",
                                "target_lang_sense_clarification": "стать заметнее других"
                            },
                            {
                                "target_lang_word": "перетянуть внимание",
                                "target_lang_sense_clarification": "отвлечь внимание на себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9562268c-224b-352f-9d7e-1be96f5b46b1",
                    "sense_definition": "To move quietly and secretly so that no one sees or hears you.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>stole</w> out of the room without waking the baby.",
                            "target_lang_translations": {
                                "ru": "Она <w>выскользнула</w> из комнаты, не разбудив ребенка."
                            }
                        },
                        {
                            "text": "As evening approached, shadows began to <w>steal</w> across the garden.",
                            "target_lang_translations": {
                                "ru": "С приближением вечера тени начали <w>прокрадываться</w> по саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "sneak",
                        "creep",
                        "slip"
                    ],
                    "common_phrases": [
                        "steal away",
                        "steal out",
                        "steal into"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться тихо и тайно, чтобы никто не увидел и не услышал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокрасться",
                                "target_lang_sense_clarification": "пройти незаметно"
                            },
                            {
                                "target_lang_word": "проскользнуть",
                                "target_lang_sense_clarification": "быстро и незаметно пройти"
                            },
                            {
                                "target_lang_word": "выскользнуть",
                                "target_lang_sense_clarification": "незаметно выйти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86ee6ef0-8ca2-3369-bd3c-1550a7e63837",
                    "sense_definition": "To do something quickly and surreptitiously, often involving looking or touching.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surreptitious_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>stole</w> a glance at the answers during the difficult test.",
                            "target_lang_translations": {
                                "ru": "Он <w>украдкой взглянул</w> на ответы во время сложного теста."
                            }
                        },
                        {
                            "text": "The couple managed to <w>steal</w> a kiss when no one was watching.",
                            "target_lang_translations": {
                                "ru": "Паре удалось <w>украсть</w> поцелуй, когда никто не смотрел."
                            }
                        }
                    ],
                    "synonyms": [
                        "snatch"
                    ],
                    "common_phrases": [
                        "steal a glance",
                        "steal a look",
                        "steal a kiss"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать что-то быстро и тайком, часто касаясь взгляда или прикосновения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сделать украдкой",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "улучить",
                                "target_lang_sense_clarification": "найти момент для действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2da552f-d621-304c-b9ae-599bc7921cff",
                    "sense_definition": "To advance safely to another base in baseball without the aid of a hit or error.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sports_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The runner tried to <w>steal</w> second base but was tagged out.",
                            "target_lang_translations": {
                                "ru": "Бегущий попытался <w>украсть</w> вторую базу, но его осалили."
                            }
                        },
                        {
                            "text": "He is fast enough to <w>steal</w> bases frequently during the season.",
                            "target_lang_translations": {
                                "ru": "Он достаточно быстр, чтобы часто <w>красть</w> базы в течение сезона."
                            }
                        }
                    ],
                    "common_phrases": [
                        "steal a base",
                        "steal second",
                        "steal home"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "sports (baseball)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В бейсболе: безопасно перебежать на другую базу без помощи удара битой или ошибки соперника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украсть",
                                "target_lang_sense_clarification": "спортивный термин (украсть базу)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4927c50f-ce85-3906-bb2b-175549060001",
                    "sense_definition": "A product or service acquired at a very low and attractive price; a bargain.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "bargain_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Getting this designer coat for only twenty dollars was a real <w>steal</w>.",
                            "target_lang_translations": {
                                "ru": "Купить это дизайнерское пальто всего за двадцать долларов было настоящей <w>находкой</w>."
                            }
                        },
                        {
                            "text": "At that low price, this car is an absolute <w>steal</w>.",
                            "target_lang_translations": {
                                "ru": "По такой низкой цене эта машина — просто <w>подарок</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bargain"
                    ],
                    "antonyms": [
                        "rip-off"
                    ],
                    "common_phrases": [
                        "absolute steal",
                        "real steal",
                        "what a steal"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Товар или услуга, приобретенные по очень низкой и привлекательной цене; выгодная сделка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выгодная покупка",
                                "target_lang_sense_clarification": "удачное приобретение"
                            },
                            {
                                "target_lang_word": "находка",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "подарок",
                                "target_lang_sense_clarification": "очень дешево, почти даром"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27f38461-a346-3654-8ae6-453a934dd3fc",
                    "sense_definition": "An act of taking possession of the ball or puck from an opponent in sports.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The point guard made a quick <w>steal</w> and scored a layup.",
                            "target_lang_translations": {
                                "ru": "Разыгрывающий совершил быстрый <w>перехват</w> и забил лей-ап."
                            }
                        },
                        {
                            "text": "His defensive play led to a crucial <w>steal</w> in the final minute of the game.",
                            "target_lang_translations": {
                                "ru": "Его игра в защите привела к решающему <w>перехвату</w> на последней минуте матча."
                            }
                        }
                    ],
                    "common_phrases": [
                        "make a steal"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по отбору мяча или шайбы у соперника в спорте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перехват",
                                "target_lang_sense_clarification": "спортивный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
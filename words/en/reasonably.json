            "senses": [
                {
                    "sense_id": "f27e4d2b-0dc0-3309-8c95-3971e9d4f682",
                    "sense_definition": "To a moderate or acceptable degree; fairly or satisfactorily.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "degree_moderation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The food at the restaurant was delicious and <w>reasonably</w> priced.",
                            "target_lang_translations": {
                                "ru": "Еда в ресторане была вкусной и стоила <w>приемлемо</w>."
                            }
                        },
                        {
                            "text": "I am <w>reasonably</w> sure that I left my keys on the table.",
                            "target_lang_translations": {
                                "ru": "Я <w>довольно</w> уверен, что оставил ключи на столе."
                            }
                        }
                    ],
                    "synonyms": [
                        "fairly",
                        "quite",
                        "moderately",
                        "adequately"
                    ],
                    "antonyms": [
                        "extremely",
                        "excessively",
                        "totally"
                    ],
                    "common_phrases": [
                        "reasonably priced",
                        "reasonably well",
                        "reasonably sure",
                        "reasonably good"
                    ],
                    "target_lang_definitions": {
                        "ru": "В умеренной или приемлемой степени; довольно или удовлетворительно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "довольно",
                                "target_lang_sense_clarification": "в значительной степени"
                            },
                            {
                                "target_lang_word": "умеренно",
                                "target_lang_sense_clarification": "не чрезмерно"
                            },
                            {
                                "target_lang_word": "приемлемо",
                                "target_lang_sense_clarification": "удовлетворительно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "253736cd-0609-35f8-956c-d2e52665ba99",
                    "sense_definition": "In a sensible, logical, or fair way that shows good judgment.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "manner_logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to sit down and discuss this problem <w>reasonably</w>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно сесть и <w>разумно</w> обсудить эту проблему."
                            }
                        },
                        {
                            "text": "She behaved <w>reasonably</w> even though she was very upset.",
                            "target_lang_translations": {
                                "ru": "Она вела себя <w>разумно</w>, хотя и была очень расстроена."
                            }
                        }
                    ],
                    "synonyms": [
                        "sensibly",
                        "logically",
                        "rationally"
                    ],
                    "antonyms": [
                        "unreasonably",
                        "irrationally"
                    ],
                    "common_phrases": [
                        "act reasonably",
                        "behave reasonably",
                        "discuss reasonably"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разумным, логичным или справедливым образом; здраво."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разумно",

{
    "word_family": [
        "który",
        "którykolwiek"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "66855ab2-03be-37d1-a59e-4020b8dfb4ac",
                    "sense_definition": "Oznacza bliżej nieokreśloną osobę lub rzecz wybraną z pewnej grupy; pewien, jakiś, jeden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zrobił to <w>któryś</w> z nich, ale nie wiem dokładnie który.",
                            "target_lang_translations": {
                                "ru": "Это сделал <w>кто-то</w> из них, но я не знаю точно, кто."
                            }
                        },
                        {
                            "text": "Czy <w>któraś</w> z was mogłaby mi pomóc w kuchni?",
                            "target_lang_translations": {
                                "ru": "Могла бы <w>кто-нибудь</w> из вас помочь мне на кухне?"
                            }
                        },
                        {
                            "text": "W <w>którymś</w> momencie wszyscy zrozumieliśmy swój błąd.",
                            "target_lang_translations": {
                                "ru": "В <w>какой-то</w> момент мы все поняли свою ошибку."
                            }
                        }
                    ],
                    "synonyms": [
                        "dowolny"
                    ],
                    "common_phrases": [
                        "któryś z nich",
                        "któraś z was",
                        "w którymś momencie",
                        "któryś dzień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает неопределенное лицо или предмет, выбранный из группы; какой-то, один из."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "какой-то",
                                "target_lang_sense_clarification": "неопределенный объект или лицо"
                            },
                            {
                                "target_lang_word": "кто-то",
                                "target_lang_sense_clarification": "о людях, часто с предлогом «из»"
                            },
                            {
                                "target_lang_word": "один",
                                "target_lang_sense_clarification": "выделение одного из группы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1491dee9-0dff-37d2-9f85-3611bda9e2b3",
                    "sense_definition": "Określa przybliżony, nieznany dokładnie numer porządkowy lub krotność zdarzenia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To zdarzyło się już <w>któryś</w> raz z rzędu i zaczyna mnie to denerwować.",
                            "target_lang_translations": {
                                "ru": "Это случилось уже в <w>который</w> раз подряд, и это начинает меня раздражать."
                            }
                        },
                        {
                            "text": "Dopiero dwudziesta <w>któraś</w> osoba zgodziła się wziąć udział w ankiecie.",
                            "target_lang_translations": {
                                "ru": "Только двадцать <w>какой-то</w> человек согласился принять участие в опросе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "któryś raz",
                        "któryś z kolei"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает приблизительный, точно не известный порядковый номер или повторяемость события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "который",
                                "target_lang_sense_clarification": "в значении «уже который», многократный"
                            },
                            {
                                "target_lang_word": "какой-то",
                                "target_lang_sense_clarification": "приблизительный номер"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a5ba1eae-7623-31d1-b6e7-29dc79eb45c3",
                    "sense_definition": "Uit het zicht raken of weggaan van een bepaalde plek.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het konijn <w>verdween</w> snel in zijn hol.",
                            "target_lang_translations": {
                                "en": "The rabbit quickly <w>disappeared</w> into its burrow.",
                                "ru": "Кролик быстро <w>исчез</w> в своей норе."
                            }
                        },
                        {
                            "text": "De zon <w>verdween</w> langzaam achter de donkere wolken.",
                            "target_lang_translations": {
                                "en": "The sun slowly <w>disappeared</w> behind the dark clouds.",
                                "ru": "Солнце медленно <w>скрылось</w> за темными облаками."
                            }
                        }
                    ],
                    "synonyms": [
                        "weggaan",
                        "vertrekken"
                    ],
                    "antonyms": [
                        "verschijnen",
                        "opduiken",
                        "komen"
                    ],
                    "common_phrases": [
                        "uit het zicht verdwijnen",
                        "in het niets verdwijnen",
                        "spoorloos verdwijnen"
                    ],
                    "target_lang_definitions": {
                        "en": "To go out of sight or leave a certain place.",
                        "ru": "Скрыться из виду или уйти с определенного места."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disappear",
                                "target_lang_sense_clarification": "to go out of view"
                            },
                            {
                                "target_lang_word": "vanish",
                                "target_lang_sense_clarification": "to disappear suddenly or completely"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "исчезать",
                                "target_lang_sense_clarification": "перестать быть видимым"
                            },
                            {
                                "target_lang_word": "скрываться",
                                "target_lang_sense_clarification": "уйти из поля зрения или спрятаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88d88117-46c3-3be8-a1cd-5df88f2412b5",
                    "sense_definition": "Ophouden te bestaan of niet meer aanwezig zijn.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aan het eind van het krijt <w>verdwenen</w> de dinosauriërs.",
                            "target_lang_translations": {
                                "en": "At the end of the Cretaceous period, the dinosaurs <w>disappeared</w>.",
                                "ru": "В конце мелового периода динозавры <w>исчезли</w>."
                            }
                        },
                        {
                            "text": "De vlekken in mijn shirt zijn na het wassen <w>verdwenen</w>.",
                            "target_lang_translations": {
                                "en": "The stains in my shirt <w>disappeared</w> after washing.",
                                "ru": "Пятна на моей рубашке <w>исчезли</w> после стирки."
                            }
                        }
                    ],
                    "synonyms": [
                        "vergaan",
                        "wegraken",
                        "tenietgaan"
                    ],
                    "antonyms": [
                        "ontstaan",
                        "blijven",
                        "verschijnen"
                    ],
                    "common_phrases": [
                        "van de aardbodem verdwijnen",
                        "plotseling verdwijnen"
                    ],
                    "target_lang_definitions": {
                        "en": "To cease to exist or no longer be present.",
                        "ru": "Перестать существовать или больше не присутствовать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disappear",
                                "target_lang_sense_clarification": "to stop existing"
                            },
                            {
                                "target_lang_word": "vanish",
                                "target_lang_sense_clarification": "to cease to exist completely"
                            },
                            {
                                "target_lang_word": "die out",
                                "target_lang_sense_clarification": "to become extinct (of a group or species)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "исчезать",
                                "target_lang_sense_clarification": "прекратить существование"
                            },
                            {
                                "target_lang_word": "пропадать",
                                "target_lang_sense_clarification": "потеряться или отсутствовать"

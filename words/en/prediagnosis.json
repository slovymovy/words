                {
                    "sense_id": "0ac061a8-89d2-3b19-a566-718dc5d79596",
                    "sense_definition": "Occurring or existing before a medical diagnosis is formally made.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

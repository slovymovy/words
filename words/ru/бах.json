    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e453f46f-06b6-3be9-8d53-4de3078e00db",
                    "sense_definition": "Звукоподражание резкому, громкому и отрывистому звуку, например выстрелу, взрыву или падению тяжелого предмета.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_imitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вдруг за стеной раздался громкий <w>бах</w>, и всё стихло.",
                            "target_lang_translations": {
                                "en": "Suddenly, a loud <w>bang</w> was heard behind the wall, and everything went quiet."
                            }
                        },
                        {
                            "text": "Книга соскользнула со стола и упала на пол: <w>бах</w>!",
                            "target_lang_translations": {
                                "en": "The book slid off the table and fell to the floor: <w>bang</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "бабах",
                        "бух",
                        "бац"
                    ],
                    "common_phrases": [
                        "и тут бах",
                        "громкий бах"
                    ],
                    "target_lang_definitions": {
                        "en": "A word imitating a sharp, loud, and abrupt sound, such as a gunshot, explosion, or the fall of a heavy object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bang",
                                "target_lang_sense_clarification": "sudden loud noise"
                            },
                            {
                                "target_lang_word": "boom",
                                "target_lang_sense_clarification": "deep resonant sound"
                            }
                        ]
                    }
                }
            ]
        },

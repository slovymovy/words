{
    "word_family": [
        "hepać",
        "hepnięcie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "15c22d1c-388a-38c9-b137-f10792a49823",
                    "sense_definition": "Głośno wypuścić powietrze z żołądka przez usta; wydać dźwięk podobny do beknięcia.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "bodily_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czasem mi się zdarza, że ukradkiem <w>hepnę</w> po wypiciu coca-coli.",
                            "target_lang_translations": {
                                "ru": "Иногда случается, что я украдкой <w>рыгну</w> после того, как выпью кока-колы."
                            }
                        },
                        {
                            "text": "Nie wypada głośno <w>hepnąć</w> przy stole podczas uroczystego obiadu.",
                            "target_lang_translations": {
                                "ru": "Не подобает громко <w>рыгнуть</w> за столом во время торжественного обеда."
                            }
                        }
                    ],
                    "synonyms": [
                        "beknąć",
                        "odbić się"
                    ],
                    "common_phrases": [
                        "głośno hepnąć",
                        "hepnąć po jedzeniu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "regional",
                            "comment": "często spotykane w gwarach, np. śląskiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Громко выпустить воздух из желудка через рот; издать звук, похожий на отрыжку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыгнуть",
                                "target_lang_sense_clarification": "издать звук отрыжки"
                            },
                            {
                                "target_lang_word": "отрыгнуть",
                                "target_lang_sense_clarification": "выпустить воздух из желудка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14872c2f-5d0a-32ba-ac58-232d1ef11d11",
                    "sense_definition": "Rzucić czymś gwałtownie, często z hałasem, lub uderzyć czymś o coś.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przypływie złości <w>hepnęła</w> ciężką torbą o podłogę.",
                            "target_lang_translations": {
                                "ru": "В порыве злости она <w>грохнула</w> тяжелой сумкой об пол."
                            }
                        },
                        {
                            "text": "Musisz mocno <w>hepnąć</w> tym workiem, żeby się dobrze ułożył.",
                            "target_lang_translations": {
                                "ru": "Нужно сильно <w>бахнуть</w> этим мешком, чтобы он хорошо улегся."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzucić",
                        "cisnąć",
                        "walnąć"
                    ],
                    "antonyms": [
                        "podnieść",
                        "chwycić"
                    ],
                    "common_phrases": [
                        "hepnąć o ziemię"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "głównie gwara śląska i okoliczne"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бросить что-то резко, часто с шумом, или ударить чем-то обо что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грохнуть",
                                "target_lang_sense_clarification": "бросить с шумом или ударить"
                            },
                            {
                                "target_lang_word": "швырнуть",
                                "target_lang_sense_clarification": "резко бросить"
                            },
                            {
                                "target_lang_word": "бахнуть",
                                "target_lang_sense_clarification": "ударить с громким звуком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98b34b78-f15a-30da-8ce5-0e029303df6b",
                    "sense_definition": "Surowo kogoś upomnieć lub skrzyczeć za niewłaściwe zachowanie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verbal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ojciec <w>hepnął</w> na syna, gdy ten wrócił do domu bardzo późno.",
                            "target_lang_translations": {
                                "ru": "Отец <w>рявкнул</w> на сына, когда тот вернулся домой очень поздно."
                            }
                        },
                        {
                            "text": "Nauczycielka <w>hepnęła</w> uczniów za hałasowanie na lekcji.",
                            "target_lang_translations": {
                                "ru": "Учительница <w>прикрикнула</w> на учеников за шум на уроке."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrzyczeć",
                        "zbesztać",
                        "ofuknąć"
                    ],
                    "antonyms": [
                        "pochwalić"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Строго сделать выговор или накричать на кого-то за неподобающее поведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рявкнуть",
                                "target_lang_sense_clarification": "резко крикнуть"
                            },
                            {
                                "target_lang_word": "прикрикнуть",
                                "target_lang_sense_clarification": "строго окликнуть или повысить голос"
                            },
                            {
                                "target_lang_word": "накричать",
                                "target_lang_sense_clarification": "громко отругать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
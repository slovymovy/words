                },
                {
                    "sense_id": "cd13d40e-c0d1-3af5-b35f-e940097ecded",
                    "sense_definition": "An unexpected event that causes trouble, sadness, or damage.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Losing his job was a terrible <w>blow</w> to his confidence.",
                            "target_lang_translations": {
                                "ru": "Потеря работы стала ужасным <w>ударом</w> по его уверенности."
                            }
                        },
                        {
                            "text": "The news came as a crushing <w>blow</w> to the team.",
                            "target_lang_translations": {
                                "ru": "Эта новость стала сокрушительным <w>ударом</w> для команды."
                            }
                        }
                    ],
                    "synonyms": [
                        "setback",
                        "shock",
                        "disaster",
                        "misfortune"
                    ],
                    "common_phrases": [
                        "crushing blow",
                        "devastating blow",
                        "soften the blow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неожиданное событие, причиняющее неприятности, горе или ущерб."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удар",
                                "target_lang_sense_clarification": "потрясение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7b2b0f3-5181-36ba-981a-afc96301398a",
                    "sense_definition": "A strong wind or storm.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [

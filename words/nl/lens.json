    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "90b0d9a3-12c6-3c7b-aa1b-3fe573629889",
                    "sense_definition": "Een zeer klein, dun plaatje van kunststof dat direct op de oogbol wordt gedragen om het gezichtsvermogen te corrigeren in plaats van een bril.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "optical_aids",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij draagt liever een <w>lens</w> dan een bril.",
                            "target_lang_translations": {
                                "en": "She prefers wearing a <w>contact lens</w> to glasses."
                            }
                        },
                        {
                            "text": "Mijn rechter <w>lens</w> is per ongeluk uit mijn oog gevallen.",
                            "target_lang_translations": {
                                "en": "My right <w>contact lens</w> accidentally fell out of my eye."
                            }
                        }
                    ],
                    "synonyms": [
                        "contactlens"
                    ],
                    "common_phrases": [
                        "lenzen indoen",
                        "lenzen uitdoen",
                        "zachte lenzen",
                        "harde lenzen"
                    ],
                    "target_lang_definitions": {
                        "en": "A very small, thin piece of plastic worn directly on the eyeball to correct vision instead of glasses."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "contact lens",
                                "target_lang_sense_clarification": "vision correction"
                            },
                            {
                                "target_lang_word": "contact",
                                "target_lang_sense_clarification": "short form"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ae71570-59e8-39b2-baac-47c64f93e3b7",
                    "sense_definition": "Een geslepen stuk glas of transparant materiaal dat lichtstralen breekt en van richting doet veranderen, gebruikt in optische instrumenten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "optics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>lens</w> van de microscoop moet voorzichtig worden schoongemaakt.",
                            "target_lang_translations": {
                                "en": "The <w>lens</w> of the microscope must be cleaned carefully."
                            }
                        },
                        {
                            "text": "Door de bolle <w>lens</w> lijkt het kleine insect veel groter.",
                            "target_lang_translations": {
                                "en": "Through the convex <w>lens</w>, the small insect looks much bigger."
                            }
                        }
                    ],
                    "common_phrases": [
                        "holle lens",
                        "bolle lens",
                        "optische lens"
                    ],
                    "target_lang_definitions": {
                        "en": "A ground piece of glass or transparent material that refracts light rays and changes their direction, used in optical instruments."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lens",
                                "target_lang_sense_clarification": "optical component"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aadf0e34-88bb-34d2-8a83-d3fd32978ed9",
                    "sense_definition": "Het onderdeel van een camera, bestaande uit één of meer lenzen, dat het beeld opvangt en projecteert.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "photography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fotograaf kocht een nieuwe <w>lens</w> om betere portretten te maken.",
                            "target_lang_translations": {
                                "en": "The photographer bought a new <w>lens</w> to take better portraits."
                            }
                        },
                        {
                            "text": "Met deze <w>lens</w> kun je heel ver inzoomen op het landschap.",
                            "target_lang_translations": {
                                "en": "With this <w>lens</w> you can zoom in very far on the landscape."
                            }
                        }
                    ],
                    "synonyms": [
                        "objectief"
                    ],
                    "common_phrases": [
                        "door de lens kijken",
                        "de lens verwisselen",
                        "een foto nemen"
                    ],
                    "target_lang_definitions": {
                        "en": "The part of a camera, consisting of one or more lenses, that captures the image and projects it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lens",
                                "target_lang_sense_clarification": "camera part"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1671f75-6e66-3880-96bc-49d3496dc954",
                    "sense_definition": "Het transparante, flexibele deel in het oog dat zich achter de pupil bevindt en zorgt voor de scherpstelling van beelden op het netvlies.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij staar wordt de <w>lens</w> van het oog langzaam troebel.",
                            "target_lang_translations": {
                                "en": "With cataracts, the <w>lens</w> of the eye slowly becomes cloudy."
                            }
                        },
                        {
                            "text": "De <w>lens</w> verliest zijn flexibiliteit naarmate mensen ouder worden.",
                            "target_lang_translations": {
                                "en": "The <w>lens</w> loses its flexibility as people get older."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vertroebeling van de lens",
                        "accommodatie van de lens"
                    ],
                    "target_lang_definitions": {
                        "en": "The transparent, flexible part in the eye located behind the pupil that ensures the focusing of images on the retina."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lens",
                                "target_lang_sense_clarification": "anatomy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d43b9d8-1a70-30d6-822c-b39e7162ed40",
                    "sense_definition": "Een langgerekt wapen, vergelijkbaar met een harpoen, waarmee vroeger op walvissen werd gejaagd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De walvisvaarder hield de <w>lens</w> gereed om toe te slaan.",
                            "target_lang_translations": {
                                "en": "The whaler held the <w>lance</w> ready to strike."
                            }
                        },
                        {
                            "text": "In het museum ligt een oude <w>lens</w> die in de zeventiende eeuw werd gebruikt.",
                            "target_lang_translations": {
                                "en": "In the museum lies an old <w>lance</w> that was used in the seventeenth century."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical whaling term"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An elongated weapon, similar to a harpoon, used in the past to hunt whales."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lance",
                                "target_lang_sense_clarification": "whaling weapon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d586e01-f99d-39ae-86ba-b55050e8d186",
                    "sense_definition": "Een metalen pen of spie om onderdelen vast te zetten (verouderde nevenvorm van luns).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer zocht naar de <w>lens</w> om het wiel weer vast te zetten.",
                            "target_lang_translations": {
                                "en": "The farmer looked for the <w>linchpin</w> to fasten the wheel again."
                            }
                        },
                        {
                            "text": "Zonder de <w>lens</w> kan de as niet goed draaien.",
                            "target_lang_translations": {
                                "en": "Without the <w>linchpin</w>, the axle cannot turn properly."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "alternative form of luns"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A metal pin or wedge to fasten parts (obsolete variant of luns)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "linchpin",
                                "target_lang_sense_clarification": "fastener"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6038dae8-fac2-393d-992c-f5f21af14938",
                    "sense_definition": "Slap, leeg of krachtenloos; in de spreektaal vooral gebruikt als versterkend woord om aan te geven dat iemand fysiek of mentaal zwaar is aangetast.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij schrok zich <w>lens</w> toen er plotseling vuurwerk afging.",
                            "target_lang_translations": {
                                "en": "He was scared <w>stiff</w> when fireworks suddenly went off."
                            }
                        },
                        {
                            "text": "Als je zo doorgaat met die flauwe grappen, sla ik je <w>lens</w>.",
                            "target_lang_translations": {
                                "en": "If you keep going with those lame jokes, I'll beat you <w>senseless</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iemand lens slaan",
                        "zich lens schrikken",
                        "zich lens werken"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in specific expressions"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Limp, empty, or powerless; in spoken language mainly used as an intensifier to indicate that someone is physically or mentally severely affected."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "senseless",
                                "target_lang_sense_clarification": "beaten/scared state"
                            },
                            {
                                "target_lang_word": "empty",
                                "target_lang_sense_clarification": "drained/void"
                            },
                            {
                                "target_lang_word": "drained",
                                "target_lang_sense_clarification": "without energy"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "116563fb-82ad-33cd-bdf1-464d9c16cbce",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>lenzen</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>lens</w> het water uit de boot zodat we niet zinken.",
                            "target_lang_translations": {
                                "en": "I <w>pump</w> the water out of the boat so we don't sink."
                            }
                        },
                        {
                            "text": "Als ik de pomp bedien, <w>lens</w> ik het ruim.",
                            "target_lang_translations": {
                                "en": "When I operate the pump, I <w>empty</w> the hold."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'lenzen' (to pump out water)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pump out",
                                "target_lang_sense_clarification": "remove water"
                            },
                            {
                                "target_lang_word": "empty",
                                "target_lang_sense_clarification": "remove contents"
                            },
                            {
                                "target_lang_word": "bail",
                                "target_lang_sense_clarification": "remove water manually"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b8b8838-f42e-3d84-8a36-72da960780d8",
                    "sense_definition": "Gebiedende wijs van <w>lenzen</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Lens</w> de boot onmiddellijk, anders maken we water!",
                            "target_lang_translations": {
                                "en": "<w>Pump out</w> the boat immediately, otherwise we take on water!"
                            }
                        },
                        {
                            "text": "<w>Lens</w> dat water weg voordat het de motor bereikt.",
                            "target_lang_translations": {
                                "en": "<w>Pump</w> that water away before it reaches the engine."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative mood of 'lenzen' (to pump out water)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pump out",
                                "target_lang_sense_clarification": "remove water"
                            },
                            {
                                "target_lang_word": "empty",
                                "target_lang_sense_clarification": "remove contents"
                            }
                        ]
                    }
                }
            ]
        },
        {

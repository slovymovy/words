            "senses": [
                {
                    "sense_id": "66860ec0-174f-3da9-aeb4-2a3be616102a",
                    "sense_definition": "Geneigd tot langdradig, onzinnig praten of zeuren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn opa wordt soms wat <w>leuterig</w> als hij moe is en oude verhalen vertelt.",
                            "target_lang_translations": {
                                "en": "My grandpa sometimes gets a bit <w>rambling</w> when he is tired and tells old stories.",
                                "ru": "Мой дедушка иногда становится немного <w>занудным</w>, когда устает и рассказывает старые истории."
                            }
                        },
                        {
                            "text": "Ik heb geen geduld voor zijn <w>leuterige</w> opmerkingen tijdens de vergadering.",
                            "target_lang_translations": {
                                "en": "I have no patience for his <w>long-winded</w> remarks during the meeting.",
                                "ru": "У меня нет терпения слушать его <w>пустословные</w> замечания во время собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "zeurderig",
                        "zanikerig",
                        "kletserig"
                    ],
                    "antonyms": [
                        "zwijgzaam",
                        "bondig"
                    ],
                    "common_phrases": [
                        "een leuterig verhaal",
                        "leuterig gedoe"
                    ],
                    "target_lang_definitions": {
                        "en": "Inclined to long-winded, nonsensical talking or nagging.",
                        "ru": "Склонный к длинным, бессмысленным разговорам или нытью."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rambling",
                                "target_lang_sense_clarification": "talking in a confused or inconsequential way"
                            },
                            {
                                "target_lang_word": "waffling",
                                "target_lang_sense_clarification": "talking at length without saying anything important"
                            },
                            {
                                "target_lang_word": "long-winded",
                                "target_lang_sense_clarification": "tediously long in speaking"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "занудный",
                                "target_lang_sense_clarification": "надоедливый, скучный"
                            },
                            {
                                "target_lang_word": "брюзгливый",
                                "target_lang_sense_clarification": "склонный к ворчанию"
                            },
                            {
                                "target_lang_word": "пустомельный",
                                "target_lang_sense_clarification": "говорящий попусту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96a28a89-8768-3a28-8e47-cc9405e38463",
                    "sense_definition": "Onvast of wiebelend bewegend; heen en weer gaand.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude stoel stond <w>leuterig</w> op zijn ongelijke poten.",
                            "target_lang_translations": {
                                "en": "The old chair stood <w>wobbly</w> on its uneven legs.",
                                "ru": "Старый стул стоял <w>шатко</w> на своих неровных ножках."
                            }
                        },
                        {
                            "text": "Door de harde wind bewoog het riet <w>leuterig</w> langs de oever.",
                            "target_lang_translations": {
                                "en": "Due to the strong wind, the reeds moved <w>unsteadily</w> along the bank.",
                                "ru": "Из-за сильного ветра камыш <w>неустойчиво</w> качался вдоль берега."
                            }
                        }
                    ],
                    "synonyms": [

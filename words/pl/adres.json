            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a8af2486-be47-395c-ab7d-085117099e54",
                    "sense_definition": "Informacje o lokalizacji budynku lub miejscu zamieszkania osoby, obejmujące zwykle ulicę, numer domu, kod pocztowy i miasto.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy możesz mi podać swój aktualny <w>adres</w> zamieszkania?",
                            "target_lang_translations": {
                                "ru": "Ты можешь дать мне свой текущий домашний <w>адрес</w>?"
                            }
                        },
                        {
                            "text": "Wysłałem list na błędny <w>adres</w> i przesyłka do mnie wróciła.",
                            "target_lang_translations": {
                                "ru": "Я отправил письмо на неверный <w>адрес</w>, и посылка вернулась ко мне."
                            }
                        }
                    ],
                    "synonyms": [
                        "miejsce zamieszkania"
                    ],
                    "common_phrases": [
                        "adres zamieszkania",
                        "adres korespondencyjny",
                        "zmienić adres",
                        "podać adres"
                    ],
                    "target_lang_definitions": {
                        "ru": "Информация о местонахождении здания или месте жительства человека, обычно включающая улицу, номер дома, почтовый индекс и город."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адрес",
                                "target_lang_sense_clarification": "место жительства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1b3a621-f4a5-3db3-a47e-b06333eaa383",
                    "sense_definition": "Unikalny ciąg znaków umożliwiający identyfikację zasobu w sieci komputerowej, na przykład skrzynki pocztowej lub strony internetowej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę wpisać swój <w>adres</w> e-mail w formularzu rejestracyjnym.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, впишите свой <w>адрес</w> электронной почты в регистрационную форму."
                            }
                        },
                        {
                            "text": "Nie pamiętam, jaki jest dokładny <w>adres</w> tej strony internetowej.",
                            "target_lang_translations": {
                                "ru": "Я не помню, какой точный <w>адрес</w> у этого веб-сайта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "adres e-mail",
                        "adres strony",
                        "adres internetowy",
                        "adres IP"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уникальная строка символов, позволяющая идентифицировать ресурс в компьютерной сети, например, почтовый ящик или веб-сайт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адрес",
                                "target_lang_sense_clarification": "электронный или интернет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7acc8b31-da5d-3525-8714-d5d656c57aff",
                    "sense_definition": "Uroczyste, pisemne oświadczenie lub petycja skierowana do władz lub wybitnej osobistości.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "formal_document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Delegacja pracowników złożyła <w>adres</w> dziękczynny na ręce dyrektora.",
                            "target_lang_translations": {
                                "ru": "Делегация сотрудников вручила директору благодарственный <w>адрес</w>."
                            }
                        },
                        {
                            "text": "Mieszkańcy przygotowali <w>adres</w> do prezydenta miasta w sprawie ochrony parku.",
                            "target_lang_translations": {
                                "ru": "Жители подготовили <w>обращение</w> к мэру города по поводу защиты парка."
                            }

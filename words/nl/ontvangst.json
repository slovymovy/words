            "senses": [
                {
                    "sense_id": "efabed02-a1e4-340c-8792-61a804b4ee91",
                    "sense_definition": "Het in bezit krijgen van iets dat gestuurd of gegeven is, zoals post, geld of goederen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>ontvangst</w> van het geld verliep vlotjes en zonder problemen.",
                            "target_lang_translations": {
                                "ru": "<w>Получение</w> денег прошло гладко и без проблем.",
                                "en": "The <w>receipt</w> of the money went smoothly and without problems."
                            }
                        },
                        {
                            "text": "U moet de rekening betalen binnen veertien dagen na <w>ontvangst</w>.",
                            "target_lang_translations": {
                                "ru": "Вы должны оплатить счет в течение четырнадцати дней после <w>получения</w>.",
                                "en": "You must pay the bill within fourteen days of <w>receipt</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanname"
                    ],
                    "antonyms": [
                        "verzending"
                    ],
                    "common_phrases": [
                        "na ontvangst",
                        "bij ontvangst",
                        "bevestiging van ontvangst"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получение чего-либо отправленного или переданного, например почты, денег или товаров.",
                        "en": "The act of getting possession of something sent or given, such as mail, money, or goods."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получение",
                                "target_lang_sense_clarification": "процесс приобретения"
                            },
                            {
                                "target_lang_word": "принятие",
                                "target_lang_sense_clarification": "акт приема"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "receipt",
                                "target_lang_sense_clarification": "act of receiving"
                            },
                            {
                                "target_lang_word": "receiving",
                                "target_lang_sense_clarification": "process of getting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21d31954-a58c-3f7a-ba46-38adc1a893cd",
                    "sense_definition": "De manier waarop iemand wordt begroet of binnengehaald bij aankomst.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij onze <w>ontvangst</w> in het hotel kregen we allerlei lekkere hapjes.",
                            "target_lang_translations": {
                                "ru": "Во время <w>приёма</w> в отеле нас угостили всевозможными вкусными закусками.",
                                "en": "Upon our <w>reception</w> at the hotel, we were given all kinds of tasty snacks."
                            }
                        },
                        {
                            "text": "De koning kreeg een zeer warme <w>ontvangst</w> tijdens zijn bezoek aan de stad.",
                            "target_lang_translations": {
                                "ru": "Королю оказали очень теплый <w>приём</w> во время его визита в город.",
                                "en": "The king received a very warm <w>welcome</w> during his visit to the city."
                            }
                        }
                    ],
                    "synonyms": [
                        "onthaal",
                        "verwelkoming"
                    ],
                    "antonyms": [
                        "afscheid"
                    ],
                    "common_phrases": [
                        "warme ontvangst",
                        "hartelijke ontvangst",
                        "kille ontvangst"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, как кого-то приветствуют или встречают по прибытии.",
                        "en": "The way in which someone is greeted or welcomed upon arrival."

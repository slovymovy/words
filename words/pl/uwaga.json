{
    "word_family": [
        "uważać",
        "uważny",
        "uważnie",
        "zauważyć",
        "zauważalny",
        "nieuwaga"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a0ed060f-2baa-3855-9d4d-3959a4fc7aa6",
                    "sense_definition": "Skupienie myśli, wzroku lub słuchu na czymś; zdolność do koncentracji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "focus_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Student słuchał wykładu z wielką <uwagą>.",
                            "target_lang_translations": {
                                "ru": "Студент слушал лекцию с большим <вниманием>."
                            }
                        },
                        {
                            "text": "To trudne zadanie wymaga od nas szczególnej <uwagi>.",
                            "target_lang_translations": {
                                "ru": "Это трудное задание требует от нас особого <внимания>."
                            }
                        }
                    ],
                    "synonyms": [
                        "koncentracja",
                        "skupienie",
                        "uważność",
                        "atencja"
                    ],
                    "antonyms": [
                        "nieuwaga",
                        "rozproszenie"
                    ],
                    "common_phrases": [
                        "zwrócić uwagę",
                        "centrum uwagi",
                        "słuchać z uwagą",
                        "odwrócić uwagę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосредоточение мыслей, зрения или слуха на чем-либо; способность к концентрации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внимание",
                                "target_lang_sense_clarification": "сосредоточенность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b09f6c57-59fc-38be-a5eb-51f625ddd3d2",
                    "sense_definition": "Krótka wypowiedź, spostrzeżenie lub komentarz na jakiś temat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "comment_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor miał kilka cennych <uwag> do naszego projektu.",
                            "target_lang_translations": {
                                "ru": "У директора было несколько ценных <замечаний> к нашему проекту."
                            }
                        },
                        {
                            "text": "Jej złośliwa <uwaga> sprawiła mi przykrość.",
                            "target_lang_translations": {
                                "ru": "Её злобное <замечание> меня огорчило."
                            }
                        }
                    ],
                    "synonyms": [
                        "komentarz",
                        "spostrzeżenie",
                        "dopowiedzenie",
                        "adnotacja",
                        "przypis",
                        "uzupełnienie"
                    ],
                    "common_phrases": [
                        "cenna uwaga",
                        "trafna uwaga",
                        "wymiana uwag",
                        "mieć uwagi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткое высказывание, наблюдение или комментарий на какую-либо тему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замечание",
                                "target_lang_sense_clarification": "суждение, комментарий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45d4cf6f-8bf6-37b6-ae87-0776e25243bd",
                    "sense_definition": "Oficjalne upomnienie za niewłaściwe zachowanie, często wpisywane do dziennika szkolnego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reprimand_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel wpisał uczniowi <uwagę> za przeszkadzanie na lekcji.",
                            "target_lang_translations": {
                                "ru": "Учитель написал ученику <замечание> за то, что он мешал на уроке."
                            }
                        },
                        {
                            "text": "Dostałem <uwagę> za brak stroju gimnastycznego.",
                            "target_lang_translations": {
                                "ru": "Я получил <замечание> за отсутствие спортивной формы."
                            }
                        }
                    ],
                    "synonyms": [
                        "upomnienie",
                        "napomnienie",
                        "reprymenda",
                        "docinek"
                    ],
                    "antonyms": [
                        "pochwała"
                    ],
                    "common_phrases": [
                        "dostać uwagę",
                        "wpisać uwagę",
                        "zwrócić uwagę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное предупреждение за плохое поведение, часто записываемое в школьный дневник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замечание",
                                "target_lang_sense_clarification": "выговор, запись в дневнике"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e85a99b7-93d6-33e8-bbeb-295f532bfb28",
                    "sense_definition": "Okrzyk ostrzegający przed nagłym niebezpieczeństwem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "warning_exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Uwaga>, samochód nadjeżdża z dużą prędkością!",
                            "target_lang_translations": {
                                "ru": "<Осторожно>, машина приближается на большой скорости!"
                            }
                        },
                        {
                            "text": "<Uwaga> na głowę, sufit jest tutaj bardzo niski.",
                            "target_lang_translations": {
                                "ru": "<Осторожно> голову, потолок здесь очень низкий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uwaga na stopień",
                        "uwaga pies",
                        "zachować uwagę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, предупреждающий о внезапной опасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осторожно",
                                "target_lang_sense_clarification": "предупреждение об опасности"
                            },
                            {
                                "target_lang_word": "берегись",
                                "target_lang_sense_clarification": "резкое предупреждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93be5d5d-2d07-30ac-a325-32cd969781dc",
                    "sense_definition": "Okrzyk mający na celu przywołanie czyjegoś skupienia lub ogłoszenie komunikatu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attention_exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Uwaga>, pociąg wjeżdża na peron trzeci.",
                            "target_lang_translations": {
                                "ru": "<Внимание>, поезд прибывает на третью платформу."
                            }
                        },
                        {
                            "text": "<Uwaga>, proszę wszystkich o chwilę ciszy.",
                            "target_lang_translations": {
                                "ru": "<Внимание>, прошу всех о минуте тишины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prosimy o uwagę",
                        "chwila uwagi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, призванный привлечь чье-то внимание или объявить сообщение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внимание",
                                "target_lang_sense_clarification": "призыв слушать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
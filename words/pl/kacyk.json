{
    "word_family": [
        "kacykostwo"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ee778ba9-5630-3ebd-abb4-3024ebdf6436",
                    "sense_definition": "Lokalny urzędnik lub przywódca partyjny, który nadużywa swojej władzy dla prywatnych korzyści i rządzi w sposób despotyczny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lokalny <w>kacyk</w> decydował o wszystkich ważnych sprawach w miasteczku.",
                            "target_lang_translations": {
                                "ru": "Местный <w>царёк</w> решал все важные дела в городке."
                            }
                        },
                        {
                            "text": "Mieszkańcy mieli już dość rządów tego skorumpowanego <w>kacyka</w>.",
                            "target_lang_translations": {
                                "ru": "Жители уже устали от правления этого коррумпированного <w>царька</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lokalny kacyk",
                        "partyjny kacyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Местный чиновник или партийный лидер, злоупотребляющий властью ради личной выгоды и правящий деспотично."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "царёк",
                                "target_lang_sense_clarification": "местный деспот"
                            },
                            {
                                "target_lang_word": "бонза",
                                "target_lang_sense_clarification": "партийный лидер"
                            },
                            {
                                "target_lang_word": "кацик",
                                "target_lang_sense_clarification": "политический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03aba1a0-566e-309a-9805-3b8a15b1bbe3",
                    "sense_definition": "Wódz plemienia indiańskiego w Ameryce Środkowej i Południowej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hiszpańscy odkrywcy spotkali się z potężnym <w>kacykiem</w>, aby negocjować pokój.",
                            "target_lang_translations": {
                                "ru": "Испанские первооткрыватели встретились с могущественным <w>кациком</w>, чтобы договориться о мире."
                            }
                        },
                        {
                            "text": "Stary <w>kacyk</w> cieszył się wielkim szacunkiem wśród swoich ludzi.",
                            "target_lang_translations": {
                                "ru": "Старый <w>кацик</w> пользовался большим уважением среди своих людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "wódz"
                    ],
                    "common_phrases": [
                        "indiański kacyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вождь индейского племени в Центральной и Южной Америке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кацик",
                                "target_lang_sense_clarification": "титул вождя"
                            },
                            {
                                "target_lang_word": "вождь",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f6602f7-3854-316b-9edd-a7a3bb63f57c",
                    "sense_definition": "Ptak z rodziny kacykowatych zamieszkujący Amerykę, często o żółto-czarnym upierzeniu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W koronie drzewa dostrzegliśmy gniazdo budowane przez <w>kacyka</w>.",
                            "target_lang_translations": {
                                "ru": "В кроне дерева мы заметили гнездо, которое строил <w>кацик</w>."
                            }
                        },
                        {
                            "text": "Ten gatunek <w>kacyka</w> wyróżnia się pięknym śpiewem.",
                            "target_lang_translations": {
                                "ru": "Этот вид <w>кацика</w> отличается красивым пением."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Птица семейства трупиаловых, обитающая в Америке, часто с желто-черным оперением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кацик",
                                "target_lang_sense_clarification": "орнитология"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
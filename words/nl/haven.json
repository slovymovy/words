            "pos": "noun",
            "senses": [
                {
                    "sense_id": "02415529-58ed-3c40-bdee-c986e2cc6040",
                    "sense_definition": "Een natuurlijke of aangelegde plaats aan het water waar schepen kunnen aanleggen om te laden, te lossen of te schuilen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De grote schepen liggen veilig in de <w>haven</w> te wachten op hun volgende reis.",
                            "target_lang_translations": {
                                "ru": "Большие корабли безопасно стоят в <w>порту</w> в ожидании своего следующего рейса."
                            }
                        },
                        {
                            "text": "Rotterdam heeft een van de grootste en drukste <w>havens</w> ter wereld.",
                            "target_lang_translations": {
                                "ru": "Роттердам имеет один из самых больших и загруженных <w>портов</w> в мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanlegplaats"
                    ],
                    "common_phrases": [
                        "veilige haven",
                        "in de haven",
                        "de haven binnenlopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественное или искусственно созданное место у воды, где корабли могут причаливать для погрузки, разгрузки или укрытия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порт",
                                "target_lang_sense_clarification": "место стоянки и обслуживания судов"
                            },
                            {
                                "target_lang_word": "гавань",
                                "target_lang_sense_clarification": "защищенная часть акватории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b0819a0-94de-37c4-ac15-87d578918b3a",
                    "sense_definition": "Meervoud van <w>haaf</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude geschriften spreekt men soms over de woeste <w>havens</w> die de schepen moesten trotseren.",
                            "target_lang_translations": {
                                "ru": "В старых писаниях иногда говорят о бурных <w>морях</w>, которым должны были противостоять корабли."
                            }
                        },
                        {
                            "text": "De betekenis van deze <w>havens</w> is in het moderne Nederlands grotendeels verloren gegaan.",
                            "target_lang_translations": {
                                "ru": "Значение этих <w>морей</w> в современном нидерландском языке в значительной степени утрачено."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "refers to 'haaf' (sea/ocean)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от устаревшего слова \"haaf\" (открытое море, океан)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моря",
                                "target_lang_sense_clarification": "устаревшее значение"
                            }
                        ]
                    }
                }
            ]
        },

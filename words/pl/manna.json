                    "frequency": "High",
                    "semantic_group_id": "food_cereal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na śniadanie dzieci często jedzą <w>mannę</w> na mleku z sokiem malinowym."
                        },
                        {
                            "text": "Do przygotowania tego ciasta potrzebna będzie szklanka <w>manny</w>."
                        }
                    ],
                    "synonyms": [
                        "kasza manna",
                        "grysik",
                        "kaszka manna"
                    ],
                    "common_phrases": [

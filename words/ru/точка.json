                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "interjection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не пойду туда, и <w>точка</w>!",
                            "target_lang_translations": {
                                "en": "I am not going there, <w>period</w>!"
                            }

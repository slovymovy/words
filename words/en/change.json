{
    "word_family": [
        "exchange",
        "interchange",
        "changeable",
        "unchanging",
        "changeless",
        "changer"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17525a09-cd79-3f54-9226-5d9c4dbe0a2a",
                    "sense_definition": "To become different in nature, condition, or appearance.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Leaves <w>change</w> color in the autumn.",
                            "target_lang_translations": {
                                "ru": "Листья <w>меняют</w> цвет осенью."
                            }
                        },
                        {
                            "text": "My opinion on this matter will not <w>change</w>.",
                            "target_lang_translations": {
                                "ru": "Мое мнение по этому вопросу не <w>изменится</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "transform",
                        "variation",
                        "alter"
                    ],
                    "antonyms": [
                        "continuance",
                        "preservation"
                    ],
                    "common_phrases": [
                        "change for the better",
                        "times change",
                        "things change"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться другим по своей природе, состоянию или внешнему виду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "меняться",
                                "target_lang_sense_clarification": "становиться другим"
                            },
                            {
                                "target_lang_word": "изменяться",
                                "target_lang_sense_clarification": "приобретать новые свойства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "147a6a62-9baa-340f-a60d-1a2070b4898e",
                    "sense_definition": "To make someone or something different; to alter or modify.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not <w>change</w> the settings on the computer.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>меняйте</w> настройки на компьютере."
                            }
                        },
                        {
                            "text": "He decided to <w>change</w> his mind about the trip.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>изменить</w> свое решение насчет поездки."
                            }
                        }
                    ],
                    "synonyms": [
                        "alter",
                        "modify",
                        "transform"
                    ],
                    "antonyms": [
                        "conservation",
                        "preservation"
                    ],
                    "common_phrases": [
                        "change one's mind",
                        "change the subject",
                        "change the world"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать кого-то или что-то другим; изменять или модифицировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "менять",
                                "target_lang_sense_clarification": "делать иным"
                            },
                            {
                                "target_lang_word": "изменять",
                                "target_lang_sense_clarification": "вносить изменения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6ed6867-3fc3-3c58-8f17-8892b09daf47",
                    "sense_definition": "To remove one thing and put another in its place.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "replacement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>change</w> the batteries in the remote.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>поменять</w> батарейки в пульте."
                            }
                        },
                        {
                            "text": "Can you <w>change</w> the channel to the news?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>переключить</w> канал на новости?"
                            }
                        }
                    ],
                    "synonyms": [
                        "permutation",
                        "make another",
                        "transform"
                    ],
                    "common_phrases": [
                        "change the channel",
                        "change a tire",
                        "change a lightbulb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убрать одну вещь и поместить на её место другую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "менять",
                                "target_lang_sense_clarification": "заменять одно другим"
                            },
                            {
                                "target_lang_word": "заменять",
                                "target_lang_sense_clarification": "ставить что-то вместо другого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45e1689a-e864-3c83-8608-aab03b28f656",
                    "sense_definition": "To put on different clothes.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will go upstairs and <w>change</w> into something more comfortable.",
                            "target_lang_translations": {
                                "ru": "Я пойду наверх и <w>переоденусь</w> во что-нибудь более удобное."
                            }
                        },
                        {
                            "text": "She needs ten minutes to <w>change</w> before dinner.",
                            "target_lang_translations": {
                                "ru": "Ей нужно десять минут, чтобы <w>переодеться</w> перед ужином."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get changed",
                        "change into"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надевать другую одежду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переодеваться",
                                "target_lang_sense_clarification": "менять одежду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce41d560-d9bb-3de5-923f-e74b57647123",
                    "sense_definition": "To transfer from one vehicle or line of transport to another.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You have to <w>change</w> trains at the next station.",
                            "target_lang_translations": {
                                "ru": "Вам нужно <w>пересесть</w> на другой поезд на следующей станции."
                            }
                        },
                        {
                            "text": "We do not need to <w>change</w> planes to get to London.",
                            "target_lang_translations": {
                                "ru": "Нам не нужно <w>делать пересадку</w>, чтобы добраться до Лондона."
                            }
                        }
                    ],
                    "common_phrases": [
                        "change trains",
                        "change buses"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пересаживаться с одного транспортного средства или линии на другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пересаживаться",
                                "target_lang_sense_clarification": "переходить на другой транспорт"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6feb4e23-0cd4-3702-bc50-16843ac0fe38",
                    "sense_definition": "The process or result of becoming different.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to make a <w>change</w> to the plan.",
                            "target_lang_translations": {
                                "ru": "Нам нужно внести <w>изменение</w> в план."
                            }
                        },
                        {
                            "text": "The sudden <w>change</w> in weather surprised us.",
                            "target_lang_translations": {
                                "ru": "Внезапная <w>перемена</w> погоды удивила нас."
                            }
                        }
                    ],
                    "synonyms": [
                        "transformation",
                        "transition",
                        "alteration",
                        "modification",
                        "variation"
                    ],
                    "antonyms": [
                        "preservation",
                        "conservation"
                    ],
                    "common_phrases": [
                        "for a change",
                        "change of heart",
                        "climate change",
                        "subject to change"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс или результат того, что что-то становится другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изменение",
                                "target_lang_sense_clarification": "процесс или результат"
                            },
                            {
                                "target_lang_word": "перемена",
                                "target_lang_sense_clarification": "переход к новому состоянию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a4e6afa-49e4-3f5d-ac7b-6e0102f1d7b6",
                    "sense_definition": "The money returned when a payment exceeds the price of the purchase.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Here is your receipt and your <w>change</w>.",
                            "target_lang_translations": {
                                "ru": "Вот ваш чек и ваша <w>сдача</w>."
                            }
                        },
                        {
                            "text": "The cashier gave me the wrong <w>change</w>.",
                            "target_lang_translations": {
                                "ru": "Кассир дал мне неправильную <w>сдачу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "keep the change"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деньги, возвращаемые, когда платеж превышает стоимость покупки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдача",
                                "target_lang_sense_clarification": "возврат денег при покупке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66117291-5082-39c6-8123-1165ee3df595",
                    "sense_definition": "Money in the form of coins rather than paper notes.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you have any <w>change</w> for the parking meter?",
                            "target_lang_translations": {
                                "ru": "У вас есть <w>мелочь</w> для паркомата?"
                            }
                        },
                        {
                            "text": "My pockets are full of loose <w>change</w>.",
                            "target_lang_translations": {
                                "ru": "Мои карманы полны <w>мелочи</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "loose change",
                        "small change",
                        "exact change"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деньги в виде монет, а не бумажных купюр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелочь",
                                "target_lang_sense_clarification": "монеты"
                            },
                            {
                                "target_lang_word": "мелкие деньги",
                                "target_lang_sense_clarification": "разменная монета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d252e1a0-2e0a-3f5f-935c-a33a3c2e1c8c",
                    "sense_definition": "The act of replacing something with something else, often of the same kind.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "replacement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I packed a <w>change</w> of clothes in my bag.",
                            "target_lang_translations": {
                                "ru": "Я положил в сумку <w>сменную</w> одежду."
                            }
                        },
                        {
                            "text": "A <w>change</w> of scenery will do you good.",
                            "target_lang_translations": {
                                "ru": "<w>Смена</w> обстановки пойдет тебе на пользу."
                            }
                        }
                    ],
                    "synonyms": [
                        "alteration",
                        "modification"
                    ],
                    "common_phrases": [
                        "change of clothes",
                        "change of scenery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по замене чего-либо чем-то другим, часто того же вида."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смена",
                                "target_lang_sense_clarification": "замена, чередование"
                            },
                            {
                                "target_lang_word": "замена",
                                "target_lang_sense_clarification": "то, что заменяет"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "febc59f6-1467-380f-bf2d-1bdbdcf14d23",
                    "sense_definition": "Het bovenste, voorste of belangrijkste uiteinde van een voorwerp.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leader_top",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het diner zat vader plechtig aan het <w>hoofd</w> van de tafel.",
                            "target_lang_translations": {
                                "ru": "Во время ужина отец торжественно сидел во <w>главе</w> стола."
                            }
                        },
                        {
                            "text": "Het <w>hoofd</w> van de hamer vloog eraf tijdens het klussen.",
                            "target_lang_translations": {
                                "ru": "Во время работы <w>головка</w> молотка слетела."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hoofd van de tafel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя, передняя или главная часть предмета."

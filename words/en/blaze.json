{
    "word_family": [
        "ablaze",
        "blazer",
        "blazing"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc410aa6-838d-337b-bf53-74fda0d10e18",
                    "sense_definition": "A large, strong fire that burns brightly and fiercely.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fire_light_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The firefighters worked through the night to control the massive <w>blaze</w>.",
                            "target_lang_translations": {
                                "ru": "Пожарные работали всю ночь, чтобы взять под контроль сильный <w>пожар</w>."
                            }
                        },
                        {
                            "text": "We sat by the warm <w>blaze</w> of the campfire telling stories until midnight.",
                            "target_lang_translations": {
                                "ru": "Мы сидели у теплого <w>пламени</w> костра, рассказывая истории до полуночи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "roaring blaze",
                        "control the blaze",
                        "put out the blaze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой, сильный огонь, который горит ярко и неистово."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пламя",
                                "target_lang_sense_clarification": "яркий, сильный огонь"
                            },
                            {
                                "target_lang_word": "пожар",
                                "target_lang_sense_clarification": "крупное, разрушительное возгорание"
                            },
                            {
                                "target_lang_word": "огонь",
                                "target_lang_sense_clarification": "процесс горения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "898e1911-ae84-38ec-b52f-39fac9ac7a1a",
                    "sense_definition": "Very bright and intense light or heat.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fire_light_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She stepped out of the dark room into the sudden <w>blaze</w> of the midday sun.",
                            "target_lang_translations": {
                                "ru": "Она вышла из темной комнаты на внезапный <w>яркий свет</w> полуденного солнца."
                            }
                        },
                        {
                            "text": "The garden was a colorful <w>blaze</w> of blooming flowers in the spring.",
                            "target_lang_translations": {
                                "ru": "Весной сад представлял собой красочное <w>буйство</w> цветущих цветов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "blaze of sunlight",
                        "blaze of color",
                        "blaze of glory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень яркий и интенсивный свет или жар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сияние",
                                "target_lang_sense_clarification": "яркий свет, излучение"
                            },
                            {
                                "target_lang_word": "блеск",
                                "target_lang_sense_clarification": "яркое сверкание"
                            },
                            {
                                "target_lang_word": "буйство",
                                "target_lang_sense_clarification": "о красках, цвете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d52ca1d1-706a-3b3b-b23a-2ccbe4d34263",
                    "sense_definition": "A sudden and energetic display of emotion or activity.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie star arrived at the premiere in a <w>blaze</w> of publicity.",
                            "target_lang_translations": {
                                "ru": "Кинозвезда прибыла на премьеру, сопровождаемая <w>шумихой</w> в прессе."
                            }
                        },
                        {
                            "text": "He left the meeting in a sudden <w>blaze</w> of anger after the argument.",
                            "target_lang_translations": {
                                "ru": "Он покинул собрание во внезапной <w>вспышке</w> гнева после спора."
                            }
                        }
                    ],
                    "synonyms": [
                        "outburst"
                    ],
                    "common_phrases": [
                        "blaze of publicity",
                        "blaze of anger"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное и энергичное проявление эмоций или активности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вспышка",
                                "target_lang_sense_clarification": "внезапное проявление чувства"
                            },
                            {
                                "target_lang_word": "взрыв",
                                "target_lang_sense_clarification": "бурное проявление эмоций"
                            },
                            {
                                "target_lang_word": "шумиха",
                                "target_lang_sense_clarification": "публичный ажиотаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b657e45-fa01-36d7-b29a-5e77d33e766d",
                    "sense_definition": "A mark made on a tree or a sign placed to show a trail for hikers.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "marking_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hiker looked for the next painted <w>blaze</w> on the tree trunk to find the way.",
                            "target_lang_translations": {
                                "ru": "Турист искал следующую нарисованную <w>метку</w> на стволе дерева, чтобы найти дорогу."
                            }
                        },
                        {
                            "text": "We lost the path because the <w>blaze</w> was hidden by overgrown moss.",
                            "target_lang_translations": {
                                "ru": "Мы потеряли тропу, потому что <w>затёс</w> был скрыт разросшимся мхом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trail blaze",
                        "follow the blaze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Метка на дереве или знак, указывающий путь туристам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затёс",
                                "target_lang_sense_clarification": "метка, сделанная топором на дереве"
                            },
                            {
                                "target_lang_word": "метка",
                                "target_lang_sense_clarification": "знак для ориентирования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9f65009-ad5a-307e-97d1-8751bdd7fed7",
                    "sense_definition": "A very bright orange color used for high visibility, often for safety.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "color_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hunters wore vests of bright <w>blaze</w> to ensure they were seen in the woods.",
                            "target_lang_translations": {
                                "ru": "Охотники носили жилеты <w>ярко-оранжевого цвета</w>, чтобы их было видно в лесу."
                            }
                        },
                        {
                            "text": "Construction signs are often painted in <w>blaze</w> to warn drivers of danger.",
                            "target_lang_translations": {
                                "ru": "Строительные знаки часто окрашены в <w>сигнальный оранжевый</w>, чтобы предупредить водителей об опасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "safety orange",
                        "international orange"
                    ],
                    "common_phrases": [
                        "blaze orange"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень яркий оранжевый цвет, используемый для заметности, часто в целях безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ярко-оранжевый цвет",
                                "target_lang_sense_clarification": "цвет повышенной видимости"
                            },
                            {
                                "target_lang_word": "сигнальный оранжевый",
                                "target_lang_sense_clarification": "специальный цвет безопасности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81eec4a1-f521-32b4-9663-c6a04419c390",
                    "sense_definition": "A white mark or stripe on the face of a horse.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "marking_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The black stallion had a distinct white <w>blaze</w> running down its nose.",
                            "target_lang_translations": {
                                "ru": "У черного жеребца была отчетливая белая <w>проточина</w>, спускающаяся по носу."
                            }
                        },
                        {
                            "text": "You can recognize that pony by the crooked <w>blaze</w> on its forehead.",
                            "target_lang_translations": {
                                "ru": "Вы можете узнать этого пони по кривому <w>белому пятну</w> на лбу."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Белая отметина или полоса на морде лошади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проточина",
                                "target_lang_sense_clarification": "белая полоса на морде животного"
                            },
                            {
                                "target_lang_word": "белое пятно",
                                "target_lang_sense_clarification": "отметина"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0318ade2-a575-3d98-9740-89452cabfab5",
                    "sense_definition": "To burn strongly and brightly.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fire_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dry wood began to <w>blaze</w> as soon as the match touched it.",
                            "target_lang_translations": {
                                "ru": "Сухая древесина начала <w>пылать</w>, как только ее коснулась спичка."
                            }
                        },
                        {
                            "text": "A warm fire <w>blazed</w> in the hearth while it snowed outside.",
                            "target_lang_translations": {
                                "ru": "В очаге <w>полыхал</w> теплый огонь, пока на улице шел снег."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fire blazing",
                        "blaze away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гореть сильно и ярко."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пылать",
                                "target_lang_sense_clarification": "гореть ярким пламенем"
                            },
                            {
                                "target_lang_word": "полыхать",
                                "target_lang_sense_clarification": "гореть интенсивно, с размахом"
                            },
                            {
                                "target_lang_word": "гореть",
                                "target_lang_sense_clarification": "быть охваченным огнем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c352100a-029d-3f91-9a20-97d4def452fb",
                    "sense_definition": "To shine very brightly or be very conspicuous.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fire_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sun continued to <w>blaze</w> down on the hot desert sand.",
                            "target_lang_translations": {
                                "ru": "Солнце продолжало <w>палить</w> на горячий песок пустыни."
                            }
                        },
                        {
                            "text": "The city skyline began to <w>blaze</w> with lights as evening fell.",
                            "target_lang_translations": {
                                "ru": "С наступлением вечера горизонт города начал <w>сиять</w> огнями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "blaze with light",
                        "sun blazing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Светить очень ярко или быть очень заметным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сиять",
                                "target_lang_sense_clarification": "излучать яркий свет"
                            },
                            {
                                "target_lang_word": "сверкать",
                                "target_lang_sense_clarification": "ярко блестеть"
                            },
                            {
                                "target_lang_word": "палить",
                                "target_lang_sense_clarification": "о солнце: светить нестерпимо жарко"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84aff88e-51ef-3978-a8e7-3ee7a82e4d2d",
                    "sense_definition": "To be the first to do something and show others how to do it; to mark a new path.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "marking_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She helped <w>blaze</w> a trail for women in the field of science.",
                            "target_lang_translations": {
                                "ru": "Она помогла <w>проложить</w> путь женщинам в области науки."
                            }
                        },
                        {
                            "text": "The explorers had to <w>blaze</w> a path through the dense jungle vegetation.",
                            "target_lang_translations": {
                                "ru": "Исследователям пришлось <w>прорубать</w> путь сквозь густую растительность джунглей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "blaze a trail",
                        "blaze a path"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть первым в чем-то и показывать другим, как это делать; прокладывать новый путь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокладывать",
                                "target_lang_sense_clarification": "путь или тропу"
                            },
                            {
                                "target_lang_word": "прорубать",
                                "target_lang_sense_clarification": "путь через препятствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d66dcea1-7e04-3586-87df-312899280bc8",
                    "sense_definition": "To show sudden and intense anger.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His eyes started to <w>blaze</w> when he realized he had been tricked.",
                            "target_lang_translations": {
                                "ru": "Его глаза начали <w>сверкать</w>, когда он понял, что его обманули."
                            }
                        },
                        {
                            "text": "She <w>blazed</w> with fury at the unfair accusation.",
                            "target_lang_translations": {
                                "ru": "Она <w>пылала</w> от ярости из-за несправедливого обвинения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "blaze with anger",
                        "eyes blazing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявлять внезапный и сильный гнев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пылать",
                                "target_lang_sense_clarification": "гневом или яростью"
                            },
                            {
                                "target_lang_word": "сверкать",
                                "target_lang_sense_clarification": "глазами от злости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adde530c-ae64-32e5-b694-3249d63ac11e",
                    "sense_definition": "To smoke marijuana.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slang_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teenagers went behind the building to <w>blaze</w> before the concert started.",
                            "target_lang_translations": {
                                "ru": "Подростки пошли за здание <w>покурить</w> перед началом концерта."
                            }
                        },
                        {
                            "text": "He spends all his weekends just sitting around and <w>blazing</w>.",
                            "target_lang_translations": {
                                "ru": "Он проводит все выходные, просто сидя и <w>накуриваясь</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Курить марихуану."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курить",
                                "target_lang_sense_clarification": "травку (сленг)"
                            },
                            {
                                "target_lang_word": "дуть",
                                "target_lang_sense_clarification": "употреблять наркотики (сленг)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a649826e-a330-37bf-8021-75421f0a49bb",
                    "sense_definition": "A male given name.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We decided to name our new puppy <w>Blaze</w> because he runs so fast.",
                            "target_lang_translations": {
                                "ru": "Мы решили назвать нашего нового щенка <w>Блейз</w>, потому что он так быстро бегает."
                            }
                        },
                        {
                            "text": "My friend <w>Blaze</w> is coming over for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Мой друг <w>Блейз</w> придет сегодня на ужин."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Блейз",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
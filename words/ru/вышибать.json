    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "844a26bb-fa4b-3bee-8b57-8ccd0faeeaa6",
                    "sense_definition": "Резким ударом или толчком выбивать, выламывать что-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спасателям пришлось <вышибать> запертую дверь, чтобы помочь людям.",
                            "target_lang_translations": {
                                "en": "Rescuers had to <w>break down</w> the locked door to help the people."
                            }
                        },
                        {
                            "text": "В этой старой игре нужно <вышибать> фигуры деревянной битой.",
                            "target_lang_translations": {
                                "en": "In this old game, you need to <w>knock out</w> the figures with a wooden bat."
                            }
                        }
                    ],
                    "synonyms": [
                        "выбивать",
                        "выламывать"
                    ],
                    "antonyms": [
                        "вставлять"
                    ],
                    "common_phrases": [
                        "вышибать дверь",
                        "вышибать клин",
                        "клин клином вышибают"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To knock out or break something down with a sharp blow or push."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break down",
                                "target_lang_sense_clarification": "a door or barrier"
                            },
                            {
                                "target_lang_word": "knock out",
                                "target_lang_sense_clarification": "an object or game piece"
                            },
                            {
                                "target_lang_word": "kick down",
                                "target_lang_sense_clarification": "using one's foot"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "600ad078-66f1-3f46-b001-4592cebaf6a7",
                    "sense_definition": "Грубо выгонять кого-либо из помещения или здания.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_expulsion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охрана имеет право <вышибать> пьяных хулиганов из ночного клуба.",
                            "target_lang_translations": {
                                "en": "Security has the right to <w>throw out</w> drunk hooligans from the nightclub."
                            }
                        },
                        {
                            "text": "Не нужно меня <вышибать>, я сейчас сам уйду.",
                            "target_lang_translations": {
                                "en": "You don't need to <w>kick</w> me <w>out</w>, I'm leaving on my own now."
                            }
                        }
                    ],
                    "synonyms": [
                        "выгонять",
                        "вышвыривать",
                        "выставлять"
                    ],
                    "antonyms": [
                        "впускать",
                        "приглашать"
                    ],
                    "common_phrases": [
                        "вышибать за дверь",
                        "вышибать на улицу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "прост."
                        }
                    ],
                    "target_lang_definitions": {

            "senses": [
                {
                    "sense_id": "e8da9718-c3ff-311b-ab6a-ac6713a8fc8e",
                    "sense_definition": "Osoba pomagająca specjaliście przy wykonywanej przez niego pracy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "person_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>asystent</w> przygotuje dla ciebie wszystkie niezbędne dokumenty.",
                            "target_lang_translations": {
                                "ru": "Мой <w>ассистент</w> подготовит для тебя все необходимые документы."
                            }
                        },
                        {
                            "text": "<w>Asystent</w> burmistrza kontaktuje się z mediami w sprawach pilnych.",
                            "target_lang_translations": {
                                "ru": "<w>Помощник</w> мэра связывается со СМИ по срочным вопросам."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomocnik",
                        "prawa ręka"
                    ],
                    "antonyms": [
                        "przełożony",
                        "kierownik",
                        "pracodawca"
                    ],
                    "common_phrases": [
                        "asystent dyrektora",
                        "asystent osobisty",
                        "młodszy asystent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, помогающее специалисту в выполняемой им работе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ассистент",
                                "target_lang_sense_clarification": "помощник специалиста"
                            },
                            {
                                "target_lang_word": "помощник",
                                "target_lang_sense_clarification": "общая роль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9a38402-b195-3152-a18e-3009f1f6f167",
                    "sense_definition": "Stanowisko na wyższej uczelni lub w instytucie naukowym niższe od adiunkta, a także osoba je zajmująca.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ukończeniu studiów został na uniwersytecie jako <w>asystent</w> w katedrze historii.",
                            "target_lang_translations": {
                                "ru": "После окончания учебы он остался в университете как <w>ассистент</w> на кафедре истории."
                            }
                        },
                        {
                            "text": "Każdy <w>asystent</w> ma obowiązek prowadzić zajęcia ze studentami.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>ассистент</w> обязан проводить занятия со студентами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "asystent na uczelni",
                        "stanowisko asystenta",
                        "asystent naukowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Должность в высшем учебном заведении или научном институте ниже адъюнкта, а также лицо, занимающее эту должность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ассистент",
                                "target_lang_sense_clarification": "научная должность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8468f4f-ba6f-33c6-be5e-c1c7c48e9d22",
                    "sense_definition": "Program komputerowy ułatwiający użytkownikowi wykonanie określonej procedury krok po kroku.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "software",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Asystent</w> instalacji przeprowadzi cię przez proces konfiguracji programu.",
                            "target_lang_translations": {
                                "ru": "<w>Мастер</w> установки проведет вас через процесс настройки программы."
                            }
                        },
                        {
                            "text": "Jeśli nie wiesz, jak to zrobić, uruchom <w>asystenta</w> odzyskiwania danych.",
                            "target_lang_translations": {
                                "ru": "Если не знаешь, как это сделать, запусти <w>мастера</w> восстановления данных."
                            }
                        }
                    ],
                    "synonyms": [
                        "kreator"
                    ],
                    "common_phrases": [
                        "asystent instalacji",
                        "asystent pobierania",
                        "asystent aktualizacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа, облегчающая пользователю выполнение определенной процедуры шаг за шагом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "в программном обеспечении (напр. мастер установки)"
                            },
                            {
                                "target_lang_word": "помощник",
                                "target_lang_sense_clarification": "цифровой ассистент"
                            }
                        ]
                    }
                }
            ]
        }
    ]

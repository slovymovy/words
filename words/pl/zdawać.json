    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "641ed84d-171f-37f7-aac0-6839568e3e99",
                    "sense_definition": "Przystępować do egzaminu lub sprawdzianu w celu uzyskania oceny lub kwalifikacji (proces pisania lub odpowiadania).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym tygodniu będę <zdawać> trudny egzamin z matematyki.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе я буду <сдавать> сложный экзамен по математике."
                            }
                        },
                        {
                            "text": "Wszyscy uczniowie muszą <zdawać> maturę, aby dostać się na studia.",
                            "target_lang_translations": {
                                "ru": "Все ученики должны <сдавать> выпускные экзамены, чтобы поступить в вуз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zdawać egzamin",
                        "zdawać maturę",
                        "zdawać na studia",
                        "zdawać na prawo jazdy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить экзамен или тест для получения оценки или квалификации (процесс сдачи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "экзамен, тест"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84ef157b-8fc1-37b6-abbd-74190a06f94c",
                    "sense_definition": "Uzyskiwać pozytywny wynik pozwalający na przejście do następnego etapu edukacji (często używane w kontekście zagrożenia niepowodzeniem).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek niestety nie <zdaje> z fizyki w tym semestrze.",
                            "target_lang_translations": {
                                "ru": "Марек, к сожалению, не <сдает> физику в этом семестре."
                            }
                        },
                        {
                            "text": "Czy wiesz już, kto <zdaje> do następnej klasy, a kto musi powtarzać rok?",
                            "target_lang_translations": {
                                "ru": "Ты уже знаешь, кто <переходит> в следующий класс, а кто должен остаться на второй год?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zaliczać"
                    ],
                    "common_phrases": [
                        "zdawać do następnej klasy",
                        "nie zdawać z przedmiotu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать положительный результат, позволяющий перейти на следующий этап обучения (или контекст неуспеваемости)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переходить",
                                "target_lang_sense_clarification": "в следующий класс"
                            },
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "успешно завершать предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7598e2f-8bad-302a-82d7-a124fb49f50f",
                    "sense_definition": "Sprawiać określone wrażenie, wyglądać na coś lub być postrzeganym w dany sposób (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To zadanie <zdaje> się być o wiele prostsze niż myślałem.",
                            "target_lang_translations": {
                                "ru": "Это задание <кажется> намного проще, чем я думал."
                            }
                        },
                        {
                            "text": "Wszystko <zdawało> się iść zgodnie z naszym planem.",
                            "target_lang_translations": {
                                "ru": "Всё, <казалось>, шло по нашему плану."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydawać się"
                    ],
                    "common_phrases": [
                        "zdawać się być",
                        "zdaje mi się",
                        "tak się zdaje"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить определенное впечатление, выглядеть как что-то или восприниматься определенным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "казаться",
                                "target_lang_sense_clarification": "производить впечатление"
                            },
                            {
                                "target_lang_word": "представляться",
                                "target_lang_sense_clarification": "выглядеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1bbd703-b097-353c-a978-69ab1745b721",
                    "sense_definition": "Przedstawiać szczegółowy opis wydarzeń, sytuacji lub działań (używane głównie w stałych zwrotach z rzeczownikami takimi jak relacja, raport, sprawa).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziennikarz na żywo <zdaje> relację z miejsca wypadku.",
                            "target_lang_translations": {
                                "ru": "Журналист в прямом эфире <ведет> репортаж с места происшествия."
                            }
                        },
                        {
                            "text": "Musisz nam regularnie <zdawać> sprawę z postępów w projekcie.",
                            "target_lang_translations": {
                                "ru": "Ты должен регулярно <отчитываться> нам о ходе проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "relacjonować",
                        "referować",
                        "składać"
                    ],
                    "common_phrases": [
                        "zdawać relację",
                        "zdawać sprawę",
                        "zdawać raport"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представлять подробное описание событий, ситуаций или действий (отчитываться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отчитываться",
                                "target_lang_sense_clarification": "о проделанной работе"
                            },
                            {
                                "target_lang_word": "докладывать",
                                "target_lang_sense_clarification": "сообщать подробности"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "репортаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6fd9baf-b7f8-30e8-a98a-d1da28824927",
                    "sense_definition": "Przekazywać komuś oficjalnie przedmiot, funkcję, dyżur lub odpowiedzialność.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy pracownik, kończąc zmianę, <zdaje> klucze na portierni.",
                            "target_lang_translations": {
                                "ru": "Каждый сотрудник в конце смены <сдает> ключи на вахте."
                            }
                        },
                        {
                            "text": "Ustępujący dyrektor <zdawał> swoje obowiązki nowemu zarządowi.",
                            "target_lang_translations": {
                                "ru": "Уходящий директор <сдавал> свои обязанности новому правлению."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddawać",
                        "przekazywać"
                    ],
                    "common_phrases": [
                        "zdawać klucze",
                        "zdawać dyżur",
                        "zdawać sprzęt",
                        "zdawać mieszkanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально передавать кому-либо предмет, должность, дежурство или ответственность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "возвращать или передавать"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "ответственность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c7218db-519f-39ee-84f6-1a1b68fdbd2e",
                    "sense_definition": "Ufać komuś i pozwalać mu podejmować decyzje; być zależnym od czyjegoś działania lub losu (używane z zaimkiem zwrotnym „się” i przyimkiem „na”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "trust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W kwestii wyboru trasy całkowicie <zdaję> się na ciebie.",
                            "target_lang_translations": {
                                "ru": "В вопросе выбора маршрута я полностью <полагаюсь> на тебя."
                            }
                        },
                        {
                            "text": "Nie powinniśmy biernie <zdawać> się na los, lecz działać.",
                            "target_lang_translations": {
                                "ru": "Мы не должны пассивно <полагаться> на судьбу, а действовать."
                            }
                        }
                    ],
                    "synonyms": [
                        "polegać"
                    ],
                    "common_phrases": [
                        "zdawać się na los",
                        "zdawać się na kogoś"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Доверять кому-либо и позволять принимать решения; зависеть от чьих-либо действий или судьбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полагаться",
                                "target_lang_sense_clarification": "на кого-то или на судьбу"
                            },
                            {
                                "target_lang_word": "довериться",
                                "target_lang_sense_clarification": "передать решение другому"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11274e88-c5d1-3838-92d6-a6943b047e63",
                    "sense_definition": "Być przydatnym lub użytecznym do czegoś (używane z zaimkiem zwrotnym „się”, często w przeczeniach mówiących o braku sensu działania).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "utility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój upór na nic się tu nie <zdaje>, bo decyzja już zapadła.",
                            "target_lang_translations": {
                                "ru": "Твое упрямство здесь ничему не <поможет>, так как решение уже принято."
                            }
                        },
                        {
                            "text": "Krzyki na nic się <zdawały>, nikt ich nie słyszał.",
                            "target_lang_translations": {
                                "ru": "Крики ничему не <помогали>, никто их не слышал."
                            }
                        }
                    ],
                    "synonyms": [
                        "nadawać się"
                    ],
                    "common_phrases": [
                        "na nic się zdawać"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть полезным или пригодным для чего-либо (часто в отрицаниях, указывающих на бессмысленность)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помогать",
                                "target_lang_sense_clarification": "приносить пользу"
                            },
                            {
                                "target_lang_word": "годиться",
                                "target_lang_sense_clarification": "быть пригодным"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
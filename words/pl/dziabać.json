    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0cf23282-3a6f-3020-bd17-201f29d4ef9c",
                    "sense_definition": "Uderzać czymś ostrym, rąbać lub ciąć, często wykonując to niedbale lub z wysiłkiem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musiałem długo <w>dziabać</w> lód na chodniku, żeby go w końcu rozbić.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось долго <w>долбить</w> лёд на тротуаре, чтобы наконец его разбить."
                            }
                        },
                        {
                            "text": "Zamiast precyzyjnie kroić, zaczął <w>dziabać</w> drewno tępą siekierą.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы аккуратно резать, он начал <w>кромсать</w> дерево тупым топором."
                            }
                        }
                    ],
                    "synonyms": [
                        "łupać",
                        "ciapać"
                    ],
                    "common_phrases": [
                        "dziabać drewno",
                        "dziabać nożem",
                        "dziabać siekierą"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бить чем-то острым, рубить или резать, часто небрежно или с усилием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "долбить",
                                "target_lang_sense_clarification": "бить, ударять с усилием"
                            },
                            {
                                "target_lang_word": "кромсать",
                                "target_lang_sense_clarification": "небрежно резать или рубить"
                            },
                            {
                                "target_lang_word": "рубить",
                                "target_lang_sense_clarification": "дрова, лед"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44a02e33-b6dc-3c91-8b54-659a5f2b2f9f",
                    "sense_definition": "Jeść coś, zazwyczaj z apetytem, szybko lub biorąc małe kęsy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Siedzieliśmy w kuchni i zaczęliśmy <w>dziabać</w> sałatkę prosto z miski.",
                            "target_lang_translations": {
                                "ru": "Мы сидели на кухне и начали <w>уплетать</w> салат прямо из миски."
                            }
                        },
                        {
                            "text": "Nie mam czasu na normalny obiad, będę coś <w>dziabać</w> w biegu.",
                            "target_lang_translations": {
                                "ru": "У меня нет времени на нормальный обед, буду что-то <w>перехватывать</w> на бегу."
                            }
                        }
                    ],
                    "synonyms": [
                        "popijać",
                        "ciapać"
                    ],
                    "common_phrases": [
                        "dziabać obiad",
                        "dziabać widelcem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Есть что-то, обычно с аппетитом, быстро или маленькими кусочками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уплетать",
                                "target_lang_sense_clarification": "есть с аппетитом (разг.)"
                            },
                            {
                                "target_lang_word": "рубать",
                                "target_lang_sense_clarification": "жадно есть (сленг)"
                            },
                            {
                                "target_lang_word": "перехватывать",
                                "target_lang_sense_clarification": "быстро перекусывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87c543e4-5b08-37e3-abca-a75c83f083cb",
                    "sense_definition": "Usuwać chwasty z ziemi lub spulchniać glebę za pomocą narzędzia ogrodniczego.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia kazała nam iść do ogrodu i <w>dziabać</w> grządki z marchewką.",
                            "target_lang_translations": {
                                "ru": "Бабушка велела нам идти в огород и <w>тяпать</w> грядки с морковью."
                            }
                        },
                        {
                            "text": "Przez całe popołudnie będziemy <w>dziabać</w> chwasty motyką.",
                            "target_lang_translations": {
                                "ru": "Весь день мы будем <w>полоть</w> сорняки мотыгой."
                            }
                        }
                    ],
                    "synonyms": [
                        "pleć",
                        "plewić"
                    ],
                    "common_phrases": [
                        "dziabać w ogródku",
                        "dziabać motyką"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Poznań"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалять сорняки или рыхлить землю с помощью садового инструмента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тяпать",
                                "target_lang_sense_clarification": "рыхлить мотыгой (разг.)"
                            },
                            {
                                "target_lang_word": "полоть",
                                "target_lang_sense_clarification": "удалять сорняки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e911004-ba19-3bb5-850a-c056c87db6e8",
                    "sense_definition": "Kąsać, żądlić lub dziobać (o owadach, ptakach lub innych zwierzętach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj nad jeziorem, bo komary strasznie tam dzisiaj <w>dziabią</w>.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен на озере, комары там сегодня страшно <w>кусают</w>."
                            }
                        },
                        {
                            "text": "Ten łabędź jest agresywny i może cię <w>dziabać</w>, jeśli podejdziesz za blisko.",
                            "target_lang_translations": {
                                "ru": "Этот лебедь агрессивен и может тебя <w>щипать</w>, если подойдёшь слишком близко."
                            }
                        }
                    ],
                    "synonyms": [
                        "kąsać"
                    ],
                    "common_phrases": [
                        "komary dziabią"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусать, жалить или клевать (о насекомых, птицах или животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кусать",
                                "target_lang_sense_clarification": "о насекомых"
                            },
                            {
                                "target_lang_word": "клевать",
                                "target_lang_sense_clarification": "о птицах"
                            },
                            {
                                "target_lang_word": "щипать",
                                "target_lang_sense_clarification": "о гусях, лебедях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "771781d7-ef92-366e-93d9-dbcbdad95f77",
                    "sense_definition": "Wykonywać tatuaż na skórze.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój znajomy nauczył się <w>dziabać</w> wzory maszynką domowej roboty.",
                            "target_lang_translations": {
                                "ru": "Мой знакомый научился <w>бить</w> узоры самодельной машинкой."
                            }
                        },
                        {
                            "text": "Planuję iść do studia i <w>dziabać</w> sobie cały rękaw.",
                            "target_lang_translations": {
                                "ru": "Я планирую пойти в студию и <w>забить</w> себе весь рукав."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziarać",
                        "dziergać"
                    ],
                    "common_phrases": [
                        "dziabać tatuaż"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать татуировку на коже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "делать татуировку (сленг)"
                            },
                            {
                                "target_lang_word": "набивать",
                                "target_lang_sense_clarification": "наносить татуировку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e765dcf5-0c59-3e2a-be5d-1f4a0a3fe116",
                    "sense_definition": "Pić alkohol, zazwyczaj powoli, małymi łykami lub przez dłuższy czas.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Siedzieli na ławce w parku, żeby po cichu <w>dziabać</w> tanie wino.",
                            "target_lang_translations": {
                                "ru": "Они сидели на скамейке в парке, чтобы по-тихому <w>цедить</w> дешёвое вино."
                            }
                        },
                        {
                            "text": "Nie ma sensu tak <w>dziabać</w> jednego piwa przez dwie godziny.",
                            "target_lang_translations": {
                                "ru": "Нет смысла так <w>цедить</w> одно пиво два часа."
                            }
                        }
                    ],
                    "synonyms": [
                        "popijać",
                        "sączyć"
                    ],
                    "common_phrases": [
                        "dziabać wino",
                        "dziabać piwo"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пить алкоголь, обычно медленно, маленькими глотками или в течение долгого времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цедить",
                                "target_lang_sense_clarification": "пить медленно"
                            },
                            {
                                "target_lang_word": "потягивать",
                                "target_lang_sense_clarification": "пить глотками"
                            }
                        ]
                    }
                }
            ]
        }

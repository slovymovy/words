    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d84a8471-9eb5-383d-87ed-31e86ac79c04",
                    "sense_definition": "Zwycięzca zawodów sportowych lub turnieju; zdobywca pierwszego miejsca.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz zespół ma szansę zostać <w>mistrzem</w> kraju w tym sezonie.",
                            "target_lang_translations": {
                                "ru": "У нашей команды есть шанс стать <w>чемпионом</w> страны в этом сезоне."
                            }
                        },
                        {
                            "text": "Polski skoczek narciarski jest aktualnym <w>mistrzem</w> olimpijskim.",
                            "target_lang_translations": {
                                "ru": "Польский прыгун с трамплина является действующим олимпийским <w>чемпионом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mistrz świata",
                        "mistrz olimpijski",
                        "mistrz Polski",
                        "tytuł mistrza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Победитель спортивных соревнований или турнира; обладатель первого места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чемпион",
                                "target_lang_sense_clarification": "победитель соревнований"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bb64961-6aad-34d3-80d5-5fcffd2d0a55",
                    "sense_definition": "Osoba, która osiągnęła najwyższy stopień umiejętności i wiedzy w jakiejś dziedzinie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "expert",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten pianista to prawdziwy <w>mistrz</w>, gra z niezwykłym uczuciem.",
                            "target_lang_translations": {
                                "ru": "Этот пианист — настоящий <w>мастер</w>, он играет с необыкновенным чувством."
                            }
                        },
                        {
                            "text": "Mój dziadek był <w>mistrzem</w> ceremonii na każdym rodzinnym weselu.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был <w>распорядителем</w> на каждой семейной свадьбе."
                            }
                        }
                    ],
                    "synonyms": [
                        "as",
                        "maestro",
                        "koryfeusz"
                    ],
                    "common_phrases": [
                        "mistrz kuchni",
                        "mistrz słowa",
                        "mistrz ceremonii",
                        "mistrz kierownicy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, достигший высшей степени мастерства и знаний в какой-либо области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "знаток своего дела"
                            },
                            {
                                "target_lang_word": "виртуоз",
                                "target_lang_sense_clarification": "в искусстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4400d24b-bd1f-3458-868f-2637ecc169e1",
                    "sense_definition": "Tytuł rzemieślnika, który posiada najwyższe kwalifikacje w swoim zawodzie i ma prawo szkolić uczniów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody czeladnik zdał trudny egzamin i w końcu został <w>mistrzem</w>.",
                            "target_lang_translations": {
                                "ru": "Молодой подмастерье сдал трудный экзамен и наконец стал <w>мастером</w>."
                            }
                        },
                        {
                            "text": "Stary <w>mistrz</w> stolarski pokazał uczniowi, jak prawidłowo trzymać dłuto.",
                            "target_lang_translations": {
                                "ru": "Старый <w>мастер</w>-столяр показал ученику, как правильно держать стамеску."
                            }
                        }
                    ],
                    "synonyms": [
                        "majster"
                    ],
                    "common_phrases": [
                        "dyplom mistrza",
                        "mistrz budowlany",
                        "mistrz kominiarski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звание ремесленника, обладающего высшей квалификацией в своей профессии и имеющего право обучать учеников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "квалифицированный ремесленник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6311be65-4935-38f2-880b-4cd2701a18f8",
                    "sense_definition": "Najwyższy zwierzchnik lub dostojnik w niektórych zakonach rycerskich.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielki <w>Mistrz</w> zakonu podjął decyzję o rozpoczęciu bitwy.",
                            "target_lang_translations": {
                                "ru": "Великий <w>магистр</w> ордена принял решение о начале битвы."
                            }
                        },
                        {
                            "text": "Wszyscy rycerze złożyli uroczystą przysięgę wierności swojemu <w>mistrzowi</w>.",
                            "target_lang_translations": {
                                "ru": "Все рыцари принесли торжественную клятву верности своему <w>магистру</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Wielki Mistrz"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used mostly in historical contexts regarding knightly orders"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высший руководитель или сановник в некоторых рыцарских орденах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магистр",
                                "target_lang_sense_clarification": "глава ордена"
                            }
                        ]
                    }
                }
            ]
        }
    ]

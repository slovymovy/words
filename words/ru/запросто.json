    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "deff9b40-ba4f-3639-b556-bcbb64647176",
                    "sense_definition": "Очень легко, без всякого труда и усилий.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Они <запросто> могут сказать что-то подобное, не подумав.",
                            "target_lang_translations": {
                                "en": "They can <easily> say something like that without thinking."
                            }
                        },
                        {
                            "text": "Я <запросто> решу эту задачу за пять минут.",
                            "target_lang_translations": {
                                "en": "I will solve this problem <with no trouble at all> in five minutes."
                            }
                        }
                    ],
                    "synonyms": [
                        "легко"
                    ],
                    "common_phrases": [
                        "сделать запросто",
                        "запросто может быть"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Very easily, without any difficulty or effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "easily",
                                "target_lang_sense_clarification": "without difficulty"
                            },
                            {
                                "target_lang_word": "effortlessly",
                                "target_lang_sense_clarification": "with no apparent effort"
                            },
                            {
                                "target_lang_word": "with no trouble at all",
                                "target_lang_sense_clarification": "very easily"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45a400e5-2a55-31f3-b08c-af035c90db53",
                    "sense_definition": "Без соблюдения формальностей и церемоний, непринужденно, по-домашнему.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Заходи к нам <запросто>, без звонка и приглашения.",
                            "target_lang_translations": {
                                "en": "Drop in on us <without ceremony>, without a call or invitation."
                            }
                        },
                        {
                            "text": "Гости чувствовали себя <запросто> и много шутили.",
                            "target_lang_translations": {
                                "en": "The guests felt <at ease> and joked a lot."
                            }
                        }
                    ],
                    "synonyms": [
                        "по-простому"
                    ],
                    "common_phrases": [
                        "зайти запросто",
                        "одет запросто"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Without standing on ceremony, informally, or in a relaxed, home-like manner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "without ceremony",
                                "target_lang_sense_clarification": "informally"
                            },
                            {
                                "target_lang_word": "informally",
                                "target_lang_sense_clarification": "casually"
                            },
                            {
                                "target_lang_word": "at ease",
                                "target_lang_sense_clarification": "feeling comfortable (used with 'feel')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8d3d408-32c2-3b9e-be9a-9bac02235e52",
                    "sense_definition": "Как равный с равным, дружески, без соблюдения субординации.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "С начальником он говорит <запросто>, так как они давно знакомы.",
                            "target_lang_translations": {
                                "en": "He speaks <familiarly> with the boss, since they have known each other for a long time."
                            }
                        },
                        {
                            "text": "Мы общаемся <запросто>, как старые добрые друзья.",
                            "target_lang_translations": {
                                "en": "We communicate <on equal terms>, like good old friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "на равных"
                    ],
                    "common_phrases": [
                        "общаться запросто",
                        "говорить запросто"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "In a familiar, friendly way, treating someone as an equal regardless of hierarchy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "familiarly",
                                "target_lang_sense_clarification": "in a close, friendly manner"
                            },
                            {
                                "target_lang_word": "on equal terms",
                                "target_lang_sense_clarification": "without regarding hierarchy"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d6cc01aa-fde8-3b4f-a7e1-ae3f99d581b8",
                    "sense_definition": "Извлекать полезные ископаемые или природные ресурсы из недр земли.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом регионе активно <добывают> нефть и природный газ.",
                            "target_lang_translations": {
                                "en": "They actively <w>extract</w> oil and natural gas in this region."
                            }
                        },
                        {
                            "text": "Шахтёры спускаются глубоко под землю, чтобы <добывать> уголь.",
                            "target_lang_translations": {
                                "en": "Miners go deep underground to <w>mine</w> coal."
                            }
                        }
                    ],
                    "common_phrases": [
                        "добывать нефть",
                        "добывать уголь",
                        "добывать золото",
                        "добывать руду",
                        "добывать газ"
                    ],
                    "target_lang_definitions": {
                        "en": "To take minerals or natural resources out of the earth."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "extract",
                                "target_lang_sense_clarification": "general term for resources like oil, gas, or ore"
                            },
                            {
                                "target_lang_word": "mine",
                                "target_lang_sense_clarification": "specifically for solid minerals like coal, gold, or diamonds"
                            },
                            {
                                "target_lang_word": "quarry",
                                "target_lang_sense_clarification": "specifically for stone or rock"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "912b1f7a-6e5b-3f09-9abe-de3f2445d1d9",
                    "sense_definition": "Находить, получать или доставать что-либо необходимое, прилагая усилия или преодолевая трудности.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Разведчику удалось <добывать> секретную информацию о планах противника.",
                            "target_lang_translations": {
                                "en": "The scout managed to <w>obtain</w> secret information about the enemy's plans."
                            }
                        },
                        {
                            "text": "В дикой природе туристам приходилось самим <добывать> огонь и воду.",
                            "target_lang_translations": {
                                "en": "In the wild, tourists had to <w>procure</w> fire and water themselves."
                            }
                        }
                    ],
                    "synonyms": [
                        "раздобывать",
                        "доставать"
                    ],
                    "antonyms": [
                        "отдавать"
                    ],
                    "common_phrases": [
                        "добывать информацию",
                        "добывать еду",
                        "добывать огонь",
                        "добывать деньги",
                        "добывать доказательства"
                    ],
                    "target_lang_definitions": {
                        "en": "To get, find, or acquire something necessary, often by applying effort or overcoming difficulties."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "obtain",
                                "target_lang_sense_clarification": "general, implies effort or planning"
                            },
                            {
                                "target_lang_word": "procure",
                                "target_lang_sense_clarification": "formal, implies care or difficulty in getting something"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "common, informal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bf3332f-9f15-3e48-972c-b330fb98d56d",
                    "sense_definition": "Зарабатывать средства к существованию своим трудом.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец много работал, чтобы <добывать> хлеб для своей большой семьи.",
                            "target_lang_translations": {
                                "en": "Father worked hard to <w>earn</w> bread for his large family."
                            }
                        },
                        {
                            "text": "Ему приходилось <добывать> средства на жизнь тяжёлым физическим трудом.",
                            "target_lang_translations": {
                                "en": "He had to <w>earn</w> a living through hard physical labor."
                            }
                        }
                    ],
                    "synonyms": [

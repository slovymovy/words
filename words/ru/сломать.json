    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "57a98f8c-5fde-3e0d-9215-1d190a1047ab",
                    "sense_definition": "Разделить на части или нарушить целостность чего-либо твёрдого силой (ударом или сгибанием).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный ветер <сломал> старое дерево, которое росло возле нашего дома.",
                            "target_lang_translations": {
                                "en": "A strong wind <broke> the old tree that grew near our house."
                            }
                        },
                        {
                            "text": "Ребёнок случайно упал и <сломал> свою любимую пластмассовую игрушку пополам.",
                            "target_lang_translations": {
                                "en": "The child accidentally fell and <broke> his favorite plastic toy in half."
                            }
                        }
                    ],
                    "synonyms": [
                        "поломать",
                        "разломать"
                    ],
                    "antonyms": [
                        "соединить"
                    ],
                    "common_phrases": [
                        "сломать ветку",
                        "сломать ногу",
                        "сломать руку",
                        "случайно сломать",
                        "сломать пополам"
                    ],
                    "target_lang_definitions": {
                        "en": "To separate into parts or destroy the integrity of something hard by force (by hitting or bending)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "general meaning"
                            },
                            {
                                "target_lang_word": "fracture",
                                "target_lang_sense_clarification": "specifically for bones"
                            },
                            {
                                "target_lang_word": "snap",
                                "target_lang_sense_clarification": "to break something long and thin with a sharp noise"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4827cae-d3e9-3178-ad58-a75d3331dd2c",
                    "sense_definition": "Привести механизм, прибор или устройство в негодность; испортить.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой младший брат <сломал> пульт от телевизора, и теперь он не работает.",
                            "target_lang_translations": {
                                "en": "My younger brother <broke> the TV remote, and now it doesn't work."
                            }
                        },
                        {
                            "text": "Мы боялись <сломать> сложный механизм часов, поэтому не стали его разбирать.",
                            "target_lang_translations": {
                                "en": "We were afraid to <break> the complex clock mechanism, so we didn't take it apart."
                            }
                        }
                    ],
                    "synonyms": [
                        "поломать",
                        "угробить"
                    ],
                    "antonyms": [
                        "починить",
                        "восстановить"
                    ],
                    "common_phrases": [
                        "сломать компьютер",
                        "сломать часы",
                        "сломать замок",
                        "сломать машину"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a mechanism, device, or appliance to stop working; to spoil."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "to stop functioning"
                            },
                            {
                                "target_lang_word": "ruin",
                                "target_lang_sense_clarification": "to spoil completely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38df10c1-6e77-3d0c-812e-0570feaa5c49",
                    "sense_definition": "Разрушить или снести строение, здание, конструкцию.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Строители решили <сломать> ветхий сарай, чтобы построить на его месте новый гараж.",
                            "target_lang_translations": {
                                "en": "The builders decided to <tear down> the dilapidated shed to build a new garage in its place."
                            }
                        },
                        {
                            "text": "Рабочие быстро <сломали> перегородку между комнатами для расширения пространства.",
                            "target_lang_translations": {
                                "en": "The workers quickly <knocked down> the partition between the rooms to expand the space."
                            }
                        }
                    ],
                    "synonyms": [
                        "разломать"
                    ],
                    "antonyms": [
                        "восстановить"
                    ],
                    "common_phrases": [
                        "сломать дом",
                        "сломать стену",
                        "сломать забор",
                        "сломать печь"
                    ],
                    "target_lang_definitions": {
                        "en": "To destroy or take down a building, structure, or construction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demolish",
                                "target_lang_sense_clarification": "for buildings and large structures"
                            },
                            {
                                "target_lang_word": "tear down",
                                "target_lang_sense_clarification": "phrasal verb, common for buildings"
                            },
                            {
                                "target_lang_word": "knock down",
                                "target_lang_sense_clarification": "for walls or partitions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb28b7c1-b8f5-34f1-a746-36b408c6ce5b",
                    "sense_definition": "Подавить чью-либо волю или сопротивление, заставить подчиниться; лишить моральных сил.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Трудности не <сломали> его, а сделали только сильнее и увереннее в себе.",
                            "target_lang_translations": {
                                "en": "Hardships did not <break> him, but only made him stronger and more self-confident."
                            }
                        },
                        {
                            "text": "Никакие угрозы не могли <сломать> волю этого смелого человека к победе.",
                            "target_lang_translations": {
                                "en": "No threats could <break> this brave man's will to win."
                            }
                        }
                    ],
                    "synonyms": [
                        "сломить",
                        "подавить"
                    ],
                    "common_phrases": [
                        "сломать характер",
                        "сломать сопротивление",
                        "сломать жизнь",
                        "сломать себя"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in figurative contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To suppress someone's will or resistance, force to submit; to deprive of moral strength."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "figurative, regarding will or spirit"
                            },
                            {
                                "target_lang_word": "crush",
                                "target_lang_sense_clarification": "to suppress resistance completely"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
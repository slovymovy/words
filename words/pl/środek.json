    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9dbbf19e-ce1a-370d-82c9-ceff40338f53",
                    "sense_definition": "Punkt lub obszar położony centralnie, w równej odległości od krawędzi lub brzegów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_center",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stół stoi na <środku> dużego pokoju.",
                            "target_lang_translations": {
                                "ru": "Стол стоит в <центре> большой комнаты."
                            }
                        },
                        {
                            "text": "Mieszkam w samym <środku> miasta, więc wszędzie mam blisko.",
                            "target_lang_translations": {
                                "ru": "Я живу в самом <центре> города, поэтому мне везде близко."
                            }
                        }
                    ],
                    "synonyms": [
                        "centrum",
                        "serce"
                    ],
                    "common_phrases": [
                        "na środku",
                        "w środku miasta",
                        "złoty środek",
                        "środek ciężkości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точка или область, расположенная центрально, на равном расстоянии от краев или границ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "середина",
                                "target_lang_sense_clarification": "равноудаленная точка или часть"
                            },
                            {
                                "target_lang_word": "центр",
                                "target_lang_sense_clarification": "центральная точка или область"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f7e7341-f96b-3244-9cf4-fce0a2de732f",
                    "sense_definition": "Wewnętrzna część obiektu, pomieszczenia lub obszaru; przeciwieństwo zewnętrza.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_inside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zaprosiłem gości do <środka>, ponieważ zaczęło padać.",
                            "target_lang_translations": {
                                "ru": "Я пригласил гостей <внутрь>, так как начался дождь."
                            }
                        },
                        {
                            "text": "W <środku> pudełka znajdował się mały prezent.",
                            "target_lang_translations": {
                                "ru": "<Внутри> коробки находился маленький подарок."
                            }
                        }
                    ],
                    "synonyms": [
                        "wnętrze"
                    ],
                    "common_phrases": [
                        "do środka",
                        "w środku",
                        "ze środka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренняя часть объекта, помещения или области; противоположность внешней части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутренняя часть",
                                "target_lang_sense_clarification": "пространство внутри чего-либо"
                            },
                            {
                                "target_lang_word": "внутренность",
                                "target_lang_sense_clarification": "содержимое или нутро объекта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3e8df04-cfec-3d46-8f24-02ee37391834",
                    "sense_definition": "Moment w czasie przypadający mniej więcej w połowie trwania jakiegoś okresu.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_middle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obudziłem się w <środku> nocy przez hałas.",
                            "target_lang_translations": {
                                "ru": "Я проснулся посреди <ночи> из-за шума."
                            }
                        },
                        {
                            "text": "To dopiero <środek> tygodnia, a ja już jestem zmęczony.",
                            "target_lang_translations": {
                                "ru": "Это только <середина> недели, а я уже устал."
                            }
                        }
                    ],
                    "synonyms": [
                        "połowa"
                    ],
                    "common_phrases": [
                        "w środku nocy",
                        "w środku dnia",
                        "środek lata",
                        "środek zimy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Момент времени, приходящийся примерно на половину продолжительности какого-либо периода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "середина",
                                "target_lang_sense_clarification": "промежуточный момент во времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd539bc0-b319-346b-b59c-997ba4b2f8ec",
                    "sense_definition": "Substancja chemiczna, lek lub preparat służący do określonego celu, np. leczenia lub czyszczenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "instrumental_agent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz wziąć silny <środek> przeciwbólowy.",
                            "target_lang_translations": {
                                "ru": "Ты должен принять сильное обезболивающее <средство>."
                            }
                        },
                        {
                            "text": "Ten <środek> czyszczący doskonale usuwa tłuste plamy.",
                            "target_lang_translations": {
                                "ru": "Это чистящее <средство> отлично удаляет жирные пятна."
                            }
                        }
                    ],
                    "common_phrases": [
                        "środek przeciwbólowy",
                        "środek czystości",
                        "środek nasenny",
                        "środek chemiczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химическое вещество, лекарство или препарат, служащий для определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средство",
                                "target_lang_sense_clarification": "препарат или вещество"
                            },
                            {
                                "target_lang_word": "препарат",
                                "target_lang_sense_clarification": "лекарственное или химическое вещество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba408342-d84e-3ba5-ad8d-1f7bf3465b73",
                    "sense_definition": "Sposób postępowania, metoda lub narzędzie umożliwiające osiągnięcie celu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "instrumental_means",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rower to bardzo ekologiczny <środek> transportu.",
                            "target_lang_translations": {
                                "ru": "Велосипед — это очень экологичное транспортное <средство>."
                            }
                        },
                        {
                            "text": "Musimy znaleźć jakiś skuteczny <środek>, aby rozwiązać ten problem.",
                            "target_lang_translations": {
                                "ru": "Мы должны найти какое-то эффективное <средство>, чтобы решить эту проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "narzędzie",
                        "instrument"
                    ],
                    "common_phrases": [
                        "środek transportu",
                        "środek masowego przekazu",
                        "środek zapobiegawczy",
                        "cel uświęca środki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ действия, метод или инструмент, позволяющий достичь цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средство",
                                "target_lang_sense_clarification": "способ или инструмент достижения цели"
                            },
                            {
                                "target_lang_word": "способ",
                                "target_lang_sense_clarification": "метод действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "791776b4-271e-3e6c-813d-f131fdac2653",
                    "sense_definition": "Pieniądze, fundusze lub zasoby materialne przeznaczone na jakiś cel (zwykle w liczbie mnogiej).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "instrumental_funds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mam wystarczających <środków> na koncie, żeby kupić ten samochód.",
                            "target_lang_translations": {
                                "ru": "У меня нет достаточных <средств> на счете, чтобы купить эту машину."
                            }
                        },
                        {
                            "text": "Wszystkie <środki> ze zbiórki zostaną przekazane na cele charytatywne.",
                            "target_lang_translations": {
                                "ru": "Все <средства> от сбора будут переданы на благотворительные цели."
                            }
                        }
                    ],
                    "common_phrases": [
                        "środki finansowe",
                        "brak środków",
                        "środki unijne",
                        "środki do życia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деньги, фонды или материальные ресурсы, предназначенные для какой-либо цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средства",
                                "target_lang_sense_clarification": "деньги, капитал (мн. ч.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "96c00a0d-376b-3b05-bc00-b8ee6ca91893",
                    "sense_definition": "Совокупность должностных лиц, руководящих каким-либо учреждением, предприятием или организацией; руководство.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Завтра наше <w>начальство</w> проводит важное собрание для всех сотрудников.",
                            "target_lang_translations": {
                                "en": "Tomorrow our <w>management</w> is holding an important meeting for all employees."
                            }
                        },
                        {
                            "text": "Секретарь сообщила, что высокое <w>начальство</w> сейчас занято и не может нас принять.",
                            "target_lang_translations": {
                                "en": "The secretary reported that the <w>top management</w> is busy right now and cannot see us."
                            }
                        }
                    ],
                    "synonyms": [
                        "руководство",
                        "администрация",
                        "управление"
                    ],
                    "antonyms": [
                        "подчинённые",
                        "персонал"
                    ],
                    "common_phrases": [
                        "высокое начальство",
                        "местное начальство",
                        "кабинет начальства",
                        "вызвать к начальству"
                    ],
                    "target_lang_definitions": {
                        "en": "The collective body of officials leading an institution, enterprise, or organization; management."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "management",
                                "target_lang_sense_clarification": "collective body of managers"
                            },
                            {
                                "target_lang_word": "leadership",
                                "target_lang_sense_clarification": "group of leaders"
                            },
                            {
                                "target_lang_word": "authorities",
                                "target_lang_sense_clarification": "persons in power"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90336999-fa67-3465-9218-d072a1aeda5a",
                    "sense_definition": "Отдельный начальник или руководитель (обычно в разговорной речи).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моё непосредственное <w>начальство</w> сегодня уехало в командировку на неделю.",
                            "target_lang_translations": {
                                "en": "My immediate <w>boss</w> left for a business trip for a week today."
                            }
                        },
                        {
                            "text": "Ты должен понимать, что с <w>начальством</w> лучше не спорить по пустякам.",
                            "target_lang_translations": {
                                "en": "You have to understand that it is better not to argue with the <w>boss</w> over trifles."
                            }
                        }
                    ],
                    "synonyms": [
                        "начальник",
                        "шеф",
                        "руководитель",
                        "босс"
                    ],
                    "antonyms": [
                        "подчинённый"
                    ],
                    "common_phrases": [
                        "непосредственное начальство",
                        "разговор с начальством"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An individual boss or manager (usually in colloquial speech)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boss",
                                "target_lang_sense_clarification": "colloquial"
                            },
                            {
                                "target_lang_word": "chief",
                                "target_lang_sense_clarification": "colloquial"
                            },
                            {
                                "target_lang_word": "superior",
                                "target_lang_sense_clarification": "formal equivalent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d7844fa-4938-32bb-bed6-69d60aac96d3",
                    "sense_definition": "Власть, право повелевать и управлять; командование.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Научная экспедиция проходила под <w>начальством</w> известного профессора геологии.",
                            "target_lang_translations": {
                                "en": "The scientific expedition took place under the <w>leadership</w> of a famous geology professor."
                            }
                        },
                        {
                            "text": "Вскоре этот полк перешёл под <w>начальство</w> нового генерала.",
                            "target_lang_translations": {
                                "en": "Soon this regiment came under the <w>command</w> of a new general."
                            }
                        }
                    ],
                    "synonyms": [
                        "командование",
                        "руководство",
                        "главенство"
                    ],
                    "antonyms": [
                        "подчинение"
                    ],
                    "common_phrases": [
                        "под начальством"
                    ],
                    "target_lang_definitions": {
                        "en": "Power, the right to command and govern; command."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "command",
                                "target_lang_sense_clarification": "authority or control"
                            },
                            {
                                "target_lang_word": "leadership",
                                "target_lang_sense_clarification": "guidance or direction"
                            }
                        ]
                    }
                }
            ]
        }
    ]

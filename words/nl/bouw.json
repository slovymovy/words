                {
                    "sense_id": "fbb5bf67-c6f4-30f1-bc80-d5d8309a9add",
                    "sense_definition": "Een oude oppervlaktemaat op Java, gelijk aan ongeveer 0,7 hectare.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4e6cd5d2-8ee0-36d2-a66f-39d76d4e42d2",
                    "sense_definition": "The act of putting equipment, machinery, or software into position and making it ready for use.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>installation</w> of the new software took only a few minutes to complete.",
                            "target_lang_translations": {
                                "ru": "<w>Установка</w> нового программного обеспечения заняла всего несколько минут."
                            }
                        },
                        {
                            "text": "We need to schedule the <w>installation</w> of the new air conditioning unit for tomorrow.",
                            "target_lang_translations": {
                                "ru": "Нам нужно запланировать <w>монтаж</w> нового кондиционера на завтра."
                            }
                        }
                    ],
                    "synonyms": [
                        "setup",
                        "fitting",
                        "placement"
                    ],
                    "antonyms": [
                        "removal",
                        "uninstallation",
                        "dismantling"
                    ],
                    "common_phrases": [
                        "software installation",
                        "easy installation",
                        "installation guide",
                        "installation cost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс размещения оборудования, техники или программного обеспечения и подготовки их к использованию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "установка",
                                "target_lang_sense_clarification": "общий термин для программ и техники"
                            },
                            {
                                "target_lang_word": "монтаж",
                                "target_lang_sense_clarification": "сборка и крепление оборудования"
                            },
                            {
                                "target_lang_word": "инсталляция",
                                "target_lang_sense_clarification": "преимущественно ПО"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fcf3edb-d505-3af3-b6f5-0195ff9c237e",
                    "sense_definition": "A system of machines, apparatus, or equipment that has been set up for practical use.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory has a massive electrical <w>installation</w> that powers all the assembly lines.",
                            "target_lang_translations": {
                                "ru": "На заводе есть мощная электрическая <w>установка</w>, которая питает все сборочные линии."
                            }
                        },
                        {
                            "text": "Inspectors came to check the safety of the gas <w>installation</w> in the building.",
                            "target_lang_translations": {
                                "ru": "Инспекторы пришли проверить безопасность газового <w>оборудования</w> в здании."
                            }
                        }
                    ],
                    "synonyms": [
                        "system",
                        "apparatus",
                        "equipment"
                    ],
                    "common_phrases": [
                        "electrical installation",
                        "heating installation",
                        "security installation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система машин, аппаратуры или оборудования, установленная для практического использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "установка",
                                "target_lang_sense_clarification": "техническое устройство или система"
                            },
                            {
                                "target_lang_word": "оборудование",
                                "target_lang_sense_clarification": "совокупность устройств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8685158f-b517-3667-8469-b815c4447c21",
                    "sense_definition": "A large facility or area used for military purposes, such as a base or camp.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A military <w>installation</w> was built near the border to monitor activity in the region.",
                            "target_lang_translations": {
                                "ru": "Военный <w>объект</w> был построен недалеко от границы для наблюдения за активностью в регионе."
                            }
                        },
                        {
                            "text": "Unauthorized personnel are strictly forbidden from entering the government <w>installation</w>.",
                            "target_lang_translations": {
                                "ru": "Посторонним строго запрещен вход на правительственный <w>объект</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "base",
                        "facility",
                        "post",
                        "complex"
                    ],
                    "common_phrases": [
                        "military installation",
                        "defense installation",
                        "secret installation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупный объект или территория, используемые в военных целях, например, база или лагерь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объект",
                                "target_lang_sense_clarification": "военное сооружение или территория"
                            },
                            {
                                "target_lang_word": "база",
                                "target_lang_sense_clarification": "место дислокации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a42c210-f4d9-3fb3-a5d3-f9d7bd8cd981",
                    "sense_definition": "A piece of modern art constructed within a specific space, often designed to transform the perception of that space.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum is displaying a modern art <w>installation</w> made entirely of recycled plastic.",
                            "target_lang_translations": {
                                "ru": "В музее демонстрируется <w>инсталляция</w> современного искусства, сделанная полностью из переработанного пластика."
                            }
                        },
                        {
                            "text": "She created a video <w>installation</w> that projects moving images onto the walls of the gallery.",
                            "target_lang_translations": {
                                "ru": "Она создала видео-<w>инсталляцию</w>, которая проецирует движущиеся изображения на стены галереи."
                            }
                        }
                    ],
                    "synonyms": [
                        "exhibit",
                        "display"
                    ],
                    "common_phrases": [
                        "art installation",
                        "video installation",
                        "sound installation",
                        "museum installation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произведение современного искусства, созданное для конкретного пространства и часто меняющее восприятие этого пространства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инсталляция",
                                "target_lang_sense_clarification": "вид современного искусства"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
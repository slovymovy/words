            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "04ddd3d2-67c6-36fe-968e-834f321059af",
                    "sense_definition": "Оценочная характеристика ситуации или действия как лишенных смысла, бесполезных или напрасных",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "predicative_eval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сейчас уже <w>бессмысленно</w> спорить о том, кто был прав.",
                            "target_lang_translations": {
                                "en": "It is <w>pointless</w> to argue now about who was right."
                            }
                        },
                        {
                            "text": "Было бы <w>бессмысленно</w> отрицать очевидные факты.",
                            "target_lang_translations": {
                                "en": "It would be <w>senseless</w> to deny the obvious facts."
                            }
                        }
                    ],
                    "synonyms": [
                        "бесполезно",
                        "глупо",
                        "напрасно",
                        "нелепо"
                    ],
                    "antonyms": [
                        "осмысленно",
                        "осмысленно",
                        "осмысленно",
                        "осмысленно"
                    ],
                    "common_phrases": [
                        "абсолютно бессмысленно",
                        "совершенно бессмысленно",
                        "бессмысленно делать",
                        "бессмысленно отрицать"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to describe a situation or action as having no purpose, logic, or chance of success"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pointless",
                                "target_lang_sense_clarification": "lacking purpose or use"
                            },
                            {
                                "target_lang_word": "senseless",
                                "target_lang_sense_clarification": "lacking logic or reason"
                            },
                            {
                                "target_lang_word": "useless",
                                "target_lang_sense_clarification": "producing no result"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad45e688-826e-3938-8cf4-e456c3932d62",

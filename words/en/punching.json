                    "sense_definition": "Present participle of <w>punch</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boxer is currently <w>punching</w> the heavy bag in the gym.",
                            "target_lang_translations": {
                                "ru": "Боксер сейчас <w>бьет</w> тяжелую грушу в спортзале."
                            }
                        },
                        {
                            "text": "She was <w>punching</w> holes in the paper to organize her binder.",
                            "target_lang_translations": {
                                "ru": "Она <w>пробивала</w> отверстия в бумаге, чтобы привести в порядок свою папку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "punching bag",
                        "punching the air",
                        "punching above weight"
                    ],
                    "target_lang_definitions": {

                {
                    "sense_id": "3d9ce44d-be1b-3071-b113-e52b8420370a",
                    "sense_definition": "Onomatopeja naśladująca odgłos wydawany przez kurę.",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kura chodziła po podwórku i powtarzała swoje głośne <w>ko</w>, ko, ko.",
                            "target_lang_translations": {
                                "ru": "Курица ходила по двору и повторяла свое громкое <w>ко</w>-ко-ко."
                            }
                        },
                        {
                            "text": "Małe dziecko śmiało się, słysząc <w>ko</w> dochodzące z kurnika.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок смеялся, слыша <w>ко</w>, доносящееся из курятника."
                            }
                        }
                    ],
                    "synonyms": [
                        "ko-ko"
                    ],
                    "common_phrases": [
                        "robić ko ko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звукоподражание, имитирующее голос курицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ко-ко",
                                "target_lang_sense_clarification": "звукоподражание"
                            }
                        ]
                    }
                }
            ]
        },
        {

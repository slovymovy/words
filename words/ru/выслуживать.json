    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07818fda-2512-32f7-825b-a4a644ea4260",
                    "sense_definition": "Добывать или получать что-либо (звание, награду, право на пенсию) благодаря долгой или усердной службе.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "service_gain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Офицер надеялся <w>выслуживать</w> очередное звание досрочно благодаря своим успехам.",
                            "target_lang_translations": {
                                "en": "The officer hoped to <w>earn</w> his next rank early thanks to his successes."
                            }
                        },
                        {
                            "text": "Многим чиновникам приходилось годами <w>выслуживать</w> право на государственную пенсию.",
                            "target_lang_translations": {
                                "en": "Many officials had to <w>earn</w> the right to a state pension over many years."
                            }
                        }
                    ],
                    "synonyms": [
                        "зарабатывать",
                        "заслуживать"
                    ],
                    "common_phrases": [
                        "выслуживать пенсию",
                        "выслуживать звание",
                        "выслуживать награду"
                    ],
                    "target_lang_definitions": {
                        "en": "To obtain or receive something (rank, award, pension) through long or diligent service."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earn",
                                "target_lang_sense_clarification": "through service"
                            },
                            {
                                "target_lang_word": "merit",
                                "target_lang_sense_clarification": "by serving"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ae3b4a6-a50a-3937-94a8-226677f8ca0b",
                    "sense_definition": "Находиться на службе в течение определённого срока, необходимого для получения каких-либо льгот или завершения обязательств.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "service_gain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему оставалось <w>выслуживать</w> ещё два года, чтобы получить полный социальный пакет.",
                            "target_lang_translations": {
                                "en": "He had two more years left to <w>serve out</w> to receive the full benefits package."
                            }
                        },
                        {
                            "text": "Солдаты должны <w>выслуживать</w> установленный законом срок, прежде чем уйти в запас.",
                            "target_lang_translations": {
                                "en": "Soldiers must <w>serve out</w> the statutory term before retiring to the reserve."
                            }
                        }
                    ],
                    "synonyms": [
                        "прослуживать",
                        "отслуживать"
                    ],
                    "common_phrases": [
                        "выслуживать срок",
                        "выслуживать стаж",
                        "выслуживать ценз"
                    ],
                    "target_lang_definitions": {
                        "en": "To serve for a specific period of time necessary to receive benefits or complete obligations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve out",
                                "target_lang_sense_clarification": "a term or period"
                            },
                            {
                                "target_lang_word": "complete",
                                "target_lang_sense_clarification": "a period of service"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "936c04b8-5c36-3418-a9fc-6d40d1d7b4e3",
                    "sense_definition": "Amusing, entertaining, or causing laughter.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "humor_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dog looked quite <w>comic</w> wearing a pair of sunglasses.",
                            "target_lang_translations": {
                                "ru": "Собака выглядела довольно <w>комичной</w> в солнечных очках."
                            }
                        },
                        {
                            "text": "She made a <w>comic</w> face to cheer up her crying brother.",
                            "target_lang_translations": {
                                "ru": "Она состроила <w>смешную</w> гримасу, чтобы подбодрить плачущего брата."
                            }
                        }
                    ],
                    "synonyms": [
                        "comical",
                        "comical",
                        "comical"
                    ],
                    "antonyms": [
                        "serious"
                    ],
                    "common_phrases": [
                        "comic effect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Забавный, развлекательный или вызывающий смех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смешно́й",
                                "target_lang_sense_clarification": "вызывающий смех, веселый"
                            },
                            {
                                "target_lang_word": "коми́чный",
                                "target_lang_sense_clarification": "потешный, забавный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "847ab206-78b7-3a30-8723-144fd7acc785",
                    "sense_definition": "Unintentionally humorous or ridiculous.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "humor_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was something <w>comic</w> about his intense anger over such a small mistake.",
                            "target_lang_translations": {
                                "ru": "Было что-то <w>комичное</w> в его сильном гневе из-за такой маленькой ошибки."
                            }
                        },
                        {
                            "text": "The situation became <w>comic</w> when everyone started slipping on the ice.",
                            "target_lang_translations": {
                                "ru": "Ситуация стала <w>комичной</w>, когда все начали скользить на льду."
                            }
                        }
                    ],
                    "synonyms": [
                        "comical"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непреднамеренно смешной, нелепый или курьезный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коми́чный",
                                "target_lang_sense_clarification": "нелепый, курьезный"

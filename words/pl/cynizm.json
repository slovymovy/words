            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d8e74ed1-6d01-30e7-8aa3-8f903724ee5c",
                    "sense_definition": "Wyzywające i lekceważące odnoszenie się do wartości moralnych, nieliczenie się z uczuciami i odczuciami innych.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>cynizm</w> sprawia, że trudno mu zaufać w jakiejkolwiek sprawie.",
                            "target_lang_translations": {
                                "ru": "Его <w>цинизм</w> мешает доверять ему в каком-либо деле."
                            }
                        },
                        {
                            "text": "To był czysty <w>cynizm</w> z jego strony, by wykorzystać tę sytuację dla własnych korzyści.",
                            "target_lang_translations": {
                                "ru": "Это был чистый <w>цинизм</w> с его стороны — использовать эту ситуацию ради собственной выгоды."
                            }
                        }
                    ],
                    "synonyms": [
                        "cyniczność"
                    ],
                    "common_phrases": [
                        "czysty cynizm",
                        "szczyt cynizmu",
                        "pełen cynizmu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающее и пренебрежительное отношение к общепринятым нормам нравственности и морали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цинизм",
                                "target_lang_sense_clarification": "пренебрежение нормами"
                            },
                            {
                                "target_lang_word": "циничность",
                                "target_lang_sense_clarification": "свойство личности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38928c5b-d2f8-3571-9deb-2321a8f801ae",
                    "sense_definition": "Zespół poglądów greckiej szkoły filozoficznej Antystenesa, charakteryzujący się odrzuceniem norm społecznych i dóbr materialnych.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "philosophy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starożytny <w>cynizm</w> głosił, że do szczęścia wystarczy sama cnota.",
                            "target_lang_translations": {
                                "ru": "Древний <w>цинизм</w> гласил, что для счастья достаточно одной лишь добродетели."
                            }
                        },
                        {
                            "text": "Diogenes z Synopy jest najbardziej znanym przedstawicielem greckiego <w>cynizmu</w>.",
                            "target_lang_translations": {
                                "ru": "Диоген Синопский — самый известный представитель греческого <w>цинизма</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "filozofia cynizmu",
                        "grecki cynizm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учение древнегреческой философской школы, отвергавшей социальные нормы и материальные блага."
                    },
                    "translations": {
                        "ru": [

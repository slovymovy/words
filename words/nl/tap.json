                        "kraan",
                        "aftapkraan",
                        "aftappunt"
                    ],
                    "common_phrases": [
                        "van de tap",
                        "achter de tap",
                        "aan de tap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кран или механизм для розлива жидкости (например, пива или воды) из бочки или трубы.",
                        "en": "A faucet or mechanism used to control the flow of liquid (such as beer or water) from a barrel or pipe."

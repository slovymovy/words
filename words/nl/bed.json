                    "sense_id": "75d7c7b2-4c1a-348d-8355-2e1069353608",
                    "sense_definition": "Een meubelstuk dat gemaakt is om in te slapen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben erg moe en ga vroeg naar <w>bed</w>.",
                            "target_lang_translations": {
                                "en": "I am very tired and am going to <w>bed</w> early.",
                                "ru": "Я очень устал и рано ложусь в <w>кровать</w>."
                            }
                        },
                        {
                            "text": "Hij ligt nog steeds in zijn warme <w>bed</w> te slapen.",
                            "target_lang_translations": {
                                "en": "He is still sleeping in his warm <w>bed</w>.",
                                "ru": "Он всё ещё спит в своей тёплой <w>постели</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ledikant",
                        "slaapplaats"
                    ],
                    "common_phrases": [
                        "naar bed gaan",
                        "in bed liggen",
                        "uit bed komen",
                        "naar bed brengen"
                    ],
                    "target_lang_definitions": {
                        "en": "A piece of furniture made for sleeping in.",
                        "ru": "Предмет мебели, предназначенный для сна."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "33550e59-986f-337f-8cfc-209ea8d4d93f",
                    "sense_definition": "A person who brings food and drinks to customers in a restaurant; a waiter or waitress.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "occupation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We asked the <w>server</w> for the dessert menu after we finished our meal.",
                            "target_lang_translations": {
                                "ru": "Мы попросили <w>официанта</w> принести десертное меню, когда закончили есть."
                            }
                        },
                        {
                            "text": "The <w>server</w> was very polite and brought our drinks immediately.",
                            "target_lang_translations": {
                                "ru": "<w>Официант</w> был очень вежлив и сразу принес наши напитки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tip the server",
                        "ask the server"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который приносит еду и напитки посетителям в ресторане."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "официант",
                                "target_lang_sense_clarification": "мужчина"
                            },
                            {
                                "target_lang_word": "официантка",
                                "target_lang_sense_clarification": "женщина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55e9dab2-55dd-3ac5-8d82-9b175757e742",
                    "sense_definition": "A central computer that holds data and programs for many other computers to use over a network.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "computing_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company website went offline because the <w>server</w> crashed.",
                            "target_lang_translations": {
                                "ru": "Сайт компании стал недоступен, потому что <w>сервер</w> вышел из строя."
                            }
                        },
                        {
                            "text": "You need to connect to the file <w>server</w> to access the shared documents.",
                            "target_lang_translations": {
                                "ru": "Вам нужно подключиться к файловому <w>серверу</w>, чтобы получить доступ к общим документам."
                            }
                        }
                    ],
                    "antonyms": [
                        "client"
                    ],
                    "common_phrases": [
                        "file server",
                        "web server",
                        "server down",
                        "connect to the server"
                    ],
                    "target_lang_definitions": {
                        "ru": "Центральный компьютер, который хранит данные и программы для использования другими компьютерами в сети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сервер",
                                "target_lang_sense_clarification": "компьютер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f12253db-e07f-3bc9-9d9a-98faa9c1c92d",
                    "sense_definition": "The player who hits the ball to start a point in sports like tennis or volleyball.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>server</w> tossed the ball high into the air before striking it.",
                            "target_lang_translations": {
                                "ru": "<w>Подающий</w> высоко подбросил мяч в воздух перед ударом."
                            }
                        },
                        {
                            "text": "In this game, the <w>server</w> has two chances to get the ball over the net.",
                            "target_lang_translations": {
                                "ru": "В этой игре у <w>подающего</w> есть две попытки, чтобы перебросить мяч через сетку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "first server",
                        "powerful server"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игрок, который подает мяч, начиная розыгрыш очка в таких видах спорта, как теннис или волейбол."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подающий",
                                "target_lang_sense_clarification": "игрок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8862948e-7d6a-3193-a3bc-d8a368c9edd6",
                    "sense_definition": "A computer program that manages access to a centralized resource or service in a network.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "computing_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The email <w>server</w> processes thousands of messages every hour.",
                            "target_lang_translations": {
                                "ru": "Почтовый <w>сервер</w> обрабатывает тысячи сообщений каждый час."
                            }
                        },
                        {
                            "text": "Developers installed the database <w>server</w> to manage the application's records.",
                            "target_lang_translations": {
                                "ru": "Разработчики установили <w>сервер</w> баз данных для управления записями приложения."
                            }
                        }
                    ],
                    "antonyms": [
                        "client"
                    ],
                    "common_phrases": [
                        "proxy server",
                        "application server",
                        "mail server"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа, управляющая доступом к централизованному ресурсу или услуге в сети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сервер",
                                "target_lang_sense_clarification": "программа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74265037-b0a4-3195-9023-fea0942f191a",
                    "sense_definition": "A virtual community space on a platform where members can communicate via text or voice channels.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_community",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He invited me to join his private gaming <w>server</w> so we could chat while playing.",
                            "target_lang_translations": {
                                "ru": "Он пригласил меня на свой частный игровой <w>сервер</w>, чтобы мы могли общаться во время игры."
                            }
                        },
                        {
                            "text": "The moderators of the <w>server</w> banned the user for breaking the rules.",
                            "target_lang_translations": {
                                "ru": "Модераторы <w>сервера</w> заблокировали пользователя за нарушение правил."
                            }
                        }
                    ],
                    "common_phrases": [
                        "join a server",
                        "Discord server",
                        "private server"
                    ],
                    "target_lang_definitions": {
                        "ru": "Виртуальное пространство сообщества на платформе, где участники могут общаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сервер",
                                "target_lang_sense_clarification": "сообщество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc97a4d3-3535-3bfa-a289-7190ec31c66d",
                    "sense_definition": "A utensil, such as a large spoon or slice, used for serving food.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "utensil",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She used the silver cake <w>server</w> to place a slice of pie on my plate.",
                            "target_lang_translations": {
                                "ru": "Она использовала серебряную <w>лопатку</w> для торта, чтобы положить кусок пирога мне на тарелку."
                            }
                        },
                        {
                            "text": "Please pass the salad <w>server</w> so everyone can get some greens.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, передай <w>ложку</w> для салата, чтобы все могли положить себе зелени."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cake server",
                        "salad server",
                        "pie server"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кухонная принадлежность, например, лопатка или большая ложка, используемая для раскладывания еды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лопатка",
                                "target_lang_sense_clarification": "для торта"
                            },
                            {
                                "target_lang_word": "ложка",
                                "target_lang_sense_clarification": "для раскладывания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f673995-d742-3956-baad-0346270fe1fc",
                    "sense_definition": "An assistant to a priest during a religious service.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young <w>server</w> carried the candles to the altar during the procession.",
                            "target_lang_translations": {
                                "ru": "Юный <w>алтарник</w> нес свечи к алтарю во время процессии."
                            }
                        },
                        {
                            "text": "He volunteered as a <w>server</w> at the local church every Sunday morning.",
                            "target_lang_translations": {
                                "ru": "Он был добровольным <w>служителем</w> в местной церкви каждое воскресное утро."
                            }
                        }
                    ],
                    "synonyms": [
                        "altar server",
                        "acolyte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помощник священника во время религиозной службы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "служитель",
                                "target_lang_sense_clarification": "церковный"
                            },
                            {
                                "target_lang_word": "алтарник",
                                "target_lang_sense_clarification": "помощник у алтаря"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ad8e7ff-3fb1-3cc8-a8ac-0ca98ee4c390",
                    "sense_definition": "A tray used for carrying dishes or presenting items.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "utensil",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The butler presented the letter to the guest on a small silver <w>server</w>.",
                            "target_lang_translations": {
                                "ru": "Дворецкий преподнес гостю письмо на маленьком серебряном <w>подносе</w>."
                            }
                        },
                        {
                            "text": "She placed the crystal glasses carefully on the <w>server</w> to carry them out.",
                            "target_lang_translations": {
                                "ru": "Она осторожно поставила хрустальные бокалы на <w>поднос</w>, чтобы вынести их."
                            }
                        }
                    ],
                    "synonyms": [
                        "salver"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поднос, используемый для переноски посуды или подачи предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поднос",
                                "target_lang_sense_clarification": "для подачи"
                            }
                        ]
                    }
                }
            ]
        },

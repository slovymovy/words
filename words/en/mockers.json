    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2d78d0c7-c69e-3b23-ba4e-cae5567890e0",
                    "sense_definition": "Plural of <w>mocker</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>mockers</w> stood at the back of the room, laughing quietly at the speaker's mistakes.",
                            "target_lang_translations": {
                                "ru": "В глубине комнаты стояли <w>насмешники</w>, тихо посмеиваясь над ошибками оратора."
                            }
                        },
                        {
                            "text": "She decided to ignore the <w>mockers</w> and continue pursuing her unusual hobby.",
                            "target_lang_translations": {
                                "ru": "Она решила не обращать внимания на <w>насмешников</w> и продолжить заниматься своим необычным хобби."
                            }
                        }
                    ],
                    "synonyms": [
                        "scoffers",
                        "ridiculers",
                        "teasers"
                    ],
                    "antonyms": [
                        "admirers",
                        "supporters",
                        "praisers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, которые насмехаются, издеваются или иронизируют над кем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "насмешники",
                                "target_lang_sense_clarification": "те, кто подвергает кого-то насмешкам"
                            },
                            {
                                "target_lang_word": "зубоскалы",
                                "target_lang_sense_clarification": "разг.; те, кто постоянно шутит и смеется над другими"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "905df1f2-a676-36d1-8944-4d778cefd78a",
                    "sense_definition": "A jinx or bad luck that prevents success.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden rain put the <w>mockers</w> on our plans for a beach picnic.",
                            "target_lang_translations": {
                                "ru": "Внезапный дождь поставил <w>крест</w> на наших планах устроить пикник на пляже."
                            }
                        },
                        {
                            "text": "He worried that talking about the deal too soon would put the <w>mockers</w> on it.",
                            "target_lang_translations": {
                                "ru": "Он беспокоился, что разговоры о сделке раньше времени навлекут на неё <w>сглаз</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "jinx",
                        "curse",
                        "hex"
                    ],
                    "antonyms": [
                        "luck",
                        "charm"
                    ],
                    "common_phrases": [
                        "put the mockers on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сглаз, неудача или обстоятельство, разрушающее планы (часто используется в идиоме 'put the mockers on')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сглаз",
                                "target_lang_sense_clarification": "дурное влияние, приносящее неудачу"
                            },
                            {
                                "target_lang_word": "крест",
                                "target_lang_sense_clarification": "в значении 'конец планам' (в выражении 'поставить крест')"
                            }
                        ]
                    }
                }
            ]
        }
    ]

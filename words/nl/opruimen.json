                    "synonyms": [
                        "elimineren",
                        "uitschakelen"
                    ],
                    "common_phrases": [
                        "sneeuw opruimen",
                        "obstakels opruimen"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove or take away something that is obstructing or no longer needed.",

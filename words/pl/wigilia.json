            "senses": [
                {
                    "sense_id": "6ce41289-55e9-33e3-a31c-8ca4080c1f3b",
                    "sense_definition": "Tradycyjna, uroczysta kolacja spożywana wieczorem w dniu poprzedzającym Boże Narodzenie",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "christmas_context",
                    "name_type": "no",
                    "examples": [

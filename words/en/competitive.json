{
    "word_family": [
        "compete",
        "competition",
        "competitor",
        "competitively",
        "competitiveness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e379b372-d28c-3d1a-b2bd-083b30818568",
                    "sense_definition": "Relating to a situation in which people or organizations fight against each other to win or gain an advantage.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Professional football is a highly <w>competitive</w> sport that requires intense training.",
                            "target_lang_translations": {
                                "ru": "Профессиональный футбол — это высоко <w>конкурентный</w> вид спорта, требующий интенсивных тренировок."
                            }
                        },
                        {
                            "text": "After years of practice, she finally retired from <w>competitive</w> swimming.",
                            "target_lang_translations": {
                                "ru": "После многих лет тренировок она наконец ушла из <w>соревновательного</w> плавания."
                            }
                        }
                    ],
                    "antonyms": [
                        "anticompetitive"
                    ],
                    "common_phrases": [
                        "competitive sport",
                        "competitive environment",
                        "competitive gaming",
                        "competitive exam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к ситуации, в которой люди или организации борются друг с другом за победу или преимущество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конкурентный",
                                "target_lang_sense_clarification": "о среде, рынке или ситуации"
                            },
                            {
                                "target_lang_word": "соревновательный",
                                "target_lang_sense_clarification": "о спорте, играх или процессе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1256c8c3-29c4-3aee-bc4e-04826204132b",
                    "sense_definition": "Having a strong desire to win or be more successful than others.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother is very <w>competitive</w> and gets upset whenever he loses a board game.",
                            "target_lang_translations": {
                                "ru": "Мой брат очень <w>азартный</w> и расстраивается всякий раз, когда проигрывает в настольную игру."
                            }
                        },
                        {
                            "text": "You need a <w>competitive</w> spirit to succeed in this aggressive industry.",
                            "target_lang_translations": {
                                "ru": "Вам нужен <w>дух соперничества</w>, чтобы преуспеть в этой агрессивной индустрии."
                            }
                        }
                    ],
                    "antonyms": [
                        "cooperative"
                    ],
                    "common_phrases": [
                        "competitive spirit",
                        "competitive nature",
                        "highly competitive",
                        "competitive streak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий сильным желанием победить или быть успешнее других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "азартный",
                                "target_lang_sense_clarification": "увлеченный, стремящийся к выигрышу"
                            },
                            {
                                "target_lang_word": "склонный к соперничеству",
                                "target_lang_sense_clarification": "о характере человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7eafbece-e6a7-33c6-a65d-d683dcd8e4e7",
                    "sense_definition": "Good enough to be as successful as other rivals, often by offering good value or low prices.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The store offers a wide range of goods at very <w>competitive</w> prices.",
                            "target_lang_translations": {
                                "ru": "Магазин предлагает широкий ассортимент товаров по очень <w>конкурентным</w> ценам."
                            }
                        },
                        {
                            "text": "We must improve our efficiency to maintain a <w>competitive</w> edge in the global market.",
                            "target_lang_translations": {
                                "ru": "Мы должны повысить нашу эффективность, чтобы сохранить <w>конкурентное</w> преимущество на мировом рынке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "competitive price",
                        "competitive advantage",
                        "competitive market",
                        "competitive salary",
                        "competitive edge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достаточно хороший, чтобы быть успешным наравне с соперниками, часто благодаря выгодной цене или качеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конкурентоспособный",
                                "target_lang_sense_clarification": "способный выдержать конкуренцию"
                            },
                            {
                                "target_lang_word": "конкурентный",
                                "target_lang_sense_clarification": "выгодный, сопоставимый с другими (о цене)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa1ae097-35b8-3673-9a70-03f813239979",
                    "sense_definition": "Relating to the inhibition of the effects of a chemical substance by another substance competing with it for binding or bonding.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The drug acts as a <w>competitive</w> antagonist at the receptor site.",
                            "target_lang_translations": {
                                "ru": "Препарат действует как <w>конкурентный</w> антагонист на участке рецептора."
                            }
                        },
                        {
                            "text": "Enzyme activity was reduced due to <w>competitive</w> inhibition by the substrate analog.",
                            "target_lang_translations": {
                                "ru": "Активность фермента снизилась из-за <w>конкурентного</w> ингибирования аналогом субстрата."
                            }
                        }
                    ],
                    "common_phrases": [
                        "competitive inhibitor",
                        "competitive antagonist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к подавлению действия химического вещества другим веществом, конкурирующим с ним за связывание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конкурентный",
                                "target_lang_sense_clarification": "в биохимии (ингибирование)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
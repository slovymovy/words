            "senses": [
                {
                    "sense_id": "2907ffb1-7991-300a-9359-906435d564a5",
                    "sense_definition": "Shaped like the head of an arrow.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The archaeologist studied the <w>arrowheaded</w> characters inscribed on the ancient clay tablet.",
                            "target_lang_translations": {
                                "ru": "Археолог изучал <w>клинообразные</w> знаки, высеченные на древней глиняной табличке."
                            }
                        },
                        {
                            "text": "We identified the plant by its distinct <w>arrowheaded</w> leaves growing near the water.",
                            "target_lang_translations": {
                                "ru": "Мы опознали растение по его характерным <w>стреловидным</w> листьям, растущим у воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "arrow-shaped",
                        "cuneiform",
                        "sagittate"
                    ],
                    "common_phrases": [
                        "arrowheaded characters",
                        "arrowheaded leaves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий форму наконечника стрелы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стреловидный",
                                "target_lang_sense_clarification": "о форме листьев, узоров и т.д."
                            },
                            {
                                "target_lang_word": "клинообразный",
                                "target_lang_sense_clarification": "о форме знаков или символов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c61ae250-6077-3b4d-88cc-442a05565847",
                    "sense_definition": "Having an arrowhead attached to the tip.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum display featured an <w>arrowheaded</w> spear from the Bronze Age.",
                            "target_lang_translations": {
                                "ru": "В музейной экспозиции было представлено копье с <w>наконечником</w> бронзового века."
                            }
                        },
                        {
                            "text": "He carefully prepared the <w>arrowheaded</w> shaft before placing it in his quiver.",
                            "target_lang_translations": {
                                "ru": "Он тщательно подготовил древко с <w>наконечником</w>, прежде чем положить его в колчан."
                            }
                        }
                    ],
                    "synonyms": [
                        "tipped"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий прикрепленный к концу наконечник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снабженный наконечником",
                                "target_lang_sense_clarification": "об оружии или снарядах"
                            }
                        ]
                    }
                }
            ]
        }

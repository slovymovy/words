                {
                    "sense_id": "e8e0407e-6714-307a-ad62-33622c2c780a",
                    "sense_definition": "państwo w Europie Środkowej, graniczące między innymi z Polską, ze stolicą w Berlinie",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W te wakacje planujemy pojechać całą rodziną do <w>Niemiec</w>.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы планируем поехать всей семьей в <w>Германию</w>."
                            }
                        },
                        {
                            "text": "<w>Niemcy</w> są najważniejszym partnerem handlowym dla wielu krajów europejskich.",
                            "target_lang_translations": {
                                "ru": "<w>Германия</w> является важнейшим торговым партнером для многих европейских стран."
                            }
                        }
                    ],
                    "synonyms": [
                        "RFN",
                        "Republika Federalna Niemiec",
                        "Rajch"
                    ],
                    "common_phrases": [
                        "wyjechać do Niemiec",
                        "granica z Niemcami",
                        "życie w Niemczech"
                    ],
                    "target_lang_definitions": {
                        "ru": "государство в Центральной Европе, граничащее, в частности, с Польшей, со столицей в Берлине"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Германия",
                                "target_lang_sense_clarification": "страна"
                            }

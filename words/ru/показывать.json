{
    "word_family": [
        "показ",
        "показание",
        "показатель",
        "показательный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "773c9151-dbec-3312-bd42-5f1983950639",
                    "sense_definition": "Давать возможность увидеть что-либо, делать что-либо доступным для обозрения",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_presentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <показать> тебе мой новый дом.",
                            "target_lang_translations": {
                                "en": "I want to <show> you my new house."
                            }
                        },
                        {
                            "text": "Он часто <показывает> друзьям свои детские фотографии.",
                            "target_lang_translations": {
                                "en": "He often <shows> his childhood photos to his friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "демонстрировать",
                        "предъявлять"
                    ],
                    "antonyms": [
                        "прятать",
                        "скрывать"
                    ],
                    "common_phrases": [
                        "показывать пример",
                        "показывать дорогу",
                        "показывать лицо"
                    ],
                    "target_lang_definitions": {
                        "en": "To allow someone to see something; to make something visible or available for viewing"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "general sense"
                            },
                            {
                                "target_lang_word": "display",
                                "target_lang_sense_clarification": "to present to view"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31a70106-3d88-34b8-bcf9-22955cfb1ebe",
                    "sense_definition": "Указывать на кого-либо или что-либо жестом или предметом, обращая на это внимание",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_presentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гид <показывает> туристам самые красивые здания в городе.",
                            "target_lang_translations": {
                                "en": "The guide <shows> the tourists the most beautiful buildings in the city."
                            }
                        },
                        {
                            "text": "Мама учит ребёнка не <показывать> пальцем на людей.",
                            "target_lang_translations": {
                                "en": "Mom teaches the child not to <point> a finger at people."
                            }
                        }
                    ],
                    "synonyms": [
                        "указывать"
                    ],
                    "common_phrases": [
                        "показывать пальцем",
                        "показывать рукой",
                        "показывать на дверь"
                    ],
                    "target_lang_definitions": {
                        "en": "To direct attention to someone or something using a gesture or object"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "point",
                                "target_lang_sense_clarification": "usually with 'at' or 'to'"
                            },
                            {
                                "target_lang_word": "indicate",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "to guide attention"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16cfc778-1c75-361c-98c9-774443a8ec84",
                    "sense_definition": "Обозначать какую-либо величину или данные (о приборах, часах, измерительных инструментах)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мои часы <показывают>, что мы уже опаздываем.",
                            "target_lang_translations": {
                                "en": "My watch <shows> that we are already late."
                            }
                        },
                        {
                            "text": "Термометр на улице <показывает> двадцать градусов тепла.",
                            "target_lang_translations": {
                                "en": "The thermometer outside <shows> twenty degrees above zero."
                            }
                        }
                    ],
                    "common_phrases": [
                        "часы показывают",
                        "время показывает"
                    ],
                    "target_lang_definitions": {
                        "en": "To display a specific value, reading, or data (of instruments, clocks, etc.)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "general sense"
                            },
                            {
                                "target_lang_word": "read",
                                "target_lang_sense_clarification": "of a measuring instrument"
                            },
                            {
                                "target_lang_word": "indicate",
                                "target_lang_sense_clarification": "to give a reading"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4eea171-b997-3a26-82ad-7ae67217e510",
                    "sense_definition": "Транслировать изображение или находиться в рабочем состоянии (о телевизоре, экране)",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "device_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старый телевизор на даче плохо <показывает> из-за слабой антенны.",
                            "target_lang_translations": {
                                "en": "The old TV at the dacha <works> poorly because of the weak antenna."
                            }
                        },
                        {
                            "text": "В кинотеатре сегодня <показывают> новый интересный фильм.",
                            "target_lang_translations": {
                                "en": "They are <showing> a new interesting film at the cinema today."
                            }
                        }
                    ],
                    "common_phrases": [
                        "телевизор не показывает",
                        "показывать фильм"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "в значении 'работать исправно'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To broadcast a program/image or to be in working order (of a screen or TV)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "to broadcast or screen"
                            },
                            {
                                "target_lang_word": "play",
                                "target_lang_sense_clarification": "of a movie or content"
                            },
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "function properly (colloquial)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26df909d-aa48-3eb4-8e6a-dac2bd6edb90",
                    "sense_definition": "Проявлять какие-либо качества, свойства или добиваться результатов",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "demonstration_of_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спортсмен <показывает> отличные результаты на соревнованиях.",
                            "target_lang_translations": {
                                "en": "The athlete <shows> excellent results at the competition."
                            }
                        },
                        {
                            "text": "В сложной ситуации он <показывает> свой настоящий характер.",
                            "target_lang_translations": {
                                "en": "In a difficult situation, he <shows> his true character."
                            }
                        }
                    ],
                    "synonyms": [
                        "демонстрировать",
                        "выставлять"
                    ],
                    "antonyms": [
                        "скрывать"
                    ],
                    "common_phrases": [
                        "показывать класс",
                        "показывать себя",
                        "показывать результаты"
                    ],
                    "target_lang_definitions": {
                        "en": "To manifest qualities, abilities, or achieve results"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demonstrate",
                                "target_lang_sense_clarification": "to show clearly"
                            },
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "to reveal qualities"
                            },
                            {
                                "target_lang_word": "display",
                                "target_lang_sense_clarification": "to exhibit a quality"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e8b6b25-0ed3-301b-835d-085f8c63408c",
                    "sense_definition": "Давать официальные свидетельские показания на допросе или в суде",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Свидетель <показывает>, что видел подозреваемого возле банка.",
                            "target_lang_translations": {
                                "en": "The witness <testifies> that he saw the suspect near the bank."
                            }
                        },
                        {
                            "text": "На суде он отказался <показывать> против своих друзей.",
                            "target_lang_translations": {
                                "en": "In court, he refused to <testify> against his friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "свидетельствовать"
                    ],
                    "common_phrases": [
                        "показывать на суде",
                        "показывать против"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "юридический термин"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To give official testimony during an interrogation or in court"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "testify",
                                "target_lang_sense_clarification": "legal context"
                            },
                            {
                                "target_lang_word": "give evidence",
                                "target_lang_sense_clarification": "legal context"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
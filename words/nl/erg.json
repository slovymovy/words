                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "very much",
                                "target_lang_sense_clarification": "used with verbs like miss, love"
                            },
                            {
                                "target_lang_word": "a lot",
                                "target_lang_sense_clarification": "common adverbial usage"
                            },
                            {
                                "target_lang_word": "heavily",
                                "target_lang_sense_clarification": "used specifically with rain or snow"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сильно",
                                "target_lang_sense_clarification": "интенсивно"
                            },
                            {
                                "target_lang_word": "очень",
                                "target_lang_sense_clarification": "много"
                            }
                        ]
                    }
                }
            ]
        },
        {

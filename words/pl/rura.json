{
    "word_family": [
        "rurka",
        "rurowy",
        "rurociąg",
        "orurowanie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "013a76f6-5b4b-3c2c-b5a7-ba9102d43336",
                    "sense_definition": "Długi, pusty w środku przedmiot o przekroju koła, służący do transportu cieczy, gazów lub jako element konstrukcyjny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hydraulik musiał wymienić pękniętą <w>rurę</w> w łazience.",
                            "target_lang_translations": {
                                "ru": "Сантехнику пришлось заменить лопнувшую <w>трубу</w> в ванной."
                            }
                        },
                        {
                            "text": "Z tej starej <w>rury</w> wycieka brudna woda.",
                            "target_lang_translations": {
                                "ru": "Из этой старой <w>трубы</w> течет грязная вода."
                            }
                        }
                    ],
                    "synonyms": [
                        "tuba",
                        "duchówka",
                        "piecyk",
                        "piekarnik"
                    ],
                    "common_phrases": [
                        "rura wydechowa",
                        "rura kanalizacyjna",
                        "pęknięta rura"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный полый предмет круглого сечения для транспортировки жидкостей, газов или как элемент конструкции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "труба",
                                "target_lang_sense_clarification": "полый предмет"
                            },
                            {
                                "target_lang_word": "трубопровод",
                                "target_lang_sense_clarification": "система труб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b677aa9-1c89-3f81-9a77-a0264bd48784",
                    "sense_definition": "Pionowy, metalowy drążek przymocowany do podłogi i sufitu, używany do ćwiczeń gimnastycznych i tańca.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra trenuje akrobatyczny taniec na <w>rurze</w>.",
                            "target_lang_translations": {
                                "ru": "Моя сестра занимается акробатическими танцами на <w>пилоне</w>."
                            }
                        },
                        {
                            "text": "Zawodniczka wykonała bardzo trudną figurę na <w>rurze</w>.",
                            "target_lang_translations": {
                                "ru": "Спортсменка выполнила очень сложную фигуру на <w>шесте</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "taniec na rurze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальный металлический шест, прикрепленный к полу и потолку, используемый для гимнастики и танцев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пилон",
                                "target_lang_sense_clarification": "для танцев"
                            },
                            {
                                "target_lang_word": "шест",
                                "target_lang_sense_clarification": "гимнастический снаряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31fd1f93-9c48-3b96-957f-10f9e0217bea",
                    "sense_definition": "Część pieca kuchennego, w której piecze się potrawy; piekarnik.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "kitchen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia wstawiła ciasto do <w>rury</w>, żeby się upiekło.",
                            "target_lang_translations": {
                                "ru": "Бабушка поставила пирог в <w>духовку</w>, чтобы он испекся."
                            }
                        },
                        {
                            "text": "W starej kuchni węglowej chleb pieczono w gorącej <w>rurze</w>.",
                            "target_lang_translations": {
                                "ru": "В старой угольной плите хлеб пекли в горячей <w>духовке</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "piekarnik",
                        "duchówka"
                    ],
                    "common_phrases": [
                        "wstawić do rury"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often refers to the oven part of old stoves"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть кухонной плиты, в которой запекают еду; духовка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "духовка",
                                "target_lang_sense_clarification": "бытовая техника"
                            },
                            {
                                "target_lang_word": "духовой шкаф",
                                "target_lang_sense_clarification": "кухонное оборудование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9713eed-e808-324e-9be9-13af9926f0aa",
                    "sense_definition": "Pospolite, lekceważące lub obraźliwe określenie kobiety.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie słuchaj tej głupiej <w>rury</w>, ona nie ma racji.",
                            "target_lang_translations": {
                                "ru": "Не слушай эту глупую <w>мымру</w>, она не права."
                            }
                        },
                        {
                            "text": "Sąsiedzi kłócili się i nazwał ją starą <w>rurą</w>.",
                            "target_lang_translations": {
                                "ru": "Соседи ссорились, и он назвал её старой <w>каргой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stara rura"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубое, пренебрежительное или оскорбительное название женщины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карга",
                                "target_lang_sense_clarification": "о старой или злой женщине"
                            },
                            {
                                "target_lang_word": "мымра",
                                "target_lang_sense_clarification": "о неприятной женщине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3157d51-1e73-3603-9fbf-751023642eca",
                    "sense_definition": "Potoczne określenie lufy broni palnej, zwłaszcza dużej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czołg wycelował swoją długą <w>rurę</w> w stronę celu.",
                            "target_lang_translations": {
                                "ru": "Танк нацелил свой длинный <w>ствол</w> в сторону цели."
                            }
                        },
                        {
                            "text": "Żołnierz musiał wyczyścić <w>rurę</w> po strzelaniu.",
                            "target_lang_translations": {
                                "ru": "Солдат должен был почистить <w>ствол</w> после стрельбы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название ствола огнестрельного оружия, особенно крупного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ствол",
                                "target_lang_sense_clarification": "часть оружия"
                            },
                            {
                                "target_lang_word": "дуло",
                                "target_lang_sense_clarification": "отверстие ствола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f9a6680-5182-345c-aee6-e49b45df541e",
                    "sense_definition": "Regionalne, kruche ciastko o wygiętym kształcie, wypiekane w Poznaniu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem tradycyjne poznańskie <w>rury</w> na straganie.",
                            "target_lang_translations": {
                                "ru": "Я купил традиционное познанское <w>печенье</w> в ларьке."
                            }
                        },
                        {
                            "text": "Te słodkie <w>rury</w> są sprzedawane głównie w listopadzie.",
                            "target_lang_translations": {
                                "ru": "Это сладкое <w>печенье</w> продают в основном в ноябре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rury poznańskie"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Poznań"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное хрупкое печенье изогнутой формы, выпекаемое в Познани."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печенье",
                                "target_lang_sense_clarification": "кондитерское изделие"
                            },
                            {
                                "target_lang_word": "рура",
                                "target_lang_sense_clarification": "транслитерация названия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7c75c794-c352-33a9-b293-63a3ffdb06ed",
                    "sense_definition": "Okrzyk wzywający do natychmiastowej ucieczki lub szybkiego działania.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopaki, policja idzie, <w>rura</w>!",
                            "target_lang_translations": {
                                "ru": "Пацаны, полиция идет, <w>валим</w>!"
                            }
                        },
                        {
                            "text": "Kiedy zobaczyliśmy ochroniarza, krzyknąłem tylko: <w>rura</w>!",
                            "target_lang_translations": {
                                "ru": "Когда мы увидели охранника, я крикнул только: <w>газу</w>!"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, призывающий к немедленному бегству или быстрым действиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Газу!",
                                "target_lang_sense_clarification": "призыв ускориться"
                            },
                            {
                                "target_lang_word": "Валим!",
                                "target_lang_sense_clarification": "призыв уходить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
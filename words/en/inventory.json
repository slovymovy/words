                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>inventory</w> the warehouse before the shipment arrives.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>инвентаризировать</w> склад до прибытия груза."

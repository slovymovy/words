    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dcdf4d6d-0d28-3661-8b8c-f4501214d975",
                    "sense_definition": "Osoba, która broni kogoś lub czegoś przed atakiem, zagrożeniem lub krzywdą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Obrońcy</w> miasta musieli złożyć broń po długim oblężeniu.",
                            "target_lang_translations": {
                                "ru": "<w>Защитники</w> города были вынуждены сложить оружие после долгой осады."
                            }
                        },
                        {
                            "text": "Mój dziadek był znanym <w>obrońcą</w> praw zwierząt w naszej okolicy.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был известным <w>защитником</w> прав животных в нашей округе."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzecznik"
                    ],
                    "antonyms": [
                        "napastnik",
                        "agresor"
                    ],
                    "common_phrases": [
                        "obrońca praw człowieka",
                        "obrońca ojczyzny",
                        "obrońca uciśnionych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который защищает кого-либо или что-либо от нападения, угрозы или вреда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защитник",
                                "target_lang_sense_clarification": "тот, кто защищает"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6031e3d5-5f84-3568-8ce9-6cfc38a507bd",
                    "sense_definition": "Zawodnik w grach zespołowych, którego zadaniem jest zapobieganie zdobyciu punktów przez przeciwnika.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten piłkarz gra na pozycji <w>obrońcy</w> i rzadko strzela gole.",
                            "target_lang_translations": {
                                "ru": "Этот футболист играет на позиции <w>защитника</w> и редко забивает голы."
                            }
                        },
                        {
                            "text": "Nasz najlepszy <w>obrońca</w> doznał kontuzji w pierwszej połowie meczu.",
                            "target_lang_translations": {
                                "ru": "Наш лучший <w>защитник</w> получил травму в первом тайме матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "defensor"
                    ],
                    "antonyms": [
                        "napastnik"
                    ],
                    "common_phrases": [
                        "środkowy obrońca",
                        "lewy obrońca",
                        "boczny obrońca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игрок в командных видах спорта, чья задача — помешать противнику набрать очки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защитник",
                                "target_lang_sense_clarification": "в спорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2297bd74-1236-3c3f-b2ec-c8199bf6b631",
                    "sense_definition": "Prawnik reprezentujący oskarżonego w procesie sądowym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd przydzielił oskarżonemu <w>obrońcę</w> z urzędu.",
                            "target_lang_translations": {
                                "ru": "Суд назначил обвиняемому государственного <w>защитника</w>."
                            }
                        },
                        {
                            "text": "<w>Obrońca</w> wygłosił mowę końcową, prosząc o uniewinnienie swojego klienta.",
                            "target_lang_translations": {
                                "ru": "<w>Защитник</w> произнес заключительную речь, прося оправдать своего клиента."
                            }
                        }
                    ],
                    "synonyms": [
                        "adwokat",
                        "defensor"
                    ],
                    "antonyms": [
                        "oskarżyciel",
                        "prokurator"
                    ],
                    "common_phrases": [
                        "obrońca z urzędu",
                        "obrońca sądowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юрист, представляющий интересы обвиняемого в судебном процессе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защитник",
                                "target_lang_sense_clarification": "в суде"
                            },
                            {
                                "target_lang_word": "адвокат",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

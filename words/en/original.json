{
    "word_family": [
        "origin",
        "originate",
        "originality",
        "originally",
        "originator"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b61c5aaf-631c-32b2-9fb8-4a19bf45de93",
                    "sense_definition": "Relating to the origin or beginning; preceding all others.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>original</w> plan was to go to the beach, but it rained.",
                            "target_lang_translations": {
                                "ru": "<w>Первоначальный</w> план состоял в том, чтобы пойти на пляж, но пошел дождь."
                            }
                        },
                        {
                            "text": "The house still has its <w>original</w> windows from the 1920s.",
                            "target_lang_translations": {
                                "ru": "В доме сохранились <w>первоначальные</w> окна 1920-х годов."
                            }
                        }
                    ],
                    "synonyms": [
                        "initial"
                    ],
                    "antonyms": [
                        "ultimate"
                    ],
                    "common_phrases": [
                        "original plan",
                        "original purpose",
                        "original state"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к происхождению или началу; предшествующий всем остальным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "первоначальный",
                                "target_lang_sense_clarification": "существующий с самого начала"
                            },
                            {
                                "target_lang_word": "исходный",
                                "target_lang_sense_clarification": "начальный, отправной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee62a83c-b9ff-3aff-a5c3-168b62fdafc6",
                    "sense_definition": "First in a series of copies or versions; authentic and not a copy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must show the <w>original</w> document to the officer, not a photocopy.",
                            "target_lang_translations": {
                                "ru": "Вы должны предъявить офицеру <w>подлинный</w> документ, а не ксерокопию."
                            }
                        },
                        {
                            "text": "The <w>original</w> version of the song is much better than the cover.",
                            "target_lang_translations": {
                                "ru": "<w>Оригинальная</w> версия песни намного лучше кавера."
                            }
                        }
                    ],
                    "antonyms": [
                        "copy",
                        "reproduction"
                    ],
                    "common_phrases": [
                        "original version",
                        "original document",
                        "original text"
                    ],
                    "target_lang_definitions": {
                        "ru": "Первый в серии копий или версий; подлинный, а не копия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оригинальный",
                                "target_lang_sense_clarification": "являющийся оригиналом, не копия"
                            },
                            {
                                "target_lang_word": "подлинный",
                                "target_lang_sense_clarification": "настоящий, достоверный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfabb707-c509-3eea-b284-622d87f19bf0",
                    "sense_definition": "Fresh, different, and newly created; not derived from something else.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The student presented an <w>original</w> idea for the science project.",
                            "target_lang_translations": {
                                "ru": "Студент представил <w>оригинальную</w> идею для научного проекта."
                            }
                        },
                        {
                            "text": "Her writing style is highly <w>original</w> and unlike anyone else's.",
                            "target_lang_translations": {
                                "ru": "Ее стиль письма весьма <w>своеобразен</w> и не похож ни на чей другой."
                            }
                        }
                    ],
                    "antonyms": [
                        "derivative"
                    ],
                    "common_phrases": [
                        "original idea",
                        "original work",
                        "highly original"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свежий, необычный и недавно созданный; не заимствованный у других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оригинальный",
                                "target_lang_sense_clarification": "неповторимый, творческий"
                            },
                            {
                                "target_lang_word": "своеобразный",
                                "target_lang_sense_clarification": "отличающийся от других, особенный"
                            },
                            {
                                "target_lang_word": "самобытный",
                                "target_lang_sense_clarification": "самостоятельный в своем развитии или творчестве"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3985f990-57a8-35f6-ba54-64a5a6950ff0",
                    "sense_definition": "An object or creation from which all later copies and variations are derived.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please sign the <w>original</w> and keep a copy for your records.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подпишите <w>оригинал</w> и оставьте копию себе."
                            }
                        },
                        {
                            "text": "This movie is a remake, but the <w>original</w> was released in the fifties.",
                            "target_lang_translations": {
                                "ru": "Этот фильм — ремейк, а <w>оригинал</w> вышел в пятидесятых."
                            }
                        }
                    ],
                    "synonyms": [
                        "prototype"
                    ],
                    "antonyms": [
                        "copy",
                        "reproduction",
                        "remake",
                        "derivative"
                    ],
                    "common_phrases": [
                        "in the original",
                        "keep the original"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объект или творение, на основе которого создаются все последующие копии и вариации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оригинал",
                                "target_lang_sense_clarification": "первоначальный образец"
                            },
                            {
                                "target_lang_word": "подлинник",
                                "target_lang_sense_clarification": "настоящая вещь, рукопись или документ"
                            },
                            {
                                "target_lang_word": "первоисточник",
                                "target_lang_sense_clarification": "исходный текст или материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "beb52bd5-6449-39c8-ab25-ae44adf67de0",
                    "sense_definition": "A person with a unique and interesting personality or creative talent.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a true <w>original</w> who never follows the latest trends.",
                            "target_lang_translations": {
                                "ru": "Он настоящий <w>оригинал</w>, который никогда не следит за последними трендами."
                            }
                        },
                        {
                            "text": "The fashion industry needs an <w>original</w> like her to shake things up.",
                            "target_lang_translations": {
                                "ru": "Индустрии моды нужен такой <w>оригинал</w>, как она, чтобы встряхнуть ситуацию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "true original"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек с уникальной и интересной личностью или творческим талантом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оригинал",
                                "target_lang_sense_clarification": "чудак, необычный человек"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a01834ee-b193-338d-ab90-8a226821308b",
                    "sense_definition": "The act of officially suggesting someone or something for a job, position, or prize.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "act_and_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <w>nomination</w> for the award came as a pleasant surprise to everyone in the office.",
                            "target_lang_translations": {
                                "ru": "Её <w>выдвижение</w> на эту награду стало для всех в офисе приятным сюрпризом."
                            }
                        },
                        {
                            "text": "The committee announced his <w>nomination</w> for the position of president late yesterday evening.",
                            "target_lang_translations": {
                                "ru": "Вчера поздно вечером комитет объявил о его <w>выдвижении</w> на пост президента."
                            }
                        }
                    ],
                    "synonyms": [
                        "proposal",
                        "recommendation",
                        "submission",
                        "selection"
                    ],
                    "antonyms": [
                        "rejection",
                        "disqualification"
                    ],
                    "common_phrases": [
                        "accept the nomination",
                        "secure the nomination",
                        "presidential nomination",
                        "nomination for an award"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по официальному предложению кого-либо на должность или на соискание награды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдвиже́ние",
                                "target_lang_sense_clarification": "кандидатуры"
                            },
                            {
                                "target_lang_word": "номина́ция",
                                "target_lang_sense_clarification": "на премию или в конкурсе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a7c269b-fb04-3bf6-b325-2ebfcd46be30",
                    "sense_definition": "The state of having been proposed for a position or honor, or the specific entry that is proposed.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "act_and_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The film received a <w>nomination</w> for best picture but did not win the final trophy.",
                            "target_lang_translations": {
                                "ru": "Фильм получил <w>номинацию</w> на звание лучшей картины, но не выиграл главный приз."
                            }
                        },
                        {
                            "text": "There were several strong <w>nominations</w> for the committee to consider this year.",
                            "target_lang_translations": {
                                "ru": "В этом году комитету предстояло рассмотреть несколько сильных <w>кандидатур</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "candidacy",
                        "entry"
                    ],
                    "common_phrases": [
                        "receive a nomination",
                        "list of nominations",
                        "withdraw a nomination"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние выдвижения на должность или награду, а также конкретная категория в конкурсе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "номина́ция",
                                "target_lang_sense_clarification": "категория или статус"
                            },
                            {
                                "target_lang_word": "кандидату́ра",
                                "target_lang_sense_clarification": "предложенное лицо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4c917c5-6c89-3242-9447-5c238f5c8df2",
                    "sense_definition": "A formal right, device, or means by which a person is appointed to a position.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The contract included a <w>nomination</w> clause that allowed the owner to appoint a successor.",
                            "target_lang_translations": {
                                "ru": "Контракт включал пункт о <w>назначении</w>, который позволял владельцу назначить преемника."
                            }
                        },
                        {
                            "text": "He exercised his power of <w>nomination</w> to fill the vacant seat on the board.",
                            "target_lang_translations": {
                                "ru": "Он воспользовался своим правом <w>назначения</w>, чтобы заполнить вакантное место в совете."
                            }
                        }
                    ],
                    "synonyms": [
                        "appointment",
                        "designation"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical or legal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное право или способ назначения кого-либо на должность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "назначе́ние",
                                "target_lang_sense_clarification": "на должность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            {
                                "target_lang_word": "активно",
                                "target_lang_sense_clarification": "о профессиональной деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5940a88-04a1-3614-82f3-0b55b0879bd8",

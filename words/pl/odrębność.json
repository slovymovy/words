                            {
                                "target_lang_word": "самобытность",
                                "target_lang_sense_clarification": "оригинальность, уникальность (часто о культуре)"
                            },
                            {
                                "target_lang_word": "индивидуальность",
                                "target_lang_sense_clarification": "неповторимость личности"

{
    "word_family": [
        "reservering",
        "reservatie",
        "reserve"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "94ca2ff3-7aed-3ba4-844a-d4ed8df07d1d",
                    "sense_definition": "Een plaats, kaartje of dienst laten vrijhouden om er later gebruik van te maken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij willen graag een tafel <w>reserveren</w> voor vier personen.",
                            "target_lang_translations": {
                                "en": "We would like to <w>reserve</w> a table for four people.",
                                "ru": "Мы хотели бы <w>забронировать</w> столик на четверых."
                            }
                        },
                        {
                            "text": "Heb je al een kamer <w>gereserveerd</w> in dat nieuwe hotel?",
                            "target_lang_translations": {
                                "en": "Have you already <w>booked</w> a room in that new hotel?",
                                "ru": "Ты уже <w>забронировал</w> номер в том новом отеле?"
                            }
                        }
                    ],
                    "synonyms": [
                        "boeken",
                        "bespreken"
                    ],
                    "antonyms": [
                        "annuleren"
                    ],
                    "common_phrases": [
                        "een tafel reserveren",
                        "een kamer reserveren",
                        "kaarten reserveren"
                    ],
                    "target_lang_definitions": {
                        "en": "To arrange for a place, ticket, or service to be kept for later use.",
                        "ru": "Зарезервировать или заказать место, билет или услугу для использования в будущем."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "book",
                                "target_lang_sense_clarification": "to arrange for a seat, room, etc. in advance"
                            },
                            {
                                "target_lang_word": "reserve",
                                "target_lang_sense_clarification": "to keep for a specific person or time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бронировать",
                                "target_lang_sense_clarification": "заказывать заранее (столик, номер, билет)"
                            },
                            {
                                "target_lang_word": "резервировать",
                                "target_lang_sense_clarification": "оставлять за собой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "780c60e5-b077-3f72-9f93-ae131de59d46",
                    "sense_definition": "Iets bewaren of opzijzetten voor later gebruik of een specifiek doel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>reserveer</w> elke maand een vast bedrag voor onvoorziene uitgaven.",
                            "target_lang_translations": {
                                "en": "I <w>set aside</w> a fixed amount every month for unforeseen expenses.",
                                "ru": "Я каждый месяц <w>откладываю</w> фиксированную сумму на непредвиденные расходы."
                            }
                        },
                        {
                            "text": "Je moet voldoende tijd <w>reserveren</w> om de opdracht goed uit te voeren.",
                            "target_lang_translations": {
                                "en": "You need to <w>set aside</w> enough time to complete the assignment well.",
                                "ru": "Нужно <w>выделить</w> достаточно времени, чтобы хорошо выполнить задание."
                            }
                        }
                    ],
                    "synonyms": [
                        "bewaren",
                        "opzijzetten",
                        "apart houden"
                    ],
                    "antonyms": [
                        "opmaken",
                        "uitgeven"
                    ],
                    "common_phrases": [
                        "tijd reserveren",
                        "geld reserveren"
                    ],
                    "target_lang_definitions": {
                        "en": "To keep or put something aside for future use or a specific purpose.",
                        "ru": "Откладывать или сохранять что-либо для использования позже или для определенной цели."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "set aside",
                                "target_lang_sense_clarification": "to keep money or time for a particular purpose"
                            },
                            {
                                "target_lang_word": "reserve",
                                "target_lang_sense_clarification": "to keep back for future use"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "сберегать (деньги, ресурсы)"
                            },
                            {
                                "target_lang_word": "выделять",
                                "target_lang_sense_clarification": "назначать для определенной цели (время)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "97b261b9-f1cb-3e49-833f-61bad4f94e5b",
                    "sense_definition": "Przebywać w jakimś miejscu, być z kimś lub zajmować się czymś przez pewien czas.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każde lato <spędzamy> nad morzem, ciesząc się słońcem i plażą.",
                            "target_lang_translations": {
                                "ru": "Каждое лето мы <проводим> на море, наслаждаясь солнцем и пляжем."
                            }
                        },
                        {
                            "text": "Bardzo lubię <spędzać> wolny czas na czytaniu ciekawych książek.",
                            "target_lang_translations": {
                                "ru": "Я очень люблю <проводить> свободное время за чтением интересных книг."
                            }
                        }
                    ],
                    "synonyms": [
                        "przebywać"
                    ],
                    "common_phrases": [
                        "spędzać czas",
                        "spędzać wakacje",
                        "spędzać urlop",
                        "spędzać święta",
                        "miło spędzać czas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться где-либо, быть с кем-либо или заниматься чем-либо в течение определенного времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проводить",
                                "target_lang_sense_clarification": "время"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff827a77-696d-3919-a932-16085ec4366e",
                    "sense_definition": "Zmuszać ludzi lub zwierzęta do przejścia i zgromadzenia się w jednym miejscu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action_gather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pasterze zaczęli <spędzać> stado owiec do zagrody przed nadchodzącą nocą.",
                            "target_lang_translations": {
                                "ru": "Пастухи начали <сгонять> стадо овец в загон перед наступлением ночи."
                            }
                        },
                        {
                            "text": "Strażnicy musieli <spędzać> wszystkich gapiów w bezpieczne miejsce z dala od pożaru.",
                            "target_lang_translations": {
                                "ru": "Охранникам пришлось <сгонять> всех зевак в безопасное место подальше от пожара."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaganiać",
                        "gromadzić",
                        "zbierać"
                    ],
                    "antonyms": [
                        "rozpędzać"
                    ],
                    "common_phrases": [
                        "spędzać bydło",
                        "spędzać owce",
                        "spędzać ludzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принуждать людей или животных переместиться и собраться в одном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сгонять",
                                "target_lang_sense_clarification": "собирать в одно место"
                            },
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "в группу, в стадо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40442b35-5a29-33c1-8457-cfab5e33a541",
                    "sense_definition": "Zmuszać kogoś lub coś do opuszczenia zajmowanego miejsca; usuwać lub wyganiać.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action_remove",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik musiał wielokrotnie <spędzać> ptaki z pola, aby nie zjadły ziarna.",
                            "target_lang_translations": {
                                "ru": "Фермеру приходилось много раз <сгонять> птиц с поля, чтобы они не склевали зерно."
                            }
                        },
                        {
                            "text": "To zmartwienie <spędza> mi sen z powiek każdej nocy.",
                            "target_lang_translations": {
                                "ru": "Это беспокойство <прогоняет> мой сон каждую ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "przepędzać",
                        "wyganiać",
                        "usuwać"
                    ],
                    "common_phrases": [
                        "spędzać sen z powiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-либо или что-либо покинуть занимаемое место; удалять или прогонять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сгонять",
                                "target_lang_sense_clarification": "заставлять слезть или уйти"
                            },
                            {
                                "target_lang_word": "прогонять",
                                "target_lang_sense_clarification": "удалять, выгонять"
                            }
                        ]
                    }
                }
            ]
        }
    ]

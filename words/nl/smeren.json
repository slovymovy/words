                    ],
                    "synonyms": [
                        "oliën",
                        "invetten"
                    ],
                    "common_phrases": [
                        "de ketting smeren",
                        "de keel smeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить масло или жир на движущиеся части для уменьшения трения."

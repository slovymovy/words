    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0d96fb59-c732-3294-a5f4-f41351031b1c",
                    "sense_definition": "Having qualities that people like; attractive or interesting.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The idea of working from home is very <w>appealing</w> to many employees.",
                            "target_lang_translations": {
                                "ru": "Идея работы из дома очень <w>привлекательна</w> для многих сотрудников."
                            }
                        },
                        {
                            "text": "She found the bright colors and modern design visually <w>appealing</w>.",
                            "target_lang_translations": {
                                "ru": "Она сочла яркие цвета и современный дизайн визуально <w>привлекательными</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "attractive",
                        "engaging",
                        "pleasing",
                        "tempting"
                    ],
                    "antonyms": [
                        "unappealing"
                    ],
                    "common_phrases": [
                        "visually appealing",
                        "find appealing",
                        "appealing idea",
                        "broadly appealing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий качествами, которые нравятся людям; привлекательный или интересный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привлекательный",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "заманчивый",
                                "target_lang_sense_clarification": "об идее, предложении или перспективе"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "120d2f51-3c1b-3f50-93c0-900259d1f865",
                    "sense_definition": "Present participle and gerund of <w>appeal</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police are <w>appealing</w> for witnesses to the accident.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>призывает</w> свидетелей аварии откликнуться."
                            }
                        },
                        {
                            "text": "He is <w>appealing</w> against the decision made by the local council.",
                            "target_lang_translations": {
                                "ru": "Он <w>обжалует</w> решение, принятое местным советом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "appealing for help",
                        "appealing against"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола appeal (обращаться, призывать, обжаловать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "призывающий",
                                "target_lang_sense_clarification": "обращающийся с просьбой или требованием"
                            },
                            {
                                "target_lang_word": "обжалующий",
                                "target_lang_sense_clarification": "подающий апелляцию (юридический термин)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fe028098-13a2-3206-8abd-8bedfbd88d84",
                    "sense_definition": "The act of making a serious or urgent request.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His desperate <w>appealings</w> for mercy were ignored by the captors.",
                            "target_lang_translations": {
                                "ru": "Его отчаянные <w>мольбы</w> о пощаде были проигнорированы похитителями."
                            }
                        },
                        {
                            "text": "The constant <w>appealings</w> of the crowd eventually moved the king.",
                            "target_lang_translations": {
                                "ru": "Постоянные <w>призывы</w> толпы в конце концов тронули короля."
                            }
                        }
                    ],
                    "synonyms": [
                        "pleas",
                        "entreaties"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "The plural form is rarely used in modern English."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, выражающее серьезную или настойчивую просьбу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мольбы",
                                "target_lang_sense_clarification": "эмоциональные, отчаянные просьбы"
                            },
                            {
                                "target_lang_word": "призывы",
                                "target_lang_sense_clarification": "настойчивые обращения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "kil",
        "verkilling"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bfa366a2-1dfc-3142-aba6-ea8a55219a01",
                    "sense_definition": "Koud worden, of in figuurlijke zin minder hartelijk en afstandelijker worden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen de zon onderging, <w>verkilde</w> de lucht onmiddellijk.",
                            "target_lang_translations": {
                                "en": "When the sun set, the air immediately <w>grew cold</w>.",
                                "ru": "Когда солнце село, воздух мгновенно <w>остыл</w>."
                            }
                        },
                        {
                            "text": "Hun vriendschap <w>verkilde</w> nadat ze naar een andere stad verhuisde.",
                            "target_lang_translations": {
                                "en": "Their friendship <w>cooled</w> after she moved to another city.",
                                "ru": "Их дружба <w>охладела</w> после того, как она переехала в другой город."
                            }
                        }
                    ],
                    "synonyms": [
                        "afkoelen",
                        "bekoelen"
                    ],
                    "antonyms": [
                        "opwarmen"
                    ],
                    "common_phrases": [
                        "de relatie verkilde"
                    ],
                    "target_lang_definitions": {
                        "en": "To become cold, or figuratively to become less warm and more distant.",
                        "ru": "Становиться холодным или, в переносном смысле, менее сердечным и более отстраненным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cool",
                                "target_lang_sense_clarification": "to become less enthusiastic or friendly"
                            },
                            {
                                "target_lang_word": "grow cold",
                                "target_lang_sense_clarification": "to drop in temperature or warmth"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "остывать",
                                "target_lang_sense_clarification": "понижаться в температуре"
                            },
                            {
                                "target_lang_word": "охладевать",
                                "target_lang_sense_clarification": "об отношениях или чувствах"
                            },
                            {
                                "target_lang_word": "холодать",
                                "target_lang_sense_clarification": "о погоде или воздухе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afbb342d-1b83-31fc-bd71-8072ab3ac3e5",
                    "sense_definition": "Iets koud maken of de temperatuur ervan doen dalen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De snijdende wind <w>verkilde</w> mijn handen en gezicht.",
                            "target_lang_translations": {
                                "en": "The biting wind <w>chilled</w> my hands and face.",
                                "ru": "Пронзительный ветер <w>леденил</w> мне руки и лицо."
                            }
                        },
                        {
                            "text": "Zijn cynische opmerking <w>verkilde</w> de sfeer in de vergaderzaal.",
                            "target_lang_translations": {
                                "en": "His cynical remark <w>chilled</w> the atmosphere in the meeting room.",
                                "ru": "Его циничное замечание <w>охладило</w> атмосферу в конференц-зале."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To make something cold or cause its temperature to drop.",
                        "ru": "Делать что-либо холодным или понижать температуру чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chill",
                                "target_lang_sense_clarification": "to make cold"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "охлаждать",
                                "target_lang_sense_clarification": "делать холодным"
                            },
                            {
                                "target_lang_word": "леденить",
                                "target_lang_sense_clarification": "вызывать чувство сильного холода"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
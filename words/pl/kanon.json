                            {
                                "target_lang_word": "золотой фонд",
                                "target_lang_sense_clarification": "лучшие образцы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e5c93cb-9315-3338-a293-77c3f7924c27",
                    "sense_definition": "Ogólnie przyjęta zasada, norma lub wzorzec postępowania, estetyki czy piękna.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "rule_standard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Współczesny <w>kanon</w> piękna różni się znacznie od tego z epoki baroku.",
                            "target_lang_translations": {
                                "ru": "Современный <w>канон</w> красоты значительно отличается от канона эпохи барокко."
                            }
                        },
                        {
                            "text": "Artysta ten łamie wszelkie ustalone <w>kanony</w> sztuki.",
                            "target_lang_translations": {
                                "ru": "Этот художник нарушает все установленные <w>каноны</w> искусства."
                            }
                        }

                {
                    "sense_id": "be310345-7481-3915-ba74-c43d32fe8cc9",
                    "sense_definition": "A major Semitic language originating from the Arabian peninsula, spoken across the Middle East and North Africa.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "language",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "She is learning to speak <w>Arabic</w> at the university.",
                            "target_lang_translations": {
                                "ru": "Она учится говорить <w>по-арабски</w> в университете."
                            }
                        },
                        {
                            "text": "The document was translated from <w>Arabic</w> into English.",
                            "target_lang_translations": {
                                "ru": "Документ был переведен с <w>арабского</w> на английский."
                            }
                        }
                    ],
                    "common_phrases": [
                        "speak Arabic",
                        "learn Arabic",
                        "Classical Arabic",
                        "Egyptian Arabic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Семитский язык, возникший на Аравийском полуострове, на котором говорят на Ближнем Востоке и в Северной Африке."

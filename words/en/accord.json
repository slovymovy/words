{
    "word_family": [
        "accordance",
        "accordingly"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "35f30782-0636-3e73-83a6-9c2f8d5459ba",
                    "sense_definition": "A voluntary desire or spontaneous impulse to act, usually without being asked or forced.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "volition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to leave the company of her own <w>accord</w>.",
                            "target_lang_translations": {
                                "ru": "Она решила уйти из компании по собственной <w>воле</w>."
                            }
                        },
                        {
                            "text": "The heavy door seemed to open of its own <w>accord</w>.",
                            "target_lang_translations": {
                                "ru": "Казалось, тяжелая дверь открылась сама <w>собой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "of one's own accord",
                        "of its own accord"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добровольное желание или спонтанный порыв к действию, обычно без просьбы или принуждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воля",
                                "target_lang_sense_clarification": "желание, собственное намерение"
                            },
                            {
                                "target_lang_word": "побуждение",
                                "target_lang_sense_clarification": "внутренний порыв"
                            },
                            {
                                "target_lang_word": "желание",
                                "target_lang_sense_clarification": "стремление сделать что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42f9c2cb-f7e3-3c7b-808b-d26b51b2399d",
                    "sense_definition": "A formal agreement or treaty between countries, groups, or organizations.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two nations signed a peace <w>accord</w> to end the conflict.",
                            "target_lang_translations": {
                                "ru": "Две страны подписали мирное <w>соглашение</w>, чтобы положить конец конфликту."
                            }
                        },
                        {
                            "text": "The Geneva <w>Accord</w> was a significant step in international diplomacy.",
                            "target_lang_translations": {
                                "ru": "Женевское <w>соглашение</w> стало важным шагом в международной дипломатии."
                            }
                        }
                    ],
                    "synonyms": [
                        "treaty"
                    ],
                    "common_phrases": [
                        "peace accord",
                        "trade accord",
                        "sign an accord"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное соглашение или договор между странами, группами или организациями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соглашение",
                                "target_lang_sense_clarification": "официальная договоренность"
                            },
                            {
                                "target_lang_word": "договор",
                                "target_lang_sense_clarification": "официальный документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d94fbc6-7189-3ffa-80bb-ae3c7bcfad6b",
                    "sense_definition": "A state of agreement, harmony, or unity in opinion or action.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agreement_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee members acted with one <w>accord</w> to solve the crisis.",
                            "target_lang_translations": {
                                "ru": "Члены комитета действовали в полном <w>согласии</w> для разрешения кризиса."
                            }
                        },
                        {
                            "text": "His behavior was not in <w>accord</w> with the school's principles.",
                            "target_lang_translations": {
                                "ru": "Его поведение не находилось в <w>соответствии</w> с принципами школы."
                            }
                        }
                    ],
                    "synonyms": [
                        "consent",
                        "assent"
                    ],
                    "antonyms": [
                        "discord"
                    ],
                    "common_phrases": [
                        "with one accord",
                        "in accord with"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние согласия, гармонии или единства во мнениях или действиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласие",
                                "target_lang_sense_clarification": "единство мнений, отсутствие противоречий"
                            },
                            {
                                "target_lang_word": "единодушие",
                                "target_lang_sense_clarification": "полное совпадение мнений"
                            },
                            {
                                "target_lang_word": "гармония",
                                "target_lang_sense_clarification": "согласованность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0ec6a2ac-82a8-33fe-b311-b300d17c445b",
                    "sense_definition": "To give or grant someone power, status, or recognition formally.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "granting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The visiting president was <w>accorded</w> a warm welcome by the crowd.",
                            "target_lang_translations": {
                                "ru": "Приезжему президенту был <w>оказан</w> теплый прием толпой."
                            }
                        },
                        {
                            "text": "Every citizen should be <w>accorded</w> the right to a fair trial.",
                            "target_lang_translations": {
                                "ru": "Каждому гражданину должно быть <w>предоставлено</w> право на справедливое судебное разбирательство."
                            }
                        }
                    ],
                    "synonyms": [
                        "render"
                    ],
                    "antonyms": [
                        "withhold"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально предоставлять или даровать кому-либо власть, статус или признание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предоставлять",
                                "target_lang_sense_clarification": "давать право или возможность"
                            },
                            {
                                "target_lang_word": "оказывать",
                                "target_lang_sense_clarification": "проявлять уважение, оказывать прием"
                            },
                            {
                                "target_lang_word": "даровать",
                                "target_lang_sense_clarification": "торжественно давать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58245670-950a-3cc6-acac-e4a4c656f5a7",
                    "sense_definition": "To be consistent with or match something; to be in harmony.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His version of the events does not <w>accord</w> with the evidence found.",
                            "target_lang_translations": {
                                "ru": "Его версия событий не <w>согласуется</w> с найденными уликами."
                            }
                        },
                        {
                            "text": "We need to ensure that our actions <w>accord</w> with our stated values.",
                            "target_lang_translations": {
                                "ru": "Нам нужно убедиться, что наши действия <w>соответствуют</w> нашим заявленным ценностям."
                            }
                        }
                    ],
                    "synonyms": [
                        "agree",
                        "sit"
                    ],
                    "antonyms": [
                        "clash"
                    ],
                    "common_phrases": [
                        "accord with"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствовать чему-либо или совпадать с чем-либо; находиться в гармонии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соответствовать",
                                "target_lang_sense_clarification": "быть сообразным, подходящим"
                            },
                            {
                                "target_lang_word": "согласовываться",
                                "target_lang_sense_clarification": "быть в согласии, не противоречить"
                            },
                            {
                                "target_lang_word": "гармонировать",
                                "target_lang_sense_clarification": "находиться в гармонии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
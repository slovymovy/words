            "senses": [
                {
                    "sense_id": "a073c1c5-bcc3-3c9e-9d16-2136defdcf3e",
                    "sense_definition": "The reporting of news, events, or topics in newspapers, on television, radio, or the internet.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The election received massive media <w>coverage</w> across all major channels.",
                            "target_lang_translations": {
                                "ru": "Выборы получили широкое <w>освещение</w> в СМИ по всем основным каналам."
                            }
                        },
                        {
                            "text": "We provide live <w>coverage</w> of sports events from around the world.",
                            "target_lang_translations": {
                                "ru": "Мы обеспечиваем <w>освещение</w> спортивных событий со всего мира в прямом эфире."
                            }
                        }
                    ],
                    "synonyms": [
                        "reporting",
                        "reportage",
                        "press"
                    ],
                    "common_phrases": [
                        "media coverage",
                        "press coverage",
                        "live coverage",
                        "news coverage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Информирование о новостях, событиях или темах в газетах, на телевидении, радио или в интернете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освещение",
                                "target_lang_sense_clarification": "событий в СМИ"
                            },
                            {
                                "target_lang_word": "репортаж",
                                "target_lang_sense_clarification": "с места событий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91329ff6-f130-3178-b662-19186f8f1e71",
                    "sense_definition": "The geographic area where a mobile phone network, radio station, or other service is available and effective.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mobile phone <w>coverage</w> is often poor in remote mountain areas.",
                            "target_lang_translations": {
                                "ru": "Мобильное <w>покрытие</w> часто бывает плохим в отдаленных горных районах."
                            }
                        },
                        {
                            "text": "The company is working to expand its network <w>coverage</w> to rural towns.",
                            "target_lang_translations": {
                                "ru": "Компания работает над расширением зоны <w>покрытия</w> своей сети в сельских городах."

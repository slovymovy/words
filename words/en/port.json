                    "frequency": "VeryLow",
                    "semantic_group_id": "luggage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He packed his clothes into his <w>port</w> and headed for the train station.",
                            "target_lang_translations": {
                                "ru": "Он упаковал одежду в свой <w>чемодан</w> и направился на вокзал."
                            }
                        },
                        {
                            "text": "The children threw their <w>ports</w> on the rack outside the classroom.",
                            "target_lang_translations": {
                                "ru": "Дети бросили свои <w>портфели</w> на полку возле класса."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Australia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чемодан или школьный портфель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чемодан",
                                "target_lang_sense_clarification": "багаж"
                            },
                            {
                                "target_lang_word": "портфель",
                                "target_lang_sense_clarification": "школьная сумка"
                            }
                        ]
                    }
                }
            ]

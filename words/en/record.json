{
    "word_family": [
        "recording",
        "recorder"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "211dc091-3d72-3bbe-92da-5e0c9e9dd083",
                    "sense_definition": "Information about something that is written down or stored on a computer so that it can be looked at in the future.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "info_log",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor looked at my medical <w>records</w> before prescribing the medication.",
                            "target_lang_translations": {
                                "ru": "Врач просмотрел мои медицинские <w>записи</w>, прежде чем выписать лекарство."
                            }
                        },
                        {
                            "text": "We have no <w>record</w> of you making this payment to us.",
                            "target_lang_translations": {
                                "ru": "У нас нет <w>записи</w> о том, что вы совершали этот платеж нам."
                            }
                        }
                    ],
                    "synonyms": [
                        "log"
                    ],
                    "common_phrases": [
                        "keep a record",
                        "medical record",
                        "criminal record",
                        "for the record",
                        "on record"
                    ],
                    "target_lang_definitions": {
                        "ru": "Информация о чем-либо, записанная или сохраненная на компьютере для последующего использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запись",
                                "target_lang_sense_clarification": "информация"
                            },
                            {
                                "target_lang_word": "документация",
                                "target_lang_sense_clarification": "совокупность документов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28449607-461e-38eb-8fdd-311b3b50c0ae",
                    "sense_definition": "The best result or highest level ever achieved in a sport or activity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "achievement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He broke the world <w>record</w> for the 100-meter sprint.",
                            "target_lang_translations": {
                                "ru": "Он побил мировой <w>рекорд</w> в беге на 100 метров."
                            }
                        },
                        {
                            "text": "The team set a new <w>record</w> for the most points scored in a single game.",
                            "target_lang_translations": {
                                "ru": "Команда установила новый <w>рекорд</w> по количеству очков, набранных за одну игру."
                            }
                        }
                    ],
                    "common_phrases": [
                        "break a record",
                        "set a record",
                        "world record",
                        "hold the record"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лучший результат или наивысший уровень, когда-либо достигнутый в спорте или другой деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рекорд",
                                "target_lang_sense_clarification": "высшее достижение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "366e562c-a402-3cbb-9d9b-1b8d6e86f607",
                    "sense_definition": "A flat plastic disc on which music is stored, used especially in the past.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "audio_music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandfather has a large collection of old jazz <w>records</w>.",
                            "target_lang_translations": {
                                "ru": "У моего дедушки большая коллекция старых джазовых <w>пластинок</w>."
                            }
                        },
                        {
                            "text": "I still like listening to <w>records</w> better than CDs because of the sound quality.",
                            "target_lang_translations": {
                                "ru": "Мне всё ещё больше нравится слушать <w>пластинки</w>, чем компакт-диски, из-за качества звука."
                            }
                        }
                    ],
                    "synonyms": [
                        "vinyl",
                        "disc",
                        "phonograph record"
                    ],
                    "common_phrases": [
                        "play a record",
                        "vinyl record"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоский пластиковый диск, на котором записана музыка; использовался преимущественно в прошлом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пластинка",
                                "target_lang_sense_clarification": "музыкальный носитель"
                            },
                            {
                                "target_lang_word": "грампластинка",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b7640ff-6ee9-3d37-b98a-5caf0f462e16",
                    "sense_definition": "A data structure in computing that stores a set of related data items.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "info_log",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Each <w>record</w> in the database contains a customer's name and address.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>запись</w> в базе данных содержит имя и адрес клиента."
                            }
                        },
                        {
                            "text": "The program processes one <w>record</w> at a time to generate the report.",
                            "target_lang_translations": {
                                "ru": "Программа обрабатывает одну <w>запись</w> за раз для создания отчета."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Структура данных в информатике, содержащая набор связанных элементов данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запись",
                                "target_lang_sense_clarification": "в базе данных"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3a6aa9bb-82b4-32d2-b31c-276808ab60a6",
                    "sense_definition": "To store sounds or moving pictures using electronic equipment so that they can be heard or seen later.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "audio_music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>recorded</w> the interview in a professional studio.",
                            "target_lang_translations": {
                                "ru": "Они <w>записали</w> интервью в профессиональной студии."
                            }
                        },
                        {
                            "text": "Can you <w>record</w> that TV show for me so I can watch it tomorrow?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>записать</w> это телешоу для меня, чтобы я мог посмотреть его завтра?"
                            }
                        }
                    ],
                    "antonyms": [
                        "erase"
                    ],
                    "common_phrases": [
                        "record a song",
                        "record a video",
                        "record live"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сохранять звуки или движущиеся изображения с помощью электронного оборудования для последующего воспроизведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "записывать",
                                "target_lang_sense_clarification": "несовершенный вид"
                            },
                            {
                                "target_lang_word": "записать",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3643acc5-f701-3a3e-a9f3-8934edc48c8e",
                    "sense_definition": "To write information down or store it in a computer so that it can be looked at in the future.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "info_log",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The thermometer <w>recorded</w> a temperature of 30 degrees yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера термометр <w>зафиксировал</w> температуру в 30 градусов."
                            }
                        },
                        {
                            "text": "I wanted to <w>record</w> every detail of what happened for the benefit of future generations.",
                            "target_lang_translations": {
                                "ru": "Я хотел <w>записать</w> каждую деталь того, что произошло, на благо будущих поколений."
                            }
                        }
                    ],
                    "synonyms": [
                        "log"
                    ],
                    "antonyms": [
                        "erase"
                    ],
                    "common_phrases": [
                        "record details",
                        "record data",
                        "record information"
                    ],
                    "target_lang_definitions": {
                        "ru": "Записывать информацию или сохранять её в компьютере для использования в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "записывать",
                                "target_lang_sense_clarification": "фиксировать информацию"
                            },
                            {
                                "target_lang_word": "регистрировать",
                                "target_lang_sense_clarification": "о приборах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c2f4c072-a1aa-3027-b8bc-70256eea1af4",
                    "sense_definition": "Better, greater, or more extreme than any previous amount or level.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "achievement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company announced <w>record</w> profits for the third quarter in a row.",
                            "target_lang_translations": {
                                "ru": "Компания объявила о <w>рекордной</w> прибыли третий квартал подряд."
                            }
                        },
                        {
                            "text": "He finished the race in <w>record</w> time, beating all previous competitors.",
                            "target_lang_translations": {
                                "ru": "Он закончил гонку за <w>рекордное</w> время, опередив всех предыдущих соперников."
                            }
                        }
                    ],
                    "synonyms": [
                        "record-breaking",
                        "record-setting"
                    ],
                    "common_phrases": [
                        "record time",
                        "record high",
                        "record number",
                        "record profits"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лучший, больший или более экстремальный, чем любой предыдущий уровень или количество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рекордный",
                                "target_lang_sense_clarification": "о показателях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "63944657-716f-30f0-b349-228580ceb860",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Records</w> was known for his extensive library of historical documents.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Рекордс</w> был известен своей обширной библиотекой исторических документов."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <w>Records</w> at the conference.",
                            "target_lang_translations": {
                                "ru": "Я встретил женщину по имени Сара <w>Рекордс</w> на конференции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "A surname."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]
}
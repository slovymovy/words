                {
                    "sense_id": "c13d575e-0966-3e15-934a-63fa49654ff3",
                    "sense_definition": "Op de hoogte van de laatste informatie of stand van zaken; niet achterlopend.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben weer helemaal <w>bij</w> met mijn administratie.",

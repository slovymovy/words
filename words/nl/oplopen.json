            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f9c2a81-921d-3fd8-878e-5db7dc98457c",
                    "sense_definition": "Lopend naar boven gaan of een oppervlak betreden.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_upwards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij zag haar broer de trap <w>oplopen</w>.",
                            "target_lang_translations": {
                                "en": "She saw her brother <w>walk up</w> the stairs."
                            }
                        },
                        {
                            "text": "Toen we het pad <w>opliepen</w>, begon de hond te blaffen.",
                            "target_lang_translations": {
                                "en": "When we <w>walked up</w> the path, the dog started barking."
                            }
                        }
                    ],
                    "synonyms": [
                        "opgaan",
                        "bestijgen"
                    ],
                    "antonyms": [
                        "aflopen",
                        "afgaan"
                    ],
                    "common_phrases": [
                        "de trap oplopen",
                        "het pad oplopen",
                        "de straat oplopen"
                    ],
                    "target_lang_definitions": {
                        "en": "To go upwards while walking or to enter onto a surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "walk up",
                                "target_lang_sense_clarification": "stairs, hill, or path"
                            },
                            {
                                "target_lang_word": "walk onto",
                                "target_lang_sense_clarification": "a street, field, or stage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3e4aeb8-af67-3f07-9c67-f1d7cc8a8154",
                    "sense_definition": "Toenemen in aantal, hoogte, bedrag of intensiteit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "increase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kosten voor de reparatie kunnen flink <w>oplopen</w>.",
                            "target_lang_translations": {
                                "en": "The costs for the repair can <w>run up</w> significantly."
                            }
                        },
                        {
                            "text": "In de middag zal de temperatuur snel <w>oplopen</w>.",
                            "target_lang_translations": {
                                "en": "In the afternoon the temperature will <w>rise</w> quickly."
                            }
                        }
                    ],
                    "synonyms": [
                        "stijgen",
                        "toenemen",
                        "groeien"
                    ],
                    "antonyms": [
                        "dalen",
                        "afnemen"
                    ],
                    "common_phrases": [
                        "de kosten lopen op",
                        "hoog oplopen",
                        "de spanning loopt op",
                        "de temperatuur loopt op"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase in number, height, amount, or intensity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "increase",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "temperature, tension, water level"
                            },
                            {
                                "target_lang_word": "run up",
                                "target_lang_sense_clarification": "costs, debts, score"
                            },
                            {
                                "target_lang_word": "mount",
                                "target_lang_sense_clarification": "pressure, tension, costs"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cb08ff9-b06c-3a28-8d7f-390b0532d578",
                    "sense_definition": "Een ziekte, verwonding, schade of vertraging onbedoeld krijgen of opdoen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "incur_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft tijdens de wedstrijd een zware blessure <w>opgelopen</w>.",
                            "target_lang_translations": {
                                "en": "He <w>sustained</w> a serious injury during the match."
                            }
                        },
                        {
                            "text": "Door de staking hebben we veel vertraging <w>opgelopen</w>.",
                            "target_lang_translations": {
                                "en": "Due to the strike, we <w>incurred</w> a lot of delay."
                            }
                        }
                    ],
                    "synonyms": [
                        "opdoen",
                        "krijgen"
                    ],
                    "common_phrases": [
                        "vertraging oplopen",
                        "schade oplopen",
                        "een blessure oplopen",
                        "een virus oplopen",
                        "een infectie oplopen"
                    ],
                    "target_lang_definitions": {
                        "en": "To unintentionally acquire or incur a disease, injury, damage, or delay."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "incur",
                                "target_lang_sense_clarification": "delay, damage, penalty"
                            },
                            {
                                "target_lang_word": "sustain",
                                "target_lang_sense_clarification": "injury, damage"
                            },
                            {
                                "target_lang_word": "contract",
                                "target_lang_sense_clarification": "disease, virus"
                            },
                            {
                                "target_lang_word": "pick up",
                                "target_lang_sense_clarification": "virus, injury (informal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e680b191-7f64-3fee-966a-1558fcbf61bf",
                    "sense_definition": "Schuin omhooggaan of hellen van een weg of terrein.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "slope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het pad <w>loopt</w> hier steil <w>op</w> naar de top van de heuvel.",
                            "target_lang_translations": {
                                "en": "The path <w>slopes up</w> steeply here to the top of the hill."
                            }
                        },
                        {
                            "text": "Je moet terugschakelen als de weg begint <w>op</w> te <w>lopen</w>.",
                            "target_lang_translations": {
                                "en": "You have to shift down when the road starts to <w>rise</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stijgen",
                        "hellen"
                    ],
                    "antonyms": [
                        "aflopen"
                    ],
                    "common_phrases": [
                        "de weg loopt op",
                        "steil oplopen"
                    ],
                    "target_lang_definitions": {
                        "en": "To slope upwards (regarding a road or terrain)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slope up",
                                "target_lang_sense_clarification": "terrain, path"
                            },
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "road, ground"
                            },
                            {
                                "target_lang_word": "ascend",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a546865e-ea96-318c-ae90-fb9a65521afb",
                    "sense_definition": "The act of identifying someone or something that has been seen, heard, or known before.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "identification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He looked at her for a moment before <w>recognition</w> dawned on his face.",
                            "target_lang_translations": {
                                "ru": "Он смотрел на нее мгновение, прежде чем на его лице отразилось <w>узнавание</w>."
                            }
                        },
                        {
                            "text": "The town has changed beyond all <w>recognition</w> in the last ten years.",
                            "target_lang_translations": {
                                "ru": "За последние десять лет город изменился до <w>неузнаваемости</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "identification",
                        "detection"
                    ],
                    "common_phrases": [
                        "beyond recognition",
                        "facial recognition",
                        "voice recognition",
                        "sign of recognition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт идентификации кого-либо или чего-либо, что было увидено, услышано или известно ранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узнавание",
                                "target_lang_sense_clarification": "идентификация знакомого объекта или человека"
                            },
                            {
                                "target_lang_word": "распознавание",
                                "target_lang_sense_clarification": "процесс различения или идентификации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e02a0196-51a0-3d77-abe3-b6aa4e61ddc9",
                    "sense_definition": "Public praise, respect, or admiration given to someone for their achievements or service.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "appreciation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She received a special award in <w>recognition</w> of her volunteer work.",
                            "target_lang_translations": {
                                "ru": "Она получила специальную награду в знак <w>признания</w> её волонтерской работы."
                            }
                        },
                        {
                            "text": "The artist finally gained international <w>recognition</w> for his unique paintings.",
                            "target_lang_translations": {
                                "ru": "Художник наконец получил международное <w>признание</w> за свои уникальные картины."
                            }
                        }
                    ],
                    "synonyms": [
                        "appreciation",
                        "acknowledgment",
                        "credit",
                        "honor"
                    ],
                    "antonyms": [
                        "disregard"
                    ],
                    "common_phrases": [

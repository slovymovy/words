                    "common_phrases": [
                        "corkscrew shape"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спиральная форма или объект, напоминающий извивающуюся металлическую часть открывалки для бутылок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спираль",
                                "target_lang_sense_clarification": "форма"
                            },
                            {
                                "target_lang_word": "штопор",
                                "target_lang_sense_clarification": "форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f45c2e8e-7140-398a-849f-aca006ea1c30",

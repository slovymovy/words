{
    "word_family": [
        "verlies",
        "verliezer",
        "verlieslatend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d30906f8-5c5b-398d-bea2-92457a0ff29f",
                    "sense_definition": "Het niet meer hebben van iets wat je eerst wel had; iets kwijtraken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "loss_possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben bang dat ik mijn sleutels ga <w>verliezen</w> als ik ga hardlopen.",
                            "target_lang_translations": {
                                "en": "I am afraid I will <w>lose</w> my keys when I go running.",
                                "ru": "Я боюсь, что <w>потеряю</w> свои ключи, когда пойду бегать."
                            }
                        },
                        {
                            "text": "Hij wilde zijn goede baan niet <w>verliezen</w> door elke dag te laat te komen.",
                            "target_lang_translations": {
                                "en": "He did not want to <w>lose</w> his good job by arriving late every day.",
                                "ru": "Он не хотел <w>потерять</w> свою хорошую работу из-за ежедневных опозданий."
                            }
                        }
                    ],
                    "synonyms": [
                        "kwijtraken",
                        "kwijtspelen"
                    ],
                    "antonyms": [
                        "vinden",
                        "krijgen",
                        "behouden"
                    ],
                    "common_phrases": [
                        "de hoop verliezen",
                        "uit het oog verliezen",
                        "zijn verstand verliezen",
                        "bloed verliezen"
                    ],
                    "target_lang_definitions": {
                        "en": "To no longer have something that you previously possessed; to misplace something.",
                        "ru": "Перестать иметь что-то, что было раньше; потерять или утратить что-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lose",
                                "target_lang_sense_clarification": "to cease to have or misplace"
                            },
                            {
                                "target_lang_word": "misplace",
                                "target_lang_sense_clarification": "to put in the wrong place and lose temporarily"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "терять",
                                "target_lang_sense_clarification": "перестать обладать чем-либо"
                            },
                            {
                                "target_lang_word": "лишаться",
                                "target_lang_sense_clarification": "утрачивать что-то важное (работу, надежду)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41d605c7-516c-33c1-97ee-9e50a7b5e7b4",
                    "sense_definition": "Niet winnen bij een spel, wedstrijd of strijd; verslagen worden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "loss_competition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij zullen de belangrijke wedstrijd zeker niet <w>verliezen</w> als we goed samenwerken.",
                            "target_lang_translations": {
                                "en": "We will certainly not <w>lose</w> the important match if we work well together.",
                                "ru": "Мы точно не <w>проиграем</w> этот важный матч, если будем хорошо работать вместе."
                            }
                        },
                        {
                            "text": "De politieke partij kan veel stemmen <w>verliezen</w> door dit grote schandaal.",
                            "target_lang_translations": {
                                "en": "The political party could <w>lose</w> many votes due to this major scandal.",
                                "ru": "Политическая партия может <w>потерять</w> много голосов из-за этого крупного скандала."
                            }
                        }
                    ],
                    "antonyms": [
                        "winnen"
                    ],
                    "common_phrases": [
                        "de strijd verliezen",
                        "van iemand verliezen",
                        "dik verliezen"
                    ],
                    "target_lang_definitions": {
                        "en": "To fail to win a game, match, or struggle; to be defeated.",
                        "ru": "Не выиграть в игре, соревновании или борьбе; потерпеть поражение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lose",
                                "target_lang_sense_clarification": "to be defeated in a competition"
                            },
                            {
                                "target_lang_word": "be defeated",
                                "target_lang_sense_clarification": "passive form indicating loss in battle or sport"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "проигрывать",
                                "target_lang_sense_clarification": "потерпеть неудачу в соревновании"
                            },
                            {
                                "target_lang_word": "потерпеть поражение",
                                "target_lang_sense_clarification": "быть побежденным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "035d0c99-a4ec-3853-a0e7-1e4e93240ec1",
                    "sense_definition": "Een dierbare kwijtraken doordat deze persoon overlijdt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "loss_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij heeft haar man vorig jaar <w>verloren</w> aan een zeer ernstige ziekte.",
                            "target_lang_translations": {
                                "en": "She <w>lost</w> her husband last year to a very serious illness.",
                                "ru": "Она <w>потеряла</w> мужа в прошлом году из-за очень серьезной болезни."
                            }
                        },
                        {
                            "text": "Het is verschrikkelijk voor ouders om een jong kind te <w>verliezen</w> bij een ongeluk.",
                            "target_lang_translations": {
                                "en": "It is terrible for parents to <w>lose</w> a young child in an accident.",
                                "ru": "Для родителей ужасно <w>потерять</w> маленького ребенка в результате несчастного случая."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zijn ouders verliezen",
                        "het leven verliezen",
                        "een vriend verliezen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be deprived of a loved one through death.",
                        "ru": "Лишиться близкого человека из-за его смерти."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lose",
                                "target_lang_sense_clarification": "to suffer the death of someone"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "терять",
                                "target_lang_sense_clarification": "лишиться кого-то из-за смерти"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "284456d2-a25f-3db8-b62d-f5aac3dace1f",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>verlies</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zware financiële <w>verliezen</w> op de beurs zorgden voor paniek bij de beleggers.",
                            "target_lang_translations": {
                                "en": "The heavy financial <w>losses</w> on the stock market caused panic among the investors.",
                                "ru": "Тяжелые финансовые <w>убытки</w> на бирже вызвали панику среди инвесторов."
                            }
                        },
                        {
                            "text": "Beide legers leden grote <w>verliezen</w> tijdens de lange en uitputtende oorlog.",
                            "target_lang_translations": {
                                "en": "Both armies suffered heavy <w>losses</w> during the long and exhausting war.",
                                "ru": "Обе армии понесли большие <w>потери</w> во время долгой и изнурительной войны."
                            }
                        }
                    ],
                    "antonyms": [
                        "winsten"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural form of the noun 'loss'.",
                        "ru": "Множественное число существительного 'verlies' (потеря, убыток)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "losses",
                                "target_lang_sense_clarification": "plural of loss"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "потери",
                                "target_lang_sense_clarification": "утраты, жертвы (военные или общие)"
                            },
                            {
                                "target_lang_word": "убытки",
                                "target_lang_sense_clarification": "финансовые потери"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
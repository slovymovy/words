{
    "word_family": [
        "collaborate",
        "collaborator",
        "collaborative",
        "collaboratively",
        "collaborationist"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b2a07b4-598f-3edb-b3b9-f6e784acc2a6",
                    "sense_definition": "The action of working with someone to produce or create something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The project was a successful <w>collaboration</w> between the two universities.",
                            "target_lang_translations": {
                                "ru": "Этот проект стал успешным примером <w>сотрудничества</w> двух университетов."
                            }
                        },
                        {
                            "text": "We worked in close <w>collaboration</w> with the design team to finish the task on time.",
                            "target_lang_translations": {
                                "ru": "Мы работали в тесном <w>сотрудничестве</w> с командой дизайнеров, чтобы закончить задачу вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "cooperation",
                        "partnership",
                        "teamwork",
                        "alliance"
                    ],
                    "antonyms": [
                        "competition",
                        "isolation"
                    ],
                    "common_phrases": [
                        "in collaboration with",
                        "close collaboration",
                        "international collaboration",
                        "successful collaboration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по совместной работе с кем-либо для создания или производства чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сотру́дничество",
                                "target_lang_sense_clarification": "участие в общем деле"
                            },
                            {
                                "target_lang_word": "совме́стная рабо́та",
                                "target_lang_sense_clarification": "процесс взаимодействия"
                            },
                            {
                                "target_lang_word": "коопера́ция",
                                "target_lang_sense_clarification": "объединение усилий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0a7d3fd-b92b-3e8b-852a-32a46687e0a6",
                    "sense_definition": "Something produced by working together, such as a book, song, or study.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Their new album is a <w>collaboration</w> featuring several famous artists.",
                            "target_lang_translations": {
                                "ru": "Их новый альбом — это <w>совместная работа</w> с участием нескольких известных артистов."
                            }
                        },
                        {
                            "text": "This book is a <w>collaboration</w> between a historian and a journalist.",
                            "target_lang_translations": {
                                "ru": "Эта книга — <w>совместный труд</w> историка и журналиста."
                            }
                        }
                    ],
                    "synonyms": [
                        "joint effort",
                        "joint venture"
                    ],
                    "antonyms": [
                        "solo effort"
                    ],
                    "common_phrases": [
                        "latest collaboration",
                        "musical collaboration",
                        "artistic collaboration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продукт совместной деятельности, например, книга, песня или исследование."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совме́стная рабо́та",
                                "target_lang_sense_clarification": "результат труда"
                            },
                            {
                                "target_lang_word": "совме́стный прое́кт",
                                "target_lang_sense_clarification": "о творческих или научных проектах"
                            },
                            {
                                "target_lang_word": "коллабора́ция",
                                "target_lang_sense_clarification": "современный термин, часто в искусстве и моде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5918d34a-74e4-367a-8f30-53a2a4ba81d5",
                    "sense_definition": "Traitorous cooperation with an enemy, especially an occupying force during a war.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was put on trial for his <w>collaboration</w> with the enemy forces.",
                            "target_lang_translations": {
                                "ru": "Его судили за <w>сотрудничество</w> с вражескими войсками."
                            }
                        },
                        {
                            "text": "History books often discuss the extent of <w>collaboration</w> during the occupation.",
                            "target_lang_translations": {
                                "ru": "Учебники истории часто обсуждают масштабы <w>коллаборационизма</w> во время оккупации."
                            }
                        }
                    ],
                    "synonyms": [
                        "collusion",
                        "fraternization"
                    ],
                    "antonyms": [
                        "resistance"
                    ],
                    "common_phrases": [
                        "accused of collaboration",
                        "wartime collaboration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предательское сотрудничество с врагом, особенно с оккупационными силами во время войны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коллаборациони́зм",
                                "target_lang_sense_clarification": "политическое сотрудничество с врагом"
                            },
                            {
                                "target_lang_word": "сотру́дничество с враго́м",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
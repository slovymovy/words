            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9a73115e-bf4b-3262-a16a-a92888714d57",
                    "sense_definition": "Een keuze maken uit een aantal mogelijkheden of een grotere verzameling.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_choose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je mag voor je verjaardag zelf een cadeau <w>uitzoeken</w>.",
                            "target_lang_translations": {
                                "en": "You can <w>pick out</w> a gift yourself for your birthday.",
                                "ru": "Ты можешь сам <w>выбрать</w> подарок на свой день рождения."
                            }
                        },
                        {
                            "text": "We hebben de rijpe vruchten <w>uitgezocht</w> voor de fruitsalade.",
                            "target_lang_translations": {
                                "en": "We <w>selected</w> the ripe fruits for the fruit salad.",
                                "ru": "Мы <w>отобрали</w> спелые фрукты для фруктового салата."
                            }
                        }
                    ],
                    "synonyms": [
                        "kiezen",
                        "selecteren",
                        "uitkiezen"
                    ],
                    "common_phrases": [
                        "iets moois uitzoeken",
                        "kleren uitzoeken",
                        "zelf uitzoeken"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a choice from a number of possibilities or a larger collection.",
                        "ru": "Сделать выбор из ряда возможностей или большой коллекции."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pick out",
                                "target_lang_sense_clarification": "to choose specific items from a group"
                            },
                            {
                                "target_lang_word": "choose",
                                "target_lang_sense_clarification": "to decide on an option"
                            },
                            {
                                "target_lang_word": "select",
                                "target_lang_sense_clarification": "to carefully choose from a range of options"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выбирать",
                                "target_lang_sense_clarification": "взять то, что нравится"
                            },
                            {
                                "target_lang_word": "подбирать",
                                "target_lang_sense_clarification": "найти подходящее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e5421d6-a24a-3f9a-8c91-a6355d20274b",
                    "sense_definition": "Grondig onderzoeken hoe iets in elkaar zit of wat de oorzaak van een probleem is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_investigate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie moet precies <w>uitzoeken</w> wat er die avond is gebeurd.",
                            "target_lang_translations": {
                                "en": "The police need to <w>find out</w> exactly what happened that night.",
                                "ru": "Полиция должна точно <w>выяснить</w>, что произошло в тот вечер."
                            }
                        },
                        {
                            "text": "Dat is een ingewikkelde kwestie die we nog goed moeten <w>uitzoeken</w>.",
                            "target_lang_translations": {
                                "en": "That is a complicated issue that we still need to <w>figure out</w> properly.",
                                "ru": "Это сложный вопрос, в котором нам еще нужно хорошенько <w>разобраться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderzoeken",
                        "uitpluizen",
                        "nagaan",
                        "achterhalen"
                    ],
                    "common_phrases": [
                        "tot op de bodem uitzoeken",
                        "laten uitzoeken",
                        "even uitzoeken"
                    ],
                    "target_lang_definitions": {
                        "en": "To investigate thoroughly how something works or the cause of a problem.",
                        "ru": "Тщательно исследовать, как что-то устроено или в чем причина проблемы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "figure out",
                                "target_lang_sense_clarification": "to solve or understand something through thinking"
                            },
                            {
                                "target_lang_word": "find out",
                                "target_lang_sense_clarification": "to discover facts or information"
                            },
                            {
                                "target_lang_word": "investigate",
                                "target_lang_sense_clarification": "to examine a matter systematically"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выяснять",
                                "target_lang_sense_clarification": "узнавать подробности, расследовать"

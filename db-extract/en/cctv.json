            {
                "entry_id": "65eb9b66-04ce-39f2-a14a-84f63156821c",
                "word": "CCTV",
                "pos": "noun",
                "lang_code": "en",
                "forms": [
                    {
                        "form_id": "48188fbd-4fa9-45fa-a750-0cdd81cb102a",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e884457e-022c-371c-8e76-09ad63bf4936",
                    "sense_definition": "Почётное звание, присваиваемое победителю в спортивных соревнованиях или конкурсах.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "achievement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш боксёр сумел защитить свой <w>титул</w> в тяжёлом поединке.",
                            "target_lang_translations": {
                                "en": "Our boxer managed to defend his <w>title</w> in a tough fight."
                            }
                        },
                        {
                            "text": "Команда завоевала <w>титул</w> чемпионов мира второй раз подряд.",
                            "target_lang_translations": {
                                "en": "The team won the world championship <w>title</w> for the second time in a row."
                            }
                        }
                    ],
                    "synonyms": [
                        "звание",
                        "статус"
                    ],
                    "common_phrases": [
                        "титул чемпиона",
                        "завоевать титул",
                        "защитить титул",
                        "почётный титул"
                    ],
                    "target_lang_definitions": {
                        "en": "An honorary designation awarded to a winner in sports competitions or contests."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "title",
                                "target_lang_sense_clarification": "sports championship"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9333e29-ac39-3169-9339-54d463899cec",
                    "sense_definition": "Наследственное или пожалованное официальное почётное звание в сословно-дворянском обществе (граф, князь, барон и т. п.).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nobility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Император пожаловал ему графский <w>титул</w> за верную службу государству.",
                            "target_lang_translations": {
                                "en": "The emperor granted him the <w>title</w> of count for his faithful service to the state."
                            }
                        },
                        {
                            "text": "Она очень гордилась своим древним дворянским <w>титулом</w>.",
                            "target_lang_translations": {
                                "en": "She was very proud of her ancient noble <w>title</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "звание",
                        "чин"
                    ],
                    "common_phrases": [
                        "дворянский титул",
                        "королевский титул",
                        "носить титул",
                        "получить титул"
                    ],
                    "target_lang_definitions": {
                        "en": "A hereditary or granted official honorary rank in nobility (count, prince, baron, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "title",
                                "target_lang_sense_clarification": "rank of nobility"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17461ae8-68ce-342c-9c18-1dabce302014",
                    "sense_definition": "Первая страница книги, на которой напечатаны её название, имя автора и выходные данные.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "book",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На <w>титуле</w> старинной книги едва можно было разобрать год издания.",
                            "target_lang_translations": {
                                "en": "On the <w>title page</w> of the old book, one could barely make out the year of publication."
                            }
                        },
                        {
                            "text": "Дизайнер разработал красивый <w>титул</w> для нового романа.",
                            "target_lang_translations": {
                                "en": "The designer developed a beautiful <w>title page</w> for the new novel."
                            }
                        }
                    ],
                    "common_phrases": [
                        "разворот титула",
                        "на обороте титула"
                    ],
                    "target_lang_definitions": {
                        "en": "The first page of a book listing its name, author, and publication details."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "title page",
                                "target_lang_sense_clarification": "book layout"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bc1d24f-1291-384d-ae7f-a09c91f9c66a",
                    "sense_definition": "Юридическое основание, подтверждающее наличие какого-либо права (обычно на имущество).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Адвокат проверил законный <w>титул</w> владения земельным участком.",
                            "target_lang_translations": {
                                "en": "The lawyer checked the legal <w>title</w> to the land plot."
                            }
                        },
                        {
                            "text": "Без надлежащего <w>титула</w> продажа недвижимости невозможна.",
                            "target_lang_translations": {
                                "en": "Without a proper <w>title</w>, the sale of real estate is impossible."
                            }
                        }
                    ],
                    "common_phrases": [
                        "титул собственности",
                        "законный титул"
                    ],
                    "target_lang_definitions": {
                        "en": "A legal basis confirming a specific right (usually to property)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "title",
                                "target_lang_sense_clarification": "legal ownership"
                            }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d2e96705-6888-3875-a364-a77fa2a26678",
                    "sense_definition": "Dowiedzieć się czegoś, zdobyć informację poprzez przeczytanie tekstu (np. w gazecie, książce, ogłoszeniu).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "info_gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj <w>wyczytałem</w> w lokalnej prasie, że planowany jest remont mostu.",
                            "target_lang_translations": {
                                "ru": "Вчера я <w>вычитал</w> в местной прессе, что планируется ремонт моста."
                            }
                        },
                        {
                            "text": "Student <w>wyczytał</w> w podręczniku, że ten proces chemiczny jest nieodwracalny.",
                            "target_lang_translations": {
                                "ru": "Студент <w>вычитал</w> в учебнике, что этот химический процесс необратим."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyrzucić",
                        "wymówić"
                    ],
                    "common_phrases": [
                        "wyczytać w gazecie",
                        "wyczytać w internecie",
                        "wyczytać informację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узнать что-то, получить информацию, прочитав текст (например, в газете, книге, объявлении)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вычитать",
                                "target_lang_sense_clarification": "найти информацию в тексте"
                            },
                            {
                                "target_lang_word": "прочитать",
                                "target_lang_sense_clarification": "узнать из чтения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "372ce6b1-465e-37ae-998d-fb159cab9858",
                    "sense_definition": "Odczytać coś na głos, wymawiając kolejne pozycje z zapisanego ciągu (np. nazwiska z listy).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vocalizing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczycielka otworzyła dziennik i <w>wyczytała</w> listę obecności.",
                            "target_lang_translations": {
                                "ru": "Учительница открыла журнал и <w>зачитала</w> список присутствующих."
                            }
                        },
                        {
                            "text": "Kiedy <w>wyczytano</w> nazwisko zwycięzcy, na sali rozległy się gromkie brawa.",
                            "target_lang_translations": {
                                "ru": "Когда <w>огласили</w> фамилию победителя, в зале раздались громкие аплодисменты."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymówić",
                        "wyrzucić"
                    ],
                    "common_phrases": [
                        "wyczytać listę obecności",
                        "wyczytać nazwisko",
                        "wyczytać wyrok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочитать вслух, произнося пункты из записанного списка (например, фамилии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачитать",
                                "target_lang_sense_clarification": "огласить список или документ"
                            },
                            {
                                "target_lang_word": "огласить",
                                "target_lang_sense_clarification": "объявить вслух"
                            },
                            {
                                "target_lang_word": "выкликнуть",
                                "target_lang_sense_clarification": "громко назвать имя из списка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5576d09e-8840-3e27-9da0-31bc584a3d31",
                    "sense_definition": "Domyślić się czegoś, wnioskując z obserwacji czyjegoś wyglądu, zachowania lub innych oznak (często z twarzy lub oczu).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "deduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z jej smutnego spojrzenia można było <w>wyczytać</w> ogromny ból i tęsknotę.",
                            "target_lang_translations": {
                                "ru": "В её грустном взгляде можно было <w>прочитать</w> огромную боль и тоску."
                            }
                        },
                        {
                            "text": "Próbowałem <w>wyczytać</w> między wierszami, co autor miał naprawdę na myśli.",
                            "target_lang_translations": {
                                "ru": "Я пытался <w>прочитать</w> между строк, что автор имел в виду на самом деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyrzucić",
                        "wymówić"
                    ],
                    "common_phrases": [
                        "wyczytać z twarzy",
                        "wyczytać z oczu",
                        "wyczytać między wierszami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Догадаться о чем-то, делая выводы на основе наблюдения за чьим-то видом, поведением или другими признаками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прочитать",
                                "target_lang_sense_clarification": "понять по лицу или глазам"
                            },
                            {
                                "target_lang_word": "угадать",
                                "target_lang_sense_clarification": "понять скрытый смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0e78fba-43d4-3693-8e5d-19da8e3764f8",
                    "sense_definition": "Pobrać lub rozpoznać dane z nośnika, urządzenia lub ekranu (o maszynach, komputerach, skanerach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "machine_reading",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Skaner w kasie nie mógł <w>wyczytać</w> kodu kreskowego z pogniecionego opakowania.",
                            "target_lang_translations": {
                                "ru": "Сканер на кассе не мог <w>считать</w> штрихкод с помятой упаковки."
                            }
                        },
                        {
                            "text": "Program antywirusowy <w>wyczytał</w> zagrożenie w pobranym pliku.",
                            "target_lang_translations": {
                                "ru": "Антивирусная программа <w>распознала</w> угрозу в скачанном файле."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyczytać dane",
                        "wyczytać kod",
                        "wyczytać błąd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать или распознать данные с носителя, устройства или экрана (о машинах, компьютерах, сканерах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "получить данные (о технике)"
                            },
                            {
                                "target_lang_word": "распознать",
                                "target_lang_sense_clarification": "идентифицировать информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32cc6e46-920b-3847-9925-e4689335aacd",
                    "sense_definition": "Zdołać odczytać tekst, który jest niewyraźny, zniszczony lub trudny do rozszyfrowania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "deciphering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napis na starym nagrobku był tak zatarty, że ledwo udało się go <w>wyczytać</w>.",
                            "target_lang_translations": {
                                "ru": "Надпись на старом надгробие была так стерта, что её едва удалось <w>разобрать</w>."
                            }
                        },
                        {
                            "text": "Lekarz wypisał receptę tak niestarannie, że aptekarz nie mógł jej <w>wyczytać</w>.",
                            "target_lang_translations": {
                                "ru": "Врач выписал рецепт так небрежно, что аптекарь не мог его <w>разобрать</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ledwo wyczytać",
                        "trudno wyczytać",
                        "dać się wyczytać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суметь прочитать текст, который написан неразборчиво, поврежден или труден для расшифровки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разобрать",
                                "target_lang_sense_clarification": "прочитать с трудом"
                            },
                            {
                                "target_lang_word": "прочитать",
                                "target_lang_sense_clarification": "расшифровать написанное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ebb4299-dab5-38fa-aa52-2fd59b19d48d",
                    "sense_definition": "Przeczytać wszystko, co było dostępne w danym zbiorze; zapoznać się z całą zawartością.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "exhausting_resource",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wakacji <w>wyczytałem</w> wszystkie kryminały, które zabrałem ze sobą.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска я <w>перечитал</w> все детективы, которые взял с собой."
                            }
                        },
                        {
                            "text": "Biblioteka była mała, więc szybko <w>wyczytaliśmy</w> wszystkie interesujące nas pozycje.",
                            "target_lang_translations": {
                                "ru": "Библиотека была маленькой, поэтому мы быстро <w>перечитали</w> все интересующие нас книги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyczytać wszystko",
                        "wyczytać do końca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочитать всё, что было доступно в данном наборе; ознакомиться со всем содержимым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перечитать",
                                "target_lang_sense_clarification": "прочитать всё множество книг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c544433f-03fe-370b-8797-33f4a5ee2b92",
                    "sense_definition": "Zniszczyć książkę lub czasopismo poprzez wielokrotne, intensywne czytanie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta książka kucharska jest już tak <w>wyczytana</w>, że wypadają z niej kartki.",
                            "target_lang_translations": {
                                "ru": "Эта поваренная книга уже так <w>зачитана</w>, что из неё выпадают страницы."
                            }
                        },
                        {
                            "text": "Dzieci tak bardzo lubiły tę bajkę, że <w>wyczytały</w> ją niemal do strzępów.",
                            "target_lang_translations": {
                                "ru": "Дети так любили эту сказку, что <w>зачитали</w> её почти до дыр."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyczytać książkę",
                        "wyczytany egzemplarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испортить книгу или журнал в результате многократного, интенсивного чтения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачитать",
                                "target_lang_sense_clarification": "истрепать книгу чтением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "800a301e-b2c4-34ba-ad58-9b119f108302",
                    "sense_definition": "Robić komuś wyrzuty, wypominać dawne przewinienia lub błędy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "reproach",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiadka <w>wyczytała</w> mi wszystkie hałasy, które rzekomo robiłem w nocy.",
                            "target_lang_translations": {
                                "ru": "Соседка <w>высказала</w> мне за все шумы, которые я якобы производил ночью."
                            }
                        },
                        {
                            "text": "Nie powinieneś mu teraz <w>wyczytać</w> tego, co zrobił niechcący wiele lat temu.",
                            "target_lang_translations": {
                                "ru": "Ты не должен ему сейчас <w>припоминать</w> то, что он сделал нечаянно много лет назад."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymówić",
                        "wyrzucić"
                    ],
                    "common_phrases": [
                        "wyczytać błędy",
                        "wyczytać grzechy"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",

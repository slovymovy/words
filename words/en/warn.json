{
    "word_family": [
        "warning",
        "forewarn",
        "warner"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "840a8ef9-2dfb-3a98-b6e3-20ddd41eea4c",
                    "sense_definition": "To make someone aware of impending danger, a problem, or an unpleasant situation.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The signs <warn> drivers of the sharp curve ahead.",
                            "target_lang_translations": {
                                "ru": "Знаки <предупреждают> водителей о крутом повороте впереди."
                            }
                        },
                        {
                            "text": "She phoned to <warn> him about the heavy traffic on the highway.",
                            "target_lang_translations": {
                                "ru": "Она позвонила, чтобы <предупредить> его о сильных пробках на шоссе."
                            }
                        }
                    ],
                    "synonyms": [
                        "alert",
                        "caution",
                        "forewarn"
                    ],
                    "common_phrases": [
                        "warn about",
                        "warn of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщать кому-либо о надвигающейся опасности, проблеме или неприятной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предупреждать",
                                "target_lang_sense_clarification": "об опасности или проблеме"
                            },
                            {
                                "target_lang_word": "предостерегать",
                                "target_lang_sense_clarification": "остерегать от беды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09d7e22f-d972-377d-80b5-d66c31694773",
                    "sense_definition": "To caution or advise someone strongly against unwise or unacceptable behavior.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher <warned> the students not to run in the hallway.",
                            "target_lang_translations": {
                                "ru": "Учитель <предупредил> учеников, чтобы они не бегали в коридоре."
                            }
                        },
                        {
                            "text": "I <warn> you that this kind of behavior will not be tolerated.",
                            "target_lang_translations": {
                                "ru": "Я <предупреждаю> вас, что такое поведение не будет терпимо."
                            }
                        }
                    ],
                    "synonyms": [
                        "admonish",
                        "counsel"
                    ],
                    "common_phrases": [
                        "warn against",
                        "I warn you"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво советовать кому-либо избегать неразумного или недопустимого поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предостерегать",
                                "target_lang_sense_clarification": "от дурных поступков"
                            },
                            {
                                "target_lang_word": "предупреждать",
                                "target_lang_sense_clarification": "делать замечание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1274c7c-b8f5-3d7e-8eca-1f0ba4e9e360",
                    "sense_definition": "To notify or inform someone in advance about a fact or future event.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <warned> his friend that he would be arriving late for the party.",
                            "target_lang_translations": {
                                "ru": "Он <предупредил> друга, что опоздает на вечеринку."
                            }
                        },
                        {
                            "text": "The company <warned> employees that there might be changes to the schedule.",
                            "target_lang_translations": {
                                "ru": "Компания <предупредила> сотрудников, что в расписании могут быть изменения."
                            }
                        }
                    ],
                    "synonyms": [
                        "notify",
                        "inform",
                        "apprise"
                    ],
                    "common_phrases": [
                        "warn that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заранее уведомлять или информировать кого-либо о факте или предстоящем событии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предупреждать",
                                "target_lang_sense_clarification": "уведомлять заранее"
                            },
                            {
                                "target_lang_word": "извещать",
                                "target_lang_sense_clarification": "информировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9d40a0f-7f1b-352e-a1f1-79570f4a0b2e",
                    "sense_definition": "To order someone to go away or stay away from a specific place.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The security guard <warned> the trespassers off the private property.",
                            "target_lang_translations": {
                                "ru": "Охранник <потребовал> от нарушителей <уйти> с частной территории."
                            }
                        },
                        {
                            "text": "Farmers often put up signs to <warn> hikers away from the livestock.",
                            "target_lang_translations": {
                                "ru": "Фермеры часто ставят знаки, чтобы <предупредить> туристов <держаться подальше> от скота."
                            }
                        }
                    ],
                    "common_phrases": [
                        "warn off",
                        "warn away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приказывать кому-либо уйти или держаться подальше от определенного места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отгонять",
                                "target_lang_sense_clarification": "заставлять уйти"
                            },
                            {
                                "target_lang_word": "предупреждать",
                                "target_lang_sense_clarification": "требовать не приближаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d8d9db0-950d-39dd-8bcb-1e7c6e6d3e6d",
                    "sense_definition": "To officially summon someone to attend a formal meeting or perform a legal duty.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The witness was <warned> to appear in court next Monday.",
                            "target_lang_translations": {
                                "ru": "Свидетеля <обязали> явиться в суд в следующий понедельник."
                            }
                        },
                        {
                            "text": "The sheriff <warned> the jury members of their upcoming service dates.",
                            "target_lang_translations": {
                                "ru": "Шериф <уведомил> присяжных о датах их предстоящей службы."
                            }
                        }
                    ],
                    "synonyms": [
                        "summon",
                        "cite"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or legal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально вызывать кого-либо для участия в официальном собрании или выполнения юридической обязанности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "официально или в суд"
                            },
                            {
                                "target_lang_word": "уведомлять",
                                "target_lang_sense_clarification": "официально информировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e52a2c0-e0cc-342d-8245-d6a7678fe317",
                    "sense_definition": "To make a mechanical sound indicating that a clock is about to strike the hour.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old grandfather clock <warned> loudly before striking midnight.",
                            "target_lang_translations": {
                                "ru": "Старые напольные часы громко <зашипели> перед тем, как пробить полночь."
                            }
                        },
                        {
                            "text": "Just as the mechanism <warned>, everyone looked up at the tower.",
                            "target_lang_translations": {
                                "ru": "Как только механизм <издал предупреждающий звук>, все посмотрели на башню."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать механический звук, указывающий на то, что часы скоро пробьют час."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шипеть",
                                "target_lang_sense_clarification": "о звуке часов перед боем"
                            },
                            {
                                "target_lang_word": "издавать звук",
                                "target_lang_sense_clarification": "сигнализировать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a2986731-46b9-37df-ad71-61a694c7bb5c",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <Warn> is known for his contributions to the local community.",
                            "target_lang_translations": {
                                "ru": "Мистер <Уорн> известен своим вкладом в местное сообщество."
                            }
                        },
                        {
                            "text": "We met a family named <Warn> during our trip to Germany.",
                            "target_lang_translations": {
                                "ru": "Мы встретили семью по фамилии <Уорн> во время нашей поездки в Германию."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Уорн",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
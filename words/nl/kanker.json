                {
                    "sense_id": "31b5a814-92c1-3bbe-8507-c858aa56e959",
                    "sense_definition": "Een ernstige ziekte waarbij lichaamscellen zich ongecontroleerd delen en gezonde weefsels aantasten",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_disease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Roken verhoogt de kans op het krijgen van <w>kanker</w> aanzienlijk.",
                            "target_lang_translations": {
                                "ru": "Курение значительно повышает риск заболевания <w>раком</w>."
                            }
                        },
                        {
                            "text": "Na een lange behandeling is hij gelukkig genezen van <w>kanker</w>.",
                            "target_lang_translations": {
                                "ru": "К счастью, после длительного лечения он излечился от <w>рака</w>."
                            }
                        }

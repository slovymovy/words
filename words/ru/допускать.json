    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f86b1eaf-7253-3e76-b7b2-2002d002b686",
                    "sense_definition": "Разрешать кому-либо войти куда-либо, получить доступ к чему-либо или принять участие в чем-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охрана не <допускает> посторонних на территорию завода без пропуска."
                        },
                        {
                            "text": "Студента не <допускают> к экзаменам, пока он не сдаст все зачеты."
                        }
                    ],
                    "synonyms": [
                        "пускать",
                        "разрешать"
                    ],
                    "antonyms": [
                        "не пускать",
                        "запрещать",
                        "отстранять"
                    ],
                    "common_phrases": [
                        "допускать к работе",
                        "допускать к экзаменам",
                        "допускать к участию"
                    ]
                },
                {
                    "sense_id": "d7ba21f0-7025-3120-b0f3-01d237f43b86",
                    "sense_definition": "Позволять чему-либо произойти или существовать; не препятствовать чему-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители не должны <допускать>, чтобы дети играли на проезжей части."
                        },
                        {
                            "text": "Мы не можем <допускать> такого неуважительного отношения к клиентам."
                        }
                    ],
                    "synonyms": [
                        "позволять",
                        "разрешать"
                    ],
                    "antonyms": [
                        "предотвращать",
                        "запрещать",
                        "исключать"
                    ],
                    "common_phrases": [
                        "этого нельзя допускать",
                        "допускать возможность",
                        "допускать беспорядок"
                    ]
                },
                {
                    "sense_id": "ad3f44f7-ab96-33f8-a1cd-14dbde14f615",
                    "sense_definition": "Совершать что-либо нежелательное (ошибку, неточность, нарушение) по невнимательности или незнанию.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Даже опытный специалист иногда <допускает> ошибки в сложных расчетах."
                        },
                        {
                            "text": "Старайся внимательно проверять текст и не <допускать> опечаток."
                        }
                    ],
                    "synonyms": [
                        "совершать",
                        "делать"
                    ],
                    "antonyms": [
                        "избегать"
                    ],
                    "common_phrases": [
                        "допускать ошибку",
                        "допускать неточность",
                        "допускать нарушение"
                    ]
                },
                {
                    "sense_id": "d19d7959-2e63-357f-a805-f7062897eee3",
                    "sense_definition": "Считать что-либо возможным или вероятным; предполагать.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <допускаю>, что он мог опоздать из-за сильных пробок на дорогах."
                        },
                        {
                            "text": "Ученые <допускают> существование жизни на других планетах."
                        }
                    ],
                    "synonyms": [
                        "предполагать",
                        "полагать"
                    ],
                    "antonyms": [
                        "исключать",
                        "отрицать"
                    ],
                    "common_phrases": [
                        "допускать мысль",
                        "допускать вероятность",
                        "смею допускать"
                    ]
                }
            ]
        }
    ]

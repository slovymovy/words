    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "473da15f-aafb-3d3a-a299-ad4c6db3730c",
                    "sense_definition": "Wypukłe zniekształcenie powierzchni w formie bąbla, powstające zazwyczaj na skutek odspojenia się warstwy materiału, np. tynku, farby lub gumy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "deformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na karoserii samochodu pojawił się brzydki <w>purchel</w> rdzy.",
                            "target_lang_translations": {
                                "ru": "На кузове автомобиля появился уродливый <w>пузырь</w> ржавчины."
                            }
                        },
                        {
                            "text": "Musisz usunąć ten <w>purchel</w> ze ściany, zanim zaczniesz malowanie.",
                            "target_lang_translations": {
                                "ru": "Ты должен удалить это <w>вздутие</w> со стены, прежде чем начнешь красить."
                            }
                        }
                    ],
                    "synonyms": [
                        "bąbel",
                        "wybrzuszenie",
                        "purchla"
                    ],
                    "common_phrases": [
                        "purchel-na-lakierze",
                        "purchel-na-ścianie",
                        "purchel-na-oponie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często w kontekście motoryzacyjnym lub budowlanym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпуклая деформация поверхности в виде пузыря, возникающая обычно в результате отслоения слоя материала (например, штукатурки, краски или резины)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пузырь",
                                "target_lang_sense_clarification": "на краске, обоях или штукатурке"
                            },
                            {
                                "target_lang_word": "вздутие",
                                "target_lang_sense_clarification": "деформация поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45453b41-1b5e-338e-bd02-2720caf811e3",
                    "sense_definition": "Niewykształcone jajo ptasie pozbawione twardej skorupki wapiennej, otoczone jedynie elastyczną błoną.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "egg",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda kura zniosła dzisiaj miękki <w>purchel</w> zamiast normalnego jajka.",
                            "target_lang_translations": {
                                "ru": "Молодая курица снесла сегодня мягкое <w>яйцо без скорлупы</w> вместо нормального."
                            }
                        },
                        {
                            "text": "W gnieździe leżał tylko mały <w>purchel</w>, co zaniepokoiło hodowcę.",
                            "target_lang_translations": {
                                "ru": "В гнезде лежало только маленькое <w>яйцо без скорлупы</w>, что обеспокоило птицевода."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Недоразвитое птичье яйцо без твердой известковой скорлупы, покрытое только эластичной оболочкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яйцо без скорлупы",
                                "target_lang_sense_clarification": "описательный перевод"
                            },
                            {
                                "target_lang_word": "литое яйцо",
                                "target_lang_sense_clarification": "термин в птицеводстве"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "99892388-f6a6-30b3-a5f5-14dd2877db82",
                    "sense_definition": "Of or related to technology, machines, engineering, or practical sciences.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tech_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are experiencing some <w>technical</w> difficulties with our website.",
                            "target_lang_translations": {
                                "ru": "У нас возникли некоторые <w>технические</w> неполадки с сайтом."
                            }
                        },
                        {
                            "text": "The company provides <w>technical</w> support for all its products.",
                            "target_lang_translations": {
                                "ru": "Компания предоставляет <w>техническую</w> поддержку для всех своих продуктов."
                            }
                        }
                    ],
                    "synonyms": [
                        "technological"
                    ],
                    "common_phrases": [
                        "technical support",
                        "technical issue",
                        "technical problem",
                        "technical assistance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к технике, машинам, инженерии или прикладным наукам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "технический",
                                "target_lang_sense_clarification": "относящийся к технике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cf04b0c-b486-3b8f-98fb-e91e8a22ffbc",
                    "sense_definition": "Connected with the specific language, methods, or details of a particular subject, profession, or craft.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "specialized",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The report is full of <w>technical</w> terms that are hard for a layperson to understand.",
                            "target_lang_translations": {
                                "ru": "Отчет полон <w>технических</w> терминов, которые трудно понять непрофессионалу."
                            }
                        },
                        {
                            "text": "She has the <w>technical</w> knowledge required to fix the engine.",
                            "target_lang_translations": {
                                "ru": "У нее есть <w>технические</w> знания, необходимые для починки двигателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "technological",
                        "technic#Adjective"
                    ],
                    "antonyms": [
                        "non-technical",
                        "nontechnical"
                    ],
                    "common_phrases": [
                        "technical term",
                        "technical knowledge",
                        "technical detail",
                        "technical data"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный со специфической терминологией, методами или деталями конкретной профессии или предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "специальный",
                                "target_lang_sense_clarification": "узкопрофессиональный"
                            },
                            {
                                "target_lang_word": "технический",
                                "target_lang_sense_clarification": "относящийся к деталям или терминам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98641366-2e58-384b-9e03-677ea278fb08",
                    "sense_definition": "Relating to the practical skills or methods used to perform an artistic or sporting activity.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "skill_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pianist displayed amazing <w>technical</w> skill during the performance.",
                            "target_lang_translations": {
                                "ru": "Пианист продемонстрировал удивительное <w>техническое</w> мастерство во время выступления."
                            }
                        },
                        {
                            "text": "It was a <w>technical</w> climb that required ropes and safety equipment.",
                            "target_lang_translations": {
                                "ru": "Это было <w>технически сложное</w> восхождение, требовавшее веревок и страховки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "technical skill",
                        "technical ability",
                        "technical perfection"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к технике исполнения, практическим навыкам или методам в искусстве или спорте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "технический",
                                "target_lang_sense_clarification": "относящийся к мастерству"
                            },
                            {
                                "target_lang_word": "техничный",
                                "target_lang_sense_clarification": "выполненный с высоким мастерством"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d79b8a8a-eb48-30d7-93e1-f2a8769a91a1",
                    "sense_definition": "Having practical knowledge or skill in science, technology, or how machines work.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_attribute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am not very <w>technical</w>, so I asked my brother to set up the computer.",
                            "target_lang_translations": {
                                "ru": "Я не очень <w>разбираюсь в технике</w>, поэтому попросил брата настроить компьютер."
                            }
                        },
                        {
                            "text": "They are looking for a <w>technical</w> co-founder to help build the app.",
                            "target_lang_translations": {
                                "ru": "Они ищут <w>технического</w> соучредителя, чтобы помочь создать приложение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "technical staff",
                        "technical mind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий практическими знаниями в области техники; разбирающийся в том, как работают устройства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "технически подкованный",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "разбирающийся в технике",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8709a50-a854-3bd3-b31f-da80c33e20aa",
                    "sense_definition": "According to the exact interpretation of a rule or law, often ignoring its practical meaning or intent.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "strict_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Strictly speaking, it was a <w>technical</w> violation of the contract.",
                            "target_lang_translations": {
                                "ru": "Строго говоря, это было <w>формальное</w> нарушение контракта."
                            }
                        },
                        {
                            "text": "In a <w>technical</w> sense, they are still married, even though they have lived apart for years.",
                            "target_lang_translations": {
                                "ru": "В <w>формальном</w> смысле они все еще женаты, хотя живут раздельно уже много лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "technical sense",
                        "technical violation",
                        "technical breach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основанный на буквальном, строгом толковании правил или закона, часто в ущерб сути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формальный",
                                "target_lang_sense_clarification": "юридический, процедурный"
                            },
                            {
                                "target_lang_word": "чисто технический",
                                "target_lang_sense_clarification": "относящийся к нюансам правил"
                            }
                        ]
                    }
                }
            ]
        },
        {

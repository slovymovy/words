    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d965a48f-41c6-382b-832b-657197160e2d",
                    "sense_definition": "Szybko przemieścić się biegiem w dół (np. ze schodów, ze wzgórza).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopiec szybko <zbiegł> po schodach, żeby otworzyć drzwi listonoszowi.",
                            "target_lang_translations": {
                                "ru": "Мальчик быстро <сбежал> по лестнице, чтобы открыть дверь почтальону."
                            }
                        },
                        {
                            "text": "Musimy ostrożnie <zbiec> ze stromego zbocza, aby się nie przewrócić.",
                            "target_lang_translations": {
                                "ru": "Мы должны осторожно <сбежать> с крутого склона, чтобы не упасть."
                            }
                        }
                    ],
                    "antonyms": [
                        "wbiec"
                    ],
                    "common_phrases": [
                        "zbiec ze schodów",
                        "zbiec na dół",
                        "zbiec z góry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро спуститься бегом вниз (например, по лестнице или с холма)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбежать",
                                "target_lang_sense_clarification": "спуститься бегом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "229c9d00-6249-3a58-8b23-902cea055bcc",
                    "sense_definition": "Zmniejszyć swoje wymiary pod wpływem prania, wilgoci lub temperatury (o tkaninach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój ulubiony wełniany sweter niestety <zbiegł> się w praniu.",
                            "target_lang_translations": {
                                "ru": "Мой любимый шерстяной свитер, к сожалению, <сел> после стирки."
                            }
                        },
                        {
                            "text": "Kupuj ubrania o rozmiar większe, bo bawełna może się <zbiec> w wysokiej temperaturze.",
                            "target_lang_translations": {
                                "ru": "Покупай одежду на размер больше, так как хлопок может <сесть> при высокой температуре."
                            }
                        }
                    ],
                    "synonyms": [
                        "skurczyć się"
                    ],
                    "antonyms": [
                        "rozciągnąć się"
                    ],
                    "common_phrases": [
                        "zbiec się w praniu"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшиться в размере под воздействием стирки, влаги или температуры (о ткани)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сесть",
                                "target_lang_sense_clarification": "об одежде/ткани"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bd04f42-0588-3073-9e7f-bc0822f59761",
                    "sense_definition": "Uciec potajemnie z miejsca zamknięcia, więzienia lub niewoli.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "escape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więzień wykorzystał zamieszanie na spacerniaku i zdołał <zbiec>.",
                            "target_lang_translations": {
                                "ru": "Заключенный воспользовался суматохой на прогулочном дворе и сумел <сбежать>."
                            }
                        },
                        {
                            "text": "Sprawca wypadku <zbiegł> z miejsca zdarzenia przed przyjazdem policji.",
                            "target_lang_translations": {
                                "ru": "Виновник аварии <скрылся> с места происшествия до приезда полиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "uciec",
                        "umknąć"
                    ],
                    "antonyms": [
                        "zostać"
                    ],
                    "common_phrases": [
                        "zbiec z więzienia",
                        "zbiec z miejsca wypadku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тайно убежать из места заключения, тюрьмы или плена; скрыться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбежать",
                                "target_lang_sense_clarification": "совершить побег"
                            },
                            {
                                "target_lang_word": "скрыться",
                                "target_lang_sense_clarification": "уйти незамеченным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4142b5ff-c5cb-3b6d-a0ca-7c5db753a61e",
                    "sense_definition": "Wystąpić w tym samym czasie (o zdarzeniach, okolicznościach lub terminach).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "convergence_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasze urlopy szczęśliwie <zbiegły> się w tym samym terminie, więc pojedziemy razem.",
                            "target_lang_translations": {
                                "ru": "Наши отпуска удачно <совпали> по времени, поэтому мы поедем вместе."
                            }
                        },
                        {
                            "text": "Wiele niefortunnych okoliczności <zbiegło> się w czasie, powodując opóźnienie projektu.",
                            "target_lang_translations": {
                                "ru": "Множество неудачных обстоятельств <совпало> во времени, вызвав задержку проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokryć się",
                        "nałożyć się"
                    ],
                    "common_phrases": [
                        "zbiec się w czasie",
                        "okoliczności się zbiegły"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Произойти в одно и то же время (о событиях, обстоятельствах или сроках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совпасть",
                                "target_lang_sense_clarification": "по времени или обстоятельствам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3104ca36-38e7-3ac5-8723-9de739332b22",
                    "sense_definition": "Zgromadzić się w jednym miejscu, biegnąc lub idąc z różnych stron.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "convergence_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na dźwięk syreny alarmowej wszyscy mieszkańcy <zbiegli> się na rynek.",
                            "target_lang_translations": {
                                "ru": "На звук сирены все жители <сбежались> на рыночную площадь."
                            }
                        },
                        {
                            "text": "Dzieci natychmiast <zbiegły> się do kuchni, gdy poczuły zapach ciasta.",
                            "target_lang_translations": {
                                "ru": "Дети тут же <сбежались> на кухню, когда почувствовали запах пирога."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgromadzić się",
                        "zlecieć się"
                    ],
                    "antonyms": [
                        "rozbiec się"
                    ],
                    "common_phrases": [
                        "tłum się zbiegł",
                        "zbiec się w jedno miejsce"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Собраться в одном месте, прибежав или придя с разных сторон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбежаться",
                                "target_lang_sense_clarification": "собраться бегом"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
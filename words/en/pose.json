    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "56861268-93aa-30c7-adbc-f09d7db7e0b0",
                    "sense_definition": "To move into and stay in a particular position, usually to be photographed, painted, or drawn.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_physical_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The entire family <w>posed</w> for a photograph in front of the historic castle.",
                            "target_lang_translations": {
                                "ru": "Вся семья <w>позировала</w> для фотографии на фоне старинного замка."
                            }
                        },
                        {
                            "text": "She had to <w>pose</w> for hours while the artist painted her portrait.",
                            "target_lang_translations": {
                                "ru": "Ей пришлось часами <w>позировать</w>, пока художник писал её портрет."
                            }
                        }
                    ],
                    "synonyms": [
                        "model",
                        "sit"
                    ],
                    "common_phrases": [
                        "pose for a photograph",
                        "pose for a portrait",
                        "pose for the camera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать определенное положение тела, обычно для того, чтобы быть сфотографированным или нарисованным художником."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позировать",
                                "target_lang_sense_clarification": "быть моделью для фото или картины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da313064-e240-36ca-b3d9-e604ca42ee47",
                    "sense_definition": "To exist in a way that creates a problem, danger, or difficulty that must be dealt with.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy snowfall <w>posed</w> a significant challenge for the morning commuters.",
                            "target_lang_translations": {
                                "ru": "Сильный снегопад <w>создал</w> серьезные трудности для утренних пассажиров."
                            }
                        },
                        {
                            "text": "Pollution continues to <w>pose</w> a threat to the health of marine life.",
                            "target_lang_translations": {
                                "ru": "Загрязнение продолжает <w>представлять</w> угрозу для здоровья морских обитателей."
                            }
                        }
                    ],
                    "synonyms": [
                        "constitute",
                        "present",
                        "create",
                        "cause"
                    ],
                    "antonyms": [
                        "resolve",
                        "eliminate"
                    ],
                    "common_phrases": [
                        "pose a threat",
                        "pose a risk",
                        "pose a danger",
                        "pose a challenge",
                        "pose a problem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Являться причиной возникновения проблемы, опасности или трудности, с которой необходимо справиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представлять",
                                "target_lang_sense_clarification": "собой угрозу или опасность"
                            },
                            {
                                "target_lang_word": "создавать",
                                "target_lang_sense_clarification": "проблему или трудность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7f42c55-2dbb-36d2-bde3-7cc009f722e9",
                    "sense_definition": "To pretend to be a different person or to have a particular profession in order to deceive others.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_pretense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The journalist <w>posed</w> as a student to investigate the conditions at the university.",
                            "target_lang_translations": {
                                "ru": "Журналист <w>выдал себя</w> за студента, чтобы расследовать обстановку в университете."
                            }
                        },
                        {
                            "text": "He was arrested for <w>posing</w> as a police officer and stopping cars on the highway.",
                            "target_lang_translations": {
                                "ru": "Его арестовали за то, что он <w>выдавал себя</w> за полицейского и останавливал машины на шоссе."
                            }
                        }
                    ],
                    "synonyms": [
                        "impersonate",
                        "masquerade"
                    ],
                    "common_phrases": [
                        "pose as someone",
                        "pose as a doctor",
                        "pose as a police officer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Притворяться другим человеком или представителем определенной профессии с целью обмана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдавать себя",
                                "target_lang_sense_clarification": "за кого-либо"
                            },
                            {
                                "target_lang_word": "притворяться",
                                "target_lang_sense_clarification": "кем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39ec2a19-5512-37f2-a619-e244ce7d36c2",
                    "sense_definition": "To ask a question, especially one that requires careful thought or is difficult to answer.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The moderator <w>posed</w> a difficult question to the candidates during the debate.",
                            "target_lang_translations": {
                                "ru": "Модератор <w>задал</w> кандидатам сложный вопрос во время дебатов."
                            }
                        },
                        {
                            "text": "This discovery <w>poses</w> an interesting question about the origins of the universe.",
                            "target_lang_translations": {
                                "ru": "Это открытие <w>ставит</w> интересный вопрос о происхождении вселенной."
                            }
                        }
                    ],
                    "synonyms": [
                        "ask",
                        "raise",
                        "put forward",
                        "propound"
                    ],
                    "antonyms": [
                        "answer",
                        "reply"
                    ],
                    "common_phrases": [
                        "pose a question",
                        "pose a riddle",
                        "pose a dilemma"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задавать вопрос или выдвигать проблему, требующую тщательного обдумывания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задавать",
                                "target_lang_sense_clarification": "вопрос"
                            },
                            {
                                "target_lang_word": "ставить",
                                "target_lang_sense_clarification": "вопрос или проблему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67e0c0c9-c5e7-3ec7-803a-d6767827dfdd",
                    "sense_definition": "To behave, dress, or speak in an affected manner to impress others or attract attention.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_pretense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spends most of his time at the gym <w>posing</w> in front of the mirror rather than exercising.",
                            "target_lang_translations": {
                                "ru": "Он проводит большую часть времени в спортзале, <w>позируя</w> перед зеркалом, а не тренируясь."
                            }
                        },
                        {
                            "text": "She told him to stop <w>posing</w> and just be himself for once.",
                            "target_lang_translations": {
                                "ru": "Она сказала ему перестать <w>рисоваться</w> и хоть раз побыть самим собой."
                            }
                        }
                    ],
                    "synonyms": [
                        "posturize",
                        "show off"
                    ],
                    "common_phrases": [
                        "stop posing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя неестественно, манерничать, чтобы произвести впечатление на окружающих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рисоваться",
                                "target_lang_sense_clarification": "вести себя манерно"
                            },
                            {
                                "target_lang_word": "позировать",
                                "target_lang_sense_clarification": "вести себя неестественно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a72f538f-cc4b-358d-a6f6-859ceeb742bd",
                    "sense_definition": "A particular position in which someone stands, sits, or lies, especially for a photograph, painting, or performance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_physical_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gymnast finished her routine and held a dramatic <w>pose</w> for the judges.",
                            "target_lang_translations": {
                                "ru": "Гимнастка закончила выступление и застыла в эффектной <w>позе</w> перед судьями."
                            }
                        },
                        {
                            "text": "Please adopt a more natural <w>pose</w> so the photo does not look staged.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, примите более естественную <w>позу</w>, чтобы фото не выглядело постановочным."
                            }
                        }
                    ],
                    "synonyms": [
                        "posture",
                        "stance",
                        "position",
                        "attitude"
                    ],
                    "common_phrases": [
                        "strike a pose",
                        "hold a pose",
                        "yoga pose"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенное положение тела, которое кто-либо принимает, особенно для фотографии, картины или выступления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поза",
                                "target_lang_sense_clarification": "положение тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68f2bdbf-6ff3-30f6-8d82-d464625230a3",
                    "sense_definition": "A way of behaving, speaking, or dressing that is intended to impress other people but is not sincere.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_pretense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His apparent indifference to the award was just a <w>pose</w> to hide his excitement.",
                            "target_lang_translations": {
                                "ru": "Его кажущееся безразличие к награде было лишь <w>позой</w>, скрывающей волнение."
                            }
                        },
                        {
                            "text": "She adopted a <w>pose</w> of sophistication that fooled no one at the party.",
                            "target_lang_translations": {
                                "ru": "Она приняла <w>позу</w> утонченности, которая никого не обманула на вечеринке."
                            }
                        }
                    ],
                    "synonyms": [
                        "affectation",
                        "pretense",
                        "act",
                        "facade"
                    ],
                    "antonyms": [
                        "sincerity",
                        "authenticity"
                    ],
                    "common_phrases": [
                        "mere pose",
                        "adopt a pose"
                    ],
                    "target_lang_definitions": {
                        "ru": "Манера поведения, речи или одежды, предназначенная для того, чтобы произвести впечатление, но лишенная искренности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поза",
                                "target_lang_sense_clarification": "притворство"
                            },
                            {
                                "target_lang_word": "рисовка",
                                "target_lang_sense_clarification": "манерность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "916d19bb-2976-3bb6-acbb-3d3731370c26",
                    "sense_definition": "A common cold or head cold; catarrh.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old medical text described a remedy for curing a <w>pose</w> or cough.",
                            "target_lang_translations": {
                                "ru": "В старинном медицинском тексте описывалось средство для лечения <w>насморка</w> или кашля."
                            }
                        },
                        {
                            "text": "He stayed indoors to recover from a troublesome <w>pose</w> that affected his breathing.",
                            "target_lang_translations": {
                                "ru": "Он остался дома, чтобы вылечить мучительный <w>насморк</w>, который мешал ему дышать."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "caa213bf-bfaa-3bcd-9b7e-424acfef22d7",
                    "sense_definition": "Pospolite przekleństwo wyrażające złość, zniecierpliwienie lub nagłe zaskoczenie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotions_anger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "O <w>cholera</w>, zapomniałem wyłączyć żelazko przed wyjściem!",
                            "target_lang_translations": {
                                "ru": "О <w>чёрт</w>, я забыл выключить утюг перед уходом!"
                            }
                        },
                        {
                            "text": "Gdzie, do <w>cholery</w>, położyłem kluczyki od samochodu?",
                            "target_lang_translations": {
                                "ru": "Куда, к <w>чёрту</w>, я положил ключи от машины?"
                            }
                        }
                    ],
                    "synonyms": [
                        "kurde",
                        "psiakrew",
                        "do diaska",
                        "do kroćset"
                    ],
                    "common_phrases": [
                        "do cholery",
                        "jasna cholera",
                        "o cholera",
                        "niech to cholera"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo powszechnie używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенное ругательство, выражающее злость, нетерпение или внезапное удивление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрт",
                                "target_lang_sense_clarification": "самое частое соответствие"
                            },
                            {
                                "target_lang_word": "холера",
                                "target_lang_sense_clarification": "бранное слово"
                            },
                            {
                                "target_lang_word": "блин",
                                "target_lang_sense_clarification": "разговорное, эвфемизм"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9dc1431b-ac73-30b5-b5a4-20426af2235d",
                    "sense_definition": "Potoczne, obraźliwe określenie osoby nieznośnej, kłótliwej lub złośliwej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "insults",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta wredna <w>cholera</w> znowu zrobiła nam awanturę o hałas.",
                            "target_lang_translations": {
                                "ru": "Эта вредная <w>холера</w> снова устроила нам скандал из-за шума."
                            }
                        },
                        {
                            "text": "Nie bądź taką <w>cholerą</w> i pomóż mi wreszcie z tym zadaniem.",
                            "target_lang_translations": {
                                "ru": "Не будь такой <w>холерой</w> и помоги мне наконец с этим заданием."
                            }
                        }
                    ],
                    "synonyms": [
                        "(o kobiecie) baba",
                        "(o mężczyźnie) dziad"
                    ],
                    "common_phrases": [
                        "stara cholera",
                        "wredna cholera"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane jako wyzwisko"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, оскорбительное название невыносимого, сварливого или злобного человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "холера",
                                "target_lang_sense_clarification": "бранное, о человеке"
                            },
                            {
                                "target_lang_word": "зараза",
                                "target_lang_sense_clarification": "разговорное, о человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a592397-fbb2-3c1e-9221-8cb81eff1ed3",
                    "sense_definition": "Groźna choroba zakaźna przewodu pokarmowego, wywoływana przez bakterie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "diseases",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych czasach <w>cholera</w> dziesiątkowała ludność wielkich miast.",
                            "target_lang_translations": {
                                "ru": "В прежние времена <w>холера</w> косила население больших городов."
                            }
                        },
                        {
                            "text": "Podróżując do niektórych krajów, warto zaszczepić się na <w>cholerę</w>.",
                            "target_lang_translations": {
                                "ru": "Путешествуя в некоторые страны, стоит привиться от <w>холеры</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "epidemia cholery",
                        "szczepionka na cholerę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опасное инфекционное заболевание желудочно-кишечного тракта, вызываемое бактериями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "холера",
                                "target_lang_sense_clarification": "болезнь"

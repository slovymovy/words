                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пускал почки",
                                "target_lang_sense_clarification": "о деревьях и кустарниках"
                            },
                            {
                                "target_lang_word": "распускался",
                                "target_lang_sense_clarification": "начинал цвести или зеленеть"
                            }

            "senses": [
                {
                    "sense_id": "ceaea6af-0b83-3187-8915-b9d96d7e4b2c",
                    "sense_definition": "Iets wat dient om een ruimte of voorwerp te versieren en mooier te maken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>decoratie</w> in de feestzaal zorgde direct voor een goede sfeer.",
                            "target_lang_translations": {
                                "ru": "<w>Декор</w> в банкетном зале сразу создал хорошую атмосферу.",
                                "en": "The <w>decoration</w> in the banquet hall immediately created a good atmosphere."
                            }
                        },
                        {
                            "text": "In de kerstboom werd prachtige <w>decoratie</w> gehangen.",
                            "target_lang_translations": {
                                "ru": "На елку повесили красивые <w>украшения</w>.",
                                "en": "Beautiful <w>decorations</w> were hung in the Christmas tree."
                            }
                        }
                    ],
                    "synonyms": [
                        "versiering",
                        "versiersel",
                        "ornamentatie"
                    ],
                    "common_phrases": [
                        "feestelijke decoratie",
                        "wanddecoratie",
                        "kerstdecoratie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то, что служит для украшения помещения или предмета, делая его красивее.",
                        "en": "Something serving to adorn a space or object and make it more beautiful."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украшение",

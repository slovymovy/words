{
    "word_family": [
        "China",
        "chinezen"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "73a22336-44c4-3c7e-a007-70b777c752b8",
                    "sense_definition": "Behorend tot of afkomstig uit China, de inwoners of de cultuur.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adj_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <Chinese> economie groeit elk jaar erg snel.",
                            "target_lang_translations": {
                                "ru": "<w>Китайская</w> экономика растет очень быстро с каждым годом."
                            }
                        },
                        {
                            "text": "Wij hebben de <Chinese> Muur bezocht tijdens onze vakantie.",
                            "target_lang_translations": {
                                "ru": "Мы посетили Великую <w>Китайскую</w> стену во время нашего отпуска."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Chinese Muur",
                        "Chinese kool",
                        "Chinees Nieuwjaar",
                        "Chinese vrijwilliger"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Китаю, его жителям или культуре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китайский",
                                "target_lang_sense_clarification": "общий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1716df2b-46f7-300c-a73a-7dd102495491",
                    "sense_definition": "Met betrekking tot de Chinese taal.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_lang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij bestudeert de <Chinese> grammatica al jaren.",
                            "target_lang_translations": {
                                "ru": "Он изучает <w>китайскую</w> грамматику уже много лет."
                            }
                        },
                        {
                            "text": "In de bibliotheek staan veel <Chinese> boeken.",
                            "target_lang_translations": {
                                "ru": "В библиотеке много <w>китайских</w> книг."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Chinese karakters",
                        "Chinese taal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к китайскому языку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китайский",
                                "target_lang_sense_clarification": "о языке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bd611532-8155-3ec4-86a0-152316403699",
                    "sense_definition": "Een restaurant waar gerechten uit de Chinese keuken worden geserveerd.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_restaurant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zullen we vanavond eten bestellen bij de <Chinees>?",
                            "target_lang_translations": {
                                "ru": "Закажем сегодня ужин в <w>китайском ресторане</w>?"
                            }
                        },
                        {
                            "text": "De <Chinees> op de hoek is op dinsdag gesloten.",
                            "target_lang_translations": {
                                "ru": "<w>Китайский ресторан</w> на углу закрыт по вторникам."
                            }
                        }
                    ],
                    "synonyms": [
                        "Chinees restaurant"
                    ],
                    "common_phrases": [
                        "bij de Chinees",
                        "naar de Chinees"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "verwijst naar 'Chinees restaurant'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ресторан, где подают блюда китайской кухни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китайский ресторан",
                                "target_lang_sense_clarification": "заведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "630d1082-cb42-3128-a65f-2a977a8ef603",
                    "sense_definition": "Gerechten of voedsel uit de Chinese keuken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb jij zin om vanavond <Chinees> te eten?",
                            "target_lang_translations": {
                                "ru": "Хочешь сегодня поесть <w>китайской еды</w>?"
                            }
                        },
                        {
                            "text": "We hebben gisteren heerlijk <Chinees> gehaald voor het feest.",
                            "target_lang_translations": {
                                "ru": "Мы вчера купили вкусную <w>китайскую еду</w> для вечеринки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Chinees halen",
                        "Chinees eten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Блюда или еда китайской кухни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китайская еда",
                                "target_lang_sense_clarification": "пища"
                            },
                            {
                                "target_lang_word": "китайская кухня",
                                "target_lang_sense_clarification": "кулинария"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10a6d4ac-0b24-35db-92b8-6173825de93d",
                    "sense_definition": "Een mannelijke inwoner van China of iemand van Chinese afkomst.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <Chinees> vroeg in het Engels de weg naar het station.",
                            "target_lang_translations": {
                                "ru": "<w>Китаец</w> спросил дорогу на вокзал по-английски."
                            }
                        },
                        {
                            "text": "Hij is een geboren <Chinees> die nu in Nederland woont.",
                            "target_lang_translations": {
                                "ru": "Он урожденный <w>китаец</w>, который сейчас живет в Нидерландах."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Житель Китая мужского пола или человек китайского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китаец",
                                "target_lang_sense_clarification": "мужчина"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "2b9c4d7a-4c3f-39c3-ab5c-f981e4becc51",
                    "sense_definition": "De officiële taal die in China en door Chinezen wereldwijd wordt gesproken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "name_lang",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Ik vind <Chinees> een van de moeilijkste talen om te leren.",
                            "target_lang_translations": {
                                "ru": "Я считаю <w>китайский</w> одним из самых сложных языков для изучения."
                            }
                        },
                        {
                            "text": "Spreek jij een beetje <Chinees> of alleen Engels?",
                            "target_lang_translations": {
                                "ru": "Ты говоришь немного <w>по-китайски</w> или только по-английски?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "Mandarijn Chinees",
                        "traditioneel Chinees"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный язык, на котором говорят в Китае и китайцы по всему миру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "китайский язык",
                                "target_lang_sense_clarification": "язык"
                            },
                            {
                                "target_lang_word": "китайский",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6e39532d-546e-357e-8063-395c5cb64a2f",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <chinezen>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <chinees> vanavond met mijn collega's na het werk.",
                            "target_lang_translations": {
                                "ru": "Я <w>ем китайскую еду</w> сегодня вечером с коллегами после работы."
                            }
                        },
                        {
                            "text": "Als ik geen zin heb om te koken, <chinees> ik vaak.",
                            "target_lang_translations": {
                                "ru": "Если мне не хочется готовить, я часто <w>ем китайское</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола chinezen (есть китайскую еду)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ем китайскую еду",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
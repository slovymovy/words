    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "eb889d0a-f749-3066-b6a5-fc15e9daee12",
                    "sense_definition": "Брать кого-либо в свое транспортное средство, чтобы помочь добраться до нужного места; везти попутно.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой сосед часто <w>подвозит</w> меня до станции метро по утрам.",
                            "target_lang_translations": {
                                "en": "My neighbor often <w>gives</w> me <w>a lift</w> to the metro station in the mornings."
                            }
                        },
                        {
                            "text": "Я не люблю, когда меня <w>подвозят</w> незнакомые люди, я предпочитаю такси.",
                            "target_lang_translations": {
                                "en": "I don't like it when strangers <w>give</w> me <w>a ride</w>; I prefer taxis."
                            }
                        }
                    ],
                    "synonyms": [
                        "подбрасывать"
                    ],
                    "common_phrases": [
                        "подвозить до дома",
                        "подвозить до работы",
                        "подвозить друга",
                        "подвозить попутчика"
                    ],
                    "target_lang_definitions": {
                        "en": "To take someone in one's vehicle to help them reach a destination; to give a lift."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give a lift",
                                "target_lang_sense_clarification": "to take a passenger to a destination"
                            },
                            {
                                "target_lang_word": "give a ride",
                                "target_lang_sense_clarification": "to transport someone in a vehicle"
                            },
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "to take someone somewhere by car"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e884b9da-8903-3982-a81c-f577b81339ee",
                    "sense_definition": "Доставлять грузы, товары или людей к определенному месту, используя транспорт.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Грузовики регулярно <w>подвозят</w> свежие продукты в этот магазин.",
                            "target_lang_translations": {
                                "en": "Trucks regularly <w>deliver</w> fresh products to this store."
                            }
                        },
                        {
                            "text": "Во время строительства рабочим постоянно <w>подвозят</w> кирпичи и цемент.",
                            "target_lang_translations": {
                                "en": "During construction, bricks and cement are constantly <w>brought</w> to the workers."
                            }
                        }
                    ],
                    "synonyms": [
                        "доставлять",
                        "привозить"
                    ],
                    "antonyms": [
                        "увозить",
                        "отвозить"
                    ],
                    "common_phrases": [
                        "подвозить боеприпасы",
                        "подвозить материалы",
                        "подвозить товары",
                        "подвозить продовольствие"
                    ],
                    "target_lang_definitions": {
                        "en": "To deliver cargo, goods, or people to a specific location using transport."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deliver",
                                "target_lang_sense_clarification": "to transport goods to a destination"
                            },
                            {
                                "target_lang_word": "bring",
                                "target_lang_sense_clarification": "to carry or convey something to a place"
                            },
                            {
                                "target_lang_word": "supply",
                                "target_lang_sense_clarification": "to provide goods or materials"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
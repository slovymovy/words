    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7f6649c4-3858-354d-956c-aa8b12e3f3eb",
                    "sense_definition": "Sterk emotioneel verbonden met iemand of iets; wanneer je iets niet graag wilt loslaten of verliezen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn oma is erg <w>gehecht</w> aan haar oude huis.",
                            "target_lang_translations": {
                                "en": "My grandma is very <w>attached</w> to her old house.",
                                "ru": "Моя бабушка очень <w>привязана</w> к своему старому дому."
                            }
                        },
                        {
                            "text": "De kinderen zijn zeer <w>gehecht</w> aan hun leraar.",
                            "target_lang_translations": {
                                "en": "The children are very <w>attached</w> to their teacher.",
                                "ru": "Дети очень <w>привязаны</w> к своему учителю."
                            }
                        }
                    ],
                    "synonyms": [
                        "verknocht",
                        "toegedaan"
                    ],
                    "antonyms": [
                        "onverschillig"
                    ],
                    "common_phrases": [
                        "ergens aan gehecht zijn",
                        "zeer gehecht"
                    ],
                    "target_lang_definitions": {
                        "en": "Strongly emotionally connected to someone or something; when you do not want to let go of or lose something.",
                        "ru": "Сильно эмоционально связанный с кем-то или чем-то; когда не хочется что-то отпускать или терять."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attached",
                                "target_lang_sense_clarification": "emotionally connected"
                            },
                            {
                                "target_lang_word": "fond",
                                "target_lang_sense_clarification": "having affection for"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "привязанный",
                                "target_lang_sense_clarification": "эмоционально"
                            }
                        ]
                    }
                },
                {

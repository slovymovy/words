                    "translations": {
                        "ru": []
                    }
                },
                {
                    "sense_id": "d4d36dbb-33f2-3c43-a4e7-cd3cf7e9e4f8",
                    "sense_definition": "The lexical aspect of verbs or predicates that describes a situation that does not change over time.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "linguistics_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In linguistics, verbs are often classified into <w>states</w> and events.",
                            "target_lang_translations": {
                                "ru": "В лингвистике глаголы часто классифицируются на <w>состояния</w> и события."
                            }
                        },
                        {
                            "text": "Stative verbs express a <w>state</w> rather than an action.",
                            "target_lang_translations": {
                                "ru": "Статальные глаголы выражают <w>состояние</w>, а не действие."
                            }
                        }
                    ],
                    "antonyms": [
                        "occurrence"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "linguistics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лингвистическая категория, описывающая неизменную ситуацию (статальность)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состоя́ние",
                                "target_lang_sense_clarification": "в лингвистике: отсутствие изменения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ddfac2c5-e914-3bb9-8649-9ac590a5b03d",
                    "sense_definition": "To express something definitely or clearly in speech or writing.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>state</w> your name and address for the record.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>назовите</w> свое имя и адрес для протокола."
                            }
                        },
                        {
                            "text": "The report <w>states</w> that the company has made a profit.",
                            "target_lang_translations": {
                                "ru": "В отчете <w>утверждается</w>, что компания получила прибыль."
                            }
                        }
                    ],
                    "synonyms": [
                        "declare",
                        "announce",
                        "express",
                        "specify",
                        "say"
                    ],
                    "common_phrases": [
                        "state clearly",
                        "state the obvious",
                        "state the facts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально заявлять, утверждать или ясно излагать факты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заявля́ть",
                                "target_lang_sense_clarification": "официально сообщать"
                            },
                            {
                                "target_lang_word": "утвержда́ть",
                                "target_lang_sense_clarification": "настаивать на истинности"
                            },
                            {
                                "target_lang_word": "излага́ть",
                                "target_lang_sense_clarification": "последовательно выражать мысли"
                            }
                        ]
                    }
                }
            ]
        },

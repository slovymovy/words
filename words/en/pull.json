                            "text": "I don't have enough <w>pull</w> to get you an interview.",
                            "target_lang_translations": {
                                "ru": "У меня недостаточно <w>связей</w>, чтобы устроить тебе собеседование."
                            }
                        }
                    ],
                    "synonyms": [
                        "influence",
                        "sway",
                        "clout"
                    ],
                    "common_phrases": [
                        "have pull with"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

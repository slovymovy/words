                    ],
                    "target_lang_definitions": {
                        "ru": "Удалять шерсть со шкуры или кожи, обычно в процессе дубления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обезволашивать",
                                "target_lang_sense_clarification": "технический термин"

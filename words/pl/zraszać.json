    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7f440886-5b04-3b37-b19c-d3cfdf29d281",
                    "sense_definition": "Spryskiwać coś cieczą rozbitą na drobne krople, zazwyczaj w celu nawilżenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_spraying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pamiętaj, aby codziennie <zraszać> liście tej egzotycznej rośliny doniczkowej.",
                            "target_lang_translations": {
                                "ru": "Не забудь ежедневно <опрыскивать> листья этого экзотического комнатного растения."
                            }
                        },
                        {
                            "text": "Przed prasowaniem warto lekko <zraszać> przesuszoną bieliznę wodą.",
                            "target_lang_translations": {
                                "ru": "Перед глажкой стоит слегка <сбрызгивать> пересушенное белье водой."
                            }
                        }
                    ],
                    "synonyms": [
                        "rosić",
                        "kropić",
                        "siąpić"
                    ],
                    "common_phrases": [
                        "zraszać rośliny",
                        "zraszać trawnik",
                        "zraszać wodą",
                        "zraszać pranie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обрызгивать или опрыскивать что-либо мелкими каплями жидкости, обычно для увлажнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опрыскивать",
                                "target_lang_sense_clarification": "растения, поверхности"
                            },
                            {
                                "target_lang_word": "сбрызгивать",
                                "target_lang_sense_clarification": "белье, волосы"
                            },
                            {
                                "target_lang_word": "орошать",
                                "target_lang_sense_clarification": "газон, землю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da944125-4f98-302e-aa90-78615c41f78b",
                    "sense_definition": "O deszczu lub mgle: opadać na coś delikatnie, pokrywając powierzchnię wilgocią.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nature_wetting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drobny kapuśniaczek zaczął powoli <zraszać> suchy chodnik przed domem.",
                            "target_lang_translations": {
                                "ru": "Мелкий грибной дождь начал медленно <кропить> сухой тротуар перед домом."
                            }
                        },
                        {
                            "text": "Gęsta poranna mgła unosiła się nad rzeką i <zraszała> przybrzeżne trawy.",
                            "target_lang_translations": {
                                "ru": "Густой утренний туман поднимался над рекой и <орошал> прибрежные травы."
                            }
                        }
                    ],
                    "synonyms": [
                        "mżyć",
                        "siąpić",
                        "kropić",
                        "rosić"
                    ],
                    "common_phrases": [
                        "deszcz zrasza ziemię",
                        "mgła zrasza łąki"
                    ],
                    "target_lang_definitions": {
                        "ru": "О дожде или тумане: мягко оседать на поверхность, покрывая её влагой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "орошать",
                                "target_lang_sense_clarification": "покрывать влагой"
                            },
                            {
                                "target_lang_word": "кропить",
                                "target_lang_sense_clarification": "о мелком дожде"
                            },
                            {
                                "target_lang_word": "смачивать",
                                "target_lang_sense_clarification": "делать влажным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "228d9caa-edf7-35c6-b158-45cfb2136d5b",
                    "sense_definition": "Pokrywać powierzchnię ciała lub przedmiotu kroplami cieczy, takiej jak pot, łzy lub krew.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nature_wetting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po intensywnym biegu krople zimnego potu zaczęły <zraszać> jego plecy.",
                            "target_lang_translations": {
                                "ru": "После интенсивного бега капли холодного пота начали <покрывать> его спину."
                            }
                        },
                        {
                            "text": "Wzruszyła się tak bardzo, że łzy zaczęły obficie <zraszać> jej policzki.",
                            "target_lang_translations": {
                                "ru": "Она так растрогалась, что слезы начали обильно <орошать> её щеки."
                            }
                        }
                    ],
                    "synonyms": [
                        "rosić"
                    ],
                    "common_phrases": [
                        "pot zrasza czoło",
                        "łzy zraszają twarz",
                        "krew zrasza ziemię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать поверхность тела или предмета каплями жидкости (потом, слезами, кровью)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "орошать",
                                "target_lang_sense_clarification": "слезами, кровью (книжн.)"
                            },
                            {
                                "target_lang_word": "покрывать",
                                "target_lang_sense_clarification": "каплями пота"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
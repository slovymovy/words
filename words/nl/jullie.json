    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "5a6396c5-4575-31f6-a3bc-398e5e98c330",
                    "sense_definition": "Verwijst naar de personen tegen wie men spreekt (meervoud) als onderwerp van de zin.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "subject_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>jullie</w> klaar om te vertrekken?",
                            "target_lang_translations": {
                                "en": "Are <w>you</w> ready to leave?",
                                "ru": "<w>Вы</w> готовы отправляться?"
                            }
                        },
                        {
                            "text": "<w>Jullie</w> komen precies op het juiste moment binnen.",
                            "target_lang_translations": {
                                "en": "<w>You</w> are coming in at exactly the right moment.",
                                "ru": "<w>Вы</w> заходите как раз вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "je"
                    ],
                    "antonyms": [
                        "wij",
                        "zij"
                    ],
                    "common_phrases": [
                        "weten jullie",
                        "kunnen jullie",
                        "hebben jullie",
                        "zijn jullie"
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to the people spoken to (plural) as the subject of the sentence.",
                        "ru": "Обозначает людей, к которым обращаются (во множественном числе), в роли подлежащего."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you",
                                "target_lang_sense_clarification": "plural subject"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вы",
                                "target_lang_sense_clarification": "личное местоимение (множественное число)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "605e8556-aaae-3c6f-9be5-db46af4f17ac",
                    "sense_definition": "Verwijst naar de personen tegen wie men spreekt (meervoud) als lijdend of meewerkend voorwerp, of na een voorzetsel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "object_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal het morgen aan <w>jullie</w> geven.",
                            "target_lang_translations": {
                                "en": "I will give it to <w>you</w> tomorrow.",
                                "ru": "Я дам это <w>вам</w> завтра."
                            }
                        },
                        {
                            "text": "De leraar heeft <w>jullie</w> gisteren in de stad gezien.",
                            "target_lang_translations": {
                                "en": "The teacher saw <w>you</w> in the city yesterday.",
                                "ru": "Учитель видел <w>вас</w> вчера в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "je"
                    ],
                    "antonyms": [
                        "ons",
                        "hen"
                    ],
                    "common_phrases": [
                        "aan jullie",
                        "voor jullie",
                        "met jullie",
                        "bij jullie"
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to the people spoken to (plural) as a direct or indirect object, or after a preposition.",
                        "ru": "Обозначает людей, к которым обращаются (во множественном числе), в роли дополнения или после предлога."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you",
                                "target_lang_sense_clarification": "plural object"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вас",
                                "target_lang_sense_clarification": "винительный/родительный падеж"
                            },
                            {
                                "target_lang_word": "вам",
                                "target_lang_sense_clarification": "дательный падеж"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "d4b44f9b-3b8d-32e8-82eb-eaa0207146ed",
                    "sense_definition": "Geeft aan dat iets toebehoort aan de personen tegen wie men spreekt (meervoud).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possessive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jullie</w> leraar is vandaag helaas ziek.",
                            "target_lang_translations": {
                                "en": "<w>Your</w> teacher is unfortunately sick today.",
                                "ru": "<w>Ваш</w> учитель, к сожалению, сегодня болен."
                            }
                        },
                        {
                            "text": "Ik ben erg benieuwd naar <w>jullie</w> mening over dit onderwerp.",
                            "target_lang_translations": {
                                "en": "I am very curious about <w>your</w> opinion on this subject.",
                                "ru": "Мне очень интересно <w>ваше</w> мнение по этому вопросу."
                            }
                        }
                    ],
                    "synonyms": [
                        "je"
                    ],
                    "antonyms": [
                        "onze",
                        "hun"
                    ],
                    "common_phrases": [
                        "jullie huis",
                        "jullie mening",
                        "jullie beurt",
                        "jullie ouders"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates that something belongs to the people spoken to (plural).",
                        "ru": "Указывает на принадлежность чего-либо людям, к которым обращаются (во множественном числе)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "your",
                                "target_lang_sense_clarification": "plural possessive"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ваш",
                                "target_lang_sense_clarification": "притяжательное местоимение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
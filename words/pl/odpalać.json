            "pos": "verb",
            "senses": [
                {
                    "sense_id": "658c0056-dc45-30a4-9bb5-8dccc905ec8e",
                    "sense_definition": "Uruchamiać urządzenie, silnik, maszynę lub program komputerowy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "start_machine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą zawsze długo <w>odpalam</w> samochód przed jazdą do pracy.",
                            "target_lang_translations": {
                                "ru": "Зимой я всегда долго <w>завожу</w> машину перед поездкой на работу."
                            }
                        },
                        {
                            "text": "Mój syn od razu po szkole <w>odpala</w> komputer i gra w gry.",
                            "target_lang_translations": {
                                "ru": "Мой сын сразу после школы <w>включает</w> компьютер и играет в игры."
                            }
                        }
                    ],
                    "antonyms": [
                        "gasić",
                        "wyłączać"
                    ],
                    "common_phrases": [
                        "odpalać samochód",
                        "odpalać silnik",
                        "odpalać komputer",
                        "odpalać grę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech instead of 'uruchamiać' or 'włączać'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Запускать устройство, двигатель, машину или компьютерную программу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заводить",
                                "target_lang_sense_clarification": "двигатель, автомобиль"
                            },
                            {
                                "target_lang_word": "запускать",
                                "target_lang_sense_clarification": "программу, игру, компьютер"
                            },
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "устройство, прибор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd43b912-63fd-3f76-8dd1-9ebf8f9cd358",
                    "sense_definition": "Powodować zapłon czegoś, zapalać ogień lub materiał wybuchowy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ignite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sylwestra o północy wszyscy <w>odpalamy</w> sztuczne ognie.",
                            "target_lang_translations": {
                                "ru": "В новогоднюю ночь в полночь мы все <w>запускаем</w> фейерверки."
                            }
                        },
                        {
                            "text": "Nie wolno <w>odpalać</w> grilla na balkonie w bloku.",
                            "target_lang_translations": {
                                "ru": "Нельзя <w>разжигать</w> гриль на балконе в многоквартирном доме."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapalać"
                    ],
                    "antonyms": [
                        "gasić"
                    ],
                    "common_phrases": [
                        "odpalać papierosa",
                        "odpalać fajerwerki",
                        "odpalać znicz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать возгорание чего-либо, зажигать огонь или взрывчатое вещество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зажигать",
                                "target_lang_sense_clarification": "свечу, огонь"
                            },
                            {
                                "target_lang_word": "разжигать",
                                "target_lang_sense_clarification": "гриль, костер"
                            },
                            {
                                "target_lang_word": "запускать",
                                "target_lang_sense_clarification": "фейерверки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a78d01e-81e4-353b-92b6-5d011b033140",
                    "sense_definition": "Wystrzeliwać pocisk, rakietę lub ładunek w określonym kierunku.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wojsko <w>odpala</w> rakiety testowe na specjalnym poligonie.",
                            "target_lang_translations": {
                                "ru": "Военные <w>запускают</w> испытательные ракеты на специальном полигоне."
                            }
                        },
                        {
                            "text": "Okręt wojenny <w>odpala</w> torpedy w stronę celu.",
                            "target_lang_translations": {
                                "ru": "Военный корабль <w>выпускает</w> торпеды в сторону цели."
                            }
                        }
                    ],
                    "common_phrases": [

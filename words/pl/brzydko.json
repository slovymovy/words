                {
                    "sense_id": "b4236f82-1b64-39bb-8b8f-10570862d270",
                    "sense_definition": "W sposób nieestetyczny, nieatrakcyjny wizualnie lub sprawiający przykre wrażenie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Elżbieta zawsze przychodzi do pracy tak <w>brzydko</w> ubrana.",
                            "target_lang_translations": {
                                "ru": "Эльжбета всегда приходит на работу так <w>некрасиво</w> одетой."
                            }
                        },
                        {
                            "text": "Ten stary budynek wygląda bardzo <w>brzydko</w> na tle nowoczesnych biurowców.",
                            "target_lang_translations": {
                                "ru": "Это старое здание выглядит очень <w>некрасиво</w> на фоне современных офисных центров."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieładnie"
                    ],
                    "antonyms": [
                        "ładnie",
                        "pięknie",
                        "ślicznie"
                    ],
                    "common_phrases": [
                        "brzydko wyglądać",
                        "brzydko pachnieć",
                        "brzydko pisać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неэстетично, визуально непривлекательно или производя неприятное впечатление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "некрасиво",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "уродливо",
                                "target_lang_sense_clarification": "крайне некрасиво"
                            },
                            {
                                "target_lang_word": "дурно",
                                "target_lang_sense_clarification": "о внешнем виде или вкусе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6b95d12-4910-3d1a-99af-98e52f9149c8",
                    "sense_definition": "W sposób świadczący o braku kultury, złym wychowaniu lub braku szacunku dla innych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz przyznać, że Rafał <w>brzydko</w> się zachował, nie puszczając ich przodem.",
                            "target_lang_translations": {
                                "ru": "Ты должен признать, что Рафал <w>некрасиво</w> повел себя, не пропустив их вперед."
                            }
                        },
                        {
                            "text": "Nie wolno tak <w>brzydko</w> odzywać się do starszych osób.",
                            "target_lang_translations": {
                                "ru": "Нельзя так <w>грубо</w> разговаривать со старшими."
                            }
                        }
                    ],
                    "synonyms": [
                        "niegrzecznie",
                        "nieładnie"
                    ],
                    "antonyms": [
                        "grzecznie",
                        "ładnie"
                    ],
                    "common_phrases": [
                        "brzydko się zachowywać",
                        "brzydko postąpić",
                        "brzydko mówić o kimś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, свидетельствующим о невоспитанности, плохих манерах или неуважении."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "be30205a-830b-37cc-bc2d-579e213a5bd4",
                    "sense_definition": "Iemand die niet meer meedoet aan een wedstrijd, programma of organisatie, vaak omdat hij of zij is weggestemd of heeft opgegeven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was helaas de eerste <w>afvaller</w> in de spannende talentenjacht.",
                            "target_lang_translations": {
                                "ru": "К сожалению, он стал первым <w>выбывшим</w> в захватывающем шоу талантов."
                            }
                        },
                        {
                            "text": "Door de zware training waren er veel <w>afvallers</w> binnen de groep.",
                            "target_lang_translations": {
                                "ru": "Из-за тяжелых тренировок в группе было много <w>отсеявшихся</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitvaller"
                    ],
                    "antonyms": [
                        "winnaar",
                        "finalist"
                    ],
                    "common_phrases": [
                        "de eerste afvaller",
                        "lijst met afvallers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто больше не участвует в соревновании, программе или организации (выбыл или сдался)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбывший",
                                "target_lang_sense_clarification": "участник, покинувший соревнование"
                            },
                            {
                                "target_lang_word": "отсеявшийся",
                                "target_lang_sense_clarification": "тот, кто не прошел отбор или сдался"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5a1db69-387b-313c-b86c-2d7f1083555c",
                    "sense_definition": "Iemand die gewicht verliest, meestal door een dieet te volgen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De succesvolle <w>afvaller</w> paste eindelijk weer in haar oude broek.",
                            "target_lang_translations": {
                                "ru": "Успешно <w>похудевшая</w> наконец-то снова влезала в свои старые брюки."
                            }
                        },
                        {
                            "text": "Als <w>afvaller</w> moet je goed op je voeding blijven letten.",
                            "target_lang_translations": {
                                "ru": "Как <w>худеющий</w>, ты должен продолжать внимательно следить за своим питанием."
                            }
                        }
                    ],
                    "synonyms": [
                        "lijner"
                    ],
                    "common_phrases": [
                        "succesvolle afvaller"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который сбрасывает вес, обычно соблюдая диету."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "худеющий",
                                "target_lang_sense_clarification": "человек в процессе снижения веса"
                            },
                            {
                                "target_lang_word": "похудевший",
                                "target_lang_sense_clarification": "человек, сбросивший вес"
                            }
                        ]
                    }
                }
            ]
        }
    ]

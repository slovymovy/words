    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d254ed1b-656a-34f6-8dee-ccf185bfdf85",
                    "sense_definition": "A non-native person who comes to a country from another country to permanently settle there.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "human_migration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many <w>immigrants</w> move to the city in search of better job opportunities.",
                            "target_lang_translations": {
                                "ru": "Многие <w>иммигранты</w> переезжают в город в поисках лучших возможностей для трудоустройства."
                            }
                        },
                        {
                            "text": "The government is discussing new policies to help undocumented <w>immigrants</w> integrate into society.",
                            "target_lang_translations": {
                                "ru": "Правительство обсуждает новую политику, призванную помочь нелегальным <w>иммигрантам</w> интегрироваться в общество."
                            }
                        }
                    ],
                    "synonyms": [
                        "settler",
                        "newcomer"
                    ],
                    "antonyms": [
                        "emigrant"
                    ],
                    "common_phrases": [
                        "illegal immigrant",
                        "undocumented immigrant",
                        "recent immigrant",
                        "immigrant population",
                        "immigrant community"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который переезжает в другую страну для постоянного проживания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иммигрант",
                                "target_lang_sense_clarification": "человек, переехавший в другую страну"
                            },
                            {
                                "target_lang_word": "переселенец",
                                "target_lang_sense_clarification": "человек, сменивший место жительства; поселенец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99be12f2-63cd-30b5-8d9c-114ffd5037ee",
                    "sense_definition": "A plant or animal that establishes itself in an area where it previously did not exist.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biological_migration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The biologist identified the bird as an <w>immigrant</w> from a warmer climate.",
                            "target_lang_translations": {
                                "ru": "Биолог определил эту птицу как <w>иммигранта</w> из более теплого климата."
                            }
                        },
                        {
                            "text": "This <w>immigrant</w> species has thrived in the local ecosystem without natural predators.",
                            "target_lang_translations": {
                                "ru": "Этот вид-<w>иммигрант</w> процветает в местной экосистеме в отсутствие естественных хищников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "immigrant species"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение или животное, которое обосновывается на территории, где оно ранее не существовало."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иммигрант",
                                "target_lang_sense_clarification": "биол. вид, вселившийся на новую территорию"
                            },
                            {
                                "target_lang_word": "вселенец",
                                "target_lang_sense_clarification": "организм, распространившийся за пределы своего ареала"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bf02601d-cee5-3e1f-8b3c-caee4e552854",
                    "sense_definition": "Of or relating to immigrants or the act of immigrating.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She applied for an <w>immigrant</w> visa to join her family in the United States.",
                            "target_lang_translations": {
                                "ru": "Она подала заявление на получение <w>иммиграционной</w> визы, чтобы воссоединиться с семьей в Соединенных Штатах."
                            }
                        },
                        {
                            "text": "The museum features an exhibition on the city's rich <w>immigrant</w> history.",
                            "target_lang_translations": {
                                "ru": "В музее представлена выставка, посвященная богатой <w>иммигрантской</w> истории города."
                            }
                        }
                    ],
                    "common_phrases": [

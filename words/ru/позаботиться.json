    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5440f9d1-25aa-3acd-9d77-d586540342e0",
                    "sense_definition": "Проявить внимание и участие к кому-либо или чему-либо, обеспечить уход или защиту.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "care_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Света <позаботилась> о своём младшем брате, пока родители были на работе.",
                            "target_lang_translations": {
                                "en": "Sveta <took care> of her younger brother while her parents were at work."
                            }
                        },
                        {
                            "text": "Врач настоятельно рекомендовал пациенту <позаботиться> о своём здоровье.",
                            "target_lang_translations": {
                                "en": "The doctor strongly recommended that the patient <take care> of his health."
                            }
                        }
                    ],
                    "synonyms": [
                        "присмотреть",
                        "поухаживать"
                    ],
                    "antonyms": [
                        "бросить",
                        "оставить"
                    ],
                    "common_phrases": [
                        "позаботиться о здоровье",
                        "позаботиться о детях",
                        "позаботиться о себе",
                        "позаботиться о безопасности"
                    ],
                    "target_lang_definitions": {
                        "en": "To show attention and concern for someone or something, to provide care or protection."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take care of",
                                "target_lang_sense_clarification": "general term for providing for needs or safety"
                            },
                            {
                                "target_lang_word": "look after",
                                "target_lang_sense_clarification": "to watch over or tend to someone"
                            },
                            {
                                "target_lang_word": "care for",
                                "target_lang_sense_clarification": "to provide physical needs or help"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4cc16314-4c6c-3fb5-966f-ae29ec2a2904",
                    "sense_definition": "Предпринять необходимые действия для устройства или организации чего-либо; сделать так, чтобы что-то было выполнено.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "care_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы заранее <позаботились> о покупке билетов на поезд, чтобы не опоздать.",
                            "target_lang_translations": {
                                "en": "We <took care> of buying train tickets in advance so as not to be late."
                            }
                        },
                        {
                            "text": "Организаторы <позаботились> о том, чтобы всем гостям было удобно и интересно.",
                            "target_lang_translations": {
                                "en": "The organizers <made sure> that all guests were comfortable and interested."
                            }
                        }
                    ],
                    "synonyms": [
                        "устроить",
                        "организовать",
                        "обеспечить",
                        "подготовить"
                    ],
                    "antonyms": [
                        "запустить"
                    ],
                    "common_phrases": [
                        "позаботиться заранее",
                        "позаботиться о билетах",
                        "позаботиться о ночлеге",
                        "позаботиться о будущем"
                    ],
                    "target_lang_definitions": {
                        "en": "To take necessary actions to arrange or organize something; to ensure that something is done."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take care of",
                                "target_lang_sense_clarification": "to deal with or handle a task"
                            },
                            {
                                "target_lang_word": "see to",
                                "target_lang_sense_clarification": "to attend to a matter or ensure it is done"
                            },
                            {
                                "target_lang_word": "make sure",
                                "target_lang_sense_clarification": "to ensure a specific outcome"
                            }
                        ]
                    }
                }
            ]
        }
    ]

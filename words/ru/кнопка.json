                            },
                            {
                                "target_lang_word": "press stud",
                                "target_lang_sense_clarification": "clothing fastener (UK)"
                            },
                            {
                                "target_lang_word": "popper",
                                "target_lang_sense_clarification": "informal"
                            }
                        ]
                    }
                },

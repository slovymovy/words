    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ba647196-2148-3c3e-841d-2fe01d7003ce",
                    "sense_definition": "Een advocaat of juridisch adviseur die een cliënt bijstaat en vertegenwoordigt in een rechtszaak.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_counsel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte overlegde fluisterend met zijn <w>raadsman</w>.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый шепотом совещался со своим <w>адвокатом</w>."
                            }
                        },
                        {
                            "text": "Zonder de aanwezigheid van zijn <w>raadsman</w> wilde hij geen vragen beantwoorden.",
                            "target_lang_translations": {
                                "ru": "Без присутствия своего <w>адвоката</w> он не хотел отвечать на вопросы."
                            }
                        }
                    ],
                    "synonyms": [
                        "advocaat",
                        "pleitbezorger"
                    ],
                    "common_phrases": [
                        "zijn raadsman raadplegen",
                        "juridisch raadsman"
                    ],
                    "target_lang_definitions": {
                        "ru": "Адвокат или юридический советник, который помогает клиенту и представляет его в суде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адвокат",
                                "target_lang_sense_clarification": "юрист, защищающий интересы в суде"
                            },
                            {
                                "target_lang_word": "защитник",
                                "target_lang_sense_clarification": "сторона защиты в судебном процессе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fc5aa7e-ef1c-3d64-9858-788ac9062832",
                    "sense_definition": "Iemand die een ander van advies dient, vaak in een vertrouwenspositie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "advisor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koning luisterde vaak naar zijn oudste <w>raadsman</w>.",
                            "target_lang_translations": {
                                "ru": "Король часто прислушивался к своему старейшему <w>советнику</w>."
                            }
                        },
                        {
                            "text": "Hij was jarenlang een trouwe <w>raadsman</w> voor de familie.",
                            "target_lang_translations": {
                                "ru": "Он долгие годы был верным <w>советником</w> семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "raadgever",
                        "adviseur"
                    ],
                    "common_phrases": [
                        "wijze raadsman",
                        "trouwe raadsman"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто дает советы другому, часто занимая доверенную позицию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "советник",
                                "target_lang_sense_clarification": "человек, дающий наставления или советы"
                            }
                        ]
                    }
                }
            ]
        }
    ]

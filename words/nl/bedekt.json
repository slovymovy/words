    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ce2fd71a-d337-3d22-a986-52a0cc84f4b4",
                    "sense_definition": "Voorzien van een laag of bedekking waardoor het onderliggende niet meer (volledig) zichtbaar is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De toppen van de bergen waren <w>bedekt</w> met een dikke laag sneeuw.",
                            "target_lang_translations": {
                                "ru": "Вершины гор были <w>покрыты</w> толстым слоем снега."
                            }
                        },
                        {
                            "text": "De tafel is <w>bedekt</w> met een rood kleed voor het feest.",
                            "target_lang_translations": {
                                "ru": "Стол <w>покрыт</w> красной скатертью к празднику."
                            }
                        }
                    ],
                    "synonyms": [
                        "afgedekt",
                        "belegd"
                    ],
                    "antonyms": [
                        "onbedekt"
                    ],
                    "common_phrases": [
                        "bedekt met",
                        "geheel bedekt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабженный слоем или покрытием, из-за которого находящееся под ним больше не (полностью) видно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрытый",
                                "target_lang_sense_clarification": "имеющий что-либо на поверхности"
                            },
                            {
                                "target_lang_word": "укрытый",
                                "target_lang_sense_clarification": "спрятанный или защищенный слоем чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6258bad-1d6d-3136-a260-63bc4a92bead",
                    "sense_definition": "Niet openlijk of direct uitgesproken; op een verhulde manier gebracht.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij uitte een <w>bedekt</w> dreigement aan het adres van zijn concurrent.",
                            "target_lang_translations": {
                                "ru": "Он высказал <w>завуалированную</w> угрозу в адрес своего конкурента."
                            }
                        },
                        {
                            "text": "In <w>bedekte</w> termen liet zij weten dat ze het er niet mee eens was.",
                            "target_lang_translations": {
                                "ru": "В <w>завуалированных</w> выражениях она дала понять, что не согласна с этим."
                            }
                        }
                    ],
                    "synonyms": [
                        "verhuld",
                        "verbloemd",
                        "impliciet"
                    ],
                    "common_phrases": [
                        "in bedekte termen",
                        "een bedekte toespeling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не высказанный открыто или прямо; представленный в скрытой форме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завуалированный",
                                "target_lang_sense_clarification": "неявный, выраженный намеками"
                            },
                            {
                                "target_lang_word": "скрытый",
                                "target_lang_sense_clarification": "тайный, не очевидный"
                            }
                        ]
                    }
                }
            ]
        },
        {

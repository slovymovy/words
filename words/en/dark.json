    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c3ab080e-9af6-3382-9269-d8a25e2b9eb2",
                    "sense_definition": "Having little or no light.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "lack_of_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The room was too <w>dark</w> for us to read the book.",
                            "target_lang_translations": {
                                "ru": "Комната была слишком <w>тёмной</w>, чтобы мы могли читать книгу."
                            }
                        },
                        {
                            "text": "It gets <w>dark</w> very early in the winter evenings.",
                            "target_lang_translations": {
                                "ru": "Зимними вечерами очень рано становится <w>темно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dim",
                        "gloomy",
                        "unlit"
                    ],
                    "antonyms": [
                        "bright",
                        "light",
                        "lit",
                        "sunny"
                    ],
                    "common_phrases": [
                        "dark room",
                        "get dark",
                        "dark night",
                        "dark clouds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий мало света или совсем без света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тёмный",
                                "target_lang_sense_clarification": "лишённый света, освещения"
                            }
                        ]
                    }
                },
                {

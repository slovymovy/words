    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "879bd367-c0e4-35c3-bebf-cf790c89e500",
                    "sense_definition": "A sweet candy flavored with peppermint or spearmint, often eaten to freshen breath.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_flavor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She offered me a <w>mint</w> after we finished our dinner.",
                            "target_lang_translations": {
                                "ru": "Она предложила мне <w>мятную конфету</w> после того, как мы закончили ужин."
                            }
                        },
                        {
                            "text": "I always keep a packet of <w>mints</w> in my pocket.",
                            "target_lang_translations": {
                                "ru": "Я всегда держу пачку <w>мятных леденцов</w> в кармане."
                            }
                        }
                    ],
                    "common_phrases": [
                        "breath mint",
                        "after-dinner mint"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкая конфета со вкусом перечной или колосистой мяты, которую часто едят для освежения дыхания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мятная конфета",
                                "target_lang_sense_clarification": "sweet"
                            },
                            {
                                "target_lang_word": "мятный леденец",
                                "target_lang_sense_clarification": "hard candy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbd2a226-c22c-3905-ac47-28031aadb7c0",
                    "sense_definition": "The aromatic flavor derived from the mint plant, used in food and drinks.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "food_flavor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I love the taste of chocolate and <w>mint</w> together.",
                            "target_lang_translations": {
                                "ru": "Я обожаю вкус шоколада и <w>мяты</w> вместе."
                            }
                        },
                        {
                            "text": "This ice cream has a strong <w>mint</w> flavor.",
                            "target_lang_translations": {
                                "ru": "У этого мороженого сильный <w>мятный</w> вкус."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ароматный вкус, получаемый из растения мята, используемый в еде и напитках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мята",
                                "target_lang_sense_clarification": "flavor"
                            },
                            {
                                "target_lang_word": "мятный вкус",
                                "target_lang_sense_clarification": "taste"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65e171ff-2576-3714-91fd-ab34bd43dae2",
                    "sense_definition": "An aromatic herb with fresh-smelling leaves, often used in cooking, tea, or medicine.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food_flavor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We grow fresh <w>mint</w> in a pot on the windowsill.",
                            "target_lang_translations": {
                                "ru": "Мы выращиваем свежую <w>мяту</w> в горшке на подоконнике."
                            }
                        },
                        {
                            "text": "The recipe calls for a handful of chopped <w>mint</w> leaves.",
                            "target_lang_translations": {
                                "ru": "В рецепте требуется горсть нарезанных листьев <w>мяты</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mentha"

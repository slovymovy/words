    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "30eb30d9-ad39-3d6c-b030-b1a4e00fb960",
                    "sense_definition": "Drzewo liściaste o sercowatych liściach i żółtawych, intensywnie pachnących kwiatach, często sadzone w parkach i alejach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "botany_tree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym ogrodzie rośnie stara, rozłożysta <w>lipa</w>, która daje dużo cienia w upalne dni.",
                            "target_lang_translations": {
                                "ru": "В нашем саду растет старая, раскидистая <w>липа</w>, которая дает много тени в жаркие дни."
                            }
                        },
                        {
                            "text": "Pszczoły zbierają nektar, z którego powstaje pyszny miód, kiedy kwitnie <w>lipa</w>.",
                            "target_lang_translations": {
                                "ru": "Пчелы собирают нектар, из которого получается вкусный мед, когда цветет <w>липа</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kwitnąca lipa",
                        "miód z lipy",
                        "siedzieć pod lipą",
                        "aleja lip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лиственное дерево с сердцевидными листьями и желтоватыми, сильно пахнущими цветами, часто высаживаемое в парках и на аллеях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "липа",
                                "target_lang_sense_clarification": "дерево"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04817748-e7d0-3e04-a224-c5f5e29dda0f",
                    "sense_definition": "Napar przygotowany z suszonych kwiatów drzewa lipowego, stosowany często jako domowy środek na przeziębienie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food_drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy jestem chory, mama zawsze parzy mi gorącą <w>lipę</w> z dodatkiem miodu i cytryny.",
                            "target_lang_translations": {
                                "ru": "Когда я болею, мама всегда заваривает мне горячую <w>липу</w> с медом и лимоном."
                            }
                        },
                        {
                            "text": "Wieczorem lubię wypić kubek <w>lipy</w>, ponieważ działa ona uspokajająco i pomaga zasnąć.",
                            "target_lang_translations": {
                                "ru": "Вечером я люблю выпить чашку <w>липы</w>, так как она действует успокаивающе и помогает уснуть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "herbatka z lipy",
                        "pić lipę",
                        "napar z lipy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настой, приготовленный из сушеных цветков липового дерева, часто используемый как домашнее средство от простуды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "липовый чай",
                                "target_lang_sense_clarification": "напиток"
                            },
                            {
                                "target_lang_word": "настой липы",
                                "target_lang_sense_clarification": "лекарственное средство"
                            },
                            {
                                "target_lang_word": "липа",
                                "target_lang_sense_clarification": "разг. о напитке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2af4cccf-2b61-37c8-acae-c19a46130967",
                    "sense_definition": "Nieprawda, oszustwo lub zmyślona historia, w którą trudno uwierzyć.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "colloquial_lie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wierz w to, co on mówi, to jest zwykła <w>lipa</w> wymyślona na poczekaniu.",
                            "target_lang_translations": {
                                "ru": "Не верь тому, что он говорит, это обычная <w>липа</w>, придуманная на ходу."
                            }
                        },
                        {
                            "text": "Cała ta historia o jego wielkiej wygranej na loterii to była straszna <w>lipa</w>.",
                            "target_lang_translations": {
                                "ru": "Вся эта история о его крупном выигрыше в лотерею была сплошная <w>липа</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bujda",
                        "kit",
                        "nieprawda",
                        "kłamstwo"
                    ],
                    "antonyms": [
                        "prawda"
                    ],
                    "common_phrases": [
                        "to jakaś lipa",
                        "wciskać lipę",
                        "straszna lipa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неправда, обман или выдуманная история, в которую трудно поверить."

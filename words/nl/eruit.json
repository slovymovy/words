{
    "word_family": [
        "uit",
        "hieruit",
        "daaruit",
        "waaruit"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ba736deb-904c-3206-bab1-12d2729a6fea",
                    "sense_definition": "Vanuit de binnenkant van iets dat eerder genoemd is naar buiten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction_pronominal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De doos was zwaar, dus ik haalde de boeken <w>eruit</w>.",
                            "target_lang_translations": {
                                "en": "The box was heavy, so I took the books <w>out</w>."
                            }
                        },
                        {
                            "text": "Hij viel in het diepe water en kon <w>eruit</w> klimmen via de ladder.",
                            "target_lang_translations": {
                                "en": "He fell into the deep water and was able to climb <w>out</w> via the ladder."
                            }
                        }
                    ],
                    "synonyms": [
                        "naar buiten"
                    ],
                    "antonyms": [
                        "erin"
                    ],
                    "common_phrases": [
                        "eruit halen",
                        "eruit vallen",
                        "eruit gaan"
                    ],
                    "target_lang_definitions": {
                        "en": "From the inside of something previously mentioned to the outside."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "out of it",
                                "target_lang_sense_clarification": "referring to a specific source"
                            },
                            {
                                "target_lang_word": "out",
                                "target_lang_sense_clarification": "adverbial direction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e170ddce-0e11-3af3-8527-f5fd88d126a6",
                    "sense_definition": "Een dwingende oproep aan iemand om een ruimte direct te verlaten.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "imperative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Eruit</w>! Ik wil je hier nooit meer zien.",
                            "target_lang_translations": {
                                "en": "<w>Get out</w>! I never want to see you here again."
                            }
                        },
                        {
                            "text": "De bewaker wees naar de deur en riep luid: <w>eruit</w>!",
                            "target_lang_translations": {
                                "en": "The guard pointed to the door and shouted loudly: <w>get out</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "wegwezen",
                        "buiten"
                    ],
                    "antonyms": [
                        "binnen"
                    ],
                    "common_phrases": [
                        "eruit met jou"
                    ],
                    "target_lang_definitions": {
                        "en": "A forceful command for someone to leave a space immediately."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Get out",
                                "target_lang_sense_clarification": "imperative command"
                            },
                            {
                                "target_lang_word": "Out",
                                "target_lang_sense_clarification": "short command"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd6da4d3-32cc-3c65-af35-5143a40ebd80",
                    "sense_definition": "Gebruikt als onderdeel van een werkwoord om een beweging naar buiten, een oplossing of een uiterlijk aan te geven.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "separable_verb_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij ziet <w>eruit</w> alsof ze net terug is van vakantie.",
                            "target_lang_translations": {
                                "en": "She <w>looks</w> as if she just returned from vacation."
                            }
                        },
                        {
                            "text": "We hebben lang gepraat en we hopen dat we <w>eruit</w> komen.",
                            "target_lang_translations": {
                                "en": "We talked for a long time and we hope we can work it <w>out</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "eruit zien",
                        "eruit komen",
                        "eruit springen",
                        "eruit flappen"
                    ],
                    "target_lang_definitions": {
                        "en": "Used as part of a verb to indicate outward movement, a solution, or appearance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "out",
                                "target_lang_sense_clarification": "part of phrasal verbs"
                            },
                            {
                                "target_lang_word": "out of it",
                                "target_lang_sense_clarification": "figurative"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
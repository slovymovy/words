                {
                    "sense_id": "ffc522af-804a-3a3c-971b-899dae4beff2",
                    "sense_definition": "Kosten die betaald zijn voor het verkrijgen van officiële informatie of documenten.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De advocaat specificeerde het <w>uitschot</w> op de eindafrekening voor de cliënt.",
                            "target_lang_translations": {

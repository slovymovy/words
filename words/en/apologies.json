    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f32e796b-b333-3d0e-8dc1-70bc47c52f62",
                    "sense_definition": "The plural form of <w>apology</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "expression_of_regret",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please accept my <w>apologies</w> for the delay in responding to your email.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, примите мои <w>извинения</w> за задержку с ответом на ваше письмо."
                            }
                        },
                        {
                            "text": "He offered his sincere <w>apologies</w> for the misunderstanding.",
                            "target_lang_translations": {
                                "ru": "Он принес свои искренние <w>извинения</w> за недоразумение."
                            }
                        }
                    ],
                    "synonyms": [
                        "regrets",
                        "excuses"
                    ],
                    "common_phrases": [
                        "accept my apologies",
                        "sincere apologies",
                        "offer apologies"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова \"извинение\"; выражение сожаления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извинения",
                                "target_lang_sense_clarification": "выражение сожаления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3656547c-ef67-329f-a42b-0d8853341f19",
                    "sense_definition": "A formal notification that someone is unable to attend a meeting or event.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "meeting_absence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chairman read out the <w>apologies</w> from the absent board members.",
                            "target_lang_translations": {
                                "ru": "Председатель зачитал <w>извинения</w> отсутствующих членов совета."
                            }
                        },
                        {
                            "text": "I cannot attend the conference, so I will send my <w>apologies</w> to the organizer.",
                            "target_lang_translations": {
                                "ru": "Я не смогу присутствовать на конференции, поэтому отправлю организатору свои <w>извинения</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "regrets"
                    ],
                    "common_phrases": [
                        "send apologies",
                        "apologies for absence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное уведомление о том, что кто-то не сможет присутствовать на встрече или мероприятии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извинения",
                                "target_lang_sense_clarification": "за отсутствие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "a4ce6c2e-23bd-3b83-bbeb-e5a7647c25fc",
                    "sense_definition": "A polite expression used to say sorry for a mistake or inconvenience.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "expression_of_regret",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Apologies</w>, I did not mean to interrupt your conversation.",
                            "target_lang_translations": {
                                "ru": "<w>Прошу прощения</w>, я не хотел прерывать ваш разговор."
                            }
                        },
                        {
                            "text": "My <w>apologies</w> for the confusion caused by the schedule change.",
                            "target_lang_translations": {
                                "ru": "Приношу свои <w>извинения</w> за путаницу, вызванную изменением расписания."
                            }
                        }
                    ],
                    "synonyms": [
                        "sorry",
                        "pardon"
                    ],
                    "common_phrases": [
                        "my apologies"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливое выражение, используемое для того, чтобы попросить прощения за ошибку или неудобство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прошу прощения",
                                "target_lang_sense_clarification": "вежливая фраза"
                            },
                            {
                                "target_lang_word": "мои извинения",
                                "target_lang_sense_clarification": "краткая форма"
                            },
                            {
                                "target_lang_word": "извините",
                                "target_lang_sense_clarification": "общеупотребительное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

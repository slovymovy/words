    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "97729c44-fd70-3ca9-9757-81f584320bb6",
                    "sense_definition": "Rozpocząć wykonywanie jakiejś czynności lub działania (użycie w stylu podniosłym lub książkowym).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "start_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starzec usiadł przy kominku i <począł> opowiadać historię swojego życia.",
                            "target_lang_translations": {
                                "ru": "Старик сел у камина и <начал> рассказывать историю своей жизни."
                            }
                        },
                        {
                            "text": "Gdy tylko wzeszło słońce, ptaki <poczęły> głośno śpiewać.",
                            "target_lang_translations": {
                                "ru": "Как только взошло солнце, птицы <стали> громко петь."
                            }
                        }
                    ],
                    "synonyms": [
                        "zacząć",
                        "rozpocząć"
                    ],
                    "common_phrases": [
                        "począć mówić",
                        "począć płakać",
                        "począć działać"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "styl książkowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать выполнение какого-либо действия (употребляется в возвышенном или книжном стиле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начать",
                                "target_lang_sense_clarification": "приступить к действию"
                            },
                            {
                                "target_lang_word": "стать",
                                "target_lang_sense_clarification": "начать делать что-либо (часто в повествовании)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3fafcdb-3bfc-32de-9313-546f5e8223ec",
                    "sense_definition": "Doprowadzić do zapłodnienia i powstania nowego życia.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młode małżeństwo od dłuższego czasu stara się <począć> dziecko.",
                            "target_lang_translations": {
                                "ru": "Молодая пара уже долгое время пытается <зачать> ребенка."
                            }
                        },
                        {
                            "text": "Według legendy bohater został <poczęty> w magicznych okolicznościach.",
                            "target_lang_translations": {
                                "ru": "Согласно легенде, герой был <зачат> при магических обстоятельствах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "począć dziecko",
                        "począć syna",
                        "począć życie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести к оплодотворению и возникновению новой жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачать",
                                "target_lang_sense_clarification": "дать начало новой жизни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adf216c1-0a3a-3aab-8f49-02cc1daec25a",
                    "sense_definition": "O istocie żywej: zacząć istnieć w łonie matki w chwili zapłodnienia (używane z zaimkiem 'się').",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "biology_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy człowiek <począł> się z połączenia dwóch komórek rodzicielskich.",
                            "target_lang_translations": {
                                "ru": "Каждый человек <зародился> из соединения двух родительских клеток."
                            }
                        },
                        {
                            "text": "Rodzice często wspominają moment, w którym ich dziecko <poczęło> się.",
                            "target_lang_translations": {
                                "ru": "Родители часто вспоминают момент, когда их ребенок <был зачат>."
                            }
                        }
                    ],
                    "synonyms": [
                        "powstać"
                    ],
                    "common_phrases": [
                        "począć się z miłości"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О живом существе: возникнуть в утробе матери в момент оплодотворения (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зародиться",
                                "target_lang_sense_clarification": "возникнуть, появиться"
                            },
                            {
                                "target_lang_word": "зачаться",
                                "target_lang_sense_clarification": "биологический процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "460ce810-004f-3e25-b6ff-25b691fdc158",
                    "sense_definition": "Zacząć istnieć, pojawić się lub wziąć swój początek (używane z zaimkiem 'się').",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "origin_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W jego umyśle <poczęła> się nowa, genialna myśl.",
                            "target_lang_translations": {
                                "ru": "В его уме <зародилась> новая гениальная мысль."
                            }
                        },
                        {
                            "text": "Wielka kłótnia <poczęła> się od jednego nieporozumienia.",
                            "target_lang_translations": {
                                "ru": "Большая ссора <началась> с одного недоразумения."
                            }
                        }
                    ],
                    "synonyms": [
                        "powstać",
                        "narodzić się"
                    ],
                    "common_phrases": [
                        "począć się w głowie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "styl książkowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать существовать, появиться или взять свое начало (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зародиться",
                                "target_lang_sense_clarification": "о мыслях, идеях"
                            },
                            {
                                "target_lang_word": "возникнуть",
                                "target_lang_sense_clarification": "появиться"
                            },
                            {
                                "target_lang_word": "начаться",
                                "target_lang_sense_clarification": "о событиях"
                            }
                        ]
                    }
                }
            ]
        }

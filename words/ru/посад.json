                },
                {
                    "sense_id": "36897497-b9bb-3ee3-b774-959356716753",
                    "sense_definition": "Пригород, предместье или посёлок городского типа.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "57ef42ba-4b28-316a-b06e-3c51cb931d6a",
                    "sense_definition": "To allow liquid or gas to escape through a hole or crack.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_leak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old pipe began to <w>leak</w> water all over the kitchen floor.",
                            "target_lang_translations": {
                                "ru": "Старая труба начала <w>протекать</w>, заливая водой весь пол на кухне."
                            }
                        },
                        {
                            "text": "We realized that the car was <w>leaking</w> oil onto the driveway.",
                            "target_lang_translations": {
                                "ru": "Мы поняли, что из машины <w>подтекает</w> масло на подъездную дорожку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leak oil",
                        "leak water",
                        "leak gas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пропускать жидкость или газ через отверстие или трещину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протекать",
                                "target_lang_sense_clarification": "о трубе, крыше или емкости"
                            },
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "пропускать воду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50c824ae-e7c1-389f-9fbf-9bf33d7c079e",
                    "sense_definition": "(of a liquid or gas) To escape from a container through a hole or crack.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_leak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Water <w>leaked</w> from the ceiling during the heavy rainstorm yesterday.",

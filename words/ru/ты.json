    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "45262e26-9b41-34fb-8440-080162dd4284",
                    "sense_definition": "Личное местоимение второго лица единственного числа. Употребляется при обращении к одному лицу (обычно к близкому человеку, родственнику, другу или ребёнку) или к животному.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "address_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Привет, как <ты> себя чувствуешь сегодня утром?"
                        },
                        {
                            "text": "Я очень надеюсь, что <ты> сможешь прийти на мой день рождения."
                        },
                        {
                            "text": "Где <ты> был вчера вечером после работы?"
                        }
                    ],
                    "antonyms": [
                        "вы",
                        "я"
                    ],
                    "common_phrases": [
                        "как ты",
                        "где ты",
                        "кто ты",
                        "это ты",
                        "ты и я"
                    ]
                },
                {
                    "sense_id": "9b0a5b99-724b-3d2f-9438-3171db2f8f21",
                    "sense_definition": "Употребляется в обобщённом значении «любой человек», «всякий» при описании ситуаций, применимых к каждому.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "address_generic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Никогда не знаешь, где <ты> найдёшь своё настоящее счастье."
                        },
                        {
                            "text": "Когда <ты> живёшь в большом городе, времени всегда не хватает."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в разговорной речи для обобщения"
                        }
                    ]
                },
                {
                    "sense_id": "7834156b-1868-344e-90ae-d1614d09f033",
                    "sense_definition": "Употребляется при обращении к Богу, святым, а также в высоком стиле при риторическом обращении к стране, городу, душе или абстрактным понятиям.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "address_solemn",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "О Русь, <ты> велика и необъятна в своих бескрайних просторах."
                        },
                        {
                            "text": "Господи, на всё воля Твоя, и <Ты> всё видишь."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "характерно для поэтического или религиозного стиля"
                        }
                    ]
                }
            ]
        }
    ]
}
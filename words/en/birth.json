    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a3ee17e0-fa63-3968-9ee4-9a20a9de434d",
                    "sense_definition": "The process of a baby or young animal coming out of its mother's body; the beginning of a life.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>birth</w> of their first child was the happiest moment of their lives.",
                            "target_lang_translations": {
                                "ru": "<w>Рождение</w> их первого ребенка было самым счастливым моментом в их жизни."
                            }
                        },
                        {
                            "text": "Please write your full name and date of <w>birth</w> on the application form.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, напишите свое полное имя и дату <w>рождения</w> в анкете."
                            }
                        }
                    ],
                    "antonyms": [
                        "death"
                    ],
                    "common_phrases": [
                        "give birth",
                        "date of birth",
                        "at birth",
                        "blind from birth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс появления ребенка или детеныша из тела матери; начало жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рождение",
                                "target_lang_sense_clarification": "появление на свет"
                            },
                            {
                                "target_lang_word": "роды",
                                "target_lang_sense_clarification": "медицинский процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9fb66f8-f0a2-319c-ba94-b513cc7de09e",
                    "sense_definition": "The beginning, start, or origin of something non-living, such as an idea, organization, or era.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Historians have written many books about the <w>birth</w> of the Roman Empire.",
                            "target_lang_translations": {
                                "ru": "Историки написали много книг о <w>зарождении</w> Римской империи."
                            }
                        },
                        {
                            "text": "The <w>birth</w> of the internet completely changed how people communicate with each other.",
                            "target_lang_translations": {
                                "ru": "<w>Рождение</w> интернета полностью изменило то, как люди общаются друг с другом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "birth of a nation",
                        "birth of an idea",
                        "birth of an era"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начало, возникновение или исток чего-либо неживого, например идеи, организации или эпохи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарождение",
                                "target_lang_sense_clarification": "начальная стадия развития"
                            },
                            {
                                "target_lang_word": "возникновение",
                                "target_lang_sense_clarification": "появление"
                            },
                            {
                                "target_lang_word": "рождение",
                                "target_lang_sense_clarification": "метафорическое начало"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d02ee1a-8d97-3729-b8df-acf3cda85cbf",
                    "sense_definition": "A person's family background, ancestry, or social standing inherited from their parents.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Despite being of noble <w>birth</w>, he chose to live a simple life among the common people.",
                            "target_lang_translations": {
                                "ru": "Несмотря на благородное <w>происхождение</w>, он предпочел жить простой жизнью среди обычных людей."
                            }
                        },
                        {
                            "text": "She is French by <w>birth</w>, but she has lived in the United States for twenty years.",
                            "target_lang_translations": {
                                "ru": "Она француженка по <w>рождению</w>, но живет в Соединенных Штатах уже двадцать лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "of noble birth",
                        "country of birth",
                        "British by birth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Семейное происхождение человека, родословная или социальное положение, унаследованное от родителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "происхождение",
                                "target_lang_sense_clarification": "родословная, принадлежность к роду"
                            },
                            {
                                "target_lang_word": "рождение",
                                "target_lang_sense_clarification": "в контексте обстоятельств появления на свет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61afd70a-3955-399e-87db-aa2a40a66213",
                    "sense_definition": "That which is born; an offspring or creation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poet described his poems as rare <w>births</w> of his imagination.",
                            "target_lang_translations": {
                                "ru": "Поэт описывал свои стихи как редкие <w>порождения</w> своего воображения."
                            }
                        },
                        {
                            "text": "The animal tends to the <w>birth</w> until it is strong enough to survive alone.",
                            "target_lang_translations": {
                                "ru": "Животное заботится о <w>детеныше</w>, пока он не станет достаточно сильным, чтобы выжить самостоятельно."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly found in older literature"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что рождено; потомство или творение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порождение",
                                "target_lang_sense_clarification": "результат творчества или процесса"
                            },
                            {
                                "target_lang_word": "потомство",
                                "target_lang_sense_clarification": "детеныши"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a4193898-f01e-35d2-a3bb-eeb7664ca827",
                    "sense_definition": "To deliver a baby or young animal from the body.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer stayed in the barn all night while the cow <w>birthed</w> her calf.",
                            "target_lang_translations": {
                                "ru": "Фермер оставался в сарае всю ночь, пока корова <w>рожала</w> теленка."
                            }
                        },
                        {
                            "text": "She chose to <w>birth</w> her baby at home with the assistance of a certified midwife.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>рожать</w> ребенка дома с помощью сертифицированной акушерки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "birth a baby",
                        "birth a calf"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить на свет ребенка или детеныша."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рожать",
                                "target_lang_sense_clarification": "производить на свет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ae737b0-02a7-35a5-b0fd-a44e12a33e6e",
                    "sense_definition": "To bring something into existence; to originate or create something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chaotic political situation <w>birthed</w> a new movement for social justice.",
                            "target_lang_translations": {
                                "ru": "Хаотичная политическая ситуация <w>породила</w> новое движение за социальную справедливость."
                            }
                        },
                        {
                            "text": "Years of research and experimentation finally <w>birthed</w> a cure for the disease.",
                            "target_lang_translations": {
                                "ru": "Годы исследований и экспериментов наконец <w>породили</w> лекарство от этой болезни."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать начало чему-либо; создавать или быть источником чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порождать",
                                "target_lang_sense_clarification": "быть причиной возникновения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cb0973a5-a002-350e-96dd-ccd2a3122b16",
                    "sense_definition": "Related to a person's biological parents rather than adoptive parents.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to search for her <w>birth</w> mother after she turned eighteen.",
                            "target_lang_translations": {
                                "ru": "Она решила разыскать свою <w>биологическую</w> мать после того, как ей исполнилось восемнадцать."
                            }
                        },
                        {
                            "text": "The adopted child had no contact with his <w>birth</w> family while growing up.",
                            "target_lang_translations": {
                                "ru": "У приемного ребенка не было контактов с его <w>родной</w> семьей, пока он рос."
                            }
                        }
                    ],
                    "synonyms": [
                        "biological",
                        "blood"
                    ],
                    "common_phrases": [
                        "birth mother",
                        "birth father",
                        "birth parents"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к биологическим родителям человека, а не к приемным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "биологический",
                                "target_lang_sense_clarification": "родной по крови"
                            },
                            {
                                "target_lang_word": "родной",
                                "target_lang_sense_clarification": "связанный кровным родством"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ffa03eb9-5e7a-3384-8f21-9f4c3814e0d4",
                    "sense_definition": "Свойство того, что имеет большое значение, вес или влияние.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "meaning_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы все понимаем огромную <w>важность</w> этого события для страны.",
                            "target_lang_translations": {
                                "en": "We all understand the huge <w>importance</w> of this event for the country."
                            }
                        },
                        {
                            "text": "Врач объяснил пациенту <w>важность</w> правильного питания.",
                            "target_lang_translations": {
                                "en": "The doctor explained to the patient the <w>importance</w> of proper nutrition."
                            }
                        }
                    ],
                    "synonyms": [
                        "значимость",
                        "значение",
                        "значительность"
                    ],
                    "antonyms": [
                        "незначительность",
                        "пустяк"
                    ],
                    "common_phrases": [
                        "особая важность",
                        "придавать важность",
                        "иметь важность",
                        "первостепенная важность"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of having great value, weight, or influence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "importance",
                                "target_lang_sense_clarification": "general significance or value"
                            },
                            {
                                "target_lang_word": "significance",
                                "target_lang_sense_clarification": "meaning or consequence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79b71db3-1c28-3ed0-9057-c76b9560feb4",
                    "sense_definition": "Гордый, величественный вид или поведение; напыщенность.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он говорил с такой <w>важностью</w>, словно был директором всего мира.",
                            "target_lang_translations": {
                                "en": "He spoke with such <w>self-importance</w>, as if he were the director of the whole world."
                            }
                        },
                        {
                            "text": "Кот прошёл мимо нас с комичной <w>важностью</w> и лёг на диван.",
                            "target_lang_translations": {
                                "en": "The cat walked past us with comical <w>self-importance</w> and lay on the sofa."
                            }
                        }
                    ],
                    "synonyms": [
                        "напыщенность",
                        "величавость"
                    ],
                    "antonyms": [
                        "скромность",
                        "простота"
                    ],
                    "common_phrases": [
                        "напустить на себя важность",
                        "с видом важности"
                    ],
                    "target_lang_definitions": {
                        "en": "A proud, majestic appearance or behavior; pomposity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "self-importance",
                                "target_lang_sense_clarification": "exaggerated sense of one's own value"
                            },
                            {
                                "target_lang_word": "pomposity",
                                "target_lang_sense_clarification": "pretentious behavior"
                            },
                            {
                                "target_lang_word": "airs",
                                "target_lang_sense_clarification": "affected manner"
                            }
                        ]
                    }
                }
            ]
        }

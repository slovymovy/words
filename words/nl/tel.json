                    "sense_id": "3e828003-40ca-3e07-9369-a5fa00980b2d",
                    "sense_definition": "Een zeer korte tijdsduur; een ogenblik of seconde.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wacht even een <w>tel</w> voordat je weggaat.",
                            "target_lang_translations": {
                                "ru": "Подожди <w>секунду</w>, прежде чем уйти.",
                                "en": "Wait a <w>second</w> before you leave."
                            }
                        },
                        {
                            "text": "In een <w>tel</w> was de vogel gevlogen.",
                            "target_lang_translations": {
                                "ru": "В один <w>миг</w> птица улетела.",
                                "en": "In a <w>moment</w> the bird had flown."
                            }
                        }
                    ],
                    "synonyms": [
                        "seconde",
                        "ogenblik",
                        "moment"
                    ],
                    "antonyms": [
                        "eeuwigheid"
                    ],
                    "common_phrases": [
                        "wacht een tel",
                        "in een tel",
                        "geen tel later"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень короткий промежуток времени; мгновение или секунда.",
                        "en": "A very short period of time; a moment or second."
                    },
                    "translations": {

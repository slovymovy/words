            "senses": [
                {
                    "sense_id": "5a063609-8b13-3319-8c2c-a6b56314ed0f",
                    "sense_definition": "Предоставлять или делать что-либо полезное для кого-то (помощь, услугу, поддержку).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "provide_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи скорой помощи должны немедленно <оказывать> помощь пострадавшим.",
                            "target_lang_translations": {
                                "en": "Ambulance doctors must immediately <render> aid to the victims."
                            }
                        },
                        {
                            "text": "Наша компания <оказывает> широкий спектр юридических услуг населению.",
                            "target_lang_translations": {
                                "en": "Our company <provides> a wide range of legal services to the public."
                            }
                        }
                    ],
                    "synonyms": [
                        "осуществлять",
                        "осуществлять"
                    ],
                    "common_phrases": [
                        "оказывать помощь",
                        "оказывать поддержку",
                        "оказывать услуги",
                        "оказывать содействие"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide or do something useful for someone (help, service, support)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "provide",
                                "target_lang_sense_clarification": "services, support"
                            },
                            {
                                "target_lang_word": "render",
                                "target_lang_sense_clarification": "formal; aid, assistance"
                            },
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "help, support"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "291c865c-4835-3f76-bcbb-ba531ce05f34",
                    "sense_definition": "Воздействовать на кого-либо или что-либо, производя определённый эффект (влияние, давление).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "exert_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Климат может <оказывать> значительное влияние на экономическое развитие региона.",
                            "target_lang_translations": {
                                "en": "The climate can <exert> a significant influence on the region's economic development."
                            }
                        },
                        {
                            "text": "Никто не имеет права <оказывать> давление на присяжных заседателей.",
                            "target_lang_translations": {
                                "en": "No one has the right to <exert> pressure on the jurors."
                            }
                        }
                    ],
                    "synonyms": [
                        "осуществлять",
                        "осуществлять"
                    ],
                    "common_phrases": [
                        "оказывать влияние",
                        "оказывать давление",
                        "оказывать воздействие",
                        "оказывать эффект"
                    ],
                    "target_lang_definitions": {
                        "en": "To affect someone or something, producing a certain effect (influence, pressure)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exert",
                                "target_lang_sense_clarification": "pressure, influence"
                            },
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "an effect, an influence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e8d333b-5939-35fc-b01c-ed14772c31db",
                    "sense_definition": "Совершать активные действия в ответ на нападение или принуждение (обычно сопротивление).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "counter_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При задержании преступник пытался <оказывать> активное сопротивление полиции.",
                            "target_lang_translations": {
                                "en": "During the arrest, the criminal tried to <put up> active resistance to the police."
                            }
                        },
                        {
                            "text": "Войска противника перестали <оказывать> сопротивление и начали отступать.",
                            "target_lang_translations": {
                                "en": "The enemy troops ceased to <offer> resistance and began to retreat."
                            }
                        }
                    ],
                    "common_phrases": [
                        "оказывать сопротивление",
                        "оказывать неповиновение"
                    ],
                    "target_lang_definitions": {
                        "en": "To take active action in response to an attack or coercion (usually resistance)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put up",
                                "target_lang_sense_clarification": "resistance"
                            },
                            {
                                "target_lang_word": "offer",
                                "target_lang_sense_clarification": "formal; resistance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d384b6f-f5ac-34a0-8626-d7a81b9e0752",
                    "sense_definition": "Проявлять определённое отношение, чувства или знаки внимания к кому-либо (уважение, почести).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "show_attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодые люди должны <оказывать> уважение старшему поколению.",
                            "target_lang_translations": {
                                "en": "Young people should <show> respect to the older generation."
                            }
                        },
                        {
                            "text": "Хозяин дома любил <оказывать> особые знаки внимания своим гостям.",
                            "target_lang_translations": {
                                "en": "The host loved to <pay> special attention to his guests."
                            }
                        }
                    ],
                    "synonyms": [
                        "осуществлять",
                        "осуществлять"
                    ],
                    "common_phrases": [
                        "оказывать уважение",
                        "оказывать честь",
                        "оказывать внимание",
                        "оказывать приём"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {

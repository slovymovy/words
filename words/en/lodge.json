    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a2dbd59d-f87f-3ade-9254-b5fa8eb0bc68",
                    "sense_definition": "A small house in the country, used especially by people on holiday or taking part in sports, or a hotel in a rural area.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We rented a wooden <w>lodge</w> in the mountains for our winter vacation.",
                            "target_lang_translations": {
                                "ru": "Мы арендовали деревянный <w>домик</w> в горах для нашего зимнего отпуска."
                            }
                        },
                        {
                            "text": "The hunting <w>lodge</w> was decorated with antlers and rustic furniture.",
                            "target_lang_translations": {
                                "ru": "<w>Охотничий домик</w> был украшен оленьими рогами и грубой мебелью."
                            }
                        }
                    ],
                    "synonyms": [
                        "sleep over",
                        "stay over",
                        "stop over",
                        "tarry",
                        "sojourn",
                        "quarter",
                        "stay",
                        "stop",
                        "keep",
                        "stop off"
                    ],
                    "common_phrases": [
                        "ski lodge",
                        "hunting lodge",
                        "mountain lodge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой загородный дом, используемый для отдыха или занятий спортом, либо отель в сельской местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до́мик",
                                "target_lang_sense_clarification": "небольшое строение для отдыха"
                            },
                            {
                                "target_lang_word": "охо́тничий до́мик",
                                "target_lang_sense_clarification": "для охоты"
                            },
                            {
                                "target_lang_word": "котте́дж",
                                "target_lang_sense_clarification": "современный загородный дом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ee13e98-e9b7-3de5-8e34-983bf67c3552",
                    "sense_definition": "A shelter constructed by a beaver on a pond or lake, typically made of mud and sticks.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_animal_home",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The beaver swam back to its <w>lodge</w> carrying a fresh branch.",
                            "target_lang_translations": {
                                "ru": "Бобр поплыл обратно в свою <w>хатку</w>, неся свежую ветку."
                            }
                        },
                        {
                            "text": "You can see the top of the beaver <w>lodge</w> sticking out of the water.",
                            "target_lang_translations": {
                                "ru": "Вы можете видеть верхушку бобровой <w>хатки</w>, торчащую из воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "stop",
                        "keep",
                        "quarter",
                        "stay",
                        "stop off",
                        "stop over",
                        "tarry"
                    ],
                    "common_phrases": [
                        "beaver lodge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жилище бобра, построенное из веток и ила на пруду или озере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ха́тка",
                                "target_lang_sense_clarification": "жилище бобра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc533e9a-629c-36d3-8a31-178eaaf15753",
                    "sense_definition": "A small house or room at the entrance of a large estate, park, or college, often used by a porter or caretaker.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Visitors must report to the porter's <w>lodge</w> before entering the university grounds.",
                            "target_lang_translations": {
                                "ru": "Посетители должны отметиться в <w>сторожке</w> привратника перед входом на территорию университета."
                            }
                        },
                        {
                            "text": "He collected his mail from the gate <w>lodge</w> on his way out.",
                            "target_lang_translations": {
                                "ru": "Он забрал свою почту из <w>сторожки</w> у ворот по пути наружу."
                            }
                        }
                    ],
                    "synonyms": [
                        "stop",
                        "keep",
                        "quarter",
                        "stay",
                        "stop off",
                        "stop over",
                        "tarry"
                    ],
                    "common_phrases": [
                        "porter's lodge",
                        "gate lodge"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK, Canada"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой дом или помещение у ворот усадьбы, парка или колледжа, часто для привратника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сторо́жка",
                                "target_lang_sense_clarification": "помещение для сторожа"
                            },
                            {
                                "target_lang_word": "до́мик привратника",
                                "target_lang_sense_clarification": "у ворот"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d7b893a-42c2-35a5-a0a0-58047fa32762",
                    "sense_definition": "A traditional home of some indigenous North American peoples, such as a tipi or wigwam.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tribe gathered inside the ceremonial <w>lodge</w> for the council meeting.",
                            "target_lang_translations": {
                                "ru": "Племя собралось внутри церемониального <w>вигвама</w> для совета."
                            }
                        },
                        {
                            "text": "Smoke rose slowly from the center of the wooden <w>lodge</w>.",
                            "target_lang_translations": {
                                "ru": "Дым медленно поднимался из центра деревянного <w>жилища</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stop",
                        "keep",
                        "quarter",
                        "stay",
                        "stop off",
                        "stop over",
                        "tarry"
                    ],
                    "common_phrases": [
                        "sweat lodge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционное жилище некоторых коренных народов Северной Америки (например, вигвам или типи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вигва́м",
                                "target_lang_sense_clarification": "традиционное жилище"

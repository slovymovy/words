    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c7a31e0-c4d8-3d53-994c-c9a8bf4c7978",
                    "sense_definition": "Разговорное выражение, обозначающее безнадёжную ситуацию, конец, провал или гибель.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если мы не успеем сдать проект вовремя, нам всем <хана>.",
                            "target_lang_translations": {
                                "en": "If we don't manage to submit the project on time, we are all <done for>."
                            }
                        },
                        {
                            "text": "После такой серьёзной поломки этому старому компьютеру точно <хана>.",
                            "target_lang_translations": {
                                "en": "After such a serious breakdown, this old computer is definitely <toast>."
                            }
                        }
                    ],
                    "synonyms": [
                        "каюк",
                        "капут",
                        "кранты",
                        "крышка",
                        "кирдык",
                        "капец",
                        "пипец"
                    ],
                    "common_phrases": [
                        "тебе хана",
                        "полная хана",
                        "всем хана"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial expression denoting a hopeless situation, the end, failure, or death."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "done for",
                                "target_lang_sense_clarification": "in a hopeless situation or doomed"
                            },
                            {
                                "target_lang_word": "toast",
                                "target_lang_sense_clarification": "slang for ruined, broken, or in trouble"
                            },
                            {
                                "target_lang_word": "the end",
                                "target_lang_sense_clarification": "final ruin or death"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "639ac19c-71fe-3444-8ac0-753fefff43e2",
                    "sense_definition": "Форма родительного или винительного падежа единственного числа существительного <w>хан</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Власть великого <хана> распространялась на огромные территории.",
                            "target_lang_translations": {
                                "en": "The power of the great <Khan> extended over vast territories."
                            }
                        },
                        {
                            "text": "Послы с уважением приветствовали <хана> и преподнесли ему дары.",
                            "target_lang_translations": {
                                "en": "The ambassadors greeted the <Khan> with respect and presented him with gifts."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive or accusative case form of the noun 'khan'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Khan",
                                "target_lang_sense_clarification": "title of a ruler in Central Asia"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f113a97-8ce6-3f6e-bf9a-af316f4fe361",
                    "sense_definition": "Постоялый двор или гостиница в странах Ближнего Востока и Средней Азии.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Уставшие путники остановились на ночлег в старой каменной <хане>.",
                            "target_lang_translations": {
                                "en": "The tired travelers stopped for the night at an old stone <khan>."
                            }
                        },
                        {
                            "text": "Во дворе <ханы> было шумно от множества торговцев и верблюдов.",
                            "target_lang_translations": {
                                "en": "In the courtyard of the <khan>, it was noisy with many merchants and camels."
                            }
                        }
                    ],
                    "synonyms": [

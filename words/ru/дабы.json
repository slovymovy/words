            "senses": [
                {
                    "sense_id": "b8157ab8-1673-36c7-816a-adcfc2f2c110",
                    "sense_definition": "С той целью, чтобы; для того, чтобы.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "purpose_conjunction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он говорил медленно и чётко, <w>дабы</w> каждое его слово было понятно слушателям.",
                            "target_lang_translations": {
                                "en": "He spoke slowly and clearly <w>so that</w> every word of his would be understood by the listeners."
                            }
                        },
                        {
                            "text": "Мы вышли из дома пораньше, <w>дабы</w> не опоздать на утренний поезд.",
                            "target_lang_translations": {
                                "en": "We left the house early <w>so as</w> not to be late for the morning train."
                            }
                        }
                    ],
                    "synonyms": [
                        "чтобы"
                    ],
                    "common_phrases": [
                        "дабы не",
                        "дабы избежать"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "устар."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "With the purpose that; in order to (often formal or archaic)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "so that",
                                "target_lang_sense_clarification": "indicating purpose"
                            },
                            {
                                "target_lang_word": "in order to",
                                "target_lang_sense_clarification": "formal expression of purpose"
                            },
                            {
                                "target_lang_word": "lest",
                                "target_lang_sense_clarification": "used in negative contexts (дабы не)"
                            }
                        ]
                    }
                }
            ]
        },
        {

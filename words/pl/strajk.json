            "senses": [
                {
                    "sense_id": "6089b6a6-2dea-3861-9d7e-811428a6116e",
                    "sense_definition": "Dobrowolny i zbiorowy protest pracowników, polegający najczęściej na przerwaniu pracy w celu wymuszenia na pracodawcy spełnienia określonych postulatów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Związkowcy zapowiedzieli bezterminowy <w>strajk</w> w obronie miejsc pracy.",
                            "target_lang_translations": {
                                "ru": "Члены профсоюза объявили бессрочную <w>забастовку</w> в защиту рабочих мест."
                            }
                        },
                        {
                            "text": "<w>Strajki</w> kolejarzy sparaliżowały cały kraj.",
                            "target_lang_translations": {
                                "ru": "<w>Забастовки</w> железнодорожников парализовали всю страну."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strajk głodowy",
                        "strajk generalny",
                        "strajk włoski",
                        "ogłosić strajk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добровольный коллективный протест работников, обычно выражающийся в прекращении работы с целью добиться от работодателя выполнения определённых требований."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забастовка",
                                "target_lang_sense_clarification": "протест работников"
                            },
                            {
                                "target_lang_word": "стачка",
                                "target_lang_sense_clarification": "организованное прекращение работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8bece1e-b643-3614-a42b-36f8c5b6a641",
                    "sense_definition": "W kręglach: zbicie wszystkich kręgli za pierwszym rzutem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Udało mu się rzucić <w>strajk</w> już w pierwszej rundzie.",
                            "target_lang_translations": {
                                "ru": "Ему удалось выбить <w>страйк</w> уже в первом раунде."
                            }
                        },
                        {
                            "text": "Za każdy <w>strajk</w> gracz otrzymuje premię punktową.",
                            "target_lang_translations": {
                                "ru": "За каждый <w>страйк</w> игрок получает бонусные очки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzucić strajk"
                    ],
                    "target_lang_definitions": {
                        "ru": "В боулинге: ситуация, когда все кегли сбиты первым броском."
                    },
                    "translations": {

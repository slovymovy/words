{
    "word_family": [
        "magia",
        "mag",
        "magicznie"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "555e1d4f-b959-39ca-a214-fdbf9f55bf3a",
                    "sense_definition": "Związany z magią, czarami i zjawiskami nadprzyrodzonymi.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literal_magic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starej baśni zła królowa posiadała <w>magiczne</w> lustro, które mówiło prawdę.",
                            "target_lang_translations": {
                                "ru": "В старой сказке у злой королевы было <w>волшебное</w> зеркало, которое говорило правду."
                            }
                        },
                        {
                            "text": "Czarodziej wypowiedział <w>magiczne</w> zaklęcie i zamienił kamień w złoto.",
                            "target_lang_translations": {
                                "ru": "Волшебник произнес <w>магическое</w> заклинание и превратил камень в золото."
                            }
                        }
                    ],
                    "synonyms": [
                        "czarodziejski"
                    ],
                    "antonyms": [
                        "niemagiczny"
                    ],
                    "common_phrases": [
                        "magiczna różdżka",
                        "magiczne zaklęcie",
                        "magiczna moc",
                        "magiczny świat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с магией, колдовством и сверхъестественными явлениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магический",
                                "target_lang_sense_clarification": "относящийся к магии"
                            },
                            {
                                "target_lang_word": "волшебный",
                                "target_lang_sense_clarification": "обладающий силой волшебства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aab660b0-c62e-35c5-b2c7-6d913e044b69",
                    "sense_definition": "Mający niezwykły, zachwycający urok, wywołujący silne pozytywne emocje.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_charm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To był <w>magiczny</w> wieczór, którego nigdy nie zapomnę.",
                            "target_lang_translations": {
                                "ru": "Это был <w>волшебный</w> вечер, который я никогда не забуду."
                            }
                        },
                        {
                            "text": "Zachód słońca nad morzem stworzył niesamowitą, wręcz <w>magiczną</w> atmosferę.",
                            "target_lang_translations": {
                                "ru": "Закат над морем создал удивительную, прямо-таки <w>магическую</w> атмосферу."
                            }
                        }
                    ],
                    "synonyms": [
                        "czarujący",
                        "niezwykły",
                        "bajkowy",
                        "zachwycający"
                    ],
                    "antonyms": [
                        "zwyczajny",
                        "pospolity",
                        "przeciętny"
                    ],
                    "common_phrases": [
                        "magiczna chwila",
                        "magiczny wieczór",
                        "magiczna atmosfera",
                        "magiczny dotyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий необыкновенным очарованием, вызывающий сильные положительные эмоции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волшебный",
                                "target_lang_sense_clarification": "чарующий, прекрасный"
                            },
                            {
                                "target_lang_word": "магический",
                                "target_lang_sense_clarification": "завораживающий, таинственный"
                            },
                            {
                                "target_lang_word": "сказочный",
                                "target_lang_sense_clarification": "невероятный, как в сказке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4901a2fb-5b6a-34a8-a6cc-2e462ea4f439",
                    "sense_definition": "Oparty na wierze w tajemnicze związki przyczynowe, niezgodny z racjonalnym myśleniem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "irrational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myślenie <w>magiczne</w> jest charakterystyczne dla wczesnego etapu rozwoju dziecka.",
                            "target_lang_translations": {
                                "ru": "<w>Магическое</w> мышление характерно для раннего этапа развития ребенка."
                            }
                        },
                        {
                            "text": "Nie możemy liczyć na <w>magiczne</w> rozwiązanie tego skomplikowanego problemu ekonomicznego.",
                            "target_lang_translations": {
                                "ru": "Мы не можем рассчитывать на <w>магическое</w> решение этой сложной экономической проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "irracjonalny"
                    ],
                    "antonyms": [
                        "racjonalny",
                        "logiczny",
                        "naukowy"
                    ],
                    "common_phrases": [
                        "myślenie magiczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основанный на вере в таинственные причинно-следственные связи, противоречащий рациональному мышлению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магический",
                                "target_lang_sense_clarification": "иррациональный, в контексте мышления"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
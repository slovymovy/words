                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>arm</w> of his coat was torn during the hike.",
                            "target_lang_translations": {
                                "ru": "<w>Рукав</w> его пальто порвался во время похода."
                            }
                        },
                        {
                            "text": "She touched the <w>arm</w> of his shirt to get his attention.",
                            "target_lang_translations": {
                                "ru": "Она коснулась <w>рукава</w> его рубашки, чтобы привлечь его внимание."
                            }
                        }
                    ],
                    "synonyms": [
                        "sleeve"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть одежды, закрывающая руку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рукав",
                                "target_lang_sense_clarification": "часть одежды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30dd07cf-225e-376b-afc6-8646dafcfe5d",

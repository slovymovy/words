            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ef05b363-2984-3177-b2c4-489b926cf66a",
                    "sense_definition": "Wiedzieć, co coś znaczy; pojmować treść wypowiedzi, tekstu lub istotę sytuacji za pomocą umysłu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cognitive_understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety nie <rozumiem>, co do mnie mówisz po francusku.",
                            "target_lang_translations": {
                                "ru": "К сожалению, я не <понимаю>, что ты мне говоришь по-французски."
                            }
                        },
                        {
                            "text": "Czy uczniowie <rozumieją> ten trudny temat z matematyki?",
                            "target_lang_translations": {
                                "ru": "<Понимают> ли ученики эту сложную тему по математике?"
                            }
                        },
                        {
                            "text": "Dopiero teraz <rozumiem>, jak bardzo się myliłem.",
                            "target_lang_translations": {
                                "ru": "Только сейчас я <понимаю>, как сильно я ошибался."
                            }
                        }
                    ],
                    "synonyms": [
                        "pojmować",
                        "kojarzyć",
                        "zdawać sobie sprawę",
                        "chwytać",
                        "łapać",
                        "kumać",
                        "kapować",
                        "czaić"
                    ],
                    "common_phrases": [
                        "dać do zrozumienia",
                        "źle zrozumieć",
                        "rozumieć bez słów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знать, что что-то значит; постигать смысл высказывания, текста или суть ситуации с помощью разума."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "постигать смысл"
                            },
                            {
                                "target_lang_word": "осознавать",
                                "target_lang_sense_clarification": "отдавать себе отчет"
                            },
                            {
                                "target_lang_word": "соображать",
                                "target_lang_sense_clarification": "разг. понимать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bc80406-4b11-3979-9361-5ef8831e8e7b",
                    "sense_definition": "Odczuwać empatię wobec kogoś; akceptować czyjeś powody, uczucia lub zachowanie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotional_understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doskonale <rozumiem> twój smutek po stracie przyjaciela.",
                            "target_lang_translations": {
                                "ru": "Я прекрасно <понимаю> твою грусть после потери друга."
                            }
                        },
                        {
                            "text": "Rodzice starają się, ale nie zawsze <rozumieją> problemy swoich dzieci.",
                            "target_lang_translations": {
                                "ru": "Родители стараются, но не всегда <понимают> проблемы своих детей."
                            }
                        }
                    ],
                    "synonyms": [
                        "wczuwać się"
                    ],
                    "common_phrases": [
                        "w pełni rozumieć",
                        "rozumieć czyjś ból"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявлять эмпатию к кому-либо; принимать чьи-то мотивы, чувства или поведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "сочувствовать"
                            },
                            {
                                "target_lang_word": "входить в положение",
                                "target_lang_sense_clarification": "проявлять эмпатию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65f4ed4e-a70e-3f39-9a7d-39fe8f884cc9",
                    "sense_definition": "Mieć z kimś dobre relacje, podobne poglądy i łatwość porozumiewania się (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional_understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z moim bratem <rozumiemy> się bez zbędnych słów.",
                            "target_lang_translations": {
                                "ru": "Мы с братом <понимаем> друг друга без лишних слов."
                            }
                        },
                        {
                            "text": "Oni <rozumieją> się nawzajem jak nikt inny w tej firmie.",
                            "target_lang_translations": {
                                "ru": "Они <понимают> друг друга как никто другой в этой фирме."
                            }
                        }
                    ],
                    "synonyms": [
                        "znać się"
                    ],
                    "common_phrases": [
                        "rozumieć się bez słów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь с кем-то хорошие отношения, схожие взгляды и легкость в общении (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понимать друг друга",
                                "target_lang_sense_clarification": "находить общий язык"
                            },
                            {
                                "target_lang_word": "ладить",
                                "target_lang_sense_clarification": "иметь хорошие отношения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58d6f25f-d6ce-3d27-8f46-3fab6e18dc50",
                    "sense_definition": "Interpretować fakty, słowa lub zdarzenia w określony sposób; nadawać czemuś konkretne znaczenie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cognitive_understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jak mam <rozumieć> twoje nagłe milczenie?",
                            "target_lang_translations": {
                                "ru": "Как мне <понимать> твое внезапное молчание?"
                            }
                        },
                        {
                            "text": "Ten przepis można <rozumieć> na dwa różne sposoby.",
                            "target_lang_translations": {
                                "ru": "Это правило можно <понимать> двояко."
                            }
                        }
                    ],
                    "synonyms": [

                    ],
                    "common_phrases": [
                        "полная луна",
                        "свет луны",
                        "под луной",
                        "выть на луну"
                    ],
                    "target_lang_definitions": {
                        "en": "The natural satellite of the Earth, visible in the sky as a glowing disk or crescent, shining with reflected sunlight."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "moon",
                                "target_lang_sense_clarification": "Earth's natural satellite"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f98c14aa-9d0a-3c58-9a16-b2815e1bcfb6",
                    "sense_definition": "Nadziemna kondygnacja budynku znajdująca się bezpośrednio nad parterem lub nad innym piętrem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "architecture_levels",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi dziadkowie mieszkają na drugim <w>piętrze</w> w bloku bez windy.",
                            "target_lang_translations": {
                                "ru": "Мои дедушка и бабушка живут на третьем <w>этаже</w> в доме без лифта."
                            }
                        },
                        {
                            "text": "Biuro obsługi klienta znajduje się na pierwszym <w>piętrze</w>.",
                            "target_lang_translations": {
                                "ru": "Центр обслуживания клиентов находится на втором <w>этаже</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Śląsk Cieszyński) sztok"
                    ],
                    "antonyms": [
                        "parter"
                    ],
                    "common_phrases": [
                        "pierwsze piętro",
                        "mieszkać na piętrze",
                        "wejść na piętro",
                        "ostatnie piętro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уровень здания, расположенный над первым этажом (партером) или над другим этажом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этаж",
                                "target_lang_sense_clarification": "часть здания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bdba9be-b97d-36cf-abea-c24be5968a98",
                    "sense_definition": "Strefa występowania określonych gatunków roślin w górach, zmieniająca się wraz z wysokością nad poziomem morza.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "nature_zones",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Tatrach wyraźnie widać, jak zmienia się <w>piętro</w> roślinne wraz ze wzrostem wysokości.",
                            "target_lang_translations": {
                                "ru": "В Татрах отчетливо видно, как меняется растительный <w>пояс</w> с увеличением высоты."
                            }
                        },
                        {
                            "text": "Kosodrzewina tworzy zwarte <w>piętro</w> powyżej górnej granicy lasu.",
                            "target_lang_translations": {
                                "ru": "Горная сосна образует сплошной <w>пояс</w> выше верхней границы леса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piętro roślinne",
                        "piętro kosodrzewiny",
                        "piętro halne",
                        "piętro reglowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высотная зона растительности в горах, меняющаяся в зависимости от высоты над уровнем моря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пояс",
                                "target_lang_sense_clarification": "растительный"
                            },
                            {
                                "target_lang_word": "зона",
                                "target_lang_sense_clarification": "высотная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0af0159-02ce-3c01-801c-6b917fb99d55",
                    "sense_definition": "Warstwa lasu tworzona przez korony drzew lub roślinność znajdującą się na podobnej wysokości.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nature_zones",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele gatunków ptaków zakłada gniazda w najwyższym <w>piętrze</w> lasu.",
                            "target_lang_translations": {
                                "ru": "Многие виды птиц вьют гнезда в верхнем <w>ярусе</w> леса."
                            }
                        },
                        {
                            "text": "Gęste <w>piętro</w> koron drzew ogranicza dostęp światła do runa leśnego.",
                            "target_lang_translations": {
                                "ru": "Густой <w>ярус</w> крон деревьев ограничивает доступ света к лесной подстилке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piętro drzew",
                        "piętro koron",
                        "dolne piętro lasu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой леса, образованный кронами деревьев или растительностью на определенной высоте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ярус",
                                "target_lang_sense_clarification": "леса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "642c140b-8b7b-3354-ab14-4b3d5da5c9c3",
                    "sense_definition": "Jednostka podziału dziejów Ziemi w geologii, będąca częścią oddziału.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geology_mining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>piętro</w> geologiczne charakteryzuje się występowaniem specyficznych skamieniałości.",
                            "target_lang_translations": {
                                "ru": "Этот геологический <w>ярус</w> характеризуется наличием специфических окаменелостей."
                            }
                        },
                        {
                            "text": "Badania stratygraficzne pozwoliły precyzyjnie określić wiek tego <w>piętra</w>.",
                            "target_lang_translations": {
                                "ru": "Стратиграфические исследования позволили точно определить возраст этого <w>яруса</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piętro geologiczne",
                        "piętro stratygraficzne"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Геологическое подразделение, часть отдела, объединяющая слои горных пород, образовавшиеся в течение одного века."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ярус",
                                "target_lang_sense_clarification": "геологический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c454931c-aaa0-32c7-ab1f-b3fcf1d74803",
                    "sense_definition": "Część złoża w kopalni wydzielona w pionie, przygotowana do eksploatacji.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geology_mining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Górnicy zjechali windą na najniższe <w>piętro</w> wydobywcze.",
                            "target_lang_translations": {
                                "ru": "Шахтеры спустились на лифте на самый нижний добычной <w>горизонт</w>."
                            }
                        },
                        {
                            "text": "Eksploatacja tego <w>piętra</w> kopalni została zakończona w ubiegłym roku.",
                            "target_lang_translations": {
                                "ru": "Разработка этого <w>горизонта</w> шахты была завершена в прошлом году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piętro wydobywcze",
                        "poziom wydobywczy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть месторождения в шахте, выделенная по вертикали и подготовленная к разработке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горизонт",
                                "target_lang_sense_clarification": "в шахте"
                            },
                            {
                                "target_lang_word": "этаж",
                                "target_lang_sense_clarification": "горное дело"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ed116be8-f898-3074-ad40-f62bef9dcec3",
                    "sense_definition": "Stevig dichtgeknepen zodat de vingers naar binnen gebogen zijn, meestal verwijzend naar handen of vuisten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij sloeg met zijn <w>gebalde</w> vuist hard op de tafel.",
                            "target_lang_translations": {
                                "en": "He hit the table hard with his <w>clenched</w> fist.",
                                "ru": "Он сильно ударил <w>сжатым</w> кулаком по столу."
                            }
                        },
                        {
                            "text": "Ze stond met <w>gebalde</w> handen te wachten op het oordeel.",
                            "target_lang_translations": {
                                "en": "She stood waiting for the verdict with <w>clenched</w> hands.",
                                "ru": "Она стояла со <w>сжатыми</w> руками, ожидая приговора."
                            }
                        }
                    ],
                    "synonyms": [
                        "samengebald"
                    ],
                    "common_phrases": [
                        "gebalde vuist",
                        "met gebalde vuisten"
                    ],
                    "target_lang_definitions": {
                        "en": "Tightly squeezed shut so that the fingers are bent inward, usually referring to hands or fists.",
                        "ru": "Крепко сжатый, так что пальцы загнуты внутрь; обычно относится к рукам или кулакам."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clenched",
                                "target_lang_sense_clarification": "tightly closed, usually referring to fists"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сжатый",
                                "target_lang_sense_clarification": "о кулаке или руке"
                            }
                        ]
                    }
                }
            ]
        },

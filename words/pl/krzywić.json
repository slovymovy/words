{
    "word_family": [
        "krzywy",
        "krzywa",
        "krzywizna",
        "skrzywić",
        "wykrzywić",
        "zakrzywić",
        "pokrzywić"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5991dba2-1b70-3649-9aa0-4dcc2e8d5b82",
                    "sense_definition": "Robić grymas twarzy wyrażający niezadowolenie, ból, niesmak lub dezaprobatę (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "facial_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent zaczął się <w>krzywić</w>, gdy pielęgniarka dotknęła jego rany.",
                            "target_lang_translations": {
                                "ru": "Пациент начал <w>морщиться</w>, когда медсестра коснулась его раны."
                            }
                        },
                        {
                            "text": "Nie powinieneś się <w>krzywić</w> na widok jedzenia, które przygotowała babcia.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <w>кривиться</w> при виде еды, которую приготовила бабушка."
                            }
                        }
                    ],
                    "synonyms": [
                        "deformować się"
                    ],
                    "antonyms": [
                        "prostować się",
                        "prostować"
                    ],
                    "common_phrases": [
                        "krzywić-się z bólu",
                        "krzywić-się z niesmakiem",
                        "krzywić-się na widok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать гримасу, выражающую недовольство, боль или отвращение (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морщиться",
                                "target_lang_sense_clarification": "от боли или недовольства"
                            },
                            {
                                "target_lang_word": "кривиться",
                                "target_lang_sense_clarification": "выражать отвращение или пренебрежение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "136dac4f-c29c-3fef-9df6-d9bc54c2d75d",
                    "sense_definition": "Zmieniać kształt czegoś prostego na wygięty lub nierówny; powodować deformację fizyczną.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_deformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wilgoć zaczęła powoli <w>krzywić</w> drewniane deski na tarasie.",
                            "target_lang_translations": {
                                "ru": "Влага начала медленно <w>коробить</w> деревянные доски на террасе."
                            }
                        },
                        {
                            "text": "Ciągłe noszenie ciężkiego plecaka na jednym ramieniu może <w>krzywić</w> kręgosłup.",
                            "target_lang_translations": {
                                "ru": "Постоянное ношение тяжелого рюкзака на одном плече может <w>искривлять</w> позвоночник."
                            }
                        }
                    ],
                    "synonyms": [
                        "deformować",
                        "wyginać się"
                    ],
                    "antonyms": [
                        "prostować"
                    ],
                    "common_phrases": [
                        "krzywić kręgosłup",
                        "krzywić plecy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменять форму чего-либо прямого на изогнутую или неровную; вызывать физическую деформацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искривлять",
                                "target_lang_sense_clarification": "делать кривым, деформировать"
                            },
                            {
                                "target_lang_word": "коробить",
                                "target_lang_sense_clarification": "деформировать (обычно о дереве от влаги)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4582266f-b32b-389b-9a40-83877e439fa2",
                    "sense_definition": "Wykrzywiać usta lub twarz, tworząc nienaturalny wyraz, zwykle drwiący lub płaczliwy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "facial_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe dziecko zaczęło <w>krzywić</w> buzię w podkówkę, szykując się do płaczu.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок начал <w>кривить</w> ротик подковкой, готовясь заплакать."
                            }
                        },
                        {
                            "text": "Starzec lubił złośliwie <w>krzywić</w> usta, gdy słyszał głupie pytania.",
                            "target_lang_translations": {
                                "ru": "Старик любил злобно <w>кривить</w> губы, когда слышал глупые вопросы."
                            }
                        }
                    ],
                    "synonyms": [
                        "deformować"
                    ],
                    "common_phrases": [
                        "krzywić usta",
                        "krzywić twarz",
                        "krzywić minę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искажать рот или черты лица, создавая неестественное выражение, обычно насмешливое или плаксивое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кривить",
                                "target_lang_sense_clarification": "рот, губы или лицо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fe1a387-73d9-3d18-81b9-9bba64b5b244",
                    "sense_definition": "Tracić prosty kształt, ulegać wygięciu lub zniekształceniu (o przedmiotach; używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_deformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod wpływem wysokiej temperatury plastikowa obudowa zaczęła się <w>krzywić</w>.",
                            "target_lang_translations": {
                                "ru": "Под воздействием высокой температуры пластиковый корпус начал <w>искривляться</w>."
                            }
                        },
                        {
                            "text": "Stare ramy okienne mogą się <w>krzywić</w>, przez co okna stają się nieszczelne.",
                            "target_lang_translations": {
                                "ru": "Старые оконные рамы могут <w>коробиться</w>, из-за чего окна становятся негерметичными."
                            }
                        }
                    ],
                    "synonyms": [
                        "deformować się",
                        "wyginać się"
                    ],
                    "antonyms": [
                        "prostować się"
                    ],
                    "common_phrases": [
                        "deski się krzywią",
                        "obraz się krzywi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Терять прямую форму, изгибаться или деформироваться (о предметах; используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искривляться",
                                "target_lang_sense_clarification": "терять правильную форму"
                            },
                            {
                                "target_lang_word": "коробиться",
                                "target_lang_sense_clarification": "деформироваться от влаги или температуры"
                            },
                            {
                                "target_lang_word": "деформироваться",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "1a7f7732-b5c3-3a10-84f5-670798ca0645",
                    "sense_definition": "Verwijzend naar het bereiken van de leeftijd van acht jaar.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "ordinal_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kreeg mijn eerste fiets toen ik op mijn <w>achtste</w> jarig was.",
                            "target_lang_translations": {

                {
                    "sense_id": "282f127f-7eb4-3c7a-b29a-5523cc999207",
                    "sense_definition": "Ophouden met een handeling, beweging of activiteit; niet langer doorgaan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cease_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>stoppen</w> meestal om vijf uur met werken.",
                            "target_lang_translations": {
                                "en": "We usually <w>stop</w> working at five o'clock.",
                                "ru": "Мы обычно <w>заканчиваем</w> работать в пять часов."
                            }
                        },
                        {
                            "text": "Het heeft de hele dag geregend, maar nu is het <w>gestopt</w>.",
                            "target_lang_translations": {
                                "en": "It rained all day, but now it has <w>stopped</w>.",
                                "ru": "Весь день шел дождь, но сейчас он <w>прекратился</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ophouden",
                        "beëindigen"
                    ],
                    "antonyms": [
                        "beginnen",
                        "starten",
                        "aanvangen"
                    ],
                    "common_phrases": [
                        "stoppen met roken",
                        "plotseling stoppen",
                        "ermee stoppen"
                    ],
                    "target_lang_definitions": {
                        "en": "To cease an action, movement, or activity; to no longer continue.",
                        "ru": "Перестать совершать действие или движение; больше не продолжать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "general term for ceasing an action"
                            },
                            {
                                "target_lang_word": "quit",
                                "target_lang_sense_clarification": "often used for habits or jobs"
                            },
                            {
                                "target_lang_word": "cease",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "переставать",
                                "target_lang_sense_clarification": "общее значение прекращения действия"
                            },
                            {
                                "target_lang_word": "прекращать",

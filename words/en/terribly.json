    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e02018c0-36cc-34bd-9ebb-c9412e89c01f",
                    "sense_definition": "Very; extremely; used to emphasize the degree of something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>terribly</w> busy right now and cannot answer the phone.",
                            "target_lang_translations": {
                                "ru": "Он сейчас <w>ужасно</w> занят и не может ответить на звонок."
                            }
                        },
                        {
                            "text": "I am <w>terribly</w> sorry for arriving so late to the meeting.",
                            "target_lang_translations": {
                                "ru": "Мне <w>ужасно</w> жаль, что я так опоздал на встречу."
                            }
                        }
                    ],
                    "synonyms": [
                        "extremely",
                        "very",
                        "awfully"
                    ],
                    "antonyms": [
                        "slightly",
                        "somewhat"
                    ],
                    "common_phrases": [
                        "terribly sorry",
                        "terribly wrong",
                        "terribly important",
                        "terribly busy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень; чрезвычайно; используется для усиления степени чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ужасно",
                                "target_lang_sense_clarification": "разг. очень, в высшей степени"
                            },
                            {
                                "target_lang_word": "страшно",
                                "target_lang_sense_clarification": "разг. очень сильно"
                            },
                            {
                                "target_lang_word": "жутко",
                                "target_lang_sense_clarification": "разг. чрезвычайно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1e30b7e-62a6-3d0a-99ca-bcb37abadf3c",
                    "sense_definition": "In a very bad, unpleasant, or poor manner.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She practiced for weeks, but she still sang <w>terribly</w>.",
                            "target_lang_translations": {
                                "ru": "Она тренировалась неделями, но всё равно пела <w>ужасно</w>."
                            }
                        },
                        {
                            "text": "The team played <w>terribly</w> and lost the match by five goals.",
                            "target_lang_translations": {
                                "ru": "Команда сыграла <w>ужасно</w> и проиграла матч с разницей в пять голов."
                            }
                        }
                    ],
                    "synonyms": [
                        "horribly",
                        "awfully",
                        "very",
                        "extremely",
                        "grisly"
                    ],
                    "antonyms": [
                        "well",
                        "superbly",
                        "wonderfully"
                    ],
                    "common_phrases": [
                        "suffer terribly",
                        "miss terribly",
                        "go terribly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень плохо, скверно или неприятно."

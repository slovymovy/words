            "senses": [
                {
                    "sense_id": "e82bdbc7-65bf-3057-9928-d45f87db8439",
                    "sense_definition": "Создавать что-либо новое (план, проект, технологию, программу) в процессе тщательной работы и исследований.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша компания <w>разрабатывает</w> новое приложение для изучения языков.",
                            "target_lang_translations": {
                                "en": "Our company is <w>developing</w> a new app for learning languages."
                            }
                        },
                        {
                            "text": "Ученые <w>разрабатывают</w> эффективное лекарство от гриппа.",
                            "target_lang_translations": {
                                "en": "Scientists are <w>developing</w> an effective cure for the flu."
                            }
                        },
                        {
                            "text": "Правительство <w>разрабатывает</w> план по улучшению экономики страны.",
                            "target_lang_translations": {
                                "en": "The government is <w>working out</w> a plan to improve the country's economy."
                            }
                        }
                    ],
                    "synonyms": [
                        "создавать",
                        "проектировать",
                        "придумывать",
                        "конструировать"
                    ],
                    "antonyms": [
                        "уничтожать"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aede3e8a-6e05-3649-a100-f231e2399681",
                    "sense_definition": "Jedna z dwóch równych części, na które podzielono całość.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "part_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jacek zjadł jedną <w>połowę</w> słodkiego placka, a ja zjadłem drugą.",
                            "target_lang_translations": {
                                "ru": "Яцек съел одну <w>половину</w> сладкого пирога, а я съел вторую."
                            }
                        },
                        {
                            "text": "Musisz oddać mi <w>połowę</w> pieniędzy, które wczoraj wspólnie zarobiliśmy.",
                            "target_lang_translations": {
                                "ru": "Ты должен вернуть мне <w>половину</w> денег, которые мы вчера вместе заработали."
                            }
                        }
                    ],
                    "synonyms": [
                        "pół",
                        "połówka"
                    ],
                    "common_phrases": [
                        "pierwsza połowa",
                        "druga połowa",
                        "połowa ceny",
                        "połowa sukcesu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из двух равных частей, на которые делится целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "половина",
                                "target_lang_sense_clarification": "одна из двух равных частей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81b20f52-1de6-3d78-90bc-80d7a7905983",
                    "sense_definition": "Moment lub miejsce znajdujące się w równej odległości od początku i końca czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zatrzymaliśmy się w <w>połowie</w> drogi, żeby odpocząć i zjeść obiad.",
                            "target_lang_translations": {
                                "ru": "Мы остановились на <w>середине</w> пути, чтобы отдохнуть и пообедать."
                            }
                        },
                        {
                            "text": "Jestem dopiero w <w>połowie</w> czytania tej książki, ale bardzo mi się podoba.",
                            "target_lang_translations": {
                                "ru": "Я пока только на <w>середине</w> этой книги, но она мне очень нравится."
                            }
                        }
                    ],
                    "synonyms": [
                        "środek"
                    ],
                    "common_phrases": [
                        "w połowie drogi",
                        "w połowie czasu",
                        "w połowie miesiąca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Момент или место, находящееся на равном расстоянии от начала и конца чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "середина",
                                "target_lang_sense_clarification": "центр, средняя часть"
                            },
                            {
                                "target_lang_word": "половина",
                                "target_lang_sense_clarification": "середина (обычно о времени)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b99d1af6-353c-33dd-9783-83a41cb1bb94",
                    "sense_definition": "Znaczna, bliżej nieokreślona część jakiejś zbiorowości lub całości, niekoniecznie równa dokładnie 50%.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "part_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chyba <w>połowa</w> mieszkańców naszego miasta przyszła na ten darmowy koncert.",
                            "target_lang_translations": {
                                "ru": "Кажется, <w>половина</w> жителей нашего города пришла на этот бесплатный концерт."
                            }
                        },
                        {
                            "text": "Mój brat zwiedził już <w>połowę</w> świata, ale wciąż chce podróżować.",
                            "target_lang_translations": {
                                "ru": "Мой брат объездил уже <w>половину</w> мира, но всё ещё хочет путешествовать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "połowa ludzi",
                        "połowa miasta",
                        "połowa świata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Значительная, неопределенная часть какой-либо группы или целого, не обязательно ровно 50%."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "половина",
                                "target_lang_sense_clarification": "значительная часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d29b8c9e-5a6a-3242-ba11-918a9c71e2bd",
                    "sense_definition": "Potoczne lub żartobliwe określenie męża, żony lub partnera życiowego.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja lepsza <w>połowa</w> została dzisiaj w domu z chorymi dziećmi.",
                            "target_lang_translations": {
                                "ru": "Моя лучшая <w>половина</w> осталась сегодня дома с больными детьми."
                            }
                        },
                        {
                            "text": "Czy przyjdziesz na przyjęcie urodzinowe sam, czy ze swoją drugą <w>połową</w>?",
                            "target_lang_translations": {
                                "ru": "Ты придешь на день рождения один или со своей второй <w>половиной</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "połowica"
                    ],
                    "common_phrases": [
                        "druga połowa",
                        "lepsza połowa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często używane w wyrażeniu 'druga połowa' lub 'lepsza połowa'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное или шутливое название мужа, жены или спутника жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "половина",
                                "target_lang_sense_clarification": "супруг или супруга"
                            }
                        ]
                    }
                }
            ]
        }
    ]

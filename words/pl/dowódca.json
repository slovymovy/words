    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0a0565e3-4b4a-3a1a-b371-1a35917581aa",
                    "sense_definition": "Osoba kierująca grupą wojskowych lub oddziałem zbrojnym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "military_leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wojsko czeka tylko na rozkaz <w>dowódcy</w>.",
                            "target_lang_translations": {
                                "ru": "Войско ждет только приказа <w>командира</w>."
                            }
                        },
                        {
                            "text": "Podczas wojny mój dziadek był <w>dowódcą</w> klucza.",
                            "target_lang_translations": {
                                "ru": "Во время войны мой дедушка был <w>командиром</w> звена."
                            }
                        }
                    ],
                    "synonyms": [
                        "komendant",
                        "wódz",
                        "głównodowodzący"
                    ],
                    "antonyms": [
                        "szeregowy"
                    ],
                    "common_phrases": [
                        "naczelny dowódca",
                        "rozkaz dowódcy",
                        "mianować dowódcą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, командующее воинским подразделением или вооруженным отрядом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "командир",
                                "target_lang_sense_clarification": "военный"
                            },
                            {
                                "target_lang_word": "военачальник",
                                "target_lang_sense_clarification": "высокого ранга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6bda5d3-3809-3342-a1b2-72e82db93ea5",
                    "sense_definition": "Osoba stojąca na czele grupy ludzi wykonującej określone zadanie lub pełniącej służbę (np. w straży pożarnej, policji lub podczas wyprawy).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Dowódca</w> akcji ratunkowej zarządził natychmiastową ewakuację mieszkańców.",
                            "target_lang_translations": {
                                "ru": "<w>Руководитель</w> спасательной операции распорядился о немедленной эвакуации жителей."
                            }
                        },
                        {
                            "text": "Strażacy sprawnie wykonywali wszystkie polecenia swojego <w>dowódcy</w>.",
                            "target_lang_translations": {
                                "ru": "Пожарные четко выполняли все приказы своего <w>командира</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "naczelnik",
                        "komendant",
                        "komendant"
                    ],
                    "common_phrases": [
                        "dowódca akcji",
                        "dowódca zmiany",
                        "dowódca wyprawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, возглавляющее группу людей при выполнении задания или несении службы (например, в пожарной охране, полиции)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "командир",
                                "target_lang_sense_clarification": "отряда, экипажа"
                            },
                            {
                                "target_lang_word": "руководитель",
                                "target_lang_sense_clarification": "операции, акции"
                            },
                            {
                                "target_lang_word": "начальник",
                                "target_lang_sense_clarification": "службы, смены"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9cb05bb-9b89-3e09-afcc-52eb99cbfe4c",
                    "sense_definition": "Przywódca grupy przestępczej lub bandy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "criminal_leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja od dawna ścigała <w>dowódcę</w> tej niebezpiecznej szajki.",
                            "target_lang_translations": {
                                "ru": "Полиция давно разыскивала <w>главаря</w> этой опасной шайки."
                            }
                        },
                        {
                            "text": "Wszyscy członkowie gangu bali się swojego bezwzględnego <w>dowódcy</w>.",
                            "target_lang_translations": {
                                "ru": "Все члены банды боялись своего безжалостного <w>главаря</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wódz",
                        "komendant"
                    ],
                    "common_phrases": [
                        "dowódca bandy",
                        "dowódca gangu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лидер преступной группировки или банды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "главарь",
                                "target_lang_sense_clarification": "банды"
                            },
                            {
                                "target_lang_word": "предводитель",
                                "target_lang_sense_clarification": "шайки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

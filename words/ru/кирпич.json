                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A product pressed into a hard rectangular block (usually tea)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brick",
                                "target_lang_sense_clarification": "compressed block"
                            },
                            {
                                "target_lang_word": "block",
                                "target_lang_sense_clarification": "compressed mass"
                            }
                        ]
                    }
                }
            ]

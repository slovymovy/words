    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f21e857-4757-32df-b2f3-cd981b58b746",
                    "sense_definition": "Uczyć się czegoś bardzo intensywnie na pamięć, często bez głębszego zrozumienia materiału.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "learning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę całą noc <kuć> historię, bo jutro mam ważny egzamin.",
                            "target_lang_translations": {
                                "ru": "Мне нужно всю ночь <зубрить> историю, потому что завтра у меня важный экзамен."
                            }
                        },
                        {
                            "text": "Wielu studentów woli <kuć> na blachę niż zrozumieć trudne zagadnienia.",
                            "target_lang_translations": {
                                "ru": "Многие студенты предпочитают <зубрить> назубок, чем понимать трудные вопросы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakuwać",
                        "wkuwać"
                    ],
                    "common_phrases": [
                        "kuć na blachę",
                        "kuć na pamięć"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Учить что-то очень интенсивно наизусть, часто без глубокого понимания материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зубрить",
                                "target_lang_sense_clarification": "учить наизусть без понимания (разг.)"
                            },
                            {
                                "target_lang_word": "заучивать",
                                "target_lang_sense_clarification": "учить наизусть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "030917c8-49ff-30d3-b247-11e269cd68aa",
                    "sense_definition": "Uderzać młotem w rozgrzany metal, aby nadać mu odpowiedni kształt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "striking_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony kowal potrafi <kuć> piękne ozdoby z żelaza.",
                            "target_lang_translations": {
                                "ru": "Опытный кузнец умеет <ковать> красивые украшения из железа."
                            }
                        },
                        {
                            "text": "Trzeba <kuć> żelazo póki gorące, żeby nie stracić okazji.",
                            "target_lang_translations": {
                                "ru": "Нужно <ковать> железо, пока горячо, чтобы не упустить возможность."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykuwać"
                    ],
                    "common_phrases": [
                        "kuć żelazo",
                        "kuć los"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бить молотом по раскаленному металлу, чтобы придать ему нужную форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ковать",
                                "target_lang_sense_clarification": "обрабатывать металл ковкой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "909ec5d5-e641-36ad-aab7-37f9a71b8d19",
                    "sense_definition": "Robić otwór lub wgłębienie w twardym materiale, takim jak ściana lub kamień, używając dłuta lub innego narzędzia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "striking_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy zaczęli <kuć> ściany, aby wymienić starą instalację elektryczną.",
                            "target_lang_translations": {
                                "ru": "Рабочие начали <долбить> стены, чтобы заменить старую электропроводку."
                            }
                        },
                        {
                            "text": "Rzeźbiarz musiał ostrożnie <kuć> w marmurze, by nie zniszczyć dzieła.",
                            "target_lang_translations": {
                                "ru": "Скульптор должен был осторожно <высекать> из мрамора, чтобы не испортить произведение."
                            }
                        }
                    ],
                    "synonyms": [
                        "dłutować"
                    ],
                    "common_phrases": [
                        "kuć dziurę",
                        "kuć w ścianie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать отверстие или углубление в твердом материале (стене, камне) с помощью инструмента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "долбить",
                                "target_lang_sense_clarification": "делать отверстия ударами"
                            },
                            {
                                "target_lang_word": "штробить",
                                "target_lang_sense_clarification": "делать канавки в стенах (строительное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6b5a239-e6e9-3298-8a7b-f88a5e465329",
                    "sense_definition": "Uderzać dziobem w korę drzewa (o ptakach, np. dzięciołach).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "striking_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słychać było, jak dzięcioł zaczął głośno <kuć> w stary dąb.",
                            "target_lang_translations": {
                                "ru": "Было слышно, как дятел начал громко <долбить> старый дуб."
                            }
                        },
                        {
                            "text": "Ptaki te potrafią <kuć> w drewnie z dużą siłą w poszukiwaniu owadów.",
                            "target_lang_translations": {
                                "ru": "Эти птицы могут <долбить> дерево с большой силой в поисках насекомых."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziobać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бить клювом по коре дерева (о птицах, например, дятлах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "долбить",
                                "target_lang_sense_clarification": "стучать клювом"
                            },
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "издавать звук удара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac475f96-0fdd-3b80-a754-f3fdd23cb361",
                    "sense_definition": "Przymocowywać podkowy do kopyt konia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "striking_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kowal przyjechał do stajni, aby <kuć> nasze konie przed sezonem.",
                            "target_lang_translations": {
                                "ru": "Кузнец приехал в конюшню, чтобы <подковать> наших лошадей перед сезоном."
                            }
                        },
                        {
                            "text": "Trzeba regularnie <kuć> konie, które pracują na twardym podłożu.",
                            "target_lang_translations": {
                                "ru": "Нужно регулярно <подковывать> лошадей, которые работают на твердом грунте."
                            }
                        }
                    ],
                    "synonyms": [
                        "podkuwać"
                    ],
                    "common_phrases": [
                        "kuć konia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикреплять подковы к копытам лошади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подковывать",
                                "target_lang_sense_clarification": "ставить подковы"
                            },
                            {
                                "target_lang_word": "ковать",
                                "target_lang_sense_clarification": "подковывать лошадь (устар./проф.)"
                            }
                        ]
                    }
                }
            ]
        },
        {

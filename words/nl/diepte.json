            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cb89cf96-17f3-3506-985c-378a87fa39f6",
                    "sense_definition": "De afstand van de bovenkant tot de bodem of van de voorkant tot de achterkant.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "dimension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>diepte</w> van het zwembad is precies twee meter.",
                            "target_lang_translations": {
                                "en": "The <w>depth</w> of the swimming pool is exactly two meters."
                            }
                        },
                        {
                            "text": "We moeten de <w>diepte</w> van de kast meten voordat we hem kopen.",
                            "target_lang_translations": {
                                "en": "We need to measure the <w>depth</w> of the cupboard before we buy it."
                            }
                        }
                    ],
                    "antonyms": [
                        "ondiepte",
                        "hoogte"
                    ],
                    "common_phrases": [
                        "in de diepte",
                        "de diepte van"
                    ],
                    "target_lang_definitions": {
                        "en": "The distance from the top to the bottom or from the front to the back."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "depth",
                                "target_lang_sense_clarification": "physical measurement of distance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5fc0802-9277-3560-996d-85c7c2969b7b",
                    "sense_definition": "Een laaggelegen plaats of afgrond, vaak ver onder de zeespiegel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen we boven op de berg stonden, zagen we het dorpje in de <w>diepte</w> liggen.",
                            "target_lang_translations": {
                                "en": "When we stood on top of the mountain, we saw the village lying in the <w>depths</w> below."
                            }
                        },
                        {
                            "text": "In de <w>diepten</w> van de oceaan leven veel bijzondere dieren.",
                            "target_lang_translations": {
                                "en": "Many special animals live in the <w>depths</w> of the ocean."
                            }
                        }
                    ],
                    "antonyms": [
                        "hoogte"
                    ],
                    "common_phrases": [
                        "uit de diepte",
                        "in de diepte"
                    ],
                    "target_lang_definitions": {
                        "en": "A low-lying place, abyss, or deep part of a body of water."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "depths",
                                "target_lang_sense_clarification": "a deep place or abyss, often plural"
                            },
                            {
                                "target_lang_word": "abyss",
                                "target_lang_sense_clarification": "a very deep or bottomless chasm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cce4f378-e364-37f0-a59b-f81d2dbe20a1",
                    "sense_definition": "De mate van geestelijke of emotionele inhoud en serieusheid.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het gesprek miste enige <w>diepte</w> en bleef erg oppervlakkig.",
                            "target_lang_translations": {
                                "en": "The conversation lacked any <w>depth</w> and remained very superficial."
                            }
                        },
                        {
                            "text": "De schrijver zoekt in zijn nieuwe roman naar meer psychologische <w>diepte</w>.",
                            "target_lang_translations": {
                                "en": "The writer is looking for more psychological <w>depth</w> in his new novel."
                            }
                        }
                    ],
                    "synonyms": [
                        "diepgang"
                    ],
                    "common_phrases": [
                        "de diepte ingaan"
                    ],
                    "target_lang_definitions": {
                        "en": "The degree of mental or emotional content, complexity, and seriousness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "depth",
                                "target_lang_sense_clarification": "intellectual or emotional intensity"
                            },
                            {
                                "target_lang_word": "profundity",
                                "target_lang_sense_clarification": "great depth of knowledge or thought"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "74be78c6-983d-3329-9d87-89731420586f",
                    "sense_definition": "A series of steps that allow you to go from one level of a building to another.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "staircase_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She ran up the <w>stairs</w> to answer the phone in her bedroom.",
                            "target_lang_translations": {
                                "ru": "Она взбежала по <w>лестнице</w>, чтобы ответить на звонок в спальне."
                            }
                        },
                        {
                            "text": "A long flight of <w>stairs</w> led down to the basement.",
                            "target_lang_translations": {
                                "ru": "Длинная <w>лестница</w> вела вниз в подвал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "flight of stairs",
                        "up the stairs",
                        "down the stairs",
                        "climb the stairs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ряд ступеней, позволяющий переходить с одного уровня здания на другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лестница",
                                "target_lang_sense_clarification": "сооружение"
                            },
                            {
                                "target_lang_word": "ступеньки",
                                "target_lang_sense_clarification": "ряд ступеней"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73785256-d7a7-3e20-8607-a4035b2c4ad8",
                    "sense_definition": "A single step in a staircase.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "staircase_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sat on the bottom <w>stair</w> to tie his shoes before leaving.",
                            "target_lang_translations": {
                                "ru": "Он сел на нижнюю <w>ступеньку</w>, чтобы завязать шнурки перед уходом."
                            }
                        },
                        {
                            "text": "Be careful because the top <w>stair</w> is loose and might break.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, потому что верхняя <w>ступенька</w> шатается и может сломаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "step"
                    ],
                    "common_phrases": [
                        "bottom stair",
                        "top stair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный выступ или перекладина в лестнице."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ступенька",
                                "target_lang_sense_clarification": "отдельный элемент"
                            },
                            {
                                "target_lang_word": "ступень",
                                "target_lang_sense_clarification": "элемент конструкции"
                            }
                        ]
                    }
                }
            ]
        },
        {

                },
                {
                    "sense_id": "1d354a98-497a-3eed-89a5-72c4be8ac6fb",
                    "sense_definition": "Historyczny i geograficzny region w Azji Wschodniej, kolebka starożytnej cywilizacji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Starożytne <w>Chiny</w> dały światu papier, proch i kompas.",
                            "target_lang_translations": {
                                "ru": "Древний <w>Китай</w> дал миру бумагу, порох и компас."
                            }
                        },
                        {
                            "text": "Jedwabny Szlak łączył <w>Chiny</w> z Europą i Bliskim Wschodem.",
                            "target_lang_translations": {
                                "ru": "Шелковый путь соединял <w>Китай</w> с Европой и Ближним Востоком."
                            }
                        }
                    ],
                    "synonyms": [

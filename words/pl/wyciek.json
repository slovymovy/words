                {
                    "sense_id": "549fa1be-bbe9-3cfc-bd63-4e131a15402b",
                    "sense_definition": "Proces niekontrolowanego wydostawania się płynu lub gazu na zewnątrz przez nieszczelność.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_leak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hydraulik szybko naprawił <w>wyciek</w> wody w naszej łazience.",
                            "target_lang_translations": {
                                "ru": "Сантехник быстро устранил <w>течь</w> воды в нашей ванной."
                            }
                        },
                        {
                            "text": "W fabryce nastąpił niebezpieczny <w>wyciek</w> gazu, co spowodowało ewakuację.",
                            "target_lang_translations": {
                                "ru": "На заводе произошла опасная <w>утечка</w> газа, что привело к эвакуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyciekanie",
                        "wypływ",
                        "wycieknięcie"
                    ],
                    "common_phrases": [
                        "wyciek gazu",
                        "wyciek oleju",
                        "wyciek wody",
                        "niekontrolowany wyciek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс неконтролируемого выхода жидкости или газа наружу через неплотность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утечка",
                                "target_lang_sense_clarification": "выход газа или жидкости"
                            },
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "нарушение герметичности (обычно о жидкостях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fba2e178-1545-333f-a556-5f9e73537f07",
                    "sense_definition": "Ujawnienie tajnych lub poufnych informacji osobom niepowołanym.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "info_leak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten ogromny <w>wyciek</w> danych dotknął miliony użytkowników portalu.",
                            "target_lang_translations": {
                                "ru": "Эта огромная <w>утечка</w> данных затронула миллионы пользователей портала."
                            }
                        },
                        {
                            "text": "Firma potwierdziła <w>wyciek</w> informacji o nowym produkcie przed premierą.",
                            "target_lang_translations": {
                                "ru": "Компания подтвердила <w>утечку</w> информации о новом продукте до премьеры."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypłynięcie"
                    ],
                    "common_phrases": [
                        "wyciek danych",
                        "wyciek informacji",
                        "wyciek haseł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскрытие секретной или конфиденциальной информации посторонним лицам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утечка",
                                "target_lang_sense_clarification": "информации или данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cc66d6d-0486-3d5b-8b19-fae8aa083430",
                    "sense_definition": "Płyn lub substancja, która wydostała się na zewnątrz; ślad po takim wydostaniu się.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_leak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na podłodze garażu zauważyłem tłusty <w>wyciek</w> z silnika samochodu.",
                            "target_lang_translations": {
                                "ru": "На полу гаража я заметил жирный <w>подтёк</w> из двигателя автомобиля."
                            }
                        },
                        {
                            "text": "Lekarz dokładnie zbadał ropny <w>wyciek</w> z ucha pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врач тщательно осмотрел гнойные <w>выделения</w> из уха пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydzielina"
                    ],
                    "common_phrases": [
                        "tamować wyciek",
                        "usunąć wyciek",
                        "wyciek ropny"
                    ],
                    "target_lang_definitions": {

                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This issue is <w>outside</w> my area of expertise.",
                            "target_lang_translations": {
                                "ru": "Этот вопрос <w>вне</w> моей компетенции."
                            }

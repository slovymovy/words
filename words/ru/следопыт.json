    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "16625440-5836-3595-90c4-912aa13ac489",
                    "sense_definition": "Человек, который умеет находить людей или животных по оставленным ими следам, обычно в лесу или дикой местности.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "tracker_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный <w>следопыт</w> сразу заметил сломанную ветку у тропы.",
                            "target_lang_translations": {
                                "en": "The experienced <w>tracker</w> immediately noticed a broken branch by the trail."
                            }
                        },
                        {
                            "text": "Даже лучший <w>следопыт</w> не смог бы найти их в этой метели.",
                            "target_lang_translations": {
                                "en": "Even the best <w>tracker</w> could not have found them in this blizzard."
                            }
                        }
                    ],
                    "synonyms": [
                        "охотник"
                    ],
                    "common_phrases": [
                        "опытный следопыт",
                        "индейский следопыт",
                        "навыки следопыта"
                    ],
                    "target_lang_definitions": {
                        "en": "A person skilled in finding people or animals by the tracks they left, usually in a forest or wilderness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tracker",
                                "target_lang_sense_clarification": "someone who follows tracks"
                            },
                            {
                                "target_lang_word": "pathfinder",
                                "target_lang_sense_clarification": "explorer of new routes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41c2885f-c674-32a2-b9c5-36944e359830",
                    "sense_definition": "Энтузиаст, занимающийся поиском мест исторических событий, неизвестных захоронений или документов прошлого.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historian_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый год этот <w>следопыт</w> отправляется в экспедицию на места сражений.",
                            "target_lang_translations": {
                                "en": "Every year this <w>pathfinder</w> goes on an expedition to battle sites."
                            }
                        },
                        {
                            "text": "Молодой <w>следопыт</w> передал найденные документы в местный музей.",
                            "target_lang_translations": {
                                "en": "The young <w>pathfinder</w> handed over the found documents to the local museum."
                            }
                        }
                    ],
                    "synonyms": [
                        "поисковик"
                    ],
                    "common_phrases": [
                        "отряд следопытов",
                        "красные следопыты",
                        "движение следопытов"
                    ],
                    "target_lang_definitions": {
                        "en": "An enthusiast engaged in searching for sites of historical events, unknown graves, or documents of the past."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pathfinder",
                                "target_lang_sense_clarification": "history enthusiast searching for war relics"
                            },
                            {
                                "target_lang_word": "searcher",
                                "target_lang_sense_clarification": "member of a search party"
                            }
                        ]
                    }
                }
            ]
        }
    ]

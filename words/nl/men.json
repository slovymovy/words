{
    "word_family": [
        "mennen",
        "menner"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "fd0152cb-f7a3-39da-afe7-a7585fcd1bd7",
                    "sense_definition": "Verwijst naar personen in het algemeen of naar een onbepaald persoon (iemand, maar niemand in het bijzonder).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Men</w> zegt dat het morgen gaat regenen.",
                            "target_lang_translations": {
                                "ru": "<w>Говорят</w>, что завтра пойдет дождь."
                            }
                        },
                        {
                            "text": "In dit land eet <w>men</w> veel aardappelen.",
                            "target_lang_translations": {
                                "ru": "В этой стране <w>едят</w> много картофеля."
                            }
                        }
                    ],
                    "synonyms": [
                        "ze",
                        "je"
                    ],
                    "common_phrases": [
                        "men zegt",
                        "men neme"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на людей вообще или на неопределенное лицо (кто-то, но никто конкретно)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "люди",
                                "target_lang_sense_clarification": "в общем смысле"
                            },
                            {
                                "target_lang_word": "все",
                                "target_lang_sense_clarification": "обобщающее значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1a37b37d-f85e-39a8-805e-5ca14305a7c3",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>mennen</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "conjugation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>men</w> de paarden veilig naar de stal.",
                            "target_lang_translations": {
                                "ru": "Я <w>веду</w> лошадей в конюшню в целости и сохранности."
                            }
                        },
                        {
                            "text": "Als koetsier <w>men</w> ik dagelijks door de stad.",
                            "target_lang_translations": {
                                "ru": "Как кучер я ежедневно <w>правлю</w> экипажем в городе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "1-е лицо единственного числа настоящего времени от глагола mennen (править лошадьми)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "править",
                                "target_lang_sense_clarification": "лошадьми, повозкой"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "упряжкой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f75febf-e4c6-38c0-927d-12a652c0b97a",
                    "sense_definition": "Gebiedende wijs van <w>mennen</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "conjugation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Men</w> de koets voorzichtig over de hobbelige weg!",
                            "target_lang_translations": {
                                "ru": "<w>Правь</w> каретой осторожно на ухабистой дороге!"
                            }
                        },
                        {
                            "text": "<w>Men</w> die paarden niet te snel, anders gebeuren er ongelukken.",
                            "target_lang_translations": {
                                "ru": "Не <w>гони</w> этих лошадей слишком быстро, иначе случатся несчастья."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола mennen (править лошадьми)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "править",
                                "target_lang_sense_clarification": "лошадьми, повозкой"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "упряжкой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d28a2e4d-a150-39b5-87c9-4157f842e06d",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>mennen</w> (bij inversie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "conjugation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Men</w> je de paarden zelf of laat je dat doen?",
                            "target_lang_translations": {
                                "ru": "Ты <w>правишь</w> лошадьми сам или поручаешь это другим?"
                            }
                        },
                        {
                            "text": "Hoe vaak <w>men</w> je met dit slechte weer?",
                            "target_lang_translations": {
                                "ru": "Как часто ты <w>выезжаешь</w> в такую плохую погоду?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "2-е лицо единственного числа настоящего времени от глагола mennen (при инверсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "править",
                                "target_lang_sense_clarification": "лошадьми, повозкой"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "упряжкой"
                            }
                        ]
                    }
                }
            ]
        }

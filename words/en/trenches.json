                    "frequency": "High",
                    "semantic_group_id": "literal_ditches",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers dug deep <w>trenches</w> to protect themselves from enemy fire.",
                            "target_lang_translations": {
                                "ru": "Солдаты вырыли глубокие <w>окопы</w>, чтобы защититься от вражеского огня."
                            }
                        },
                        {
                            "text": "Farmers created irrigation <w>trenches</w> to water the crops.",
                            "target_lang_translations": {
                                "ru": "Фермеры создали ирригационные <w>траншеи</w> для полива посевов."
                            }
                        }
                    ],
                    "synonyms": [
                        "ditches",
                        "channels",
                        "dugouts"
                    ],
                    "common_phrases": [
                        "dig trenches",
                        "deep trenches"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинные узкие углубления в земле, используемые военными для укрытия или в строительстве и сельском хозяйстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окопы",
                                "target_lang_sense_clarification": "военные укрепления"
                            },
                            {
                                "target_lang_word": "траншеи",
                                "target_lang_sense_clarification": "строительные или ирригационные канавы"
                            }

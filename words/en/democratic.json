                {
                    "sense_id": "43b6e90c-4c06-366e-be92-42da78897c0e",
                    "sense_definition": "Relating to democracy or the principle of government chosen by the people.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "political_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Citizens in a <w>democratic</w> country have the right to choose their leaders.",
                            "target_lang_translations": {
                                "ru": "Граждане <w>демократической</w> страны имеют право выбирать своих лидеров."
                            }
                        },
                        {
                            "text": "Free and fair elections are essential for a <w>democratic</w> government.",
                            "target_lang_translations": {
                                "ru": "Свободные и честные выборы необходимы для <w>демократического</w> правительства."
                            }
                        }
                    ],
                    "antonyms": [
                        "undemocratic",
                        "nondemocratic"
                    ],
                    "common_phrases": [
                        "democratic society",
                        "democratic government",
                        "democratic process",
                        "democratic country",
                        "democratic elections"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к демократии или принципу управления, при котором власть выбирается народом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демократический",
                                "target_lang_sense_clarification": "относящийся к политическому устройству"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ad065e6-f414-3de5-a68c-9be6afc2b41e",

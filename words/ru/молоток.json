            "senses": [
                {
                    "sense_id": "5967985b-88ef-3e03-9f2a-5698ec12a6c3",
                    "sense_definition": "Возглас одобрения или похвалы за хорошо выполненное действие.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "praise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Всё починил за пять минут? <Молоток>!",
                            "target_lang_translations": {
                                "en": "Fixed everything in five minutes? <Well done>!"
                            }
                        },
                        {
                            "text": "Быстро ты справился с задачей, <молоток>!",
                            "target_lang_translations": {
                                "en": "You handled the task quickly, <good job>!"
                            }

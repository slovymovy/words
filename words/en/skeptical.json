    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0ffc08d4-b577-3294-90ed-696235d049f6",
                    "sense_definition": "Having, or expressing doubt; questioning.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "doubt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My teacher was <w>skeptical</w> when I told her my dog ate my homework.",
                            "target_lang_translations": {
                                "ru": "Моя учительница была <w>скептически настроена</w>, когда я сказал ей, что собака съела мою домашнюю работу."
                            }
                        },
                        {
                            "text": "Many experts remain <w>skeptical</w> about the effectiveness of the new diet plan.",
                            "target_lang_translations": {
                                "ru": "Многие эксперты остаются <w>скептически настроенными</w> по поводу эффективности нового плана диеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "doubtful",
                        "dubious",
                        "incredulous",
                        "questioning"
                    ],
                    "antonyms": [
                        "convinced",
                        "certain",
                        "trusting"
                    ],
                    "common_phrases": [
                        "skeptical about",
                        "skeptical of",
                        "remain skeptical",
                        "highly skeptical"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий или выражающий сомнение; недоверчивый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скептический",
                                "target_lang_sense_clarification": "выражающий сомнение"
                            },
                            {
                                "target_lang_word": "недоверчивый",
                                "target_lang_sense_clarification": "склонный к недоверию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8427559d-f023-3b40-ab57-c5c058b38f4f",
                    "sense_definition": "Of or relating to philosophical skepticism or the skeptics.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "doubt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>skeptical</w> view suggests that we cannot be certain of anything.",
                            "target_lang_translations": {
                                "ru": "<w>Скептический</w> взгляд предполагает, что мы ни в чем не можем быть уверены."
                            }
                        },
                        {
                            "text": "He studied <w>skeptical</w> philosophy to understand the limits of human reasoning.",
                            "target_lang_translations": {
                                "ru": "Он изучал <w>скептическую</w> философию, чтобы понять пределы человеческого мышления."
                            }
                        }
                    ],
                    "antonyms": [
                        "dogmatic"
                    ],
                    "common_phrases": [
                        "skeptical philosophy",
                        "skeptical arguments"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к философскому скептицизму или скептикам."
                    },
                    "translations": {
                        "ru": [
                            {

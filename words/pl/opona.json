                },
                {
                    "sense_id": "5165aaa2-0ca4-3646-9f62-89b7b1e45c09",
                    "sense_definition": "Ozdobna tkanina wieszana na ścianie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ściany komnaty zdobiła piękna, haftowana <w>opona</w>.",
                            "target_lang_translations": {
                                "ru": "Стены комнаты украшал красивый вышитый <w>гобелен</w>."
                            }
                        },
                        {
                            "text": "W muzeum podziwialiśmy zabytkowe <w>opony</w> ze scenami myśliwskimi.",
                            "target_lang_translations": {
                                "ru": "В музее мы любовались старинными <w>гобеленами</w> со сценами охоты."
                            }
                        }
                    ],
                    "synonyms": [

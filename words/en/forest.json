                },
                {
                    "sense_id": "db8d5d5f-8038-3b3f-a8c6-866d5dd1be5e",
                    "sense_definition": "The football club Nottingham Forest.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "org_name",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "<w>Forest</w> won the match yesterday.",
                            "target_lang_translations": {
                                "ru": "<w>Форест</w> выиграл вчерашний матч."

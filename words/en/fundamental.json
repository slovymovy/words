    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6e48d8c2-9d81-329d-826e-bc3c78c6c7a1",
                    "sense_definition": "Affecting the central nature or most important part of something; essential and extremely important.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "core_importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a <w>fundamental</w> difference between the two political parties.",
                            "target_lang_translations": {
                                "ru": "Между двумя политическими партиями существует <w>фундаментальная</w> разница."
                            }
                        },
                        {
                            "text": "Freedom of speech is a <w>fundamental</w> right in a free society.",
                            "target_lang_translations": {
                                "ru": "Свобода слова — это <w>фундаментальное</w> право в свободном обществе."
                            }
                        }
                    ],
                    "synonyms": [
                        "elementary",
                        "groundlaying",
                        "elementary"
                    ],
                    "common_phrases": [
                        "fundamental change",
                        "fundamental difference",
                        "fundamental issue",
                        "fundamental right"
                    ],
                    "target_lang_definitions": {
                        "ru": "Затрагивающий самую суть или наиболее важную часть чего-либо; существенный и чрезвычайно важный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фундаментальный",
                                "target_lang_sense_clarification": "относящийся к самой сути"
                            },
                            {
                                "target_lang_word": "существенный",
                                "target_lang_sense_clarification": "значительный, важный"
                            },
                            {
                                "target_lang_word": "основной",
                                "target_lang_sense_clarification": "главный, центральный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cccca04c-a9fc-3847-8926-7f1be8f03732",
                    "sense_definition": "Serving as a base or foundation on which something is built or established.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "core_importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Understanding grammar is <w>fundamental</w> to learning a new language.",
                            "target_lang_translations": {
                                "ru": "Понимание грамматики является <w>основополагающим</w> для изучения нового языка."
                            }
                        },
                        {
                            "text": "The team needs to address the <w>fundamental</w> causes of the problem.",
                            "target_lang_translations": {
                                "ru": "Команде необходимо устранить <w>фундаментальные</w> причины проблемы."

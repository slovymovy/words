                    ],
                    "common_phrases": [
                        "grać w piłkę",
                        "grać w karty",
                        "grać w szachy",
                        "grać w gry",
                        "grać mecz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участвовать в спортивной, общественной или компьютерной игре, следуя её правилам."

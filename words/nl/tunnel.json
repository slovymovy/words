                },
                {
                    "sense_id": "51335544-afeb-3e3d-a1f1-277942e150b3",
                    "sense_definition": "Gebiedende wijs van <w>tunnelen</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Tunnel</w> die verbinding onmiddellijk voor extra veiligheid!",
                            "target_lang_translations": {
                                "ru": "<w>Туннелируй</w> это соединение немедленно для дополнительной безопасности!"
                            }
                        },
                        {
                            "text": "<w>Tunnel</w> door de berg heen zodat de weg vlak blijft.",
                            "target_lang_translations": {
                                "ru": "<w>Прокладывай туннель</w> сквозь гору, чтобы дорога оставалась ровной."
                            }
                        }
                    ],
                    "traits": [

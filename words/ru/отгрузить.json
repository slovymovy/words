    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f267a8f8-e980-3c97-8b0a-9620a644751e",
                    "sense_definition": "Погрузив, отправить товары, продукцию или иной груз по назначению.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "shipping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Завод обязался <w>отгрузить</w> первую партию оборудования до конца месяца.",
                            "target_lang_translations": {
                                "en": "The factory undertook to <w>ship</w> the first batch of equipment by the end of the month."
                            }
                        },
                        {
                            "text": "Менеджер сообщил, что склад не успеет <w>отгрузить</w> товар сегодня вечером.",
                            "target_lang_translations": {
                                "en": "The manager reported that the warehouse would not have time to <w>ship</w> the goods this evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "отправить",
                        "выслать",
                        "экспедировать"
                    ],
                    "antonyms": [
                        "принять",
                        "получить"
                    ],
                    "common_phrases": [
                        "отгрузить товар",
                        "отгрузить продукцию",
                        "отгрузить партию",
                        "отгрузить со склада"
                    ],
                    "target_lang_definitions": {
                        "en": "To send goods, products, or other cargo to a destination after loading."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ship",
                                "target_lang_sense_clarification": "transport goods"
                            },
                            {
                                "target_lang_word": "dispatch",
                                "target_lang_sense_clarification": "send off goods"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3afa6d76-406a-35b6-bea9-52dda529c76f",
                    "sense_definition": "Снять или выгрузить часть груза, обычно для облегчения транспортного средства.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "unloading",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чтобы пройти мелководье, капитану пришлось <w>отгрузить</w> часть угля на баржу.",
                            "target_lang_translations": {
                                "en": "To pass the shallows, the captain had to <w>offload</w> part of the coal onto a barge."
                            }
                        },
                        {
                            "text": "Водитель решил <w>отгрузить</w> лишние ящики, чтобы не превышать допустимый вес.",
                            "target_lang_translations": {
                                "en": "The driver decided to <w>offload</w> the extra boxes so as not to exceed the permissible weight."
                            }
                        }
                    ],
                    "synonyms": [
                        "сгрузить",
                        "выгрузить"
                    ],
                    "antonyms": [
                        "догрузить"
                    ],
                    "common_phrases": [
                        "отгрузить часть",
                        "отгрузить лишнее"
                    ],
                    "target_lang_definitions": {
                        "en": "To remove or unload part of the cargo, usually to lighten a vehicle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "offload",
                                "target_lang_sense_clarification": "remove part of a load"
                            },
                            {
                                "target_lang_word": "unload",
                                "target_lang_sense_clarification": "remove cargo"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
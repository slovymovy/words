            "senses": [
                {
                    "sense_id": "b2498731-c7d5-38ff-8258-9861b6ef5696",
                    "sense_definition": "The act or process of using numbers to find an amount or result.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to do a quick <w>calculation</w> to see if we can afford the trip.",

{
    "word_family": [
        "подпись",
        "подписка",
        "подписчик",
        "подписание",
        "подписной"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "abd5ba9f-8de9-3f07-87ae-69d2ba6684d7",
                    "sense_definition": "Поставить свою подпись на документе, тексте или изображении для подтверждения, согласия или удостоверения авторства.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "signing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор должен <подписать> этот приказ сегодня вечером.",
                            "target_lang_translations": {
                                "en": "The director must <sign> this order tonight."
                            }
                        },
                        {
                            "text": "Я забыл <подписать> своё сочинение перед тем, как сдать его учителю.",
                            "target_lang_translations": {
                                "en": "I forgot to <sign> my essay before handing it in to the teacher."
                            }
                        },
                        {
                            "text": "Мы готовы <подписать> договор на ваших условиях.",
                            "target_lang_translations": {
                                "en": "We are ready to <sign> the contract on your terms."
                            }
                        }
                    ],
                    "synonyms": [
                        "расписаться",
                        "поставить подпись",
                        "скрепить подписью"
                    ],
                    "common_phrases": [
                        "подписать договор",
                        "подписать документ",
                        "подписать заявление",
                        "подписать контракт",
                        "подписать указ"
                    ],
                    "target_lang_definitions": {
                        "en": "To place one's signature on a document, text, or image to confirm, agree, or certify authorship."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sign",
                                "target_lang_sense_clarification": "to write one's signature on a document"
                            },
                            {
                                "target_lang_word": "put one's signature",
                                "target_lang_sense_clarification": "phrase"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0fdfcba-0282-30df-93f6-c01c4248ddb7",
                    "sense_definition": "Написать текст внизу чего-либо или добавить запись к уже существующему тексту; сделать поясняющую надпись.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "writing_addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка попросила меня <подписать> старую фотографию на память.",
                            "target_lang_translations": {
                                "en": "Grandmother asked me to <sign> the old photograph as a keepsake."
                            }
                        },
                        {
                            "text": "Нужно <подписать> эту коробку, чтобы мы знали, что лежит внутри.",
                            "target_lang_translations": {
                                "en": "We need to <label> this box so we know what is inside."
                            }
                        }
                    ],
                    "synonyms": [
                        "приписать",
                        "дописать"
                    ],
                    "common_phrases": [
                        "подписать фотографию",
                        "подписать открытку",
                        "подписать снизу"
                    ],
                    "target_lang_definitions": {
                        "en": "To write text on an object to identify it, or to add a note/inscription to something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "label",
                                "target_lang_sense_clarification": "to mark an object with a name or description"
                            },
                            {
                                "target_lang_word": "caption",
                                "target_lang_sense_clarification": "to add a description to a picture or photo"
                            },
                            {
                                "target_lang_word": "sign",
                                "target_lang_sense_clarification": "to write a name or dedication on something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eec95d93-af43-30ab-a225-698a76cb077b",
                    "sense_definition": "Оформить для кого-либо получение периодического издания (газеты, журнала) или услуги.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "subscription",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец решил <подписать> меня на детский научный журнал.",
                            "target_lang_translations": {
                                "en": "Father decided to <subscribe> me to a children's science magazine."
                            }
                        },
                        {
                            "text": "Мы хотим <подписать> родителей на местную газету, чтобы они знали новости.",
                            "target_lang_translations": {
                                "en": "We want to <subscribe> our parents to the local newspaper so they know the news."
                            }
                        }
                    ],
                    "synonyms": [
                        "сделать подписчиком"
                    ],
                    "common_phrases": [
                        "подписать на газету",
                        "подписать на журнал"
                    ],
                    "target_lang_definitions": {
                        "en": "To arrange for someone to receive a periodical publication (newspaper, magazine) or service."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "subscribe",
                                "target_lang_sense_clarification": "to arrange a subscription for someone"
                            },
                            {
                                "target_lang_word": "sign up",
                                "target_lang_sense_clarification": "to register someone for a service"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
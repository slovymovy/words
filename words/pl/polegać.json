    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "123affaa-1795-397e-b393-e0b0e667da7a",
                    "sense_definition": "Ufać komuś lub czemuś; być pewnym, że ktoś nie zawiedzie lub że coś zadziała zgodnie z oczekiwaniami.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "zaufanie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trudnych chwilach zawsze możesz na mnie <w>polegać</w>.",
                            "target_lang_translations": {
                                "ru": "В трудные моменты ты всегда можешь на меня <w>полагаться</w>."
                            }
                        },
                        {
                            "text": "Niestety nie mogę <w>polegać</w> na jego obietnicach, bo często kłamie.",
                            "target_lang_translations": {
                                "ru": "К сожалению, я не могу <w>полагаться</w> на его обещания, потому что он часто врет."
                            }
                        }
                    ],
                    "synonyms": [
                        "liczyć",
                        "zdawać się"
                    ],
                    "common_phrases": [
                        "polegać na sobie",
                        "można na nim polegać",
                        "polegać na intuicji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доверять кому-то или чему-то; быть уверенным, что кто-то не подведет или что-то сработает как ожидается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полагаться",
                                "target_lang_sense_clarification": "всецело доверяться кому-либо или надеяться на что-либо"
                            },
                            {
                                "target_lang_word": "рассчитывать",
                                "target_lang_sense_clarification": "возлагать надежды, ожидать помощи или поддержки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2ed1b1f-7944-3639-bfce-ee16d764c483",
                    "sense_definition": "Mieć w czymś swoją podstawę, przyczynę lub istotę; opierać się na określonej zasadzie lub działaniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "istota_rzeczy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały problem <w>polega</w> na tym, że nie mamy wystarczająco dużo czasu.",
                            "target_lang_translations": {
                                "ru": "Вся проблема <w>заключается</w> в том, что у нас недостаточно времени."
                            }
                        },
                        {
                            "text": "Ta gra karciana <w>polega</w> na zbieraniu par takich samych obrazków.",
                            "target_lang_translations": {
                                "ru": "Эта карточная игра <w>заключается</w> в сборе пар одинаковых картинок."
                            }
                        }
                    ],
                    "synonyms": [
                        "opierać się"
                    ],
                    "common_phrases": [
                        "polegać na tym",
                        "na czym to polega",
                        "zadanie polega na"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь что-то своей сутью, основой или причиной; сводиться к чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заключаться",
                                "target_lang_sense_clarification": "содержаться внутри, составлять сущность чего-либо"
                            },
                            {
                                "target_lang_word": "состоять",
                                "target_lang_sense_clarification": "иметь что-то своим содержанием или основой"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
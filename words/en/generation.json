{
    "word_family": [
        "generate",
        "generative",
        "generator",
        "generational",
        "regeneration",
        "degenerate",
        "degeneration"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "02e20839-ccd0-3826-b49d-35da8c29552c",
                    "sense_definition": "All the people born and living at about the same time, regarded collectively.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "people_time_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The younger <w>generation</w> is very comfortable with digital technology.",
                            "target_lang_translations": {
                                "ru": "Молодое <w>поколение</w> очень свободно владеет цифровыми технологиями."
                            }
                        },
                        {
                            "text": "We must protect the environment for future <w>generations</w>.",
                            "target_lang_translations": {
                                "ru": "Мы должны защитить окружающую среду для будущих <w>поколений</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cohort",
                        "age group",
                        "peers"
                    ],
                    "common_phrases": [
                        "younger generation",
                        "future generations",
                        "new generation",
                        "older generation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Все люди, родившиеся и живущие примерно в одно и то же время, рассматриваемые как группа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поколение",
                                "target_lang_sense_clarification": "группа людей одного возраста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b49afa41-135b-3cf2-affa-4313e9b69106",
                    "sense_definition": "The act or process of producing or creating something, especially energy, heat, or income.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "production_creation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dam is used for the <w>generation</w> of hydroelectric power.",
                            "target_lang_translations": {
                                "ru": "Плотина используется для <w>выработки</w> гидроэлектроэнергии."
                            }
                        },
                        {
                            "text": "The government is focusing on job <w>generation</w> to boost the economy.",
                            "target_lang_translations": {
                                "ru": "Правительство сосредоточено на <w>создании</w> рабочих мест для стимулирования экономики."
                            }
                        }
                    ],
                    "synonyms": [
                        "production",
                        "creation",
                        "formation"
                    ],
                    "antonyms": [
                        "consumption",
                        "destruction"
                    ],
                    "common_phrases": [
                        "power generation",
                        "electricity generation",
                        "income generation",
                        "heat generation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс производства или создания чего-либо, особенно энергии, тепла или дохода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выработка",
                                "target_lang_sense_clarification": "энергии или тепла"
                            },
                            {
                                "target_lang_word": "создание",
                                "target_lang_sense_clarification": "рабочих мест, дохода"
                            },
                            {
                                "target_lang_word": "генерация",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83d3ce16-aa73-3868-88e7-c601ebdc381f",
                    "sense_definition": "A single step in the line of descent of a family, consisting of parents and their children.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "people_time_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This necklace has been passed down from <w>generation</w> to <w>generation</w>.",
                            "target_lang_translations": {
                                "ru": "Это ожерелье передавалось из <w>поколения</w> в <w>поколение</w>."
                            }
                        },
                        {
                            "text": "There is often a misunderstanding known as the <w>generation</w> gap between parents and teenagers.",
                            "target_lang_translations": {
                                "ru": "Между родителями и подростками часто возникает недопонимание, известное как разрыв <w>поколений</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "step",
                        "stage"
                    ],
                    "common_phrases": [
                        "generation gap",
                        "from generation to generation",
                        "three generations"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная ступень в родословной семьи, состоящая из родителей и их детей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поколение",
                                "target_lang_sense_clarification": "ступень в родословной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40b40b9e-ab48-36a0-88db-c33912bcde45",
                    "sense_definition": "A specific stage of development in the design and manufacture of a product, especially in technology.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "technology_version_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The next <w>generation</w> of gaming consoles will offer incredible graphics.",
                            "target_lang_translations": {
                                "ru": "Следующее <w>поколение</w> игровых консолей предложит невероятную графику."
                            }
                        },
                        {
                            "text": "She bought a second-<w>generation</w> tablet that is much faster than the original.",
                            "target_lang_translations": {
                                "ru": "Она купила планшет второго <w>поколения</w>, который намного быстрее оригинального."
                            }
                        }
                    ],
                    "synonyms": [
                        "version",
                        "model",
                        "iteration"
                    ],
                    "common_phrases": [
                        "next generation",
                        "first generation",
                        "latest generation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенная стадия развития в разработке и производстве продукта, особенно в технике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поколение",
                                "target_lang_sense_clarification": "версия технологии или устройства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f5eec94-c8f2-35db-9de2-ed8cf0ce5202",
                    "sense_definition": "The average period of time, usually considered to be about thirty years, in which children grow up, become adults, and have children of their own.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "people_time_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Attitudes toward marriage have changed significantly since a <w>generation</w> ago.",
                            "target_lang_translations": {
                                "ru": "Отношение к браку существенно изменилось по сравнению с тем, что было <w>поколение</w> назад."
                            }
                        },
                        {
                            "text": "It may take a <w>generation</w> for the political landscape to shift completely.",
                            "target_lang_translations": {
                                "ru": "Может потребоваться целое <w>поколение</w>, чтобы политический ландшафт полностью изменился."
                            }
                        }
                    ],
                    "common_phrases": [
                        "a generation ago",
                        "for a generation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Средний период времени, обычно около тридцати лет, за который дети вырастают и становятся взрослыми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поколение",
                                "target_lang_sense_clarification": "период времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67b1608b-870c-3503-bfb4-bb78d290d4d5",
                    "sense_definition": "The offspring or descendants of a person or animal considered as a group.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "people_time_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She regarded her grandchildren as a new <w>generation</w> of the family line.",
                            "target_lang_translations": {
                                "ru": "Она считала своих внуков новым <w>поколением</w> семейного рода."
                            }
                        },
                        {
                            "text": "The farm has been worked by the same family for every <w>generation</w> since the war.",
                            "target_lang_translations": {
                                "ru": "На этой ферме работала одна и та же семья в каждом <w>поколении</w> после войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "progeny",
                        "offspring",
                        "descendants"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потомство или потомки человека или животного, рассматриваемые как группа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потомство",
                                "target_lang_sense_clarification": "дети, потомки"
                            },
                            {
                                "target_lang_word": "поколение",
                                "target_lang_sense_clarification": "группа потомков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b91a93e7-85c6-3520-a58a-276efa0d0126",
                    "sense_definition": "A specific version or era within a series of popular culture products.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technology_version_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fans are excited about the new <w>generation</w> of the movie franchise.",
                            "target_lang_translations": {
                                "ru": "Фанаты в восторге от нового <w>поколения</w> кинофраншизы."
                            }
                        },
                        {
                            "text": "Each <w>generation</w> of the game introduces new characters and maps.",
                            "target_lang_translations": {
                                "ru": "Каждое <w>поколение</w> игры представляет новых персонажей и карты."
                            }
                        }
                    ],
                    "synonyms": [
                        "iteration",
                        "series"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная версия или эра в серии продуктов популярной культуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поколение",
                                "target_lang_sense_clarification": "часть серии или франшизы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83759839-aa31-3975-b248-da316c47e926",
                    "sense_definition": "The biological process of producing offspring or creating a living organism.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "production_creation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist studied the <w>generation</w> of cells in the laboratory.",
                            "target_lang_translations": {
                                "ru": "Ученый изучал <w>образование</w> клеток в лаборатории."
                            }
                        },
                        {
                            "text": "Spontaneous <w>generation</w> was an early theory about how life arises.",
                            "target_lang_translations": {
                                "ru": "Самопроизвольное <w>зарождение</w> было ранней теорией о том, как возникает жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "procreation",
                        "reproduction",
                        "breeding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Биологический процесс производства потомства или создания живого организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарождение",
                                "target_lang_sense_clarification": "возникновение жизни"
                            },
                            {
                                "target_lang_word": "образование",
                                "target_lang_sense_clarification": "формирование клеток/тканей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f70ce41a-fa3f-3aa4-807c-e6d0d5e53058",
                    "sense_definition": "A copy of a recording or image made from an earlier copy, often implying a loss of quality.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "technology_version_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The video quality was poor because it was a fourth-<w>generation</w> copy.",
                            "target_lang_translations": {
                                "ru": "Качество видео было плохим, потому что это была <w>копия</w> в четвертом поколении."
                            }
                        },
                        {
                            "text": "With digital files, there is no loss of quality in the next <w>generation</w>.",
                            "target_lang_translations": {
                                "ru": "С цифровыми файлами нет потери качества в следующем <w>поколении</w> копирования."
                            }
                        }
                    ],
                    "synonyms": [
                        "copy",
                        "duplicate"
                    ],
                    "antonyms": [
                        "original",
                        "master"
                    ],
                    "common_phrases": [
                        "second generation",
                        "third generation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Копия записи или изображения, сделанная с более ранней копии, часто подразумевающая потерю качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копия",
                                "target_lang_sense_clarification": "дубликат"
                            },
                            {
                                "target_lang_word": "поколение",
                                "target_lang_sense_clarification": "стадия копирования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fa9442d-fb55-3429-a8d3-9f9b9c90f799",
                    "sense_definition": "A race, family, or breed of people.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "people_time_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient text speaks of a <w>generation</w> of vipers.",
                            "target_lang_translations": {
                                "ru": "В древнем тексте говорится о змеином <w>отродье</w>."
                            }
                        },
                        {
                            "text": "He was the last of a noble <w>generation</w>.",
                            "target_lang_translations": {
                                "ru": "Он был последним из благородного <w>рода</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lineage",
                        "breed",
                        "stock"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "literary or biblical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Род, семья или племя людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "род",
                                "target_lang_sense_clarification": "семья, происхождение"
                            },
                            {
                                "target_lang_word": "племя",
                                "target_lang_sense_clarification": "группа людей"
                            },
                            {
                                "target_lang_word": "порождение",
                                "target_lang_sense_clarification": "библейский/устаревший контекст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29b69540-1890-3d47-a5cf-8604ac0a8db8",
                    "sense_definition": "The formation of a geometric figure by the motion of a point, line, or surface.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "production_creation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>generation</w> of a cylinder involves rotating a rectangle around one side.",
                            "target_lang_translations": {
                                "ru": "<w>Образование</w> цилиндра включает вращение прямоугольника вокруг одной стороны."
                            }
                        },
                        {
                            "text": "Computer software handles the <w>generation</w> of complex curves.",
                            "target_lang_translations": {
                                "ru": "Компьютерное программное обеспечение управляет <w>построением</w> сложных кривых."
                            }
                        }
                    ],
                    "synonyms": [
                        "formation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образование геометрической фигуры движением точки, линии или поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "образование",
                                "target_lang_sense_clarification": "геометрических фигур"
                            },
                            {
                                "target_lang_word": "формирование",
                                "target_lang_sense_clarification": "создание формы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "827bafd9-7c72-3a11-93a0-6c9080755720",
                    "sense_definition": "A single iteration of the rules applied to a pattern in a cellular automaton.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technology_version_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The simulation ran for one thousand <w>generations</w>.",
                            "target_lang_translations": {
                                "ru": "Симуляция длилась одну тысячу <w>поколений</w>."
                            }
                        },
                        {
                            "text": "The pattern repeats itself every thirty <w>generations</w>.",
                            "target_lang_translations": {
                                "ru": "Узор повторяется каждые тридцать <w>поколений</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "iteration",
                        "step",
                        "tick"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одиночная итерация правил, применяемых к шаблону в клеточном автомате."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поколение",
                                "target_lang_sense_clarification": "шаг симуляции"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
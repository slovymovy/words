                    "sense_id": "ebdabc6b-2193-3202-8eef-845da052e455",
                    "sense_definition": "Iets vernietigen of laten verteren door vuur",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fire_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer <w>verbrandde</w> het tuinafval achter zijn huis.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>сжег</w> садовый мусор за своим домом.",
                                "en": "The farmer <w>burned</w> the garden waste behind his house."
                            }
                        },
                        {
                            "text": "Hij heeft al zijn oude brieven in de open haard <w>verbrand</w>.",
                            "target_lang_translations": {
                                "ru": "Он <w>сжег</w> все свои старые письма в камине.",
                                "en": "He <w>burned</w> all his old letters in the fireplace."
                            }

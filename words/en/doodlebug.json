    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1112076e-062a-320d-8752-f163cfe818bd",
                    "sense_definition": "An affectionate nickname for a person, especially a child or a pet.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Come here, my little <w>doodlebug</w>, and give me a hug.",
                            "target_lang_translations": {
                                "ru": "Иди сюда, моя маленькая <w>букашка</w>, и обними меня."
                            }
                        },
                        {
                            "text": "She called her youngest son <w>doodlebug</w> until he was a teenager.",
                            "target_lang_translations": {
                                "ru": "Она называла своего младшего сына <w>букашкой</w>, пока он не стал подростком."
                            }
                        }
                    ],
                    "synonyms": [
                        "sweetie",
                        "darling",
                        "munchkin"
                    ],
                    "common_phrases": [
                        "little doodlebug",
                        "cute doodlebug"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое прозвище для человека, особенно ребенка или домашнего животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "букашка",
                                "target_lang_sense_clarification": "ласковое обращение"
                            },
                            {
                                "target_lang_word": "козявка",
                                "target_lang_sense_clarification": "шутливо-ласковое обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ba9df62-99c8-3b01-a5b0-a4bdfae97b9a",
                    "sense_definition": "The larva of an antlion, known for digging conical pits in sandy soil to trap prey.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "insects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We watched the <w>doodlebug</w> wait at the bottom of its sandy trap for an ant.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали, как <w>личинка муравьиного льва</w> ждала муравья на дне своей песчаной ловушки."
                            }
                        },
                        {
                            "text": "In the southern United States, children often call the antlion larva a <w>doodlebug</w>.",
                            "target_lang_translations": {
                                "ru": "На юге США дети часто называют личинку муравьиного льва <w>муравьиным львом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "antlion larva"
                    ],
                    "common_phrases": [
                        "doodlebug pit"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in the US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Личинка муравьиного льва, известная тем, что роет конические ямки в песчаной почве для ловли добычи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "личинка муравьиного льва",
                                "target_lang_sense_clarification": "энтомология"
                            },
                            {
                                "target_lang_word": "муравьиный лев",
                                "target_lang_sense_clarification": "насекомое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30ee6ae0-a9c1-3f06-983b-a8145d65f817",
                    "sense_definition": "A V-1 flying bomb used by Germany during World War II.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "weapons",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The terrifying sound of a <w>doodlebug</w> engine cutting out signaled an imminent explosion.",
                            "target_lang_translations": {
                                "ru": "Ужасающий звук заглохшего двигателя <w>Фау-1</w> предвещал скорый взрыв."
                            }
                        },
                        {
                            "text": "Many buildings in London were destroyed by a <w>doodlebug</w> during the war.",
                            "target_lang_translations": {
                                "ru": "Многие здания в Лондоне были разрушены <w>самолётом-снарядом</w> во время войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "V-1",
                        "buzz bomb"
                    ],
                    "common_phrases": [
                        "flying doodlebug"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "WWII era"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Самолёт-снаряд Фау-1, использовавшийся Германией во время Второй мировой войны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Фау-1",
                                "target_lang_sense_clarification": "историческое название"
                            },
                            {
                                "target_lang_word": "самолёт-снаряд",
                                "target_lang_sense_clarification": "тип оружия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3f578bb-2aba-3b7f-8f4f-b7b570988a18",
                    "sense_definition": "A self-propelled railroad car used for light passenger or freight service.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old <w>doodlebug</w> rattled down the branch line carrying mail and a few passengers.",
                            "target_lang_translations": {
                                "ru": "Старая <w>автомотриса</w> с грохотом ехала по ветке, перевозя почту и нескольких пассажиров."
                            }
                        },
                        {
                            "text": "Railroad enthusiasts restored a vintage <w>doodlebug</w> for the museum exhibition.",
                            "target_lang_translations": {
                                "ru": "Любители железных дорог отреставрировали старинную <w>автомотрису</w> для музейной выставки."
                            }
                        }
                    ],
                    "synonyms": [
                        "railcar"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Самоходный железнодорожный вагон, используемый для легких пассажирских или грузовых перевозок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "автомотриса",
                                "target_lang_sense_clarification": "железнодорожный транспорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62f0714b-6ae2-354e-89b9-ba56a86f8384",
                    "sense_definition": "A woodlouse or pill bug.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "insects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He lifted the damp rock and found a <w>doodlebug</w> curling into a ball.",
                            "target_lang_translations": {
                                "ru": "Он поднял влажный камень и обнаружил <w>мокрицу</w>, сворачивающуюся в шарик."
                            }
                        },
                        {
                            "text": "In some local dialects, a pill bug is referred to as a <w>doodlebug</w>.",
                            "target_lang_translations": {
                                "ru": "В некоторых местных диалектах мокрицу называют <w>мокрицей</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "woodlouse",
                        "pill bug",
                        "roly-poly"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK dialect / Australia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мокрица (ракообразное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мокрица",
                                "target_lang_sense_clarification": "зоология"

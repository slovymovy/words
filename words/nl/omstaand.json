{
    "word_family": [
        "omstander",
        "omstandigheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "badd6a6d-025c-336b-8958-6d913eb6ed2b",
                    "sense_definition": "Zich in de directe omgeving bevindend.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>omstaande</w> gebouwen liepen lichte schade op door de trillingen.",
                            "target_lang_translations": {
                                "en": "The <w>surrounding</w> buildings sustained slight damage from the vibrations.",
                                "ru": "<w>Окружающие</w> здания получили легкие повреждения от вибрации."
                            }
                        },
                        {
                            "text": "Hij botste per ongeluk tegen enkele <w>omstaande</w> mensen in de drukke hal.",
                            "target_lang_translations": {
                                "en": "He accidentally bumped into some people <w>standing nearby</w> in the busy hall.",
                                "ru": "Он случайно столкнулся с несколькими <w>стоящими рядом</w> людьми в переполненном холле."
                            }
                        }
                    ],
                    "synonyms": [
                        "ommezijde"
                    ],
                    "common_phrases": [
                        "omstaande gebouwen",
                        "omstaande personen"
                    ],
                    "target_lang_definitions": {
                        "en": "Located in the immediate surroundings or vicinity.",
                        "ru": "Находящийся в непосредственной близости."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "surrounding",
                                "target_lang_sense_clarification": "encircling or located all around"
                            },
                            {
                                "target_lang_word": "nearby",
                                "target_lang_sense_clarification": "situated close by"
                            },
                            {
                                "target_lang_word": "adjacent",
                                "target_lang_sense_clarification": "next to or adjoining"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "окружающий",
                                "target_lang_sense_clarification": "находящийся вокруг"
                            },
                            {
                                "target_lang_word": "стоящий рядом",
                                "target_lang_sense_clarification": "расположенный поблизости"
                            },
                            {
                                "target_lang_word": "близлежащий",
                                "target_lang_sense_clarification": "о зданиях или местности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d05ee9e2-c850-3a41-8c3d-645c6a9e35d1",
                    "sense_definition": "Vermeld of gedrukt op de achterkant van een pagina of document.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor de algemene voorwaarden verwijzen wij naar de <w>omstaande</w> tekst.",
                            "target_lang_translations": {
                                "en": "For the general terms and conditions, we refer to the text <w>overleaf</w>.",
                                "ru": "Что касается общих условий, мы ссылаемся на текст <w>на обороте</w>."
                            }
                        },
                        {
                            "text": "Op de <w>omstaande</w> bladzijde vindt u een gedetailleerde kaart van het gebied.",
                            "target_lang_translations": {
                                "en": "On the page <w>overleaf</w> you will find a detailed map of the area.",
                                "ru": "На <w>обратной</w> стороне страницы вы найдете подробную карту местности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zie omstaande",
                        "omstaande tekst"
                    ],
                    "target_lang_definitions": {
                        "en": "Mentioned or printed on the back of a page or document.",
                        "ru": "Указанный или напечатанный на обратной стороне страницы или документа."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overleaf",
                                "target_lang_sense_clarification": "on the other side of the page"
                            },
                            {
                                "target_lang_word": "on the reverse",
                                "target_lang_sense_clarification": "on the back side"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "на обороте",
                                "target_lang_sense_clarification": "расположенный на обратной стороне"
                            },
                            {
                                "target_lang_word": "оборотный",
                                "target_lang_sense_clarification": "относящийся к обратной стороне"
                            }
                        ]
                    }
                }
            ]
        },

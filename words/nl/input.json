        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "18f2e738-3bf3-3e33-bac9-0217abd1d9f8",
                    "sense_definition": "Gegevens of informatie die in een computer of systeem worden ingevoerd.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "data_processing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De computer heeft de juiste <w>input</w> nodig om de berekening te maken.",
                            "target_lang_translations": {
                                "ru": "Компьютеру требуются правильные <w>входные данные</w> для выполнения вычислений.",
                                "en": "The computer needs the correct <w>input</w> to make the calculation."
                            }
                        },
                        {
                            "text": "Zonder correcte <w>input</w> kan het programma niet werken.",
                            "target_lang_translations": {
                                "ru": "Без правильного <w>ввода</w> программа не сможет работать.",
                                "en": "Without correct <w>input</w>, the program cannot work."
                            }
                        }
                    ],
                    "synonyms": [
                        "invoer"
                    ],
                    "antonyms": [
                        "output"
                    ],
                    "common_phrases": [
                        "data input",
                        "input verwerken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Данные или информация, вводимые в компьютер или систему.",
                        "en": "Data or information entered into a computer or system."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "входные данные",
                                "target_lang_sense_clarification": "информация"
                            },
                            {
                                "target_lang_word": "ввод",
                                "target_lang_sense_clarification": "процесс или данные"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "input",
                                "target_lang_sense_clarification": "data or information"
                            },
                            {
                                "target_lang_word": "data entry",
                                "target_lang_sense_clarification": "process"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f91d985b-a744-39a6-92da-a65a7f8ace24",
                    "sense_definition": "Een bijdrage aan een proces, discussie of product, vaak in de vorm van ideeën of advies.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "contribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We waarderen jouw <w>input</w> tijdens deze vergadering enorm.",
                            "target_lang_translations": {
                                "ru": "Мы очень ценим твой <w>вклад</w> в это собрание.",
                                "en": "We greatly appreciate your <w>input</w> during this meeting."
                            }
                        },
                        {
                            "text": "De manager vroeg om <w>input</w> van alle medewerkers voor het nieuwe plan.",
                            "target_lang_translations": {
                                "ru": "Менеджер попросил всех сотрудников высказать свое <w>мнение</w> касательно нового плана.",
                                "en": "The manager asked for <w>input</w> from all employees for the new plan."
                            }
                        }
                    ],
                    "antonyms": [
                        "output"
                    ],
                    "common_phrases": [
                        "input leveren",
                        "om input vragen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вклад в процесс, обсуждение или продукт, часто в виде идей или советов.",
                        "en": "A contribution to a process, discussion, or product, often in the form of ideas or advice."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вклад",
                                "target_lang_sense_clarification": "участие"
                            },
                            {
                                "target_lang_word": "мнение",
                                "target_lang_sense_clarification": "идеи или отзыв"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "input",
                                "target_lang_sense_clarification": "ideas or advice"
                            },
                            {
                                "target_lang_word": "contribution",
                                "target_lang_sense_clarification": "general participation"
                            },
                            {
                                "target_lang_word": "feedback",
                                "target_lang_sense_clarification": "opinion or reaction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f5b5951-7055-384f-920c-9d7eba088a47",
                    "sense_definition": "Energie of een signaal dat aan een apparaat of systeem wordt toegevoerd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het signaal op de <w>input</w> van de versterker is te zwak.",
                            "target_lang_translations": {
                                "ru": "Сигнал на <w>входе</w> усилителя слишком слабый.",
                                "en": "The signal at the amplifier's <w>input</w> is too weak."
                            }
                        },
                        {
                            "text": "Sluit de microfoon aan op de juiste <w>input</w> aan de achterkant.",
                            "target_lang_translations": {
                                "ru": "Подключите микрофон к соответствующему <w>входу</w> на задней панели.",
                                "en": "Connect the microphone to the correct <w>input</w> on the back."
                            }
                        }
                    ],
                    "synonyms": [
                        "invoer"
                    ],
                    "antonyms": [
                        "output"
                    ],
                    "common_phrases": [

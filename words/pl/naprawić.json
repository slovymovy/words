{
    "word_family": [
        "naprawiać",
        "naprawa",
        "naprawczy",
        "naprawialny",
        "nienaprawialny"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a7ea7df1-c185-3661-913b-309be8decf15",
                    "sense_definition": "Przywrócić zepsuty lub uszkodzony przedmiot do stanu używalności; usunąć usterkę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój tata potrafi <w>naprawić</w> prawie każde urządzenie domowe.",
                            "target_lang_translations": {
                                "ru": "Мой папа может <w>починить</w> почти любой бытовой прибор."
                            }
                        },
                        {
                            "text": "Muszę oddać buty do szewca, żeby je <w>naprawił</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно отдать обувь сапожнику, чтобы он её <w>починил</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "reg. śl. sprawić"
                    ],
                    "antonyms": [
                        "zepsuć",
                        "uszkodzić",
                        "zniszczyć"
                    ],
                    "common_phrases": [
                        "naprawić samochód",
                        "naprawić kran",
                        "naprawić zegarek",
                        "naprawić dach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести сломанный или поврежденный предмет в рабочее состояние; устранить неисправность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "починить",
                                "target_lang_sense_clarification": "самый распространенный глагол для ремонта вещей"
                            },
                            {
                                "target_lang_word": "отремонтировать",
                                "target_lang_sense_clarification": "более формально или о крупном ремонте"
                            },
                            {
                                "target_lang_word": "исправить",
                                "target_lang_sense_clarification": "устранить дефект или неполадку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41fc025a-ae50-398e-bc74-8cb9b719d5a9",
                    "sense_definition": "Usunąć skutki błędu, poprawić złą sytuację lub wynagrodzić wyrządzoną krzywdę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym <w>naprawić</w> swój błąd i przeprosić za to, co powiedziałem.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>исправить</w> свою ошибку и извиниться за то, что сказал."
                            }
                        },
                        {
                            "text": "Trudno będzie <w>naprawić</w> relacje między nimi po tak wielkiej kłótni.",
                            "target_lang_translations": {
                                "ru": "Будет трудно <w>наладить</w> отношения между ними после такой крупной ссоры."
                            }
                        }
                    ],
                    "antonyms": [
                        "pogorszyć"
                    ],
                    "common_phrases": [
                        "naprawić błąd",
                        "naprawić szkodę",
                        "naprawić relacje",
                        "naprawić krzywdy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устранить последствия ошибки, улучшить плохую ситуацию или возместить причиненный ущерб."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исправить",
                                "target_lang_sense_clarification": "ошибку, ситуацию"
                            },
                            {
                                "target_lang_word": "наладить",
                                "target_lang_sense_clarification": "отношения, связи"
                            },
                            {
                                "target_lang_word": "загладить",
                                "target_lang_sense_clarification": "вину, причиненный вред"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "302892c5-73d7-3979-b0db-03674703b9b3",
                    "sense_definition": "Odzyskać sprawność, dobry stan lub ulec poprawie (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam nadzieję, że pogoda wkrótce <w>się naprawi</w> i pójdziemy na spacer.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, что погода скоро <w>наладится</w>, и мы пойдем гулять."
                            }
                        },
                        {
                            "text": "Po restarcie systemu komputer sam <w>się naprawił</w>.",
                            "target_lang_translations": {
                                "ru": "После перезагрузки системы компьютер сам <w>починился</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "zepsuć się",
                        "pogorszyć się"
                    ],
                    "common_phrases": [
                        "pogoda się naprawiła",
                        "wszystko się naprawi",
                        "zdrowie się naprawiło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восстановить работоспособность, хорошее состояние или улучшиться (используется с местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наладиться",
                                "target_lang_sense_clarification": "о погоде, делах или механизмах"
                            },
                            {
                                "target_lang_word": "исправиться",
                                "target_lang_sense_clarification": "стать лучше, скорректировать поведение"
                            },
                            {
                                "target_lang_word": "починиться",
                                "target_lang_sense_clarification": "разговорное, о предметах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca2cd2d1-70b1-320d-8713-08c2c4c9c0b9",
                    "sense_definition": "Nalać płynu do naczynia (znaczenie regionalne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz chętnie <w>naprawił</w> gościom wina do kielichów.",
                            "target_lang_translations": {
                                "ru": "Хозяин охотно <w>налил</w> гостям вина в бокалы."
                            }
                        },
                        {
                            "text": "Proszę mi <w>naprawić</w> jeszcze trochę herbaty.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>налейте</w> мне еще немного чая."
                            }
                        }
                    ],
                    "synonyms": [
                        "nalać"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "używane głównie na Śląsku lub w języku dawnym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Налить жидкость в посуду (региональное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "налить",
                                "target_lang_sense_clarification": "стандартный перевод"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b620819e-d2a1-3c7f-abbc-14e74e5321cf",
                    "sense_definition": "An elevated landmass smaller than a mountain.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient castle stands on a high <w>hill</w> overlooking the city.",
                            "target_lang_translations": {
                                "ru": "Старинный замок стоит на высоком <w>холме</w>, возвышающемся над городом."
                            }
                        },
                        {
                            "text": "We decided to walk up the <w>hill</w> to see the sunset.",
                            "target_lang_translations": {
                                "ru": "Мы решили подняться на <w>холм</w>, чтобы посмотреть на закат."
                            }
                        }
                    ],
                    "synonyms": [
                        "mound"
                    ],
                    "antonyms": [
                        "dale"
                    ],
                    "common_phrases": [
                        "up the hill",
                        "down the hill",
                        "top of the hill",
                        "steep hill"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвышенность, меньшая по размеру, чем гора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "холм",
                                "target_lang_sense_clarification": "небольшая возвышенность"
                            },
                            {
                                "target_lang_word": "пригорок",
                                "target_lang_sense_clarification": "маленький холм"
                            },
                            {
                                "target_lang_word": "возвышенность",
                                "target_lang_sense_clarification": "общее понятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d3e6337-e254-3f7f-a4da-f3896d94a18a",
                    "sense_definition": "A sloping part of a road.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My bicycle is too heavy to ride up this steep <w>hill</w>.",
                            "target_lang_translations": {
                                "ru": "Мой велосипед слишком тяжёл, чтобы въехать на этот крутой <w>подъём</w>."
                            }
                        },
                        {
                            "text": "Be careful when you drive down the <w>hill</w> in icy weather.",
                            "target_lang_translations": {
                                "ru": "Будьте осторожны на <w>спуске</w> в гололёд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "steep hill",
                        "bottom of the hill"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наклонный участок дороги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подъём",
                                "target_lang_sense_clarification": "дорога вверх"
                            },
                            {
                                "target_lang_word": "спуск",
                                "target_lang_sense_clarification": "дорога вниз"
                            },
                            {
                                "target_lang_word": "горка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c655b72d-6bfe-314b-8cfd-b21172d53750",
                    "sense_definition": "A small mound of earth piled around a plant, or the group of plants growing in it.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer checked every <w>hill</w> of potatoes for signs of pests.",
                            "target_lang_translations": {
                                "ru": "Фермер проверял каждый <w>холмик</w> картофеля на наличие вредителей."
                            }
                        },
                        {
                            "text": "You should plant the seeds in a small <w>hill</w> of loose soil.",
                            "target_lang_translations": {
                                "ru": "Семена следует сажать в небольшой <w>холмик</w> рыхлой земли."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая кучка земли, нагребённая вокруг растения, или группа растений, растущих на ней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "холмик",
                                "target_lang_sense_clarification": "кучка земли"
                            },
                            {
                                "target_lang_word": "кучка",
                                "target_lang_sense_clarification": "насыпь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "172057ed-2812-3a93-a6c9-1b2d20321e35",
                    "sense_definition": "The pitcher's mound in baseball.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager walked out to the <w>hill</w> to speak with his pitcher.",
                            "target_lang_translations": {
                                "ru": "Менеджер вышел на <w>горку</w>, чтобы поговорить со своим питчером."
                            }
                        },
                        {
                            "text": "He has been dominant on the <w>hill</w> throughout the entire season.",
                            "target_lang_translations": {
                                "ru": "Он доминировал на <w>горке</w> на протяжении всего сезона."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "baseball"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Питчерская горка в бейсболе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горка",
                                "target_lang_sense_clarification": "бейсбол"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "56e3cddd-2be2-37fa-87f6-59671720b65b",
                    "sense_definition": "A common surname of English origin, historically for someone who lived near a hill.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Hill</w> has been teaching mathematics at our school for ten years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Хилл</w> преподаёт математику в нашей школе уже десять лет."
                            }
                        },
                        {
                            "text": "I have an appointment with Dr. <w>Hill</w> tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "У меня назначен приём у доктора <w>Хилла</w> на завтрашнее утро."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "common surname"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространённая фамилия английского происхождения, исторически обозначавшая того, кто жил у холма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хилл",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f8a9e0e-1191-3730-bc6f-6a5c68f7169d",
                    "sense_definition": "Capitol Hill; often used to refer to the United States Congress.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The new bill is currently being debated by politicians on the <w>Hill</w>.",
                            "target_lang_translations": {
                                "ru": "Новый законопроект сейчас обсуждается политиками на <w>Холме</w>."
                            }
                        },
                        {
                            "text": "She works as a senior staffer on the <w>Hill</w> in Washington.",
                            "target_lang_translations": {
                                "ru": "Она работает старшим сотрудником на <w>Холме</w> в Вашингтоне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the Hill"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "US politics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Капитолийский холм; часто используется для обозначения Конгресса США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Капитолийский холм",
                                "target_lang_sense_clarification": "местоположение Конгресса"
                            },
                            {
                                "target_lang_word": "Холм",
                                "target_lang_sense_clarification": "политический сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5327a5a3-c00d-3dae-91ca-3f10c6aa9788",
                    "sense_definition": "A common name for various towns and villages in English-speaking countries.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They visited a small historic town called <w>Hill</w> during their trip.",
                            "target_lang_translations": {
                                "ru": "Во время поездки они посетили небольшой исторический городок под названием <w>Хилл</w>."
                            }
                        },
                        {
                            "text": "The map indicates that the village of <w>Hill</w> is five miles away.",
                            "target_lang_translations": {
                                "ru": "Карта показывает, что деревня <w>Хилл</w> находится в пяти милях отсюда."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространённое название различных городов и деревень в англоязычных странах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хилл",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f4f84c6d-8a57-3742-bba5-9d56aa69d175",
                    "sense_definition": "To heap soil around the base of a plant.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Farmers often <w>hill</w> their potatoes to protect the tubers from the sun.",
                            "target_lang_translations": {
                                "ru": "Фермеры часто <w>окучивают</w> картофель, чтобы защитить клубни от солнца."
                            }
                        },
                        {
                            "text": "After planting, you should <w>hill</w> up the earth around the stems.",
                            "target_lang_translations": {
                                "ru": "После посадки следует <w>нагрести</w> землю вокруг стеблей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сгребать землю вокруг основания растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окучивать",
                                "target_lang_sense_clarification": "сельское хозяйство"
                            },
                            {
                                "target_lang_word": "нагребать",
                                "target_lang_sense_clarification": "землю"
                            }
                        ]
                    }
                }
            ]
        }
    ]

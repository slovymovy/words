                    ],
                    "common_phrases": [
                        "kaliber broni",
                        "duży kaliber",
                        "mały kaliber"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренний диаметр ствола огнестрельного оружия или диаметр пули, обычно выражаемый в миллиметрах или дюймах."
                    },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "af47d7e2-aaf9-316b-a5ce-c505a5603b44",
                    "sense_definition": "To run or travel somewhere with great speed and urgency.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>dash</w> if we want to catch the last train home.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>побежать</w>, если мы хотим успеть на последний поезд домой."
                            }
                        },
                        {
                            "text": "She <w>dashed</w> across the street to avoid the rain.",
                            "target_lang_translations": {
                                "ru": "Она <w>ринулась</w> через улицу, чтобы укрыться от дождя."
                            }
                        }
                    ],
                    "synonyms": [
                        "rush",
                        "sprint",
                        "race",
                        "bolt",
                        "hurry"
                    ],
                    "antonyms": [
                        "dawdle",
                        "linger"
                    ],
                    "common_phrases": [
                        "dash off",
                        "dash away",
                        "dash across"
                    ],
                    "target_lang_definitions": {

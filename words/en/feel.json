    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cfdb6ea2-231d-3312-95ab-24209d6f0625",
                    "sense_definition": "To become aware of something through the skin or physical contact.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>feel</w> the warmth of the sun on your face."
                        },
                        {
                            "text": "She <w>felt</w> the soft texture of the blanket."
                        }
                    ],
                    "synonyms": [
                        "touch"
                    ],
                    "common_phrases": [
                        "feel pain",
                        "feel the heat",
                        "feel a pulse"
                    ]
                },
                {
                    "sense_id": "f2545bae-89d9-3fa9-8265-ad2a68394e37",
                    "sense_definition": "To experience an emotion or physical sensation.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "internal_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>feel</w> happy when I am with my friends."
                        },
                        {
                            "text": "He <w>felt</w> tired after working all day."

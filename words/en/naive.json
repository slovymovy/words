    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f5c9f51e-a170-3691-86d2-cc2299fb396f",
                    "sense_definition": "Lacking worldly experience, wisdom, or judgement; unsophisticated.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It would be <w>naive</w> to think that such a complex problem has a simple solution.",
                            "target_lang_translations": {
                                "ru": "Было бы <w>наивно</w> полагать, что у такой сложной проблемы есть простое решение."
                            }
                        },
                        {
                            "text": "She had a <w>naive</w> belief that everyone she met was kind and honest.",
                            "target_lang_translations": {
                                "ru": "У неё была <w>наивная</w> вера в то, что все, кого она встречала, были добрыми и честными."
                            }
                        }
                    ],
                    "synonyms": [
                        "unsophisticated",
                        "innocent",
                        "artless",
                        "guileless",
                        "credulous",
                        "green",
                        "unworldly",
                        "simple",
                        "childlike",
                        "callous",
                        "clueless"
                    ],
                    "antonyms": [
                        "sophisticated",
                        "worldly",
                        "wily",
                        "mature"
                    ],
                    "common_phrases": [
                        "naive assumption",
                        "politically naive",
                        "naive view",
                        "somewhat naive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий жизненного опыта, мудрости или рассудительности; простодушный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наивный",
                                "target_lang_sense_clarification": "неопытный, доверчивый"
                            },
                            {
                                "target_lang_word": "простодушный",
                                "target_lang_sense_clarification": "открытый, бесхитростный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19d155be-1e51-3997-b088-fc18131a6d74",
                    "sense_definition": "Produced in a simple, childlike style, deliberately rejecting sophisticated techniques.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum is famous for its collection of <w>naive</w> art from the early twentieth century.",
                            "target_lang_translations": {
                                "ru": "Музей славится своей коллекцией <w>наивного</w> искусства начала двадцатого века."
                            }
                        },
                        {
                            "text": "He paints in a <w>naive</w> style that uses bright colors and ignores traditional perspective.",
                            "target_lang_translations": {
                                "ru": "Он пишет в <w>наивном</w> стиле, используя яркие цвета и игнорируя традиционную перспективу."
                            }
                        }
                    ],
                    "synonyms": [
                        "simple",
                        "childlike",
                        "unpretentious",
                        "artless"
                    ],
                    "antonyms": [
                        "sophisticated",
                        "pretentious",
                        "affected"
                    ],
                    "common_phrases": [
                        "naive art",
                        "naive painter",
                        "naive style"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical (art)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполненный в простом, детском стиле, намеренно отвергающий сложные техники (об искусстве)."
                    },
                    "translations": {
                        "ru": [
                            {

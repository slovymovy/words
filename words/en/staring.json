                },
                {
                    "sense_id": "4daca37a-d2f9-3d18-97e6-d8c732afeba9",
                    "sense_definition": "Sensational, lurid.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "visual_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The newspaper published a <w>staring</w> headline about the scandal to boost sales.",

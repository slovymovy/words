{
    "word_family": [
        "horror",
        "horrify",
        "horribly",
        "horrid",
        "horrific"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "074d874c-6980-3a8c-879a-625af8282670",
                    "sense_definition": "Tremendously bad or unpleasant.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quality_bad",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We watched a <w>horrible</w> movie that was boring and poorly made.",
                            "target_lang_translations": {
                                "ru": "Мы посмотрели <w>ужасный</w> фильм, который был скучным и плохо сделанным."
                            }
                        },
                        {
                            "text": "The weather was <w>horrible</w> during our entire vacation.",
                            "target_lang_translations": {
                                "ru": "Погода была <w>ужасной</w> в течение всего нашего отпуска."
                            }
                        }
                    ],
                    "synonyms": [
                        "bad",
                        "frightening"
                    ],
                    "antonyms": [
                        "wonderful",
                        "great",
                        "excellent"
                    ],
                    "common_phrases": [
                        "horrible weather",
                        "horrible smell",
                        "horrible taste",
                        "feel horrible"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно плохой или неприятный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ужасный",
                                "target_lang_sense_clarification": "очень плохой"
                            },
                            {
                                "target_lang_word": "отвратительный",
                                "target_lang_sense_clarification": "неприятный, мерзкий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c9d3ef0-d3ce-32f9-9a28-b4ec2a45feca",
                    "sense_definition": "Causing horror, shock, or intense fear.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causing_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Strangers were scared by the <w>horrible</w> mask he wore.",
                            "target_lang_translations": {
                                "ru": "Незнакомцев напугала <w>страшная</w> маска, которая была на нём."
                            }
                        },
                        {
                            "text": "It was a <w>horrible</w> accident that shocked the whole town.",
                            "target_lang_translations": {
                                "ru": "Это был <w>ужасный</w> несчастный случай, потрясший весь город."
                            }
                        }
                    ],
                    "synonyms": [
                        "frightening"
                    ],
                    "antonyms": [
                        "pleasant",
                        "delightful"
                    ],
                    "common_phrases": [
                        "horrible accident",
                        "horrible crime",
                        "horrible nightmare",
                        "horrible sight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий ужас, шок или сильный страх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страшный",
                                "target_lang_sense_clarification": "пугающий"
                            },
                            {
                                "target_lang_word": "ужасный",
                                "target_lang_sense_clarification": "трагический, шокирующий"
                            },
                            {
                                "target_lang_word": "чудовищный",
                                "target_lang_sense_clarification": "крайне жестокий или пугающий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "69df9712-cb50-3928-bfe3-8cf04eb030f1",
                    "sense_definition": "A thing that causes horror; a terrifying thing or prospect.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_thing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer listed every possible <w>horrible</w> that could result from the new law.",
                            "target_lang_translations": {
                                "ru": "Юрист перечислил все возможные <w>ужасы</w>, которые могли возникнуть из-за нового закона."
                            }
                        },
                        {
                            "text": "She tried to ignore the <w>horribles</w> of the war and focus on survival.",
                            "target_lang_translations": {
                                "ru": "Она пыталась не замечать <w>ужасов</w> войны и сосредоточиться на выживании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "parade of horribles"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечто, вызывающее ужас; пугающая вещь или перспектива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ужас",
                                "target_lang_sense_clarification": "пугающее событие или явление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af69bc8e-8205-3eef-b47a-374c6369b3cb",
                    "sense_definition": "A person wearing a comic or grotesque costume in a parade.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children dressed up as <w>horribles</w> for the annual town parade.",
                            "target_lang_translations": {
                                "ru": "Дети нарядились <w>ряжеными</w> для ежегодного городского парада."
                            }
                        },
                        {
                            "text": "He laughed at the <w>horribles</w> marching down the street in their strange masks.",
                            "target_lang_translations": {
                                "ru": "Он смеялся над <w>ряжеными</w>, марширующими по улице в своих странных масках."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек в комичном или гротескном костюме на параде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ряженый",
                                "target_lang_sense_clarification": "человек в маскарадном костюме"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
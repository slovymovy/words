            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2f88a7c7-badd-39b1-8074-5376d29e846a",
                    "sense_definition": "Być źródłem fizycznego bólu, powodować dolegliwości cielesne u kogoś (zwykle używane w 3. osobie).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cause_pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Od samego rana strasznie <boli> mnie głowa i nie mogę się na niczym skupić.",
                            "target_lang_translations": {
                                "ru": "С самого утра у меня ужасно <болит> голова, и я ни на чём не могу сосредоточиться."
                            }
                        },
                        {
                            "text": "Jeśli ten ząb będzie cię nadal <bolał>, musisz koniecznie pójść do dentysty.",
                            "target_lang_translations": {
                                "ru": "Если этот зуб продолжит у тебя <болеть>, тебе нужно обязательно пойти к стоматологу."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Śląsk Cieszyński) zjimać",
                        "gw. (Śląsk Cieszyński) tropić"
                    ],
                    "common_phrases": [
                        "głowa mnie boli",
                        "brzuch boli",
                        "gardło boli",
                        "strasznie boleć",
                        "nic nie boli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть источником физической боли, причинять телесные страдания (обычно используется в 3-м лице)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болеть",
                                "target_lang_sense_clarification": "о части тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f03f7aa7-d4eb-3112-b26e-9790e16764a0",
                    "sense_definition": "Sprawiać przykrość, wywoływać cierpienie psychiczne, ranić czyjeś uczucia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cause_pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego obojętność <bolała> ją znacznie bardziej niż jakiekolwiek kłótnie czy sprzeczki.",
                            "target_lang_translations": {
                                "ru": "Его безразличие <ранило> её гораздо больше, чем любые ссоры или перепалки."
                            }
                        },
                        {
                            "text": "Prawda czasem <boli>, ale jest lepsza niż najsłodsze kłamstwo.",
                            "target_lang_translations": {
                                "ru": "Правда иногда <причиняет боль>, но она лучше, чем самая сладкая ложь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prawda boli",
                        "serce boli",
                        "to bardzo boli",
                        "słowa bolą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинять огорчение, вызывать душевные страдания, ранить чьи-то чувства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ранить",
                                "target_lang_sense_clarification": "причинять душевную боль"
                            },
                            {
                                "target_lang_word": "причинять боль",
                                "target_lang_sense_clarification": "делать больно"
                            },
                            {
                                "target_lang_word": "задевать",
                                "target_lang_sense_clarification": "обижать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "293c2445-f082-3d06-b51f-52913fdc7103",
                    "sense_definition": "Odczuwać głęboki żal, smutek lub troskę z jakiegoś powodu; ubolewać nad czymś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "grief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary profesor często <bolał> nad upadkiem obyczajów wśród współczesnej młodzieży.",
                            "target_lang_translations": {
                                "ru": "Старый профессор часто <сокрушался> по поводу падения нравов среди современной молодёжи."
                            }
                        },
                        {
                            "text": "Wszyscy szczerze <bolejemy> z powodu tej ogromnej i niespodziewanej straty.",
                            "target_lang_translations": {
                                "ru": "Мы все искренне <скорбим> из-за этой огромной и неожиданной утраты."
                            }
                        }
                    ],
                    "synonyms": [
                        "ubolewać",
                        "żałować",
                        "współczuć",
                        "narzekać"
                    ],
                    "common_phrases": [
                        "boleć nad stratą",
                        "boleć nad losem",
                        "boleć nad upadkiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "często spotykane w literaturze"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствовать глубокую жалость, грусть или тревогу по какой-то причине; сокрушаться о чём-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сокрушаться",
                                "target_lang_sense_clarification": "сильно жалеть, печалиться"
                            },
                            {
                                "target_lang_word": "скорбеть",
                                "target_lang_sense_clarification": "испытывать глубокую печаль"
                            },
                            {
                                "target_lang_word": "болеть душой",
                                "target_lang_sense_clarification": "сильно переживать"
                            }
                        ]
                    }
                }
            ]
        }
    ]

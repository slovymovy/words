{
    "word_family": [
        "magisterium",
        "magisterka",
        "magisterski"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b7ffddc6-84ca-3f06-b5f8-c3c46d521ba8",
                    "sense_definition": "Osoba, która ukończyła wyższe studia i uzyskała tytuł zawodowy magistra.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "academic_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój starszy brat jest już <magistrem> ekonomii.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат уже <магистр> экономики."
                            }
                        },
                        {
                            "text": "Wszyscy <magistrowie> muszą odebrać swoje dyplomy w dziekanacie.",
                            "target_lang_translations": {
                                "ru": "Все <магистры> должны забрать свои дипломы в деканате."
                            }
                        }
                    ],
                    "synonyms": [
                        "mgr"
                    ],
                    "common_phrases": [
                        "pan magister",
                        "pani magister"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, окончивший высшее учебное заведение и получивший профессиональную степень магистра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магистр",
                                "target_lang_sense_clarification": "человек со степенью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54607b94-a5f2-3fc9-85d0-6a1373e80d67",
                    "sense_definition": "Tytuł zawodowy nadawany przez wyższe uczelnie po ukończeniu studiów drugiego stopnia lub jednolitych studiów magisterskich.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "academic_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po pięciu latach ciężkiej nauki w końcu obroniła <magistra>.",
                            "target_lang_translations": {
                                "ru": "После пяти лет тяжёлой учёбы она наконец защитила <магистерскую>."
                            }
                        },
                        {
                            "text": "Tytuł <magistra> jest niezbędny, aby rozpocząć studia doktoranckie.",
                            "target_lang_translations": {
                                "ru": "Степень <магистра> необходима, чтобы поступить в аспирантуру."
                            }
                        }
                    ],
                    "synonyms": [
                        "magisterium",
                        "mgr"
                    ],
                    "common_phrases": [
                        "tytuł magistra",
                        "obrona magistra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Профессиональная степень, присваиваемая высшими учебными заведениями после окончания магистратуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "степень магистра",
                                "target_lang_sense_clarification": "учёная степень"
                            },
                            {
                                "target_lang_word": "магистр",
                                "target_lang_sense_clarification": "разг. название степени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1793fe55-c126-3203-9288-a79b44f4e5e3",
                    "sense_definition": "Zwyczajowa, grzecznościowa forma zwracania się do aptekarza lub farmaceuty.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pharmacy_address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pani <magister> poleciła mi bardzo skuteczny syrop na kaszel.",
                            "target_lang_translations": {
                                "ru": "<Провизор> посоветовала мне очень эффективный сироп от кашля."
                            }
                        },
                        {
                            "text": "Dzień dobry, panie <magistrze>, czy dostanę lek przeciwbólowy bez recepty?",
                            "target_lang_translations": {
                                "ru": "Добрый день, <фармацевт>, получу ли я обезболивающее без рецепта?"
                            }
                        }
                    ],
                    "synonyms": [
                        "mgr",
                        "magisterka"
                    ],
                    "common_phrases": [
                        "proszę pani magister"
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционная вежливая форма обращения к аптекарю или фармацевту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провизор",
                                "target_lang_sense_clarification": "аптечный работник с высшим образованием"
                            },
                            {
                                "target_lang_word": "фармацевт",
                                "target_lang_sense_clarification": "аптекарь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
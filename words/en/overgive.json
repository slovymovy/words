    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb33a6d0-ad8c-3084-a91d-ffd878380fd9",
                    "sense_definition": "To give more than is necessary, reasonable, or healthy, often to the point of excess.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She realized she had a tendency to <w>overgive</w> in her relationships, leaving her exhausted.",
                            "target_lang_translations": {
                                "ru": "Она поняла, что имеет склонность <w>отдавать слишком много</w> в отношениях, что приводило её к истощению."
                            }
                        },
                        {
                            "text": "It is possible to <w>overgive</w> to your children and prevent them from becoming independent.",
                            "target_lang_translations": {
                                "ru": "Можно <w>отдавать</w> детям <w>слишком много</w> и тем самым помешать им стать независимыми."
                            }
                        }
                    ],
                    "synonyms": [
                        "lavish",
                        "shower"
                    ],
                    "antonyms": [
                        "withhold",
                        "hoard"
                    ],
                    "common_phrases": [
                        "tendency to overgive",
                        "stop overgiving"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать больше, чем необходимо, разумно или полезно, часто до степени излишества или в ущерб себе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдавать слишком много",
                                "target_lang_sense_clarification": "общая фраза, описывающая чрезмерную отдачу сил или ресурсов"
                            },
                            {
                                "target_lang_word": "чрезмерно жертвовать собой",
                                "target_lang_sense_clarification": "в контексте отношений и эмоционального выгорания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce018faa-58e3-3d6f-9ddf-1d9dca43af60",
                    "sense_definition": "To surrender, hand over, or relinquish possession of something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The commander refused to <w>overgive</w> the fortress despite the long siege.",
                            "target_lang_translations": {
                                "ru": "Командир отказался <w>сдать</w> крепость, несмотря на долгую осаду."
                            }
                        },
                        {
                            "text": "He was forced to <w>overgive</w> his title and lands to the conqueror.",
                            "target_lang_translations": {
                                "ru": "Он был вынужден <w>передать</w> свой титул и земли завоевателю."
                            }
                        }
                    ],
                    "synonyms": [
                        "surrender",
                        "relinquish",
                        "yield",
                        "hand over"
                    ],
                    "antonyms": [
                        "keep",
                        "retain",
                        "hold"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly found in older literature"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сдавать, передавать или отказываться от владения чем-либо (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "крепость, позицию или территорию"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "титул, земли или владения"
                            },
                            {
                                "target_lang_word": "уступать",
                                "target_lang_sense_clarification": "под давлением или по принуждению"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
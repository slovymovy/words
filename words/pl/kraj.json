            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0185d2d-a937-38e9-806d-1c5bc735b320",
                    "sense_definition": "Terytorium zamieszkiwane przez naród, posiadające własną organizację polityczną i granice; państwo.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geo_political",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polska to piękny i bezpieczny <w>kraj</w>.",
                            "target_lang_translations": {
                                "ru": "Польша — красивая и безопасная <w>страна</w>."
                            }
                        },
                        {
                            "text": "W wakacje planujemy wyjechać w ciepły <w>kraj</w>.",
                            "target_lang_translations": {
                                "ru": "В отпуск мы планируем поехать в тёплые <w>края</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "państwo"
                    ],
                    "common_phrases": [
                        "kraj ojczysty",
                        "wyjechać z kraju",
                        "powrót do kraju",
                        "kraj pochodzenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория, населенная народом, имеющая собственную политическую организацию и границы; государство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страна",
                                "target_lang_sense_clarification": "территория, государство"
                            },
                            {
                                "target_lang_word": "государство",
                                "target_lang_sense_clarification": "политическая организация"
                            },
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "местность, часто во мн.ч. (тёплые края)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ca7fb44-61d3-39e1-81c1-8b4429b0c88e",
                    "sense_definition": "Określony obszar geograficzny, często wyróżniający się specyficznymi cechami przyrodniczymi lub historycznymi.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "geo_political",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten górzysty <w>kraj</w> przyciąga wielu turystów.",
                            "target_lang_translations": {
                                "ru": "Этот горный <w>край</w> привлекает много туристов."
                            }
                        },
                        {
                            "text": "Poeta często wspominał swój rodzinny <w>kraj</w>.",
                            "target_lang_translations": {
                                "ru": "Поэт часто вспоминал свой родной <w>край</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kraina",
                        "region"
                    ],
                    "common_phrases": [
                        "kraj lat dziecinnych",
                        "zwiedzać kraj"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенная географическая область, часто отличающаяся специфическими природными или историческими особенностями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "область, местность, родина"
                            },
                            {
                                "target_lang_word": "земля",
                                "target_lang_sense_clarification": "поэтическое: территория"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ba947db-3368-33d0-900e-596897693cba",

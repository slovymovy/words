    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a2dfa181-cc33-3f22-8c85-bc648e9b0582",
                    "sense_definition": "Using new methods, ideas, or styles that are not yet established or proven.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "innovative_trial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist is known for her <w>experimental</w> approach to painting.",
                            "target_lang_translations": {
                                "ru": "Художница известна своим <w>экспериментальным</w> подходом к живописи."
                            }
                        },
                        {
                            "text": "We are currently testing an <w>experimental</w> engine design to improve fuel efficiency.",
                            "target_lang_translations": {
                                "ru": "В настоящее время мы тестируем <w>экспериментальную</w> конструкцию двигателя для повышения топливной эффективности."
                            }
                        }
                    ],
                    "synonyms": [
                        "innovative",
                        "trial",
                        "pilot",
                        "avant-garde"
                    ],
                    "antonyms": [
                        "conventional",
                        "traditional",
                        "proven",
                        "established"
                    ],
                    "common_phrases": [
                        "experimental music",
                        "experimental stage",
                        "experimental drug",
                        "experimental treatment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основанный на новых, еще не проверенных методах или идеях; пробный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспериментальный",
                                "target_lang_sense_clarification": "новаторский, пробный"
                            },
                            {
                                "target_lang_word": "опытный",
                                "target_lang_sense_clarification": "пилотный, пробный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01131058-bc5e-3d80-81d2-9cf817d55846",
                    "sense_definition": "Relating to or based on scientific experiments.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "scientific",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chemistry is an <w>experimental</w> science that requires careful observation.",
                            "target_lang_translations": {
                                "ru": "Химия — это <w>экспериментальная</w> наука, требующая тщательного наблюдения."
                            }
                        },
                        {
                            "text": "The scientists gathered <w>experimental</w> evidence to support their theory.",
                            "target_lang_translations": {
                                "ru": "Ученые собрали <w>экспериментальные</w> доказательства в поддержку своей теории."
                            }
                        }
                    ],
                    "synonyms": [
                        "empirical",
                        "scientific"
                    ],
                    "antonyms": [
                        "theoretical"
                    ],
                    "common_phrases": [
                        "experimental data",
                        "experimental results",
                        "experimental method",
                        "experimental physics"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к научным опытам или основанный на них."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспериментальный",
                                "target_lang_sense_clarification": "научный, основанный на опыте"
                            },
                            {
                                "target_lang_word": "опытный",
                                "target_lang_sense_clarification": "эмпирический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "512b6d64-c1bd-36df-876b-84acce57f495",
                    "sense_definition": "Used as a subject or tool within a scientific test.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "scientific",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>experimental</w> subjects were monitored closely for any side effects.",
                            "target_lang_translations": {
                                "ru": "За <w>подопытными</w> внимательно наблюдали на предмет побочных эффектов."
                            }
                        },
                        {
                            "text": "Researchers removed the glands in <w>experimental</w> animals to study the effects.",
                            "target_lang_translations": {
                                "ru": "Исследователи удалили железы у <w>подопытных</w> животных, чтобы изучить последствия."
                            }
                        }
                    ],
                    "synonyms": [
                        "test"
                    ],
                    "common_phrases": [
                        "experimental animal",
                        "experimental group"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используемый в качестве объекта исследования в научном опыте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подопытный",
                                "target_lang_sense_clarification": "об объекте эксперимента"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2ad1da4e-1f40-3ac0-bddc-e4b628f6c2ab",
                    "sense_definition": "A subject, such as a person or animal, participating in an experiment.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>experimentals</w> received the new medication while the others got a placebo.",
                            "target_lang_translations": {
                                "ru": "<w>Испытуемые</w> получили новое лекарство, в то время как остальные получили плацебо."
                            }
                        },
                        {
                            "text": "We need to ensure the safety of all <w>experimentals</w> involved in the trial.",
                            "target_lang_translations": {
                                "ru": "Нам необходимо обеспечить безопасность всех <w>подопытных</w>, участвующих в испытании."
                            }
                        }
                    ],
                    "synonyms": [
                        "subject"
                    ],
                    "target_lang_definitions": {
                        "ru": "Субъект (человек или животное), участвующий в эксперименте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подопытный",
                                "target_lang_sense_clarification": "объект эксперимента"
                            },
                            {
                                "target_lang_word": "испытуемый",
                                "target_lang_sense_clarification": "участник эксперимента"
                            }
                        ]
                    }
                }
            ]
        }
    ]

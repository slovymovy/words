            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3cdef44f-4939-3361-a316-e81748c2ff73",
                    "sense_definition": "Otwarta, ogrodzona balustradą płyta wystająca ze ściany budynku, połączona z wnętrzem drzwiami.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem bardzo lubię czytać książki i pić kawę na <w>balkonie</w>.",
                            "target_lang_translations": {
                                "ru": "Летом я очень люблю читать книги и пить кофе на <w>балконе</w>."
                            }
                        },
                        {
                            "text": "Wyszedłem na <w>balkon</w>, żeby zobaczyć pokaz sztucznych ogni.",
                            "target_lang_translations": {
                                "ru": "Я вышел на <w>балкон</w>, чтобы посмотреть фейерверк."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyjść na balkon",
                        "stać na balkonie",
                        "mieszkanie z balkonem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытая, огражденная перилами площадка, выступающая из стены здания и соединенная с внутренним помещением дверью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балкон",
                                "target_lang_sense_clarification": "архитектурный элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d31bb361-e506-3f64-ab6e-5e930642eb43",
                    "sense_definition": "Wyższa część widowni w teatrze, kinie lub sali koncertowej, znajdująca się na piętrze.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiliśmy bilety na <w>balkon</w>, bo na dole nie było już wolnych miejsc.",
                            "target_lang_translations": {
                                "ru": "Мы купили билеты на <w>балкон</w>, потому что внизу уже не было свободных мест."
                            }
                        },
                        {
                            "text": "Z pierwszego <w>balkonu</w> mieliśmy doskonały widok na całą scenę.",
                            "target_lang_translations": {
                                "ru": "С первого <w>балкона</w> у нас был отличный вид на всю сцену."
                            }
                        }
                    ],
                    "antonyms": [
                        "parter"
                    ],
                    "common_phrases": [
                        "bilet na balkon",
                        "miejsce na balkonie",
                        "siedzieć na balkonie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхний ярус зрительного зала в театре, кино или концертном зале."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балкон",

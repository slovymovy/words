                    "common_phrases": [
                        "aan wal",
                        "van wal steken",
                        "aan lager wal raken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суша или пристань, особенно в противопоставлении воде или кораблю",
                        "en": "The solid land or the quay, especially in contrast to the water or a ship"
                    },

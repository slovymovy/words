    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "18127aa3-65d8-3398-925b-23c9141c0230",
                    "sense_definition": "Taki, który nie opowiada się za żadną ze stron w sporze i jest bezstronny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "impartiality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sędzia w tym meczu musi być całkowicie <w>neutralny</w>.",
                            "target_lang_translations": {
                                "ru": "Судья в этом матче должен быть полностью <w>нейтральным</w>."
                            }
                        },
                        {
                            "text": "Staram się pozostać <w>neutralny</w>, gdy moi przyjaciele się kłócą.",
                            "target_lang_translations": {
                                "ru": "Я стараюсь оставаться <w>нейтральным</w>, когда мои друзья ссорятся."
                            }
                        }
                    ],
                    "synonyms": [
                        "obiektywny",
                        "wyważony"
                    ],
                    "antonyms": [
                        "stronniczy"
                    ],
                    "common_phrases": [
                        "neutralny grunt",
                        "neutralny obserwator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто не принимает ничью сторону в споре; беспристрастный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтральный",
                                "target_lang_sense_clarification": "не занимающий ничью сторону"
                            },
                            {
                                "target_lang_word": "беспристрастный",
                                "target_lang_sense_clarification": "справедливый, объективный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b7693ee-ff8d-3c4b-9c90-8693c3559e14",
                    "sense_definition": "Taki, który zachowuje neutralność polityczną i nie uczestniczy w wojnie między innymi państwami.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "impartiality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szwajcaria jest znana jako kraj <w>neutralny</w>.",
                            "target_lang_translations": {
                                "ru": "Швейцария известна как <w>нейтральная</w> страна."
                            }
                        },
                        {
                            "text": "Statek wpłynął na wody <w>neutralne</w>, aby uniknąć ataku.",
                            "target_lang_translations": {
                                "ru": "Корабль вошел в <w>нейтральные</w> воды, чтобы избежать атаки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "państwo neutralne",
                        "terytorium neutralne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придерживающийся нейтралитета; не участвующий в военных конфликтах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтральный",
                                "target_lang_sense_clarification": "о государстве или территории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d9fea57-f6ab-39e1-b440-1dbe60c94dc5",
                    "sense_definition": "Taki, który nie wywołuje skutków chemicznych lub nie ma charakterystycznych cech, takich jak smak czy zapach.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten szampon ma <w>neutralny</w> odczyn pH dla skóry.",
                            "target_lang_translations": {
                                "ru": "Этот шампунь имеет <w>нейтральный</w> pH для кожи."
                            }
                        },
                        {
                            "text": "Woda destylowana ma smak <w>neutralny</w>.",
                            "target_lang_translations": {
                                "ru": "Дистиллированная вода имеет <w>нейтральный</w> вкус."
                            }
                        }
                    ],
                    "synonyms": [
                        "obojętny",
                        "bezpieczny"
                    ],
                    "common_phrases": [
                        "odczyn neutralny",
                        "neutralny zapach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не обладающий ярко выраженными свойствами (вкусом, запахом) или химически неактивный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтральный",
                                "target_lang_sense_clarification": "о вкусе, запахе, pH"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42eb472a-cdc2-3da0-ab5a-9e9c63512bfd",
                    "sense_definition": "Taki, który nie wyraża ocen, sądów lub emocji.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego głos był <w>neutralny</w>, więc nie wiedziałem, co myśli.",
                            "target_lang_translations": {
                                "ru": "Его голос был <w>нейтральным</w>, поэтому я не знал, о чем он думает."
                            }
                        },
                        {
                            "text": "Urzędnik zachował <w>neutralny</w> wyraz twarzy podczas rozmowy.",
                            "target_lang_translations": {
                                "ru": "Чиновник сохранял <w>нейтральное</w> выражение лица во время разговора."
                            }
                        }
                    ],
                    "synonyms": [
                        "obojętny"
                    ],
                    "antonyms": [
                        "emocjonalny"
                    ],
                    "common_phrases": [
                        "neutralny ton",
                        "neutralny wyraz twarzy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не выражающий эмоций или личного отношения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нейтральный",
                                "target_lang_sense_clarification": "сдержанный, без эмоций"
                            },
                            {
                                "target_lang_word": "бесстрастный",
                                "target_lang_sense_clarification": "не выражающий чувств"
                            }
                        ]
                    }
                }
            ]
        }
    ]

        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3ded6688-d0f9-3c1d-8e16-a03b6eb4c6cc",
                    "sense_definition": "Вступить в знакомство с кем-либо, узнать кого-то впервые, представиться друг другу.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "meeting_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Приятно <w>познакомиться</w>, меня зовут Александр.",
                            "target_lang_translations": {
                                "en": "Nice to <w>meet</w> you, my name is Alexander."
                            }
                        },
                        {
                            "text": "Я давно хотел <w>познакомиться</w> с твоим братом.",
                            "target_lang_translations": {
                                "en": "I have wanted to <w>meet</w> your brother for a long time."
                            }
                        }
                    ],
                    "synonyms": [
                        "встретиться"
                    ],
                    "antonyms": [
                        "раззнакомиться"
                    ],
                    "common_phrases": [
                        "приятно познакомиться",
                        "рад познакомиться",
                        "хочу познакомиться",
                        "давай познакомимся"
                    ],
                    "target_lang_definitions": {
                        "en": "To meet someone for the first time; to establish a social connection."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meet",
                                "target_lang_sense_clarification": "to be introduced to someone"
                            },
                            {
                                "target_lang_word": "get to know",
                                "target_lang_sense_clarification": "to start learning about someone"
                            },
                            {
                                "target_lang_word": "make the acquaintance",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfec1948-2a3c-3ef3-9293-2a205baa27a3",
                    "sense_definition": "Получить сведения о чём-либо, изучить или осмотреть что-то новое для себя.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "learning_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы приехали в город, чтобы <w>познакомиться</w> с местной культурой.",
                            "target_lang_translations": {
                                "en": "The tourists came to the city to <w>get to know</w> the local culture."
                            }
                        },
                        {
                            "text": "Вам необходимо внимательно <w>познакомиться</w> с инструкцией перед началом работы.",
                            "target_lang_translations": {
                                "en": "You need to carefully <w>familiarize yourself</w> with the instructions before starting work."
                            }
                        }
                    ],
                    "synonyms": [
                        "ознакомиться",
                        "узнать"
                    ],
                    "common_phrases": [
                        "познакомиться с культурой",
                        "познакомиться с документами",
                        "поближе познакомиться",
                        "познакомиться с историей"
                    ],
                    "target_lang_definitions": {
                        "en": "To learn about something; to examine or study something new."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "familiarize oneself",
                                "target_lang_sense_clarification": "with information, rules, or documents"
                            },
                            {
                                "target_lang_word": "get to know",
                                "target_lang_sense_clarification": "a place, culture, or subject"
                            }
                        ]
                    }
                }
            ]
        }
    ]

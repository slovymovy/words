            "senses": [
                {
                    "sense_id": "52353b87-50e5-3ff5-9f1c-bafb9d5b91dc",
                    "sense_definition": "Czynność, którą się wykonuje; sposób spędzania czasu, hobby lub zadanie do wykonania.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rozwiązywanie krzyżówek uważam za ciekawe <w>zajęcie</w>.",
                            "target_lang_translations": {
                                "ru": "Я считаю разгадывание кроссвордов интересным <w>занятием</w>."
                            }
                        },
                        {
                            "text": "W deszczowe dni dzieciom trudno znaleźć sobie jakieś <w>zajęcie</w> w domu.",
                            "target_lang_translations": {
                                "ru": "В дождливые дни детям трудно найти себе какое-нибудь <w>занятие</w> дома."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ciekawe zajęcie",
                        "szukać zajęcia",
                        "domowe zajęcia",
                        "zajęcie czasu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, которое кто-либо выполняет; способ времяпрепровождения, хобби или задача."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "занятие",
                                "target_lang_sense_clarification": "времяпрепровождение"
                            },
                            {
                                "target_lang_word": "дело",
                                "target_lang_sense_clarification": "задача, то, чем занимаются"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36c64082-3103-3922-89f8-39d2b8a27a1a",
                    "sense_definition": "Praca zarobkowa, stałe źródło utrzymania lub obowiązki zawodowe.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat od miesiąca nie może znaleźć żadnego stałego <w>zajęcia</w>.",
                            "target_lang_translations": {
                                "ru": "Мой брат уже месяц не может найти никакого постоянного <w>занятия</w>."
                            }
                        },
                        {
                            "text": "Jego obecne <w>zajęcie</w> pozwala mu na częste podróże służbowe.",
                            "target_lang_translations": {
                                "ru": "Его нынешнее <w>занятие</w> позволяет ему часто ездить в командировки."
                            }
                        }
                    ],
                    "synonyms": [
                        "profesja"
                    ],
                    "common_phrases": [
                        "stałe zajęcie",
                        "płatne zajęcie",
                        "główne zajęcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оплачиваемая работа, постоянный источник дохода или профессиональные обязанности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работа",
                                "target_lang_sense_clarification": "труд, служба"
                            },
                            {
                                "target_lang_word": "занятие",
                                "target_lang_sense_clarification": "род деятельности, профессия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31cd404a-8c75-3fb2-adf9-9a3ddfde5d2f",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>zająć</w>: czynność polegająca na zdobyciu czegoś, wejściu na jakiś teren lub wypełnieniu miejsca.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę o szybkie <w>zajęcie</w> miejsc w autokarze, ponieważ zaraz ruszamy.",
                            "target_lang_translations": {
                                "ru": "Прошу быстро <w>занять</w> места в автобусе, так как мы скоро отправляемся."
                            }
                        },
                        {
                            "text": "<w>Zajęcie</w> miasta przez wrogie wojska trwało zaledwie kilka godzin.",
                            "target_lang_translations": {
                                "ru": "<w>Занятие</w> города вражескими войсками длилось всего несколько часов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zajęcie miejsca",
                        "zajęcie terytorium",
                        "zajęcie stanowiska"
                    ],
                    "traits": [
                        {

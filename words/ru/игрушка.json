                            },
                            {
                                "target_lang_word": "plaything",
                                "target_lang_sense_clarification": "victim of fate"
                            },
                            {
                                "target_lang_word": "toy",
                                "target_lang_sense_clarification": "tool in someone's hands"
                            }
                        ]

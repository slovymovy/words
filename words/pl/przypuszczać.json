    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "badd8afe-11c6-3593-a01e-c025ebdb3411",
                    "sense_definition": "Uważać coś za prawdopodobne, domyślać się czegoś, nie mając jednak całkowitej pewności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Przypuszczam</w>, że pociąg spóźni się z powodu śnieżycy.",
                            "target_lang_translations": {
                                "ru": "<w>Предполагаю</w>, что поезд опоздает из-за метели."
                            }
                        },
                        {
                            "text": "Lekarz <w>przypuszcza</w>, że infekcja ustąpi po kilku dniach leczenia.",
                            "target_lang_translations": {
                                "ru": "Врач <w>предполагает</w>, что инфекция пройдет через несколько дней лечения."
                            }
                        }
                    ],
                    "synonyms": [
                        "sądzić",
                        "podejrzewać",
                        "zakładać",
                        "domyślać się",
                        "mniemać"
                    ],
                    "antonyms": [
                        "wiedzieć",
                        "mieć pewność"
                    ],
                    "common_phrases": [
                        "przypuszczać, że"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать что-либо вероятным, догадываться о чем-либо, не имея полной уверенности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предполагать",
                                "target_lang_sense_clarification": "считать возможным, думать"
                            },
                            {
                                "target_lang_word": "допускать",
                                "target_lang_sense_clarification": "считать возможным, не исключать"
                            },
                            {
                                "target_lang_word": "полагать",
                                "target_lang_sense_clarification": "думать, считать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8219748d-7779-3a1a-a41b-4b3959d56afa",
                    "sense_definition": "Rozpocząć gwałtowne działanie zbrojne lub agresywne przeciwko komuś (zwykle w połączeniu z rzeczownikami 'atak' lub 'szturm').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "O świcie wojska nieprzyjaciela <w>przypuściły</w> zmasowany atak na miasto.",
                            "target_lang_translations": {
                                "ru": "На рассвете вражеские войска <w>предприняли</w> массированную атаку на город."
                            }
                        },
                        {
                            "text": "Generał zdecydował <w>przypuścić</w> szturm na pozycje wroga.",
                            "target_lang_translations": {
                                "ru": "Генерал решил <w>начать</w> штурм позиций врага."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpocząć",
                        "zainicjować"
                    ],
                    "common_phrases": [
                        "przypuścić atak",
                        "przypuścić szturm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать резкие военные или агрессивные действия (обычно атаку или штурм)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предпринимать",
                                "target_lang_sense_clarification": "совершать действие (атаку)"
                            },
                            {
                                "target_lang_word": "начинать",
                                "target_lang_sense_clarification": "приступать к действию (штурму)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "497647d2-0587-3374-8195-b47138f2b9dc",
                    "sense_definition": "Pozwolić komuś zbliżyć się do kogoś lub czegoś; dać komuś do czegoś dostęp.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary hrabia nikogo do siebie nie <w>przypuszczał</w> w czasie choroby.",
                            "target_lang_translations": {
                                "ru": "Старый граф никого к себе не <w>подпускал</w> во время болезни."
                            }
                        },
                        {
                            "text": "Nie <w>przypuszczano</w> go do tajemnic, które znała tylko starszyzna rodu.",
                            "target_lang_translations": {
                                "ru": "Его не <w>допускали</w> к тайнам, которые знали только старейшины рода."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopuścić",
                        "pozwolić"
                    ],
                    "antonyms": [
                        "zabronić",
                        "odsunąć"
                    ],
                    "common_phrases": [
                        "nie przypuszczać do siebie"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie rzadziej używane, częściej spotykane w literaturze"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Позволить кому-либо приблизиться или получить доступ к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подпускать",

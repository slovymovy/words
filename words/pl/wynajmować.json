                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снимать",
                                "target_lang_sense_clarification": "жильё, помещение"
                            },
                            {
                                "target_lang_word": "арендовать",
                                "target_lang_sense_clarification": "имущество, транспорт"
                            },
                            {
                                "target_lang_word": "брать напрокат",
                                "target_lang_sense_clarification": "вещи, транспорт"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9dea59df-f06f-32ac-aa8d-fb49de5a4b2e",
                    "sense_definition": "Roślina, której owoce, korzenie, liście lub kwiatostany służą człowiekowi za pokarm.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zalecił mi jeść więcej świeżych <w>warzyw</w>.",
                            "target_lang_translations": {
                                "ru": "Врач посоветовал мне есть больше свежих <w>овощей</w>."
                            }
                        },
                        {
                            "text": "Na targu kupiliśmy owoce i <w>warzywa</w> na cały tydzień.",
                            "target_lang_translations": {
                                "ru": "На рынке мы купили фрукты и <w>овощи</w> на всю неделю."
                            }
                        }
                    ],
                    "synonyms": [
                        "jarzyna",
                        "zielenina"
                    ],
                    "common_phrases": [
                        "świeże warzywa",
                        "owoce i warzywa",
                        "warzywa na patelnię",
                        "zupa z warzyw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение, плоды, корни, листья или соцветия которого служат пищей человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "овощ",
                                "target_lang_sense_clarification": "растение или плод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bcbc094-c54e-3efe-aba5-0096522fde02",
                    "sense_definition": "Człowiek chory, pozbawiony kontaktu ze światem zewnętrznym z powodu trwałego uszkodzenia mózgu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po tragicznym wypadku motocyklowym chłopak został <w>warzywem</w>.",
                            "target_lang_translations": {
                                "ru": "После трагической аварии на мотоцикле парень стал <w>овощем</w>."
                            }
                        },
                        {
                            "text": "Boję się, że na starość będę żyć w szpitalu jak <w>warzywo</w>.",
                            "target_lang_translations": {
                                "ru": "Я боюсь, что в старости буду жить в больнице как <w>овощ</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zrobić z kogoś warzywo",
                        "leżeć jak warzywo"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "określenie potoczne, często uważane za obraźliwe"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Больной человек, лишенный контакта с внешним миром из-за необратимого повреждения мозга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "овощ",
                                "target_lang_sense_clarification": "перен. о больном человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "000bf22f-7fd8-3c8f-9ceb-95de1e73c868",
                    "sense_definition": "Dawne określenie na zupę, w szczególności na rosół.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodyni podała gościom gorące <w>warzywo</w> z makaronem.",
                            "target_lang_translations": {
                                "ru": "Хозяйка подала гостям горячую <w>похлёбку</w> с лапшой."
                            }
                        },
                        {
                            "text": "To staropolskie <w>warzywo</w> miało wyjątkowy, mięsny smak.",
                            "target_lang_translations": {
                                "ru": "Это старопольское <w>варево</w> имело исключительный мясной вкус."
                            }
                        }
                    ],
                    "synonyms": [
                        "rosół"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "znaczenie historyczne, obecnie nieużywane"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название супа, в частности бульона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "похлёбка",
                                "target_lang_sense_clarification": "суп (устар.)"
                            },
                            {
                                "target_lang_word": "варево",
                                "target_lang_sense_clarification": "жидкое кушанье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df494f3b-16f3-3c09-8434-c0cb078f8447",
                    "sense_definition": "Czynność gotowania potraw lub warzenia piwa.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mnisi rozpoczęli poranne <w>warzywo</w> piwa w klasztorze.",
                            "target_lang_translations": {
                                "ru": "Монахи начали утреннюю <w>варку</w> пива в монастыре."
                            }
                        },
                        {
                            "text": "Długie <w>warzywo</w> strawy wydobyło z niej głęboki aromat.",
                            "target_lang_translations": {
                                "ru": "Долгая <w>варка</w> еды раскрыла её глубокий аромат."
                            }
                        }
                    ],
                    "synonyms": [
                        "warzenie"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "znaczenie etymologiczne, obecnie zastąpione słowem warzenie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по приготовлению пищи или варке пива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "варка",
                                "target_lang_sense_clarification": "процесс кипячения"
                            },
                            {
                                "target_lang_word": "варение",
                                "target_lang_sense_clarification": "процесс приготовления"
                            }
                        ]
                    }
                }
            ]
        }
    ]

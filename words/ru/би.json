                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Машина сделала «<w>би</w>», предупреждая пешеходов об опасности.",
                            "target_lang_translations": {
                                "en": "The car went \"<w>beep</w>\", warning pedestrians of danger."
                            }

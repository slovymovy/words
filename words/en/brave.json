{
    "word_family": [
        "bravery",
        "bravely"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "86019056-5572-3d30-b658-ffa2305ec781",
                    "sense_definition": "Strong in the face of fear; courageous.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "courageous_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>brave</w> firefighter saved the child from the burning house.",
                            "target_lang_translations": {
                                "ru": "<w>Храбрый</w> пожарный спас ребенка из горящего дома."
                            }
                        },
                        {
                            "text": "You must be <w>brave</w> and tell the truth about what happened.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть <w>смелым</w> и сказать правду о том, что случилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "courageous",
                        "fearless",
                        "bold",
                        "daring",
                        "heroic",
                        "unafraid",
                        "valiant",
                        "dauntless",
                        "intrepid",
                        "plucky",
                        "gutsy"
                    ],
                    "antonyms": [
                        "cowardly",
                        "afraid",
                        "fearful",
                        "timid",
                        "frightened",
                        "weak",
                        "shy"
                    ],
                    "common_phrases": [
                        "be brave",
                        "brave face",
                        "brave man",
                        "brave soldier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный перед лицом страха; мужественный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "храбрый",
                                "target_lang_sense_clarification": "не испытывающий страха; самый общий термин"
                            },
                            {
                                "target_lang_word": "смелый",
                                "target_lang_sense_clarification": "решительный, не боящийся трудностей"
                            },
                            {
                                "target_lang_word": "отважный",
                                "target_lang_sense_clarification": "очень смелый, полный отваги (часто в книжном стиле)"
                            },
                            {
                                "target_lang_word": "бесстрашный",
                                "target_lang_sense_clarification": "не знающий страха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe5bd5c6-d13d-3829-b947-1ec3dbbccf9f",
                    "sense_definition": "Having any sort of superiority or excellence.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "excellent_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a <w>brave</w> day for a celebration in the village.",
                            "target_lang_translations": {
                                "ru": "Это был <w>славный</w> день для праздника в деревне."
                            }
                        },
                        {
                            "text": "He gave a <w>brave</w> speech that impressed the entire audience.",
                            "target_lang_translations": {
                                "ru": "Он произнес <w>прекрасную</w> речь, которая впечатлила всю аудиторию."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий превосходством или совершенством (устар.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "славный",
                                "target_lang_sense_clarification": "замечательный, достойный похвалы"
                            },
                            {
                                "target_lang_word": "прекрасный",
                                "target_lang_sense_clarification": "очень хороший, превосходный"
                            },
                            {
                                "target_lang_word": "дивный",
                                "target_lang_sense_clarification": "удивительный, чудесный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f633edc2-a25a-3b1b-aee4-83acbdfc87e1",
                    "sense_definition": "Making a fine show or display.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "showy_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers made a <w>brave</w> sight in their colorful uniforms.",
                            "target_lang_translations": {
                                "ru": "Солдаты представляли собой <w>эффектное</w> зрелище в своих красочных мундирах."
                            }
                        },
                        {
                            "text": "The ship looked <w>brave</w> with all its flags flying in the wind.",
                            "target_lang_translations": {
                                "ru": "Корабль выглядел <w>нарядно</w> со всеми своими флагами, развевающимися на ветру."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выглядящий нарядно или эффектно (устар.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарядный",
                                "target_lang_sense_clarification": "красиво одетый или украшенный"
                            },
                            {
                                "target_lang_word": "эффектный",
                                "target_lang_sense_clarification": "производящий сильное впечатление внешним видом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1364f777-8e76-3ae1-8b3e-5c6a3a36c298",
                    "sense_definition": "Foolish or unwise.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "foolish_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a <w>brave</w> decision to ignore the warnings of the locals.",
                            "target_lang_translations": {
                                "ru": "Это было <w>самонадеянное</w> решение — проигнорировать предупреждения местных жителей."
                            }
                        },
                        {
                            "text": "He is <w>brave</w> to think he can finish the work without help.",
                            "target_lang_translations": {
                                "ru": "Он <w>глуп</w>, если думает, что сможет закончить работу без помощи."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Scottish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Глупый или неразумный (диал.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самонадеянный",
                                "target_lang_sense_clarification": "излишне уверенный в себе, опрометчивый"
                            },
                            {
                                "target_lang_word": "глупый",
                                "target_lang_sense_clarification": "неразумный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c6268581-7953-32b2-abdf-7172adff61fd",
                    "sense_definition": "To encounter with courage and fortitude, to defy, to provoke.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "endure_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to <w>brave</w> the storm to get to the concert on time.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>бросить вызов</w> шторму, чтобы попасть на концерт вовремя."
                            }
                        },
                        {
                            "text": "She had to <w>brave</w> the heavy traffic to arrive at the meeting.",
                            "target_lang_translations": {
                                "ru": "Ей пришлось <w>преодолеть</w> сильные пробки, чтобы попасть на встречу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brave the storm",
                        "brave the elements",
                        "brave the cold",
                        "brave the weather"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смело встречать (опасность), бросать вызов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросать вызов",
                                "target_lang_sense_clarification": "смело противостоять чему-либо"
                            },
                            {
                                "target_lang_word": "преодолевать",
                                "target_lang_sense_clarification": "справляться с трудностями или препятствиями"
                            },
                            {
                                "target_lang_word": "смело встречать",
                                "target_lang_sense_clarification": "не бояться столкнуться с чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3d470d6-99ad-36f8-8790-af2a210660bb",
                    "sense_definition": "To adorn; to make fine or showy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "adorn_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>braved</w> herself with jewels for the grand ball.",
                            "target_lang_translations": {
                                "ru": "Она <w>украсила</w> себя драгоценностями для грандиозного бала."
                            }
                        },
                        {
                            "text": "The hall was <w>braved</w> with flowers and ribbons for the feast.",
                            "target_lang_translations": {
                                "ru": "Зал был <w>наряжен</w> цветами и лентами к пиру."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшать, наряжать (устар.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украшать",
                                "target_lang_sense_clarification": "делать красивым, придавать нарядный вид"
                            },
                            {
                                "target_lang_word": "наряжать",
                                "target_lang_sense_clarification": "одевать красиво или празднично"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6b735bbf-6fa8-3da0-88c3-eeea14a514d2",

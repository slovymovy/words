    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fdc72cab-0ef1-33ac-9942-d889f21acbba",
                    "sense_definition": "To have something or someone as a part of a whole; to contain.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "containment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The price of the hotel room <w>includes</w> breakfast and dinner."
                        },
                        {
                            "text": "Does this history book <w>include</w> a section on the Roman Empire?"
                        }
                    ],
                    "synonyms": [
                        "contain",
                        "comprise",
                        "involve",
                        "cover",
                        "encompass",
                        "embody",
                        "come with"
                    ],
                    "antonyms": [
                        "exclude",
                        "lack",
                        "omit"
                    ],
                    "common_phrases": [
                        "include tax",
                        "include details",
                        "not include",
                        "include everything"
                    ]
                },
                {
                    "sense_id": "a187588c-45c0-3a14-a590-98a4a88e0b96",
                    "sense_definition": "To make someone or something part of a group, list, or set.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>include</w> my name on the guest list for the party."
                        },
                        {
                            "text": "The coach decided to <w>include</w> the new player in the starting lineup."
                        }
                    ],
                    "synonyms": [
                        "add",
                        "incorporate",
                        "count",
                        "admit"
                    ],
                    "antonyms": [
                        "exclude",
                        "drop",
                        "omit"
                    ],
                    "common_phrases": [
                        "include in the list",
                        "include someone in",
                        "include a note"
                    ]
                },
                {
                    "sense_id": "99a1ae24-99ac-3564-8440-82861f5ea0fd",
                    "sense_definition": "To use a directive that allows the use of source code from another file.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>include</w> the standard library to use these functions."
                        },
                        {
                            "text": "The script will automatically <w>include</w> the configuration file at startup."
                        }
                    ],
                    "common_phrases": [
                        "include a library",
                        "include a file",
                        "include a header"
                    ]
                },
                {
                    "sense_id": "7a0d418f-da2e-3dfc-a33a-69a2ac500bbf",
                    "sense_definition": "To enclose or confine within limits.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "enclosure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I could have willingly ranged, but these straits wherein I am <w>included</w> will not permit."
                        },
                        {
                            "text": "The ancient walls <w>include</w> the inner sanctum of the temple."
                        }
                    ],
                    "synonyms": [
                        "enclose",
                        "encircle",
                        "circumscribe"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Original Latin sense."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4b5b6c53-49de-35fd-b893-9f61024fe050",
                    "sense_definition": "A residential area located on the outskirts of a city or large town.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "residential_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to move to a quiet <w>suburb</w> to raise their children away from the noise.",
                            "target_lang_translations": {
                                "ru": "Они решили переехать в тихий <w>пригород</w>, чтобы растить детей вдали от шума."
                            }
                        },
                        {
                            "text": "Living in a leafy <w>suburb</w> often means you need a car to get around.",
                            "target_lang_translations": {
                                "ru": "Жизнь в зелёном <w>пригороде</w> часто означает, что для передвижения вам понадобится машина."
                            }
                        }
                    ],
                    "synonyms": [
                        "outskirts",
                        "residential area"
                    ],
                    "antonyms": [
                        "city center",
                        "downtown",
                        "inner city"
                    ],
                    "common_phrases": [
                        "leafy suburb",
                        "quiet suburb",
                        "in the suburbs",
                        "suburb of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жилой район, расположенный на окраине города или крупного населенного пункта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пригород",
                                "target_lang_sense_clarification": "населённый пункт вблизи города"
                            },
                            {
                                "target_lang_word": "предместье",
                                "target_lang_sense_clarification": "посёлок, примыкающий к городу (часто историч.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "278c5c09-5fba-3587-babc-5c511d54c730",
                    "sense_definition": "The outer part or surrounding environment of something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "outer_part_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poet described wandering through the <w>suburbs</w> of sorrow in his latest work.",
                            "target_lang_translations": {
                                "ru": "Поэт описал блуждание по <w>окраинам</w> скорби в своей последней работе."
                            }
                        },
                        {
                            "text": "They felt they were living in the <w>suburbs</w> of history, far removed from the main events.",
                            "target_lang_translations": {
                                "ru": "Они чувствовали, что живут на <w>периферии</w> истории, вдали от главных событий."
                            }
                        }
                    ],
                    "synonyms": [
                        "periphery",
                        "fringe"
                    ],
                    "antonyms": [
                        "center",
                        "core"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly used in literary or figurative contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешняя часть или периферия чего-либо (часто в переносном смысле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "периферия",
                                "target_lang_sense_clarification": "внешняя, удаленная от центра часть"
                            },
                            {
                                "target_lang_word": "окраина",
                                "target_lang_sense_clarification": "крайняя часть, предел"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6004afd9-73ce-3785-982b-d78ed8549011",
                    "sense_definition": "Człowiek o wyjątkowo dużym wzroście i potężnej budowie ciała.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "human_description",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek to prawdziwy <w>olbrzym</w>, który musi schylać się w drzwiach.",
                            "target_lang_translations": {
                                "ru": "Мой дядя — настоящий <w>великан</w>, которому приходится пригибаться в дверях."
                            }
                        },
                        {
                            "text": "Ten koszykarz wygląda jak <w>olbrzym</w> przy reszcie zawodników.",
                            "target_lang_translations": {
                                "ru": "Этот баскетболист выглядит как <w>гигант</w> на фоне остальных игроков."
                            }
                        }
                    ],
                    "synonyms": [
                        "wielkolud",
                        "drągal",
                        "dryblas",
                        "gigant",
                        "nieułomek"
                    ],
                    "antonyms": [
                        "karzeł",
                        "liliput"
                    ],
                    "common_phrases": [
                        "prawdziwy olbrzym",
                        "silny jak olbrzym",
                        "wzrost olbrzyma"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек исключительно высокого роста и мощного телосложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "великан",
                                "target_lang_sense_clarification": "человек очень высокого роста"
                            },
                            {
                                "target_lang_word": "гигант",
                                "target_lang_sense_clarification": "человек огромных размеров и силы"
                            },
                            {
                                "target_lang_word": "исполин",
                                "target_lang_sense_clarification": "книжн. человек богатырского сложения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa8b6a09-646f-3eed-bb6f-022c1979f186",
                    "sense_definition": "Istota fantastyczna z baśni i mitów, przypominająca człowieka, ale o ogromnych rozmiarach i wielkiej sile.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mythical_creature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej starej legendzie dzielny rycerz pokonał groźnego <w>olbrzyma</w>.",
                            "target_lang_translations": {
                                "ru": "В этой старой легенде храбрый рыцарь победил грозного <w>великана</w>."
                            }
                        },
                        {
                            "text": "Baśniowy <w>olbrzym</w> pilnował wejścia do ukrytej jaskini ze skarbami.",
                            "target_lang_translations": {
                                "ru": "Сказочный <w>великан</w> охранял вход в тайную пещеру с сокровищами."
                            }
                        }
                    ],
                    "synonyms": [
                        "wielkolud",
                        "gigant",
                        "kolos"
                    ],
                    "antonyms": [
                        "krasnoludek",
                        "gnom",
                        "skrzat",
                        "karzeł"
                    ],
                    "common_phrases": [
                        "baśniowy olbrzym",
                        "walka z olbrzymem",
                        "zły olbrzym"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often appears in folklore contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фантастическое существо из сказок и мифов, напоминающее человека, но обладающее огромными размерами и силой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "великан",
                                "target_lang_sense_clarification": "сказочное существо"
                            },
                            {
                                "target_lang_word": "гигант",
                                "target_lang_sense_clarification": "мифологическое существо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c986d09-0980-3399-b8dd-a773047ffb1d",
                    "sense_definition": "Zwierzę, roślina lub przedmiot o rozmiarach znacznie większych niż przeciętne dla swojego gatunku lub rodzaju.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "large_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary dąb to leśny <w>olbrzym</w>, który rośnie tu od kilkuset lat.",
                            "target_lang_translations": {
                                "ru": "Этот старый дуб — лесной <w>великан</w>, который растет здесь уже несколько сотен лет."
                            }
                        },
                        {
                            "text": "W porcie zacumował stalowy <w>olbrzym</w>, który zabiera tysiące kontenerów.",
                            "target_lang_translations": {
                                "ru": "В порту пришвартовался стальной <w>гигант</w>, который забирает тысячи контейнеров."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolos",
                        "gigant"
                    ],
                    "antonyms": [
                        "miniatura",
                        "miniaturka"
                    ],
                    "common_phrases": [
                        "stalowy olbrzym",
                        "leśny olbrzym",
                        "morski olbrzym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Животное, растение или предмет, размеры которого значительно превышают средние показатели для своего вида."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гигант",
                                "target_lang_sense_clarification": "о предметах и объектах"
                            },
                            {
                                "target_lang_word": "великан",
                                "target_lang_sense_clarification": "о деревьях и животных"
                            },
                            {
                                "target_lang_word": "исполин",
                                "target_lang_sense_clarification": "о величественных объектах природы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5b37385-c617-3e75-acd3-036138b124ba",
                    "sense_definition": "Gwiazda o bardzo dużej jasności i promieniu, znacznie większa od gwiazd ciągu głównego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za miliardy lat nasze Słońce zmieni się w czerwonego <w>olbrzyma</w>.",
                            "target_lang_translations": {
                                "ru": "Через миллиарды лет наше Солнце превратится в красного <w>гиганта</w>."
                            }
                        },
                        {
                            "text": "Astronomowie obserwują błękitnego <w>olbrzyma</w> w odległej galaktyce.",
                            "target_lang_translations": {
                                "ru": "Астрономы наблюдают голубого <w>гиганта</w> в далекой галактике."
                            }
                        }
                    ],
                    "antonyms": [
                        "karzeł"
                    ],
                    "common_phrases": [
                        "czerwony olbrzym",
                        "błękitny olbrzym"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "astronomy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Звезда очень высокой светимости и радиуса, значительно большего, чем у звезд главной последовательности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гигант",
                                "target_lang_sense_clarification": "астрономический термин"
                            }
                        ]
                    }
                }
            ]
        }

                {
                    "sense_id": "9476628a-c690-39f1-b8fa-472dd6f84596",
                    "sense_definition": "Instytucja lub miejsce zapewniające tymczasową opiekę i dach nad głową bezdomnym ludziom lub zwierzętom.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wolontariusze regularnie odwiedzają miejskie <w>przytulisko</w>, aby wyprowadzać psy na spacer.",
                            "target_lang_translations": {
                                "ru": "Волонтеры регулярно посещают городской <w>приют</w>, чтобы выгуливать собак."
                            }
                        },
                        {
                            "text": "Zimą to <w>przytulisko</w> dla bezdomnych jest zawsze przepełnione potrzebującymi.",
                            "target_lang_translations": {
                                "ru": "Зимой этот <w>приют</w> для бездомных всегда переполнен нуждающимися."
                            }
                        }
                    ],
                    "synonyms": [
                        "schronisko",
                        "przytułek"
                    ],
                    "common_phrases": [
                        "przytulisko dla zwierząt",
                        "prowadzić przytulisko",
                        "miejskie przytulisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение или место, предоставляющее временный уход и кров бездомным людям или животным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приют",
                                "target_lang_sense_clarification": "учреждение для бездомных или животных"
                            },
                            {
                                "target_lang_word": "ночлежка",
                                "target_lang_sense_clarification": "место для ночлега бездомных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35fa3d7e-9d60-3147-ad22-636e8c83e075",
                    "sense_definition": "Miejsce dające poczucie bezpieczeństwa, spokoju lub fizyczną ochronę przed trudnymi warunkami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "refuge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stara leśniczówka stanowiła dla wędrowców bezpieczne <w>przytulisko</w> podczas gwałtownej burzy.",
                            "target_lang_translations": {
                                "ru": "Старый домик лесника стал для путников безопасным <w>убежищем</w> во время сильной грозы."
                            }
                        },
                        {
                            "text": "Po wielu latach tułaczki wreszcie znalazł swoje <w>przytulisko</w> w małym domku na wsi.",
                            "target_lang_translations": {
                                "ru": "После долгих лет скитаний он наконец нашел свое <w>пристанище</w> в маленьком домике в деревне."
                            }
                        }
                    ],
                    "synonyms": [
                        "schronienie"
                    ],
                    "common_phrases": [
                        "znaleźć przytulisko",
                        "bezpieczne przytulisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, дающее чувство безопасности, покоя или физическую защиту от тяжелых условий."
                    },
                    "translations": {
                        "ru": [
                            {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6f739d55-0430-3cb9-b3b7-61da0fd4ae18",
                    "sense_definition": "Регулярное передвижение транспортного средства (самолёта, судна, автобуса) по определённому маршруту.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш <w>рейс</w> задержали из-за сильного тумана.",
                            "target_lang_translations": {
                                "en": "Our <w>flight</w> was delayed due to heavy fog."
                            }
                        },
                        {
                            "text": "Этот автобус выполняет <w>рейс</w> между городами дважды в день.",
                            "target_lang_translations": {
                                "en": "This bus makes a <w>trip</w> between the cities twice a day."
                            }
                        }
                    ],
                    "synonyms": [
                        "поездка",
                        "перелёт",
                        "путешествие"
                    ],
                    "common_phrases": [
                        "чартерный рейс",
                        "прямой рейс",
                        "отменить рейс",
                        "номер рейса"
                    ],
                    "target_lang_definitions": {
                        "en": "A scheduled journey made by a vehicle, especially an aircraft, bus, or ship, along a specific route."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flight",
                                "target_lang_sense_clarification": "for aircraft"
                            },
                            {
                                "target_lang_word": "trip",
                                "target_lang_sense_clarification": "general scheduled journey"
                            },
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "for buses or trains"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dd71753-61b9-3fc6-94bb-67f740ad31e7",
                    "sense_definition": "Хождение туда и обратно для переноски или доставки чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {

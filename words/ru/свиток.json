    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ad5acb3c-dd4e-3600-9f07-cdb87c5a3bdf",
                    "sense_definition": "Полоса бумаги, пергамента или другого материала для письма, свёрнутая в трубку.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "scroll_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он достал из сумки бумажный <w>свиток</w> и положил его на стол.",
                            "target_lang_translations": {
                                "en": "He took a paper <w>scroll</w> out of his bag and put it on the table."
                            }
                        },
                        {
                            "text": "Чтобы прочитать послание, нужно аккуратно развернуть <w>свиток</w>.",
                            "target_lang_translations": {
                                "en": "To read the message, one must carefully unroll the <w>scroll</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "рулон",
                        "свёртка"
                    ],
                    "common_phrases": [
                        "развернуть свиток",
                        "свернуть в свиток",
                        "бумажный свиток"
                    ],
                    "target_lang_definitions": {
                        "en": "A strip of paper, parchment, or other writing material, rolled into a tube."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scroll",
                                "target_lang_sense_clarification": "roll of paper or parchment"
                            },
                            {
                                "target_lang_word": "roll",
                                "target_lang_sense_clarification": "rolled object"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "522ad718-20f3-3b96-b843-b6e9e3f9d350",
                    "sense_definition": "Старинная рукопись, написанная на длинной полосе материала и хранимая в свёрнутом виде.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scroll_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Археологи обнаружили в пещере древний <w>свиток</w> с неизвестными текстами.",
                            "target_lang_translations": {
                                "en": "Archaeologists discovered an ancient <w>scroll</w> with unknown texts in the cave."
                            }
                        },
                        {
                            "text": "В музее хранится уникальный пергаментный <w>свиток</w> двенадцатого века.",
                            "target_lang_translations": {
                                "en": "A unique parchment <w>scroll</w> from the twelfth century is kept in the museum."
                            }
                        }
                    ],
                    "synonyms": [
                        "рукопись",
                        "манускрипт"
                    ],
                    "common_phrases": [
                        "древний свиток",
                        "пергаментный свиток",
                        "свитки Мёртвого моря"
                    ],
                    "target_lang_definitions": {
                        "en": "An ancient manuscript written on a long strip of material and kept in a rolled form."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scroll",
                                "target_lang_sense_clarification": "ancient manuscript"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80352254-6d0f-32e9-b25b-3d480083e356",
                    "sense_definition": "Длинная, непрерывная череда событий, воспоминаний или мыслей (переносное значение).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед его мысленным взором развернулся пёстрый <w>свиток</w> прошлых лет.",
                            "target_lang_translations": {
                                "en": "A colorful <w>scroll</w> of past years unfolded before his mind's eye."
                            }
                        },
                        {
                            "text": "Жизнь казалась ему бесконечным <w>свитком</w> забот и тревог.",
                            "target_lang_translations": {
                                "en": "Life seemed to him an endless <w>scroll</w> of cares and anxieties."
                            }
                        }
                    ],
                    "synonyms": [
                        "череда",
                        "цепь",
                        "вереница"
                    ],
                    "common_phrases": [
                        "свиток истории",
                        "свиток лет"
                    ],
                    "target_lang_definitions": {
                        "en": "A long, continuous series of events, memories, or thoughts (figurative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scroll",
                                "target_lang_sense_clarification": "figurative: series of events"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
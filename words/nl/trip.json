                {
                    "sense_id": "c7171183-bcce-3690-92bf-0fb17ac76c54",
                    "sense_definition": "Een korte reis of uitstapje, meestal voor ontspanning of plezier.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "journey",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gaan dit weekend op <w>trip</w> naar de Ardennen.",
                            "target_lang_translations": {
                                "ru": "В эти выходные мы отправляемся в <w>поездку</w> в Арденны."
                            }
                        },
                        {
                            "text": "Het was een lange <w>trip</w> met de auto naar het zuiden.",
                            "target_lang_translations": {
                                "ru": "Это была долгая <w>поездка</w> на машине на юг."
                            }

                            {
                                "target_lang_word": "sin",
                                "target_lang_sense_clarification": "moral offense"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "преступление",
                                "target_lang_sense_clarification": "в переносном смысле"
                            },
                            {
                                "target_lang_word": "кощунство",
                                "target_lang_sense_clarification": "надругательство над ценностями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4fc98ae-027b-3216-b29f-f89ca7a38bf7",

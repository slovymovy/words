    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "945cff3e-fd21-3075-a5f1-b92ba4c947b0",
                    "sense_definition": "Znajdujący się na dole lub stanowiący niższą część jakiejś całości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę podpisać się w <w>dolnym</w> rogu tego dokumentu.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, распишитесь в <w>нижнем</w> углу этого документа."
                            }
                        },
                        {
                            "text": "Moja babcia mieszka na <w>dolnym</w> piętrze naszego domu, żeby nie musiała chodzić po schodach.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка живет на <w>нижнем</w> этаже нашего дома, чтобы ей не приходилось ходить по лестнице."
                            }
                        }
                    ],
                    "antonyms": [
                        "górny"
                    ],
                    "common_phrases": [
                        "dolna część",
                        "dolna półka",
                        "dolny róg",
                        "dolna warga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся внизу или составляющий нижнюю часть чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нижний",
                                "target_lang_sense_clarification": "о положении в пространстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f750c2c4-a7de-3787-aba2-d75aa68e0e98",
                    "sense_definition": "Dotyczący obszaru geograficznego położonego niżej na mapie lub w biegu rzeki bliżej jej ujścia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location_geo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Dolny</w> Śląsk to region w Polsce słynący z pięknych zamków i pałaców.",
                            "target_lang_translations": {
                                "ru": "<w>Нижняя</w> Силезия — это регион в Польше, славящийся красивыми замками и дворцами."
                            }
                        },
                        {
                            "text": "Woda w <w>dolnym</w> biegu rzeki płynie znacznie wolniej niż w górach.",
                            "target_lang_translations": {
                                "ru": "Вода в <w>нижнем</w> течении реки течет значительно медленнее, чем в горах."
                            }
                        }
                    ],
                    "antonyms": [
                        "górny"
                    ],
                    "common_phrases": [
                        "Dolny Śląsk",
                        "dolny bieg rzeki",
                        "dolna Odra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к географической области, расположенной ниже по карте или по течению реки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нижний",
                                "target_lang_sense_clarification": "географический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a5dced6-7848-3d3f-b355-a989b171268b",
                    "sense_definition": "O dźwięku lub głosie: mający niską częstotliwość, basowy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Śpiewaczka miała problem z czystym wydobyciem dźwięków w <w>dolnym</w> rejestrze.",
                            "target_lang_translations": {
                                "ru": "Певица испытывала трудности с чистым извлечением звуков в <w>нижнем</w> регистре."
                            }
                        },
                        {
                            "text": "Pianista uderzył w klawisze, wydobywając głęboki, <w>dolny</w> ton.",
                            "target_lang_translations": {
                                "ru": "Пианист ударил по клавишам, извлекая глубокий <w>нижний</w> тон."
                            }
                        }
                    ],
                    "antonyms": [
                        "górny"
                    ],
                    "common_phrases": [
                        "dolny rejestr",
                        "dolne nuty"
                    ],
                    "target_lang_definitions": {
                        "ru": "О звуке или голосе: имеющий низкую частоту, басовый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нижний",
                                "target_lang_sense_clarification": "о регистре"
                            },
                            {
                                "target_lang_word": "низкий",
                                "target_lang_sense_clarification": "о звуке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "199a9e1d-221b-3624-9e59-a92124a14a01",
                    "sense_definition": "W geologii: odnoszący się do wczesnego, starszego okresu danej ery.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Skamieniałości te pochodzą z okresu zwanego <w>dolną</w> kredą.",
                            "target_lang_translations": {
                                "ru": "Эти окаменелости относятся к периоду, называемому <w>нижним</w> мелом."
                            }
                        },
                        {
                            "text": "Badania stratygraficzne potwierdziły, że warstwa ta uformowała się w <w>dolnym</w> plejstocenie.",
                            "target_lang_translations": {
                                "ru": "Стратиграфические исследования подтвердили, что этот слой сформировался в <w>нижнем</w> плейстоцене."
                            }
                        }
                    ],
                    "antonyms": [
                        "górny"
                    ],
                    "common_phrases": [
                        "dolny karbon",
                        "dolna kreda"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В геологии: относящийся к раннему, более старому периоду данной эры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нижний",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "214df126-78ea-300a-a479-44e7b77e493f",
                    "sense_definition": "Предоставить, вручить что-либо (деньги, документы, товар) в официальном порядке или из запасов.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кассир должен <выдать> зарплату всем сотрудникам сегодня вечером.",
                            "target_lang_translations": {
                                "en": "The cashier must <issue> the salary to all employees this evening."
                            }
                        },
                        {
                            "text": "Вам нужно <выдать> новый пропуск для входа в офисное здание.",
                            "target_lang_translations": {
                                "en": "They need to <issue> you a new pass to enter the office building."
                            }
                        }
                    ],
                    "synonyms": [
                        "дать"
                    ],
                    "antonyms": [
                        "принять",
                        "забрать"
                    ],
                    "common_phrases": [
                        "выдать зарплату",
                        "выдать паспорт",
                        "выдать кредит",
                        "выдать справку"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide or hand over something (money, documents, goods) officially or from stock."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "issue",
                                "target_lang_sense_clarification": "documents, salary, supplies"
                            },
                            {
                                "target_lang_word": "give out",
                                "target_lang_sense_clarification": "distribute generally"
                            },
                            {
                                "target_lang_word": "hand out",
                                "target_lang_sense_clarification": "physically give"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cfe995b-fddb-37f0-b785-a83ca2df5500",
                    "sense_definition": "Раскрыть тайну, обнаружить чьё-либо присутствие или сообщить секретную информацию (часто ненамеренно или предательски).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дрожащий голос мог случайно <выдать> его сильное волнение.",
                            "target_lang_translations": {
                                "en": "A trembling voice could accidentally <betray> his strong agitation."
                            }
                        },
                        {
                            "text": "Он боялся <выдать> секрет, поэтому молчал весь вечер.",
                            "target_lang_translations": {
                                "en": "He was afraid to <give away> the secret, so he stayed silent all evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "раскрыть",
                        "спалить"
                    ],
                    "antonyms": [
                        "скрыть",
                        "утаить"
                    ],
                    "common_phrases": [
                        "выдать секрет",
                        "выдать тайну",
                        "выдать себя",
                        "выдать с головой"
                    ],
                    "target_lang_definitions": {
                        "en": "To reveal a secret, discover someone's presence, or communicate confidential information (often unintentionally)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give away",
                                "target_lang_sense_clarification": "a secret or presence"
                            },
                            {
                                "target_lang_word": "betray",
                                "target_lang_sense_clarification": "feelings or presence"
                            },
                            {
                                "target_lang_word": "reveal",
                                "target_lang_sense_clarification": "information"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "759018fc-018a-3a89-a0dd-07ae1b675cbb",
                    "sense_definition": "Представить кого-либо или что-либо не тем, кем или чем они являются на самом деле.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мошенник пытался <выдать> дешёвую подделку за оригинал известной картины.",
                            "target_lang_translations": {
                                "en": "The fraudster tried to <pass off> a cheap fake as an original famous painting."
                            }
                        },
                        {
                            "text": "Она очень хотела <выдать> желаемое за действительное.",
                            "target_lang_translations": {
                                "en": "She really wanted to <pass off> wishful thinking as reality."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выдать желаемое за действительное",
                        "выдать за своего",
                        "выдать чёрное за белое"
                    ],
                    "target_lang_definitions": {
                        "en": "To present someone or something as different from what they actually are."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pass off",
                                "target_lang_sense_clarification": "a fake as real"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d398138-4331-37cc-a3a0-e019694ecdef",
                    "sense_definition": "Содействовать вступлению женщины в брак (обычно о дочери, сестре или родственнице).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец решил удачно <выдать> дочь замуж за богатого соседа.",
                            "target_lang_translations": {
                                "en": "The father decided to successfully <marry off> his daughter to a rich neighbor."
                            }
                        },
                        {
                            "text": "В старину родители старались <выдать> девушек замуж как можно раньше.",
                            "target_lang_translations": {
                                "en": "In the old days, parents tried to <marry> girls <off> as early as possible."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выдать замуж",
                        "выдать дочь"
                    ],
                    "target_lang_definitions": {
                        "en": "To arrange for a woman (usually a daughter or relative) to get married."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "marry off",
                                "target_lang_sense_clarification": "arrange marriage for"
                            },
                            {
                                "target_lang_word": "give in marriage",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4745e8ec-c0c7-3fda-9275-ed0d0b881faf",
                    "sense_definition": "Передать лицо, совершившее преступление, властям другого государства или правоохранительным органам.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Соседняя страна отказалась <выдать> беглого политика нашим властям.",
                            "target_lang_translations": {
                                "en": "The neighboring country refused to <extradite> the fugitive politician to our authorities."
                            }
                        },
                        {
                            "text": "Полиция согласилась <выдать> преступника после долгих международных переговоров.",
                            "target_lang_translations": {
                                "en": "The police agreed to <hand over> the criminal after long international negotiations."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выдать преступника",
                        "выдать властям"
                    ],
                    "target_lang_definitions": {
                        "en": "To transfer a person who has committed a crime to the authorities of another state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "extradite",
                                "target_lang_sense_clarification": "legal context"
                            },
                            {
                                "target_lang_word": "hand over",
                                "target_lang_sense_clarification": "to authorities"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88a108d6-a68d-32d0-9103-a60c7a62d274",
                    "sense_definition": "Сделать или сказать что-то неожиданное, странное, смешное или экстраординарное.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ну ты и <выдал>, я от тебя такого поступка совсем не ожидал!",
                            "target_lang_translations": {
                                "en": "You really <pulled a stunt>, I didn't expect such behavior from you at all!"
                            }
                        },
                        {
                            "text": "Вчера наш строгий начальник <выдал> очень смешную шутку.",
                            "target_lang_translations": {
                                "en": "Yesterday our strict boss <came out with> a very funny joke."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ну ты выдал",
                        "выдать номер"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To do or say something unexpected, strange, funny, or extraordinary."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come out with",
                                "target_lang_sense_clarification": "a joke or remark"
                            },
                            {
                                "target_lang_word": "pull",
                                "target_lang_sense_clarification": "a stunt or trick"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd6c0365-2d50-3337-985b-29087cb1f413",
                    "sense_definition": "Произвести, изготовить продукцию или обеспечить результат (о механизмах, предприятиях, недрах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый завод должен <выдать> первую партию автомобилей уже в мае.",
                            "target_lang_translations": {
                                "en": "The new factory is due to <produce> the first batch of cars as early as May."
                            }
                        },
                        {
                            "text": "Старый электрический генератор не может <выдать> необходимую мощность.",
                            "target_lang_translations": {
                                "en": "The old electric generator cannot <output> the necessary power."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выдать продукцию",
                        "выдать результат",
                        "выдать мощность",
                        "выдать на-гора"
                    ],
                    "target_lang_definitions": {
                        "en": "To produce goods or provide a result (regarding mechanisms, enterprises, or resources)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "produce",
                                "target_lang_sense_clarification": "goods or results"
                            },
                            {
                                "target_lang_word": "output",
                                "target_lang_sense_clarification": "power or data"
                            },
                            {
                                "target_lang_word": "generate",
                                "target_lang_sense_clarification": "power"
                            }
                        ]
                    }
                }
            ]
        }
    ]

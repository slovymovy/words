                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести себя",
                                "target_lang_sense_clarification": "о поведении"
                            },
                            {
                                "target_lang_word": "держаться",
                                "target_lang_sense_clarification": "о манере поведения"
                            }
                        ]
                    }
                },
                {

                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма именительного, винительного и звательного падежей множественного числа от слова «słuchawka» (например, телефонная трубка, душевая лейка, стетоскоп)."
                    },
                    "translations": {

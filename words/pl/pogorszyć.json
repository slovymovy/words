{
    "word_family": [
        "pogarszać",
        "pogorszenie",
        "gorszy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6d769a9f-ae43-3734-88e9-3bfcc47fe959",
                    "sense_definition": "Sprawić, że stan czegoś lub kogoś stanie się gorszy, mniej korzystny lub trudniejszy niż był wcześniej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "change_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twoja interwencja może tylko <w>pogorszyć</w> i tak napiętą sytuację.",
                            "target_lang_translations": {
                                "ru": "Твое вмешательство может только <w>усугубить</w> и без того напряженную ситуацию."
                            }
                        },
                        {
                            "text": "Palenie papierosów znacznie <w>pogorszyło</w> jego stan zdrowia w ostatnich latach.",
                            "target_lang_translations": {
                                "ru": "Курение сигарет значительно <w>ухудшило</w> состояние его здоровья в последние годы."
                            }
                        }
                    ],
                    "synonyms": [
                        "popsuć",
                        "zaostrzyć",
                        "skomplikować",
                        "zepsuć"
                    ],
                    "antonyms": [
                        "polepszyć",
                        "poprawić",
                        "ulepszyć",
                        "naprawić"
                    ],
                    "common_phrases": [
                        "pogorszyć sprawę",
                        "pogorszyć sytuację",
                        "pogorszyć stan zdrowia",
                        "pogorszyć relacje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать состояние чего-либо или кого-либо хуже, менее благоприятным или более трудным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухудшить",
                                "target_lang_sense_clarification": "сделать хуже"
                            },
                            {
                                "target_lang_word": "усугубить",
                                "target_lang_sense_clarification": "сделать более тяжелым (о ситуации, проблеме)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "236299cb-1aff-32c6-9070-46f60058f876",
                    "sense_definition": "Stać się gorszym; ulec zmianie na niekorzyść (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "change_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety pogoda <w>pogorszyła</w> się i musieliśmy odwołać wycieczkę.",
                            "target_lang_translations": {
                                "ru": "К сожалению, погода <w>испортилась</w>, и нам пришлось отменить экскурсию."
                            }
                        },
                        {
                            "text": "Jej samopoczucie gwałtownie <w>pogorszyło</w> się po zjedzeniu nieświeżego posiłku.",
                            "target_lang_translations": {
                                "ru": "Её самочувствие резко <w>ухудшилось</w> после того, как она съела несвежую еду."
                            }
                        }
                    ],
                    "synonyms": [
                        "popsuć się",
                        "podupaść",
                        "zaostrzyć się"
                    ],
                    "antonyms": [
                        "polepszyć się",
                        "poprawić się"
                    ],
                    "common_phrases": [
                        "pogorszyć się znacznie",
                        "pogorszyć się gwałtownie",
                        "pogoda się pogorszyła",
                        "zdrowie się pogorszyło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать хуже; измениться в неблагоприятную сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухудшиться",
                                "target_lang_sense_clarification": "стать хуже"
                            },
                            {
                                "target_lang_word": "испортиться",
                                "target_lang_sense_clarification": "стать плохим (о погоде, настроении)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Весь огромный и интересный мир открыт <передо> мной.",
                            "target_lang_translations": {
                                "en": "The whole vast and interesting world is open <w>before</w> me."
                            }
                        },
                        {
                            "text": "Директор решил выступить с важной речью <передо> всеми сотрудниками.",
                            "target_lang_translations": {
                                "en": "The director decided to give an important speech <w>in front of</w> all the employees."
                            }
                        }

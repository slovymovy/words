{
    "word_family": [
        "tubka",
        "tubowy",
        "tubista"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "856359e0-8c70-30ac-8dbc-75a70dd2550e",
                    "sense_definition": "Duży instrument dęty blaszany o niskim brzmieniu, używany w orkiestrach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "music_instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat uczy się grać na <w>tubie</w> w szkole muzycznej.",
                            "target_lang_translations": {
                                "ru": "Мой брат учится играть на <w>тубе</w> в музыкальной школе."
                            }
                        },
                        {
                            "text": "W orkiestrze dętej <w>tuba</w> stanowi podstawę sekcji basowej.",
                            "target_lang_translations": {
                                "ru": "В духовом оркестре <w>туба</w> составляет основу басовой секции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grać na tubie",
                        "tuba basowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой медный духовой инструмент с низким звучанием, используемый в оркестрах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туба",
                                "target_lang_sense_clarification": "музыкальный инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2175a84-b411-32a2-8c70-c4be7c8e863b",
                    "sense_definition": "Rodzaj szalika zszytego w pętlę, zakładanego przez głowę, ściśle otaczającego szyję.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą noszę ciepłą <w>tubę</w>, która chroni mnie przed wiatrem.",
                            "target_lang_translations": {
                                "ru": "Зимой я ношу теплый <w>снуд</w>, который защищает меня от ветра."
                            }
                        },
                        {
                            "text": "Ta wełniana <w>tuba</w> jest wygodniejsza niż tradycyjny szalik.",
                            "target_lang_translations": {
                                "ru": "Этот шерстяной <w>снуд</w> удобнее, чем традиционный шарф."
                            }
                        }
                    ],
                    "synonyms": [
                        "komin"
                    ],
                    "common_phrases": [
                        "szal-tuba",
                        "wełniana tuba"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность шарфа, сшитого в кольцо, который надевается через голову и плотно облегает шею."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снуд",
                                "target_lang_sense_clarification": "шарф"
                            },
                            {
                                "target_lang_word": "шарф-труба",
                                "target_lang_sense_clarification": "одежда"
                            },
                            {
                                "target_lang_word": "шарф-хомут",
                                "target_lang_sense_clarification": "одежда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b88c2a03-6f78-3101-8092-5d6c0f832c61",
                    "sense_definition": "Sztywne, walcowate opakowanie służące zazwyczaj do przechowywania lub transportu zwiniętych arkuszy papieru.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Student przyniósł swój projekt na zajęcia w tekturowej <w>tubie</w>.",
                            "target_lang_translations": {
                                "ru": "Студент принес свой проект на занятия в картонном <w>тубусе</w>."
                            }
                        },
                        {
                            "text": "Kupiłem specjalną <w>tubę</w>, aby bezpiecznie wysłać plakat pocztą.",
                            "target_lang_translations": {
                                "ru": "Я купил специальный <w>тубус</w>, чтобы безопасно отправить плакат по почте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tuba na rysunki",
                        "kartonowa tuba"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жесткая цилиндрическая упаковка, обычно используемая для хранения или транспортировки свернутых листов бумаги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тубус",
                                "target_lang_sense_clarification": "футляр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f991662a-fae9-3b8e-a267-12bbb17800e8",
                    "sense_definition": "Urządzenie w kształcie lejka służące do wzmacniania głosu i kierowania go w jedną stronę.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ratownik na plaży krzyczał przez <w>tubę</w>, ostrzegając pływaków.",
                            "target_lang_translations": {
                                "ru": "Спасатель на пляже кричал в <w>рупор</w>, предупреждая пловцов."
                            }
                        },
                        {
                            "text": "Głos spikera dobiegał z wielkiej <w>tuby</w> zamontowanej na słupie.",
                            "target_lang_translations": {
                                "ru": "Голос диктора доносился из огромного <w>рупора</w>, установленного на столбе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mówić przez tubę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство в форме воронки для усиления голоса и направления его в одну сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рупор",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "мегафон",
                                "target_lang_sense_clarification": "устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c616e0e-1592-3345-99ba-6e415fa36d28",
                    "sense_definition": "Osoba, instytucja lub medium gorliwie i bezkrytycznie głoszące czyjeś poglądy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta gazeta stała się <w>tubą</w> propagandową rządu.",
                            "target_lang_translations": {
                                "ru": "Эта газета стала пропагандистским <w>рупором</w> правительства."
                            }
                        },
                        {
                            "text": "Nie chcę być niczyją <w>tubą</w>, wolę mieć własne zdanie.",
                            "target_lang_translations": {
                                "ru": "Я не хочу быть чьим-либо <w>рупором</w>, я предпочитаю иметь собственное мнение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tuba propagandowa",
                        "tuba rządu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, учреждение или средство массовой информации, рьяно и некритично провозглашающие чьи-либо взгляды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рупор",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "1b1691c5-9841-3c36-9a22-7e02c50060c7",
                    "sense_definition": "Rzeka w azjatyckiej części Rosji, będąca prawym dopływem Jeniseju.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Rzeka <w>Tuba</w> przepływa przez Kraj Krasnojarski.",
                            "target_lang_translations": {
                                "ru": "Река <w>Туба</w> протекает через Красноярский край."
                            }
                        },
                        {
                            "text": "Wody <w>Tuby</w> zasilają potężny Jenisej.",
                            "target_lang_translations": {
                                "ru": "Воды <w>Тубы</w> питают могучий Енисей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Река в азиатской части России, правый приток Енисея."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Туба",
                                "target_lang_sense_clarification": "река"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
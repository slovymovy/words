    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2482f362-d37f-3ac6-b828-082f64f53cd5",
                    "sense_definition": "Een opmerking, verhaaltje of handeling die bedoeld is om mensen te laten lachen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vertelde een leuke <w>grap</w> waar iedereen hard om moest lachen.",
                            "target_lang_translations": {
                                "ru": "Он рассказал смешную <w>шутку</w>, над которой все громко смеялись."
                            }
                        },
                        {
                            "text": "Het was maar een <w>grap</w>, je moet het niet zo serieus nemen.",
                            "target_lang_translations": {
                                "ru": "Это была всего лишь <w>шутка</w>, не стоит воспринимать это так серьезно."
                            }
                        }
                    ],
                    "synonyms": [
                        "mop",
                        "grol"
                    ],
                    "common_phrases": [
                        "een grap maken",
                        "een grap uithalen",
                        "voor de grap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Замечание, история или действие, призванное рассмешить людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шутка",
                                "target_lang_sense_clarification": "смешное высказывание или поступок"
                            },
                            {
                                "target_lang_word": "анекдот",
                                "target_lang_sense_clarification": "короткая смешная история"
                            },
                            {
                                "target_lang_word": "розыгрыш",
                                "target_lang_sense_clarification": "шутливое действие над кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a5040e2-0dc7-3c86-9efc-78eac05d8a8f",
                    "sense_definition": "Een vervelende gebeurtenis of iets dat onverwacht veel geld of moeite kost.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cost",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De reparatie van de auto werd uiteindelijk een dure <w>grap</w>.",
                            "target_lang_translations": {
                                "ru": "Ремонт машины в итоге стал дорогим <w>удовольствием</w>."
                            }
                        },
                        {
                            "text": "Het hele project is een kostbare <w>grap</w> geworden voor het bedrijf.",
                            "target_lang_translations": {
                                "ru": "Весь этот проект стал для компании затратной <w>затеей</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een dure grap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприятное событие или дело, требующее неожиданно больших затрат денег или усилий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удовольствие",
                                "target_lang_sense_clarification": "иронично, в выражении \"дорогое удовольствие\""
                            },
                            {
                                "target_lang_word": "затея",
                                "target_lang_sense_clarification": "предприятие или дело"
                            },
                            {
                                "target_lang_word": "дело",
                                "target_lang_sense_clarification": "ситуация или случай"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "63a10894-b7d1-3221-8302-d17f647c6a97",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>grappen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>grap</w> maar wat, wees alsjeblieft niet boos op mij.",
                            "target_lang_translations": {
                                "ru": "Я просто <w>шучу</w>, пожалуйста, не сердись на меня."
                            }
                        },
                        {
                            "text": "Soms <w>grap</w> ik over dingen die eigenlijk heel serieus zijn.",
                            "target_lang_translations": {
                                "ru": "Иногда я <w>шучу</w> о вещах, которые на самом деле очень серьезны."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма 1-го лица единственного числа настоящего времени глагола grappen."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шучу",
                                "target_lang_sense_clarification": "я шучу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3c98c88-96d2-37a9-ad67-eef084ed292c",
                    "sense_definition": "Gebiedende wijs van <w>grappen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Grap</w> niet over zulke gevoelige onderwerpen in dit gezelschap.",
                            "target_lang_translations": {
                                "ru": "Не <w>шути</w> на такие деликатные темы в этой компании."
                            }
                        },
                        {
                            "text": "<w>Grap</w> er maar niet over, want hij vindt het helemaal niet leuk.",
                            "target_lang_translations": {
                                "ru": "Лучше не <w>шути</w> об этом, потому что ему это совсем не нравится."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола grappen."

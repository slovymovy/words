    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6a289c5b-6e5d-37f8-a8fd-f44e0b336d71",
                    "sense_definition": "Оградить или защитить кого-либо или что-либо от опасности, неприятностей или вредного воздействия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители всегда стараются <уберечь> своих детей от глупых ошибок.",
                            "target_lang_translations": {
                                "en": "Parents always try to <w>protect</w> their children from foolish mistakes."
                            }
                        },
                        {
                            "text": "Тёплая одежда помогла <уберечь> его от простуды в такой мороз.",
                            "target_lang_translations": {
                                "en": "Warm clothes helped <w>protect</w> him from a cold in such freezing weather."
                            }
                        }
                    ],
                    "synonyms": [
                        "оградить",
                        "защитить"
                    ],
                    "common_phrases": [
                        "уберечь от беды",
                        "уберечь от простуды",
                        "уберечь от ошибок"
                    ],
                    "target_lang_definitions": {
                        "en": "To guard or protect someone or something from danger, trouble, or harmful influence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "protect",
                                "target_lang_sense_clarification": "general protection from harm"
                            },
                            {
                                "target_lang_word": "save",
                                "target_lang_sense_clarification": "rescue from danger or trouble"
                            },
                            {
                                "target_lang_word": "shield",
                                "target_lang_sense_clarification": "screen from danger or impact"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e91c6fa0-1991-3bdc-8656-b2a7feb84a06",
                    "sense_definition": "Сохранить что-либо в целости и сохранности, не дать пропасть или испортиться.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ей чудом удалось <уберечь> старинную вазу во время переезда.",
                            "target_lang_translations": {
                                "en": "She miraculously managed to <w>save</w> the antique vase during the move."
                            }
                        },
                        {
                            "text": "Мы должны <уберечь> природу для будущих поколений.",
                            "target_lang_translations": {
                                "en": "We must <w>preserve</w> nature for future generations."
                            }
                        }
                    ],
                    "synonyms": [
                        "сохранить"
                    ],
                    "common_phrases": [
                        "уберечь здоровье",
                        "уберечь нервы",
                        "уберечь имущество"
                    ],
                    "target_lang_definitions": {
                        "en": "To keep something safe and sound, preventing it from being lost or damaged."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "preserve",
                                "target_lang_sense_clarification": "maintain in original state"
                            },
                            {
                                "target_lang_word": "save",
                                "target_lang_sense_clarification": "prevent from being lost or ruined"
                            },
                            {
                                "target_lang_word": "keep safe",
                                "target_lang_sense_clarification": "ensure safety of an object"
                            }
                        ]
                    }
                }
            ]
        }
    ]

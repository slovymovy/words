    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5012d6c4-0ad7-31b1-be68-faeed2c7ff14",
                    "sense_definition": "Центральная часть скелета человека и животных, состоящая из цепи костей (позвонков), идущая вдоль спины.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy_skeleton",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач внимательно осмотрел мой <w>позвоночник</w> на рентгеновском снимке.",
                            "target_lang_translations": {
                                "en": "The doctor carefully examined my <w>spine</w> on the X-ray."
                            }
                        },
                        {
                            "text": "У жирафа очень длинный и гибкий <w>позвоночник</w>, что позволяет ему доставать до высоких веток.",
                            "target_lang_translations": {
                                "en": "The giraffe has a very long and flexible <w>spine</w>, which allows it to reach high branches."
                            }
                        }
                    ],
                    "synonyms": [
                        "хребет",
                        "позвоночный столб",
                        "спинной хребет"
                    ],
                    "common_phrases": [
                        "искривление позвоночника",
                        "перелом позвоночника",
                        "шейный отдел позвоночника",
                        "травма позвоночника"
                    ],
                    "target_lang_definitions": {
                        "en": "The central part of the skeleton in humans and animals, consisting of a chain of bones (vertebrae) running along the back."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spine",
                                "target_lang_sense_clarification": "anatomy"
                            },
                            {
                                "target_lang_word": "backbone",
                                "target_lang_sense_clarification": "common term"
                            },
                            {
                                "target_lang_word": "spinal column",
                                "target_lang_sense_clarification": "medical/technical"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a08e861-b1b4-3bdf-acdd-0f2cb3c5ef83",
                    "sense_definition": "Внутренняя основа, стержень, на котором держится что-либо (в переносном смысле).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Экономика — это <w>позвоночник</w> любого сильного государства.",
                            "target_lang_translations": {
                                "en": "The economy is the <w>backbone</w> of any strong state."
                            }
                        },
                        {
                            "text": "У этого фильма есть крепкий сюжетный <w>позвоночник</w>, который держит зрителя в напряжении.",
                            "target_lang_translations": {
                                "en": "This movie has a strong narrative <w>spine</w> that keeps the viewer in suspense."
                            }
                        }
                    ],
                    "synonyms": [
                        "стержень",
                        "основа"
                    ],
                    "target_lang_definitions": {
                        "en": "The internal foundation or core structure that supports something (figuratively)."
                    },
                    "translations": {
                        "en": [
                            {

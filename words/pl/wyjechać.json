{
    "word_family": [
        "wyjazd",
        "wyjazdowy",
        "jechać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "993fdde0-8935-356a-8d37-cd5422bd9d8b",
                    "sense_definition": "Opuścić miejsce pobytu, udając się w podróż środkiem transportu",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_depart",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <wyjechać> wcześnie rano, żeby uniknąć korków.",
                            "target_lang_translations": {
                                "ru": "Мы должны <выехать> рано утром, чтобы избежать пробок."
                            }
                        },
                        {
                            "text": "Moi rodzice planują <wyjechać> na wakacje do Włoch.",
                            "target_lang_translations": {
                                "ru": "Мои родители планируют <уехать> в отпуск в Италию."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyruszyć",
                        "odjechać"
                    ],
                    "antonyms": [
                        "przyjechać",
                        "wrócić"
                    ],
                    "common_phrases": [
                        "wyjechać za granicę",
                        "wyjechać na wakacje",
                        "wyjechać z miasta",
                        "wyjechać w podróż"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покинуть место пребывания, отправляясь в поездку на транспорте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уехать",
                                "target_lang_sense_clarification": "покинуть место, отправиться куда-то"
                            },
                            {
                                "target_lang_word": "выехать",
                                "target_lang_sense_clarification": "начать движение, отправиться в путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "263f0534-b64a-3c5f-967c-b15fbde36a0e",
                    "sense_definition": "Wydostać się pojazdem z zamkniętego pomieszczenia lub zza przeszkody na otwartą przestrzeń",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_exit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo trudno jest <wyjechać> z tego ciasnego parkingu.",
                            "target_lang_translations": {
                                "ru": "Очень трудно <выехать> с этой тесной парковки."
                            }
                        },
                        {
                            "text": "Samochód nagle <wyjechał> zza rogu budynku.",
                            "target_lang_translations": {
                                "ru": "Машина внезапно <выехала> из-за угла здания."
                            }
                        }
                    ],
                    "antonyms": [
                        "wjechać"
                    ],
                    "common_phrases": [
                        "wyjechać z garażu",
                        "wyjechać na ulicę",
                        "wyjechać zza zakrętu",
                        "wyjechać na prostą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбраться на транспортном средстве из закрытого пространства или из-за препятствия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выехать",
                                "target_lang_sense_clarification": "покинуть пределы чего-либо на транспорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2b9ddc1-fdbf-386a-8d1a-7718e1e51bce",
                    "sense_definition": "Przemieścić się pojazdem na wyżej położony teren lub wjechać na górę czegoś",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rowerzysta z trudem zdołał <wyjechać> na strome wzgórze.",
                            "target_lang_translations": {
                                "ru": "Велосипедист с трудом смог <въехать> на крутой холм."
                            }
                        },
                        {
                            "text": "Nie uda nam się tam <wyjechać> samochodem bez napędu na cztery koła.",
                            "target_lang_translations": {
                                "ru": "Нам не удастся <заехать> туда на машине без полного привода."
                            }
                        }
                    ],
                    "synonyms": [
                        "wjechać"
                    ],
                    "antonyms": [
                        "zjechać"
                    ],
                    "common_phrases": [
                        "wyjechać na górę",
                        "wyjechać na wzniesienie",
                        "wyjechać pod górę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подняться на возвышенность или въехать наверх чего-либо на транспортном средстве"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "въехать",
                                "target_lang_sense_clarification": "подняться наверх на транспорте"
                            },
                            {
                                "target_lang_word": "заехать",
                                "target_lang_sense_clarification": "взобраться на транспорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3037d15-fb01-3fd7-9aa1-8e712e2a9851",
                    "sense_definition": "Odezwać się w sposób niestosowny, zaskakujący lub niemądry",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "idiom_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek niepotrzebnie <wyjechał> z tym żartem przy dyrektorze.",
                            "target_lang_translations": {
                                "ru": "Марек зря <вылез> с этой шуткой при директоре."
                            }
                        },
                        {
                            "text": "Wstyd mi, że musiałem <wyjechać> z taką głupią propozycją.",
                            "target_lang_translations": {
                                "ru": "Мне стыдно, что мне пришлось <вылезти> с таким глупым предложением."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyskoczyć"
                    ],
                    "common_phrases": [
                        "wyjechać z tekstem",
                        "wyjechać z pytaniem",
                        "wyjechać z propozycją"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сказать что-то неуместное, неожиданное или глупое"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вылезти",
                                "target_lang_sense_clarification": "разг. выступить с чем-то неуместным"
                            },
                            {
                                "target_lang_word": "выдать",
                                "target_lang_sense_clarification": "разг. сказать неожиданно или глупо"
                            },
                            {
                                "target_lang_word": "ляпнуть",
                                "target_lang_sense_clarification": "разг. сказать не подумав"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The large white sails began to <w>belly</w> out as the wind picked up.",
                            "target_lang_translations": {
                                "ru": "Большие белые паруса начали <w>надуваться</w>, когда ветер усилился."
                            }
                        },
                        {
                            "text": "His loose shirt <w>bellied</w> in the breeze as he rode his bicycle.",
                            "target_lang_translations": {
                                "ru": "Его свободная рубашка <w>раздувалась</w> на ветру, пока он ехал на велосипеде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "belly out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раздуваться наружу или выпячиваться, часто из-за ветра или давления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надуваться",
                                "target_lang_sense_clarification": "наполняться воздухом"
                            },
                            {
                                "target_lang_word": "раздуваться",
                                "target_lang_sense_clarification": "увеличиваться в объеме"
                            }
                        ]

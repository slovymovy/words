    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "b9ec973a-c3f7-34e5-8047-cccca5404368",
                    "sense_definition": "Założyciel buddyzmu, Siddhartha Gautama, czczony jako duchowy nauczyciel, który osiągnął stan pełnego oświecenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "founder",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "<w>Budda</w> nauczał, że przyczyną cierpienia jest pragnienie.",
                            "target_lang_translations": {
                                "ru": "<w>Будда</w> учил, что причиной страдания является желание."
                            }
                        },
                        {
                            "text": "Wielu pielgrzymów podróżuje do miejsc związanych z życiem <w>Buddy</w>.",
                            "target_lang_translations": {
                                "ru": "Многие паломники путешествуют по местам, связанным с жизнью <w>Будды</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Oświecony",
                        "Przebudzony"
                    ],
                    "common_phrases": [
                        "nauki Buddy",
                        "życie Buddy",
                        "uśmiech Buddy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основатель буддизма Сиддхартха Гаутама, почитаемый как духовный учитель, достигший состояния полного просветления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Будда",
                                "target_lang_sense_clarification": "основатель буддизма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5554004a-8ac8-3c70-928a-8594b64e07d8",
                    "sense_definition": "Figurka, rzeźba lub posąg przedstawiający Buddę, często pełniący funkcję religijną lub dekoracyjną.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "statue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na komodzie w salonie stoi mały, złoty <w>budda</w>.",
                            "target_lang_translations": {
                                "ru": "На комоде в гостиной стоит маленький золотой <w>будда</w>."
                            }
                        },
                        {
                            "text": "Turysta kupił drewnianego <w>buddę</w> na lokalnym targu jako pamiątkę.",
                            "target_lang_translations": {
                                "ru": "Турист купил деревянного <w>будду</w> на местном рынке в качестве сувенира."
                            }
                        }
                    ],
                    "synonyms": [
                        "posążek",
                        "figurka"
                    ],
                    "common_phrases": [
                        "złoty budda",
                        "kamienny budda",
                        "śmiejący się budda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Статуэтка, скульптура или статуя, изображающая Будду, часто используемая в религиозных или декоративных целях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "будда",
                                "target_lang_sense_clarification": "статуэтка или изображение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d2abdac-c421-3eaf-b845-bba6ba518767",
                    "sense_definition": "W buddyzmie: człowiek, który osiągnął stan pełnego oświecenia i wyzwolił się z cyklu narodzin i śmierci.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "enlightened_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Według wierzeń każdy człowiek ma potencjał, by stać się <w>buddą</w>.",
                            "target_lang_translations": {
                                "ru": "Согласно верованиям, у каждого человека есть потенциал стать <w>буддой</w>."
                            }
                        },
                        {
                            "text": "W tej świątyni oddaje się cześć wielu różnym <w>buddom</w>.",
                            "target_lang_translations": {
                                "ru": "В этом храме поклоняются многим разным <w>буддам</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "oświecony"
                    ],
                    "common_phrases": [
                        "żywy budda",
                        "przyszły budda"
                    ],
                    "target_lang_definitions": {
                        "ru": "В буддизме: человек, достигший состояния полного просветления и освободившийся от цикла перерождений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "будда",
                                "target_lang_sense_clarification": "просветленное существо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
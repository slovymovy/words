            "senses": [
                {
                    "sense_id": "7238e97d-ce4b-35c2-8d03-356b6347d9e8",
                    "sense_definition": "Displeasing to the eye; aesthetically unpleasing.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The witch in the story had a very <w>ugly</w> face.",
                            "target_lang_translations": {
                                "ru": "У ведьмы в сказке было очень <w>уродливое</w> лицо."
                            }
                        },
                        {
                            "text": "I think this is the <w>ugliest</w> building in the whole city.",
                            "target_lang_translations": {
                                "ru": "Я думаю, это самое <w>уродливое</w> здание во всём городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "unattractive",
                        "hideous",
                        "unsightly",
                        "repulsive",
                        "homely",
                        "ugly as sin"
                    ],
                    "antonyms": [
                        "beautiful",
                        "pretty",
                        "attractive",
                        "handsome",
                        "good-looking",
                        "gorgeous"
                    ],
                    "common_phrases": [
                        "ugly duckling",
                        "ugly face",
                        "look ugly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприятный на вид; эстетически непривлекательный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уродливый",
                                "target_lang_sense_clarification": "о внешности"
                            },
                            {
                                "target_lang_word": "некрасивый",
                                "target_lang_sense_clarification": "лишенный красоты"
                            },
                            {
                                "target_lang_word": "безобразный",
                                "target_lang_sense_clarification": "крайне некрасивый"
                            },
                            {
                                "target_lang_word": "страшный",
                                "target_lang_sense_clarification": "разг. очень некрасивый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8094f79-9659-3dc0-b802-cb6246665474",
                    "sense_definition": "Unpleasant, dangerous, or likely to cause trouble or violence.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "situational_unpleasantness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The argument started to get <w>ugly</w> very quickly.",
                            "target_lang_translations": {
                                "ru": "Спор очень быстро стал <w>скверным</w>."
                            }
                        },
                        {
                            "text": "There was an <w>ugly</w> scene at the meeting when the news was announced.",
                            "target_lang_translations": {
                                "ru": "Когда объявили новости, на собрании произошла <w>безобразная</w> сцена."
                            }
                        }
                    ],
                    "synonyms": [
                        "bad"
                    ],
                    "antonyms": [
                        "nice",
                        "pleasing"
                    ],
                    "common_phrases": [
                        "get ugly",
                        "turn ugly",
                        "ugly situation",
                        "ugly mood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприятный, опасный или чреватый проблемами или насилием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скверный",
                                "target_lang_sense_clarification": "о ситуации или настроении"
                            },
                            {
                                "target_lang_word": "безобразный",
                                "target_lang_sense_clarification": "возмутительный, недопустимый"
                            },
                            {
                                "target_lang_word": "неприятный",
                                "target_lang_sense_clarification": "вызывающий неприязнь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f31eda9-1c77-33ea-9500-97aefd57e3d9",
                    "sense_definition": "Offensive to one's sensibilities or morality; shocking or distasteful.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "moral_offense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He finally had to face the <w>ugly</w> truth about his business partner.",
                            "target_lang_translations": {
                                "ru": "Ему наконец пришлось признать <w>неприглядную</w> правду о своём деловом партнёре."
                            }
                        },
                        {
                            "text": "It was an <w>ugly</w> trick to play on someone so vulnerable.",
                            "target_lang_translations": {
                                "ru": "Это была <w>гнусная</w> выходка по отношению к такому уязвимому человеку."
                            }
                        }
                    ],
                    "synonyms": [
                        "vile",
                        "immoral",
                        "corrupt"
                    ],
                    "antonyms": [
                        "moral",
                        "nice"
                    ],
                    "common_phrases": [
                        "ugly truth",
                        "ugly lie",
                        "ugly side"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оскорбительный для чувств или морали; шокирующий или отвратительный."
                    },
                    "translations": {

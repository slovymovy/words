{
    "word_family": [
        "storm",
        "stormachtig",
        "bestormen",
        "stormwind",
        "stormloop"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c7bbab21-12d4-3265-bb63-60e8956a7685",
                    "sense_definition": "Bijzonder sterk en hevig waaien.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>stormde</w> geweldig die nacht en er verging een aantal schepen.",
                            "target_lang_translations": {
                                "ru": "В ту ночь сильно <w>штормило</w>, и несколько кораблей затонуло."
                            }
                        },
                        {
                            "text": "Als het buiten <w>stormt</w>, blijven de meeste mensen liever binnen.",
                            "target_lang_translations": {
                                "ru": "Когда на улице <w>штормит</w>, большинство людей предпочитают оставаться дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "spoken"
                    ],
                    "common_phrases": [
                        "het stormt",
                        "hard stormen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дуть очень сильно и порывисто; бушевать (о погоде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штормить",
                                "target_lang_sense_clarification": "безличный глагол, о погоде"
                            },
                            {
                                "target_lang_word": "бушевать",
                                "target_lang_sense_clarification": "о стихии, ветре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b80712e-b8a4-3be4-8c8e-95b5499f5405",
                    "sense_definition": "Zich met grote snelheid, wildheid of emotie voortbewegen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bel ging en de kinderen <w>stormden</w> direct naar buiten.",
                            "target_lang_translations": {
                                "ru": "Прозвенел звонок, и дети сразу <w>ринулись</w> на улицу."
                            }
                        },
                        {
                            "text": "Hij <w>stormde</w> woedend de vergaderzaal uit zonder iets te zeggen.",
                            "target_lang_translations": {
                                "ru": "Он в ярости <w>вылетел</w> из конференц-зала, не сказав ни слова."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naar binnen stormen",
                        "naar buiten stormen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться с большой скоростью, яростью или на эмоциях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нестись",
                                "target_lang_sense_clarification": "быстро двигаться"
                            },
                            {
                                "target_lang_word": "мчаться",
                                "target_lang_sense_clarification": "стремительно бежать"
                            },
                            {
                                "target_lang_word": "рываться",
                                "target_lang_sense_clarification": "резко устремляться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f9ee6bf8-7242-33d7-a494-c4b9fe4f9ba8",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>storm</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Koraalriffen zijn belangrijk voor de bescherming van kustlijnen tegen erosie en <w>stormen</w>.",
                            "target_lang_translations": {
                                "ru": "Коралловые рифы важны для защиты береговых линий от эрозии и <w>штормов</w>."
                            }
                        },
                        {
                            "text": "In het najaar trekken er vaak zware <w>stormen</w> over het land.",
                            "target_lang_translations": {
                                "ru": "Осенью над страной часто проходят сильные <w>бури</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zware stormen",
                        "hevige stormen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число существительного «шторм» (сильная буря)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штормы",
                                "target_lang_sense_clarification": "мн. ч. от шторм"
                            },
                            {
                                "target_lang_word": "бури",
                                "target_lang_sense_clarification": "сильные ненастья"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "a41548e7-1451-3b84-8cd8-20f941f87d88",
                    "sense_definition": "Een watervogel uit de familie van de eendachtigen (Anatidae).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zwemt een <w>eend</w> in de vijver.",
                            "target_lang_translations": {
                                "ru": "В пруду плавает <w>утка</w>.",
                                "en": "A <w>duck</w> is swimming in the pond."
                            }
                        },
                        {
                            "text": "Hij voelde zich een vreemde <w>eend</w> in de bijt toen hij niemand kende op het feest.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя белой <w>вороной</w>, так как никого не знал на вечеринке.",
                                "en": "He felt like an odd <w>duck</w> when he didn't know anyone at the party."
                            }
                        }
                    ],
                    "synonyms": [
                        "doks",
                        "drijfsijs"
                    ],
                    "common_phrases": [
                        "vreemde eend in de bijt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Водоплавающая птица из семейства утиных (Anatidae).",
                        "en": "A waterbird from the family Anatidae."
                    },
                    "translations": {

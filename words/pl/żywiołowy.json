    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f54ee196-10c6-3612-9a69-1dfa4715e7d5",
                    "sense_definition": "Taki, który jest skutkiem działania nieopanowanych sił natury.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "nature_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klęska <w>żywiołowa</w> zniszczyła wiele domów w tej okolicy.",
                            "target_lang_translations": {
                                "ru": "<w>Стихийное</w> бедствие разрушило много домов в этой округе."
                            }
                        },
                        {
                            "text": "Mieszkańcy bali się <w>żywiołowej</w> siły rzeki podczas powodzi.",
                            "target_lang_translations": {
                                "ru": "Жители боялись <w>стихийной</w> силы реки во время наводнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "gwałtowny"
                    ],
                    "common_phrases": [
                        "klęska żywiołowa",
                        "siła żywiołowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с действием неукротимых сил природы; вызванный стихией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стихийный",
                                "target_lang_sense_clarification": "о силах природы, бедствиях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97cb1db8-eb85-3360-ae3d-ca47ecb08b84",
                    "sense_definition": "Pełen energii, gwałtowny i kierowany silnymi emocjami (o człowieku lub jego zachowaniu).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "temperament",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po godzinie <w>żywiołowych</w> dyskusji zebranie wreszcie się skończyło.",
                            "target_lang_translations": {
                                "ru": "После часа <w>бурных</w> дискуссий собрание наконец закончилось."
                            }
                        },
                        {
                            "text": "Jej <w>żywiołowy</w> temperament sprawia, że zawsze jest w centrum uwagi.",
                            "target_lang_translations": {
                                "ru": "Её <w>бурный</w> темперамент делает так, что она всегда в центре внимания."
                            }
                        }
                    ],
                    "synonyms": [
                        "porywczy",
                        "impulsywny",
                        "wybuchowy",
                        "ekspansywny",
                        "burzliwy",
                        "gwałtowny"
                    ],
                    "antonyms": [
                        "spokojny",
                        "opanowany"
                    ],
                    "common_phrases": [
                        "żywiołowy temperament",
                        "żywiołowa dyskusja",
                        "żywiołowa reakcja",
                        "żywiołowe oklaski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный энергии, порывистый и управляемый сильными эмоциями (о человеке или его поведении)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бурный",
                                "target_lang_sense_clarification": "о реакции, дискуссии, проявлении чувств"
                            },
                            {
                                "target_lang_word": "темпераментный",
                                "target_lang_sense_clarification": "о человеке, характере"
                            },
                            {
                                "target_lang_word": "неукротимый",
                                "target_lang_sense_clarification": "об энергии, силе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60bbda17-1fed-35a2-a329-766944bfd896",
                    "sense_definition": "Dziejący się w sposób nagły, nieplanowany i spontaniczny.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "spontaneity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obserwujemy <w>żywiołowy</w> rozwój handlu w internecie.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдаем <w>стихийное</w> развитие торговли в интернете."
                            }
                        },
                        {
                            "text": "Wybuchł <w>żywiołowy</w> protest przeciwko nowym przepisom.",
                            "target_lang_translations": {
                                "ru": "Вспыхнул <w>стихийный</w> протест против новых правил."
                            }
                        }
                    ],
                    "synonyms": [
                        "spontaniczny"
                    ],
                    "antonyms": [
                        "planowany"
                    ],
                    "common_phrases": [
                        "żywiołowy rozwój",
                        "żywiołowy proces"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий внезапно, незапланированно и спонтанно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стихийный",
                                "target_lang_sense_clarification": "неорганизованный, спонтанный"
                            },
                            {
                                "target_lang_word": "спонтанный",
                                "target_lang_sense_clarification": "самопроизвольный"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1662790c-dc08-3d65-9e00-6e66e542ac65",
                    "sense_definition": "Op dit voorwerp, op deze plaats of met betrekking tot wat net genoemd is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt de zware doos <w>hierop</w> zetten.",
                            "target_lang_translations": {
                                "en": "You can put the heavy box <w>on this</w>.",
                                "ru": "Ты можешь поставить тяжелую коробку <w>на это</w>."
                            }
                        },
                        {
                            "text": "De directeur had <w>hierop</w> geen duidelijk antwoord.",
                            "target_lang_translations": {
                                "en": "The director had no clear answer <w>to this</w>.",
                                "ru": "У директора не было ясного ответа <w>на это</w>."
                            }
                        },
                        {
                            "text": "Klik <w>hierop</w> om naar de volgende pagina te gaan.",
                            "target_lang_translations": {
                                "en": "Click <w>on this</w> to go to the next page.",
                                "ru": "Нажмите <w>сюда</w>, чтобы перейти на следующую страницу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "antwoord hierop",
                        "reactie hierop",
                        "klik hierop"
                    ],
                    "target_lang_definitions": {
                        "en": "On this object, place, or regarding what was just mentioned.",
                        "ru": "На этот предмет, на это место или касательно того, что только что было упомянуто."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on this",
                                "target_lang_sense_clarification": "referring to a physical object or location"
                            },
                            {
                                "target_lang_word": "to this",
                                "target_lang_sense_clarification": "referring to a statement, question, or abstract concept"
                            },
                            {
                                "target_lang_word": "on it",
                                "target_lang_sense_clarification": "referring to a previously mentioned object"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "на это",
                                "target_lang_sense_clarification": "указание на объект или тему"
                            },
                            {
                                "target_lang_word": "сюда",
                                "target_lang_sense_clarification": "указание места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48b285b5-0f61-3cac-9a6e-8d263d9df1f2",
                    "sense_definition": "Direct na dit moment of als reactie op deze gebeurtenis.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hierop</w> besloot de voorzitter de vergadering te sluiten.",
                            "target_lang_translations": {
                                "en": "<w>Thereupon</w> the chairman decided to close the meeting.",
                                "ru": "<w>После этого</w> председатель решил закрыть собрание."
                            }
                        },
                        {
                            "text": "Hij beledigde haar en <w>hierop</w> liep ze direct weg.",
                            "target_lang_translations": {
                                "en": "He insulted her and <w>following this</w> she walked away immediately.",
                                "ru": "Он оскорбил ее, и <w>в ответ на это</w> она сразу ушла."
                            }
                        }
                    ],
                    "synonyms": [
                        "daarna",
                        "vervolgens"
                    ],
                    "common_phrases": [
                        "vlak hierop",
                        "kort hierop"
                    ],
                    "target_lang_definitions": {
                        "en": "Immediately after this moment or as a reaction to this event.",
                        "ru": "Сразу после этого момента или как реакция на это событие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thereupon",
                                "target_lang_sense_clarification": "formal, indicating a direct consequence or sequence"
                            },
                            {

                },
                {
                    "sense_id": "72d44fdc-9cf9-34c9-985a-c8d7c13990ea",
                    "sense_definition": "Of low social rank or status; ordinary.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He rose from being a <w>simple</w> worker to the manager of the factory.",
                            "target_lang_translations": {
                                "ru": "Он прошел путь от <w>простого</w> рабочего до директора завода."
                            }
                        },
                        {
                            "text": "The story is about a <w>simple</w> soldier who became a hero during the war.",
                            "target_lang_translations": {
                                "ru": "Это история о <w>простом</w> солдате, который стал героем во время войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "ordinary",
                        "common",
                        "humble",
                        "lowly"
                    ],
                    "antonyms": [
                        "eminent",
                        "noble",
                        "highborn"
                    ],
                    "common_phrases": [
                        "simple folk",
                        "simple soldier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Низкого социального ранга или статуса; обычный."

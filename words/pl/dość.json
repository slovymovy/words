                            {
                                "target_lang_word": "довольно",
                                "target_lang_sense_clarification": "стоп, достаточно"
                            },
                            {
                                "target_lang_word": "достаточно",
                                "target_lang_sense_clarification": "команда остановиться"
                            }
                        ]
                    }
                }
            ]
        },
        {

                            },
                            {
                                "target_lang_word": "каракули",
                                "target_lang_sense_clarification": "неразборчивый почерк"
                            },
                            {
                                "target_lang_word": "пачкотня",
                                "target_lang_sense_clarification": "небрежная работа"

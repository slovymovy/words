                },
                {
                    "sense_id": "09b4f159-b23a-3463-9bd0-66e9129b67a6",
                    "sense_definition": "De verzameling landen in Europa en Noord-Amerika met een westerse cultuur en politiek.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geopolitical_region",
                    "name_type": "no",
                    "examples": [

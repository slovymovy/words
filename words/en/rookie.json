                },
                {
                    "sense_id": "9e3e4ccd-8af7-3e77-8c9f-5dc90df3600c",
                    "sense_definition": "A new recruit in the police force or armed services.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "novice_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sergeant was assigned to train the <w>rookie</w> on proper patrol procedures.",

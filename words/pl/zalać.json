    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4d29fbc9-443f-3b0b-8e0b-88c42e515bda",
                    "sense_definition": "Wlać płyn do naczynia lub pokryć nim jakiś produkt, zwykle w celu przyrządzenia potrawy, napoju lub napełnienia zbiornika.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "liquid_manipulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <zalać> torebkę herbaty wrzątkiem i poczekać pięć minut, aż się zaparzy.",
                            "target_lang_translations": {
                                "ru": "Ты должен <залить> чайный пакетик кипятком и подождать пять минут, пока он заварится."
                            }
                        },
                        {
                            "text": "Przed długą podróżą warto <zalać> bak samochodu do pełna, żeby nie szukać stacji po drodze.",
                            "target_lang_translations": {
                                "ru": "Перед долгой поездкой стоит <залить> полный бак, чтобы не искать заправку по дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "napełnić",
                        "wlać"
                    ],
                    "antonyms": [
                        "wylać",
                        "odlać",
                        "opróżnić"
                    ],
                    "common_phrases": [
                        "zalać wrzątkiem",
                        "zalać kawę",
                        "zalać do pełna",
                        "zalać wodą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Налить жидкость в емкость или покрыть ею продукт, обычно для приготовления пищи или заправки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "залить",
                                "target_lang_sense_clarification": "покрыть жидкостью (напр. чай, хлопья)"
                            },
                            {
                                "target_lang_word": "наполнить",
                                "target_lang_sense_clarification": "заполнить емкость (напр. бак)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "402b8526-b37b-3181-9095-26f06d6ff755",
                    "sense_definition": "O wodzie lub innej cieczy: pokryć duży obszar, powodując zniszczenia; spowodować powódź w jakimś miejscu lub pomieszczeniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po gwałtownych opadach deszczu rzeka wylała i <zalała> okoliczne pola uprawne.",
                            "target_lang_translations": {
                                "ru": "После сильных ливней река вышла из берегов и <затопила> окрестные поля."
                            }
                        },
                        {
                            "text": "Pęknięta rura w łazience sprawiła, że niechcący <zalaliśmy> mieszkanie sąsiada z dołu.",
                            "target_lang_translations": {
                                "ru": "Лопнувшая труба в ванной привела к тому, что мы нечаянно <залили> квартиру соседа снизу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatopić",
                        "podtopić"
                    ],
                    "antonyms": [
                        "osuszyć"
                    ],
                    "common_phrases": [
                        "zalać mieszkanie",
                        "zalać sąsiada",
                        "woda zalała piwnicę",
                        "rzeka zalała miasto"
                    ],
                    "target_lang_definitions": {
                        "ru": "О воде или другой жидкости: покрыть большую территорию или помещение, причиняя ущерб."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затопить",
                                "target_lang_sense_clarification": "о наводнении или местности"
                            },
                            {
                                "target_lang_word": "залить",
                                "target_lang_sense_clarification": "о квартире или соседях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98b2ef8c-13fe-34b5-9cbe-29029ad90921",
                    "sense_definition": "Przypadkowo wylać płyn na jakiś przedmiot, ubranie lub powierzchnię, często powodując plamę lub uszkodzenie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Byłem bardzo nieostrożny i <zalałem> ważne dokumenty kubkiem gorącej kawy.",
                            "target_lang_translations": {
                                "ru": "Я был очень неосторожен и <залил> важные документы чашкой горячего кофе."
                            }
                        },
                        {
                            "text": "Jeśli <zalejesz> klawiaturę laptopa słodkim napojem, natychmiast odłącz zasilanie.",
                            "target_lang_translations": {
                                "ru": "Если <зальешь> клавиатуру ноутбука сладким напитком, немедленно отключи питание."
                            }
                        }
                    ],
                    "synonyms": [
                        "zachlapać",
                        "poplamić",
                        "oblać"
                    ],
                    "antonyms": [
                        "wyczyścić",
                        "wysuszyć"
                    ],
                    "common_phrases": [
                        "zalać stół",
                        "zalać ubranie",
                        "zalać klawiaturę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случайно пролить жидкость на предмет или поверхность, часто пачкая или повреждая их."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "залить",
                                "target_lang_sense_clarification": "пролить жидкость на что-то"
                            },
                            {
                                "target_lang_word": "испачкать",
                                "target_lang_sense_clarification": "поставить пятно жидкостью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2477442-1ced-3be8-b4e3-c97919bcf6c0",
                    "sense_definition": "O zjawiskach takich jak światło, dźwięk lub silne emocje: wypełnić sobą całą przestrzeń lub całkowicie kogoś opanować.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy rano otworzyłem okiennice, jasne słońce natychmiast <zalało> moją sypialnię.",
                            "target_lang_translations": {
                                "ru": "Когда я утром открыл ставни, яркое солнце мгновенно <залило> мою спальню."
                            }
                        },
                        {
                            "text": "Kiedy usłyszała tę wspaniałą nowinę, wielka fala radości <zalała> jej serce.",
                            "target_lang_translations": {
                                "ru": "Когда она услышала эту замечательную новость, огромная волна радости <захлестнула> её сердце."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypełnić",
                        "ogarnąć",
                        "przepełnić"
                    ],
                    "common_phrases": [
                        "krew kogoś zalała",
                        "słońce zalało pokój",
                        "zalać się łzami",
                        "fala gorąca zalała"
                    ],
                    "target_lang_definitions": {
                        "ru": "О свете, звуке или эмоциях: заполнить собой пространство или полностью овладеть кем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "залить",
                                "target_lang_sense_clarification": "о свете или звуке"
                            },
                            {
                                "target_lang_word": "охватить",
                                "target_lang_sense_clarification": "о чувствах или эмоциях"
                            },
                            {
                                "target_lang_word": "наполнить",
                                "target_lang_sense_clarification": "заполнить пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1e370f5-710d-3536-a170-45cac10711fe",
                    "sense_definition": "Wypić bardzo dużą ilość alkoholu, doprowadzając się do stanu silnego upojenia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "alcohol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój znajomy postanowił <zalać> się w trupa po tym, jak niespodziewanie stracił pracę.",
                            "target_lang_translations": {
                                "ru": "Мой знакомый решил <напиться> в стельку после того, как неожиданно потерял работу."
                            }
                        },
                        {
                            "text": "W weekendy lubili spotykać się w barze, żeby <zalać> robaka i zapomnieć o problemach.",
                            "target_lang_translations": {
                                "ru": "По выходным они любили встречаться в баре, чтобы <залить> горе и забыть о проблемах."
                            }
                        }
                    ],
                    "synonyms": [
                        "upić się",
                        "schlać się",
                        "nawalić się"
                    ],
                    "antonyms": [
                        "wytrzeźwieć"
                    ],
                    "common_phrases": [
                        "zalać robaka",
                        "zalać pałę",
                        "zalać się w trupa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпить большое количество алкоголя, сильно опьянеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напиться",
                                "target_lang_sense_clarification": "употребить много алкоголя"
                            },
                            {
                                "target_lang_word": "залить",
                                "target_lang_sense_clarification": "в идиомах (залить горе, залить глаза)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce29d4f8-b987-35f8-81fc-1f07f85f45de",
                    "sense_definition": "Dostarczyć do silnika spalinowego zbyt dużą dawkę paliwa, co uniemożliwia zapłon i uruchomienie maszyny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Próbowałem odpalić stary motocykl na mrozie, ale chyba <zalałem> świecę.",
                            "target_lang_translations": {
                                "ru": "Я пытался завести старый мотоцикл на морозе, но, кажется, <залил> свечу."
                            }
                        },
                        {
                            "text": "Nie naciskaj gwałtownie gazu przy rozruchu, bo możesz <zalać> silnik i nigdzie nie pojedziesz.",
                            "target_lang_translations": {
                                "ru": "Не нажимай резко на газ при запуске, иначе можешь <залить> двигатель и никуда не поедешь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zalać silnik",
                        "zalać świece"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подать в двигатель слишком много топлива, что препятствует зажиганию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "залить",
                                "target_lang_sense_clarification": "свечи или двигатель"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверка",
                                "target_lang_sense_clarification": "действие по глаголу"
                            },
                            {
                                "target_lang_word": "отбор",
                                "target_lang_sense_clarification": "процесс выбора"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "65a79905-4ae7-35d3-9b8f-7b6efedf2983",
                    "sense_definition": "Past participle of <w>give</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have <w>given</w> you all the information I have.",
                            "target_lang_translations": {
                                "ru": "Я <w>дал</w> тебе всю информацию, которая у меня есть."
                            }
                        },
                        {
                            "text": "She has <w>given</w> up sugar for a month.",
                            "target_lang_translations": {
                                "ru": "Она <w>отказалась</w> от сахара на месяц."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие прошедшего времени от глагола give (давать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "данный",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "дать",
                                "target_lang_sense_clarification": "глагол"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "a5d4309c-8dc4-38af-8370-e67a49eab3fc",
                    "sense_definition": "Considering or taking into account a specific fact or situation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Given</w> the bad weather, we decided to stay indoors.",
                            "target_lang_translations": {
                                "ru": "<w>Учитывая</w> плохую погоду, мы решили остаться дома."
                            }
                        },
                        {
                            "text": "It was a surprising result, <w>given</w> that he had not studied much.",
                            "target_lang_translations": {
                                "ru": "Это был неожиданный результат, <w>учитывая</w>, что он мало занимался."
                            }
                        }
                    ],
                    "synonyms": [
                        "considering"
                    ],
                    "common_phrases": [
                        "given the circumstances",
                        "given the fact",
                        "given that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимая во внимание определенный факт или ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учитывая",
                                "target_lang_sense_clarification": "принимая во внимание"
                            },
                            {
                                "target_lang_word": "ввиду",
                                "target_lang_sense_clarification": "из-за"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5fb8d247-81f5-3f28-ae05-e64ae18a4e57",
                    "sense_definition": "Particular, specific, or previously arranged.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "specified",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bacteria count in a <w>given</w> area can vary significantly.",
                            "target_lang_translations": {
                                "ru": "Количество бактерий в <w>данной</w> области может значительно варьироваться."
                            }
                        },
                        {
                            "text": "You must complete the test within the <w>given</w> time limit.",
                            "target_lang_translations": {
                                "ru": "Вы должны завершить тест в течение <w>отведенного</w> времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "specific",
                        "particular",
                        "specified"
                    ],
                    "common_phrases": [
                        "at any given time",
                        "within a given period",
                        "on a given day"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный, определенный или заранее оговоренный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "данный",
                                "target_lang_sense_clarification": "этот, рассматриваемый"
                            },
                            {
                                "target_lang_word": "определенный",
                                "target_lang_sense_clarification": "установленный"
                            },
                            {
                                "target_lang_word": "конкретный",
                                "target_lang_sense_clarification": "специфический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38b13017-87d6-3131-8f75-38109c5473a5",
                    "sense_definition": "Inclined or disposed to do something; prone.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "prone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>given</w> to making long speeches after dinner.",
                            "target_lang_translations": {
                                "ru": "Он <w>склонен</w> произносить длинные речи после ужина."
                            }
                        },
                        {
                            "text": "She was <w>given</w> to fits of laughter at inappropriate moments.",
                            "target_lang_translations": {
                                "ru": "Она была <w>склонна</w> к приступам смеха в неподходящие моменты."
                            }
                        }
                    ],
                    "synonyms": [
                        "prone",
                        "inclined",
                        "disposed"
                    ],
                    "common_phrases": [
                        "given to complaining",
                        "given to excess"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный или расположенный к чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склонный",
                                "target_lang_sense_clarification": "имеющий предрасположенность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "678c0578-a5a1-3ce1-9af5-549948f72d4b",
                    "sense_definition": "A known fact, condition, or situation that is accepted as true.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is a <w>given</w> that the internet has changed how we communicate.",
                            "target_lang_translations": {
                                "ru": "То, что интернет изменил наше общение, — это <w>данность</w>."
                            }
                        },
                        {
                            "text": "We accepted the budget constraints as a <w>given</w> and planned accordingly.",
                            "target_lang_translations": {
                                "ru": "Мы приняли бюджетные ограничения как <w>данность</w> и планировали соответственно."
                            }
                        }
                    ],
                    "synonyms": [
                        "certainty",
                        "fact"
                    ],
                    "common_phrases": [
                        "take as a given",
                        "it is a given"
                    ],
                    "target_lang_definitions": {
                        "ru": "Известный факт, условие или ситуация, принимаемые за истину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "данность",
                                "target_lang_sense_clarification": "факт"
                            }
                        ]
                    }
                }
            ]
        },

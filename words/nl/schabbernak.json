            "senses": [
                {
                    "sense_id": "67ff0ae7-1d83-308a-8444-e61ff48a4887",
                    "sense_definition": "Een persoon, meestal een kind, die ondeugend is en streken uithaalt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleine <w>schabbernak</w> heeft voor de grap zout in de suikerpot gedaan.",
                            "target_lang_translations": {
                                "ru": "Маленький <w>проказник</w> ради шутки насыпал соль в сахарницу.",
                                "en": "The little <w>rascal</w> put salt in the sugar bowl as a joke."
                            }
                        },
                        {
                            "text": "Mijn neefje is een echte <w>schabbernak</w> die de hele dag door het huis rent.",
                            "target_lang_translations": {
                                "ru": "Мой племянник — настоящий <w>сорванец</w>, который целый день носится по дому.",
                                "en": "My nephew is a real <w>scallywag</w> who runs around the house all day."
                            }
                        }
                    ],
                    "synonyms": [
                        "kwajongen",
                        "rekel",
                        "belhamel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обычно ребенок, который шалит и устраивает проделки.",
                        "en": "A person, usually a child, who is mischievous and plays pranks."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сорванец",
                                "target_lang_sense_clarification": "озорной, бойкий ребенок"

                    "sense_id": "3c9688c1-4f61-32d9-a776-8f508f565329",
                    "sense_definition": "Een rond, plat schijfje waarop digitale gegevens zoals muziek of bestanden worden opgeslagen; afkorting van <w>compact disc</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hier heeft u mijn <w>cd</w> met alle foto's van de reis.",
                            "target_lang_translations": {
                                "ru": "Вот мой <w>компакт-диск</w> со всеми фотографиями из поездки.",
                                "en": "Here is my <w>CD</w> with all the photos from the trip."
                            }
                        },
                        {
                            "text": "Zij luistert graag naar muziek op een <w>cd</w> in de auto.",
                            "target_lang_translations": {
                                "ru": "Она любит слушать музыку на <w>компакт-диске</w> в машине.",
                                "en": "She likes listening to music on a <w>CD</w> in the car."
                            }
                        }
                    ],
                    "synonyms": [
                        "compact disc"
                    ],
                    "common_phrases": [
                        "een cd branden",
                        "een cd opzetten",
                        "een cd draaien"
                    ],
                    "target_lang_definitions": {

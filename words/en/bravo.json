{
    "word_family": [
        "brave",
        "bravado",
        "bravura"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7f5c0bc1-893a-3ef3-9df9-9ee654511b33",
                    "sense_definition": "Used to express acclaim or strong approval, especially to a performer.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "acclaim_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Bravo</w>, you have done a brilliant job on the presentation!",
                            "target_lang_translations": {
                                "ru": "<w>Браво</w>, ты проделал блестящую работу над презентацией!"
                            }
                        },
                        {
                            "text": "The audience stood up and shouted <w>bravo</w> at the end of the opera.",
                            "target_lang_translations": {
                                "ru": "Зрители встали и кричали «<w>браво</w>» в конце оперы."
                            }
                        }
                    ],
                    "synonyms": [
                        "well done",
                        "good job",
                        "congratulations",
                        "encore"
                    ],
                    "common_phrases": [
                        "shout bravo",
                        "cry bravo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения одобрения или восхищения, особенно исполнителем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "браво",
                                "target_lang_sense_clarification": "междометие"
                            },
                            {
                                "target_lang_word": "молодец",
                                "target_lang_sense_clarification": "похвала за хорошую работу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3453d089-a7b3-3b0d-9236-bb7ca015408d",
                    "sense_definition": "A shout or cry of \"bravo\" to express approval.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "acclaim_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was a roar of <w>bravos</w> from the crowd as the winner was announced.",
                            "target_lang_translations": {
                                "ru": "Толпа разразилась гулом <w>возгласов «браво»</w>, когда объявили победителя."
                            }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4e5d76a4-4e47-3d7d-b7df-3d7657eb215d",
                    "sense_definition": "Заниматься продажей товаров, вести коммерческие дела или обмен с кем-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "commerce_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя бабушка каждое лето <торгует> свежими овощами на рынке.",
                            "target_lang_translations": {
                                "en": "My grandmother <sells> fresh vegetables at the market every summer."
                            }
                        },
                        {
                            "text": "Эта крупная компания успешно <торгует> с зарубежными партнёрами.",
                            "target_lang_translations": {
                                "en": "This large company successfully <trades> with foreign partners."
                            }
                        },
                        {
                            "text": "В этом маленьком киоске <торгуют> газетами и журналами.",
                            "target_lang_translations": {
                                "en": "They <sell> newspapers and magazines in this small kiosk."
                            }
                        }
                    ],
                    "synonyms": [
                        "сговариваться",
                        "сговариваться"
                    ],
                    "antonyms": [
                        "покупать",
                        "приобретать"
                    ],
                    "common_phrases": [
                        "торговать на рынке",
                        "торговать оптом",
                        "торговать в розницу",
                        "торговать с заграницей",
                        "торговать лицом"
                    ],
                    "target_lang_definitions": {
                        "en": "To engage in the sale of goods, conduct commercial business, or exchange with someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sell",
                                "target_lang_sense_clarification": "to exchange goods for money"
                            },
                            {
                                "target_lang_word": "trade",
                                "target_lang_sense_clarification": "to engage in commerce or exchange"
                            },
                            {
                                "target_lang_word": "deal in",
                                "target_lang_sense_clarification": "to buy and sell specific products"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "825c8c32-2bd5-38a1-98eb-d68d05b76804",
                    "sense_definition": "Быть открытым для покупателей, работать (о магазине, рынке или другом торговом заведении).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "commerce_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В праздничные дни этот магазин не <торгует>.",
                            "target_lang_translations": {
                                "en": "This shop is not <open> on holidays."
                            }
                        },
                        {
                            "text": "Местный рынок <торгует> только по выходным до обеда.",
                            "target_lang_translations": {
                                "en": "The local market <is open> only on weekends until lunch."
                            }
                        }
                    ],
                    "synonyms": [
                        "сговариваться",
                        "сговариваться"
                    ],
                    "common_phrases": [
                        "магазин торгует",
                        "рынок торгует",
                        "лавка торгует"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be open for customers, to operate (about a shop, market, or other commercial establishment)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be open",
                                "target_lang_sense_clarification": "to be available for business"
                            },
                            {
                                "target_lang_word": "trade",
                                "target_lang_sense_clarification": "to operate as a business"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad3fad85-21d5-3ea0-af8f-d684c9cd8456",
                    "sense_definition": "Использовать что-либо ценное, личное или морально значимое ради материальной выгоды (в переносном смысле).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Честный человек никогда не станет <торговать> своей совестью ради денег.",
                            "target_lang_translations": {
                                "en": "An honest person would never <sell> their conscience for money."
                            }
                        },
                        {
                            "text": "Политики не должны <торговать> интересами своей страны.",
                            "target_lang_translations": {
                                "en": "Politicians should not <trade> their country's interests."
                            }
                        },
                        {
                            "text": "Он отказался <торговать> секретами фирмы, несмотря на предложенную взятку.",
                            "target_lang_translations": {
                                "en": "He refused to <sell> company secrets, despite the offered bribe."
                            }
                        }
                    ],
                    "common_phrases": [
                        "торговать совестью",
                        "торговать принципами",
                        "торговать телом",
                        "торговать собой"
                    ],
                    "target_lang_definitions": {
                        "en": "To use something valuable, personal, or morally significant for material gain (figuratively)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sell",
                                "target_lang_sense_clarification": "to betray or compromise values for money"
                            },
                            {
                                "target_lang_word": "trade",
                                "target_lang_sense_clarification": "to exchange or exploit something valuable"
                            }
                        ]
                    }
                }
            ]
        }
    ]

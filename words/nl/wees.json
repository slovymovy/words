                {
                    "sense_id": "06ff198d-f9ad-3b91-8af8-51650122afb3",
                    "sense_definition": "Gebiedende wijs van <w>zijn</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "imperative_zijn",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wees voorzichtig als je de drukke straat oversteekt.",
                            "target_lang_translations": {
                                "en": "<w>Be</w> careful when crossing the busy street.",
                                "ru": "<w>Будь</w> осторожен, когда переходишь оживленную улицу."
                            }
                        },
                        {
                            "text": "Wees niet bang, want ik blijf bij je.",
                            "target_lang_translations": {
                                "en": "Do not <w>be</w> afraid, for I am staying with you.",
                                "ru": "Не <w>бойся</w>, ведь я останусь с тобой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wees voorzichtig",
                        "wees stil",
                        "wees welkom",
                        "wees gerust"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative form of <w>to be</w>.",
                        "ru": "Форма повелительного наклонения глагола <w>zijn</w> (быть)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "imperative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "будь",
                                "target_lang_sense_clarification": "повелительное наклонение (ед.ч.)"

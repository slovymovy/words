        "chronic"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6a163cad-6e50-37e7-9220-f71876df4624",
                    "sense_definition": "A written record of historical events arranged in the order in which they happened.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The monk wrote a detailed <w>chronicle</w> of the war.",
                            "target_lang_translations": {
                                "ru": "Монах написал подробную <w>летопись</w> войны."
                            }
                        },
                        {
                            "text": "This book is a fascinating <w>chronicle</w> of the twentieth century.",
                            "target_lang_translations": {
                                "ru": "Эта книга — увлекательная <w>хроника</w> двадцатого века."
                            }
                        }
                    ],
                    "synonyms": [
                        "record",
                        "history",
                        "annals",
                        "journal"
                    ],
                    "common_phrases": [
                        "historical chronicle",
                        "ancient chronicle",
                        "family chronicle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Письменная запись исторических событий в том порядке, в котором они происходили."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хроника",
                                "target_lang_sense_clarification": "запись событий в хронологическом порядке"
                            },
                            {
                                "target_lang_word": "летопись",
                                "target_lang_sense_clarification": "историческое повествование по годам"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "399dd5e7-53bc-3490-99d3-c687d6820c60",
                    "sense_definition": "To record a series of events in a factual and detailed way.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The documentary <w>chronicles</w> the rise of the famous band.",
                            "target_lang_translations": {
                                "ru": "Этот документальный фильм <w>описывает</w> становление известной группы."
                            }
                        },
                        {
                            "text": "She <w>chronicled</w> her travels in a daily blog.",
                            "target_lang_translations": {
                                "ru": "Она <w>вела хронику</w> своих путешествий в ежедневном блоге."
                            }
                        }
                    ],
                    "synonyms": [
                        "record"
                    ],
                    "common_phrases": [
                        "chronicle the history",
                        "chronicle events",
                        "chronicle life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Записывать или описывать последовательность событий фактическим и подробным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести хронику",
                                "target_lang_sense_clarification": "регулярно записывать происходящие события"
                            },
                            {
                                "target_lang_word": "описывать",
                                "target_lang_sense_clarification": "подробно излагать ход событий"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
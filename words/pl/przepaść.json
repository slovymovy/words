{
    "word_family": [
        "przepadać",
        "przepadnięcie",
        "upadek",
        "wypadek",
        "przypadek",
        "zapadnia"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "21a285c2-cfeb-39ce-8222-9519a4a19db8",
                    "sense_definition": "Bardzo głęboka, stroma dziura w ziemi lub przestrzeń o wielkiej głębokości, często występująca w górach.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musieliśmy bardzo uważać, żeby nie spaść w głęboką <w>przepaść</w>.",
                            "target_lang_translations": {
                                "ru": "Мы должны были быть очень осторожны, чтобы не упасть в глубокую <w>пропасть</w>."
                            }
                        },
                        {
                            "text": "Samochód zatrzymał się tuż nad <w>przepaścią</w>, co uratowało pasażerów.",
                            "target_lang_translations": {
                                "ru": "Машина остановилась прямо над <w>пропастью</w>, что спасло пассажиров."
                            }
                        }
                    ],
                    "synonyms": [
                        "otchłań",
                        "urwisko",
                        "czeluść"
                    ],
                    "common_phrases": [
                        "nad przepaścią",
                        "skraj przepaści",
                        "głęboka przepaść"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень глубокая, крутая яма в земле или пространство огромной глубины, часто встречающееся в горах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "про́пасть",
                                "target_lang_sense_clarification": "глубокий обрыв, бездна"
                            },
                            {
                                "target_lang_word": "обрыв",
                                "target_lang_sense_clarification": "крутой склон"
                            },
                            {
                                "target_lang_word": "бездна",
                                "target_lang_sense_clarification": "огромная глубина"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "35f14a44-cf36-31cc-9072-bfeca7e1ac57",
                    "sense_definition": "Zniknąć nagle i bez śladu; stać się niemożliwym do odnalezienia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_disappear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój ulubiony długopis gdzieś <w>przepadł</w> i nie mogę go znaleźć.",
                            "target_lang_translations": {
                                "ru": "Моя любимая ручка где-то <w>пропала</w>, и я не могу её найти."
                            }
                        },
                        {
                            "text": "Pies uciekł do lasu i <w>przepadł</w> bez wieści na wiele dni.",
                            "target_lang_translations": {
                                "ru": "Пёс убежал в лес и <w>пропал</w> без вести на много дней."
                            }
                        }
                    ],
                    "synonyms": [
                        "czeluść",
                        "otchłań",
                        "urwisko"
                    ],
                    "antonyms": [
                        "odnaleźć się"
                    ],
                    "common_phrases": [
                        "przepaść bez wieści",
                        "kamień w wodę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно исчезнуть без следа; потеряться так, что невозможно найти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропа́сть",
                                "target_lang_sense_clarification": "исчезнуть, потеряться"
                            },
                            {
                                "target_lang_word": "исчезнуть",
                                "target_lang_sense_clarification": "перестать быть видимым, скрыться"
                            },
                            {
                                "target_lang_word": "затеряться",
                                "target_lang_sense_clarification": "потеряться среди чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdbefc6f-31c3-39ea-868b-ae3ff10b938b",
                    "sense_definition": "Stracić ważność (np. bilet, termin) lub zostać utraconym z powodu niewykorzystania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_forfeit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli nie odbierzesz nagrody w ciągu tygodnia, to ona <w>przepadnie</w>.",
                            "target_lang_translations": {
                                "ru": "Если ты не заберёшь награду в течение недели, она <w>пропадёт</w>."
                            }
                        },
                        {
                            "text": "Kupiłem bilety do kina, ale zachorowałem i wszystko <w>przepadło</w>.",
                            "target_lang_translations": {
                                "ru": "Я купил билеты в кино, но заболел, и всё <w>пропало</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kaucja przepadła",
                        "termin przepadł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерять силу (например, о билете или сроке) или быть утраченным из-за неиспользования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропа́сть",
                                "target_lang_sense_clarification": "остаться неиспользованным, пройти зря"
                            },
                            {
                                "target_lang_word": "сгореть",
                                "target_lang_sense_clarification": "разг. утратить силу (о билетах, путевках)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
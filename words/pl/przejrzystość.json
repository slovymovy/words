    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1e7fec49-857d-3e68-9ac2-ddbc8601a45f",
                    "sense_definition": "Właściwość fizyczna substancji, np. wody lub powietrza, pozwalająca na swobodne przenikanie światła i wyraźne widzenie tego, co znajduje się dalej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_transparency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Duża <w>przejrzystość</w> wody w jeziorze pozwalała dostrzec dno nawet na głębokości kilku metrów.",
                            "target_lang_translations": {
                                "ru": "Высокая <w>прозрачность</w> воды в озере позволяла разглядеть дно даже на глубине нескольких метров."
                            }
                        },
                        {
                            "text": "Mgła opadła i <w>przejrzystość</w> powietrza znacznie się poprawiła.",
                            "target_lang_translations": {
                                "ru": "Туман рассеялся, и <w>прозрачность</w> воздуха значительно улучшилась."
                            }
                        }
                    ],
                    "synonyms": [
                        "przezroczystość"
                    ],
                    "common_phrases": [
                        "przejrzystość wody",
                        "przejrzystość powietrza",
                        "idealna przejrzystość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое свойство вещества, например воды или воздуха, позволяющее свету свободно проникать сквозь него и отчетливо видеть то, что находится дальше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачность",
                                "target_lang_sense_clarification": "физическое свойство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9a3e84d-d62e-38a9-8b22-b03d2ea66d4c",
                    "sense_definition": "Cecha tekstu, wypowiedzi lub układu, oznaczająca, że są one łatwe do zrozumienia, logiczne i uporządkowane.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel pochwalił ucznia za <w>przejrzystość</w> wywodu w napisanym wypracowaniu.",
                            "target_lang_translations": {
                                "ru": "Учитель похвалил ученика за <w>ясность</w> изложения в написанном сочинении."
                            }
                        },
                        {
                            "text": "<w>Przejrzystość</w> zasad gry sprawia, że nowi zawodnicy szybko uczą się reguł.",
                            "target_lang_translations": {
                                "ru": "<w>Прозрачность</w> правил игры позволяет новым игрокам быстро учиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "jasność",
                        "klarowność"
                    ],
                    "common_phrases": [
                        "przejrzystość tekstu",
                        "przejrzystość zasad",
                        "przejrzystość układu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеристика текста, высказывания или структуры, означающая, что они легки для понимания, логичны и упорядочены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ясность",
                                "target_lang_sense_clarification": "изложения, мыслей"
                            },
                            {
                                "target_lang_word": "прозрачность",
                                "target_lang_sense_clarification": "структуры, системы"
                            },
                            {
                                "target_lang_word": "чёткость",
                                "target_lang_sense_clarification": "понятность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7076591c-132c-3b92-9164-35de843e138e",
                    "sense_definition": "Jawność działań, procedur lub finansów w instytucjach i organizacjach, umożliwiająca ich publiczną kontrolę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obywatele domagają się większej <w>przejrzystości</w> w wydawaniu publicznych pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Граждане требуют большей <w>прозрачности</w> в расходовании государственных средств."
                            }
                        },
                        {
                            "text": "Firma dba o pełną <w>przejrzystość</w> swoich procedur rekrutacyjnych.",
                            "target_lang_translations": {
                                "ru": "Компания заботится о полной <w>прозрачности</w> своих процедур найма."
                            }
                        }
                    ],
                    "synonyms": [
                        "transparentność"
                    ],
                    "common_phrases": [
                        "przejrzystość finansowa",
                        "brak przejrzystości",
                        "przejrzystość działań"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытость действий, процедур или финансов в учреждениях и организациях, делающая возможным общественный контроль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачность",
                                "target_lang_sense_clarification": "действий, процедур"
                            },
                            {
                                "target_lang_word": "открытость",
                                "target_lang_sense_clarification": "доступность информации"
                            }
                        ]
                    }
                }
            ]
        }

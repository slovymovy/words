                {
                    "sense_id": "d67977ca-646f-37a0-8e03-65adfa91a283",
                    "sense_definition": "Pojazd na kółkach przeznaczony do przewożenia małych dzieci.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "vehicles_baby",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama włożyła śpiące dziecko do <w>wózka</w>.",
                            "target_lang_translations": {
                                "ru": "Мама положила спящего ребенка в <w>коляску</w>."
                            }
                        },
                        {
                            "text": "Na spacerze rodzice prowadzili <w>wózek</w> na zmianę.",
                            "target_lang_translations": {
                                "ru": "На прогулке родители везли <w>коляску</w> по очереди."
                            }
                        }

            "senses": [
                {
                    "sense_id": "8152496c-ed0d-36ac-82d6-8eb5eb53ec2d",
                    "sense_definition": "Een voornaam voor jongens.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn tolk had mij <w>Omer</w> aanbevolen voor deze klus.",
                            "target_lang_translations": {
                                "ru": "Мой переводчик порекомендовал мне <w>Омера</w> для этой работы.",
                                "en": "My interpreter had recommended <w>Omer</w> to me for this job."
                            }
                        },
                        {
                            "text": "Ze hebben hun pasgeboren zoon <w>Omer</w> genoemd.",
                            "target_lang_translations": {
                                "ru": "Они назвали своего новорожденного сына <w>Омером</w>.",
                                "en": "They named their newborn son <w>Omer</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя.",
                        "en": "A first name for boys."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Омер",
                                "target_lang_sense_clarification": "имя"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "Omer",
                                "target_lang_sense_clarification": "male given name"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ede143a3-17b4-3403-9b8d-cfbe7210b2c7",
                    "sense_definition": "De periode van negenenveertig dagen tussen de joodse feestdagen Pesach en Sjavoeot.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "jewish_ritual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de <w>omer</w> gelden er bepaalde regels van rouw.",
                            "target_lang_translations": {
                                "ru": "Во время <w>омера</w> действуют определенные правила траура.",
                                "en": "During the <w>Omer</w>, certain rules of mourning apply."
                            }
                        },
                        {
                            "text": "Het tellen van de <w>omer</w> verbindt de uittocht uit Egypte met het ontvangen van de wet.",
                            "target_lang_translations": {
                                "ru": "Счёт <w>омера</w> связывает исход из Египта с получением закона.",
                                "en": "The counting of the <w>Omer</w> connects the Exodus from Egypt with the receiving of the law."
                            }
                        }
                    ],
                    "synonyms": [
                        "omertijd"
                    ],
                    "common_phrases": [
                        "omer tellen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период в сорок девять дней между еврейскими праздниками Песах и Шавуот.",
                        "en": "The period of forty-nine days between the Jewish holidays of Passover and Shavuot."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "омер",
                                "target_lang_sense_clarification": "период времени"
                            },
                            {
                                "target_lang_word": "счёт омера",
                                "target_lang_sense_clarification": "ритуал"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "Omer",
                                "target_lang_sense_clarification": "period of time"
                            },
                            {
                                "target_lang_word": "Counting of the Omer",
                                "target_lang_sense_clarification": "ritual"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eae457f8-65b0-36f9-a901-54e624c01ace",
                    "sense_definition": "Een schoof graan, in het bijzonder het offer van een gerstschoof dat in de tempel werd gebracht op de tweede dag van Pesach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "jewish_ritual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De priester bewoog de <w>omer</w> voor het altaar.",
                            "target_lang_translations": {
                                "ru": "Священник возносил <w>омер</w> перед алтарем.",
                                "en": "The priest waved the <w>omer</w> before the altar."
                            }
                        },
                        {
                            "text": "In de tempel werd jaarlijks een <w>omer</w> gerst geofferd.",
                            "target_lang_translations": {
                                "ru": "В храме ежегодно приносили в жертву <w>омер</w> ячменя.",
                                "en": "In the temple, an <w>omer</w> of barley was offered annually."
                            }
                        }
                    ],
                    "synonyms": [
                        "schoof",
                        "garve"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сноп зерна, в частности, приношение снопа ячменя в Храм на второй день Песаха.",
                        "en": "A sheaf of grain, specifically the offering of a barley sheaf brought to the temple on the second day of Passover."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "омер",
                                "target_lang_sense_clarification": "сноп"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "omer",
                                "target_lang_sense_clarification": "sheaf offering"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47f1fe46-dd0a-3b94-8c79-e232e9a7e4dd",
                    "sense_definition": "Een oude inhoudsmaat voor koren, gelijk aan een tiende deel van een efa.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volgens de tekst verzamelde iedereen een <w>omer</w> manna per dag.",
                            "target_lang_translations": {
                                "ru": "Согласно тексту, каждый собирал один <w>омер</w> манны в день.",
                                "en": "According to the text, everyone gathered one <w>omer</w> of manna per day."
                            }
                        },
                        {
                            "text": "Een <w>omer</w> is een oude maat die we tegenwoordig niet meer gebruiken.",
                            "target_lang_translations": {
                                "ru": "<w>Омер</w> — это старинная мера, которую мы сегодня больше не используем.",
                                "en": "An <w>omer</w> is an old measure that we no longer use today."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "biblical measurement"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Древняя мера объема сыпучих тел, равная одной десятой ефы.",
                        "en": "An ancient measure of volume for grain, equal to a tenth of an ephah."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "омер",
                                "target_lang_sense_clarification": "мера объёма"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "omer",
                                "target_lang_sense_clarification": "unit of measurement"
                            }
                        ]

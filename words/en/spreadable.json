                {
                    "sense_id": "fe3e077f-4b36-3a82-be60-4cdbc9417a7f",
                    "sense_definition": "Able to be distributed in a thin layer over a surface, usually referring to soft food.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This warm margarine is much more <w>spreadable</w> than that lump of cold butter.",
                            "target_lang_translations": {
                                "ru": "Этот теплый маргарин гораздо <w>легче намазывается</w>, чем тот кусок холодного масла."
                            }
                        },
                        {
                            "text": "I prefer buying <w>spreadable</w> cheese because it is easier to use on soft bread.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю покупать <w>мягкий</w> сыр, потому что его легче использовать на мягком хлебе."
                            }
                        }
                    ],
                    "synonyms": [
                        "soft",
                        "smearable"
                    ],
                    "antonyms": [
                        "hard",
                        "solid"
                    ],
                    "common_phrases": [
                        "spreadable butter",
                        "spreadable cheese",
                        "easily spreadable"
                    ],
                    "target_lang_definitions": {

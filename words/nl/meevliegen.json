    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3a132bce-2e3f-37f3-a2ec-4af5e8e4015c",
                    "sense_definition": "Als passagier meereizen in een vliegtuig, vaak samen met iemand anders.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "samen_vliegen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader is piloot, dus ik mag soms goedkoop met hem <w>meevliegen</w>.",
                            "target_lang_translations": {
                                "en": "My father is a pilot, so sometimes I can <w>fly along</w> with him cheaply."
                            }
                        },
                        {
                            "text": "Zij had een extra ticket over, waardoor haar vriendin ook kon <w>meevliegen</w>.",
                            "target_lang_translations": {
                                "en": "She had an extra ticket left, so her friend could also <w>fly along</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "meereizen"
                    ],
                    "antonyms": [
                        "achterblijven"
                    ],
                    "common_phrases": [
                        "gratis meevliegen",
                        "met iemand meevliegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To travel along as a passenger in an airplane, often together with someone else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fly along",
                                "target_lang_sense_clarification": "travel together by plane"
                            },
                            {
                                "target_lang_word": "fly with",
                                "target_lang_sense_clarification": "accompany someone on a flight"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d200fc0-f58b-3fea-b987-fccf4fcdf090",
                    "sense_definition": "Samen met anderen door de lucht vliegen, vaak in een groep of zwerm.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "samen_vliegen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jonge vogels zijn eindelijk sterk genoeg om met de groep <w>mee te vliegen</w>.",
                            "target_lang_translations": {
                                "en": "The young birds are finally strong enough to <w>fly along</w> with the group."
                            }
                        },
                        {
                            "text": "Tijdens de vogeltrek zie je duizenden ganzen die samen naar het zuiden <w>meevliegen</w>.",
                            "target_lang_translations": {
                                "en": "During the migration, you see thousands of geese <w>flying along</w> together to the south."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in formatie meevliegen",
                        "met de zwerm meevliegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To fly through the air together with others, often in a group or flock."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fly along",
                                "target_lang_sense_clarification": "move through the air together"
                            },
                            {
                                "target_lang_word": "fly with",
                                "target_lang_sense_clarification": "join a group in flight"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "semantic_group_id": "name_given",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My friend <w>Rick</w> is coming to the party tonight.",
                            "target_lang_translations": {
                                "ru": "Мой друг <w>Рик</w> придёт сегодня вечером на вечеринку."
                            }
                        },
                        {
                            "text": "Is <w>Rick</w> short for Richard or Patrick?",
                            "target_lang_translations": {
                                "ru": "<w>Рик</w> — это сокращение от Ричарда или Патрика?"
                            }

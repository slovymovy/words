{
    "word_family": [
        "полоть",
        "выполоть",
        "наполоть",
        "прополка"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e6501bc9-4ac6-3b16-964c-2bf18c5ab3f4",
                    "sense_definition": "Очистить землю от сорняков, вырывая их.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "weeding_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка попросила меня <прополоть> грядку с клубникой до обеда.",
                            "target_lang_translations": {
                                "en": "Grandma asked me to <weed> the strawberry patch before lunch."
                            }
                        },
                        {
                            "text": "Нам нужно срочно <прополоть> огород, пока сорняки не заглушили овощи.",
                            "target_lang_translations": {
                                "en": "We need to urgently <weed> the vegetable garden before the weeds choke the vegetables."
                            }
                        }
                    ],
                    "synonyms": [
                        "вы́полоть"
                    ],
                    "common_phrases": [
                        "прополоть грядку",
                        "прополоть огород",
                        "прополоть картошку"
                    ],
                    "target_lang_definitions": {
                        "en": "To clear the ground of weeds by pulling them out."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weed",
                                "target_lang_sense_clarification": "to remove unwanted plants from an area"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b79504b3-951c-35b3-a426-d7f0d9079746",
                    "sense_definition": "Провести определённое время, занимаясь удалением сорняков.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "weeding_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она <прополола> в саду всё утро и очень устала.",
                            "target_lang_translations": {
                                "en": "She <weeded> in the garden all morning and got very tired."
                            }
                        },
                        {
                            "text": "Мы <пропололи> целый день, но так и не закончили работу.",
                            "target_lang_translations": {
                                "en": "We <weeded> all day, but still didn't finish the work."
                            }
                        }
                    ],
                    "common_phrases": [
                        "прополоть всё утро",
                        "прополоть целый день"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend a certain amount of time removing weeds."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weed",
                                "target_lang_sense_clarification": "to spend time doing the activity of weeding"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95dbbb4b-3a8d-3b7c-a27a-4259e1737ff3",
                    "sense_definition": "Проредить густо растущие растения, удаляя лишние ростки.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "thinning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Садовник решил <прополоть> морковь, чтобы корнеплоды выросли крупными.",
                            "target_lang_translations": {
                                "en": "The gardener decided to <thin out> the carrots so the roots would grow large."
                            }
                        },
                        {
                            "text": "Нужно аккуратно <прополоть> всходы, оставив только самые сильные растения.",
                            "target_lang_translations": {
                                "en": "It is necessary to carefully <thin out> the shoots, leaving only the strongest plants."
                            }
                        }
                    ],
                    "synonyms": [
                        "вы́полоть",
                        "вы́полоть"
                    ],
                    "common_phrases": [
                        "прополоть морковь",
                        "прополоть всходы"
                    ],
                    "target_lang_definitions": {
                        "en": "To thin out thickly growing plants by removing excess sprouts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thin out",
                                "target_lang_sense_clarification": "to remove excess plants to allow others to grow"
                            },
                            {
                                "target_lang_word": "weed",
                                "target_lang_sense_clarification": "to remove excess seedlings"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3b65f6cc-b92c-3db5-8689-cfcf7a901fab",
                    "sense_definition": "Afrykański ssak z rodziny koniowatych, wyróżniający się charakterystyczną sierścią w czarno-białe pasy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zebra</w> ma sierść w czarno-białe paski.",
                            "target_lang_translations": {
                                "ru": "У <w>зебры</w> шерсть в черно-белую полоску."
                            }
                        },
                        {
                            "text": "Podczas wycieczki do zoo dzieci karmiły <w>zebrę</w> trawą.",
                            "target_lang_translations": {
                                "ru": "Во время экскурсии в зоопарк дети кормили <w>зебру</w> травой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stado zebr",
                        "afrykańska zebra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Африканское млекопитающее из семейства лошадиных, отличающееся характерной черно-белой полосатой шерстью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зебра",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1986540-f06e-3405-a95d-9f9b3d322b47",
                    "sense_definition": "Miejsce na jezdni przeznaczone do przechodzenia pieszych, oznaczone szerokimi białymi pasami.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez ulicę należy przechodzić wyłącznie na <w>zebrze</w>.",
                            "target_lang_translations": {
                                "ru": "Улицу следует переходить исключительно по <w>зебре</w>."
                            }
                        },
                        {
                            "text": "Kierowca zatrzymał się przed <w>zebrą</w>, aby przepuścić idących ludzi.",
                            "target_lang_translations": {
                                "ru": "Водитель остановился перед <w>зеброй</w>, чтобы пропустить идущих людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasy",
                        "przejście dla pieszych"
                    ],
                    "common_phrases": [
                        "przechodzić na zebrze",
                        "wejść na zebrę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Место на проезжей части, предназначенное для перехода пешеходов, обозначенное широкими белыми полосами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зебра",
                                "target_lang_sense_clarification": "разметка"
                            },
                            {
                                "target_lang_word": "пешеходный переход",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d60d5e96-4ff7-323a-8a12-2c7d63aa8fcd",
                    "sense_definition": "Wzór w czarno-białe paski, przypominający umaszczenie afrykańskiego zwierzęcia.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej nowa torebka ma modny wzór w <w>zebrę</w>.",
                            "target_lang_translations": {
                                "ru": "У её новой сумочки модный узор под <w>зебру</w>."
                            }
                        },
                        {
                            "text": "Na urodziny upiekła pyszne ciasto <w>zebrę</w>.",
                            "target_lang_translations": {
                                "ru": "На день рождения она испекла вкусный торт <w>Зебра</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wzór w zebrę",
                        "ciasto zebra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узор в черно-белую полоску, напоминающий окрас африканского животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зебра",
                                "target_lang_sense_clarification": "узор или торт"
                            }
                        ]
                    }
                }
            ]
        }
    ]

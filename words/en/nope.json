    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "19c41160-b19c-3b8e-8b78-017369fae865",
                    "sense_definition": "A negative response or refusal; a colloquial form of \"no\".",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "negation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Did you finish your homework yet? <w>Nope</w>, not yet.",
                            "target_lang_translations": {
                                "ru": "Ты уже закончил домашнюю работу? <w>Не-а</w>, еще нет."
                            }
                        },
                        {
                            "text": "<w>Nope</w>, I have never been to that restaurant before.",
                            "target_lang_translations": {
                                "ru": "<w>Не-а</w>, я никогда раньше не был в этом ресторане."
                            }
                        }
                    ],
                    "antonyms": [
                        "yup",
                        "yep",
                        "yeah"
                    ],
                    "common_phrases": [
                        "nope sorry",
                        "just nope"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрицательный ответ или отказ; разговорная форма слова \"нет\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не-а",
                                "target_lang_sense_clarification": "разговорное, неформальное отрицание"
                            },
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "стандартное отрицание"
                            }
                        ]
                    }
                }
            ]
        },
        {

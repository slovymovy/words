                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>haak</w> graag een warme sjaal voor de winter.",
                            "target_lang_translations": {

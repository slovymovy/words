                            {
                                "target_lang_word": "обоссыш",
                                "target_lang_sense_clarification": "грубое, презрительное название пьяницы"
                            },
                            {
                                "target_lang_word": "алкаш",
                                "target_lang_sense_clarification": "разговорное название алкоголика"
                            }

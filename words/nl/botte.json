    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bed35457-765c-38cd-b17a-b901e860ed0f",
                    "sense_definition": "Verbogen vorm van het bijvoeglijk naamwoord <w>bot</w> (niet scherp; of lomp/onbeleefd).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met die <w>botte</w> schaar krijg je het papier nooit netjes geknipt.",
                            "target_lang_translations": {
                                "ru": "Этими <w>тупыми</w> ножницами ты никогда не разрежешь бумагу аккуратно.",
                                "en": "With those <w>blunt</w> scissors you will never get the paper cut neatly."
                            }
                        },
                        {
                            "text": "Zijn <w>botte</w> opmerking kwetste haar gevoelens enorm.",
                            "target_lang_translations": {
                                "ru": "Его <w>грубое</w> замечание сильно задело её чувства.",
                                "en": "His <w>blunt</w> remark hurt her feelings enormously."
                            }
                        }
                    ],
                    "synonyms": [
                        "stompe",
                        "lompe",
                        "onbeleefde"
                    ],
                    "antonyms": [
                        "scherpe",
                        "beleefde",
                        "spitse"
                    ],
                    "common_phrases": [
                        "een botte bijl",
                        "een botte weigering",
                        "botte pech"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склоняемая форма прилагательного \"bot\" (неострый; или грубый/невежливый).",
                        "en": "Inflected form of the adjective \"bot\" (not sharp; or rude/impolite)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тупой",
                                "target_lang_sense_clarification": "неострый (о предметах)"
                            },
                            {
                                "target_lang_word": "грубый",
                                "target_lang_sense_clarification": "невежливый, резкий (о поведении)"
                            },
                            {
                                "target_lang_word": "резкий",
                                "target_lang_sense_clarification": "прямолинейный до грубости"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "blunt",
                                "target_lang_sense_clarification": "not sharp; or direct/insensitive"
                            },
                            {
                                "target_lang_word": "dull",
                                "target_lang_sense_clarification": "not sharp (of a blade)"
                            },
                            {
                                "target_lang_word": "rude",
                                "target_lang_sense_clarification": "impolite or ill-mannered"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "67908c39-107d-3a73-b03c-765497be887d",
                    "sense_definition": "Een laars; stevig schoeisel dat de voet en een deel van het been bedekt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "footwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij trok zijn rubberen <w>botten</w> aan om in de tuin te werken.",
                            "target_lang_translations": {
                                "ru": "Он надел свои резиновые <w>сапоги</w>, чтобы поработать в саду.",
                                "en": "He put on his rubber <w>boots</w> to work in the garden."
                            }
                        },
                        {
                            "text": "Zet je vuile <w>botten</w> maar in de gang, anders wordt de vloer vies.",
                            "target_lang_translations": {
                                "ru": "Поставь свои грязные <w>сапоги</w> в коридоре, иначе пол испачкается.",
                                "en": "Just put your dirty <w>boots</w> in the hallway, otherwise the floor will get dirty."
                            }
                        }
                    ],
                    "synonyms": [
                        "laars"
                    ],
                    "common_phrases": [
                        "zijn botten vagen aan"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in Vlaanderen"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сапог; прочная обувь, закрывающая стопу и часть ноги (региональное, Бельгия).",
                        "en": "A boot; sturdy footwear that covers the foot and part of the leg (regional, Belgium)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сапог",
                                "target_lang_sense_clarification": "высокая обувь"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "boot",
                                "target_lang_sense_clarification": "footwear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9827c9bb-7327-3a51-86dd-96a58c68de26",
                    "sense_definition": "Een houten vat, kuip of een gevlochten draagmand.",

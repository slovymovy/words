    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0fc7f35c-db27-358e-88d4-2756e611b071",
                    "sense_definition": "Happening all the time or repeatedly without stopping.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The technology in this field is <w>constantly</w> evolving and improving.",
                            "target_lang_translations": {
                                "ru": "Технологии в этой области <w>постоянно</w> развиваются и совершенствуются."
                            }
                        },
                        {
                            "text": "She is <w>constantly</w> checking her phone for new messages.",
                            "target_lang_translations": {
                                "ru": "Она <w>постоянно</w> проверяет свой телефон в ожидании новых сообщений."
                            }
                        }
                    ],
                    "synonyms": [
                        "continuously",
                        "incessantly",
                        "nonstop",
                        "ceaselessly"
                    ],
                    "antonyms": [
                        "occasionally",
                        "rarely",
                        "sporadically"
                    ],
                    "common_phrases": [
                        "constantly changing",
                        "constantly evolving",
                        "constantly growing",
                        "constantly reminded"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящее всё время или часто повторяющееся без остановки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постоянно",
                                "target_lang_sense_clarification": "всё время, регулярно"
                            },
                            {
                                "target_lang_word": "непрерывно",
                                "target_lang_sense_clarification": "без пауз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cee734c-845f-37e7-a300-30336e7d3306",
                    "sense_definition": "In a consistent manner that does not change or vary.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The temperature in the laboratory must be <w>constantly</w> maintained at twenty degrees.",
                            "target_lang_translations": {
                                "ru": "Температура в лаборатории должна <w>постоянно</w> поддерживаться на уровне двадцати градусов."
                            }
                        },
                        {
                            "text": "They <w>constantly</w> deliver high-quality results regardless of the circumstances.",
                            "target_lang_translations": {
                                "ru": "Они <w>неизменно</w> выдают качественные результаты, независимо от обстоятельств."
                            }
                        }
                    ],
                    "synonyms": [
                        "consistently",
                        "invariably",
                        "uniformly"
                    ],
                    "antonyms": [
                        "inconsistently",
                        "variably"
                    ],
                    "common_phrases": [
                        "constantly maintained"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последовательно, без изменений или колебаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неизменно",
                                "target_lang_sense_clarification": "всегда одинаково"
                            },
                            {
                                "target_lang_word": "постоянно",
                                "target_lang_sense_clarification": "стабильно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "157e0abe-7a1e-3337-8402-59cfe7738528",
                    "sense_definition": "With firm loyalty, resolution, or steadfastness.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He served his king <w>constantly</w> throughout the long and difficult war.",
                            "target_lang_translations": {
                                "ru": "Он <w>верно</w> служил своему королю на протяжении всей долгой и тяжелой войны."
                            }
                        },
                        {
                            "text": "She remained <w>constantly</w> true to her vows despite the temptation.",
                            "target_lang_translations": {
                                "ru": "Она оставалась <w>неизменно</w> верна своим клятвам, несмотря на искушение."
                            }
                        }
                    ],
                    "antonyms": [
                        "faithlessly",
                        "disloyally"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "describing loyalty or resolve"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "С твёрдой преданностью, решимостью или стойкостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верно",
                                "target_lang_sense_clarification": "преданно"
                            },
                            {
                                "target_lang_word": "неизменно",
                                "target_lang_sense_clarification": "стойко"
                            }
                        ]
                    }
                }
            ]
        }
    ]

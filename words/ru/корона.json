                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A historical name for a group of territories united under the rule of a single monarch."
                    },
                    "translations": {
                        "en": [

{
    "word_family": [
        "seer",
        "sight",
        "foresee",
        "oversee",
        "unseen",
        "sightseeing"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ad7bf8da-3391-3cf2-9f22-4711f3bdf734",
                    "sense_definition": "Present participle and gerund of <w>see</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>seeing</w> my friends for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Я <w>встречаюсь</w> с друзьями за ужином сегодня вечером."
                            }
                        },
                        {
                            "text": "She admitted to <w>seeing</w> the accident happen.",
                            "target_lang_translations": {
                                "ru": "Она призналась, что <w>видела</w>, как произошла авария."
                            }
                        }
                    ],
                    "common_phrases": [
                        "seeing is believing",
                        "worth seeing",
                        "seeing someone"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle and gerund"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола <w>see</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "видеть",
                                "target_lang_sense_clarification": "воспринимать зрением"
                            },
                            {
                                "target_lang_word": "встречаться",
                                "target_lang_sense_clarification": "иметь романтические или дружеские отношения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "e7e25d86-8775-3ced-b606-6972754b817a",
                    "sense_definition": "Considering the fact that; inasmuch as.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Seeing</w> that it is raining, we should probably stay indoors.",
                            "target_lang_translations": {
                                "ru": "<w>Поскольку</w> идет дождь, нам, вероятно, следует остаться дома."
                            }
                        },
                        {
                            "text": "We decided to leave early, <w>seeing</w> as the work was finished.",
                            "target_lang_translations": {
                                "ru": "Мы решили уйти пораньше, <w>учитывая</w>, что работа была закончена."
                            }
                        }
                    ],
                    "synonyms": [
                        "sighted"
                    ],
                    "common_phrases": [
                        "seeing as",
                        "seeing that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учитывая тот факт, что; поскольку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поскольку",
                                "target_lang_sense_clarification": "союз причины"
                            },
                            {
                                "target_lang_word": "принимая во внимание",
                                "target_lang_sense_clarification": "устойчивое выражение"
                            },
                            {
                                "target_lang_word": "раз",
                                "target_lang_sense_clarification": "разговорный союз"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9ac5014b-7122-3aae-b928-8ed2e2e9068c",
                    "sense_definition": "Having the ability to see; not blind.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>seeing</w> eye dog guided his owner through the busy street.",
                            "target_lang_translations": {
                                "ru": "<w>Зрячая</w> собака-поводырь провела своего хозяина через оживленную улицу."
                            }
                        },
                        {
                            "text": "It is often difficult for a <w>seeing</w> person to understand the experience of blindness.",
                            "target_lang_translations": {
                                "ru": "<w>Зрячему</w> человеку часто трудно понять опыт слепоты."
                            }
                        }
                    ],
                    "synonyms": [
                        "sighted"
                    ],
                    "antonyms": [
                        "blind"
                    ],
                    "common_phrases": [
                        "seeing eye dog"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий способностью видеть; не слепой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зрячий",
                                "target_lang_sense_clarification": "обладающий зрением"
                            },
                            {
                                "target_lang_word": "видящий",
                                "target_lang_sense_clarification": "способный видеть"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9884f7f1-544f-35c6-a587-c108ede937c0",
                    "sense_definition": "The act or faculty of perceiving with the eyes.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a difference between mere looking and true <w>seeing</w>.",
                            "target_lang_translations": {
                                "ru": "Есть разница между простым смотрением и истинным <w>ви́дением</w>."
                            }
                        },
                        {
                            "text": "His <w>seeing</w> was impaired by the sudden flash of light.",
                            "target_lang_translations": {
                                "ru": "Его <w>зрение</w> ухудшилось из-за внезапной вспышки света."
                            }
                        }
                    ],
                    "synonyms": [
                        "sighted"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или способность воспринимать глазами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зрение",
                                "target_lang_sense_clarification": "способность видеть"
                            },
                            {
                                "target_lang_word": "ви́дение",
                                "target_lang_sense_clarification": "процесс восприятия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc33a5c1-0082-3fa5-a6e1-fa53247016f9",
                    "sense_definition": "The clarity or stability of a telescopic image caused by atmospheric conditions.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The astronomer waited for the <w>seeing</w> to improve before taking the photo.",
                            "target_lang_translations": {
                                "ru": "Астроном ждал, пока <w>видимость</w> улучшится, прежде чем сделать снимок."
                            }
                        },
                        {
                            "text": "Poor <w>seeing</w> can make stars appear to twinkle violently.",
                            "target_lang_translations": {
                                "ru": "Плохая <w>видимость</w> может заставить звезды сильно мерцать."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Четкость или стабильность телескопического изображения, зависящая от атмосферных условий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "видимость",
                                "target_lang_sense_clarification": "астрономическая"
                            },
                            {
                                "target_lang_word": "астроклимат",
                                "target_lang_sense_clarification": "качество изображения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
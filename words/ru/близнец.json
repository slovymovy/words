                        "типичный Близнец"
                    ],
                    "target_lang_definitions": {
                        "en": "A person born under the zodiac sign Gemini (May 21 – June 21)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Gemini",
                                "target_lang_sense_clarification": "zodiac sign"

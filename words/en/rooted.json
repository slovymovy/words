                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The server was <w>rooted</w> by hackers last night.",
                            "target_lang_translations": {
                                "ru": "Сервер был <w>взломан</w> хакерами прошлой ночью."
                            }

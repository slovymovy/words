    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5c9b8685-0040-3059-b760-214329ee2e32",
                    "sense_definition": "Having the distinctive qualities or features of a particular type of person, thing, or group.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This painting is a <w>typical</w> example of the artist's early work.",
                            "target_lang_translations": {
                                "ru": "Эта картина — <w>типичный</w> пример раннего творчества художника."
                            }
                        },
                        {
                            "text": "She gave a <w>typical</w> response that avoided answering the question directly.",
                            "target_lang_translations": {
                                "ru": "Она дала <w>типичный</w> ответ, уклонившись от прямого ответа на вопрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "characteristic",
                        "representative",
                        "classic"
                    ],
                    "antonyms": [
                        "atypical"
                    ],
                    "common_phrases": [
                        "typical example",
                        "typical behavior",
                        "typical feature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий характерными качествами или чертами определённого типа людей, вещей или группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "типичный",
                                "target_lang_sense_clarification": "воплощающий характерные черты"
                            },
                            {
                                "target_lang_word": "характерный",
                                "target_lang_sense_clarification": "свойственный кому-либо или чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fe9f9f9-d6b1-3f79-84f0-d89593fbad04",
                    "sense_definition": "Happening in the usual way; normal and expected.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a <w>typical</w> Sunday morning with rain and hot coffee.",
                            "target_lang_translations": {
                                "ru": "Это было <w>обычное</w> воскресное утро с дождём и горячим кофе."
                            }
                        },
                        {
                            "text": "It is <w>typical</w> of him to arrive late to every meeting.",
                            "target_lang_translations": {
                                "ru": "Для него <w>типично</w> опаздывать на каждое собрание."
                            }
                        }
                    ],
                    "synonyms": [
                        "normal",
                        "average",
                        "standard",
                        "usual"
                    ],
                    "antonyms": [
                        "atypical"
                    ],
                    "common_phrases": [
                        "just typical",
                        "typical day",
                        "typical weather"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий привычным образом; нормальный и ожидаемый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обычный",
                                "target_lang_sense_clarification": "привычный, нормальный"
                            },
                            {
                                "target_lang_word": "типичный",
                                "target_lang_sense_clarification": "ожидаемый, стандартный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "585db65d-76ba-380a-bc57-4e346a71de58",
                    "sense_definition": "Relating to the specific subdivision of a biological group that contains the type species.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>typical</w> subgenus is distributed throughout the northern hemisphere.",
                            "target_lang_translations": {
                                "ru": "<w>Типовой</w> подрод распространён по всему северному полушарию."
                            }
                        },
                        {
                            "text": "Researchers identified the specimen as belonging to the <w>typical</w> variety of the plant.",
                            "target_lang_translations": {
                                "ru": "Исследователи определили, что образец относится к <w>типовой</w> разновидности этого растения."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к конкретному подразделению биологической группы, содержащему типовой вид."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "типовой",
                                "target_lang_sense_clarification": "в биологической систематике"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "932d4c12-82fd-3909-b7b9-c9c59c816ba0",
                    "sense_definition": "A specimen, drug, or thing that fits the standard type, often used in medical or biological contexts.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor decided to prescribe one of the <w>typicals</w> to treat the condition.",
                            "target_lang_translations": {
                                "ru": "Врач решил назначить один из <w>типичных антипсихотиков</w> для лечения этого состояния."
                            }
                        },
                        {
                            "text": "In the study of moths, the <w>typicals</w> were far more common than the dark variants.",
                            "target_lang_translations": {
                                "ru": "В исследовании мотыльков <w>типичные формы</w> встречались гораздо чаще, чем тёмные варианты."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Образец, препарат или объект, соответствующий стандартному типу (часто в медицине или биологии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "типичный антипсихотик",
                                "target_lang_sense_clarification": "медицинский термин"
                            },
                            {
                                "target_lang_word": "типичная форма",
                                "target_lang_sense_clarification": "биологический образец"
                            }
                        ]
                    }
                }
            ]
        }

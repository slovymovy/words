    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c7d0ee2-4264-3de3-9d54-6ee0e5ae6c4c",
                    "sense_definition": "An item of furniture with a flat top surface raised above the ground, usually on one or more legs.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please put your plates on the <w>table</w> when you are finished eating.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поставь тарелки на <w>стол</w>, когда закончишь есть."
                            }
                        },
                        {
                            "text": "We sat around the large wooden <w>table</w> in the kitchen to talk.",
                            "target_lang_translations": {
                                "ru": "Мы сидели вокруг большого деревянного <w>стола</w> на кухне и разговаривали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lay the table",
                        "clear the table",
                        "sit at the table",
                        "kitchen table"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет мебели с плоской верхней поверхностью, приподнятой над землей, обычно на одной или нескольких ножках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стол",
                                "target_lang_sense_clarification": "мебель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ffc806d-91b6-3ef9-aa39-1644b440384e",
                    "sense_definition": "A set of facts or figures displayed in rows and columns for easy reference.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "data_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The results of the survey are shown in the <w>table</w> below.",
                            "target_lang_translations": {
                                "ru": "Результаты опроса показаны в <w>таблице</w> ниже."
                            }
                        },
                        {
                            "text": "You can find the correct price by looking at this reference <w>table</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете найти правильную цену, посмотрев в эту справочную <w>таблицу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "grid"
                    ],
                    "common_phrases": [
                        "table of contents",
                        "periodic table",
                        "data table",
                        "pivot table"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор фактов или цифр, представленных в строках и столбцах для удобства использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таблица",
                                "target_lang_sense_clarification": "данные"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a2d24a9-cdc3-3669-b3b2-c957b4dd289b",
                    "sense_definition": "A list of multiplications of numbers, often used for teaching arithmetic to children.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "data_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young students are learning their multiplication <w>tables</w> in math class.",
                            "target_lang_translations": {
                                "ru": "Юные ученики учат <w>таблицу</w> умножения на уроке математики."
                            }
                        },
                        {
                            "text": "He recited the three times <w>table</w> without making a single mistake.",
                            "target_lang_translations": {
                                "ru": "Он рассказал <w>таблицу</w> умножения на три без единой ошибки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "multiplication table",
                        "times table",
                        "learn your tables"
                    ],
                    "target_lang_definitions": {
                        "ru": "Список произведений чисел, часто используемый для обучения детей арифметике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таблица умножения",
                                "target_lang_sense_clarification": "математика"
                            },
                            {
                                "target_lang_word": "таблица",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5659407-eaf0-32b6-877b-c5fbe20c2bca",
                    "sense_definition": "A group of people sitting together at a table, especially for a meal at a restaurant.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>table</w> in the corner ordered another bottle of red wine.",
                            "target_lang_translations": {
                                "ru": "<w>Столик</w> в углу заказал еще одну бутылку красного вина."
                            }
                        },
                        {
                            "text": "Our <w>table</w> was very noisy during the wedding reception.",
                            "target_lang_translations": {
                                "ru": "Наш <w>стол</w> был очень шумным во время свадебного банкета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "table for two",
                        "wait tables",
                        "under the table"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, сидящих вместе за столом, особенно во время еды в ресторане."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столик",
                                "target_lang_sense_clarification": "в ресторане"
                            },
                            {
                                "target_lang_word": "стол",
                                "target_lang_sense_clarification": "компания людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8385f440-280a-3704-85a6-420887d1de82",
                    "sense_definition": "A list of sports teams or players shown in order of their score or standing.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "data_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After winning the match, the team moved to the top of the league <w>table</w>.",
                            "target_lang_translations": {
                                "ru": "После победы в матче команда поднялась на вершину турнирной <w>таблицы</w>."
                            }
                        },
                        {
                            "text": "They are currently fighting to stay off the bottom of the <w>table</w> this season.",
                            "target_lang_translations": {
                                "ru": "В этом сезоне они борются за то, чтобы не остаться на дне <w>таблицы</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "league table",
                        "top of the table",
                        "bottom of the table"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Список спортивных команд или игроков, расположенных в порядке их очков или положения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "турнирная таблица",
                                "target_lang_sense_clarification": "спорт"
                            },
                            {
                                "target_lang_word": "таблица",
                                "target_lang_sense_clarification": "рейтинг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "df915c4e-c299-34ab-ab38-06383392d86a",
                    "sense_definition": "To formally propose a topic or motion for discussion at a meeting or assembly.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "meeting_procedure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee decided to <w>table</w> a motion to discuss the new budget immediately.",
                            "target_lang_translations": {
                                "ru": "Комитет решил <w>вынести на обсуждение</w> предложение немедленно обсудить новый бюджет."
                            }
                        },
                        {
                            "text": "She <w>tabled</w> a question regarding the safety of the new building construction.",
                            "target_lang_translations": {
                                "ru": "Она <w>вынесла на обсуждение</w> вопрос о безопасности строительства нового здания."
                            }
                        }
                    ],
                    "synonyms": [
                        "propose#Verb"
                    ],
                    "common_phrases": [
                        "table a motion",
                        "table a proposal",
                        "table an amendment"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально предложить тему или предложение для обсуждения на собрании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выносить на обсуждение",
                                "target_lang_sense_clarification": "предлагать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3806ae2c-863c-3ac4-be46-3ebd64d9bb4f",
                    "sense_definition": "To postpone the discussion of a topic or resolution to a later time.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "meeting_procedure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Due to lack of time, we had to <w>table</w> the discussion until next week.",
                            "target_lang_translations": {
                                "ru": "Из-за нехватки времени нам пришлось <w>отложить</w> обсуждение до следующей недели."
                            }
                        },
                        {
                            "text": "The senator moved to <w>table</w> the amendment indefinitely so it would not pass.",
                            "target_lang_translations": {
                                "ru": "Сенатор предложил <w>отложить</w> поправку на неопределенный срок, чтобы она не прошла."
                            }
                        }
                    ],
                    "synonyms": [
                        "moot"
                    ],
                    "common_phrases": [
                        "table the discussion",
                        "table the bill"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отложить обсуждение темы или резолюции на более позднее время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "переносить срок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8a070a9-f97f-3d59-a6ff-bd60c786f4df",
                    "sense_definition": "To arrange data or information into a table or grid format.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "data_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The researchers need to <w>table</w> the survey results before they can analyze them.",
                            "target_lang_translations": {
                                "ru": "Исследователям нужно <w>свести в таблицу</w> результаты опроса, прежде чем они смогут их проанализировать."
                            }
                        },
                        {
                            "text": "It took hours to <w>table</w> all the figures from the annual financial report.",
                            "target_lang_translations": {
                                "ru": "Потребовались часы, чтобы <w>свести в таблицу</w> все цифры из годового финансового отчета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "table the results",
                        "table data"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организовать данные или информацию в формате таблицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сводить в таблицу",
                                "target_lang_sense_clarification": "систематизировать"
                            },
                            {
                                "target_lang_word": "составлять таблицу",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
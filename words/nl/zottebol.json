    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "98bf5069-edf7-3919-86e9-b5706b6df3b2",
                    "sense_definition": "Iemand die zich dwaas, gek of grappig gedraagt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>zottebol</w> maakte iedereen aan het lachen met zijn gekke dansjes.",
                            "target_lang_translations": {
                                "en": "The <w>goofball</w> made everyone laugh with his crazy dances."
                            }
                        },
                        {
                            "text": "Doe niet zo'n <w>zottebol</w> en luister even serieus naar wat ik zeg.",
                            "target_lang_translations": {
                                "en": "Don't be such a <w>clown</w> and listen seriously to what I'm saying for a moment."
                            }
                        }
                    ],
                    "synonyms": [
                        "dwaas",
                        "grappenmaker",
                        "clown",
                        "pias",
                        "zot"
                    ],
                    "common_phrases": [
                        "een vrolijke zottebol",
                        "rare zottebol"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who behaves foolishly, crazily, or funnily."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "goofball",
                                "target_lang_sense_clarification": "a playful, silly person"
                            },
                            {
                                "target_lang_word": "clown",
                                "target_lang_sense_clarification": "someone who acts funny to make others laugh"
                            },
                            {
                                "target_lang_word": "fool",
                                "target_lang_sense_clarification": "someone behaving without sense"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4f2b0a9-e3c3-316e-8f62-93352dc3cbc8",
                    "sense_definition": "Een afgeplatte bol die gebruikt wordt bij het spel zottebollen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rol de <w>zottebol</w> zo dicht mogelijk naar het doel om punten te scoren.",
                            "target_lang_translations": {
                                "en": "Roll the <w>flattened ball</w> as close as possible to the target to score points."
                            }
                        },
                        {
                            "text": "Voor dit oude volksspel heb je een houten <w>zottebol</w> nodig.",
                            "target_lang_translations": {
                                "en": "For this old folk game, you need a wooden <w>game bowl</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {

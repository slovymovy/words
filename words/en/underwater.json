                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gardeners often <w>underwater</w> their plants during the winter months.",
                            "target_lang_translations": {
                                "ru": "Садоводы часто <w>недоливают</w> свои растения в зимние месяцы."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "10a8daf8-860f-38c2-9b6d-4a853ac80acd",
                    "sense_definition": "Osoba oglądająca film, przedstawienie, pokaz, koncert lub inne widowisko.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spectator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy <w>widzowie</w> wstali z miejsc i zaczęli klaskać po zakończeniu spektaklu.",
                            "target_lang_translations": {
                                "ru": "Все <w>зрители</w> встали со своих мест и начали аплодировать после окончания спектакля."
                            }
                        },
                        {
                            "text": "Ten popularny serial przyciąga przed telewizory miliony <w>widzów</w> każdego tygodnia.",
                            "target_lang_translations": {
                                "ru": "Этот популярный сериал каждую неделю собирает у экранов миллионы <w>зрителей</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "uczestnik"
                    ],
                    "common_phrases": [
                        "drodzy widzowie",
                        "miliony widzów",
                        "widzowie kinowi",
                        "przed oczami widzów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который смотрит фильм, спектакль, представление, концерт или другое зрелище."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зритель",
                                "target_lang_sense_clarification": "в театре, кино, на стадионе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ff031c0-7726-326a-8ebd-b2ef85868bb8",
                    "sense_definition": "Bierny obserwator jakichś wydarzeń, który nie bierze w nich czynnego udziału.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "observer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stał z boku jako bierny <w>widz</w> kłótni, nie chcąc się wtrącać.",
                            "target_lang_translations": {
                                "ru": "Он стоял в стороне как пассивный <w>зритель</w> ссоры, не желая вмешиваться."
                            }
                        },
                        {
                            "text": "Nie bądź tylko <w>widzem</w> swojego życia, ale zacznij aktywnie działać.",
                            "target_lang_translations": {
                                "ru": "Не будь просто <w>зрителем</w> своей жизни, а начни действовать активно."
                            }
                        }
                    ],
                    "synonyms": [
                        "gap"
                    ],
                    "antonyms": [
                        "uczestnik"
                    ],
                    "common_phrases": [
                        "bierny widz",
                        "przypadkowy widz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пассивный наблюдатель каких-либо событий, не принимающий в них активного участия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наблюдатель",
                                "target_lang_sense_clarification": "со стороны"
                            },
                            {
                                "target_lang_word": "зритель",
                                "target_lang_sense_clarification": "пассивный свидетель"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "70e2e398-afc4-3a25-9a5d-721da27af137",
                    "sense_definition": "Nazwisko polskie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Widz</w> został poproszony o podpisanie dokumentów w recepcji.",
                            "target_lang_translations": {
                                "ru": "Господина <w>Видза</w> попросили подписать документы на ресепшене."
                            }
                        },
                        {
                            "text": "Nie znam nikogo, kto nosiłby nazwisko <w>Widz</w>.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого, кто носил бы фамилию <w>Видз</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "male or female"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Видз",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "transact",
        "transactional",
        "transactor"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c7f24525-77c4-327f-9ca0-10b484d17e18",
                    "sense_definition": "An instance of buying or selling something; a business deal or exchange of money.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bank charges a small fee for every foreign <w>transaction</w>.",
                            "target_lang_translations": {
                                "ru": "Банк взимает небольшую комиссию за каждую зарубежную <w>транзакцию</w>."
                            }
                        },
                        {
                            "text": "We completed the <w>transaction</w> and shook hands on the deal.",
                            "target_lang_translations": {
                                "ru": "Мы завершили <w>сделку</w> и ударили по рукам."
                            }
                        }
                    ],
                    "synonyms": [
                        "deal",
                        "purchase",
                        "exchange",
                        "trade"
                    ],
                    "common_phrases": [
                        "business transaction",
                        "financial transaction",
                        "cash transaction",
                        "secure transaction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случай купли-продажи чего-либо; деловая сделка или обмен деньгами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сделка",
                                "target_lang_sense_clarification": "деловое соглашение"
                            },
                            {
                                "target_lang_word": "транзакция",
                                "target_lang_sense_clarification": "банковская или финансовая"
                            },
                            {
                                "target_lang_word": "операция",
                                "target_lang_sense_clarification": "финансовое действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fc94b73-eb29-3431-8e3c-e331b724ca6e",
                    "sense_definition": "The action or process of conducting business or negotiations.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "business_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>transaction</w> of official business took longer than expected due to the new regulations.",
                            "target_lang_translations": {
                                "ru": "<w>Ведение</w> официальных дел заняло больше времени, чем ожидалось, из-за новых правил."
                            }
                        },
                        {
                            "text": "He is responsible for the proper <w>transaction</w> of all legal matters in the firm.",
                            "target_lang_translations": {
                                "ru": "Он отвечает за надлежащее <w>ведение</w> всех юридических дел в фирме."
                            }
                        }
                    ],
                    "synonyms": [
                        "conduct",
                        "execution",
                        "handling",
                        "management"
                    ],
                    "common_phrases": [
                        "transaction of business"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс ведения бизнеса или переговоров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ведение",
                                "target_lang_sense_clarification": "дел или бизнеса"
                            },
                            {
                                "target_lang_word": "осуществление",
                                "target_lang_sense_clarification": "деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d30fe875-6340-3279-beaf-7a53b52ae3f3",
                    "sense_definition": "A single operation or group of operations in a computer system that must all succeed or all fail to maintain data integrity.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If the system crashes, the incomplete <w>transaction</w> will be rolled back automatically.",
                            "target_lang_translations": {
                                "ru": "Если система даст сбой, незавершенная <w>транзакция</w> будет автоматически отменена."
                            }
                        },
                        {
                            "text": "Database <w>transactions</w> prevent data corruption when multiple users update records simultaneously.",
                            "target_lang_translations": {
                                "ru": "<w>Транзакции</w> базы данных предотвращают повреждение данных, когда несколько пользователей обновляют записи одновременно."
                            }
                        }
                    ],
                    "synonyms": [
                        "operation"
                    ],
                    "common_phrases": [
                        "database transaction",
                        "atomic transaction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единичная операция или группа операций в компьютерной системе, которые должны быть выполнены успешно или отменены целиком для сохранения целостности данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "транзакция",
                                "target_lang_sense_clarification": "в информатике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9dadbe62-0a32-311d-8c96-24a719220925",
                    "sense_definition": "Published reports or records of the proceedings of a learned society.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His research paper was finally published in the <w>transactions</w> of the Royal Society.",
                            "target_lang_translations": {
                                "ru": "Его исследовательская работа была наконец опубликована в <w>трудах</w> Королевского общества."
                            }
                        },
                        {
                            "text": "The library archives the historical <w>transactions</w> of various scientific associations.",
                            "target_lang_translations": {
                                "ru": "Библиотека архивирует исторические <w>труды</w> различных научных ассоциаций."
                            }
                        }
                    ],
                    "synonyms": [
                        "proceedings",
                        "records"
                    ],
                    "common_phrases": [
                        "philosophical transactions"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Опубликованные отчеты или протоколы заседаний научного общества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "труды",
                                "target_lang_sense_clarification": "научного общества"
                            },
                            {
                                "target_lang_word": "записки",
                                "target_lang_sense_clarification": "ученые записки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
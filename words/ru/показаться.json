    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "205405f8-dd75-32ad-9884-36ce75ba495c",
                    "sense_definition": "Почудиться, возникнуть в воображении (обычно используется в безличных конструкциях).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "impression_imagination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне <показалось>, что кто-то тихо постучал во входную дверь.",
                            "target_lang_translations": {
                                "en": "It <seemed> to me that someone quietly knocked on the front door."
                            }
                        },
                        {
                            "text": "Это тебе не <показалось>, я действительно сказал эту фразу.",
                            "target_lang_translations": {
                                "en": "You didn't <imagine> it, I really said that phrase."
                            }
                        }
                    ],
                    "synonyms": [
                        "померещиться",
                        "почудиться",
                        "привидеться"
                    ],
                    "common_phrases": [
                        "мне показалось",
                        "тебе показалось",
                        "показалось странным"
                    ],
                    "target_lang_definitions": {
                        "en": "To be imagined or perceived; to arise in one's mind (often used in impersonal constructions like 'it seemed to me')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seem",
                                "target_lang_sense_clarification": "to give the impression of being; used in 'it seemed to me'"
                            },
                            {
                                "target_lang_word": "imagine",
                                "target_lang_sense_clarification": "to form a mental image; used in contexts like 'you didn't imagine it'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c36cdf9c-9108-349a-986a-1621f4746ff8",
                    "sense_definition": "Стать видимым, появиться в поле зрения; выступить наружу.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за серых облаков наконец <показалось> яркое весеннее солнце.",
                            "target_lang_translations": {
                                "en": "From behind the grey clouds, the bright spring sun finally <appeared>."
                            }
                        },
                        {
                            "text": "В конце длинной улицы неожиданно <показался> знакомый синий автомобиль.",
                            "target_lang_translations": {
                                "en": "At the end of the long street, a familiar blue car unexpectedly <appeared>."
                            }
                        }
                    ],
                    "synonyms": [
                        "появиться",
                        "возникнуть"
                    ],
                    "antonyms": [
                        "исчезнуть",
                        "скрыться",
                        "пропасть"
                    ],
                    "common_phrases": [
                        "показаться на горизонте",
                        "показаться из-за туч",
                        "показаться вдали"
                    ],
                    "target_lang_definitions": {
                        "en": "To become visible; to come into view or emerge."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "to become visible"
                            },
                            {
                                "target_lang_word": "come in sight",
                                "target_lang_sense_clarification": "to become visible at a distance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "465d3f77-f218-3061-b556-8a076778945b",
                    "sense_definition": "Произвести определённое впечатление, быть воспринятым каким-либо образом.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "impression_imagination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это задание сначала <показалось> нам слишком сложным и непонятным.",
                            "target_lang_translations": {
                                "en": "This task at first <seemed> too difficult and incomprehensible to us."
                            }
                        },
                        {
                            "text": "Его поведение <показалось> всем присутствующим очень подозрительным.",
                            "target_lang_translations": {
                                "en": "His behavior <seemed> very suspicious to everyone present."
                            }
                        }
                    ],
                    "common_phrases": [
                        "показаться интересным",
                        "показаться грустным",
                        "показаться вечностью"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a specific impression; to be perceived in a certain way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seem",
                                "target_lang_sense_clarification": "to appear to be something"
                            },
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "to give a certain impression"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16150902-45e6-3d3f-9b72-7c16dd205eec",
                    "sense_definition": "Прийти куда-либо, посетить какое-то место, стать увиденным людьми.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он обещал ненадолго <показаться> на вечеринке, чтобы поздравить именинника.",
                            "target_lang_translations": {
                                "en": "He promised to briefly <show up> at the party to congratulate the birthday boy."
                            }
                        },
                        {
                            "text": "После ссоры тебе лучше не <показаться> ему на глаза некоторое время.",
                            "target_lang_translations": {
                                "en": "After the fight, you had better not <show> yourself to him for a while."
                            }
                        }
                    ],
                    "synonyms": [
                        "прийти",
                        "появиться"
                    ],
                    "common_phrases": [
                        "показаться на людях",
                        "показаться в обществе",
                        "показаться на глаза"
                    ],
                    "target_lang_definitions": {
                        "en": "To arrive at a place, visit somewhere, or be seen by people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show up",
                                "target_lang_sense_clarification": "to arrive or appear at a place"
                            },
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "to present oneself publicly"
                            },
                            {
                                "target_lang_word": "show oneself",
                                "target_lang_sense_clarification": "to let oneself be seen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40d88c25-a99e-357f-9504-88b5892c3ee6",
                    "sense_definition": "Обратиться к медицинскому работнику для осмотра и диагностики.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "medical_examination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если боль в спине не пройдёт, тебе обязательно нужно <показаться> врачу.",
                            "target_lang_translations": {
                                "en": "If the back pain doesn't go away, you definitely need to <see> a doctor."
                            }
                        },
                        {
                            "text": "Пациент решил <показаться> другому специалисту для уточнения диагноза.",
                            "target_lang_translations": {
                                "en": "The patient decided to <see> another specialist to clarify the diagnosis."
                            }
                        }
                    ],
                    "synonyms": [
                        "обратиться"
                    ],
                    "common_phrases": [
                        "показаться врачу",
                        "показаться специалисту",
                        "надо показаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To consult a medical professional for examination."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "see",
                                "target_lang_sense_clarification": "to consult a professional (e.g., a doctor)"
                            },
                            {
                                "target_lang_word": "consult",
                                "target_lang_sense_clarification": "to seek advice from a specialist"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56e0ff2f-8cc6-3d36-b557-fd0c19a8b134",
                    "sense_definition": "Произвести хорошее впечатление, прийтись по вкусу.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "preference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новая работа ему сразу не <показалась>, и он быстро уволился.",
                            "target_lang_translations": {
                                "en": "The new job didn't <appeal> to him immediately, and he quickly quit."
                            }
                        },
                        {
                            "text": "Ей очень <показался> этот тихий и уютный приморский городок.",
                            "target_lang_translations": {
                                "en": "This quiet and cozy seaside town really <appealed> to her."
                            }
                        }
                    ],
                    "synonyms": [
                        "понравиться",
                        "приглянуться"
                    ],
                    "common_phrases": [
                        "жизнь показалась мёдом",
                        "не показаться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "просторечное выражение"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To make a good impression; to be liked or found agreeable."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appeal",
                                "target_lang_sense_clarification": "to be attractive or interesting to someone"
                            },
                            {
                                "target_lang_word": "take a liking to",
                                "target_lang_sense_clarification": "to start to like (subject and object reversed in translation)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
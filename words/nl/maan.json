                },
                {
                    "sense_id": "a559ad3f-f5bd-36af-8730-889a5f1ee948",
                    "sense_definition": "*Gebiedende wijs van* <w>manen</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Maan</w> je vrienden tot kalmte voordat de ruzie escaleert!",
                            "target_lang_translations": {
                                "ru": "<w>Призови</w> друзей к спокойствию, пока ссора не обострилась!",
                                "en": "<w>Urge</w> your friends to stay calm before the argument escalates!"
                            }

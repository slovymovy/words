{
    "word_family": [
        "invite",
        "invitation",
        "inviting",
        "uninvite"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a44b4372-e78b-301d-8dbb-5f95db27c218",
                    "sense_definition": "Not asked or requested to come to an event or place.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>uninvited</w> guests arrived late and ate most of the food.",
                            "target_lang_translations": {
                                "ru": "<w>Незваные</w> гости прибыли поздно и съели большую часть еды."
                            }
                        },
                        {
                            "text": "She felt awkward showing up <w>uninvited</w> to the private meeting.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала себя неловко, явившись на частную встречу <w>без приглашения</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "unbidden",
                        "unwelcome",
                        "unsolicited"
                    ],
                    "antonyms": [
                        "invited",
                        "welcome"
                    ],
                    "common_phrases": [
                        "uninvited guest",
                        "arrive uninvited",
                        "show up uninvited"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кого не просили или не приглашали прийти на мероприятие или в какое-либо место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "незваный",
                                "target_lang_sense_clarification": "о госте или посетителе"
                            },
                            {
                                "target_lang_word": "неприглашённый",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5f607ce8-d879-3260-860c-68660a98b8be",
                    "sense_definition": "Simple past and past participle of <w>uninvite</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was <w>uninvited</w> from the wedding after the argument.",
                            "target_lang_translations": {
                                "ru": "После ссоры ему <w>отменили приглашение</w> на свадьбу."
                            }
                        },
                        {
                            "text": "She realized she had <w>uninvited</w> the wrong person by mistake.",
                            "target_lang_translations": {
                                "ru": "Она поняла, что по ошибке <w>отменила приглашение</w> не тому человеку."
                            }
                        }
                    ],
                    "synonyms": [
                        "disinvited"
                    ],
                    "antonyms": [
                        "invited"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола «отменять приглашение»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отменил приглашение",
                                "target_lang_sense_clarification": "активное действие в прошлом"
                            },
                            {
                                "target_lang_word": "отозвал приглашение",
                                "target_lang_sense_clarification": "формальный стиль"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c3fdd24a-2ddf-38c1-8f7d-a6d8bd8f4aa0",
                    "sense_definition": "A person who has not been asked to attend an event.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The host had to ask the <w>uninvited</w> to leave the premises immediately.",
                            "target_lang_translations": {
                                "ru": "Хозяину пришлось попросить <w>неприглашённых</w> немедленно покинуть помещение."
                            }
                        },
                        {
                            "text": "Several <w>uninviteds</w> waited outside the hall hoping to get a glimpse of the show.",
                            "target_lang_translations": {
                                "ru": "Несколько <w>незваных гостей</w> ждали снаружи зала, надеясь хоть мельком увидеть шоу."
                            }
                        }
                    ],
                    "synonyms": [
                        "gatecrasher",
                        "intruder"
                    ],
                    "antonyms": [
                        "invitee",
                        "guest"
                    ],
                    "common_phrases": [
                        "the uninvited"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, которого не пригласили на мероприятие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неприглашённый",
                                "target_lang_sense_clarification": "субстантивированное прилагательное"
                            },
                            {
                                "target_lang_word": "незваный гость",
                                "target_lang_sense_clarification": "устойчивое выражение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
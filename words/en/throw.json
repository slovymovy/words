                            {
                                "target_lang_word": "швырять",
                                "target_lang_sense_clarification": "бросать с силой или злостью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f8900fe-e799-3074-99f6-606dc2b19102",
                    "sense_definition": "To organize and host a social event, such as a party.",

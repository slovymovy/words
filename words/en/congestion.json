{
    "word_family": [
        "congest",
        "congested",
        "congestive"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "060d066a-0a41-3dcf-bac1-64f2d60cfe39",
                    "sense_definition": "A situation in which a place is extremely crowded and blocked with traffic or people, making movement difficult.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "traffic_crowds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city council is introducing a charge to reduce traffic <w>congestion</w> in the center.",
                            "target_lang_translations": {
                                "ru": "Городской совет вводит плату, чтобы уменьшить транспортные <w>заторы</w> в центре."
                            }
                        },
                        {
                            "text": "Commuters faced heavy <w>congestion</w> on the highway this morning due to an accident.",
                            "target_lang_translations": {
                                "ru": "Утром пассажиры столкнулись с сильной <w>пробкой</w> на шоссе из-за аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "jam",
                        "gridlock",
                        "bottleneck",
                        "overcrowding"
                    ],
                    "antonyms": [
                        "flow",
                        "clearance"
                    ],
                    "common_phrases": [
                        "traffic congestion",
                        "heavy congestion",
                        "reduce congestion",
                        "ease congestion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, когда место переполнено людьми или транспортом, что затрудняет движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробка",
                                "target_lang_sense_clarification": "дорожная"
                            },
                            {
                                "target_lang_word": "затор",
                                "target_lang_sense_clarification": "скопление транспорта или препятствие"
                            },
                            {
                                "target_lang_word": "скопление",
                                "target_lang_sense_clarification": "людей или предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50042ea7-64ad-3ec5-8f57-217ac8346581",
                    "sense_definition": "The condition of being blocked or full of fluid, particularly affecting the nose, sinuses, or lungs.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medical_symptom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This spray is very effective at relieving nasal <w>congestion</w> during a cold.",
                            "target_lang_translations": {
                                "ru": "Этот спрей очень эффективен для снятия <w>заложенности</w> носа во время простуды."
                            }
                        },
                        {
                            "text": "She complained of chest <w>congestion</w> and a persistent cough.",
                            "target_lang_translations": {
                                "ru": "Она жаловалась на <w>заложенность</w> в груди и постоянный кашель."
                            }
                        }
                    ],
                    "synonyms": [
                        "stuffiness",
                        "blockage"
                    ],
                    "common_phrases": [
                        "nasal congestion",
                        "chest congestion",
                        "relieve congestion",
                        "sinus congestion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние закупорки или заполненности жидкостью, особенно затрагивающее нос, пазухи или легкие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заложенность",
                                "target_lang_sense_clarification": "носа или груди"
                            },
                            {
                                "target_lang_word": "скопление",
                                "target_lang_sense_clarification": "слизи или жидкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7b1fdb8-96af-32a7-84df-f311249b4498",
                    "sense_definition": "The state of being overloaded or blocked, often used in reference to data networks or systems.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_blockage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Network <w>congestion</w> caused the video stream to buffer repeatedly.",
                            "target_lang_translations": {
                                "ru": "<w>Перегрузка</w> сети привела к постоянной буферизации видеопотока."
                            }
                        },
                        {
                            "text": "The airport experienced severe <w>congestion</w> during the peak holiday season.",
                            "target_lang_translations": {
                                "ru": "В аэропорту наблюдалась сильная <w>перегруженность</w> в пик праздничного сезона."
                            }
                        }
                    ],
                    "synonyms": [
                        "overload",
                        "saturation"
                    ],
                    "common_phrases": [
                        "network congestion",
                        "data congestion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние перегрузки или блокировки, часто используемое по отношению к сетям передачи данных или системам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перегрузка",
                                "target_lang_sense_clarification": "сети или системы"
                            },
                            {
                                "target_lang_word": "затор",
                                "target_lang_sense_clarification": "в передаче данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02108ccf-5024-3ec0-9737-6992622b655a",
                    "sense_definition": "An abnormal accumulation of blood or fluid in a specific organ or part of the body.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical_pathology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient was diagnosed with venous <w>congestion</w> in the lower limbs.",
                            "target_lang_translations": {
                                "ru": "У пациента диагностировали венозный <w>застой</w> в нижних конечностях."
                            }
                        },
                        {
                            "text": "Heart failure can lead to passive <w>congestion</w> of the liver.",
                            "target_lang_translations": {
                                "ru": "Сердечная недостаточность может привести к пассивному <w>полнокровию</w> печени."
                            }
                        }
                    ],
                    "synonyms": [
                        "hyperemia"
                    ],
                    "common_phrases": [
                        "venous congestion",
                        "pulmonary congestion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аномальное скопление крови или жидкости в определенном органе или части тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застой",
                                "target_lang_sense_clarification": "крови или жидкости"
                            },
                            {
                                "target_lang_word": "полнокровие",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
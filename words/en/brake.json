            "senses": [
                {
                    "sense_id": "efe6159e-a684-3101-9830-6ef5d6af8880",
                    "sense_definition": "To slow down or stop a vehicle or machine by using a braking device.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stopping_mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>brake</w> gently before you reach the sharp corner.",
                            "target_lang_translations": {
                                "ru": "Вам следует мягко <w>тормозить</w>, прежде чем вы достигнете крутого поворота."
                            }

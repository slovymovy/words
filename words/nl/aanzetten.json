                    "semantic_group_id": "intensify",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We besloten binnen te blijven omdat de wind flink begon <w>aan</w> te <w>zetten</w>.",
                            "target_lang_translations": {
                                "en": "We decided to stay inside because the wind started to really <w>pick up</w>.",
                                "ru": "Мы решили остаться дома, так как ветер начал сильно <w>усиливаться</w>."

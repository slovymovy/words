{
    "word_family": [
        "развивать",
        "развитие",
        "развитой",
        "развитый",
        "неразвитый"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d62bc51-04f6-39be-9571-61ab67079462",
                    "sense_definition": "Проходить процесс роста, совершенствоваться, переходить в более сложное или лучшее состояние (об организме, уме, обществе или бизнесе).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "growth_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок быстро <развивается> и каждый день учит новые слова.",
                            "target_lang_translations": {
                                "en": "The child <develops> quickly and learns new words every day."
                            }
                        },
                        {
                            "text": "Наша компания активно <развивается> на международном рынке.",
                            "target_lang_translations": {
                                "en": "Our company is actively <developing> in the international market."
                            }
                        }
                    ],
                    "synonyms": [
                        "эволюционировать"
                    ],
                    "antonyms": [
                        "деградировать",
                        "деэволюционировать"
                    ],
                    "common_phrases": [
                        "быстро развиваться",
                        "развиваться физически",
                        "активно развиваться",
                        "развиваться духовно"
                    ],
                    "target_lang_definitions": {
                        "en": "To undergo a process of growth, improve, or transition into a more complex or better state (regarding an organism, mind, society, or business)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "develop",
                                "target_lang_sense_clarification": "general process of growth or improvement"
                            },
                            {
                                "target_lang_word": "grow",
                                "target_lang_sense_clarification": "increase in size, maturity, or expansion"
                            },
                            {
                                "target_lang_word": "evolve",
                                "target_lang_sense_clarification": "gradual change into a more complex state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c36b6ec0-cdf3-3d31-a08d-2e9a255386a9",
                    "sense_definition": "Происходить, протекать, разворачиваться во времени (о событиях, сюжете или действиях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "event_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "События в фильме <развиваются> очень стремительно и неожиданно.",
                            "target_lang_translations": {
                                "en": "The events in the movie <unfold> very rapidly and unexpectedly."
                            }
                        },
                        {
                            "text": "Мы с интересом наблюдали, как <развивается> этот конфликт.",
                            "target_lang_translations": {
                                "en": "We watched with interest as this conflict <developed>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "события развиваются",
                        "сюжет развивается",
                        "ситуация развивается"
                    ],
                    "target_lang_definitions": {
                        "en": "To happen, proceed, or unfold over time (regarding events, a plot, or actions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unfold",
                                "target_lang_sense_clarification": "reveal or happen over time (events, stories)"
                            },
                            {
                                "target_lang_word": "develop",
                                "target_lang_sense_clarification": "progress or evolve (situations, conflicts)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9109d6a2-47a3-3532-8cd8-0e549611762a",
                    "sense_definition": "Раскручиваться, распрямляться, переставать быть свитым или завитым (о волосах, веревке, пружине).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_untwist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От высокой влажности её локоны начали <развиваться>.",
                            "target_lang_translations": {
                                "en": "Due to the high humidity, her curls began to <uncurl>."
                            }
                        },
                        {
                            "text": "Старая пружина сломалась и резко <развилась>.",
                            "target_lang_translations": {
                                "en": "The old spring broke and suddenly <uncoiled>."
                            }
                        }
                    ],
                    "synonyms": [
                        "раскручиваться",
                        "распрямляться",
                        "разматываться"
                    ],
                    "antonyms": [
                        "свиваться"
                    ],
                    "common_phrases": [
                        "кудри развиваются",
                        "спираль развивается"
                    ],
                    "target_lang_definitions": {
                        "en": "To uncoil, straighten out, or cease to be twisted or curled (regarding hair, rope, or a spring)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "untwist",
                                "target_lang_sense_clarification": "remove twists"
                            },
                            {
                                "target_lang_word": "uncurl",
                                "target_lang_sense_clarification": "straighten from a curled state (e.g., hair)"
                            },
                            {
                                "target_lang_word": "uncoil",
                                "target_lang_sense_clarification": "release from a coiled state (e.g., spring)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
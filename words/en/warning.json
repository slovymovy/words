{
    "word_family": [
        "warn",
        "warner",
        "warningly",
        "forewarn"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c95bb4b0-520c-3649-9d00-cfdc0fd2923e",
                    "sense_definition": "A statement, event, or sign that indicates a possible danger or problem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_alert",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government issued a severe weather <w>warning</w> for the entire coast.",
                            "target_lang_translations": {
                                "ru": "Правительство выпустило <w>предупреждение</w> о суровых погодных условиях для всего побережья."
                            }
                        },
                        {
                            "text": "You should read the health <w>warning</w> on the label before taking the medicine.",
                            "target_lang_translations": {
                                "ru": "Тебе следует прочитать <w>предостережение</w> о вреде для здоровья на этикетке перед приемом лекарства."
                            }
                        }
                    ],
                    "synonyms": [
                        "caution"
                    ],
                    "common_phrases": [
                        "warning sign",
                        "issue a warning",
                        "health warning",
                        "final warning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заявление, событие или знак, указывающие на возможную опасность или проблему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предупреждение",
                                "target_lang_sense_clarification": "сообщение об опасности"
                            },
                            {
                                "target_lang_word": "предостережение",
                                "target_lang_sense_clarification": "совет быть осторожным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "196576a3-8eb3-3c89-9b36-3945c174bf34",
                    "sense_definition": "The act of telling someone about a possible danger or problem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_alert",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The storm arrived suddenly and without any <w>warning</w>.",
                            "target_lang_translations": {
                                "ru": "Шторм налетел внезапно и без какого-либо <w>предупреждения</w>."
                            }
                        },
                        {
                            "text": "He shouted a loud <w>warning</w> to the people standing near the edge.",
                            "target_lang_translations": {
                                "ru": "Он выкрикнул громкое <w>предупреждение</w> людям, стоящим у края."
                            }
                        }
                    ],
                    "synonyms": [
                        "caution"
                    ],
                    "common_phrases": [
                        "without warning",
                        "give a warning",
                        "advance warning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в сообщении кому-либо о возможной опасности или проблеме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предупреждение",
                                "target_lang_sense_clarification": "действие по глаголу предупреждать"
                            },
                            {
                                "target_lang_word": "предостережение",
                                "target_lang_sense_clarification": "наставление об осторожности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d18326c2-adc0-3421-9d3b-b2789cc5020a",
                    "sense_definition": "Present participle of <w>warn</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>warning</w> the children not to play too close to the street.",
                            "target_lang_translations": {
                                "ru": "Она <w>предупреждает</w> детей не играть слишком близко к дороге."
                            }
                        },
                        {
                            "text": "The police were <w>warning</w> drivers about the icy conditions on the bridge.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>предупреждала</w> водителей о гололеде на мосту."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола warn (предупреждать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предупреждающий",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "предупреждая",
                                "target_lang_sense_clarification": "деепричастие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "4c37516c-a253-390b-9fa4-9042ffe011c4",
                    "sense_definition": "A word used on signs or notices to alert people to immediate danger.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Warning</w>: High voltage equipment is operating in this area.",
                            "target_lang_translations": {
                                "ru": "<w>Осторожно</w>: В этой зоне работает высоковольтное оборудование."
                            }
                        },
                        {
                            "text": "<w>Warning</w>: Do not enter this construction site without a helmet.",
                            "target_lang_translations": {
                                "ru": "<w>Внимание</w>: Не входите на эту стройплощадку без каски."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое на знаках или в объявлениях, чтобы предупредить людей о непосредственной опасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осторожно",
                                "target_lang_sense_clarification": "на предупреждающих знаках"
                            },
                            {
                                "target_lang_word": "внимание",
                                "target_lang_sense_clarification": "призыв обратить внимание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "deed5aec-3557-3d6b-a23f-90d1053a62de",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Warning</w> has lived in this neighborhood for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Ворнинг</w> живет в этом районе уже более двадцати лет."
                            }
                        },
                        {
                            "text": "I have an appointment with Dr. <w>Warning</w> at three o'clock.",
                            "target_lang_translations": {
                                "ru": "У меня назначен прием у доктора <w>Ворнинга</w> на три часа."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "surname"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ворнинг",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]

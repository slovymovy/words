            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c502eda9-dd6e-3235-9cdd-df0e0dbca1ee",
                    "sense_definition": "Отсутствие порядка, ясности или последовательности в чём-либо; неразбериха.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_confusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за ошибки в расписании возникла страшная <w>путаница</w>.",
                            "target_lang_translations": {
                                "en": "A terrible <w>mix-up</w> arose due to an error in the schedule."
                            }
                        },
                        {
                            "text": "Новые правила только вносят <w>путаницу</w> в работу сотрудников.",
                            "target_lang_translations": {
                                "en": "The new rules only introduce <w>confusion</w> into the employees' work."
                            }
                        }
                    ],
                    "synonyms": [
                        "неразбериха",
                        "беспорядок",
                        "хаос",
                        "сумятица"
                    ],
                    "antonyms": [
                        "порядок",
                        "ясность",
                        "чёткость"
                    ],
                    "common_phrases": [
                        "вносить путаницу",
                        "возникла путаница",
                        "полная путаница",
                        "произошла путаница"
                    ],
                    "target_lang_definitions": {
                        "en": "A lack of order, clarity, or consistency; a state of confusion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "confusion",
                                "target_lang_sense_clarification": "a situation where things are not clear or easy to understand"
                            },
                            {
                                "target_lang_word": "muddle",
                                "target_lang_sense_clarification": "an untidy or confused state of affairs"
                            },
                            {
                                "target_lang_word": "mix-up",
                                "target_lang_sense_clarification": "a mistake caused by confusion, often regarding arrangements"
                            },
                            {
                                "target_lang_word": "mess",
                                "target_lang_sense_clarification": "a situation that is full of problems or difficulties"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de2a4952-c3d1-3a23-8cff-e5c331953cdd",
                    "sense_definition": "Беспорядочно переплетённые нити, верёвки, волосы или другие предметы.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_tangle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В ящике стола лежала <w>путаница</w> из старых проводов и зарядок.",
                            "target_lang_translations": {
                                "en": "In the desk drawer lay a <w>tangle</w> of old wires and chargers."
                            }
                        },
                        {
                            "text": "Она долго пыталась расчесать <w>путаницу</w> в своих длинных волосах.",
                            "target_lang_translations": {
                                "en": "She tried for a long time to comb out the <w>tangle</w> in her long hair."
                            }
                        }
                    ],
                    "synonyms": [
                        "клубок",
                        "моток"
                    ],
                    "common_phrases": [
                        "путаница проводов",
                        "путаница из ниток"
                    ],
                    "target_lang_definitions": {
                        "en": "Disorderly intertwined threads, ropes, hair, or other objects."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tangle",
                                "target_lang_sense_clarification": "a twisted mass of threads, hair, or other things"
                            },
                            {
                                "target_lang_word": "knot",
                                "target_lang_sense_clarification": "a part where hair or string has become twisted and hard to separate"
                            },
                            {
                                "target_lang_word": "mess",
                                "target_lang_sense_clarification": "something that looks dirty or untidy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41e6841a-6344-39bd-975f-d84d6bbdc0d5",
                    "sense_definition": "Сложное, беспорядочное или хитросплетённое расположение чего-либо (улиц, коридоров, следов).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_tangle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы быстро заблудились в этой <w>путанице</w> узких переулков.",
                            "target_lang_translations": {
                                "en": "We quickly got lost in this <w>maze</w> of narrow alleys."
                            }
                        },
                        {
                            "text": "Охотник с трудом разбирал <w>путаницу</w> звериных следов на снегу.",
                            "target_lang_translations": {
                                "en": "The hunter barely deciphered the <w>tangle</w> of animal tracks on the snow."
                            }
                        }
                    ],
                    "synonyms": [
                        "лабиринт"
                    ],
                    "common_phrases": [
                        "путаница улиц",
                        "путаница следов"
                    ],
                    "target_lang_definitions": {
                        "en": "A complex, disorderly, or intricate arrangement of things like streets or tracks."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "maze",
                                "target_lang_sense_clarification": "a complex network of paths or passages"
                            },
                            {
                                "target_lang_word": "tangle",
                                "target_lang_sense_clarification": "a confused or complicated arrangement"
                            },
                            {
                                "target_lang_word": "jumble",
                                "target_lang_sense_clarification": "a confused mixture of things"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d809f258-2ea8-385b-b929-a0ecf6823e29",
                    "sense_definition": "Osoba, którą się podziwia, uwielbia i traktuje jako wzór do naśladowania, często celebryta lub artysta.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moim największym <idolem> jest znany piosenkarz, którego muzykę uwielbiam.",
                            "target_lang_translations": {
                                "ru": "Моим самым большим <w>кумиром</w> является известный певец, чью музыку я обожаю."
                            }
                        },
                        {
                            "text": "Wielu młodych ludzi marzy o tym, by zostać <idolem> tłumów.",
                            "target_lang_translations": {
                                "ru": "Многие молодые люди мечтают стать <w>кумиром</w> толпы."
                            }
                        }
                    ],
                    "synonyms": [
                        "autorytet",
                        "bożyszcze"
                    ],
                    "antonyms": [
                        "fan",
                        "kibic"
                    ],
                    "common_phrases": [
                        "idol nastolatek",
                        "mój idol",
                        "zostać idolem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, которым восхищаются, которого боготворят и считают образцом для подражания, часто знаменитость или артист."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кумир",
                                "target_lang_sense_clarification": "объект восхищения"
                            },
                            {
                                "target_lang_word": "идол",
                                "target_lang_sense_clarification": "предмет поклонения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cafa951c-1333-3c04-a3e4-a37a1768f6cf",
                    "sense_definition": "Figura, posąg lub przedmiot wyobrażający bóstwo, otaczany czcią religijną.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <idol> pochodzi z epoki brązu i został znaleziony podczas wykopalisk.",
                            "target_lang_translations": {
                                "ru": "Этот <w>идол</w> относится к бронзовому веку и был найден во время раскопок."
                            }
                        },
                        {
                            "text": "Dawne plemiona składały ofiary kamiennym <idolom> w nadziei na pomyślność.",
                            "target_lang_translations": {
                                "ru": "Древние племена приносили жертвы каменным <w>идолам</w> в надежде на благополучие."
                            }
                        }
                    ],
                    "synonyms": [
                        "bożek",
                        "bałwan"
                    ],
                    "common_phrases": [
                        "pogański idol",
                        "złoty idol",
                        "czcić idola"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фигура, статуя или предмет, изображающий божество и являющийся объектом религиозного поклонения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идол",
                                "target_lang_sense_clarification": "изображение божества"
                            },
                            {
                                "target_lang_word": "истукан",
                                "target_lang_sense_clarification": "статуя"
                            }
                        ]
                    }
                }
            ]
        }
    ]

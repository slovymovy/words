{
    "word_family": [
        "podziękowanie",
        "wdzięczność",
        "dziękczynienie",
        "wdzięczny",
        "podziękować",
        "odwdzięczać się"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "38ad52b8-31b4-3710-86fe-d6be6e19ff83",
                    "sense_definition": "Wyrażać wdzięczność słowami lub gestem za otrzymane dobro, pomoc, prezent lub życzliwość.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "gratitude_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze <dziękuję> rodzicom za wsparcie, które mi okazują każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Я всегда <благодарю> родителей за поддержку, которую они мне оказывают каждый день."
                            }
                        },
                        {
                            "text": "Uczniowie <dziękowali> nauczycielce za jej cierpliwość i przekazaną wiedzę.",
                            "target_lang_translations": {
                                "ru": "Ученики <благодарили> учительницу за её терпение и переданные знания."
                            }
                        }
                    ],
                    "synonyms": [
                        "być wdzięcznym",
                        "wyrażać wdzięczność"
                    ],
                    "common_phrases": [
                        "dziękować z góry",
                        "dziękować za pomoc",
                        "dziękować Bogu",
                        "dziękować za pamięć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать благодарность словами или жестом за полученное добро, помощь, подарок или доброжелательность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благодарить",
                                "target_lang_sense_clarification": "выражать признательность"
                            },
                            {
                                "target_lang_word": "говорить спасибо",
                                "target_lang_sense_clarification": "выражать благодарность устно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96f6b4b8-e532-32e1-9d44-97653cee27c5",
                    "sense_definition": "Uprzejmie odmawiać przyjęcia propozycji lub oferty, dając do zrozumienia brak zainteresowania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "polite_refusal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy kelner zaproponował mi deser, grzecznie <podziękowałem>, bo byłem już pełny.",
                            "target_lang_translations": {
                                "ru": "Когда официант предложил мне десерт, я вежливо <отказался>, так как был уже сыт."
                            }
                        },
                        {
                            "text": "Z takimi warunkami umowy to ja zdecydowanie <dziękuję> i poszukam innej pracy.",
                            "target_lang_translations": {
                                "ru": "С такими условиями договора я решительно <отказываюсь> и поищу другую работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ja dziękuję",
                        "dziękuję postoję"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in spoken language to mean 'no thanks'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливо отказываться от предложения, давая понять отсутствие интереса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказываться",
                                "target_lang_sense_clarification": "вежливо отклонять предложение"
                            },
                            {
                                "target_lang_word": "отклонять",
                                "target_lang_sense_clarification": "не принимать предложение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c99dca2d-acc6-3a7b-b315-8d81f0619f09",
                    "sense_definition": "Odwzajemniać się komuś za coś, reagować na czyjeś działanie w podobny sposób (często ironicznie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reciprocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie warto <dziękować> złem za dobre, które nas w życiu spotkało.",
                            "target_lang_translations": {
                                "ru": "Не стоит <отплачивать> злом за добро, которое встретилось нам в жизни."
                            }
                        },
                        {
                            "text": "W tej powieści bohaterowie często <dziękują> pięknym za nadobne swoim wrogom.",
                            "target_lang_translations": {
                                "ru": "В этом романе герои часто <платят> той же монетой своим врагам."
                            }
                        }
                    ],
                    "synonyms": [
                        "odwdzięczać się"
                    ],
                    "common_phrases": [
                        "dziękować pięknym za nadobne"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly used in specific idioms or literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отвечать кому-либо на что-либо, реагировать на чьи-то действия подобным образом (часто иронично)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отплачивать",
                                "target_lang_sense_clarification": "отвечать тем же"
                            },
                            {
                                "target_lang_word": "платить той же монетой",
                                "target_lang_sense_clarification": "идиома: поступать так же, как поступили с тобой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4ee065b-6fdd-3562-91f2-9832221204b4",
                    "sense_definition": "Potwierdzać odbiór należności i zrzekać się dalszych roszczeń (terminologia dawna lub prawnicza).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_acknowledgment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starych dokumentach wierzyciel <dziękował> dłużnikowi za spłatę całej sumy.",
                            "target_lang_translations": {
                                "ru": "В старых документах кредитор <подтверждал получение оплаты> должнику за выплату всей суммы."
                            }
                        },
                        {
                            "text": "Strony ustaliły, że po zapłacie ostatecznej kwoty będą sobie wzajemnie <dziękować> z wszelkich roszczeń.",
                            "target_lang_translations": {
                                "ru": "Стороны установили, что после уплаты окончательной суммы они будут взаимно <освобождать> друг друга от любых претензий."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical legal usage, replaced by 'kwitować'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подтверждать получение долга и отказываться от дальнейших претензий (устаревшая или юридическая терминология)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подтверждать получение",
                                "target_lang_sense_clarification": "фиксировать оплату"
                            },
                            {
                                "target_lang_word": "квитировать",
                                "target_lang_sense_clarification": "устаревший термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

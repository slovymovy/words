                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz przeładował <w>automat</w> i przygotował się do obrony.",
                            "target_lang_translations": {

                    "target_lang_definitions": {
                        "ru": "Колеса и механическая конструкция, используемые самолетом для взлета и посадки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шасси",
                                "target_lang_sense_clarification": "в авиации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3903230d-21f7-3a76-85eb-b4ad2ee0384f",
                    "sense_definition": "Illegal recreational drugs.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police arrested a dealer who was selling <w>gear</w> in the club.",
                            "target_lang_translations": {
                                "ru": "Полиция арестовала дилера, который продавал <w>наркотики</w> в клубе."
                            }
                        },
                        {
                            "text": "He ruined his life by getting hooked on the <w>gear</w>.",
                            "target_lang_translations": {
                                "ru": "Он разрушил свою жизнь, подсев на <w>наркоту</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "drugs",
                        "dope"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }

{
    "word_family": [
        "powstrzymywać",
        "powstrzymanie",
        "niepowstrzymany"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5660ab4c-7825-3046-b63d-56f4f6d8b3a9",
                    "sense_definition": "Sprawić, że ktoś lub coś przestanie działać, poruszać się lub że jakieś zjawisko nie będzie się dalej rozwijać; nie pozwolić komuś czegoś zrobić.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stopping_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy natychmiast <powstrzymać> wyciek wody w łazience.",
                            "target_lang_translations": {
                                "ru": "Мы должны немедленно <остановить> утечку воды в ванной."
                            }
                        },
                        {
                            "text": "Próbował <powstrzymać> łzy, ale emocje były zbyt silne.",
                            "target_lang_translations": {
                                "ru": "Он пытался <сдержать> слёзы, но эмоции были слишком сильны."
                            }
                        },
                        {
                            "text": "Policja zdołała <powstrzymać> tłum przed wejściem na stadion.",
                            "target_lang_translations": {
                                "ru": "Полиции удалось <сдержать> толпу от входа на стадион."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatrzymać",
                        "zahamować",
                        "wstrzymać",
                        "zablokować"
                    ],
                    "antonyms": [
                        "puścić",
                        "uwolnić"
                    ],
                    "common_phrases": [
                        "powstrzymać łzy",
                        "powstrzymać atak",
                        "powstrzymać oddech",
                        "powstrzymać kogoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остановить действие или развитие чего-либо; не дать кому-то совершить поступок; сдержать проявление чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдержать",
                                "target_lang_sense_clarification": "натиск, эмоции, обещание"
                            },
                            {
                                "target_lang_word": "удержать",
                                "target_lang_sense_clarification": "от совершения действия"
                            },
                            {
                                "target_lang_word": "остановить",
                                "target_lang_sense_clarification": "прекратить движение или процесс"
                            },
                            {
                                "target_lang_word": "предотвратить",
                                "target_lang_sense_clarification": "не допустить возникновения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9edce11-1e32-3192-94ce-16d09d190960",
                    "sense_definition": "Zapanować nad sobą, aby czegoś nie zrobić, nie powiedzieć lub nie okazać emocji (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "self_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy usłyszałem ten żart, nie mogłem się <powstrzymać> od głośnego śmiechu.",
                            "target_lang_translations": {
                                "ru": "Когда я услышал эту шутку, я не мог <удержаться> от громкого смеха."
                            }
                        },
                        {
                            "text": "Chciałem mu odpowiedzieć coś niemiłego, ale w ostatniej chwili zdołałem się <powstrzymać>.",
                            "target_lang_translations": {
                                "ru": "Я хотел ответить ему что-то неприятное, но в последний момент сумел <сдержаться>."
                            }
                        }
                    ],
                    "synonyms": [
                        "opanować się",
                        "wstrzymać się"
                    ],
                    "antonyms": [
                        "ulec"
                    ],
                    "common_phrases": [
                        "powstrzymać się od śmiechu",
                        "powstrzymać się od komentarza",
                        "powstrzymać się od głosu",
                        "ledwo się powstrzymać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Усилием воли не совершить какого-либо действия или не проявить эмоций (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдержаться",
                                "target_lang_sense_clarification": "не проявить чувств"
                            },
                            {
                                "target_lang_word": "удержаться",
                                "target_lang_sense_clarification": "от соблазна или реакции"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
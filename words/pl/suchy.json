            "senses": [
                {
                    "sense_id": "3c2d2f62-483e-3197-80e8-2da1b6cc0d06",
                    "sense_definition": "Polskie nazwisko męskie.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Suchy</w> został wybrany na przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Сухы</w> был избран председателем совета жилого комплекса."
                            }
                        },
                        {
                            "text": "Nie znam osobiście nikogo o nazwisku <w>Suchy</w>.",
                            "target_lang_translations": {
                                "ru": "Я лично не знаю никого по фамилии <w>Сухы</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "declines like an adjective"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская мужская фамилия."
                    },
                    "translations": {

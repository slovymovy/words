    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e37e3eb8-1382-3ffc-8c1b-0c491f381608",
                    "sense_definition": "Становиться заметным, обращать на себя внимание (обычно когда нужно остаться скрытым); выдавать своё присутствие.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visibility_detection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Агент старался лишний раз не <w>засвечиваться</w> в центре города.",
                            "target_lang_translations": {
                                "en": "The agent tried not to <w>get spotted</w> in the city center unnecessarily."
                            }
                        },
                        {
                            "text": "Грабителям нельзя <w>засвечиваться</w> перед камерами наблюдения.",
                            "target_lang_translations": {
                                "en": "The robbers must not <w>show up</w> on surveillance cameras."
                            }
                        }
                    ],
                    "synonyms": [
                        "обнаруживаться",
                        "попадаться",
                        "выделяться"
                    ],
                    "antonyms": [
                        "скрываться",
                        "прятаться",
                        "маскироваться"
                    ],
                    "common_phrases": [
                        "лучше не засвечиваться",
                        "засвечиваться на публике"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become noticeable or draw attention to oneself (usually when trying to remain hidden); to reveal one's presence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get spotted",
                                "target_lang_sense_clarification": "to be seen or detected, especially when trying to hide"
                            },
                            {
                                "target_lang_word": "blow one's cover",
                                "target_lang_sense_clarification": "to accidentally reveal one's secret identity or presence"
                            },
                            {
                                "target_lang_word": "show one's face",
                                "target_lang_sense_clarification": "to appear in public or be seen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1b2a6b0-ff09-3b0d-b43c-cbf6cc2efd8f",
                    "sense_definition": "Портиться или терять изображение от избыточного попадания света (о фотоплёнке, фотобумаге или кадре).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "photography_damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если открыть крышку фотоаппарата, плёнка мгновенно <w>засвечивается</w>.",
                            "target_lang_translations": {
                                "en": "If you open the camera back, the film instantly <w>gets exposed</w>."
                            }
                        },
                        {
                            "text": "Снимок <w>засвечивается</w>, когда вы снимаете прямо против солнца.",
                            "target_lang_translations": {
                                "en": "The shot <w>gets overexposed</w> when you shoot directly against the sun."
                            }
                        }
                    ],
                    "synonyms": [
                        "портиться"
                    ],
                    "common_phrases": [
                        "плёнка засвечивается",
                        "кадр засвечивается"
                    ],
                    "target_lang_definitions": {
                        "en": "To be spoiled or lose image quality due to excessive exposure to light (regarding photographic film or paper)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get overexposed",
                                "target_lang_sense_clarification": "photography: to receive too much light"
                            },
                            {
                                "target_lang_word": "get exposed",
                                "target_lang_sense_clarification": "film: to be ruined by accidental light"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "buiging",
        "buigzaam",
        "afbuigen",
        "verbuigen",
        "ombuigen",
        "doorbuigen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "445e03a9-a348-3f05-b30f-c784aa8b85c8",
                    "sense_definition": "Krom maken of krom worden; van vorm veranderen zodat het niet meer recht is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_deformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De sterke man <w>boog</w> de ijzeren staaf met zijn blote handen.",
                            "target_lang_translations": {
                                "en": "The strong man <w>bent</w> the iron bar with his bare hands.",
                                "ru": "Силач <w>согнул</w> железный прут голыми руками."
                            }
                        },
                        {
                            "text": "De bomen <w>bogen</w> diep door de zware windstoten.",
                            "target_lang_translations": {
                                "en": "The trees <w>bent</w> deep under the heavy gusts of wind.",
                                "ru": "Деревья низко <w>гнулись</w> под сильными порывами ветра."
                            }
                        }
                    ],
                    "synonyms": [
                        "plooien"
                    ],
                    "antonyms": [
                        "rechten"
                    ],
                    "common_phrases": [
                        "het hoofd buigen",
                        "door de knieën buigen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make curved or to become curved; to change shape so it is no longer straight.",
                        "ru": "Делать кривым или становиться кривым; менять форму так, что она перестает быть прямой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bend",
                                "target_lang_sense_clarification": "change shape"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гнуть",
                                "target_lang_sense_clarification": "делать кривым"
                            },
                            {
                                "target_lang_word": "сгибать",
                                "target_lang_sense_clarification": "изменять форму"
                            },
                            {
                                "target_lang_word": "гнуться",
                                "target_lang_sense_clarification": "становиться кривым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e6f0998-1da6-3c52-8bb6-5c493b658de3",
                    "sense_definition": "Het bovenlichaam of hoofd naar voren bewegen als groet, uit beleefdheid of respect.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "body_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De acteurs <w>bogen</w> voor het publiek na de voorstelling.",
                            "target_lang_translations": {
                                "en": "The actors <w>bowed</w> to the audience after the performance.",
                                "ru": "Актеры <w>поклонились</w> публике после представления."
                            }
                        },
                        {
                            "text": "Hij <w>buigt</w> altijd beleefd als hij de koning ontmoet.",
                            "target_lang_translations": {
                                "en": "He always <w>bows</w> politely when he meets the king.",
                                "ru": "Он всегда вежливо <w>кланяется</w> при встрече с королем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "diep buigen",
                        "voor iemand buigen"
                    ],
                    "target_lang_definitions": {
                        "en": "To move the upper body or head forward as a greeting, out of politeness or respect.",
                        "ru": "Наклонять верхнюю часть тела или голову вперед в качестве приветствия, из вежливости или уважения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bow",
                                "target_lang_sense_clarification": "gesture of respect"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кланяться",
                                "target_lang_sense_clarification": "приветствовать поклоном"
                            },
                            {
                                "target_lang_word": "поклониться",
                                "target_lang_sense_clarification": "сделать поклон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72af0482-4595-39e4-998e-5253553354ce",
                    "sense_definition": "Een bocht maken of van richting veranderen (bijvoorbeeld een weg of rivier).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het smalle pad <w>buigt</w> scherp naar links bij de oude eik.",
                            "target_lang_translations": {
                                "en": "The narrow path <w>bends</w> sharply to the left by the old oak.",
                                "ru": "У старого дуба узкая тропинка резко <w>поворачивает</w> влево."
                            }
                        },
                        {
                            "text": "De rivier <w>buigt</w> hier langzaam af richting de zee.",
                            "target_lang_translations": {
                                "en": "The river <w>curves</w> slowly towards the sea here.",
                                "ru": "Здесь река медленно <w>изгибается</w> в сторону моря."
                            }
                        }
                    ],
                    "synonyms": [
                        "plooien"
                    ],
                    "common_phrases": [
                        "naar rechts buigen",
                        "naar links buigen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a turn or change direction (e.g. a road or river).",
                        "ru": "Делать поворот или менять направление (например, о дороге или реке)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bend",
                                "target_lang_sense_clarification": "change direction"
                            },
                            {
                                "target_lang_word": "curve",
                                "target_lang_sense_clarification": "follow a curved path"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поворачивать",
                                "target_lang_sense_clarification": "менять направление"
                            },
                            {
                                "target_lang_word": "изгибаться",
                                "target_lang_sense_clarification": "проходить по кривой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b0c57bb-be5f-3170-acac-8176004e2659",
                    "sense_definition": "(zich buigen over) Aandacht besteden aan een probleem of vraagstuk om het op te lossen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_study",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De commissie <w>buigt</w> zich momenteel over het nieuwe wetsvoorstel.",
                            "target_lang_translations": {
                                "en": "The committee is currently <w>looking into</w> the new bill.",
                                "ru": "В данный момент комитет <w>рассматривает</w> новый законопроект."
                            }
                        },
                        {
                            "text": "De rechter <w>boog</w> zich over de ingewikkelde details van de zaak.",
                            "target_lang_translations": {
                                "en": "The judge <w>examined</w> the complicated details of the case.",
                                "ru": "Судья <w>изучал</w> сложные детали дела."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich over iets buigen"
                    ],
                    "target_lang_definitions": {
                        "en": "(zich buigen over) To devote attention to a problem or issue in order to solve it.",
                        "ru": "(zich buigen over) Уделять внимание проблеме или вопросу с целью его решения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look into",
                                "target_lang_sense_clarification": "investigate"
                            },
                            {
                                "target_lang_word": "examine",
                                "target_lang_sense_clarification": "study closely"
                            },
                            {
                                "target_lang_word": "consider",
                                "target_lang_sense_clarification": "think about"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рассматривать",
                                "target_lang_sense_clarification": "вникать в детали"
                            },
                            {
                                "target_lang_word": "изучать",
                                "target_lang_sense_clarification": "исследовать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
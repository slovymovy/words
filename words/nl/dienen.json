{
    "word_family": [
        "dienst",
        "dienaar",
        "bedienen",
        "verdienen",
        "toedienen",
        "dienstig",
        "dienster"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bc5e22bb-c013-38ea-b755-41bcbc7e3822",
                    "sense_definition": "Een bepaalde functie hebben of ergens voor gebruikt worden; van nut zijn.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik weet niet waar dit vreemde gereedschap voor <w>dient</w>.",
                            "target_lang_translations": {
                                "en": "I don't know what this strange tool is <w>used</w> for.",
                                "ru": "Я не знаю, для чего <w>служит</w> этот странный инструмент."
                            }
                        },
                        {
                            "text": "Deze oude schuur zal tijdelijk als opslagruimte <w>dienen</w>.",
                            "target_lang_translations": {
                                "en": "This old barn will temporarily <w>serve</w> as storage space.",
                                "ru": "Этот старый сарай временно будет <w>служить</w> складом."
                            }
                        },
                        {
                            "text": "Zijn fouten kunnen voor ons als waarschuwing <w>dienen</w>.",
                            "target_lang_translations": {
                                "en": "His mistakes can <w>serve</w> as a warning to us.",
                                "ru": "Его ошибки могут <w>послужить</w> нам предупреждением."
                            }
                        }
                    ],
                    "synonyms": [
                        "fungeren",
                        "gebruikt worden"
                    ],
                    "common_phrases": [
                        "nergens toe dienen",
                        "als voorbeeld dienen",
                        "tot gevolg hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a specific function or be used for a particular purpose.",
                        "ru": "Выполнять определенную функцию или использоваться для чего-либо; быть полезным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "function as"
                            },
                            {
                                "target_lang_word": "be used",
                                "target_lang_sense_clarification": "have a purpose"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "служить",
                                "target_lang_sense_clarification": "выполнять роль или функцию"
                            },
                            {
                                "target_lang_word": "годиться",
                                "target_lang_sense_clarification": "быть пригодным для чего-либо"
                            },
                            {
                                "target_lang_word": "предназначаться",
                                "target_lang_sense_clarification": "иметь целью использования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf1d9efb-c28d-3f89-acd9-3dbe9707e388",
                    "sense_definition": "Verplicht zijn om iets te doen; moeten (vaak gebruikt in formele regels, wetten of voorschriften).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "U <w>dient</w> uw aanvraag voor het einde van de maand in te leveren.",
                            "target_lang_translations": {
                                "en": "You <w>must</w> submit your application before the end of the month.",
                                "ru": "Вы <w>должны</w> подать заявление до конца месяца."
                            }
                        },
                        {
                            "text": "Alle bezoekers <w>dienen</w> zich bij de receptie te melden.",
                            "target_lang_translations": {
                                "en": "All visitors <w>must</w> report to the reception.",
                                "ru": "Все посетители <w>должны</w> отметиться на ресепшене."
                            }
                        },
                        {
                            "text": "Melk <w>dient</w> na opening gekoeld bewaard te blijven.",
                            "target_lang_translations": {
                                "en": "Milk <w>should</w> be kept refrigerated after opening.",
                                "ru": "Молоко после вскрытия <w>следует</w> хранить в холодильнике."
                            }
                        }
                    ],
                    "synonyms": [
                        "moeten",
                        "behoren"
                    ],
                    "antonyms": [
                        "mogen",
                        "hoeven"
                    ],
                    "common_phrases": [
                        "men dient te",
                        "u dient zich"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be obliged or required to do something (formal).",
                        "ru": "Быть обязанным что-то сделать (часто используется в официальных правилах и предписаниях)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "must",
                                "target_lang_sense_clarification": "requirement"
                            },
                            {
                                "target_lang_word": "have to",
                                "target_lang_sense_clarification": "obligation"
                            },
                            {
                                "target_lang_word": "should",
                                "target_lang_sense_clarification": "advisability"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть должным",
                                "target_lang_sense_clarification": "обязанность"
                            },
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "безличная форма, предписание"
                            },
                            {
                                "target_lang_word": "надлежать",
                                "target_lang_sense_clarification": "формальное требование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd9ad51a-c941-31ad-acf8-83bfa058b428",
                    "sense_definition": "Werken voor iemand of de belangen van iemand of iets (zoals een land of god) behartigen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "service",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De minister beloofde het land trouw te <w>dienen</w>.",
                            "target_lang_translations": {
                                "en": "The minister promised to faithfully <w>serve</w> the country.",
                                "ru": "Министр пообещал верно <w>служить</w> стране."
                            }
                        },
                        {
                            "text": "Hij heeft zijn meester vele jaren met toewijding <w>gediend</w>.",
                            "target_lang_translations": {
                                "en": "He <w>served</w> his master with dedication for many years.",
                                "ru": "Он преданно <w>служил</w> своему господину много лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "werken voor",
                        "bijstaan"
                    ],
                    "antonyms": [
                        "heersen",
                        "bevelen"
                    ],
                    "common_phrases": [
                        "de gemeenschap dienen",
                        "God dienen"
                    ],
                    "target_lang_definitions": {
                        "en": "To work for someone or support the interests of a person, country, or cause.",
                        "ru": "Работать на кого-то или защищать чьи-то интересы (страны, бога)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "work for"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "служить",
                                "target_lang_sense_clarification": "быть на службе, проявлять преданность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8f29600-9630-3f91-9c99-2a4ece88f4f5",
                    "sense_definition": "Als militair actief zijn in de krijgsmacht.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "service",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn grootvader <w>diende</w> tijdens de oorlog in de marine.",
                            "target_lang_translations": {
                                "en": "My grandfather <w>served</w> in the navy during the war.",
                                "ru": "Мой дедушка во время войны <w>служил</w> на флоте."
                            }
                        },
                        {
                            "text": "Hij wil graag bij de luchtmacht <w>dienen</w> als piloot.",
                            "target_lang_translations": {
                                "en": "He would like to <w>serve</w> in the air force as a pilot.",
                                "ru": "Он хочет <w>служить</w> пилотом в военно-воздушных силах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in het leger dienen",
                        "onder de wapenen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be active as a member of the armed forces.",
                        "ru": "Проходить военную службу; быть в армии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "military duty"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "служить",
                                "target_lang_sense_clarification": "военная служба"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "15a77752-7d51-32b3-b71e-749b62fdc498",
                    "sense_definition": "Druga osoba liczby pojedynczej trybu rozkazującego czasownika <w>ubawić</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ubaw</w> nas jakąś śmieszną historią, bo wszyscy są smutni.",
                            "target_lang_translations": {
                                "ru": "<w>Развесели</w> нас какой-нибудь смешной историей, а то все грустные."
                            }
                        },
                        {
                            "text": "Proszę, <w>ubaw</w> gościa rozmową, zanim podam obiad.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>развлеки</w> гостя беседой, пока я не подам обед."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма повелительного наклонения глагола \"ubawić\" (развеселить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развесели",
                                "target_lang_sense_clarification": "подними настроение"
                            },
                            {
                                "target_lang_word": "развлеки",
                                "target_lang_sense_clarification": "займи чем-то интересным"

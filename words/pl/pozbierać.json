            "senses": [
                {
                    "sense_id": "cface5b9-d251-3ee3-b84b-58438f6a358a",
                    "sense_definition": "Podnieść z ziemi lub podłogi wiele przedmiotów leżących w nieładzie i zgromadzić je w jednym miejscu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <pozbierać> wszystkie klocki, które dzieci zostawiły na dywanie.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <собрать> все кубики, которые дети оставили на ковре."
                            }
                        },
                        {
                            "text": "Pomóż mi proszę <pozbierać> te rozsypane dokumenty.",
                            "target_lang_translations": {
                                "ru": "Помоги мне, пожалуйста, <собрать> эти рассыпанные документы."
                            }
                        }
                    ],
                    "synonyms": [
                        "podnieść",
                        "zgromadzić",
                        "zebrać"
                    ],
                    "antonyms": [
                        "rozrzucić",
                        "rozsypać"
                    ],
                    "common_phrases": [
                        "pozbierać zabawki",
                        "pozbierać śmieci",
                        "pozbierać z podłogi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поднять с земли или пола множество разбросанных предметов и сложить их в одном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собрать",
                                "target_lang_sense_clarification": "разбросанные предметы"
                            },
                            {
                                "target_lang_word": "подобрать",
                                "target_lang_sense_clarification": "поднять с земли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c371f52b-be99-37a4-9965-52d585f3730b",
                    "sense_definition": "Odzyskać równowagę psychiczną, opanować emocje lub wrócić do zdrowia po trudnym przeżyciu (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_recovery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po śmierci męża długo nie mogła się <pozbierać>.",
                            "target_lang_translations": {
                                "ru": "После смерти мужа она долго не могла <прийти в себя>."
                            }
                        },
                        {
                            "text": "Mam nadzieję, że szybko się <pozbierasz> po tej grypie.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, ты быстро <оправишься> после этого гриппа."
                            }
                        }
                    ],
                    "synonyms": [
                        "dojść do siebie",
                        "otrząsnąć się"
                    ],
                    "antonyms": [
                        "załamać się"
                    ],
                    "common_phrases": [
                        "pozbierać się po rozstaniu",
                        "pozbierać się po chorobie",
                        "pozbierać się do kupy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Восстановить душевное равновесие, справиться с эмоциями или выздороветь после тяжелого переживания (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [

            "senses": [
                {
                    "sense_id": "31693157-93bc-3d70-9d53-c00f6093b270",
                    "sense_definition": "Een vuur of brand doen stoppen met branden; doven.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fire_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brandweer kon met moeite de grote brand <w>blussen</w>.",
                            "target_lang_translations": {
                                "en": "The fire brigade struggled to <w>put out</w> the large fire.",
                                "ru": "Пожарные с трудом смогли <w>потушить</w> сильный пожар."
                            }
                        },
                        {
                            "text": "De vlammen worden <w>geblust</w> met behulp van speciale vliegtoestellen.",
                            "target_lang_translations": {
                                "en": "The flames are being <w>extinguished</w> with the help of special aircraft.",
                                "ru": "Пламя <w>тушат</w> с помощью специальных самолетов."
                            }
                        }
                    ],
                    "synonyms": [
                        "doven",
                        "uitmaken"
                    ],
                    "antonyms": [
                        "aansteken",
                        "ontsteken"
                    ],
                    "common_phrases": [
                        "brand blussen",
                        "vuur blussen"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause a fire or burning to stop; to extinguish.",
                        "ru": "Заставить огонь или пожар прекратить гореть; потушить."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "extinguish",
                                "target_lang_sense_clarification": "formal or general term"
                            },
                            {
                                "target_lang_word": "put out",
                                "target_lang_sense_clarification": "common phrasal verb"
                            },
                            {
                                "target_lang_word": "douse",
                                "target_lang_sense_clarification": "to extinguish by throwing liquid over"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тушить",
                                "target_lang_sense_clarification": "огонь, пожар"
                            },
                            {
                                "target_lang_word": "гасить",
                                "target_lang_sense_clarification": "пламя, свет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e942a99-b09e-3d51-9a98-c63187aa3899",
                    "sense_definition": "Een vloeistof (zoals wijn of water) in een hete pan gieten om het braadvocht los te maken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het bakken kun je het vlees <w>blussen</w> met rode wijn.",
                            "target_lang_translations": {
                                "en": "While frying, you can <w>deglaze</w> the meat with red wine.",
                                "ru": "Во время жарки мясо можно <w>дегласировать</w> красным вином."
                            }
                        },
                        {
                            "text": "De kok zal de jus <w>blussen</w> met een beetje koude bouillon.",
                            "target_lang_translations": {
                                "en": "The chef will <w>deglaze</w> the gravy with a little cold broth.",
                                "ru": "Повар <w>дегласирует</w> соус небольшим количеством холодного бульона."
                            }
                        }
                    ],
                    "synonyms": [
                        "afblussen",
                        "deglaceren"
                    ],
                    "common_phrases": [
                        "met wijn blussen",
                        "saus blussen"
                    ],
                    "target_lang_definitions": {
                        "en": "To pour liquid (such as wine or water) into a hot pan to loosen the cooking juices.",
                        "ru": "Влить жидкость (например, вино или воду) в горячую сковороду, чтобы растворить сок от жарки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deglaze",
                                "target_lang_sense_clarification": "culinary term"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дегласировать",
                                "target_lang_sense_clarification": "кулинария"
                            }
                        ]
                    }
                }
            ]
        }
    ]

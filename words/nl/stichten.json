                {
                    "sense_id": "82b89217-15d4-3471-ab99-99fc4148bfda",
                    "sense_definition": "Iets nieuws laten ontstaan, zoals een organisatie, stad of gezin; de grondslag leggen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "founding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kaapstad werd in 1652 <w>gesticht</w> door Jan van Riebeeck en zijn mannen.",
                            "target_lang_translations": {
                                "en": "Cape Town was <w>founded</w> in 1652 by Jan van Riebeeck and his men.",
                                "ru": "Кейптаун был <w>основан</w> в 1652 году Яном ван Рибеком и его людьми."
                            }
                        },
                        {
                            "text": "Zij willen samen trouwen en een gezin <w>stichten</w>.",
                            "target_lang_translations": {
                                "en": "They want to get married and <w>start</w> a family together.",
                                "ru": "Они хотят пожениться и <w>создать</w> семью."
                            }
                        }
                    ],
                    "synonyms": [
                        "oprichten",
                        "vestigen",
                        "instellen",
                        "grondvesten"
                    ],
                    "antonyms": [
                        "opheffen",
                        "ontbinden"
                    ],
                    "common_phrases": [
                        "een gezin stichten",
                        "een vereniging stichten",
                        "een stad stichten"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring something new into existence, such as an organization, city, or family.",
                        "ru": "Создавать что-то новое, например организацию, город или семью; закладывать основу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "found",
                                "target_lang_sense_clarification": "to establish an institution or city"
                            },
                            {
                                "target_lang_word": "establish",
                                "target_lang_sense_clarification": "to set up on a firm basis"
                            },
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "used in the context of a family"
                            }
                        ],
                        "ru": [
                            {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d596223-2158-3846-b833-2dd43d2339c2",
                    "sense_definition": "Een systeem van overtuigingen en praktijken dat betrekking heeft op het goddelijke, de ziel en het leven na de dood.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "godsdienst_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vrijheid van <w>religie</w> is een belangrijk recht in een democratie.",
                            "target_lang_translations": {
                                "en": "Freedom of <w>religion</w> is an important right in a democracy.",
                                "ru": "Свобода <w>религии</w> является важным правом в демократии."
                            }
                        },
                        {
                            "text": "De overheid moet neutraal blijven en mag geen specifieke <w>religie</w> bevoordelen.",
                            "target_lang_translations": {
                                "en": "The government must remain neutral and may not favor any specific <w>religion</w>.",
                                "ru": "Государство должно сохранять нейтралитет и не может отдавать предпочтение какой-либо конкретной <w>религии</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "godsdienst",
                        "geloof"
                    ],
                    "common_phrases": [
                        "vrijheid van religie",
                        "georganiseerde religie"
                    ],
                    "target_lang_definitions": {
                        "en": "A system of beliefs and practices concerning the divine, the soul, and life after death.",
                        "ru": "Система верований и практик, связанных с божественным, душой и жизнью после смерти."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "religion",
                                "target_lang_sense_clarification": "general belief system"
                            },
                            {

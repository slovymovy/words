{
    "word_family": [
        "wysepka",
        "wyspowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "913f3cba-470b-3a9a-bc36-e222c2617370",
                    "sense_definition": "Ląd otoczony ze wszystkich stron wodami morza, jeziora lub rzeki.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robinson Crusoe spędził wiele lat na bezludnej <w>wyspie</w>.",
                            "target_lang_translations": {
                                "ru": "Робинзон Крузо провел много лет на необитаемом <w>острове</w>."
                            }
                        },
                        {
                            "text": "W te wakacje planujemy popłynąć na słoneczną <w>wyspę</w>.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы планируем отправиться на солнечный <w>остров</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bezludna wyspa",
                        "mieszkać na wyspie",
                        "egzotyczna wyspa",
                        "wyspa skarbów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок суши, со всех сторон окруженный водой моря, озера или реки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остров",
                                "target_lang_sense_clarification": "география"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf117f36-5efb-365a-8863-d0aa81857772",
                    "sense_definition": "Wolnostojący element zabudowy kuchennej, nieprzylegający do ścian, zapewniający dodatkowy blat roboczy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marzę o dużej kuchni, w której na środku stanie nowoczesna <w>wyspa</w>.",
                            "target_lang_translations": {
                                "ru": "Я мечтаю о большой кухне, посреди которой будет стоять современный <w>остров</w>."
                            }
                        },
                        {
                            "text": "Goście usiedli przy <w>wyspie</w> z kieliszkami wina, obserwując gotowanie.",
                            "target_lang_translations": {
                                "ru": "Гости сели за <w>островом</w> с бокалами вина, наблюдая за готовкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kuchnia z wyspą",
                        "wyspa kuchenna",
                        "siedzieć przy wyspie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельно стоящий элемент кухонной мебели, не примыкающий к стенам и обеспечивающий дополнительную рабочую поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кухонный остров",
                                "target_lang_sense_clarification": "мебель"
                            },
                            {
                                "target_lang_word": "остров",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3666af85-a488-3c8b-a6dd-69f1e4c37472",
                    "sense_definition": "Miejsce lub obszar wyraźnie odróżniający się charakterem od swojego otoczenia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten park to prawdziwa <w>wyspa</w> zieleni w centrum betonowego miasta.",
                            "target_lang_translations": {
                                "ru": "Этот парк — настоящий <w>островок</w> зелени в центре бетонного города."
                            }
                        },
                        {
                            "text": "Zjawisko miejskiej <w>wyspy</w> ciepła wpływa na lokalny klimat.",
                            "target_lang_translations": {
                                "ru": "Явление городского <w>острова</w> тепла влияет на местный климат."
                            }
                        }
                    ],
                    "synonyms": [
                        "enklawa"
                    ],
                    "common_phrases": [
                        "wyspa zieleni",
                        "wyspa ciepła",
                        "wyspa spokoju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или область, заметно отличающаяся по характеру от своего окружения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "островок",
                                "target_lang_sense_clarification": "переносное значение (напр. зелени, спокойствия)"
                            },
                            {
                                "target_lang_word": "остров",
                                "target_lang_sense_clarification": "переносное значение (напр. тепла)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "464bf308-5a94-3a71-9cc1-d4ee6489e21b",
                    "sense_definition": "Struktura anatomiczna w ludzkim mózgu, część kory ukryta w głębi bruzdy bocznej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uszkodzenie <w>wyspy</w> może prowadzić do zaburzeń w odczuwaniu emocji.",
                            "target_lang_translations": {
                                "ru": "Повреждение <w>островка</w> может привести к нарушениям в восприятии эмоций."
                            }
                        },
                        {
                            "text": "Badania obrazowe wykazały niewielkie zmiany w obrębie lewej <w>wyspy</w>.",
                            "target_lang_translations": {
                                "ru": "Визуализирующие исследования показали небольшие изменения в области левого <w>островка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kora wyspy",
                        "płat wyspy"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "medycyna, anatomia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Анатомическая структура в человеческом мозге, часть коры, скрытая в глубине боковой борозды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "островок",
                                "target_lang_sense_clarification": "анатомия"
                            },
                            {
                                "target_lang_word": "островковая доля",
                                "target_lang_sense_clarification": "медицина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b202908-e8ee-3e4d-a84d-8f4af678f1cb",
                    "sense_definition": "Ospa wietrzna (choroba zakaźna).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tamtym regionie starsi ludzie wciąż nazywają tę chorobę <w>wyspą</w>.",
                            "target_lang_translations": {
                                "ru": "В том регионе пожилые люди до сих пор называют эту болезнь <w>ветрянкой</w>."
                            }
                        },
                        {
                            "text": "Dziecko zachorowało na <w>wyspę</w> i musiało zostać w domu.",
                            "target_lang_translations": {
                                "ru": "Ребенок заболел <w>ветрянкой</w> и должен был остаться дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Bukowina) osypka"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "gwara bukowińska"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ветряная оспа (инфекционное заболевание)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветрянка",
                                "target_lang_sense_clarification": "разговорное название болезни"
                            },
                            {
                                "target_lang_word": "ветряная оспа",
                                "target_lang_sense_clarification": "медицинское название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
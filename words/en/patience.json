            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5f9de87f-c539-3635-a770-b10b6208f49e",
                    "sense_definition": "The quality of being patient; the ability to accept delay, trouble, or suffering without getting angry or upset.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Teaching young children requires a great deal of <w>patience</w>.",
                            "target_lang_translations": {
                                "ru": "Обучение маленьких детей требует огромного <w>терпения</w>."
                            }
                        },
                        {
                            "text": "I am beginning to lose <w>patience</w> with his constant excuses.",
                            "target_lang_translations": {
                                "ru": "Я начинаю терять <w>терпение</w> из-за его постоянных оправданий."
                            }
                        }
                    ],
                    "synonyms": [
                        "forbearance",
                        "restraint"
                    ],
                    "antonyms": [
                        "impatience"
                    ],
                    "common_phrases": [
                        "have patience",
                        "lose patience",
                        "run out of patience",
                        "test someone's patience"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность сохранять спокойствие и выдержку в ожидании чего-либо или в трудных ситуациях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терпение",
                                "target_lang_sense_clarification": "способность терпеть"

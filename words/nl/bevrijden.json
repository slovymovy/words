{
    "word_family": [
        "bevrijding",
        "bevrijder"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0203f4c6-c425-3eda-a9a8-82844e0004d4",
                    "sense_definition": "Iemand of een bevolking uit gevangenschap, slavernij of onderdrukking vrijlaten",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "liberation_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten *bevrijdden* de stad na een lange en zware strijd.",
                            "target_lang_translations": {
                                "en": "The soldiers <w>liberated</w> the city after a long and hard battle.",
                                "ru": "Солдаты <w>освободили</w> город после долгой и тяжелой битвы."
                            }
                        },
                        {
                            "text": "Het leger wist de gijzelaars veilig uit het gebouw te *bevrijden*.",
                            "target_lang_translations": {
                                "en": "The army managed to safely <w>free</w> the hostages from the building.",
                                "ru": "Армии удалось благополучно <w>освободить</w> заложников из здания."
                            }
                        }
                    ],
                    "synonyms": [
                        "vrijlaten",
                        "verlossen",
                        "vrijmaken"
                    ],
                    "antonyms": [
                        "gevangennemen",
                        "opsluiten",
                        "bezetten",
                        "onderdrukken"
                    ],
                    "common_phrases": [
                        "het land bevrijden",
                        "gevangenen bevrijden",
                        "de stad bevrijden"
                    ],
                    "target_lang_definitions": {
                        "en": "To release someone or a population from captivity, slavery, or oppression",
                        "ru": "Освобождать кого-либо или население из плена, рабства или от угнетения"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "liberate",
                                "target_lang_sense_clarification": "to set free from oppression, occupation, or slavery"
                            },
                            {
                                "target_lang_word": "free",
                                "target_lang_sense_clarification": "general term for releasing someone from confinement"
                            },
                            {
                                "target_lang_word": "set free",
                                "target_lang_sense_clarification": "to release from captivity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "освобождать",
                                "target_lang_sense_clarification": "давать свободу, спасать из неволи"
                            },
                            {
                                "target_lang_word": "вызволять",
                                "target_lang_sense_clarification": "выручать из беды или плена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e19b211a-71e1-3240-ac62-6596f295984b",
                    "sense_definition": "Iemand of iets losmaken uit een moeilijke situatie, fysieke greep of belemmering",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "liberation_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij probeerde zich met alle macht te *bevrijden* uit de stevige greep.",
                            "target_lang_translations": {
                                "en": "He tried with all his might to <w>free</w> himself from the tight grip.",
                                "ru": "Он изо всех сил пытался <w>освободиться</w> из крепкой хватки."
                            }
                        },
                        {
                            "text": "Deze nieuwe uitvinding kan ons *bevrijden* van veel saai en zwaar werk.",
                            "target_lang_translations": {
                                "en": "This new invention can <w>liberate</w> us from much boring and heavy work.",
                                "ru": "Это новое изобретение может <w>избавить</w> нас от множества скучной и тяжелой работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "losmaken",
                        "redden",
                        "ontdoen"
                    ],
                    "antonyms": [
                        "vastgrijpen",
                        "beknellen",
                        "belemmeren"
                    ],
                    "common_phrases": [
                        "zich bevrijden uit",
                        "bevrijden van"
                    ],
                    "target_lang_definitions": {
                        "en": "To release someone or something from a difficult situation, physical grip, or obstruction",
                        "ru": "Освобождать или избавлять кого-то от трудной ситуации, физического захвата или препятствия"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "free",
                                "target_lang_sense_clarification": "to release from a constraint, grip, or burden"
                            },
                            {
                                "target_lang_word": "break free",
                                "target_lang_sense_clarification": "to escape from a physical hold or restriction"
                            },
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "to let go or unfasten"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "избавлять",
                                "target_lang_sense_clarification": "спасать от чего-либо неприятного или тяжкого"
                            },
                            {
                                "target_lang_word": "высвобождать",
                                "target_lang_sense_clarification": "выпускать из зажатого состояния"
                            },
                            {
                                "target_lang_word": "освобождаться",
                                "target_lang_sense_clarification": "вырываться на свободу (возвратный глагол)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
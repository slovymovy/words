{
    "word_family": [
        "spotkanie",
        "napotykać",
        "napotkanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e522cbad-2f02-3f57-8307-b5ab2359a90d",
                    "sense_definition": "Trafiać na kogoś lub coś przypadkiem lub celowo w określonym miejscu; widzieć kogoś w przelocie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano <w>spotykam</w> moją sąsiadkę, gdy wyprowadzam psa na spacer.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <w>встречаю</w> свою соседку, когда выгуливаю собаку."
                            }
                        },
                        {
                            "text": "W tym lesie można często <w>spotkać</w> dzikie zwierzęta, takie jak sarny czy lisy.",
                            "target_lang_translations": {
                                "ru": "В этом лесу часто можно <w>встретить</w> диких животных, таких как косули или лисы."
                            }
                        }
                    ],
                    "synonyms": [
                        "widywać",
                        "znajdować"
                    ],
                    "antonyms": [
                        "mijać"
                    ],
                    "common_phrases": [
                        "spotykać znajomych",
                        "często kogoś spotykać",
                        "spotykać ludzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случайно или намеренно находить кого-то или что-то в определенном месте; видеть кого-то мимоходом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречать",
                                "target_lang_sense_clarification": "обнаруживать или видеть кого-то"
                            },
                            {
                                "target_lang_word": "попадаться",
                                "target_lang_sense_clarification": "встречаться на пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5413bf2-66fc-3695-94e7-48dd8ace6fed",
                    "sense_definition": "Spędzać z kimś czas w umówionym miejscu i terminie w celach towarzyskich lub zawodowych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "social_meeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdy piątek wieczorem <w>spotykamy</w> się z przyjaciółmi w naszej ulubionej kawiarni.",
                            "target_lang_translations": {
                                "ru": "Каждую пятницу вечером мы <w>встречаемся</w> с друзьями в нашем любимом кафе."
                            }
                        },
                        {
                            "text": "Musimy się koniecznie <w>spotkać</w>, żeby omówić szczegóły tego nowego projektu.",
                            "target_lang_translations": {
                                "ru": "Нам нужно обязательно <w>встретиться</w>, чтобы обсудить детали этого нового проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "widywać"
                    ],
                    "common_phrases": [
                        "spotykać się z przyjaciółmi",
                        "spotykać się na kawę",
                        "spotykać się w pracy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить с кем-то время в условленном месте и в условленное время в социальных или профессиональных целях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречаться",
                                "target_lang_sense_clarification": "собираться вместе"
                            },
                            {
                                "target_lang_word": "видеться",
                                "target_lang_sense_clarification": "проводить время с кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d06f731-c085-3ad0-bf03-2e15e1be756d",
                    "sense_definition": "Utrzymywać z kimś relację romantyczną i regularnie chodzić na randki.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "dating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat zaczął <w>spotykać</w> się z nową koleżanką z pracy w zeszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Мой брат начал <w>встречаться</w> с новой коллегой по работе в прошлом месяце."
                            }
                        },
                        {
                            "text": "Oni <w>spotykają</w> się ze sobą już od roku i planują wspólną przyszłość.",
                            "target_lang_translations": {
                                "ru": "Они <w>встречаются</w> уже год и планируют совместное будущее."
                            }
                        }
                    ],
                    "antonyms": [
                        "rozstawać się"
                    ],
                    "common_phrases": [
                        "spotykać się z dziewczyną",
                        "spotykać się z chłopakiem",
                        "zacząć się spotykać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддерживать с кем-то романтические отношения и регулярно ходить на свидания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречаться",
                                "target_lang_sense_clarification": "состоять в отношениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "954dd18a-bcad-3a21-8c78-50b338a79c0f",
                    "sense_definition": "Doświadczać określonej reakcji, traktowania lub sytuacji; mieć z czymś do czynienia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego odważna propozycja zmian w firmie <w>spotkała</w> się z dużą krytyką ze strony zarządu.",
                            "target_lang_translations": {
                                "ru": "Его смелое предложение изменений в компании <w>столкнулось</w> с большой критикой со стороны правления."
                            }
                        },
                        {
                            "text": "W swojej pracy zawodowej często <w>spotykam</w> się z bardzo trudnymi i skomplikowanymi problemami.",
                            "target_lang_translations": {
                                "ru": "В своей профессиональной деятельности я часто <w>сталкиваюсь</w> с очень трудными и сложными проблемами."
                            }
                        }
                    ],
                    "synonyms": [
                        "stykać się",
                        "znajdować"
                    ],
                    "common_phrases": [
                        "spotykać się z krytyką",
                        "spotykać się z odmową",
                        "spotykać się z uznaniem",
                        "spotykać się z problemami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать определенную реакцию, отношение или ситуацию; иметь с чем-то дело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сталкиваться",
                                "target_lang_sense_clarification": "иметь дело с чем-то (проблемами, критикой)"
                            },
                            {
                                "target_lang_word": "встречать",
                                "target_lang_sense_clarification": "получать реакцию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e48f0648-0c69-3bdb-8f15-68ab7d3d2743",
                    "sense_definition": "Rywalizować z kimś w meczu, grze lub zawodach sportowych.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_competition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W finale tegorocznego turnieju nasza drużyna <w>spotka</w> się z aktualnymi mistrzami świata.",
                            "target_lang_translations": {
                                "ru": "В финале турнира этого года наша команда <w>встретится</w> с действующими чемпионами мира."
                            }
                        },
                        {
                            "text": "Te dwa zespoły piłkarskie <w>spotykają</w> się ze sobą na boisku przynajmniej dwa razy w roku.",
                            "target_lang_translations": {
                                "ru": "Эти две футбольные команды <w>встречаются</w> на поле как минимум два раза в год."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spotykać się w finale",
                        "spotykać się na boisku",
                        "spotykać się w meczu"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "sport"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Соревноваться с кем-то в матче, игре или спортивном состязании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречаться",
                                "target_lang_sense_clarification": "состязаться в спорте"
                            },
                            {
                                "target_lang_word": "сражаться",
                                "target_lang_sense_clarification": "бороться за победу"
                            }
                        ]
                    }
                }
            ]
        }
    ]

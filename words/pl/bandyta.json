                    "sense_id": "7ed125c2-044e-3908-bb40-9cfc20ff9a09",
                    "sense_definition": "Człowiek, który dokonuje napadów rabunkowych, często z użyciem broni, lub dopuszcza się innych ciężkich przestępstw.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "criminal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja otoczyła budynek, w którym ukrywał się poszukiwany <w>bandyta</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция окружила здание, в котором скрывался разыскиваемый <w>бандит</w>."
                            }
                        },
                        {
                            "text": "Zamaskowany <w>bandyta</w> sterroryzował obsługę sklepu i uciekł z pieniędzmi.",
                            "target_lang_translations": {
                                "ru": "<w>Бандит</w> в маске запугал персонал магазина и сбежал с деньгами."
                            }
                        }
                    ],
                    "synonyms": [
                        "bandzior",
                        "zbir",
                        "oprych",
                        "rabuś",
                        "zbój",
                        "rzezimieszek",
                        "grabieżca",
                        "łotr"
                    ],
                    "antonyms": [
                        "policjant",
                        "stróż prawa"
                    ],
                    "common_phrases": [
                        "uzbrojony bandyta",
                        "napad bandytów",
                        "groźny bandyta",
                        "zamaskowany bandyta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который совершает грабежи, часто с применением оружия, или другие тяжкие преступления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бандит",
                                "target_lang_sense_clarification": "член преступной группировки"
                            },
                            {
                                "target_lang_word": "разбойник",
                                "target_lang_sense_clarification": "грабитель"
                            },
                            {
                                "target_lang_word": "преступник",
                                "target_lang_sense_clarification": "нарушитель закона"
                            }

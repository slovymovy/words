    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7978f6d7-7792-31be-8fc4-294337dd2c10",
                    "sense_definition": "Potoczne określenie na stan nieuzasadnionego lęku, niepokoju, obsesji lub urojenia, często wyolbrzymionego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestań panikować, to tylko twoja kolejna <w>schiza</w>.",
                            "target_lang_translations": {
                                "ru": "Перестань паниковать, это просто твоя очередная <w>паранойя</w>."
                            }
                        },
                        {
                            "text": "Mam straszną <w>schizę</w>, że nie wyłączyłem żelazka przed wyjściem.",
                            "target_lang_translations": {
                                "ru": "У меня жуткий <w>загон</w>, что я не выключил утюг перед выходом."
                            }
                        }
                    ],
                    "synonyms": [
                        "paranoja",
                        "obsesja",
                        "lęk",
                        "urojenie"
                    ],
                    "antonyms": [
                        "spokój",
                        "równowaga"
                    ],
                    "common_phrases": [
                        "mieć schizę",
                        "łapać schizę",
                        "wkręcać sobie schizę"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used commonly in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название состояния необоснованного страха, беспокойства, навязчивой идеи или бреда, часто преувеличенного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паранойя",
                                "target_lang_sense_clarification": "навязчивый страх или подозрительность"
                            },
                            {
                                "target_lang_word": "загон",
                                "target_lang_sense_clarification": "навязчивая мысль или беспокойство (сленг)"
                            },
                            {
                                "target_lang_word": "шиза",
                                "target_lang_sense_clarification": "общее сленговое название для странного состояния"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9a9550a-e809-3e2e-8eef-5fefb3945206",
                    "sense_definition": "Dziwna, szokująca lub niezrozumiała sytuacja, rzecz lub zdarzenie, które wywołuje zmieszanie lub strach.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "disturbing_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten film to była niezła <w>schiza</w>, nic z niego nie zrozumiałem.",
                            "target_lang_translations": {
                                "ru": "Этот фильм был полный <w>вынос мозга</w>, я ничего в нем не понял."
                            }
                        },
                        {
                            "text": "Spotkała mnie dzisiaj totalna <w>schiza</w> w autobusie.",
                            "target_lang_translations": {
                                "ru": "Со мной сегодня в автобусе приключилась полная <w>шиза</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "obłęd",
                        "szaleństwo",
                        "kosmos"
                    ],
                    "common_phrases": [
                        "niezła schiza",
                        "totalna schiza",
                        "ciężka schiza"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often used by youth"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Странная, шокирующая или непонятная ситуация, вещь или событие, вызывающее замешательство или страх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вынос мозга",
                                "target_lang_sense_clarification": "что-то крайне непонятное или шокирующее"
                            },
                            {
                                "target_lang_word": "шиза",
                                "target_lang_sense_clarification": "сумасшедшая ситуация"
                            },
                            {
                                "target_lang_word": "бред",
                                "target_lang_sense_clarification": "абсурдная ситуация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af476ca8-e9d5-3237-a55d-8e81b0a9cbca",
                    "sense_definition": "Stan zaburzenia świadomości, halucynacji lub dziwnego zachowania, często wywołany przez substancje psychoaktywne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po tych tabletkach miał straszną <w>schizę</w> i widział dziwne rzeczy.",
                            "target_lang_translations": {
                                "ru": "После этих таблеток он словил жуткую <w>измену</w> и видел странные вещи."
                            }
                        },
                        {
                            "text": "To była tylko chwilowa <w>schiza</w> po narkotykach.",
                            "target_lang_translations": {
                                "ru": "Это был лишь временный <w>глюк</w> после наркотиков."
                            }
                        }
                    ],
                    "synonyms": [
                        "odlot",
                        "jazda",
                        "trip"
                    ],
                    "antonyms": [
                        "trzeźwość"
                    ],
                    "common_phrases": [
                        "mieć schizę po czymś"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние измененного сознания, галлюцинаций или странного поведения, часто вызванное психоактивными веществами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "измена",
                                "target_lang_sense_clarification": "страх или паника под воздействием веществ (сленг)"
                            },
                            {
                                "target_lang_word": "глюк",
                                "target_lang_sense_clarification": "галлюцинация"
                            },
                            {
                                "target_lang_word": "бэд-трип",
                                "target_lang_sense_clarification": "негативный психоделический опыт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04733240-5509-3f41-ae34-900dfd82dabf",
                    "sense_definition": "Potoczne, skrótowe określenie na <w>schizofrenię</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "medical_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarze podejrzewają u niego <w>schizę</w> bezobjawową.",
                            "target_lang_translations": {
                                "ru": "Врачи подозревают у него бессимптомную <w>шизу</w>."
                            }
                        },
                        {
                            "text": "On leczy się na <w>schizę</w> w szpitalu psychiatrycznym.",
                            "target_lang_translations": {
                                "ru": "Он лечится от <w>шизы</w> в психиатрической больнице."
                            }
                        }
                    ],
                    "synonyms": [
                        "schizofrenia"
                    ],
                    "traits": [

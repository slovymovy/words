                {
                    "sense_id": "3b9075f6-474c-3d4c-87d7-497c78f16733",
                    "sense_definition": "A state of matter consisting of partially ionized gas and free electrons, distinct from solid, liquid, and gas.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sun is composed almost entirely of superheated <w>plasma</w>.",
                            "target_lang_translations": {
                                "ru": "Солнце почти полностью состоит из перегретой <w>плазмы</w>."
                            }
                        },
                        {
                            "text": "We decided to buy a large <w>plasma</w> screen for the conference room.",
                            "target_lang_translations": {
                                "ru": "Мы решили купить большой <w>плазменный</w> экран для конференц-зала."
                            }
                        }
                    ],
                    "common_phrases": [

{
    "word_family": [
        "dagdroom",
        "dagdromer",
        "dagdromerij"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dc334970-b0df-3a7b-b23a-146079fc72d4",
                    "sense_definition": "Fantaseren over aangename dingen of de toekomst terwijl men wakker is.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fantaseren",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>dagdroomde</w> over een lange fietsvakantie door Nederland.",
                            "target_lang_translations": {
                                "en": "I <w>daydreamed</w> about a long cycling holiday through the Netherlands.",
                                "ru": "Я <w>мечтал</w> о долгом велопутешествии по Нидерландам."
                            }
                        },
                        {
                            "text": "Tijdens de saaie les zat de leerling te <w>dagdromen</w> over de zomervakantie.",
                            "target_lang_translations": {
                                "en": "During the boring lesson, the student sat <w>daydreaming</w> about the summer holidays.",
                                "ru": "Во время скучного урока ученик <w>мечтал</w> о летних каникулах."
                            }
                        }
                    ],
                    "synonyms": [
                        "mijmeren"
                    ],
                    "common_phrases": [
                        "dagdromen over",
                        "zitten te dagdromen"
                    ],
                    "target_lang_definitions": {
                        "en": "To fantasize about pleasant things or the future while awake.",
                        "ru": "Фантазировать о приятных вещах или будущем, находясь в бодрствующем состоянии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "daydream",
                                "target_lang_sense_clarification": "to have pleasant thoughts while awake"
                            },
                            {
                                "target_lang_word": "fantasize",
                                "target_lang_sense_clarification": "to imagine things"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мечтать наяву",
                                "target_lang_sense_clarification": "фантазировать"
                            },
                            {
                                "target_lang_word": "грезить",
                                "target_lang_sense_clarification": "предаваться грезам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df153740-c3ae-33fe-8d41-42795eeebf8c",
                    "sense_definition": "Zich in een half slapende toestand bevinden en niet opletten.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "afwezig_zijn",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Piet wordt eens wakker! Wat zit je nu weer te <w>dagdromen</w>.",
                            "target_lang_translations": {
                                "en": "Piet, wake up! What are you <w>daydreaming</w> about now?",
                                "ru": "Пит, проснись! Чего ты опять <w>витаешь в облаках</w>."
                            }
                        },
                        {
                            "text": "Ze was zo moe dat ze achter haar bureau zat te <w>dagdromen</w> in plaats van te werken.",
                            "target_lang_translations": {
                                "en": "She was so tired that she sat <w>daydreaming</w> at her desk instead of working.",
                                "ru": "Она так устала, что сидела и <w>грезила</w> за своим столом вместо того, чтобы работать."
                            }
                        }
                    ],
                    "synonyms": [
                        "suffen"
                    ],
                    "common_phrases": [
                        "zitten te dagdromen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in a semi-conscious state and not paying attention.",
                        "ru": "Находиться в полусонном состоянии и не обращать внимания на происходящее."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "zone out",
                                "target_lang_sense_clarification": "to lose focus or stop paying attention"
                            },
                            {
                                "target_lang_word": "daydream",
                                "target_lang_sense_clarification": "to be absent-minded"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "витать в облаках",
                                "target_lang_sense_clarification": "быть рассеянным"
                            },
                            {
                                "target_lang_word": "замечтаться",
                                "target_lang_sense_clarification": "отвлечься от реальности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1df59c5c-bb9a-364c-ae57-d5e47f3baa54",
                    "sense_definition": "Meervoud van <w>dagdroom</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>dagdromen</w> gingen vaak over rijkdom en beroemdheid.",
                            "target_lang_translations": {
                                "en": "His <w>daydreams</w> were often about wealth and fame.",
                                "ru": "Его <w>грезы</w> часто были о богатстве и славе."
                            }
                        },
                        {
                            "text": "Ze verloor zich volledig in haar romantische <w>dagdromen</w>.",
                            "target_lang_translations": {
                                "en": "She lost herself completely in her romantic <w>daydreams</w>.",
                                "ru": "Она полностью погрузилась в свои романтические <w>мечты</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of daydream.",
                        "ru": "Множественное число от слова dagdroom (греза, мечта)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "daydreams",
                                "target_lang_sense_clarification": "plural noun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "грезы",
                                "target_lang_sense_clarification": "фантазии"
                            },
                            {
                                "target_lang_word": "мечты",
                                "target_lang_sense_clarification": "желания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
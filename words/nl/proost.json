    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "1b3916a9-c735-306d-ab32-d3d37e2949c6",
                    "sense_definition": "Een uitroep om elkaar geluk of gezondheid toe te wensen bij het heffen van het glas.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "toost",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hieven onze glazen en riepen luid: \"<w>Proost</w>!\"",
                            "target_lang_translations": {
                                "en": "We raised our glasses and shouted loudly: \"<w>Cheers</w>!\"",
                                "ru": "Мы подняли бокалы и громко крикнули: \"<w>Ваше здоровье</w>!\""
                            }
                        },
                        {
                            "text": "<w>Proost</w>, laten we drinken op een goede afloop van het project.",
                            "target_lang_translations": {
                                "en": "<w>Cheers</w>, let's drink to a successful outcome of the project.",
                                "ru": "<w>За ваше здоровье</w>, давайте выпьем за успешное завершение проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "santé",
                        "gezondheid",
                        "cheers"
                    ],
                    "common_phrases": [
                        "proost op ons",
                        "proost op de jarige"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation to wish each other luck or health when raising a glass.",
                        "ru": "Возглас при поднятии бокалов с пожеланием счастья или здоровья."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cheers",
                                "target_lang_sense_clarification": "standard toast"
                            },
                            {
                                "target_lang_word": "bottoms up",
                                "target_lang_sense_clarification": "informal toast"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ваше здоровье",
                                "target_lang_sense_clarification": "универсальный тост"
                            },
                            {
                                "target_lang_word": "за ваше здоровье",
                                "target_lang_sense_clarification": "тост"
                            },
                            {
                                "target_lang_word": "будем",
                                "target_lang_sense_clarification": "разговорный тост"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1c2305e-f248-3e77-a18c-a47356b4ec02",
                    "sense_definition": "Een reactie wanneer iemand niest (minder gebruikelijk dan 'gezondheid').",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "niezen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen hij plotseling moest niezen, zei zijn buurman: \"<w>Proost</w>!\"",
                            "target_lang_translations": {
                                "en": "When he suddenly had to sneeze, his neighbor said: \"<w>Bless you</w>!\"",
                                "ru": "Когда он внезапно чихнул, его сосед сказал: \"<w>Будьте здоровы</w>!\""
                            }
                        },
                        {
                            "text": "Sommige mensen zeggen <w>proost</w> als iemand niest, maar meestal zegt men gezondheid.",
                            "target_lang_translations": {
                                "en": "Some people say <w>bless you</w> when someone sneezes, but usually people say 'gezondheid'.",
                                "ru": "Некоторые люди говорят \"<w>будь здоров</w>\", когда кто-то чихает, но обычно говорят \"gezondheid\"."
                            }
                        }
                    ],
                    "synonyms": [
                        "gezondheid"
                    ],
                    "target_lang_definitions": {
                        "en": "A reaction when someone sneezes (less common than 'gezondheid').",
                        "ru": "Реакция на чихание (менее распространено, чем «gezondheid»)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bless you",
                                "target_lang_sense_clarification": "response to sneezing"
                            },
                            {
                                "target_lang_word": "gesundheit",
                                "target_lang_sense_clarification": "response to sneezing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "будьте здоровы",
                                "target_lang_sense_clarification": "вежливая форма или множественное число"
                            },
                            {
                                "target_lang_word": "будь здоров",
                                "target_lang_sense_clarification": "неформальное обращение к мужчине"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "wabik",
        "powab",
        "powabny",
        "zwabić",
        "przywabić",
        "wywabić"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "273b9854-b07c-36e3-9f00-fa27cce7d73a",
                    "sense_definition": "Mieć określone imię; używane wyłącznie w odniesieniu do zwierząt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "naming_animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy pies <w>wabi się</w> Reks i jest bardzo przyjazny.",
                            "target_lang_translations": {
                                "ru": "Мою новую собаку <w>зовут</w> Рекс, и она очень дружелюбная."
                            }
                        },
                        {
                            "text": "Nie pamiętam, jak <w>wabi się</w> ten mały kot sąsiadów.",
                            "target_lang_translations": {
                                "ru": "Я не помню, как <w>зовут</w> этого маленького соседского кота."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jak wabi się",
                        "pies wabi się",
                        "kot wabi się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь определенную кличку; используется исключительно по отношению к животным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зваться",
                                "target_lang_sense_clarification": "о животных"
                            },
                            {
                                "target_lang_word": "иметь кличку",
                                "target_lang_sense_clarification": "быть названным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "085beca9-7ff5-3ec2-83fa-63ac54e0efb9",
                    "sense_definition": "Przyciągać kogoś lub coś do siebie, często obiecując przyjemność, korzyść lub używając podstępu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attracting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Intensywny zapach kwiatów <w>wabi</w> pszczoły i kolorowe motyle do ogrodu.",
                            "target_lang_translations": {
                                "ru": "Интенсивный запах цветов <w>манит</w> пчел и разноцветных бабочек в сад."
                            }
                        },
                        {
                            "text": "Sklep <w>wabi</w> klientów wielkimi wyprzedażami i atrakcyjnymi wystawami.",
                            "target_lang_translations": {
                                "ru": "Магазин <w>завлекает</w> клиентов большими распродажами и красивыми витринами."
                            }
                        }
                    ],
                    "synonyms": [
                        "kusić",
                        "nęcić",
                        "przynęcać przyzwyczajać",
                        "pociągać",
                        "zwabiać"
                    ],
                    "antonyms": [
                        "odstraszać",
                        "zniechęcać"
                    ],
                    "common_phrases": [
                        "wabić klientów",
                        "wabić zapachem",
                        "wabić ofiarę",
                        "wabić spojrzeniem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Притягивать кого-то или что-то к себе, часто обещая удовольствие, выгоду или используя уловки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манить",
                                "target_lang_sense_clarification": "притягивать"
                            },
                            {
                                "target_lang_word": "завлекать",
                                "target_lang_sense_clarification": "соблазнять, привлекать хитростью"
                            },
                            {
                                "target_lang_word": "привлекать",
                                "target_lang_sense_clarification": "вызывать интерес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f7fc511-1e34-3ba6-9144-f204f3311cc9",
                    "sense_definition": "Wydawać specyficzne dźwięki w celu przywołania zwierząt, często podczas polowania lub obserwacji przyrody.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hunting_calls",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony myśliwy potrafi skutecznie <w>wabić</w> jelenie podczas rykowiska.",
                            "target_lang_translations": {
                                "ru": "Опытный охотник умеет эффективно <w>приманивать</w> оленей во время гона."
                            }
                        },
                        {
                            "text": "Nauczył się <w>wabić</w> ptaki, naśladując ich śpiew, aby zrobić im lepsze zdjęcia.",
                            "target_lang_translations": {
                                "ru": "Он научился <w>подманивать</w> птиц, подражая их пению, чтобы сделать лучшие фотографии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wabić zwierzynę",
                        "wabić jelenie",
                        "skutecznie wabić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать специфические звуки, чтобы подозвать животных, часто во время охоты или наблюдения за природой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приманивать",
                                "target_lang_sense_clarification": "подзывать животных"
                            },
                            {
                                "target_lang_word": "манить",
                                "target_lang_sense_clarification": "использовать манок"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "28bb8458-6ad3-3142-8674-876cf5660b80",
                    "sense_definition": "Człowiek nieposiadający kompetencji ani wykształcenia, który dzięki zbiegowi okoliczności lub sprytowi zdobył wysokie stanowisko i uznanie społeczne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "types_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niedługo wszyscy poznają się na tym <w>dyzmie</w> i straci on swoje stanowisko.",
                            "target_lang_translations": {
                                "ru": "Скоро все раскусят этого <w>проходимца</w>, и он потеряет свою должность."
                            }
                        },
                        {
                            "text": "W świecie polityki niestety często można spotkać niejednego <w>dyzmę</w>.",
                            "target_lang_translations": {
                                "ru": "В мире политики, к сожалению, часто можно встретить не одного такого <w>выскочку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "karierowicz",
                        "parweniusz",
                        "uzurpator"
                    ],
                    "antonyms": [
                        "fachowiec",
                        "profesjonalista"
                    ],
                    "common_phrases": [
                        "współczesny dyzma",
                        "lokalny dyzma"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек без компетенций и образования, который благодаря стечению обстоятельств или хитрости добился высокого положения и общественного признания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проходимец",
                                "target_lang_sense_clarification": "человек, добивающийся целей нечестным путем"
                            },
                            {
                                "target_lang_word": "выскочка",
                                "target_lang_sense_clarification": "человек, быстро поднявшийся в обществе без достаточных заслуг"
                            },
                            {
                                "target_lang_word": "самозванец",
                                "target_lang_sense_clarification": "человек, незаконно присвоивший себе чужие права или звание"
                            }
                        ]
                    }
                }
            ]
        },

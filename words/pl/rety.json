    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "039ddab6-1dc0-3bbc-8bea-61e8111f5e5c",
                    "sense_definition": "Wyraża silne emocje, takie jak zdziwienie, zaskoczenie, przestrach lub oburzenie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "O <w>rety</w>, zapomniałem kupić prezent na urodziny mamy.",
                            "target_lang_translations": {
                                "ru": "О <w>боже</w>, я забыл купить подарок на мамин день рождения."
                            }
                        },
                        {
                            "text": "<w>Rety</w>, ale mnie wystraszyłeś, wchodząc tak cicho do pokoju!",
                            "target_lang_translations": {
                                "ru": "<w>Мамочки</w>, как ты меня напугал, войдя в комнату так тихо!"
                            }
                        }
                    ],
                    "synonyms": [
                        "ojej",
                        "rany",
                        "olaboga"
                    ],
                    "common_phrases": [
                        "o rety"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает сильные эмоции, такие как удивление, испуг или возмущение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о боже",
                                "target_lang_sense_clarification": "выражение удивления или досады"
                            },
                            {
                                "target_lang_word": "мамочки",
                                "target_lang_sense_clarification": "выражение испуга"
                            },
                            {
                                "target_lang_word": "батюшки",
                                "target_lang_sense_clarification": "выражение крайнего удивления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5d2a22b-81ea-3162-a1ed-5eddf953ff28",
                    "sense_definition": "Wołanie o pomoc w sytuacji zagrożenia; synonim słowa <w>ratunku</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krzyczał w niebogłosy: <w>rety</w>, ludzie, ratujcie nas!",
                            "target_lang_translations": {
                                "ru": "Он кричал во всё горло: <w>караул</w>, люди, спасайте нас!"
                            }
                        },
                        {
                            "text": "<w>Rety</w>, tonę, niech ktoś mi szybko poda rękę!",
                            "target_lang_translations": {
                                "ru": "<w>Спасите</w>, я тону, пусть кто-нибудь быстро подаст мне руку!"
                            }
                        }
                    ],
                    "synonyms": [
                        "ratunku"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крик о помощи в опасной ситуации; синоним слова «спасите»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "караул",
                                "target_lang_sense_clarification": "призыв о помощи"
                            },
                            {
                                "target_lang_word": "спасите",
                                "target_lang_sense_clarification": "просьба о спасении"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2434b442-ff9d-36b6-af20-b24d27a72abf",
                    "sense_definition": "Forma fleksyjna słowa <w>ret</w> (liczba mnoga w mianowniku, bierniku i wołaczu).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym zakładzie włókienniczym odbywają się <w>rety</w> lnu i konopi.",
                            "target_lang_translations": {
                                "ru": "На этом текстильном заводе производится <w>мочка</w> льна и конопли."
                            }
                        },
                        {
                            "text": "Wszystkie <w>rety</w> muszą być przeprowadzone w odpowiedniej temperaturze wody.",
                            "target_lang_translations": {
                                "ru": "Вся <w>мочка</w> должна проводиться при соответствующей температуре воды."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма слова «ret» (процесс вымачивания волокнистых растений)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мочка",
                                "target_lang_sense_clarification": "процесс обработки льна или конопли"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
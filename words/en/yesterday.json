    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "53ef4902-a542-3a8b-b087-6a106f383f36",
                    "sense_definition": "On the day before today.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_relative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I went to the library <w>yesterday</w> to return a book.",
                            "target_lang_translations": {
                                "ru": "Я ходил в библиотеку <w>вчера</w>, чтобы вернуть книгу."
                            }
                        },
                        {
                            "text": "She arrived <w>yesterday</w> morning and was very tired.",
                            "target_lang_translations": {
                                "ru": "Она приехала <w>вчера</w> утром и была очень уставшей."
                            }
                        }
                    ],
                    "antonyms": [
                        "tomorrow"
                    ],
                    "common_phrases": [
                        "yesterday morning",
                        "yesterday afternoon",
                        "yesterday evening"
                    ],
                    "target_lang_definitions": {
                        "ru": "В день, предшествующий сегодняшнему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вчера",
                                "target_lang_sense_clarification": "наречие времени"
                            },
                            {
                                "target_lang_word": "накануне",
                                "target_lang_sense_clarification": "в предыдущий день"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb1984ff-5b7b-3ec7-a785-f7987283cf83",
                    "sense_definition": "As soon as possible; immediately (used hyperbolically to express extreme urgency).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boss is angry and wants this report finished <w>yesterday</w>.",
                            "target_lang_translations": {
                                "ru": "Начальник злится и хочет, чтобы этот отчет был готов <w>вчера</w>."
                            }
                        },
                        {
                            "text": "We are late with the project, so I need those results <w>yesterday</w>.",
                            "target_lang_translations": {
                                "ru": "Мы опаздываем с проектом, поэтому эти результаты нужны мне <w>вчера</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "the last day"
                    ],
                    "common_phrases": [
                        "want it yesterday",
                        "need it yesterday"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to exaggerate urgency"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Как можно скорее; немедленно (используется гиперболически для выражения крайней срочности)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вчера",
                                "target_lang_sense_clarification": "в ироничном значении 'очень срочно'"
                            },
                            {
                                "target_lang_word": "ещё вчера",
                                "target_lang_sense_clarification": "фразеологизм"
                            }
                        ]
                    }
                }
            ]
        },
        {

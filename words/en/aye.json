                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Aye</w> arrived late to the conference.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Ай</w> прибыл на конференцию с опозданием."
                            }

                },
                {
                    "sense_id": "2bf04d29-3c25-3b61-8082-a2e9c86c7a32",
                    "sense_definition": "Przedmioty, takie jak sznury czy łańcuchy, służące do krępowania kogoś i ograniczania ruchów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

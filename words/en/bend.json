{
    "word_family": [
        "bender",
        "bendy",
        "bendable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5892486f-d08e-3fca-ae2d-0f8743498fd4",
                    "sense_definition": "To cause something to change its shape into a curve using force.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you <w>bend</w> the pipe too far, it will break.",
                            "target_lang_translations": {
                                "ru": "Если ты слишком сильно <w>согнёшь</w> трубу, она сломается."
                            }
                        },
                        {
                            "text": "Please <w>bend</w> your knees slightly when you lift the box.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, слегка <w>согните</w> колени, когда поднимаете коробку."
                            }
                        }
                    ],
                    "synonyms": [
                        "curve",
                        "curl",
                        "flex"
                    ],
                    "antonyms": [
                        "straighten",
                        "unbend"
                    ],
                    "common_phrases": [
                        "bend the wire",
                        "bend metal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить что-то изменить форму на изогнутую с помощью силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сгибать",
                                "target_lang_sense_clarification": "придавать дугообразную форму"
                            },
                            {
                                "target_lang_word": "гнуть",
                                "target_lang_sense_clarification": "деформировать, искривлять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc24bec5-9a64-39e1-9fb6-5bb6ef0d2547",
                    "sense_definition": "To move the top part of the body downwards and forwards.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>bent</w> down to pick up the pieces of glass.",
                            "target_lang_translations": {
                                "ru": "Он <w>наклонился</w>, чтобы подобрать осколки стекла."
                            }
                        },
                        {
                            "text": "She had to <w>bend</w> over to tie her shoes.",
                            "target_lang_translations": {
                                "ru": "Ей пришлось <w>нагнуться</w>, чтобы завязать шнурки."
                            }
                        }
                    ],
                    "synonyms": [
                        "bow"
                    ],
                    "antonyms": [
                        "straighten"
                    ],
                    "common_phrases": [
                        "bend down",
                        "bend over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опускать верхнюю часть тела вниз и вперед."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наклоняться",
                                "target_lang_sense_clarification": "опускать корпус тела"
                            },
                            {
                                "target_lang_word": "нагибаться",
                                "target_lang_sense_clarification": "склоняться к земле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bef37df7-38e2-31ce-8ef3-45a4ed47d635",
                    "sense_definition": "To become curved or lose a straight shape.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Look at the trees <w>bending</w> in the strong wind.",
                            "target_lang_translations": {
                                "ru": "Посмотри на деревья, <w>гнущиеся</w> на сильном ветру."
                            }
                        },
                        {
                            "text": "The plastic spoon <w>bent</w> in the hot water.",
                            "target_lang_translations": {
                                "ru": "Пластиковая ложка <w>согнулась</w> в горячей воде."
                            }
                        }
                    ],
                    "synonyms": [
                        "curve",
                        "curl"
                    ],
                    "antonyms": [
                        "straighten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться кривым или терять прямую форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнуться",
                                "target_lang_sense_clarification": "поддаваться давлению, искривляться"
                            },
                            {
                                "target_lang_word": "сгибаться",
                                "target_lang_sense_clarification": "принимать изогнутую форму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acb7b34b-fb75-3be5-ae0a-59fa261f19f2",
                    "sense_definition": "To change direction or follow a curve (of a road, river, etc.).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The road <w>bends</w> to the right after the bridge.",
                            "target_lang_translations": {
                                "ru": "Дорога <w>поворачивает</w> направо за мостом."
                            }
                        },
                        {
                            "text": "The river <w>bends</w> sharply near the old mill.",
                            "target_lang_translations": {
                                "ru": "Река резко <w>изгибается</w> возле старой мельницы."
                            }
                        }
                    ],
                    "synonyms": [
                        "curve"
                    ],
                    "antonyms": [
                        "straighten"
                    ],
                    "common_phrases": [
                        "road bends",
                        "river bends"
                    ],
                    "target_lang_definitions": {
                        "ru": "Менять направление или следовать изгибу (о дороге, реке и т. д.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поворачивать",
                                "target_lang_sense_clarification": "менять направление"
                            },
                            {
                                "target_lang_word": "изгибаться",
                                "target_lang_sense_clarification": "иметь форму дуги (о дороге/реке)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1069f86f-5d69-386d-ab49-97b37e5bfc78",
                    "sense_definition": "To force someone to submit or to interpret rules flexibly.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They tried to <w>bend</w> me to their will, but I refused.",
                            "target_lang_translations": {
                                "ru": "Они пытались <w>подчинить</w> меня своей воле, но я отказался."
                            }
                        },
                        {
                            "text": "Sometimes you have to <w>bend</w> the rules to get things done.",
                            "target_lang_translations": {
                                "ru": "Иногда приходится <w>обходить</w> правила, чтобы добиться своего."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bend the rules",
                        "bend to someone's will"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-то подчиниться или гибко толковать правила."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подчинять",
                                "target_lang_sense_clarification": "заставлять покориться воле"
                            },
                            {
                                "target_lang_word": "обходить",
                                "target_lang_sense_clarification": "гибко толковать (правила)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11fc0aee-32ff-3732-bc60-6f345c833fb2",
                    "sense_definition": "To smoothly change the pitch of a musical note.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>bend</w> the note slightly for a bluesy effect.",
                            "target_lang_translations": {
                                "ru": "Тебе следует слегка <w>подтянуть</w> ноту для блюзового эффекта."
                            }
                        },
                        {
                            "text": "Guitarists often <w>bend</w> strings to create a crying sound.",
                            "target_lang_translations": {
                                "ru": "Гитаристы часто <w>подтягивают</w> струны, чтобы создать плачущий звук."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bend a note",
                        "bend the string"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плавно изменять высоту музыкальной ноты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подтягивать",
                                "target_lang_sense_clarification": "струну для изменения тона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "887ab796-a55e-366d-b0e6-91882952be00",
                    "sense_definition": "To apply one's energy or attention to a task.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>bent</w> the company's resources to gaining market share.",
                            "target_lang_translations": {
                                "ru": "Он <w>направил</w> ресурсы компании на завоевание доли рынка."
                            }
                        },
                        {
                            "text": "She <w>bent</w> her mind to solving the complex equation.",
                            "target_lang_translations": {
                                "ru": "Она <w>сосредоточила</w> свой ум на решении сложного уравнения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bend one's mind",
                        "bend attention"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять свою энергию или внимание на выполнение задачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "направлять",
                                "target_lang_sense_clarification": "усилия или ресурсы"
                            },
                            {
                                "target_lang_word": "сосредоточивать",
                                "target_lang_sense_clarification": "внимание или ум"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76c96e28-901f-3f02-ae39-fda47ca39eee",
                    "sense_definition": "To tie or fasten a rope to an object.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "6",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Bend</w> the sail to the yard before we set off.",
                            "target_lang_translations": {
                                "ru": "<w>Привяжи</w> парус к рею, прежде чем мы отплывем."
                            }
                        },
                        {
                            "text": "Sailors must learn how to <w>bend</w> a line to an anchor.",
                            "target_lang_translations": {
                                "ru": "Моряки должны учиться, как <w>крепить</w> трос к якорю."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Привязывать или крепить веревку к объекту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привязывать",
                                "target_lang_sense_clarification": "крепить узел или трос"
                            },
                            {
                                "target_lang_word": "крепить",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9eaf211b-8a40-3c93-8ab0-d4afdfe6748e",
                    "sense_definition": "A curved part of something, especially a road or river.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a sharp <w>bend</w> in the road ahead.",
                            "target_lang_translations": {
                                "ru": "Впереди на дороге крутой <w>поворот</w>."
                            }
                        },
                        {
                            "text": "The car disappeared around the <w>bend</w>.",
                            "target_lang_translations": {
                                "ru": "Машина скрылась за <w>поворотом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "curve"
                    ],
                    "common_phrases": [
                        "sharp bend",
                        "round the bend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изогнутая часть чего-либо, особенно дороги или реки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поворот",
                                "target_lang_sense_clarification": "изменение направления дороги"
                            },
                            {
                                "target_lang_word": "изгиб",
                                "target_lang_sense_clarification": "кривизна линии или русла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25c72344-7f2c-3c7a-b79f-b38f8c0ca074",
                    "sense_definition": "A severe condition caused by ascending too quickly from deep water; decompression sickness.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "7",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Divers ascend slowly to avoid getting the <w>bends</w>.",
                            "target_lang_translations": {
                                "ru": "Дайверы всплывают медленно, чтобы избежать <w>кессонной болезни</w>."
                            }
                        },
                        {
                            "text": "He suffered from the <w>bends</w> after surfacing too fast.",
                            "target_lang_translations": {
                                "ru": "Он пострадал от <w>кессонной болезни</w> после слишком быстрого всплытия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the bends"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common name for decompression sickness"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжелое состояние, вызванное слишком быстрым подъемом с глубины; кессонная болезнь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кессонная болезнь",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2467690-b97a-3671-8add-d53667db9138",
                    "sense_definition": "A type of knot used to join two ropes.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "6",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Use a sheet <w>bend</w> to tie these two ropes together.",
                            "target_lang_translations": {
                                "ru": "Используй шкотовый <w>узел</w>, чтобы связать эти две веревки вместе."
                            }
                        },
                        {
                            "text": "The fisherman taught me a new <w>bend</w> for the line.",
                            "target_lang_translations": {
                                "ru": "Рыбак научил меня новому <w>узлу</w> для лески."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sheet bend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип узла, используемый для соединения двух веревок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узел",
                                "target_lang_sense_clarification": "способ соединения веревок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ec99b3d-f5c6-32f8-b3ff-90e63fc478cc",
                    "sense_definition": "A diagonal band across a heraldic shield.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "8",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knight's shield displayed a golden <w>bend</w>.",
                            "target_lang_translations": {
                                "ru": "На щите рыцаря была изображена золотая <w>перевязь</w>."
                            }
                        },
                        {
                            "text": "A <w>bend</w> sinister is often associated with illegitimacy in heraldry.",
                            "target_lang_translations": {
                                "ru": "<w>Перевязь</w> влево в геральдике часто ассоциируется с незаконнорожденностью."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Диагональная полоса на геральдическом щите."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевязь",
                                "target_lang_sense_clarification": "геральдический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "5c9b1f63-b282-3599-97c1-b54f9934baff",
                    "sense_definition": "A city in Oregon, United States.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We are planning a trip to <w>Bend</w> this winter.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку в <w>Бенд</w> этой зимой."
                            }
                        },
                        {
                            "text": "<w>Bend</w> is known for its outdoor recreational activities.",
                            "target_lang_translations": {
                                "ru": "<w>Бенд</w> известен своими возможностями для активного отдыха."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в штате Орегон, США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бенд",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b00c50f-a9af-3695-a4b9-7250f3ce6a56",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Bend</w> arrived late for the meeting.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Бенд</w> опоздал на встречу."
                            }
                        },
                        {
                            "text": "I have never met anyone with the surname <w>Bend</w>.",
                            "target_lang_translations": {
                                "ru": "Я никогда не встречал никого с фамилией <w>Бенд</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бенд",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
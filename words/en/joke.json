{
    "word_family": [
        "joker",
        "jokingly",
        "jokey"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "360587ac-72a1-3d01-99f0-829f3531df37",
                    "sense_definition": "An amusing story or anecdote told to cause laughter.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He told a funny <w>joke</w> about a dog.",
                            "target_lang_translations": {
                                "ru": "Он рассказал смешной <w>анекдот</w> про собаку."
                            }
                        },
                        {
                            "text": "We all laughed at his witty <w>joke</w>.",
                            "target_lang_translations": {
                                "ru": "Мы все смеялись над его остроумной <w>шуткой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tell a joke",
                        "funny joke",
                        "hear a joke"
                    ],
                    "target_lang_definitions": {
                        "ru": "Забавная история или анекдот, рассказанные, чтобы вызвать смех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анекдот",
                                "target_lang_sense_clarification": "короткая смешная история"
                            },
                            {
                                "target_lang_word": "шутка",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "острота",
                                "target_lang_sense_clarification": "остроумное высказывание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38127af2-0c75-3088-9789-ec5118ecf93b",
                    "sense_definition": "Something said or done for amusement rather than seriously.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I hid his shoes as a <w>joke</w>.",
                            "target_lang_translations": {
                                "ru": "Я спрятал его ботинки в <w>шутку</w>."
                            }
                        },
                        {
                            "text": "Don't be angry, it was just a <w>joke</w>.",
                            "target_lang_translations": {
                                "ru": "Не злись, это была всего лишь <w>шутка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "as a joke",
                        "practical joke",
                        "just a joke"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то сказанное или сделанное ради забавы, а не всерьез."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шутка",
                                "target_lang_sense_clarification": "розыгрыш или несерьезное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efc79c99-a0ba-3b16-9e03-2e329bc2f74c",
                    "sense_definition": "A person or thing considered laughably inadequate, worthless, or a sham.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new management system is a total <w>joke</w>.",
                            "target_lang_translations": {
                                "ru": "Новая система управления — это полное <w>посмешище</w>."
                            }
                        },
                        {
                            "text": "His attempt at cooking dinner was a <w>joke</w>.",
                            "target_lang_translations": {
                                "ru": "Его попытка приготовить ужин была просто <w>смех</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "is a joke",
                        "total joke"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to criticize quality or competence"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или предмет, считающиеся смехотворно неадекватными, никчемными или фикцией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посмешище",
                                "target_lang_sense_clarification": "объект насмешек"
                            },
                            {
                                "target_lang_word": "клоун",
                                "target_lang_sense_clarification": "о человеке (презрительно)"
                            },
                            {
                                "target_lang_word": "цирк",
                                "target_lang_sense_clarification": "о ситуации (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "418d9564-cbc5-324e-8a6f-bdedab97cd8c",
                    "sense_definition": "Something that is far easier or less challenging than expected.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The final exam was a <w>joke</w>; I finished it in twenty minutes.",
                            "target_lang_translations": {
                                "ru": "Итоговый экзамен был <w>ерундой</w>; я закончил его за двадцать минут."
                            }
                        },
                        {
                            "text": "Getting tickets for the show was a <w>joke</w>.",
                            "target_lang_translations": {
                                "ru": "Достать билеты на шоу было <w>парой пустяков</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "absolute joke"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies lack of difficulty"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то, что намного легче или менее сложно, чем ожидалось."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ерунда",
                                "target_lang_sense_clarification": "пустяковое дело"
                            },
                            {
                                "target_lang_word": "пустяк",
                                "target_lang_sense_clarification": "очень легкое задание"
                            },
                            {
                                "target_lang_word": "смех",
                                "target_lang_sense_clarification": "разг. что-то несерьезное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d083d80-173f-3624-bbf4-3ba3a0958ec2",
                    "sense_definition": "To say or do something for amusement rather than seriously.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was only <w>joking</w> when I said that.",
                            "target_lang_translations": {
                                "ru": "Я просто <w>шутил</w>, когда сказал это."
                            }
                        },
                        {
                            "text": "She is always <w>joking</w> about moving to Mars.",
                            "target_lang_translations": {
                                "ru": "Она всегда <w>шутит</w> насчет переезда на Марс."
                            }
                        }
                    ],
                    "common_phrases": [
                        "only joking",
                        "must be joking",
                        "joking apart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить или делать что-то ради забавы, а не всерьез."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шутить",
                                "target_lang_sense_clarification": "говорить не всерьез"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1267649-ca2d-350f-8c97-148f4bfe4b38",
                    "sense_definition": "To tease or trick someone in a friendly manner.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Relax, I am just <w>joking</w> with you.",
                            "target_lang_translations": {
                                "ru": "Расслабься, я просто <w>шучу</w> над тобой."
                            }
                        },
                        {
                            "text": "He loves to <w>joke</w> with his little brother.",
                            "target_lang_translations": {
                                "ru": "Он любит <w>подшучивать</w> над своим младшим братом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "joke with",
                        "joke around"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дразнить или разыгрывать кого-то в дружеской манере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подшучивать",
                                "target_lang_sense_clarification": "добродушно дразнить"
                            },
                            {
                                "target_lang_word": "разыгрывать",
                                "target_lang_sense_clarification": "устраивать розыгрыш"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
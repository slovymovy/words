                },
                {
                    "sense_id": "5f969a02-ddca-37e4-b903-e6771c91f422",
                    "sense_definition": "duży sztuczny zbiornik wodny w Poznaniu, znany jako Jezioro Maltańskie",
                    "learner_level": "C1",

            "senses": [
                {
                    "sense_id": "89b7531d-abf0-387d-b0e1-3c04875c6cea",
                    "sense_definition": "По высокой цене; требуя больших денежных затрат.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Обслуживание этого старого автомобиля обходится нам слишком <w>дорого</w>.",
                            "target_lang_translations": {
                                "en": "The maintenance of this old car costs us too <w>much</w>."
                            }
                        },
                        {
                            "text": "В этом ресторане всё очень <w>дорого</w>, поэтому мы пойдём в другое место.",
                            "target_lang_translations": {
                                "en": "Everything in this restaurant is very <w>expensive</w>, so we will go to another place."
                            }
                        }
                    ],
                    "synonyms": [
                        "затратно"
                    ],
                    "antonyms": [
                        "дёшево"
                    ],
                    "common_phrases": [
                        "очень дорого",
                        "слишком дорого",
                        "дорого стоить",
                        "дорого обходиться"
                    ],
                    "target_lang_definitions": {
                        "en": "At a high price; requiring large financial expenditure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expensive",
                                "target_lang_sense_clarification": "costing a lot of money"
                            },
                            {
                                "target_lang_word": "at a high price",
                                "target_lang_sense_clarification": "formal phrase indicating high cost"
                            },
                            {
                                "target_lang_word": "expensively",
                                "target_lang_sense_clarification": "adverb describing an action performed with high cost"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "931a31c3-31fd-3481-82e8-e06d09d06a78",
                    "sense_definition": "Имея большую значимость или ценность для кого-либо; близко сердцу.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне <w>дорого</w> ваше внимание и поддержка в трудную минуту.",
                            "target_lang_translations": {
                                "en": "Your attention and support in a difficult moment are <w>dear</w> to me."
                            }
                        },
                        {
                            "text": "Это старое кольцо мне <w>дорого</w> как память о моей бабушке.",
                            "target_lang_translations": {
                                "en": "This old ring is <w>dear</w> to me as a memory of my grandmother."
                            }
                        }
                    ],
                    "synonyms": [
                        "ценно"
                    ],
                    "antonyms": [
                        "безразлично"
                    ],
                    "common_phrases": [
                        "мне дорого",
                        "дорого как память"
                    ],
                    "target_lang_definitions": {
                        "en": "Having great significance or value to someone; close to one's heart."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dear",
                                "target_lang_sense_clarification": "emotionally significant or cherished"
                            },
                            {
                                "target_lang_word": "precious",
                                "target_lang_sense_clarification": "of high worth or emotional value"
                            }
                        ]
                    }
                },
                {

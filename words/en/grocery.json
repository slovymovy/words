{
    "word_family": [
        "grocer",
        "greengrocer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "91aafea6-4751-3bf9-9404-ec53a0e9f09e",
                    "sense_definition": "A shop or store that sells groceries; a grocery store.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "store",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My mother sent me to the <w>grocery</w> to buy some milk.",
                            "target_lang_translations": {
                                "ru": "Мама послала меня в <w>продуктовый магазин</w> за молоком."
                            }
                        },
                        {
                            "text": "The local <w>grocery</w> is closed on Sundays.",
                            "target_lang_translations": {
                                "ru": "Местный <w>продуктовый</w> закрыт по воскресеньям."
                            }
                        }
                    ],
                    "synonyms": [
                        "grocery store",
                        "supermarket",
                        "market",
                        "grocer's",
                        "grocery shop",
                        "general store"
                    ],
                    "common_phrases": [
                        "local grocery",
                        "corner grocery",
                        "at the grocery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Магазин или лавка, где продаются продукты питания и товары для дома."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продуктовый магазин",
                                "target_lang_sense_clarification": "стандартное название"
                            },
                            {
                                "target_lang_word": "гастроном",
                                "target_lang_sense_clarification": "магазин с широким ассортиментом продуктов"
                            },
                            {
                                "target_lang_word": "бакалея",
                                "target_lang_sense_clarification": "магазин или отдел сухих продуктов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "065c4ee6-58e4-3069-9d4e-bc30a0e76c40",
                    "sense_definition": "Retail foodstuffs and other household supplies.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "goods",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to carry the <w>groceries</w> into the kitchen.",
                            "target_lang_translations": {
                                "ru": "Нам нужно отнести <w>продукты</w> на кухню."
                            }
                        },
                        {
                            "text": "She made a long <w>grocery</w> list for the week.",
                            "target_lang_translations": {
                                "ru": "Она составила длинный список <w>продуктов</w> на неделю."
                            }
                        }
                    ],
                    "synonyms": [
                        "groceries"
                    ],
                    "common_phrases": [
                        "grocery list",
                        "grocery shopping",
                        "grocery bag",
                        "buy groceries"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продукты питания и другие товары для дома, купленные в магазине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продукты",
                                "target_lang_sense_clarification": "общее название для еды"
                            },
                            {
                                "target_lang_word": "бакалея",
                                "target_lang_sense_clarification": "сухие продукты (крупы, мука, сахар)"
                            },
                            {
                                "target_lang_word": "покупки",
                                "target_lang_sense_clarification": "купленные товары"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "45543a88-755d-3855-b20e-fe4105425f4b",
                    "sense_definition": "To go grocery shopping.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We spent the whole morning <w>grocerying</w> for the party.",
                            "target_lang_translations": {
                                "ru": "Мы провели всё утро, <w>закупая продукты</w> для вечеринки."
                            }
                        },
                        {
                            "text": "I hate <w>grocerying</w> when the store is crowded.",
                            "target_lang_translations": {
                                "ru": "Я ненавижу <w>ходить за продуктами</w>, когда в магазине полно народу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "verbing of the noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ходить по магазинам с целью покупки продуктов питания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить за продуктами",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "закупать продукты",
                                "target_lang_sense_clarification": "процесс покупки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68825f90-3c13-313b-a52f-54f8aa934f23",
                    "sense_definition": "To furnish with groceries.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ship was fully <w>groceried</w> before leaving the port.",
                            "target_lang_translations": {
                                "ru": "Корабль был полностью <w>снабжён продовольствием</w> перед выходом из порта."
                            }
                        },
                        {
                            "text": "They <w>groceried</w> the pantry with enough food for a month.",
                            "target_lang_translations": {
                                "ru": "Они <w>закупили продукты</w> в кладовую на месяц вперёд."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжать продовольствием или запасать продукты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снабжать продовольствием",
                                "target_lang_sense_clarification": "обеспечивать едой"
                            },
                            {
                                "target_lang_word": "закупать продукты",
                                "target_lang_sense_clarification": "делать запасы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ef673e22-06b6-3c58-b628-a02c70dcf7b6",
                    "sense_definition": "Обладающий личной независимостью, не находящийся в неволе, рабстве или заключении.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "liberty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый человек хочет быть <w>свободным</w> и счастливым.",
                            "target_lang_translations": {
                                "en": "Every person wants to be <w>free</w> and happy."
                            }
                        },
                        {
                            "text": "Мы живём в <w>свободной</w> и демократической стране.",
                            "target_lang_translations": {
                                "en": "We live in a <w>free</w> and democratic country."
                            }
                        }
                    ],
                    "synonyms": [
                        "вольный",
                        "неограниченный"
                    ],
                    "antonyms": [
                        "рабский",
                        "подневольный",
                        "невольный",
                        "забитый"
                    ],
                    "common_phrases": [
                        "свободный человек",
                        "свободный народ",
                        "свободная воля"
                    ],
                    "target_lang_definitions": {
                        "en": "Possessing personal independence; not in captivity, slavery, or imprisonment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "free",
                                "target_lang_sense_clarification": "independent; not enslaved"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a662aa39-df87-3b56-9302-8d908ddc1fb0",
                    "sense_definition": "Никем и ничем не занятый; пустой, доступный для использования (о месте, времени).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "vacant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В автобусе было только одно <w>свободное</w> место у окна.",
                            "target_lang_translations": {
                                "en": "There was only one <w>vacant</w> seat by the window on the bus."
                            }
                        },
                        {
                            "text": "У меня сегодня совсем нет <w>свободного</w> времени.",
                            "target_lang_translations": {
                                "en": "I have absolutely no <w>free</w> time today."
                            }
                        }
                    ],
                    "synonyms": [
                        "вакантный",
                        "неограниченный"
                    ],
                    "antonyms": [
                        "занятый",
                        "полный"
                    ],
                    "common_phrases": [
                        "свободное место",
                        "свободное время",
                        "свободная касса"
                    ],
                    "target_lang_definitions": {
                        "en": "Not occupied by anyone or anything; empty, available for use (regarding a place or time)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "free",
                                "target_lang_sense_clarification": "available; not busy"
                            },
                            {
                                "target_lang_word": "vacant",
                                "target_lang_sense_clarification": "empty; unoccupied (of a place)"
                            },
                            {
                                "target_lang_word": "spare",
                                "target_lang_sense_clarification": "extra; available (of time)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6f49b38-4239-35a7-8b2b-a373f52fa13e",
                    "sense_definition": "Не имеющий ограничений или запретов; осуществляемый беспрепятственно.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "unrestricted",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вход в этот музей <w>свободный</w> для всех посетителей.",
                            "target_lang_translations": {
                                "en": "Entrance to this museum is <w>free</w> for all visitors."
                            }
                        },
                        {
                            "text": "Для этой работы требуется <w>свободное</w> владение английским языком.",
                            "target_lang_translations": {
                                "en": "This job requires <w>fluent</w> command of English."
                            }
                        }
                    ],
                    "synonyms": [
                        "беспрепятственный",
                        "неограниченный",
                        "вольный"
                    ],
                    "antonyms": [
                        "ограниченный",
                        "затруднённый",
                        "регламентированный"
                    ],
                    "common_phrases": [
                        "свободный вход",
                        "свободный доступ",
                        "свободный выбор"
                    ],
                    "target_lang_definitions": {
                        "en": "Having no restrictions or prohibitions; carried out without hindrance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "free",
                                "target_lang_sense_clarification": "unrestricted; unimpeded"
                            },
                            {
                                "target_lang_word": "open",
                                "target_lang_sense_clarification": "accessible to all"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1a9ab59-5343-371a-985d-013e84694bc0",
                    "sense_definition": "Просторный, широкий, не стесняющий движений (об одежде).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "loose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом я предпочитаю носить лёгкую и <w>свободную</w> одежду.",
                            "target_lang_translations": {
                                "en": "In summer, I prefer to wear light and <w>loose</w> clothing."
                            }
                        },
                        {
                            "text": "Этот свитер слишком <w>свободный</w> для тебя, примерь размер поменьше.",
                            "target_lang_translations": {
                                "en": "This sweater is too <w>loose</w> on you, try a smaller size."
                            }
                        }
                    ],
                    "antonyms": [
                        "тесный"
                    ],
                    "common_phrases": [
                        "свободный крой",
                        "свободный костюм",
                        "свободное платье"
                    ],
                    "target_lang_definitions": {
                        "en": "Spacious, wide, not restricting movement (regarding clothing)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "loose",
                                "target_lang_sense_clarification": "not tight; baggy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce752a64-75df-3312-981b-b6bb541611a8",
                    "sense_definition": "В химии и физике: находящийся в чистом виде, не связанный с другими элементами в соединение.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chemical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Растения не могут усваивать <w>свободный</w> азот непосредственно из воздуха.",
                            "target_lang_translations": {
                                "en": "Plants cannot assimilate <w>free</w> nitrogen directly from the air."
                            }
                        },
                        {
                            "text": "<w>Свободные</w> радикалы могут нанести вред клеткам организма.",
                            "target_lang_translations": {
                                "en": "<w>Free</w> radicals can cause damage to the body's cells."
                            }
                        }
                    ],
                    "antonyms": [
                        "связанный"
                    ],
                    "common_phrases": [
                        "свободный электрон",
                        "свободный радикал",
                        "свободный атом"
                    ],
                    "target_lang_definitions": {
                        "en": "In chemistry and physics: existing in a pure form, not bound to other elements in a compound."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "free",
                                "target_lang_sense_clarification": "uncombined; chemically pure"
                            },
                            {
                                "target_lang_word": "unbound",
                                "target_lang_sense_clarification": "not chemically bonded"
                            }
                        ]
                    }
                }
            ]

                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Lamb</w> will see you in his office now.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Лэм</w> примет вас в своём кабинете сейчас."
                            }

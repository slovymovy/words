    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "466adb3b-2fb7-3ba6-880f-a6786cacc664",
                    "sense_definition": "Не имеет смысла, является напрасным или ненужным (оценочная характеристика действий или ситуации).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "predicative_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спорить с ним сейчас совершенно <w>бесполезно</w>, он не станет слушать.",
                            "target_lang_translations": {
                                "en": "It is completely <w>useless</w> to argue with him right now; he won't listen."
                            }
                        },
                        {
                            "text": "Было <w>бесполезно</w> пытаться открыть дверь без ключа.",
                            "target_lang_translations": {
                                "en": "It was <w>useless</w> to try to open the door without a key."
                            }
                        }
                    ],
                    "synonyms": [
                        "бессмысленно",
                        "напрасно",
                        "зря",
                        "без толку"
                    ],
                    "common_phrases": [
                        "совершенно бесполезно",
                        "бесполезно спорить",
                        "бесполезно пытаться",
                        "абсолютно бесполезно"
                    ],
                    "target_lang_definitions": {
                        "en": "It makes no sense, is vain or unnecessary (evaluation of actions or situations)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "it is useless",
                                "target_lang_sense_clarification": "predicative phrase"
                            },
                            {
                                "target_lang_word": "it is no use",
                                "target_lang_sense_clarification": "idiomatic"
                            },
                            {
                                "target_lang_word": "pointless",
                                "target_lang_sense_clarification": "lacking purpose"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1aab9dc1-6311-3dcc-94ea-0b66256a3bef",
                    "sense_definition": "Не принося пользы, не достигая желаемой цели или результата.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "adverb_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <w>бесполезно</w> потратили целый день, ожидая хорошей погоды.",
                            "target_lang_translations": {
                                "en": "We spent the whole day <w>in vain</w> waiting for good weather."
                            }
                        },
                        {
                            "text": "Лекарство было выпито не <w>бесполезно</w>, пациенту стало значительно лучше.",
                            "target_lang_translations": {
                                "en": "The medicine was not taken <w>in vain</w>; the patient felt significantly better."
                            }
                        }
                    ],
                    "synonyms": [
                        "безуспешно",
                        "безрезультатно",
                        "тщетно",
                        "впустую",
                        "вхолостую",
                        "бесплодно"
                    ],
                    "antonyms": [
                        "с пользой",
                        "небесполезно"
                    ],
                    "common_phrases": [
                        "пройти бесполезно",
                        "потратить время бесполезно"
                    ],
                    "target_lang_definitions": {
                        "en": "Without bringing benefit, failing to achieve the desired goal or result."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in vain",
                                "target_lang_sense_clarification": "without success"
                            },
                            {
                                "target_lang_word": "uselessly",
                                "target_lang_sense_clarification": "adverb"
                            },
                            {
                                "target_lang_word": "for nothing",
                                "target_lang_sense_clarification": "colloquial"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "65e84580-a5a9-334d-9481-2632266d139c",
                    "sense_definition": "According to the way something appears, even if it might not be true; apparently.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The task was <w>seemingly</w> impossible to complete on time, yet they managed to finish it.",
                            "target_lang_translations": {
                                "ru": "Задача была <w>казалось бы</w> невыполнимой в срок, однако им удалось её закончить."
                            }
                        },
                        {
                            "text": "He remained calm and <w>seemingly</w> unaffected by the bad news.",
                            "target_lang_translations": {
                                "ru": "Он оставался спокойным и <w>с виду</w> не тронутым плохими новостями."
                            }
                        }
                    ],
                    "synonyms": [
                        "ostensibly"
                    ],
                    "antonyms": [
                        "actually",
                        "really"
                    ],
                    "common_phrases": [
                        "seemingly impossible",
                        "seemingly endless",
                        "seemingly unrelated",
                        "seemingly simple"
                    ],
                    "target_lang_definitions": {
                        "ru": "Судя по внешнему виду, даже если это может не соответствовать действительности; по-видимому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "казалось бы",
                                "target_lang_sense_clarification": "вводное слово, выражающее предположение вопреки фактам"
                            },
                            {
                                "target_lang_word": "с виду",
                                "target_lang_sense_clarification": "внешне, по внешнему впечатлению"
                            },
                            {
                                "target_lang_word": "по-видимому",
                                "target_lang_sense_clarification": "вероятно, судя по всему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b45c1d11-078e-3a96-9243-5f2ea34c220a",
                    "sense_definition": "In a polite, proper, or appropriate manner.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young gentleman behaved <w>seemingly</w> in the presence of his elders.",
                            "target_lang_translations": {
                                "ru": "Молодой джентльмен вёл себя <w>подобающе</w> в присутствии старших."
                            }
                        },
                        {
                            "text": "She was dressed <w>seemingly</w> for the solemn church service.",
                            "target_lang_translations": {
                                "ru": "Она была одета <w>пристойно</w> для торжественной церковной службы."
                            }
                        }
                    ],
                    "synonyms": [
                        "becomingly"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This usage is largely obsolete in modern English."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливо, пристойно или надлежащим образом (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подобающе",
                                "target_lang_sense_clarification": "надлежащим образом"
                            },
                            {
                                "target_lang_word": "пристойно",
                                "target_lang_sense_clarification": "в соответствии с правилами приличия"

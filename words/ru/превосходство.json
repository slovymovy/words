    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00ff6150-dff9-339f-914f-2daeb45d206b",
                    "sense_definition": "Обладание более высокими качествами, достоинствами или способностями по сравнению с кем-либо или чем-либо.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спортсмен доказал своё полное <w>превосходство</w> над соперниками в финальном забеге.",
                            "target_lang_translations": {
                                "en": "The athlete proved his complete <w>superiority</w> over his rivals in the final race."
                            }
                        },
                        {
                            "text": "Техническое <w>превосходство</w> новой модели автомобиля было очевидно для всех экспертов.",
                            "target_lang_translations": {
                                "en": "The technical <w>superiority</w> of the new car model was obvious to all experts."
                            }
                        }
                    ],
                    "synonyms": [
                        "преимущество"
                    ],
                    "antonyms": [
                        "отставание"
                    ],
                    "common_phrases": [
                        "явное превосходство",
                        "доказать превосходство",
                        "ощущать превосходство",
                        "техническое превосходство"
                    ],
                    "target_lang_definitions": {
                        "en": "Possession of higher qualities, virtues, or abilities compared to someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "superiority",
                                "target_lang_sense_clarification": "the state of being better in quality or ability"
                            },
                            {
                                "target_lang_word": "supremacy",
                                "target_lang_sense_clarification": "highest rank or complete dominance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "685b5be2-d788-359b-abf2-4f75ac08ee7a",
                    "sense_definition": "Значительное преимущество в количестве, численности или военной силе.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Армия противника имела значительное численное <w>превосходство</w> на этом участке фронта.",
                            "target_lang_translations": {
                                "en": "The enemy army had significant numerical <w>superiority</w> on this sector of the front."
                            }
                        },
                        {
                            "text": "Несмотря на <w>превосходство</w> врага в силе, защитники крепости не сдавались.",
                            "target_lang_translations": {
                                "en": "Despite the enemy's <w>superiority</w> in force, the defenders of the fortress did not surrender."
                            }
                        }
                    ],
                    "synonyms": [
                        "преимущество"
                    ],
                    "common_phrases": [
                        "численное превосходство",
                        "многократное превосходство",
                        "превосходство в силе",
                        "превосходство в воздухе"
                    ],
                    "target_lang_definitions": {
                        "en": "Significant advantage in quantity, numbers, or military force."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "superiority",
                                "target_lang_sense_clarification": "greater numbers or military strength"
                            },
                            {
                                "target_lang_word": "advantage",
                                "target_lang_sense_clarification": "favorable position or numerical lead"
                            },
                            {
                                "target_lang_word": "supremacy",
                                "target_lang_sense_clarification": "complete control or dominance, esp. military"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3f662792-147f-36ce-8978-1d7331d961f4",
                    "sense_definition": "Oddawać głos w wyborach lub innym formalnym procesie decyzyjnym, aby wyrazić swoją wolę lub wybór.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "voting_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy pełnoletni obywatel ma prawo <w>głosować</w> w wyborach prezydenckich.",
                            "target_lang_translations": {
                                "ru": "Каждый совершеннолетний гражданин имеет право <w>голосовать</w> на президентских выборах."
                            }
                        },
                        {
                            "text": "Musimy <w>głosować</w> za przyjęciem tego ważnego projektu.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>голосовать</w> за принятие этого важного проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "wybierać",
                        "opowiadać się"
                    ],
                    "antonyms": [
                        "wstrzymywać się"
                    ],
                    "common_phrases": [
                        "głosować na kandydata",
                        "głosować za ustawą",
                        "głosować przeciw",
                        "iść głosować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдавать голос на выборах или в другом официальном процессе принятия решений, чтобы выразить свою волю или выбор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голосовать",
                                "target_lang_sense_clarification": "участвовать в выборах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8174fa1-a134-3dd2-9179-571982582fdf",
                    "sense_definition": "Mówić bardzo głośno, hałasować lub wydawać donośne dźwięki.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "making_noise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zgromadzony na rynku tłum zaczął głośno <w>głosować</w>.",
                            "target_lang_translations": {
                                "ru": "Собравшаяся на площади толпа начала громко <w>шуметь</w>."
                            }
                        },
                        {
                            "text": "Psy gończe zaczęły <w>głosować</w>, gdy poczuły trop zwierzyny.",
                            "target_lang_translations": {
                                "ru": "Гончие псы начали <w>подавать голос</w>, когда почуяли след зверя."
                            }
                        }
                    ],
                    "synonyms": [
                        "hałasować",
                        "krzyczeć",
                        "wołać"
                    ],
                    "antonyms": [
                        "milczeć"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie używane rzadko, głównie w języku łowieckim lub dawnym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить очень громко, шуметь или издавать громкие звуки (устаревшее или охотничье значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подавать голос",
                                "target_lang_sense_clarification": "об охотничьих собаках"
                            },
                            {
                                "target_lang_word": "шуметь",
                                "target_lang_sense_clarification": "издавать шум"

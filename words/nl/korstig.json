{
    "word_family": [
        "korst",
        "korsten",
        "ontkorsten"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a2900e26-85fb-3924-8941-cbe0741d74d9",
                    "sense_definition": "Voorzien van een harde buitenlaag of korst, vaak gezegd van brood, gebak of een oppervlak.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "texture_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij de soep serveerden ze heerlijk <w>korstig</w> brood.",
                            "target_lang_translations": {
                                "ru": "К супу подали вкусный хлеб <w>с хрустящей корочкой</w>."
                            }
                        },
                        {
                            "text": "De aarde was droog en <w>korstig</w> door de hitte van de zon.",
                            "target_lang_translations": {
                                "ru": "Земля была сухой и <w>покрытой коркой</w> от солнечного жара."
                            }
                        }
                    ],
                    "synonyms": [
                        "krokant"
                    ],
                    "antonyms": [
                        "zacht"
                    ],
                    "common_phrases": [
                        "korstig brood",
                        "korstige laag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий твердый внешний слой или корку, часто говорится о хлебе, выпечке или поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с корочкой",
                                "target_lang_sense_clarification": "о хлебе или выпечке"
                            },
                            {
                                "target_lang_word": "покрытый коркой",
                                "target_lang_sense_clarification": "о поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4815c5e7-0aa5-3d90-b644-22114bf2a86a",
                    "sense_definition": "Bedekt met korsten, meestal verwijzend naar een genezende wond of een huidaandoening.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "texture_skin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schaafwond op zijn knie is nu droog en <w>korstig</w>.",
                            "target_lang_translations": {
                                "ru": "Ссадина на его колене теперь сухая и <w>покрылась коркой</w>."
                            }
                        },
                        {
                            "text": "Je moet niet aan de <w>korstige</w> plekjes op je huid krabben.",
                            "target_lang_translations": {
                                "ru": "Не стоит расчесывать <w>покрытые коркой</w> участки на коже."
                            }
                        }
                    ],
                    "antonyms": [
                        "gaaf"
                    ],
                    "common_phrases": [
                        "korstige wond",
                        "korstige huid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытый корками, обычно относится к заживающей ране или кожному заболеванию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрытый коркой",
                                "target_lang_sense_clarification": "о ране"
                            },
                            {
                                "target_lang_word": "покрытый струпьями",
                                "target_lang_sense_clarification": "медицинский контекст"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
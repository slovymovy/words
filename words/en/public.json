{
    "word_family": [
        "publicly",
        "publication",
        "publicize",
        "publicity",
        "publicist"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "aad7e627-aebb-34e7-804a-c2b269a5572d",
                    "sense_definition": "Open to all members of a community, often provided by the government and supported by taxes.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "access_services",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We usually take <w>public</w> transport to get to the city center.",
                            "target_lang_translations": {
                                "ru": "Мы обычно пользуемся <w>общественным</w> транспортом, чтобы добраться до центра города."
                            }
                        },
                        {
                            "text": "The city opened a new <w>public</w> library downtown.",
                            "target_lang_translations": {
                                "ru": "В центре города открылась новая <w>публичная</w> библиотека."
                            }
                        }
                    ],
                    "synonyms": [
                        "commonweal",
                        "commonweal"
                    ],
                    "antonyms": [
                        "private"
                    ],
                    "common_phrases": [
                        "public transport",
                        "public library",
                        "public park",
                        "public toilet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доступный для всех членов общества, часто предоставляемый государством и финансируемый за счет налогов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общественный",
                                "target_lang_sense_clarification": "относящийся к услугам или местам общего пользования (транспорт, туалет)"
                            },
                            {
                                "target_lang_word": "публичный",
                                "target_lang_sense_clarification": "открытый для посещения (библиотека, парк)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1e7e998-4dae-3193-a199-86949e00822b",
                    "sense_definition": "Relating to or concerning the people as a whole in a community or country.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "society_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government must consider <w>public</w> opinion before making a decision.",
                            "target_lang_translations": {
                                "ru": "Правительство должно учитывать <w>общественное</w> мнение перед принятием решения."
                            }
                        },
                        {
                            "text": "Smoking is banned in many places for reasons of <w>public</w> health.",
                            "target_lang_translations": {
                                "ru": "Курение запрещено во многих местах в целях охраны <w>общественного</w> здоровья."
                            }
                        }
                    ],
                    "synonyms": [
                        "public interest",
                        "public good",
                        "commonweal"
                    ],
                    "antonyms": [
                        "private",
                        "nonpublic"
                    ],
                    "common_phrases": [
                        "public opinion",
                        "public health",
                        "public interest",
                        "public safety"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к народу или обществу в целом; затрагивающий интересы всего сообщества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общественный",
                                "target_lang_sense_clarification": "связанный с обществом (мнение, здоровье, интерес)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9776c354-a14f-37f3-913e-71e30892361b",
                    "sense_definition": "Able to be known or seen by everyone; not hidden or secret.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "visibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The details of the agreement were made <w>public</w> yesterday.",
                            "target_lang_translations": {
                                "ru": "Детали соглашения стали <w>публичными</w> вчера."
                            }
                        },
                        {
                            "text": "It is <w>public</w> knowledge that they are planning to get married.",
                            "target_lang_translations": {
                                "ru": "Это <w>общеизвестный</w> факт, что они планируют пожениться."
                            }
                        }
                    ],
                    "antonyms": [
                        "private",
                        "unpublic"
                    ],
                    "common_phrases": [
                        "make public",
                        "public knowledge",
                        "public view"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доступный для всеобщего сведения; не скрытый и не тайный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публичный",
                                "target_lang_sense_clarification": "сделанный явным, открытым для всех"
                            },
                            {
                                "target_lang_word": "общеизвестный",
                                "target_lang_sense_clarification": "известный всем (факт, знание)"
                            },
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "не секретный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a4ba2ff-4e67-3378-bd3d-2dd8511a268f",
                    "sense_definition": "Connected with the government or state, rather than private companies or individuals.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "access_services",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has worked in the <w>public</w> sector for twenty years.",
                            "target_lang_translations": {
                                "ru": "Она проработала в <w>государственном</w> секторе двадцать лет."
                            }
                        },
                        {
                            "text": "There is a debate about reducing <w>public</w> spending on administration.",
                            "target_lang_translations": {
                                "ru": "Идут дебаты о сокращении <w>государственных</w> расходов на администрацию."
                            }
                        }
                    ],
                    "antonyms": [
                        "private"
                    ],
                    "common_phrases": [
                        "public sector",
                        "public spending",
                        "public funds",
                        "public services"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с правительством или государством, в отличие от частных компаний или лиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "государственный",
                                "target_lang_sense_clarification": "относящийся к госсектору или бюджету"
                            },
                            {
                                "target_lang_word": "общественный",
                                "target_lang_sense_clarification": "в контексте государственных фондов или служб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4d19f51-0cf8-331d-9c0c-1852a9509551",
                    "sense_definition": "Relating to a person's official life or status as a well-known figure, rather than their private life.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As a <w>public</w> figure, he has to be careful about what he says.",
                            "target_lang_translations": {
                                "ru": "Как <w>публичная</w> личность, он должен быть осторожен в своих высказываниях."
                            }
                        },
                        {
                            "text": "She decided to retire from <w>public</w> life to spend more time with her family.",
                            "target_lang_translations": {
                                "ru": "Она решила уйти из <w>публичной</w> жизни, чтобы проводить больше времени с семьей."
                            }
                        }
                    ],
                    "antonyms": [
                        "private"
                    ],
                    "common_phrases": [
                        "public figure",
                        "public life",
                        "public image"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к официальной деятельности или статусу известной личности, в отличие от частной жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публичный",
                                "target_lang_sense_clarification": "находящийся на виду у публики (фигура, жизнь)"
                            },
                            {
                                "target_lang_word": "общественный",
                                "target_lang_sense_clarification": "связанный с деятельностью в обществе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5790eb47-6d08-3cc5-b4af-544c96982ca1",
                    "sense_definition": "Of a company: having shares that can be bought and sold by anyone through a stock market.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "business",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tech startup decided to go <w>public</w> to raise capital.",
                            "target_lang_translations": {
                                "ru": "Технологический стартап решил стать <w>публичным</w>, чтобы привлечь капитал."
                            }
                        },
                        {
                            "text": "Being a <w>public</w> company requires more financial transparency.",
                            "target_lang_translations": {
                                "ru": "Статус <w>публичной</w> компании требует большей финансовой прозрачности."
                            }
                        }
                    ],
                    "antonyms": [
                        "private"
                    ],
                    "common_phrases": [
                        "go public",
                        "public company"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "business"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О компании: имеющая акции, которые могут свободно покупаться и продаваться на фондовом рынке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публичный",
                                "target_lang_sense_clarification": "акционерный, с акциями в свободном обращении"
                            },
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "тип акционерного общества"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "56b66f8a-e4e0-31e3-b157-a1c15834e7b4",
                    "sense_definition": "The people of a community or nation in general.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "society_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum is now open to the <w>public</w> every day except Monday.",
                            "target_lang_translations": {
                                "ru": "Музей теперь открыт для <w>публики</w> каждый день, кроме понедельника."
                            }
                        },
                        {
                            "text": "Members of the <w>public</w> helped the police find the missing child.",
                            "target_lang_translations": {
                                "ru": "Представители <w>общественности</w> помогли полиции найти пропавшего ребенка."
                            }
                        },
                        {
                            "text": "He rarely speaks about his family in <w>public</w>.",
                            "target_lang_translations": {
                                "ru": "Он редко говорит о своей семье на <w>людях</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "commonweal"
                    ],
                    "common_phrases": [
                        "the general public",
                        "members of the public",
                        "open to the public",
                        "in public"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, общество или население страны в целом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публика",
                                "target_lang_sense_clarification": "посетители, зрители, люди в общественном месте"
                            },
                            {
                                "target_lang_word": "общественность",
                                "target_lang_sense_clarification": "активная часть общества, граждане"
                            },
                            {
                                "target_lang_word": "народ",
                                "target_lang_sense_clarification": "люди вообще (разговорное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3259d0ff-60dc-382d-9bdc-74ad351c39e1",
                    "sense_definition": "A specific group of people who share a common interest or are the audience for a particular work.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "society_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author's new book was well received by the reading <w>public</w>.",
                            "target_lang_translations": {
                                "ru": "Новая книга автора была хорошо принята читающей <w>публикой</w>."
                            }
                        },
                        {
                            "text": "This product is designed to appeal to the buying <w>public</w>.",
                            "target_lang_translations": {
                                "ru": "Этот продукт разработан, чтобы привлечь покупающую <w>публику</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "reading public",
                        "buying public"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная группа людей, объединенных общим интересом; целевая аудитория."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публика",
                                "target_lang_sense_clarification": "аудитория читателей или покупателей"
                            },
                            {
                                "target_lang_word": "аудитория",
                                "target_lang_sense_clarification": "круг потребителей контента или товаров"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
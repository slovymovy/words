    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a504fef5-8483-3ec7-bb96-f399c6341a19",
                    "sense_definition": "An act of selecting or making a decision when faced with two or more possibilities.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_choice_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a difficult <w>choice</w> to make, but I think it was the right one.",
                            "target_lang_translations": {
                                "ru": "Это был трудный <w>выбор</w>, но я думаю, он был правильным."
                            }
                        },
                        {
                            "text": "You need to make a <w>choice</w> between staying here or moving abroad.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно сделать <w>выбор</w> между тем, чтобы остаться здесь или переехать за границу."
                            }
                        }
                    ],
                    "synonyms": [
                        "selection",
                        "option"
                    ],
                    "common_phrases": [
                        "make a choice",
                        "good choice",
                        "bad choice",
                        "by choice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по выбору или принятию решения при наличии двух или более вариантов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбор",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85831c57-b3d1-3fd0-a30c-953df2944656",
                    "sense_definition": "The person or thing that has been selected.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_choice_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This restaurant was a great <w>choice</w> for our anniversary dinner.",
                            "target_lang_translations": {
                                "ru": "Этот ресторан был отличным <w>выбором</w> для нашего юбилейного ужина."
                            }
                        },
                        {
                            "text": "The blue dress is a popular <w>choice</w> for bridesmaids this season.",
                            "target_lang_translations": {
                                "ru": "Синее платье — популярный <w>выбор</w> для подружек невесты в этом сезоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "selection"
                    ],
                    "common_phrases": [
                        "popular choice",
                        "excellent choice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или вещь, которые были выбраны."

                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "language_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher explained the complex rules of <w>Latin</w> grammar.",
                            "target_lang_translations": {
                                "ru": "Учитель объяснил сложные правила <w>латинской</w> грамматики."
                            }
                        },
                        {
                            "text": "He found an old book full of <w>Latin</w> inscriptions.",
                            "target_lang_translations": {
                                "ru": "Он нашел старую книгу, полную <w>латинских</w> надписей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Latin word",
                        "Latin grammar",
                        "Latin text"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к латинскому языку."
                    },
                    "translations": {

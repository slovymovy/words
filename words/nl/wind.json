                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De slanke <w>wind</w> rende razendsnel achter de prooi aan.",
                            "target_lang_translations": {
                                "en": "The slender <w>greyhound</w> ran lightning-fast after the prey.",
                                "ru": "Стройная <w>борзая</w> молниеносно погналась за добычей."
                            }
                        },
                        {
                            "text": "Zij hebben een prachtige <w>wind</w> als huisdier aangeschaft.",
                            "target_lang_translations": {
                                "en": "They have purchased a beautiful <w>greyhound</w> as a pet.",
                                "ru": "Они завели прекрасную <w>борзую</w> в качестве домашнего питомца."
                            }
                        }
                    ],
                    "synonyms": [
                        "bout"
                    ],
                    "target_lang_definitions": {
                        "en": "A dog breed known for its speed and slender build (short for greyhound).",
                        "ru": "Порода собак, известная своей скоростью и стройным телосложением (сокращение от windhond)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "greyhound",
                                "target_lang_sense_clarification": "dog breed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "борзая",
                                "target_lang_sense_clarification": "порода собак"
                            }
                        ]
                    }
                }
            ]
        },

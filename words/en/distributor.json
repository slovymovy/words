    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7026df2b-bd92-3c27-bf64-27e9caaf2e1a",
                    "sense_definition": "A person or company that supplies goods to stores or other businesses.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_agent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>distributor</w> supplies fresh produce to supermarkets across the country.",
                            "target_lang_translations": {
                                "ru": "<w>Дистрибьютор</w> поставляет свежие продукты в супермаркеты по всей стране."
                            }
                        },
                        {
                            "text": "We buy our parts directly from an authorized <w>distributor</w> to ensure quality.",
                            "target_lang_translations": {
                                "ru": "Мы покупаем запчасти напрямую у авторизованного <w>дистрибьютора</w>, чтобы гарантировать качество."
                            }
                        }
                    ],
                    "synonyms": [
                        "dizzy"
                    ],
                    "antonyms": [
                        "consumer",
                        "customer"
                    ],
                    "common_phrases": [
                        "authorized distributor",
                        "exclusive distributor",
                        "wholesale distributor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или компания, поставляющая товары в магазины или другим предприятиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дистрибьютор",
                                "target_lang_sense_clarification": "торговый посредник"
                            },
                            {
                                "target_lang_word": "поставщик",
                                "target_lang_sense_clarification": "снабженец"
                            },
                            {
                                "target_lang_word": "распространитель",
                                "target_lang_sense_clarification": "агент по сбыту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61332c18-bb3d-3f2e-b6f9-23db55a3fc45",
                    "sense_definition": "A device in an engine that sends electric current to the spark plugs.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic checked the <w>distributor</w> to see why the car would not start.",
                            "target_lang_translations": {
                                "ru": "Механик проверил <w>распределитель зажигания</w>, чтобы понять, почему машина не заводится."
                            }
                        },
                        {
                            "text": "Old cars often need a new <w>distributor</w> cap during a tune-up.",
                            "target_lang_translations": {
                                "ru": "Старым автомобилям часто требуется новая крышка <w>распределителя</w> во время техобслуживания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "distributor cap",
                        "ignition distributor"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "automotive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство в двигателе, распределяющее электрический ток по свечам зажигания."
                    },
                    "translations": {
                        "ru": [
                            {

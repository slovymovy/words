    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0e67d61e-c35c-33b1-be5a-f7009647c7e8",
                    "sense_definition": "Een organisatie of instelling die is opgericht voor een specifiek doel, zoals onderzoek, onderwijs of zorg.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij werkt al jaren als onderzoeker bij een medisch <w>instituut</w>.",
                            "target_lang_translations": {
                                "ru": "Он уже много лет работает исследователем в медицинском <w>институте</w>."
                            }
                        },
                        {
                            "text": "Het <w>instituut</w> biedt verschillende cursussen aan voor buitenlandse studenten.",
                            "target_lang_translations": {
                                "ru": "<w>Институт</w> предлагает различные курсы для иностранных студентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "instelling"
                    ],
                    "common_phrases": [
                        "wetenschappelijk instituut",
                        "medisch instituut",
                        "aan een instituut verbonden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организация или учреждение, созданное для определенной цели, например, для исследований, образования или ухода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "институт",
                                "target_lang_sense_clarification": "научное или учебное заведение"
                            },
                            {
                                "target_lang_word": "учреждение",
                                "target_lang_sense_clarification": "организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "145c86d6-24a8-31c6-81eb-96f227dbc5ab",
                    "sense_definition": "Een gevestigde regel, gebruik of maatschappelijke inrichting die door traditie in stand wordt gehouden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het huwelijk wordt in veel culturen gezien als een belangrijk <w>instituut</w>.",
                            "target_lang_translations": {
                                "ru": "Брак во многих культурах рассматривается как важный <w>институт</w>."
                            }
                        },
                        {
                            "text": "Kritiek op het <w>instituut</w> van de monarchie is in dit land niet ongewoon.",
                            "target_lang_translations": {
                                "ru": "Критика <w>института</w> монархии в этой стране не является чем-то необычным."
                            }
                        }
                    ],
                    "synonyms": [
                        "regel"
                    ],
                    "common_phrases": [
                        "het instituut van het huwelijk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устоявшееся правило, обычай или общественное устройство, поддерживаемое традицией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "институт",
                                "target_lang_sense_clarification": "общественное явление (например, институт брака)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "3f213dd0-3793-3949-9f23-907fcd7a9886",
                    "sense_definition": "Spójnik łączący zdania, używany do wskazania przyczyny lub powodu zaistnienia sytuacji opisanej w zdaniu nadrzędnym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cause_conjunction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Listonosz zostawił list pod drzwiami, <w>ponieważ</w> bał się groźnego psa.",
                            "target_lang_translations": {
                                "ru": "Почтальон оставил письмо под дверью, <w>потому что</w> боялся злой собаки."
                            }
                        },
                        {
                            "text": "Nie poszedłem dzisiaj do pracy, <w>ponieważ</w> bardzo źle się czułem.",
                            "target_lang_translations": {
                                "ru": "Я не пошел сегодня на работу, <w>так как</w> очень плохо себя чувствовал."
                            }
                        }
                    ],
                    "synonyms": [
                        "bo",
                        "gdyż",
                        "dlatego że",
                        "jako że",
                        "albowiem"
                    ],
                    "common_phrases": [
                        "ponieważ nie",
                        "ponieważ jest",
                        "ponieważ mam",
                        "ponieważ chcę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, используемый для указания причины действия или ситуации; соединяет придаточное предложение причины с главным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потому что",
                                "target_lang_sense_clarification": "самый распространенный и нейтральный союз для указания причины"
                            },
                            {
                                "target_lang_word": "так как",
                                "target_lang_sense_clarification": "употребляется в тех же значениях, что и «потому что», но чаще встречается в письменной речи"
                            },
                            {
                                "target_lang_word": "поскольку",
                                "target_lang_sense_clarification": "указывает на причину, часто уже известную собеседнику; имеет оттенок «раз уж»"
                            }
                        ]
                    }
                }
            ]
        },

                        "cloak",
                        "shawl"
                    ],
                    "common_phrases": [
                        "wool poncho",
                        "knitted poncho",
                        "wear a poncho"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простая свободная одежда из плотной ткани с отверстием для головы посередине, которую носят для тепла или красоты."
                    },
                    "translations": {
                        "ru": [
                            {

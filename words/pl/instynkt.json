{
    "word_family": [
        "instynktowny",
        "instynktownie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "16c48fee-4e41-38c8-a79d-dedce933e7df",
                    "sense_definition": "Wrodzona zdolność żywych organizmów do wykonywania złożonych, celowych czynności, ważnych dla przetrwania gatunku, niewynikająca z nauki.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zwierzęta w lesie kierują się głównie swoim naturalnym <w>instynktem</w>.",
                            "target_lang_translations": {
                                "ru": "Животные в лесу руководствуются в основном своим природным <w>инстинктом</w>."
                            }
                        },
                        {
                            "text": "Silny <w>instynkt</w> samozachowawczy pozwolił mu uniknąć groźnego wypadku na drodze.",
                            "target_lang_translations": {
                                "ru": "Сильный <w>инстинкт</w> самосохранения позволил ему избежать серьезной аварии на дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "popęd"
                    ],
                    "common_phrases": [
                        "instynkt samozachowawczy",
                        "instynkt przetrwania",
                        "instynkt macierzyński",
                        "instynkt stadny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Врожденная способность живых организмов совершать сложные целесообразные действия, важные для выживания вида, не являющаяся результатом обучения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инстинкт",
                                "target_lang_sense_clarification": "биологический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11d0f3fc-1eca-389c-af8b-93dd10aac309",
                    "sense_definition": "Wewnętrzne przeczucie pozwalające podejmować decyzje bez racjonalnej analizy; intuicja.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>instynkt</w> podpowiada mi, że nie powinniśmy ufać temu człowiekowi.",
                            "target_lang_translations": {
                                "ru": "Мой <w>инстинкт</w> подсказывает мне, что нам не следует доверять этому человеку."
                            }
                        },
                        {
                            "text": "W trudnych sytuacjach warto czasem zaufać <w>instynktowi</w> zamiast logice.",
                            "target_lang_translations": {
                                "ru": "В трудных ситуациях иногда стоит довериться <w>инстинкту</w> вместо логики."
                            }
                        }
                    ],
                    "synonyms": [
                        "intuicja"
                    ],
                    "common_phrases": [
                        "zaufać instynktowi",
                        "kobiecy instynkt",
                        "działać pod wpływem instynktu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутреннее чувство, позволяющее принимать решения без рационального анализа; интуиция."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интуиция",
                                "target_lang_sense_clarification": "шестое чувство"
                            },
                            {
                                "target_lang_word": "чутьё",
                                "target_lang_sense_clarification": "проницательность"
                            },
                            {
                                "target_lang_word": "инстинкт",
                                "target_lang_sense_clarification": "внутренний голос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b6736e3-548e-3af2-aa5f-845de9ca4782",
                    "sense_definition": "Wrodzona łatwość, smykałka lub talent do wykonywania określonych czynności.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten młody napastnik ma niesamowity <w>instynkt</w> strzelecki na boisku.",
                            "target_lang_translations": {
                                "ru": "У этого молодого нападающего невероятное голевое <w>чутьё</w> на поле."
                            }
                        },
                        {
                            "text": "Mój brat ma świetny <w>instynkt</w> do interesów i potrafi sprzedać wszystko.",
                            "target_lang_translations": {
                                "ru": "У моего брата отличное деловое <w>чутьё</w>, и он может продать всё что угодно."
                            }
                        }
                    ],
                    "synonyms": [
                        "talent",
                        "żyłka",
                        "nerw"
                    ],
                    "common_phrases": [
                        "instynkt handlowy",
                        "instynkt strzelecki",
                        "instynkt polityczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Врожденная легкость, сноровка или талант к выполнению определенных действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чутьё",
                                "target_lang_sense_clarification": "способность"
                            },
                            {
                                "target_lang_word": "нюх",
                                "target_lang_sense_clarification": "разг. талант"
                            },
                            {
                                "target_lang_word": "жилка",
                                "target_lang_sense_clarification": "склонность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "frequency": "Middle",
                    "semantic_group_id": "excess_financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I chose a policy with a higher <w>excess</w> to lower my monthly payments.",
                            "target_lang_translations": {
                                "ru": "Я выбрал полис с более высокой <w>франшизой</w>, чтобы снизить ежемесячные платежи."
                            }

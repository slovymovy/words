    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0f073051-6281-34bc-bc64-22315b488931",
                    "sense_definition": "A sexually promiscuous woman or a prostitute; often used as a highly offensive term of abuse.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "slang_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was criticized for using the word <w>ho</w> in his lyrics.",
                            "target_lang_translations": {
                                "ru": "Его критиковали за использование слова <w>шлюха</w> в текстах песен."
                            }
                        },
                        {
                            "text": "The phrase \"bros before <w>hoes</w>\" is considered misogynistic by many.",
                            "target_lang_translations": {
                                "ru": "Фраза «братаны важнее <w>шлюх</w>» многими считается женоненавистнической."
                            }
                        }
                    ],
                    "synonyms": [
                        "whore",
                        "prostitute",
                        "hooker"
                    ],
                    "common_phrases": [
                        "bros before hoes"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar, offensive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сексуально распущенная женщина или проститутка; часто используется как оскорбление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлюха",
                                "target_lang_sense_clarification": "сленг, грубое"
                            },
                            {
                                "target_lang_word": "проститутка",
                                "target_lang_sense_clarification": "прямое значение"
                            },
                            {
                                "target_lang_word": "девка",
                                "target_lang_sense_clarification": "разговорное, пренебрежительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e658b638-f1c2-3027-97a8-9f59530dc636",
                    "sense_definition": "Initialism of <w>head office</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "business_abbrev",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please submit your expense reports to <w>HO</w> by Friday.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отправьте отчеты о расходах в <w>головной офис</w> к пятнице."
                            }
                        },
                        {
                            "text": "She was transferred to the <w>HO</w> in London last month.",
                            "target_lang_translations": {
                                "ru": "В прошлом месяце её перевели в <w>штаб-квартиру</w> в Лондоне."
                            }
                        }
                    ],
                    "synonyms": [

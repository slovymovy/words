    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8c931469-c612-3e7a-883d-05d5e3bb3009",
                    "sense_definition": "Плотно обертывать, закрывать кого-либо или что-либо тканью, бумагой или другим материалом.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой мама всегда просила меня <заматывать> шею теплым шарфом.",
                            "target_lang_translations": {
                                "en": "In winter, Mom always asked me to <wrap> my neck with a warm scarf."
                            }
                        },
                        {
                            "text": "Мы решили <заматывать> хрупкую посуду в газету перед переездом.",
                            "target_lang_translations": {
                                "en": "We decided to <wrap> the fragile dishes in newspaper before the move."
                            }
                        }
                    ],
                    "synonyms": [
                        "укутывать",
                        "обматывать"
                    ],
                    "antonyms": [
                        "разматывать",
                        "разворачивать"
                    ],
                    "common_phrases": [
                        "заматывать шарфом",
                        "заматывать бинтом",
                        "заматывать в бумагу"
                    ],
                    "target_lang_definitions": {
                        "en": "To wrap tightly or cover someone or something with fabric, paper, or other material."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wrap up",
                                "target_lang_sense_clarification": "to cover completely, often for warmth or protection"
                            },
                            {
                                "target_lang_word": "wrap",
                                "target_lang_sense_clarification": "to enclose in paper or soft material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6074b3b-f015-33a0-945f-df73b3ecbc3a",
                    "sense_definition": "Навивать что-либо длинное (нитку, веревку, проволоку) на какой-либо предмет круговыми движениями.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рыбак начал быстро <заматывать> леску на катушку, почувствовав клев.",
                            "target_lang_translations": {
                                "en": "The fisherman started to quickly <wind> the line onto the reel when he felt a bite."
                            }
                        },
                        {
                            "text": "Нужно аккуратно <заматывать> садовый шланг после полива огорода.",
                            "target_lang_translations": {
                                "en": "You need to carefully <coil> the garden hose after watering the vegetable garden."
                            }
                        }
                    ],
                    "synonyms": [
                        "наматывать",
                        "накручивать"
                    ],
                    "antonyms": [
                        "разматывать"
                    ],
                    "common_phrases": [
                        "заматывать леску",
                        "заматывать нитки",
                        "заматывать проволоку"
                    ],
                    "target_lang_definitions": {
                        "en": "To wind something long (thread, rope, wire) onto an object using circular motions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wind",
                                "target_lang_sense_clarification": "to twist or turn something around a core"
                            },
                            {
                                "target_lang_word": "coil",
                                "target_lang_sense_clarification": "to arrange something flexible into rings or spirals"
                            },
                            {
                                "target_lang_word": "reel in",
                                "target_lang_sense_clarification": "specifically for fishing lines or cables"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b389840-19c0-32d5-8e33-2ea3aa25c0a4",
                    "sense_definition": "Сильно утомлять, лишать сил непрерывной работой, беготней или хлопотами.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта бесконечная беготня по магазинам начинает меня <заматывать>.",
                            "target_lang_translations": {
                                "en": "This endless running around the shops is starting to <wear> me <out>."
                            }
                        },
                        {
                            "text": "Начальник любит <заматывать> своих сотрудников бессмысленными поручениями.",
                            "target_lang_translations": {
                                "en": "The boss likes to <exhaust> his employees with meaningless tasks."
                            }
                        }
                    ],
                    "synonyms": [
                        "изматывать",
                        "утомлять",
                        "загонять"
                    ],
                    "common_phrases": [
                        "заматывать работой",
                        "совсем замотал"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To tire out or exhaust someone greatly through continuous work, running around, or chores."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wear out",
                                "target_lang_sense_clarification": "to make someone extremely tired"
                            },
                            {
                                "target_lang_word": "exhaust",
                                "target_lang_sense_clarification": "to drain of strength or energy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a8b3a34-932f-30fc-ac5e-7dda3e0a3f2f",
                    "sense_definition": "Не возвращать взятую на время вещь или деньги; присваивать чужое.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он решил <заматывать> взятую у друга книгу и перестал отвечать на звонки.",
                            "target_lang_translations": {
                                "en": "He decided to <keep> the book he borrowed from a friend and stopped answering calls."
                            }
                        },
                        {
                            "text": "Нечестный должник пытался <заматывать> деньги, надеясь, что о них забудут.",
                            "target_lang_translations": {
                                "en": "The dishonest debtor tried to <keep> the money, hoping it would be forgotten."
                            }
                        }
                    ],
                    "synonyms": [
                        "зажиливать",
                        "присваивать"
                    ],
                    "antonyms": [
                        "возвращать",
                        "отдавать"
                    ],
                    "common_phrases": [
                        "заматывать долг"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To fail to return a borrowed item or money; to appropriate something belonging to someone else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "to retain possession of something instead of returning it"
                            },
                            {
                                "target_lang_word": "pocket",
                                "target_lang_sense_clarification": "to take or keep dishonestly (usually money)"
                            }
                        ]
                    }
                }
            ]
        }

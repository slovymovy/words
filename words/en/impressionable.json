            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "07987239-c1f3-33a2-b0dd-535e62316e27",
                    "sense_definition": "Easily influenced by other people or ideas, especially because of being young or lacking experience.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Children are at an <w>impressionable</w> age and tend to copy the behavior of adults around them.",
                            "target_lang_translations": {
                                "ru": "Дети находятся во <w>впечатлительном</w> возрасте и склонны копировать поведение окружающих их взрослых."
                            }
                        },
                        {
                            "text": "The teacher worried that the violent movie would disturb the <w>impressionable</w> minds of her students.",
                            "target_lang_translations": {
                                "ru": "Учительница беспокоилась, что жестокий фильм потревожит <w>впечатлительные</w> умы её учеников."
                            }
                        }
                    ],
                    "synonyms": [
                        "suggestible",
                        "susceptible",
                        "malleable",
                        "influenceable"
                    ],
                    "antonyms": [
                        "unimpressionable",
                        "skeptical",
                        "cynical"
                    ],
                    "common_phrases": [
                        "impressionable age",
                        "impressionable mind",
                        "impressionable youth",
                        "young and impressionable"
                    ],
                    "target_lang_definitions": {

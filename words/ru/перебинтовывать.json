    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "23010fc7-8e74-3a5a-b804-20b45cbc35c4",
                    "sense_definition": "Накладывать бинт заново, менять старую повязку на новую или накладывать её другим способом.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_treatment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Медсестра приходит каждое утро, чтобы <w>перебинтовывать</w> мою рану.",
                            "target_lang_translations": {
                                "en": "The nurse comes every morning to <w>re-bandage</w> my wound."
                            }
                        },
                        {
                            "text": "Врач сказал, что нужно <w>перебинтовывать</w> ногу два раза в день для быстрого заживления.",
                            "target_lang_translations": {
                                "en": "The doctor said that I need to <w>re-bandage</w> the leg twice a day for quick healing."
                            }
                        }
                    ],
                    "synonyms": [
                        "перевязывать"
                    ],
                    "antonyms": [
                        "разбинтовывать"
                    ],
                    "common_phrases": [
                        "перебинтовывать рану",
                        "перебинтовывать руку",
                        "перебинтовывать ногу",
                        "тщательно перебинтовывать"
                    ],
                    "target_lang_definitions": {
                        "en": "To apply a bandage again, to change an old dressing for a new one, or to apply it in a different way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "re-bandage",
                                "target_lang_sense_clarification": "to apply a bandage again"
                            },
                            {
                                "target_lang_word": "redress",
                                "target_lang_sense_clarification": "medical term for changing a dressing"
                            },
                            {
                                "target_lang_word": "change the dressing",
                                "target_lang_sense_clarification": "phrase indicating the replacement of bandages"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d8304c6-3602-3546-9a64-d03d699d6c07",
                    "sense_definition": "Обматывать бинтом всю поверхность чего-либо, покрывать бинтами целиком.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Боксёр начал туго <w>перебинтовывать</w> кисти рук перед выходом на ринг.",
                            "target_lang_translations": {
                                "en": "The boxer started to tightly <w>wrap</w> his hands before entering the ring."
                            }
                        },
                        {
                            "text": "Чтобы зафиксировать шину, пришлось <w>перебинтовывать</w> всю голень до самого колена.",
                            "target_lang_translations": {
                                "en": "To secure the splint, we had to <w>bandage</w> the entire shin up to the knee."
                            }
                        }
                    ],
                    "synonyms": [
                        "забинтовывать"
                    ],
                    "antonyms": [
                        "разбинтовывать"
                    ],
                    "common_phrases": [
                        "туго перебинтовывать",
                        "перебинтовывать пальцы"
                    ],
                    "target_lang_definitions": {
                        "en": "To wrap the entire surface of something with a bandage; to cover completely with bandages."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bandage",
                                "target_lang_sense_clarification": "to cover with a bandage"
                            },
                            {
                                "target_lang_word": "wrap",
                                "target_lang_sense_clarification": "to cover by winding, often used in sports contexts"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "флейм",
                                "target_lang_sense_clarification": "интернет-сленг; спор ради спора"
                            },
                            {
                                "target_lang_word": "перепалка",
                                "target_lang_sense_clarification": "словесная ссора"
                            },
                            {
                                "target_lang_word": "нападки",
                                "target_lang_sense_clarification": "агрессивная критика"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5ac9274a-2f6d-3711-aa91-2b6beecbc82e",
                    "sense_definition": "To produce flames; to burn with a flame or blaze.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fire_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dry wood <w>flamed</w> up instantly when the match touched it.",
                            "target_lang_translations": {
                                "ru": "Сухое дерево мгновенно <w>вспыхнуло</w>, когда спичка коснулась его."
                            }
                        },
                        {
                            "text": "Her cheeks <w>flamed</w> with embarrassment when she realized her mistake.",
                            "target_lang_translations": {
                                "ru": "Ее щеки <w>запылали</w> от смущения, когда она поняла свою ошибку."
                            }
                        }
                    ],
                    "synonyms": [
                        "paramour"
                    ],
                    "common_phrases": [
                        "flame up",
                        "flame out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гореть ярким пламенем; вспыхивать (также о покраснении лица от эмоций)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пылать",
                                "target_lang_sense_clarification": "гореть ярко или быть красным от жара/стыда"
                            },
                            {
                                "target_lang_word": "вспыхнуть",
                                "target_lang_sense_clarification": "внезапно загореться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "792b362a-5622-3d17-a15a-dbd447cede7d",
                    "sense_definition": "To post a destructively critical or abusive message (to somebody).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "internet_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I got <w>flamed</w> for asking a simple question on the message board.",
                            "target_lang_translations": {
                                "ru": "Меня <w>зафлеймили</w> за простой вопрос на форуме."
                            }
                        },
                        {
                            "text": "Users who <w>flame</w> others will be banned from the chat room immediately.",
                            "target_lang_translations": {
                                "ru": "Пользователи, которые <w>оскорбляют</w> других, будут немедленно забанены в чате."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Публиковать оскорбительные сообщения или агрессивно критиковать кого-то в интернете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оскорблять",
                                "target_lang_sense_clarification": "наносить обиду словами"
                            },
                            {
                                "target_lang_word": "флеймить",
                                "target_lang_sense_clarification": "интернет-сленг; разжигать ссору"
                            }
                        ]
                    }
                }
            ]
        },

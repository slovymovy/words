            "senses": [
                {
                    "sense_id": "bd59a7d1-a23a-3190-b8d3-729bb53cee3b",
                    "sense_definition": "Mający duże znaczenie, wagę lub wpływ na coś; bardzo ważny.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest bardzo <w>istotny</w> szczegół, o którym nie możemy zapomnieć.",
                            "target_lang_translations": {
                                "ru": "Это очень <w>существенная</w> деталь, о которой мы не можем забыть."
                            }
                        },
                        {
                            "text": "Jego decyzja miała <w>istotny</w> wpływ na dalsze losy całej firmy.",
                            "target_lang_translations": {
                                "ru": "Его решение оказало <w>существенное</w> влияние на дальнейшую судьбу всей фирмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "ważny",
                        "znaczący",
                        "główny",
                        "zasadniczy"
                    ],
                    "antonyms": [
                        "nieistotny",
                        "błahy",
                        "drugoplanowy"
                    ],
                    "common_phrases": [
                        "istotny wpływ",
                        "istotna rola",
                        "istotny problem",
                        "istotna różnica",
                        "istotna kwestia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большое значение, вес или влияние на что-либо; очень важный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "существенный",
                                "target_lang_sense_clarification": "значительный, весомый"
                            },
                            {
                                "target_lang_word": "важный",
                                "target_lang_sense_clarification": "имеющий большое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6db1407f-d0fd-3939-ba36-816c7a5744f7",
                    "sense_definition": "Stanowiący o istocie i naturze danej rzeczy; nierozerwalnie z nią związany, podstawowy dla jej istnienia.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "essence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wolność słowa jest <w>istotnym</w> elementem każdego demokratycznego państwa.",
                            "target_lang_translations": {
                                "ru": "Свобода слова является <w>существенным</w> элементом любого демократического государства."
                            }
                        },
                        {
                            "text": "Autor pominął w swoim raporcie najbardziej <w>istotne</w> cechy opisywanego zjawiska.",
                            "target_lang_translations": {
                                "ru": "Автор упустил в своем отчете наиболее <w>существенные</w> черты описываемого явления."
                            }
                        }
                    ],
                    "synonyms": [
                        "podstawowy",
                        "fundamentalny",
                        "zasadniczy"
                    ],
                    "antonyms": [
                        "powierzchowny",
                        "akcydentalny"
                    ],
                    "common_phrases": [
                        "warunek istotny",
                        "część istotna",
                        "cecha istotna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Составляющий сущность и природу данной вещи; неразрывно с ней связанный, основной для её существования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "существенный",
                                "target_lang_sense_clarification": "касающийся самой сущности"
                            },
                            {
                                "target_lang_word": "неотъемлемый",
                                "target_lang_sense_clarification": "обязательный, неразрывный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3b750cf-4ea1-36cd-9853-edf4074ea8e4",
                    "sense_definition": "Taki, który istnieje naprawdę; rzeczywisty, faktyczny (często używane w kontekstach prawnych lub filozoficznych).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "reality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd musiał ustalić <w>istotny</w> stan rzeczy, aby wydać sprawiedliwy wyrok.",
                            "target_lang_translations": {
                                "ru": "Суд должен был установить <w>фактическое</w> положение вещей, чтобы вынести справедливый приговор."
                            }
                        },
                        {
                            "text": "Między tymi dwiema teoriami zachodzi <w>istotna</w> i rzeczywista sprzeczność.",
                            "target_lang_translations": {
                                "ru": "Между этими двумя теориями существует <w>действительное</w> и реальное противоречие."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzeczywisty",
                        "prawdziwy",
                        "faktyczny",
                        "relewantny"
                    ],
                    "antonyms": [
                        "pozorny",
                        "rzekomy"
                    ],
                    "common_phrases": [
                        "błąd istotny"
                    ],
                    "target_lang_definitions": {

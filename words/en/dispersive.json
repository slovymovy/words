    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cbb48eaf-1ee3-3901-9a96-643365b3427b",
                    "sense_definition": "Having the capability or tendency to cause dispersion, such as separating light into colors or spreading particles.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "active_dispersion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The glass prism has a <w>dispersive</w> effect that splits white light into a rainbow.",
                            "target_lang_translations": {
                                "ru": "Стеклянная призма обладает <w>дисперсионным</w> эффектом, который расщепляет белый свет на радугу."
                            }
                        },
                        {
                            "text": "Scientists calculated the <w>dispersive</w> power of the new optical material.",
                            "target_lang_translations": {
                                "ru": "Ученые рассчитали <w>дисперсионную</w> способность нового оптического материала."
                            }
                        }
                    ],
                    "synonyms": [
                        "scattering",
                        "diffusing"
                    ],
                    "antonyms": [
                        "concentrating"
                    ],
                    "common_phrases": [
                        "dispersive power",
                        "dispersive medium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий способностью вызывать дисперсию, например, разлагать свет на спектр или рассеивать частицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дисперсионный",
                                "target_lang_sense_clarification": "относящийся к оптической дисперсии"
                            },
                            {
                                "target_lang_word": "рассеивающий",
                                "target_lang_sense_clarification": "вызывающий рассеяние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53d1699b-7460-3199-954c-0c5173db75c0",
                    "sense_definition": "Tending to break apart, dissolve, or scatter into smaller particles itself; often used to describe unstable soils.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "passive_dispersion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Building on <w>dispersive</w> soil can be dangerous because the ground erodes easily when wet.",
                            "target_lang_translations": {
                                "ru": "Строительство на <w>дисперсионных</w> грунтах может быть опасным, так как земля легко размывается при намокании."
                            }
                        },
                        {
                            "text": "The engineer identified <w>dispersive</w> clay in the sample, which required specific treatment.",
                            "target_lang_translations": {
                                "ru": "Инженер обнаружил в образце <w>дисперсионную</w> глину, которая требовала специальной обработки."
                            }
                        }
                    ],
                    "synonyms": [
                        "erodible",
                        "unstable"
                    ],
                    "antonyms": [
                        "stable",
                        "cohesive"
                    ],
                    "common_phrases": [
                        "dispersive soil",
                        "dispersive clay"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный к распаду, растворению или рассыпанию на более мелкие частицы; часто используется для описания неустойчивых грунтов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дисперсионный",
                                "target_lang_sense_clarification": "о грунтах: склонный к эрозии"
                            },
                            {
                                "target_lang_word": "диспергирующий",
                                "target_lang_sense_clarification": "распадающийся на частицы"
                            }
                        ]
                    }
                }
            ]
        }
    ]

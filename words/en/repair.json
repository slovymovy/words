                            },
                            {
                                "target_lang_word": "испра́вность",
                                "target_lang_sense_clarification": "рабочее состояние"
                            },
                            {
                                "target_lang_word": "ремо́нт",
                                "target_lang_sense_clarification": "в контексте пригодности (напр., не подлежит ремонту)"
                            }
                        ]
                    }

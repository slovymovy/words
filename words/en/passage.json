    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "109dcd60-919f-31ff-bf32-7c248ba507df",
                    "sense_definition": "A long, narrow way, such as a corridor, path, or channel, that connects one place to another.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_way",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They walked down a long, dark <w>passage</w> to reach the exit.",
                            "target_lang_translations": {
                                "ru": "Они шли по длинному темному <w>коридору</w>, чтобы добраться до выхода."
                            }
                        },
                        {
                            "text": "A secret <w>passage</w> was hidden behind the bookshelf in the library.",
                            "target_lang_translations": {
                                "ru": "За книжной полкой в библиотеке был спрятан потайной <w>ход</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "passing"
                    ],
                    "common_phrases": [
                        "narrow passage",
                        "underground passage",
                        "secret passage",
                        "dark passage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный узкий путь, такой как коридор, тропа или канал, соединяющий одно место с другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проход",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "коридор",
                                "target_lang_sense_clarification": "в здании"
                            },
                            {
                                "target_lang_word": "ход",
                                "target_lang_sense_clarification": "часто скрытый или подземный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb1dc15a-5b45-38fb-bc99-616b59741c4f",
                    "sense_definition": "A short section of a text, speech, or musical composition.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "text_music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher asked the student to read a <w>passage</w> from the novel aloud.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил ученика прочитать вслух <w>отрывок</w> из романа."
                            }
                        },
                        {
                            "text": "This musical <w>passage</w> is difficult to play because of its speed.",
                            "target_lang_translations": {
                                "ru": "Этот музыкальный <w>пассаж</w> трудно сыграть из-за его скорости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "read a passage",
                        "famous passage",
                        "opening passage",
                        "musical passage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая часть текста, речи или музыкального произведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрывок",
                                "target_lang_sense_clarification": "часть текста"
                            },
                            {
                                "target_lang_word": "пассаж",
                                "target_lang_sense_clarification": "музыкальный или риторический"
                            },
                            {
                                "target_lang_word": "фрагмент",
                                "target_lang_sense_clarification": "часть целого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b053284a-9572-3524-9856-1779ac7378ee",
                    "sense_definition": "The act of moving through, across, or over a place; the process of passing.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bridge is too low to allow the <w>passage</w> of large trucks.",
                            "target_lang_translations": {
                                "ru": "Мост слишком низкий, чтобы позволить <w>проезд</w> больших грузовиков."
                            }
                        },
                        {
                            "text": "They were granted safe <w>passage</w> through the enemy territory.",
                            "target_lang_translations": {
                                "ru": "Им был предоставлен безопасный <w>проход</w> через вражескую территорию."
                            }
                        }
                    ],
                    "synonyms": [
                        "passing"
                    ],
                    "common_phrases": [
                        "safe passage",
                        "right of passage",
                        "force a passage",
                        "bird of passage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие перемещения через, сквозь или поверх места; процесс прохождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проход",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "проезд",
                                "target_lang_sense_clarification": "на транспорте"
                            },
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "пересечение пространства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86a547e8-657d-37a6-bb88-c297b8699ba0",
                    "sense_definition": "The continuous progress or advance of time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her memories faded with the <w>passage</w> of time.",
                            "target_lang_translations": {
                                "ru": "Её воспоминания поблекли с <w>течением</w> времени."
                            }
                        },
                        {
                            "text": "Despite the <w>passage</w> of years, the house remained exactly the same.",
                            "target_lang_translations": {
                                "ru": "Несмотря на <w>прошествие</w> лет, дом остался точно таким же."
                            }
                        }
                    ],
                    "synonyms": [
                        "passing"
                    ],
                    "common_phrases": [
                        "passage of time",
                        "with the passage of years"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непрерывный прогресс или течение времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "течение",
                                "target_lang_sense_clarification": "времени"
                            },
                            {
                                "target_lang_word": "ход",
                                "target_lang_sense_clarification": "развитие событий или времени"
                            },
                            {
                                "target_lang_word": "прошествие",
                                "target_lang_sense_clarification": "истечение срока"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3c91e73-fe54-3b0d-9943-ec61c4dd292e",
                    "sense_definition": "The official approval of a law or bill by a legislative body.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "law",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government celebrated the <w>passage</w> of the new education bill.",
                            "target_lang_translations": {
                                "ru": "Правительство отпраздновало <w>принятие</w> нового законопроекта об образовании."
                            }
                        },
                        {
                            "text": "Lobbyists worked hard to ensure the speedy <w>passage</w> of the act.",
                            "target_lang_translations": {
                                "ru": "Лоббисты усердно работали, чтобы обеспечить скорое <w>принятие</w> закона."
                            }
                        }
                    ],
                    "common_phrases": [
                        "passage of the bill",
                        "speedy passage",
                        "secure passage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное утверждение закона или законопроекта законодательным органом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принятие",
                                "target_lang_sense_clarification": "закона или решения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3cfab13-3375-3f5d-81d0-492fbff8f811",
                    "sense_definition": "A journey, especially by sea, or the right to travel as a passenger on a ship.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "journey",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He worked on the ship to pay for his <w>passage</w> to America.",
                            "target_lang_translations": {
                                "ru": "Он работал на корабле, чтобы оплатить свой <w>проезд</w> в Америку."
                            }
                        },
                        {
                            "text": "She booked <w>passage</w> on the first steamer leaving the port.",
                            "target_lang_translations": {
                                "ru": "Она забронировала <w>место</w> на первом пароходе, покидающем порт."
                            }
                        }
                    ],
                    "common_phrases": [
                        "book passage",
                        "work one's passage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествие, особенно по морю, или право на проезд в качестве пассажира на корабле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проезд",
                                "target_lang_sense_clarification": "право на путешествие"
                            },
                            {
                                "target_lang_word": "рейс",
                                "target_lang_sense_clarification": "путешествие"
                            },
                            {
                                "target_lang_word": "плавание",
                                "target_lang_sense_clarification": "морское путешествие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71ae1f3b-bd71-3004-8bf2-2af2e6293792",
                    "sense_definition": "A duct or channel in the body through which fluids or air can pass.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cold caused a blockage in his nasal <w>passages</w>.",
                            "target_lang_translations": {
                                "ru": "Простуда вызвала закупорку в его носовых <w>ходах</w>."
                            }
                        },
                        {
                            "text": "Air flows through the <w>passage</w> to reach the lungs.",
                            "target_lang_translations": {
                                "ru": "Воздух течет через <w>проход</w>, чтобы достичь легких."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nasal passage",
                        "air passage"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "anatomy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проток или канал в теле, через который могут проходить жидкости или воздух."

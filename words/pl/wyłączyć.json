    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1ba9a06b-6ac0-3242-b8a6-65eb2571e924",
                    "sense_definition": "Przerwać działanie urządzenia, mechanizmu lub dopływ energii (np. prądu, gazu, wody); sprawić, że coś przestaje funkcjonować.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "deactivation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pamiętaj, aby <w>wyłączyć</w> światło, kiedy wychodzisz z pokoju.",
                            "target_lang_translations": {
                                "ru": "Не забудь <w>выключить</w> свет, когда выходишь из комнаты."
                            }
                        },
                        {
                            "text": "Muszę <w>wyłączyć</w> telefon, ponieważ wchodzę na ważne spotkanie.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>выключить</w> телефон, так как иду на важную встречу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdezaktywować"
                    ],
                    "antonyms": [
                        "włączyć"
                    ],
                    "common_phrases": [
                        "wyłączyć światło",
                        "wyłączyć komputer",
                        "wyłączyć silnik",
                        "wyłączyć telefon",
                        "wyłączyć zasilanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекратить работу устройства, механизма или подачу энергии (например, электричества, газа, воды); сделать так, чтобы что-то перестало функционировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выключить",
                                "target_lang_sense_clarification": "прервать работу устройства или механизма"
                            },
                            {
                                "target_lang_word": "отключить",
                                "target_lang_sense_clarification": "отсоединить от питания или подачи ресурсов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb02106f-3cfa-3d51-b284-27af87b96058",
                    "sense_definition": "Pominąć kogoś lub coś w planach, działaniach lub grupie; uznać, że coś nie należy do zbioru lub nie bierze udziału w czymś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "exclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poważna kontuzja kolana musiała go <w>wyłączyć</w> z gry na cały sezon.",
                            "target_lang_translations": {
                                "ru": "Серьезная травма колена заставила его <w>выбыть</w> из игры на весь сезон."
                            }
                        },
                        {
                            "text": "Musimy <w>wyłączyć</w> ten wadliwy produkt z dalszej sprzedaży.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>исключить</w> этот бракованный товар из дальнейшей продажи."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykluczyć"
                    ],
                    "antonyms": [
                        "włączyć"
                    ],
                    "common_phrases": [
                        "wyłączyć z użytku",
                        "wyłączyć z gry",
                        "wyłączyć z postępowania",
                        "wyłączyć emocje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исключить кого-либо или что-либо из планов, действий или группы; посчитать, что что-то не принадлежит к набору или не участвует в чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исключить",
                                "target_lang_sense_clarification": "удалить из состава, списка или группы"
                            },
                            {
                                "target_lang_word": "выключить",
                                "target_lang_sense_clarification": "удалить из процесса или деятельности (реже)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d7636ba-adee-3804-80b2-fe5b187647af",
                    "sense_definition": "Przestać zwracać uwagę na otoczenie, stracić koncentrację lub świadomie odciąć się od bodźców zewnętrznych (często używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czasami po pracy lubię się <w>wyłączyć</w> i po prostu posłuchać muzyki.",
                            "target_lang_translations": {
                                "ru": "Иногда после работы я люблю <w>отключиться</w> и просто послушать музыку."
                            }
                        },
                        {
                            "text": "Był tak zmęczony, że jego umysł zaczął się <w>wyłączyć</w> w trakcie wykładu.",
                            "target_lang_translations": {
                                "ru": "Он был так утомлен, что его разум начал <w>отключаться</w> во время лекции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyłączyć się z rozmowy",
                        "wyłączyć myślenie",
                        "całkowicie się wyłączyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать обращать внимание на окружающее, потерять концентрацию или сознательно отстраниться от внешних раздражителей (часто используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отключиться",
                                "target_lang_sense_clarification": "перестать реагировать на внешние стимулы (разг.)"
                            },
                            {
                                "target_lang_word": "отвлечься",
                                "target_lang_sense_clarification": "переключить внимание, расслабиться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кадка",
                                "target_lang_sense_clarification": "деревянная емкость"
                            },
                            {
                                "target_lang_word": "корзина",
                                "target_lang_sense_clarification": "плетеная емкость для переноски"
                            },
                            {

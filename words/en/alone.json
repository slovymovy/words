    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "de701d9d-7063-39f6-9d98-a237e985b52c",
                    "sense_definition": "Separated from others; solitary; not having anyone else present.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "solitary_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not like being <w>alone</w> in the house at night.",
                            "target_lang_translations": {
                                "ru": "Я не люблю оставаться <w>один</w> в доме по ночам."
                            }
                        },
                        {
                            "text": "She prefers to be <w>alone</w> when she is working.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает быть <w>одна</w>, когда работает."
                            }
                        }
                    ],
                    "synonyms": [
                        "solitarily",
                        "solo"
                    ],
                    "antonyms": [
                        "together"
                    ],
                    "common_phrases": [
                        "all alone",
                        "feel alone",
                        "leave alone"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "used after a verb, not before a noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельно от других; в одиночестве; без присутствия кого-либо ещё."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "один",
                                "target_lang_sense_clarification": "без других людей"
                            },
                            {
                                "target_lang_word": "одинокий",
                                "target_lang_sense_clarification": "уединенный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbee48e1-17ab-3724-8580-7f02a06a57fb",
                    "sense_definition": "Lacking peers who share one's beliefs, experiences, or situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "shared_experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are not <w>alone</w> in feeling overwhelmed by the news.",
                            "target_lang_translations": {
                                "ru": "Вы не <w>одиноки</w> в чувстве подавленности от новостей."
                            }
                        },
                        {
                            "text": "Am I <w>alone</w> in thinking this plan is dangerous?",
                            "target_lang_translations": {
                                "ru": "Я <w>один</w> думаю, что этот план опасен?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "not alone in"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "used after a verb, not before a noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий единомышленников в данной ситуации или убеждениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "один",
                                "target_lang_sense_clarification": "единственный в своем роде"
                            },
                            {
                                "target_lang_word": "одинокий",
                                "target_lang_sense_clarification": "не имеющий поддержки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "84d33a50-a903-3de7-a48e-049654cc4231",
                    "sense_definition": "By oneself; without other people present.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "solitary_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She walked home <w>alone</w> after the party.",
                            "target_lang_translations": {
                                "ru": "Она шла домой <w>одна</w> после вечеринки."
                            }
                        },
                        {
                            "text": "He has lived <w>alone</w> for many years.",
                            "target_lang_translations": {
                                "ru": "Он жил <w>один</w> много лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "by oneself",
                        "solo",
                        "on one's own",
                        "by one's lonesome"
                    ],
                    "antonyms": [
                        "together"
                    ],
                    "common_phrases": [
                        "live alone",
                        "travel alone"
                    ],
                    "target_lang_definitions": {
                        "ru": "В одиночку; без присутствия других людей."
                    },
                    "translations": {
                        "ru": [

                },
                {
                    "sense_id": "e6537dae-933d-3e0d-9929-34cf5a63926a",
                    "sense_definition": "A group of people with extreme or unconventional views, existing on the periphery of a larger community.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "periphery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "These radical ideas are supported only by a small <w>fringe</w> of the party.",
                            "target_lang_translations": {
                                "ru": "Эти радикальные идеи поддерживает лишь небольшая <w>маргинальная часть</w> партии."
                            }
                        },
                        {
                            "text": "The movement started as a <w>fringe</w> group but has gained mainstream attention.",
                            "target_lang_translations": {
                                "ru": "Движение начиналось как <w>маргинальная</w> группа, но привлекло внимание широкой общественности."
                            }
                        }
                    ],
                    "antonyms": [
                        "mainstream"
                    ],
                    "common_phrases": [
                        "lunatic fringe",
                        "fringe group"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей с крайними или нетрадиционными взглядами, существующая на периферии большего сообщества."
                    },
                    "translations": {
                        "ru": [

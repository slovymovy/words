    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "228ecea4-712d-3fd9-8b3b-fe9c4822d115",
                    "sense_definition": "The process of bringing something to an end or the state of being ended.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_ending",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The contract allows for early <w>termination</w> if specific conditions are not met.",
                            "target_lang_translations": {
                                "ru": "Контракт предусматривает досрочное <w>расторжение</w>, если определенные условия не соблюдены."
                            }
                        },
                        {
                            "text": "We were surprised by the sudden <w>termination</w> of the project after months of hard work.",
                            "target_lang_translations": {
                                "ru": "Мы были удивлены внезапным <w>прекращением</w> проекта после месяцев упорной работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "end",
                        "conclusion",
                        "discontinuation",
                        "stoppage",
                        "close",
                        "finish",
                        "stop"
                    ],
                    "antonyms": [
                        "continuation"
                    ],
                    "common_phrases": [
                        "contract termination",
                        "early termination",
                        "termination date",
                        "notice of termination"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс доведения чего-либо до конца или состояние завершенности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прекращение",
                                "target_lang_sense_clarification": "действия или процесса"
                            },
                            {
                                "target_lang_word": "расторжение",
                                "target_lang_sense_clarification": "договора или контракта"
                            },
                            {
                                "target_lang_word": "завершение",
                                "target_lang_sense_clarification": "окончание работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c61d4bc1-878a-3df0-b111-3c0e26e88091",
                    "sense_definition": "The act of ending a person's employment at a company.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "employment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He received a formal letter confirming his <w>termination</w> from the company effective immediately.",
                            "target_lang_translations": {
                                "ru": "Он получил официальное письмо, подтверждающее его <w>увольнение</w> из компании, вступающее в силу немедленно."
                            }
                        },
                        {
                            "text": "The employee filed a lawsuit claiming wrongful <w>termination</w> without just cause.",
                            "target_lang_translations": {
                                "ru": "Сотрудник подал иск, заявляя о незаконном <w>увольнении</w> без уважительной причины."
                            }
                        }
                    ],
                    "synonyms": [
                        "dismissal",
                        "discharge"
                    ],
                    "common_phrases": [
                        "wrongful termination",
                        "termination of employment",
                        "termination for cause",
                        "termination letter"
                    ],
                    "target_lang_definitions": {

                            {
                                "target_lang_word": "полное комбо",
                                "target_lang_sense_clarification": "перевод термина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b3e22d3-43c4-333f-a01b-27653fff7cec",

            "senses": [
                {
                    "sense_id": "1d2e0721-38af-3f99-866f-a51226490bdc",
                    "sense_definition": "Present participle and gerund of <w>fleck</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist was <w>flecking</w> the canvas with blue paint to create a textured sky.",
                            "target_lang_translations": {
                                "ru": "Художник <w>покрывал брызгами</w> холст синей краской, чтобы создать текстурное небо."
                            }
                        },
                        {
                            "text": "Foam was <w>flecking</w> the horse's mouth after the long race.",
                            "target_lang_translations": {
                                "ru": "Пена <w>покрывала хлопьями</w> рот лошади после долгой скачки."
                            }
                        }
                    ],
                    "synonyms": [
                        "spotting",
                        "speckling",
                        "dotting",
                        "dappling"
                    ],
                    "common_phrases": [
                        "flecking with paint",
                        "flecking the surface"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола <w>fleck</w> (покрывать пятнами, испещрять)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрывать пятнами",
                                "target_lang_sense_clarification": "наносить пятна или отметины"
                            },
                            {
                                "target_lang_word": "испещрять",
                                "target_lang_sense_clarification": "делать пестрым, покрывая мелкими знаками или пятнами"
                            },
                            {
                                "target_lang_word": "брызгать",
                                "target_lang_sense_clarification": "разбрызгивать краску или жидкость мелкими каплями"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ac24c5f-612d-36e9-95e1-36f5dfee61a2",
                    "sense_definition": "A pattern or arrangement of small spots or marks.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The geologist examined the granite for any <w>flecking</w> of mica.",
                            "target_lang_translations": {
                                "ru": "Геолог исследовал гранит на наличие каких-либо <w>вкраплений</w> слюды."
                            }
                        },
                        {
                            "text": "There is a slight <w>flecking</w> of grey in his beard now.",
                            "target_lang_translations": {
                                "ru": "Теперь в его бороде есть небольшие <w>вкрапления</w> седины."
                            }
                        }
                    ],
                    "synonyms": [
                        "mottling",
                        "speckling",
                        "stippling"
                    ],
                    "common_phrases": [
                        "gold flecking",
                        "white flecking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узор или расположение мелких пятен или отметин; наличие вкраплений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкрапления",
                                "target_lang_sense_clarification": "наличие посторонних мелких частиц или пятен"
                            },
                            {
                                "target_lang_word": "крапчатость",
                                "target_lang_sense_clarification": "свойство поверхности, покрытой мелкими пятнышками"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0b810b03-c90e-3538-ab29-70dd354e7e01",
                    "sense_definition": "Tytuł grzecznościowy używany wobec rabina, nauczyciela religijnego lub przywódcy duchowego w judaizmie, szczególnie w chasydyzmie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religious_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chasydzki <w>rebe</w> miał w środowisku gminy pozycję dominującą.",
                            "target_lang_translations": {
                                "ru": "Хасидский <w>ребе</w> занимал доминирующее положение в общине."
                            }
                        },
                        {
                            "text": "Późno wieczorem <w>rebe</w> zawołał do siebie szamesa i kazał sobie powtórzyć nowiny.",
                            "target_lang_translations": {
                                "ru": "Поздно вечером <w>ребе</w> позвал к себе шамеса и велел повторить новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "rabin"
                    ],
                    "common_phrases": [
                        "mądry rebe",
                        "stary rebe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Почтительный титул, используемый по отношению к раввину, религиозному учителю или духовному лидеру в иудаизме, особенно в хасидизме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ребе",
                                "target_lang_sense_clarification": "титул в иудаизме"
                            },
                            {
                                "target_lang_word": "раввин",
                                "target_lang_sense_clarification": "духовный лидер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "490f0331-09f2-3d99-9ea6-e61572ea97a1",
                    "sense_definition": "W gwarze przestępczej: sędzia orzekający w sprawach karnych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więźniowie szeptali, że ten <w>rebe</w> jest wyjątkowo surowy dla złodziei.",
                            "target_lang_translations": {
                                "ru": "Заключенные шептались, что этот <w>судья</w> исключительно суров к ворам."
                            }
                        },
                        {
                            "text": "Oskarżony miał nadzieję, że <w>rebe</w> uwzględni okoliczności łagodzące.",
                            "target_lang_translations": {
                                "ru": "Обвиняемый надеялся, что <w>судья</w> учтет смягчающие обстоятельства."
                            }
                        }
                    ],
                    "synonyms": [
                        "sędzia"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gwara więzienna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "На воровском жаргоне: судья, выносящий приговор по уголовным делам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судья",
                                "target_lang_sense_clarification": "значение на жаргоне"
                            }
                        ]
                    }
                }
            ]
        }
    ]

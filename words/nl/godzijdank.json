    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f06d6a92-c048-31c9-b20a-7c734c5521aa",
                    "sense_definition": "Gelukkig; gebruikt om aan te geven dat men blij en opgelucht is over een bepaalde situatie of afloop.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relief_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben <w>godzijdank</w> nog net de laatste trein naar huis kunnen halen.",
                            "target_lang_translations": {
                                "en": "We <w>thankfully</w> just managed to catch the last train home.",
                                "ru": "Мы, <w>слава богу</w>, как раз успели на последний поезд домой."
                            }
                        },
                        {
                            "text": "Het begon hard te regenen, maar ik had <w>godzijdank</w> mijn paraplu bij me.",
                            "target_lang_translations": {
                                "en": "It started raining hard, but <w>thank God</w> I had my umbrella with me.",
                                "ru": "Начался сильный дождь, но у меня, <w>слава богу</w>, был с собой зонт."
                            }
                        }
                    ],
                    "synonyms": [
                        "gelukkig",
                        "goddank"
                    ],
                    "antonyms": [
                        "helaas",
                        "jammer genoeg"
                    ],
                    "common_phrases": [
                        "godzijdank wel",
                        "godzijdank niet",
                        "maar godzijdank"
                    ],
                    "target_lang_definitions": {
                        "en": "Fortunately; used to indicate that one is happy and relieved about a certain situation or outcome.",
                        "ru": "К счастью; используется для выражения радости и облегчения по поводу определенной ситуации или исхода."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thankfully",
                                "target_lang_sense_clarification": "expressing relief regarding a situation"
                            },
                            {
                                "target_lang_word": "fortunately",
                                "target_lang_sense_clarification": "indicating good luck"
                            },
                            {
                                "target_lang_word": "thank God",
                                "target_lang_sense_clarification": "strong expression of relief used adverbially"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "слава богу",
                                "target_lang_sense_clarification": "выражение облегчения"
                            },
                            {
                                "target_lang_word": "к счастью",
                                "target_lang_sense_clarification": "вводное слово"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "b1296ea6-e9a7-39ba-9c80-9f2881661c8f",
                    "sense_definition": "Een uitroep van grote opluchting of dankbaarheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relief_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Godzijdank</w>, iedereen is veilig uit het brandende gebouw gekomen.",
                            "target_lang_translations": {
                                "en": "<w>Thank God</w>, everyone got out of the burning building safely.",
                                "ru": "<w>Слава богу</w>, все выбрались из горящего здания целыми и невредимыми."
                            }
                        },
                        {
                            "text": "Is de operatie goed gegaan? <w>Godzijdank</w>!",
                            "target_lang_translations": {
                                "en": "Did the operation go well? <w>Thank goodness</w>!",
                                "ru": "Операция прошла хорошо? <w>Слава богу</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "goddank",
                        "gelukkig"
                    ],
                    "antonyms": [
                        "helaas"
                    ],
                    "common_phrases": [
                        "o godzijdank",
                        "ja godzijdank"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation of great relief or gratitude.",
                        "ru": "Возглас огромного облегчения или благодарности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thank God",
                                "target_lang_sense_clarification": "common exclamation of relief"
                            },
                            {
                                "target_lang_word": "thank goodness",
                                "target_lang_sense_clarification": "milder exclamation of relief"
                            },
                            {
                                "target_lang_word": "thank heavens",
                                "target_lang_sense_clarification": "idiomatic exclamation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "слава богу",

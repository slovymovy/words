        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fd343c63-b7a6-3a15-953a-127b94ff76c6",
                    "sense_definition": "Een met lucht gevulde rubberen ring om het wiel van een voertuig.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die auto heeft een lekke <w>band</w>.",
                            "target_lang_translations": {
                                "ru": "У этой машины спущена <w>шина</w>."
                            }
                        },
                        {
                            "text": "Je moet de <w>banden</w> van je fiets regelmatig oppompen.",
                            "target_lang_translations": {
                                "ru": "Нужно регулярно накачивать <w>шины</w> своего велосипеда."
                            }
                        }
                    ],
                    "synonyms": [
                        "autoband",
                        "fietsband",
                        "buitenband"
                    ],
                    "common_phrases": [
                        "lekke band",
                        "banden wisselen",
                        "zachte band"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наполненное воздухом резиновое кольцо вокруг колеса транспортного средства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шина",
                                "target_lang_sense_clarification": "резиновая оболочка колеса"

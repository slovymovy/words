            "senses": [
                {
                    "sense_id": "88f8b64c-e31c-3108-9e11-8f19498a4ba9",
                    "sense_definition": "Het opzettelijk en voortijdig beëindigen van een zwangerschap door medisch ingrijpen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medical_procedure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij besloot na lang nadenken een <w>abortus</w> te laten plegen.",
                            "target_lang_translations": {
                                "en": "She decided to have an <w>abortion</w> after long consideration.",
                                "ru": "После долгих раздумий она решила сделать <w>аборт</w>."
                            }
                        },
                        {
                            "text": "In veel landen is het recht op <w>abortus</w> een belangrijk politiek onderwerp.",
                            "target_lang_translations": {
                                "en": "In many countries, the right to <w>abortion</w> is an important political issue.",
                                "ru": "Во многих странах право на <w>аборт</w> является важным политическим вопросом."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwangerschapsafbreking",
                        "zwangerschapsonderbreking",
                        "vruchtafdrijving",
                        "abortus provocatus"
                    ],
                    "common_phrases": [
                        "abortus plegen",
                        "recht op abortus",
                        "illegale abortus"
                    ],
                    "target_lang_definitions": {
                        "en": "The intentional and premature termination of a pregnancy through medical intervention.",
                        "ru": "Преднамеренное и преждевременное прерывание беременности путем медицинского вмешательства."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "abortion",
                                "target_lang_sense_clarification": "induced termination of pregnancy"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "аборт",
                                "target_lang_sense_clarification": "искусственное прерывание беременности"
                            },
                            {
                                "target_lang_word": "прерывание беременности",
                                "target_lang_sense_clarification": "формальный/медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4efe511c-44e5-3301-8be7-ba9250b5e83c",
                    "sense_definition": "Het spontaan en onbedoeld eindigen van een zwangerschap voordat de vrucht levensvatbaar is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "medical_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts stelde vast dat er sprake was van een spontane <w>abortus</w>.",
                            "target_lang_translations": {
                                "en": "The doctor determined that it was a case of spontaneous <w>abortion</w>.",
                                "ru": "Врач установил, что произошел самопроизвольный <w>аборт</w>."
                            }
                        },
                        {
                            "text": "Een <w>abortus</w> wordt vaak veroorzaakt door ernstige afwijkingen bij de vrucht.",
                            "target_lang_translations": {
                                "en": "A <w>miscarriage</w> is often caused by serious abnormalities in the foetus.",
                                "ru": "<w>Выкидыш</w> часто вызывается серьезными аномалиями плода."
                            }
                        }
                    ],
                    "synonyms": [
                        "miskraam"
                    ],
                    "common_phrases": [
                        "spontane abortus"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "medical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The spontaneous and unintentional ending of a pregnancy before the foetus is viable.",
                        "ru": "Спонтанное и непреднамеренное окончание беременности до того, как плод станет жизнеспособным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "miscarriage",
                                "target_lang_sense_clarification": "spontaneous loss of pregnancy"
                            },
                            {
                                "target_lang_word": "abortion",
                                "target_lang_sense_clarification": "medical term for miscarriage"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выкидыш",
                                "target_lang_sense_clarification": "самопроизвольная потеря беременности"
                            },
                            {
                                "target_lang_word": "самопроизвольный аборт",

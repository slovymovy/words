    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "94242a57-9e48-3a2f-a102-742b90bec3f7",
                    "sense_definition": "To be in an upright position on one's feet.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_posture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>stand</w> over there by the window.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>встаньте</w> вон там у окна."
                            }
                        },
                        {
                            "text": "He <w>stood</w> quietly in the hallway waiting for his name to be called.",
                            "target_lang_translations": {
                                "ru": "Он тихо <w>стоял</w> в коридоре, ожидая, когда назовут его имя."
                            }
                        }
                    ],
                    "synonyms": [
                        "be upright"
                    ],
                    "antonyms": [
                        "sit",
                        "lie"
                    ],
                    "common_phrases": [
                        "stand still",
                        "stand tall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в вертикальном положении на ногах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоять",
                                "target_lang_sense_clarification": "находиться вертикально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7cabd96-5cfd-3b3b-b9b0-6e254b170839",
                    "sense_definition": "To rise to an upright position on one's feet.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_posture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Everyone <w>stood</w> up when the teacher entered the classroom.",
                            "target_lang_translations": {
                                "ru": "Все <w>встали</w>, когда учитель вошел в класс."
                            }
                        },
                        {
                            "text": "You should <w>stand</w> to greet the guests.",
                            "target_lang_translations": {
                                "ru": "Тебе следует <w>встать</w>, чтобы поприветствовать гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "rise",
                        "get up"
                    ],
                    "antonyms": [
                        "sit down"
                    ],
                    "common_phrases": [
                        "stand up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подниматься в вертикальное положение на ноги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вставать",
                                "target_lang_sense_clarification": "подниматься"
                            },
                            {
                                "target_lang_word": "встать",
                                "target_lang_sense_clarification": "подняться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a00d8d14-6720-3094-9e4d-ed412e8e4740",
                    "sense_definition": "To dislike something or someone very much; to tolerate or endure.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tolerance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I can't <w>stand</w> the noise from the construction site anymore.",
                            "target_lang_translations": {
                                "ru": "Я больше не могу <w>терпеть</w> шум со стройки."
                            }
                        },
                        {
                            "text": "She loves the city, but she cannot <w>stand</w> the traffic.",
                            "target_lang_translations": {
                                "ru": "Она любит город, но не <w>выносит</w> пробки."
                            }
                        }
                    ],
                    "synonyms": [
                        "bear",
                        "endure",
                        "tolerate"
                    ],
                    "common_phrases": [
                        "can't stand",
                        "hard to stand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно не любить что-то или кого-то; терпеть или выносить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терпеть",
                                "target_lang_sense_clarification": "мириться с чем-то"
                            },
                            {
                                "target_lang_word": "выносить",
                                "target_lang_sense_clarification": "выдерживать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a88d06f-643d-34aa-8bb1-e93b27d2ecda",
                    "sense_definition": "To be located or situated in a particular place.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old castle <w>stands</w> on top of a steep hill.",
                            "target_lang_translations": {
                                "ru": "Старый замок <w>стоит</w> на вершине крутого холма."
                            }
                        },
                        {
                            "text": "A large oak tree <w>stands</w> in the middle of the field.",
                            "target_lang_translations": {
                                "ru": "Большой дуб <w>стоит</w> посреди поля."
                            }
                        }
                    ],
                    "synonyms": [
                        "be situated",
                        "be located"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться или быть расположенным в определенном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоять",
                                "target_lang_sense_clarification": "быть расположенным"
                            },
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "быть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c05ba8fe-9f43-3ad5-be10-f0e816d0b9a3",
                    "sense_definition": "To place something in an upright or vertical position.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>stood</w> the ladder against the wall before climbing up.",
                            "target_lang_translations": {
                                "ru": "Он <w>поставил</w> лестницу к стене, прежде чем залезть наверх."
                            }
                        },
                        {
                            "text": "Please <w>stand</w> the bottle on the table carefully.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, аккуратно <w>поставь</w> бутылку на стол."
                            }
                        }
                    ],
                    "synonyms": [
                        "set",
                        "place",
                        "position"
                    ],
                    "antonyms": [
                        "lay"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать что-либо в вертикальное положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставить",
                                "target_lang_sense_clarification": "помещать вертикально"
                            },
                            {
                                "target_lang_word": "поставить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2609ea22-445c-3f73-9d02-fb77f9724bc4",
                    "sense_definition": "To successfully resist or survive a difficult situation or test.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "tolerance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This classic novel has <w>stood</w> the test of time.",
                            "target_lang_translations": {
                                "ru": "Этот классический роман <w>выдержал</w> испытание временем."
                            }
                        },
                        {
                            "text": "The old bridge <w>stood</w> the force of the flood without collapsing.",
                            "target_lang_translations": {
                                "ru": "Старый мост <w>выстоял</w> под напором наводнения, не обрушившись."
                            }
                        }
                    ],
                    "synonyms": [
                        "withstand",
                        "survive",
                        "resist"
                    ],
                    "antonyms": [
                        "succumb",
                        "fail"
                    ],
                    "common_phrases": [
                        "stand the test",
                        "stand a chance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Успешно сопротивляться или пережить трудную ситуацию или испытание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдерживать",
                                "target_lang_sense_clarification": "справляться"
                            },
                            {
                                "target_lang_word": "выстоять",
                                "target_lang_sense_clarification": "устоять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f38798d1-b6ee-35d9-ae73-29b5687d092c",
                    "sense_definition": "To remain valid, effective, or unaltered.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "validity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My offer to help you still <w>stands</w> if you change your mind.",
                            "target_lang_translations": {
                                "ru": "Мое предложение помочь тебе все еще <w>в силе</w>, если ты передумаешь."
                            }
                        },
                        {
                            "text": "The world record <w>stood</w> for ten years before it was broken.",
                            "target_lang_translations": {
                                "ru": "Мировой рекорд <w>держался</w> десять лет, прежде чем был побит."
                            }
                        }
                    ],
                    "synonyms": [
                        "remain",
                        "hold",
                        "persist"
                    ],
                    "common_phrases": [
                        "offer stands"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставаться действительным, эффективным или неизменным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставаться в силе",
                                "target_lang_sense_clarification": "быть действительным"
                            },
                            {
                                "target_lang_word": "действовать",
                                "target_lang_sense_clarification": "быть актуальным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb075ff2-9d88-3c1e-9d26-4585db55d307",
                    "sense_definition": "To be in a position where one is likely to do, gain, or lose something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "likelihood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company <w>stands</w> to make a significant profit this quarter.",
                            "target_lang_translations": {
                                "ru": "Компания <w>имеет шансы</w> получить значительную прибыль в этом квартале."
                            }
                        },
                        {
                            "text": "If we are not careful, we <w>stand</w> to lose everything we have built.",
                            "target_lang_translations": {
                                "ru": "Если мы не будем осторожны, мы <w>рискуем</w> потерять все, что построили."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stand to lose",
                        "stand to gain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть в положении, когда вероятно что-то сделать, получить или потерять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь шансы",
                                "target_lang_sense_clarification": "быть вероятным"
                            },
                            {
                                "target_lang_word": "рисковать",
                                "target_lang_sense_clarification": "быть под угрозой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b18e122e-dff1-31d5-91e2-5b7699834eac",
                    "sense_definition": "To be a candidate in an election.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to <w>stand</w> for parliament in the next election.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>баллотироваться</w> в парламент на следующих выборах."
                            }
                        },
                        {
                            "text": "Three candidates will <w>stand</w> for the position of mayor.",
                            "target_lang_translations": {
                                "ru": "Три кандидата будут <w>баллотироваться</w> на пост мэра."
                            }
                        }
                    ],
                    "synonyms": [
                        "run"
                    ],
                    "common_phrases": [
                        "stand for election"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть кандидатом на выборах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баллотироваться",
                                "target_lang_sense_clarification": "выдвигаться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "84dae0a1-f42b-3950-bd26-bed5ba0115f1",
                    "sense_definition": "A small shop, booth, or stall, often outdoors, where goods are sold.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bought some fresh fruit at a roadside <w>stand</w>.",
                            "target_lang_translations": {
                                "ru": "Мы купили свежие фрукты в придорожном <w>ларьке</w>."

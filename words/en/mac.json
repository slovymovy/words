            "senses": [
                {
                    "sense_id": "5ec79c4c-38f3-39b8-afe9-37370c6a2ca6",
                    "sense_definition": "A nickname for a person named <w>Max</w> or having a surname beginning with Mac- or Mc-.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Hey <w>Mac</w>, are you coming to the football game later?",
                            "target_lang_translations": {
                                "ru": "Эй, <w>Мак</w>, ты пойдешь позже на футбол?"
                            }
                        },
                        {
                            "text": "Everyone calls him <w>Mac</w> because his last name is MacDonald.",
                            "target_lang_translations": {
                                "ru": "Все зовут его <w>Мак</w>, потому что его фамилия Макдональд."
                            }
                        }
                    ],
                    "synonyms": [
                        "Macca"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прозвище человека по имени Макс или с фамилией, начинающейся на Мак-/Мак-."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мак",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f2e49b18-7f42-3ba2-bf8c-a7f8b999a625",
                    "sense_definition": "Uporczywie krytykować kogoś, wytykać błędy lub mieć pretensje o drobiazgi (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "criticism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel ciągle <czepia> się mojego charakteru pisma.",
                            "target_lang_translations": {
                                "ru": "Учитель постоянно <придирается> к моему почерку."
                            }
                        },
                        {
                            "text": "Dlaczego musisz się <czepiać> o takie małe błędy?",
                            "target_lang_translations": {
                                "ru": "Почему ты должен <цепляться> к таким мелким ошибкам?"
                            }
                        }
                    ],
                    "synonyms": [
                        "krytykować",
                        "szukać dziury w całym",
                        "przyczepiać się",
                        "atakować"
                    ],
                    "common_phrases": [
                        "czepiać się słówek",
                        "czepiać się szczegółów",
                        "czepiać się o byle co"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво критиковать кого-то, указывать на ошибки или выражать недовольство по мелочам (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "придираться",
                                "target_lang_sense_clarification": "критиковать по мелочам"
                            },
                            {
                                "target_lang_word": "цепляться",
                                "target_lang_sense_clarification": "разг. приставать с упреками"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "966bc8c8-d2dc-34a6-967d-ec2cd794a8eb",
                    "sense_definition": "Chwytać coś mocno rękami lub czymś innym, aby się przytrzymać lub nie spaść (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestraszone dziecko kurczowo <czepiało> się spódnicy matki.",
                            "target_lang_translations": {
                                "ru": "Испуганный ребёнок судорожно <цеплялся> за юбку матери."
                            }
                        },
                        {
                            "text": "Małpa zwinnie <czepia> się gałęzi drzewa, skacząc po lesie.",
                            "target_lang_translations": {
                                "ru": "Обезьяна ловко <цепляется> за ветки дерева, прыгая по лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "chwytać się",
                        "przytrzymywać się",
                        "lgnąć"
                    ],
                    "common_phrases": [
                        "czepiać się poręczy",
                        "czepiać się klamki",
                        "czepiać się rękawa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крепко хватать что-то руками или чем-то иным, чтобы удержаться или не упасть (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цепляться",
                                "target_lang_sense_clarification": "держаться за что-то"
                            },
                            {
                                "target_lang_word": "хвататься",
                                "target_lang_sense_clarification": "ловить рукой для опоры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90f1e731-cc67-3a0d-ab16-dcdc23c352ba",
                    "sense_definition": "Kurczowo trzymać się jakiejś myśli, nadziei lub możliwości, próbując coś ocalić (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trudnych chwilach człowiek często <czepia> się wiary.",
                            "target_lang_translations": {
                                "ru": "В трудные минуты человек часто <цепляется> за веру."
                            }
                        },
                        {
                            "text": "Tonący brzytwy się <czepia>, szukając ratunku w każdej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Утопающий и за бритву <хватается> в поисках спасения в любой ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "chwytać się"
                    ],
                    "common_phrases": [
                        "czepiać się nadziei",
                        "czepiać się życia",
                        "czepiać się myśli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отчаянно удерживать мысль, надежду или возможность, пытаясь что-то спасти (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цепляться",
                                "target_lang_sense_clarification": "за надежду, жизнь"
                            },
                            {
                                "target_lang_word": "хвататься",
                                "target_lang_sense_clarification": "использовать любой шанс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c24ceea-3b5d-3cc4-a8c4-19ee5b6c27f0",
                    "sense_definition": "Przymocowywać, zawieszać lub zaczepiać coś o coś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attaching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ogrodnik ostrożnie <czepiał> pnącza róż do drewnianej kratki.",
                            "target_lang_translations": {
                                "ru": "Садовник осторожно <цеплял> вьющиеся розы к деревянной решётке."
                            }
                        },
                        {
                            "text": "Nie powinieneś <czepiać> tych plakatów na świeżo malowanej ścianie.",
                            "target_lang_translations": {
                                "ru": "Тебе не стоит <цеплять> эти плакаты на свежеокрашенную стену."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyczepiać",
                        "przymocowywać",
                        "zawieszać",
                        "przypinać"
                    ],
                    "common_phrases": [
                        "czepiać ozdoby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикреплять, вешать или зацеплять что-то за что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цеплять",
                                "target_lang_sense_clarification": "вешать, крепить"
                            },
                            {
                                "target_lang_word": "прицеплять",
                                "target_lang_sense_clarification": "присоединять"
                            }
                        ]
                    }
                }
            ]
        }
    ]

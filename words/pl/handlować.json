    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c25dd741-6aa0-3415-8d6e-96d19573a888",
                    "sense_definition": "Zajmować się zawodowo kupnem, sprzedażą lub wymianą towarów i usług w celu osiągnięcia zysku.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "trade_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek od wielu lat z powodzeniem <w>handluje</w> używanymi samochodami z zagranicy.",
                            "target_lang_translations": {
                                "ru": "Мой дядя уже много лет успешно <w>торгует</w> подержанными автомобилями из-за границы."
                            }
                        },
                        {
                            "text": "W dawnych czasach kupcy musieli podróżować daleko, aby <w>handlować</w> rzadkimi przyprawami.",
                            "target_lang_translations": {
                                "ru": "В давние времена купцам приходилось путешествовать далеко, чтобы <w>торговать</w> редкими специями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "handlować towarem",
                        "handlować na giełdzie",
                        "handlować z zagranicą",
                        "handlować w internecie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Профессионально заниматься покупкой, продажей или обменом товаров и услуг с целью получения прибыли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговать",
                                "target_lang_sense_clarification": "осуществлять куплю-продажу"
                            },
                            {
                                "target_lang_word": "заниматься торговлей",
                                "target_lang_sense_clarification": "вести коммерческую деятельность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9299ab98-1cc1-361e-be45-ad368536a06b",
                    "sense_definition": "Radzić sobie z czymś, akceptować trudną sytuację (używane potocznie jako kalka z języka angielskiego).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_coping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest moja ostateczna decyzja, więc po prostu <w>handluj</w> z tym i przestań narzekać.",
                            "target_lang_translations": {
                                "ru": "Это мое окончательное решение, так что просто <w>смирись</w> с этим и перестань жаловаться."
                            }
                        },
                        {
                            "text": "Rzeczywistość jest brutalna i musisz z nią <w>handlować</w>, zamiast uciekać od problemów.",
                            "target_lang_translations": {
                                "ru": "Реальность жестока, и ты должен с ней <w>справляться</w>, вместо того чтобы убегать от проблем."
                            }
                        }
                    ],
                    "synonyms": [
                        "radzić"
                    ],
                    "common_phrases": [
                        "handluj z tym"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang, calque from English 'deal with it'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Справляться с чем-либо, принимать сложную ситуацию (используется как сленговая калька с английского 'deal with it')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смириться",
                                "target_lang_sense_clarification": "принять ситуацию (эквивалент 'deal with it')"
                            },
                            {
                                "target_lang_word": "справляться",
                                "target_lang_sense_clarification": "решать проблему"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
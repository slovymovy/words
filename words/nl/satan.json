                        "Beëlzebub"
                    ],
                    "common_phrases": [
                        "dienaar van Satan",
                        "het rijk van Satan",
                        "verleid door Satan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дьявол, предводитель падших ангелов и олицетворение зла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сатана",
                                "target_lang_sense_clarification": "религиозный персонаж"
                            },
                            {
                                "target_lang_word": "Дьявол",
                                "target_lang_sense_clarification": "верховный дух зла"

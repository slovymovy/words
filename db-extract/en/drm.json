                        "tags": [],
                        "examples": [
                            {
                                "example_id": "a6c4423c-e078-4eb2-a384-2d546b8622b1",
                                "sense_id": "1f584833-3959-3947-b85f-a5bf3426fc34",
                                "text": "I know removing DRM is, well, not exactly looked positively upon by the companies, but do I have a legal leg to stand on if I am only removing the DRM for my own personal use, once I have already paid for a copy of the ebook?"
                            }
                        ]
                    },

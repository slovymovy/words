                            },
                            {
                                "target_lang_word": "hold up",
                                "target_lang_sense_clarification": "to prevent from falling"
                            },
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "to carry the weight of"
                            }
                        ]
                    }
                }

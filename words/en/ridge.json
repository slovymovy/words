    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b9b11c02-b3c7-389f-95a0-e0449ccc5bd0",
                    "sense_definition": "A long, narrow hilltop, mountain range, or watershed.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hiked along the narrow <w>ridge</w> to reach the summit.",
                            "target_lang_translations": {
                                "ru": "Мы шли вдоль узкого <w>гребня</w>, чтобы добраться до вершины."
                            }
                        },
                        {
                            "text": "The sun slowly disappeared behind the mountain <w>ridge</w>.",
                            "target_lang_translations": {
                                "ru": "Солнце медленно скрылось за горным <w>хребтом</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "trough"
                    ],
                    "common_phrases": [
                        "mountain ridge",
                        "along the ridge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная узкая возвышенность, горная цепь или водораздел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хребет",
                                "target_lang_sense_clarification": "горная цепь"
                            },
                            {
                                "target_lang_word": "гребень",
                                "target_lang_sense_clarification": "вершина горы или холма"
                            },
                            {
                                "target_lang_word": "горный хребет",
                                "target_lang_sense_clarification": "цепь гор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "483099f1-cb2a-3414-86e7-2e94f6ff6856",
                    "sense_definition": "A raised line or strip on the surface of something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Run your finger over the <w>ridge</w> on the edge of the coin.",
                            "target_lang_translations": {
                                "ru": "Проведите пальцем по <w>ребру</w> на краю монеты."
                            }
                        },
                        {
                            "text": "The scar left a thick <w>ridge</w> of skin on his arm.",
                            "target_lang_translations": {
                                "ru": "Шрам оставил толстый <w>валик</w> кожи на его руке."
                            }
                        }
                    ],
                    "antonyms": [
                        "groove"
                    ],
                    "common_phrases": [
                        "bony ridge",
                        "brow ridge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приподнятая линия или полоса на поверхности чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выступ",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "гребень",
                                "target_lang_sense_clarification": "выступающий край"
                            },
                            {
                                "target_lang_word": "ребро",
                                "target_lang_sense_clarification": "край предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "100091cb-9856-3130-a94c-66e6a9705070",
                    "sense_definition": "The highest horizontal line where two roof slopes meet.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Several birds were sitting on the <w>ridge</w> of the house.",
                            "target_lang_translations": {
                                "ru": "Несколько птиц сидели на <w>коньке</w> дома."
                            }
                        },
                        {
                            "text": "The builder needed to repair the tiles along the roof <w>ridge</w>.",
                            "target_lang_translations": {
                                "ru": "Строителю нужно было починить черепицу вдоль <w>конька</w> крыши."
                            }
                        }
                    ],
                    "common_phrases": [
                        "roof ridge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхнее горизонтальное ребро крыши, где сходятся два ската."
                    },
                    "translations": {
                        "ru": [
